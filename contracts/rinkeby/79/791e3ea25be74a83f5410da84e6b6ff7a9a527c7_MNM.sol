        HeroInd memory heroind;
        HeroStr memory herostr;

        // This might have to be moved to mint instead
        if (tryToken % randR == 0){
            herostr.unique = oneClass[oneClass.length-1];
            herostr.isUsed = "False";
            heroind.isUnique = true;
            oneClass.pop();
        }

        else {
            uint256 token;

            while(token == 0){
            
                (heroind, herostr) = genHero(tryToken);

                // Create DNA
                string memory dnaStr = string(abi.encodePacked(herostr.level, herostr.class, herostr.rarity, herostr.item, herostr.pet));
                uint256 dnaHash = uint256(keccak256(abi.encodePacked(dnaStr)));
                dna = dnaHash % (10 ** 8);
                
                if (tokens[dna] == false) {
                    tokens[dna] = true;
                    tryToken ++;
                    token = tryToken;
                }

                else {
                    tryToken ++;
                }
            }
        }

        //This is working
        heroInds[numClaimed] = heroind;
        heroStrs[numClaimed] = herostr;
        _mint(_msgSender(), numClaimed, 1, "");
        numClaimed += 1;
    }

