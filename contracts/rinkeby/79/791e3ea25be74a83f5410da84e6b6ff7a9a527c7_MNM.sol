        HeroInd memory heroind;
        HeroStr memory herostr;

        if (indArr[5] == 1){
            
            herostr.unique = "Test";
            herostr.isUsed = "False";
            heroind.isUnique = true;

        }
            
        else{
  
            heroind = HeroInd(indArr[0],indArr[1],indArr[2],indArr[3],indArr[4], false);
            herostr = HeroStr(level[indArr[0]], class[indArr[1]], rarity[indArr[2]], item[indArr[3]], pet[indArr[4]], "False", "");  
            
            // Create DNA
            string memory dnaStr = string(abi.encodePacked(herostr.level, herostr.class, herostr.rarity, herostr.item, herostr.pet));
            uint256 dnaHash = uint256(keccak256(abi.encodePacked(dnaStr)));
            dna = dnaHash % (10 ** 8);
