/**
 * @title CastleStakingContract
 */
contract CastleStakingContract is OwnableUpgradeable, ERC1155HolderUpgradeable {
    using ArrayUtils for uint256[];

    modifier onlyWorkerStakingContract() {
        require(
            _msgSender() == address(workerStakingContr),
            "INVALIDOPERATION"
        );
        _;
    }

    IERC1155 internal pieceContr;
    IERC1155 internal puzzleContr;
    IERC721Upgradeable internal citizenContr;
    WorkerStakingContractInterface internal workerStakingContr;
    CrownsContractInterface internal crownsContr;

    uint8 public constant MAX_WORKERS_PER_PIECE = 2;
    uint8 public constant MAX_WORKERS_PER_PUZZLE = 50;

    /// @dev The key in this mapping a generated hash from StakingUtils.generateKey(_address, tokenId, isPuzzle)
    mapping(uint256 => StakingUtils.StakedPiecePuzzle)
        public stakedPiecePuzzles;
    /// @dev The key in the inner mapping is a boolean (0 or 1) indicating whether its a piece or puzzle
    mapping(address => mapping(uint256 => uint256[]))
        public indexedPiecePuzzlesByOwner;

    function initialize(
        IERC1155 _pieceContr,
        IERC1155 _puzzleContr,
        CrownsContractInterface _crownsContr,
        IERC721Upgradeable _citizenContr
    ) public initializer {
        __Ownable_init();
        __ERC1155Holder_init();

        pieceContr = _pieceContr;
        puzzleContr = _puzzleContr;
        crownsContr = _crownsContr;
        citizenContr = _citizenContr;
    }

    /** @notice Initializes connection to the WorkerStakingContract
     *
     * @param _workerStakingContr contract address of WorkerStakingContract
     */
    function setWorkerStakingContractAddress(address _workerStakingContr)
        external
        onlyOwner
    {
        require(address(workerStakingContr) == address(0), "ALREADYSET");

        workerStakingContr = WorkerStakingContractInterface(
            _workerStakingContr
        );
    }

    /** @notice Stakes a batch of piecePuzzle {ERC1155} tokens
     *
     * @dev Since the tokens are ERC1155 compliant one has to specify how many copies of each token
     *
     * @param _tokenIds List of tokenIds to stake
     * @param _amounts List of amounts for the respective tokenIds
     * @param _arePuzzles Indicates whether the respective tokenIds are puzzles or pieces
     */
    function stakeManyPiecePuzzles(
        uint256[] calldata _tokenIds,
        uint256[] calldata _amounts,
        bool _arePuzzles
    ) external {
        /// @dev `_arePuzzles` indicates which contract is to use
        IERC1155 piecePuzzleContr = _arePuzzles ? puzzleContr : pieceContr;

        for (uint256 i; i < _tokenIds.length; i++) {
            uint256 key = StakingUtils.generateKey(
                _msgSender(),
                _tokenIds[i],
                _arePuzzles
            );

            /// @dev address + isPuzzle used as key
            uint256[] storage stakedTokenIds = indexedPiecePuzzlesByOwner[
                _msgSender()
            ][StakingUtils.toUint(_arePuzzles)];

            /// @dev only push if non-existent
            /// @dev e.g. if a user stakes 5x tokenId 42 and then another 3x of tokenId 42
            if (!stakedTokenIds.isValueInArray(_tokenIds[i])) {
                stakedTokenIds.push(_tokenIds[i]);
            }

            stakedPiecePuzzles[key].amount += _amounts[i];
            stakedPiecePuzzles[key].tokenId = _tokenIds[i];
            stakedPiecePuzzles[key].owner = _msgSender();
        }

        piecePuzzleContr.safeBatchTransferFrom(
            _msgSender(),
            address(this),
            _tokenIds,
            _amounts,
            "0x0"
        );
    }

    /**
     * @notice Unstakes a batch of piecePuzzle {ERC1155} tokens
     *
     * @dev Since the tokens are ERC1155 compliant one has to specify how many copies of each token
     *
     * @param _tokenIds List of tokenIds to stake
     * @param _amounts List of amounts for the respective tokenIds
     * @param _arePuzzles Indicates whether the respective tokenIds are puzzles or pieces
     */
    function unstakeManyPiecePuzzles(
        uint256[] calldata _tokenIds,
        uint256[] calldata _amounts,
        bool _arePuzzles
    ) external {
        /// @dev `_arePuzzles` indicates which contract is to use
        IERC1155 piecePuzzleContr = _arePuzzles ? puzzleContr : pieceContr;
        /// @dev address + isPuzzle used as key
        uint256[] storage stakedTokenIds = indexedPiecePuzzlesByOwner[
            _msgSender()
        ][StakingUtils.toUint(_arePuzzles)];

        for (uint256 i; i < _tokenIds.length; i++) {
            StakingUtils.StakedPiecePuzzle
                storage existingStake = stakedPiecePuzzles[
                    StakingUtils.generateKey(
                        _msgSender(),
                        _tokenIds[i],
                        _arePuzzles
                    )
                ];

            require(
                stakedTokenIds.isValueInArray(_tokenIds[i]),
                "NOTYOURPIECEPUZZLE"
            );
            require(
                existingStake.amount >= _amounts[i],
                "NOTENOUGHPIECEPUZZLES"
            );
            /// @dev user has to empty a piece before unstaking it
            require(
                existingStake.workers.length == 0,
                "CANNOTUNSTAKEWHILEWORKING"
            );

            existingStake.amount -= _amounts[i];
            /// @dev remove token from indexed stake if balance == 0
            if (existingStake.amount == 0) {
                stakedTokenIds.deleteValueInArray(_tokenIds[i]);
            }
        }

        piecePuzzleContr.safeBatchTransferFrom(
            address(this),
            _msgSender(),
            _tokenIds,
            _amounts,
            "0x0"
        );
    }

    /** @notice Takes a batch of staked workers {ERC721} and assigns them to a piecePuzzle {ERC1155}
     *
     * @param _workerTokenIds Staked worker tokenId
     * @param _piecePuzzleId TokenId of piece/puzzle where the workers get to work
     * @param _isPuzzle Indicates whether the respective tokenId is a puzzle or piece
     */
    function sendWorkersToWork(
        uint256[] calldata _workerTokenIds,
        uint256 _piecePuzzleId,
        bool _isPuzzle
    ) external {
        uint256 key = StakingUtils.generateKey(
            _msgSender(),
            _piecePuzzleId,
            _isPuzzle
        );

        uint256 piecePuzzleWorkerCount = stakedPiecePuzzles[key].workers.length;
        uint256 amount = stakedPiecePuzzles[key].amount;

        _isPuzzle
            ? require(
                piecePuzzleWorkerCount + _workerTokenIds.length <=
                    MAX_WORKERS_PER_PUZZLE * amount,
                "PUZZLEFULL"
            )
            : require(
                piecePuzzleWorkerCount + _workerTokenIds.length <=
                    MAX_WORKERS_PER_PIECE * amount,
                "PIECEFULL"
            );

        for (uint256 i; i < _workerTokenIds.length; i++) {
            require(
                citizenContr.ownerOf(_workerTokenIds[i]) ==
                    address(workerStakingContr),
                "NOTSTAKED"
            );
            require(
                workerStakingContr.getStakedWorkersOwner(_workerTokenIds[i]) ==
                    _msgSender(),
                "NOTYOURWORKER"
            );

            stakedPiecePuzzles[key].workers.push(_workerTokenIds[i]);

            workerStakingContr.updateStakedWorker(
                _workerTokenIds[i],
                _piecePuzzleId,
                _isPuzzle,
                _msgSender()
            );
        }
    }

    // function getTaxRate(uint256 key) external view returns (uint256) {
    //     return stakedPiecePuzzles[key].taxRate;
    // }

    // function claimManyLandlordTaxes(uint256[] calldata puzzleTokenIds)
    //     external
    // {
    //     for (uint256 i; i < puzzleTokenIds.length; i++) {
    //         uint256 taxesClaimed;

    //         uint256 key = StakingUtils.generateKey(
    //             _msgSender(),
    //             puzzleTokenIds[i],
    //             true
    //         );
    //         uint256[] memory workers = stakedPiecePuzzles[key].workers;

    //         for (uint256 x; x < workers.length; x++) {
    //             address landlord = workerStakingContr.getStakedWorkersLandlord(
    //                 workers[x]
    //             );

    //             require(landlord == _msgSender(), "NOTYOURCASTLE");

    //             uint256[] memory arr = new uint256[](1);
    //             arr[0] = workers[x];

    //             (, uint256[] memory ownersTaxes, ) = workerStakingContr
    //                 .calculateRewardsForManyWorkers(arr);

    //             workerStakingContr.setTaxesCollectedByLandLordForWorker(
    //                 workers[x],
    //                 ownersTaxes[0]
    //             );

    //             taxesClaimed += ownersTaxes[0];
    //         }

    //         crownsContr.mint(_msgSender(), taxesClaimed);
    //     }
    // }

    /**
     * @notice Aggregates all staked piecePuzzle {ERC1155} tokens of the sender address and returns them in a 2D array
     *
     * @dev The outer array is of length 2
     * @dev The first inner array contains all pieces, the second all puzzles
     *
     * @return uint256[2][] The aggregated stake of the sender
     */
    function getAllStakedPiecePuzzles(address _account)
        external
        view
        returns (StakingUtils.StakedPiecePuzzle[][2] memory)
    {
        StakingUtils.StakedPiecePuzzle[]
            memory _stakedPieces = new StakingUtils.StakedPiecePuzzle[](
                indexedPiecePuzzlesByOwner[_account][0].length
            );
        StakingUtils.StakedPiecePuzzle[]
            memory _stakedPuzzles = new StakingUtils.StakedPiecePuzzle[](
                indexedPiecePuzzlesByOwner[_account][1].length
            );

        for (uint256 i; i < _stakedPieces.length; i++) {
            _stakedPieces[i] = stakedPiecePuzzles[
                StakingUtils.generateKey(
                    _account,
                    indexedPiecePuzzlesByOwner[_account][0][i],
                    false
                )
            ];
        }
        for (uint256 i; i < _stakedPuzzles.length; i++) {
            _stakedPuzzles[i] = stakedPiecePuzzles[
                StakingUtils.generateKey(
                    _account,
                    indexedPiecePuzzlesByOwner[_account][1][i],
                    true
                )
            ];
        }

        return [_stakedPieces, _stakedPuzzles];
    }

    /**
     * @notice Removes a worker {ERC721} from a given piecePuzzle {ERC1155}
     *
     * @dev Only callable by `address(workerStakingContr)`
     *
     * @param _key Generated key from StakingUtils.generateKey(_address, tokenId, isPuzzle)
     * @param _workerTokenId TokenId of the worker to be removed
     */
    function removeWorker(uint256 _key, uint256 _workerTokenId)
        public
        onlyWorkerStakingContract
    {
        stakedPiecePuzzles[_key].workers.deleteValueInArray(_workerTokenId);
    }

    /*** @dev Whenever an {ERC1155} `tokenId` token is transferred to this contract via {IERC1155-safeBatchTransferFrom}
     * by `operator` from `from`, this function is called.
     *
     * It must return its Solidity selector to confirm the token transfer.
     * If any other value is returned or the interface is not implemented by the recipient,
     * the transfer will be reverted.
     *
     * The selector can be obtained in Solidity with `this.onERC1155BatchReceived.selector`.
     */
    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] memory id,
        uint256[] memory value,
        bytes memory data
    ) public pure override returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
