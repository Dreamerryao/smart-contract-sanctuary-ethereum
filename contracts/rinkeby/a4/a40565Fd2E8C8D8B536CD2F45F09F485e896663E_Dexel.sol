pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "erc721a/contracts/extensions/ERC721AQueryable.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";


/**
 * @title Dexel (Decentralized Pixels) smart contract
 * @dev Extends ERC721 Non-Fungible Token Standard basic implementation
 */
contract Dexel is ERC721AQueryable, Ownable {

  event Place(address owner, uint256 tokenId, uint256 boardId);
  event Edit(address owner, uint256 tokenId, bytes data);
  event Lock(address owner, uint256 tokenId, uint256 date);

  uint256 public constant STARTING_TOKEN_ID  = 1;
  uint256 public constant BOARD_SIZE         = 1000;
  uint256 public constant BOARD_SIDE         = 125;
  uint256 public constant DEXEL_SIDE         = 8;
  uint256 public constant COLOR_LENGTH       = 3;
  uint256 public constant DATA_SIZE          = DEXEL_SIDE * DEXEL_SIDE * COLOR_LENGTH;
  uint256 public constant TOTAL_DEXEL        = (BOARD_SIZE * BOARD_SIZE) / (DEXEL_SIDE * DEXEL_SIDE);

  // tokenIDs, mint ID of the token, goes from 1 to 160000
  // boardIDs, place of the token on the board, goes from 1 to 160000
  mapping(uint256 => uint256) private _tokenIDs;
  mapping(uint256 => uint256) private _boardIDs;

  mapping(uint256 => bytes)  private _data;
  mapping(uint256 => uint)    private _lock;
  mapping(address => bool)    private _whitelist;

  uint256 private _price;
  uint256 private _mintLimit;
  uint256 private _whitelistLimit;
  bool    private _whitelistStatus;
  string  private _svg;
  string  private _webURL;


  constructor(string memory name, string memory symbol) ERC721A(name, symbol) {
    _price           = 50_000_000_000_000; // .00005 eth
    _mintLimit       = 1000;
    _whitelistLimit  = 1000;
    _whitelistStatus = false;
    _svg             = '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -0.5 8 8" shape-rendering="crispEdges">';
    _webURL          = 'https://dexel.art/';
  }

  function _startTokenId() override internal view virtual returns (uint256) {
    return STARTING_TOKEN_ID;
  }

  function getAvailablePixelBlocksCount() public view returns (uint256) {
    return TOTAL_DEXEL - totalSupply();
  }

  function mint(uint256 quantity) public payable {
    if (_whitelistStatus) {
      require(balanceOf(msg.sender) + quantity <= _whitelistLimit, 'MINT_LIMIT');
      require(_whitelist[msg.sender], 'NOT_WHITELISTED');
    }

    require(quantity > 0, 'MINT_LIMIT');
    require(quantity <= _mintLimit, 'MINT_LIMIT');
    require(_totalMinted() + quantity < TOTAL_DEXEL, 'MINT_LIMIT');
    require(_price * quantity <= msg.value, 'INSUFFICIENT_VALUE');

    _safeMint(msg.sender, quantity);
  }

  function place(uint256[] memory tokenIDs, uint256[] memory boardIDs) public payable {
    require(tokenIDs.length > 0, 'WRONG_DATA');
    require(boardIDs.length > 0, 'WRONG_DATA');

    require(tokenIDs.length == boardIDs.length, 'WRONG_DATA');

    for (uint i=0; i < tokenIDs.length; i++) {
      require(_exists(tokenIDs[i]), 'NOT_MINTED');
      require(ownerOf(tokenIDs[i]) == msg.sender, 'NOT_OWNED');
      require(_tokenIDs[boardIDs[i]] == 0, 'PLACE_TAKEN');
      require(_boardIDs[tokenIDs[i]] == 0, 'ALREADY_PLACED');
      require(boardIDs[i] > 0, 'WRONG_BOARDID');
      require(boardIDs[i] <= TOTAL_DEXEL, 'WRONG_BOARDID');
    }

    for (uint i=0; i < tokenIDs.length; i++) {
      _tokenIDs[boardIDs[i]] = tokenIDs[i];
      _boardIDs[tokenIDs[i]] = boardIDs[i];
      emit Place(msg.sender, tokenIDs[i], boardIDs[i]);
    }
  }

  function edit(uint256[] memory tokenIDs, bytes[] memory data) public {
      require(tokenIDs.length == data.length, 'WRONG_DATA');

      for (uint i=0; i < tokenIDs.length; i++) {
          require(_exists(tokenIDs[i]), 'NOT_MINTED');
          require(ownerOf(tokenIDs[i]) == msg.sender, 'NOT_OWNED');
          require(_lock[tokenIDs[i]] < block.timestamp, 'LOCKED');
          require(_boardIDs[tokenIDs[i]] > 0, 'NOT_PLACED');
          
          // TODO: Check data length equals DATA_SIZE
          _data[tokenIDs[i]] = data[i];
          emit Edit(msg.sender, tokenIDs[i], data[i]);
      }
  }

  function lock(uint256[] memory tokenIDs, uint date) public {
    require(date > block.timestamp, "INVALID_DATE");

    for (uint i=0; i < tokenIDs.length; i++) {
        require(_exists(tokenIDs[i]), 'NOT_MINTED');
        require(ownerOf(tokenIDs[i]) == msg.sender, 'NOT_OWNED');
        require(_boardIDs[tokenIDs[i]] > 0, 'NOT_PLACED');
        require(_lock[tokenIDs[i]] < date, 'LOCKED');
    }

    for (uint i=0; i < tokenIDs.length; i++) {
        _lock[tokenIDs[i]] = date;
        emit Lock(msg.sender, tokenIDs[i], date);
    }
  }

  function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    require(_exists(tokenId), "NOT_MINTED");

    string memory json    = '';
    string memory jsonUri = '';
    string memory data = bytesToString(_data[tokenId]);

    uint lockedUntil = _lock[tokenId];
    bool locked      = lockedUntil > block.timestamp;

    uint256 boardID = _boardIDs[tokenId];
    uint256 x = boardID != 0 ? ((boardID - 1) % BOARD_SIDE) : 0;
    uint256 y = boardID != 0 ? ((boardID - 1) / BOARD_SIDE) : 0;

    if (bytes(data).length == 0) {
      data = 'ffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff';
    }

    if (boardID == 0) {
      json    = Base64.encode(abi.encodePacked('{"name":"Dexel #', Strings.toString(tokenId), '", "description": "A Dexel that has not been placed on the board", "image":"', string(abi.encodePacked('data:image/svg+xml;base64,', Base64.encode(bytes(generateSvg(data))))), '"}'));
      jsonUri = string(abi.encodePacked("data:application/json;base64,", json));
    } else if (locked) {
      json    = Base64.encode(abi.encodePacked('{"name":"Dexel #', Strings.toString(tokenId), '", "description": "A Dexel that is placed on the board. Currently locked", "external_url": "', string(abi.encodePacked(_webURL, '?id=', Strings.toString(boardID))), '", "image":"', string(abi.encodePacked('data:image/svg+xml;base64,', Base64.encode(bytes(generateSvg(data))))), '", "attributes": [{"trait_type": "X", "value": "', Strings.toString(x), '"}, {"trait_type": "Y", "value": "', Strings.toString(y), '"}, {"trait_type": "Board ID", "value": "', Strings.toString(boardID), '"}, {"trait_type": "Locked", "value": "Yes"}, { "display_type": "date",  "trait_type": "Locked Until", "value": "', Strings.toString(lockedUntil), '"}]}'));
      jsonUri = string(abi.encodePacked("data:application/json;base64,", json));
    } else {
      json    = Base64.encode(abi.encodePacked('{"name":"Dexel #', Strings.toString(tokenId), '", "description": "A Dexel that is placed on the board.", "external_url": "', string(abi.encodePacked(_webURL, '?id=', Strings.toString(boardID))), '", "image":"', string(abi.encodePacked('data:image/svg+xml;base64,', Base64.encode(bytes(generateSvg(data))))), '", "attributes": [{"trait_type": "X", "value": "', Strings.toString(x), '"}, {"trait_type": "Y", "value": "', Strings.toString(y), '"}, {"trait_type": "Board ID", "value": "', Strings.toString(boardID), '"}, {"trait_type": "Locked", "value": "No"}]}'));
      jsonUri = string(abi.encodePacked("data:application/json;base64,", json));
    }    

    return jsonUri;

  }

  function generateSvg(string memory data) public view returns (string memory) {
    bytes[]  memory svg   = new bytes[]((DEXEL_SIDE * DEXEL_SIDE) + 2);
    string[] memory parts = new string[](DEXEL_SIDE * DEXEL_SIDE);
    uint256 totalLength = 0;

    bytes memory strBytes = bytes(data);
    for(uint i = 0; i < strBytes.length; i += 3) {
      parts[i/3] = string(abi.encodePacked(strBytes[i], strBytes[i+1], strBytes[i+2]));
    }

    svg[0] = abi.encodePacked(_svg);
    totalLength += svg[0].length;

    for (uint256 i=0; i < DEXEL_SIDE; i++) {
      for (uint256 j=0; j < DEXEL_SIDE; j++) {
        bytes memory encodedSvg = abi.encodePacked('<path stroke="#', parts[(i * DEXEL_SIDE) + j],'" d="M', Strings.toString(j), ' ', Strings.toString(i), 'h1" />');

        svg[(i * DEXEL_SIDE) + j + 1] = encodedSvg;
        totalLength += encodedSvg.length;
      }
    }

    svg[svg.length - 1] = '</svg>';
    totalLength += svg[svg.length - 1].length;

    return string(_concatenateArray(svg, totalLength));
  }

  function _concatenateArray(bytes[] memory input, uint256 outputLength) private pure returns (bytes memory result) {
    bytes   memory output = new bytes(outputLength);
    uint256        offset = 0;

    uint outputPtr;
    assembly { outputPtr := add(output, 32) }

    for (uint256 i=0;i<input.length;i++) {
      bytes   memory inputElement = input[i];
      uint256        length       = inputElement.length;

      uint inputElementPtr;
      assembly { inputElementPtr := add(inputElement, 32) }

      _memcpy(outputPtr + offset, inputElementPtr, length);
      offset += length;
    }

    return output;
  }

  function bytesToString(bytes memory buffer) public pure returns (string memory) {
    bytes memory converted = new bytes(buffer.length * 2);
    bytes memory _base = "0123456789abcdef";

    for (uint256 i = 0; i < buffer.length; i++) {
        converted[i * 2] = _base[uint8(buffer[i]) / _base.length];
        converted[i * 2 + 1] = _base[uint8(buffer[i]) % _base.length];
    }

    return string(converted);
  }

  function _memcpy(uint dest, uint src, uint len) private pure {
    // Copy word-length chunks while possible
    for(; len >= 32; len -= 32) {
      assembly {
        mstore(dest, mload(src))
      }
      dest += 32;
      src += 32;
    }

    // Copy remaining bytes
    if (len > 0) {
      uint mask = 256 ** (32 - len) - 1;
      assembly {
        let srcpart := and(mload(src), not(mask))
        let destpart := and(mload(dest), mask)
        mstore(dest, or(destpart, srcpart))
      }
    }
  }

  function substring(string memory str, uint startIndex, uint endIndex) public pure returns (string memory ) {
    bytes memory strBytes = bytes(str);
    bytes memory result = new bytes(endIndex-startIndex);
    for(uint i = startIndex; i < endIndex; i++) {
        result[i-startIndex] = strBytes[i];
    }
    return string(result);
  }

  function setMintLimit(uint256 mintLimit) public onlyOwner {
    require(mintLimit > 0);
    _mintLimit = mintLimit;
  } 

  function getMintLimit() public view returns (uint256) {
    return _mintLimit;
  }


  function setPrice(uint256 newPrice) public onlyOwner {
    _price = newPrice;
  } 

  function getPrice() public view returns (uint256) {
    return _price;
  }


  function setSvg(string calldata svg) public onlyOwner {
    _svg = svg;
  } 

  function getSvg() public view returns (string memory) {
    return _svg;
  }


  function setWebURL(string calldata webURL) public onlyOwner {
    _webURL = webURL;
  } 

  function getWebURL() public view returns (string memory) {
    return _webURL;
  }


  function setWhitelist(bool status) public onlyOwner {
    _whitelistStatus = status;
  } 

  function getWhitelist() public view returns (bool) {
    return _whitelistStatus;
  }


  function setWhitelistLimit(uint256 newLimit) public onlyOwner {
    _whitelistLimit = newLimit;
  } 

  function getWhitelistLimit() public view returns (uint256) {
    return _whitelistLimit;
  }


  function setUserWhitelist(address _address) public onlyOwner {
    _whitelist[_address] = true;
  }

  function getUserWhitelist(address _address) public view returns(bool) {
    bool whitelisted = _whitelist[_address];
    return whitelisted;
  }


  function getBoardID(uint256 id) public view returns (uint256) {
    return _boardIDs[id];
  }

  function getTokenID(uint256 id) public view returns (uint256) {
    return _tokenIDs[id];
  }

  function getData(uint256 id) public view returns (bytes memory) {
    return _data[id];
  }

  function getLock(uint256 id) public view returns (uint256) {
    return _lock[id];
  }
}

