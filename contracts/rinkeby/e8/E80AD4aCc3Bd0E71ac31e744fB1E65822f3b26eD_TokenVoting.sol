import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface IGnosisContract {
    function addOwnerWithThreshold(address owner, uint256 threshold) external;

    function removeOwner(
        address prevOwner,
        address owner,
        uint256 threshold
    ) external;

    function getOwners() external view returns (address[] memory);

    function getThreshold() external view returns (uint256);
}

contract TokenVoting {
    // A user needs to lock their tokens to gain governance power
    // Delegation results in handing over vote power to another voter
    // When a user wants to exit the governance, their delegator's vote power is reduced and tokens are sent back to caller
    // When user exits governance, they lose their tokens' voting power. But their delegated power remains.

    IERC20 public voteToken;
    struct Proposal {
        IGnosisContract targetGnosis;
        bool addOrRemove;
        address targetOwner;
        uint256 threshold;
        uint256 yesCount;
        uint256 noCount;
        bool passed;
    }
    struct Voter {
        bool initialize;
        address delegatedTo;
        uint256 delegatedTokens;
    }
    uint256 public proposalCount;
    uint256 private mutex = 0;
    uint256 public totalTokenBalances = 0;
    Proposal[] public proposals;
    mapping(address => Voter) public voters;
    mapping(address => uint256) public tokenBalances;
    mapping(address => mapping(uint256 => uint256)) public userVoteChoice; // 1 is YES and 2 is NO

    modifier mutexLock() {
        require(mutex == 0, "LOCKED");
        mutex = 1;
        _;
        mutex = 0;
    }

    constructor(IERC20 token) {
        voteToken = token;
    }

    /// @notice Locks tokens and gives vote power to user
    /// @dev Sets user initialize to true enabling to vote
    /// @return bool Returns true if call succeeds
    function enterGovernance() public mutexLock returns (bool) {
        // Makes voter struct valid and on boards user's token vote weight
        require(voters[msg.sender].initialize == false, "Already Initialized");
        uint256 userBalance = voteToken.balanceOf(msg.sender);
        bool success = voteToken.transferFrom(
            msg.sender,
            address(this),
            userBalance
        );
        require(success, "Token transfer failed");
        tokenBalances[msg.sender] = userBalance;
        voters[msg.sender].initialize = true;
        totalTokenBalances += userBalance;
        return true;
    }

    /// @notice Exits governance and gives back tokens
    /// @dev Removes vote power of delegate in unpassed proposals
    /// @return bool Returns true if call succeeds
    function exitGovernance() public mutexLock returns (bool) {
        // Reduce user votes on unpassed proposals and return tokens
        require(
            voters[msg.sender].initialize == true,
            "Enter governance first"
        );
        if (voters[msg.sender].delegatedTo != address(0)) {
            _removeDelegation(voters[msg.sender].delegatedTo);
        } else {
            _reduceVotes(msg.sender, tokenBalances[msg.sender]);
        }
        uint256 userBalance = tokenBalances[msg.sender];
        tokenBalances[msg.sender] = 0;
        voters[msg.sender].initialize = false;
        totalTokenBalances -= userBalance;
        bool success = voteToken.transfer(msg.sender, userBalance);
        require(success, "Transfer failed");
        return true;
    }

    /// @notice Vote on a Proposal
    /// @dev Makes double vote checks and prevents is already delegated.
    /// @param proposalId The ID of the proposal
    /// @param choice The true/false choice for yes/no respectively
    /// @return bool Transaction succeed or not
    function vote(uint256 proposalId, bool choice)
        public
        mutexLock
        returns (bool)
    {
        require(
            voters[msg.sender].delegatedTokens > 0 ||
                voters[msg.sender].initialize == true,
            "User has no vote power"
        );
        require(proposalId < proposalCount, "Proposal does not exist");
        require(
            proposals[proposalId].passed == false,
            "Proposal already passed"
        );
        require(userVoteChoice[msg.sender][proposalId] == 0, "Already voted");
        require(
            voters[msg.sender].delegatedTo == address(0),
            "Delegated, cannot vote"
        );
        Proposal storage p = proposals[proposalId];
        if (choice) {
            userVoteChoice[msg.sender][proposalId] = 1;
            p.yesCount += tokenBalances[msg.sender];
        } else {
            userVoteChoice[msg.sender][proposalId] = 2;
            p.noCount += tokenBalances[msg.sender];
        }
        return true;
    }

    /// @notice Checks if two proposals are same
    /// @dev Returns bool whether two proposals are same
    /// @param currentProposal The first proposal struct
    /// @param newProposal The second proposal struct
    /// @return bool Transaction succeed or not
    function checkIfSameProposal(
        Proposal memory currentProposal,
        Proposal memory newProposal
    ) internal pure returns (bool) {
        return
            keccak256(abi.encode(currentProposal)) !=
            keccak256(abi.encode(newProposal));
    }

    /// @notice Create a new Proposal
    /// @dev Checks for duplicates and creates a new proposal struct
    /// @param targetGnosis The target Gnosis Safe address
    /// @param addOrRemove True to add owner, false to remove
    /// @param targetOwner The address to add or remove as owner
    /// @param threshold The vote threshold to achieve to be eligible for execution
    /// @return Documents the return variables of a contractâ€™s function state variable
    function createProposal(
        address targetGnosis,
        bool addOrRemove,
        address targetOwner,
        uint256 threshold
    ) public mutexLock returns (bool) {
        require(totalTokenBalances > 0, "No Governance to vote");
        require(
            threshold >= totalTokenBalances / 2,
            "Threshold must be atleast 50%"
        );
        Proposal memory newProposal = Proposal(
            IGnosisContract(targetGnosis),
            addOrRemove,
            targetOwner,
            threshold,
            0,
            0,
            false
        );
        for (uint256 i = 0; i < proposalCount; i++) {
            Proposal memory currentProposal = proposals[i];
            require(
                checkIfSameProposal(currentProposal, newProposal),
                "Duplicated Proposal"
            );
        }
        proposals.push(newProposal);
        proposalCount += 1;
        return true;
    }

    /// @notice Execute a Proposal
    /// @dev Makes a call to the Gnosis contract to add or remove owner
    /// @param proposalId The id of proposal to be executed
    /// @return bool Returns if transaction succeeded or not
    function executeProposal(uint256 proposalId)
        public
        mutexLock
        returns (bool)
    {
        require(proposalId < proposalCount, "Proposal does not exist");
        Proposal storage p = proposals[proposalId];
        require(p.passed == false, "Proposal already passed");
        require(p.yesCount >= p.threshold, "Not enough votes to execute");
        p.passed = true;
        if (p.addOrRemove == true) addGnosisOwner(p);
        else removeGnosisOwner(p);
        return true;
    }

    /// @notice Adds an owner to the target gnosis contract
    /// @dev Makes a call to the Gnosis contract to add owner
    /// @param p The proposal struct
    function addGnosisOwner(Proposal memory p) internal {
        uint256 threshold = p.targetGnosis.getThreshold();
        if (threshold == 1) threshold = 1;
        else threshold += 1;
        address newOwner = p.targetOwner;
        p.targetGnosis.addOwnerWithThreshold(newOwner, threshold);
    }

    /// @notice Removes an owner from target gnosis contract
    /// @dev Makes a call to the Gnosis contract to remove owner
    /// @param p The proposal struct
    function removeGnosisOwner(Proposal memory p) internal {
        address[] memory owners = p.targetGnosis.getOwners();
        uint256 currentThreshold = p.targetGnosis.getThreshold();
        console.log("Threshold is", currentThreshold);
        uint256 index = 0;
        address prevOwner = address(0x01);
        while (owners[index] != p.targetOwner) {
            prevOwner = owners[index];
            index += 1;
        }
        currentThreshold = (currentThreshold > 1) ? owners.length - 1 : 1;
        require(index < owners.length, "Owner not found");
        p.targetGnosis.removeOwner(prevOwner, p.targetOwner, currentThreshold);
    }

    // Delegate Code
    // Resolve delegate targets to head delegates reject cyclic dependency.
    // When user delegates, add that user's votes to delegate's vote power
    // Additionally, if delegate made a choice on currently pending proposal, add user's power to the same choice

    // Delegate votes to another user.
    // If already delegated, reduce previous delegator's votes and increase new delegator's votes
    // If not, reduce user votes on unpassed proposals and delegate

    // Attack vectors vs Solutions
    // User votes and tries to vote again : State record prevents this
    // User votes and delegates for double votes : Remove user effects when delegating
    // Delegate votes and user withdraws from delegate for double vote : Reduce delegate's effect / Mark user as voted (Chose 1st method)
    // Re-delegate and double vote for 2 delegators : Reduce effect of prev delegator before re delegating

    /// @notice Gets the head delegator of a voter
    /// @dev Recusively searches the delegator chain to retrieve head delegator
    /// @param firstDelegator Address of the voter's intended delegator
    /// @return address the address of head delegator
    function getCoreDelegator(address firstDelegator)
        public
        view
        returns (address)
    {
        address current = firstDelegator;
        address next = voters[firstDelegator].delegatedTo;
        while (next != address(0)) {
            current = next;
            next = voters[current].delegatedTo;
        }
        return current;
    }

    /// @notice Reduces the vote effect of a target address
    /// @dev Safely reduces votes of a target address on unpassed proposals
    /// @param target Address of the target
    /// @param amount Amount of votes to reduce
    function _reduceVotes(address target, uint256 amount) internal {
        for (uint256 i = 0; i < proposals.length; i++) {
            Proposal storage p = proposals[i];
            if (p.passed == false) {
                uint256 voterChoice = userVoteChoice[target][i];
                if (voterChoice == 1) p.yesCount -= amount;
                if (voterChoice == 2) p.noCount -= amount;
            }
        }
    }

    /// @notice Increases the vote effect of a target address
    /// @dev Safely increases vote effect of a target address on unpassed proposals
    /// @param target Address of the target
    /// @param amount Amount of votes to reduce
    function _increaseVotes(address target, uint256 amount) internal {
        for (uint256 i = 0; i < proposals.length; i++) {
            Proposal storage p = proposals[i];
            if (userVoteChoice[msg.sender][i] != 0) continue; // User voted and delegated. Don't add votes to such proposals
            if (p.passed == false) {
                uint256 voterChoice = userVoteChoice[target][i];
                if (voterChoice == 1) p.yesCount += amount;
                if (voterChoice == 2) p.noCount += amount;
            }
        }
    }

    /// @notice Delegates a voter's power to an address
    /// @dev Finds head delegator and delegates to that address
    /// @param delegateAddress Address of the intended delegator
    function _delegate(address delegateAddress) internal {
        address headDelegator = getCoreDelegator(delegateAddress);
        Voter storage delegateVoter = voters[headDelegator];
        Voter storage userVoter = voters[msg.sender];
        uint256 totalVotes = userVoter.delegatedTokens +
            tokenBalances[msg.sender];
        delegateVoter.delegatedTokens += totalVotes;
        _increaseVotes(headDelegator, totalVotes);
        userVoter.delegatedTo = headDelegator;
    }

    /// @notice Remove delegation of the caller
    /// @dev Resolves to head delegator and reduces vote power
    /// @param delegateAddress Address of the voter's delegator
    function _removeDelegation(address delegateAddress) internal {
        address headDelegator = getCoreDelegator(delegateAddress);
        Voter storage userVoter = voters[msg.sender];
        Voter storage delegateVoter = voters[headDelegator];
        uint256 votesToRemove = tokenBalances[msg.sender] +
            userVoter.delegatedTokens;
        delegateVoter.delegatedTokens -= votesToRemove;
        _reduceVotes(headDelegator, votesToRemove);
        userVoter.delegatedTo = address(0);
    }

    /// @notice Delegates caller's vote power to user
    /// @dev Resolves to head delegator and delegates vote power
    /// @param delegateAddress Address of the voter's delegator
    /// @return bool Transaction succeeded or not
    function delegate(address delegateAddress) public mutexLock returns (bool) {
        require(voters[msg.sender].initialize, "Enter governance first");
        Voter storage v = voters[msg.sender];
        if (v.delegatedTo != address(0)) {
            _removeDelegation(v.delegatedTo);
            _delegate(delegateAddress);
            return true;
        }
        _reduceVotes(msg.sender, tokenBalances[msg.sender] + v.delegatedTokens);
        _delegate(delegateAddress);
        return true;
    }
}

