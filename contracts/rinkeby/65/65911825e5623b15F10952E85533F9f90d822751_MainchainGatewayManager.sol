pragma solidity ^0.8.0;

import "../contracts/Ownable.sol";

abstract contract ProxyStorage is Ownable {
    address public proxyFor;

    event ProxyUpdated(address indexed _oldContract, address indexed _newContract);

    function setProxyFor(address _newContract) public onlyOwner {
        address oldContract = proxyFor;
        proxyFor = _newContract;

        emit ProxyUpdated(oldContract, _newContract);
    }
}

