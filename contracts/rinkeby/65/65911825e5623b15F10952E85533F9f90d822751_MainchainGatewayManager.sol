pragma solidity ^0.8.0;

import "../contracts/ERC20Detailed.sol";
import "hardhat/console.sol";

abstract contract WETH is ERC20Detailed {
  event Deposit(
    address _sender,
    uint256 _value
  );

  event Withdrawal(
    address _sender,
    uint256 _value
  );

  constructor () ERC20Detailed("Wrapped Ether", "WETH", 18)
  {}

  function deposit()
    external
    payable
  {
    balanceOf[msg.sender] += msg.value;

    emit Deposit(msg.sender, msg.value);
  }

  function withdraw(uint256 _wad)
    external payable
  {
    require(balanceOf[msg.sender] >= _wad);
    balanceOf[msg.sender] -= _wad;
    payable(msg.sender).transfer(_wad);

    emit Withdrawal(msg.sender, _wad);
  }
}

