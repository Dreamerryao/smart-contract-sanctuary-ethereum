pragma solidity ^0.8.0;

import "../contracts/Ownable.sol";

import "hardhat/console.sol";

contract Registry is Ownable {
    event TokenMapped(
        address indexed _mainchainToken,
        address indexed _sidechainToken,
        uint32 _standard
    );

    struct TokenMapping {
        address mainchainToken;
        address sidechainToken;
        uint32 standard;
    }

    mapping(address => TokenMapping) public mainchainMap;
    mapping(address => TokenMapping) public sidechainMap;

    function mapToken(address _mainchainToken, address _sidechainToken, uint32 _standard) external onlyOwner {
        TokenMapping memory _map = TokenMapping(_mainchainToken, _sidechainToken, _standard);

        mainchainMap[_mainchainToken] = _map;
        sidechainMap[_sidechainToken] = _map;

        emit TokenMapped(_mainchainToken, _sidechainToken, _standard);
    }

    function isTokenMapped(address _token, uint32 _standard, bool _isMainchain)
    external
    view
    returns (bool)
    {
        TokenMapping memory _mapping;

        if (_isMainchain) {
            _mapping = mainchainMap[_token];
        } else {
            _mapping = sidechainMap[_token];
        }

        return _mapping.mainchainToken != address(0) &&
        _mapping.sidechainToken != address(0) &&
        _mapping.standard == _standard;
    }

    function clearMapToken(address _mainchainToken, address _sidechainToken) external onlyOwner
    {
        delete mainchainMap[_mainchainToken];
        delete sidechainMap[_sidechainToken];
    }

    function getMappedToken(address _token, bool _isMainchain) external view returns (address _mainchainToken, address _sidechainToken, uint32 _standard) {
        TokenMapping memory _mapping;

        if (_isMainchain) {
            _mapping = mainchainMap[_token];
        } else {
            _mapping = sidechainMap[_token];
        }

        _mainchainToken = _mapping.mainchainToken;
        _sidechainToken = _mapping.sidechainToken;
        _standard = _mapping.standard;
    }
}

