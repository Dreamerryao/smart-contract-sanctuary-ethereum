pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./interfaces/IShiniki.sol";
import "./interfaces/ISignatureVerifier.sol";
import "./ERC721C.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract Shiniki is
    Initializable,
    OwnableUpgradeable,
    IShiniki,
    ERC721C,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable,
    ERC2981Upgradeable
{
    struct SaleConfig {
        uint128 preSaleStartTime;
        uint128 preSaleDuration;
        uint128 publicSaleStartTime;
        uint128 publicSaleDuration;
        uint256 publicSalePrice;
    }

    ISignatureVerifier public SIGNATURE_VERIFIER;

    SaleConfig private saleConfig;
    uint256 public maxPerAddressDuringMint;

    //transfer nesting
    mapping(uint256 => bool) public nestingTransfer;

    //white list
    mapping(address => uint256) private whiteList;

    //number mint public
    mapping(address => uint256) private numberPublicMint;

    event Lock(address locker, uint256 tokenId, uint64 startTimestampLock);
    event UnLock(address unlocker, uint256 tokenId);

    function initialize(
        address signatureVerifier,
        uint256 maxPerAddressDuringMint_,
        uint256 collectionSize_,
        uint256 amountMintDev_,
        string memory defaultURI_
    ) external initializer {
        __ERC721_init("Shiniki", "SHINIKI", collectionSize_);
        __Ownable_init();
        SIGNATURE_VERIFIER = ISignatureVerifier(signatureVerifier);
        maxPerAddressDuringMint = maxPerAddressDuringMint_;
        currentIndex[TYPE_1] = 0;
        currentIndex[TYPE_2] = collectionSize_ / 2;
        startIndex[TYPE_1] = currentIndex[TYPE_1];
        startIndex[TYPE_2] = currentIndex[TYPE_2];
        revealed = false;
        defaultURI = defaultURI_;
        privateMint(amountMintDev_);
    }

    modifier callerIsUser() {
        require(
            tx.origin == msg.sender,
            "Shiniki: The caller is another contract"
        );
        _;
    }

    function preSaleMint(bytes32 typeMint, uint256 quantity)
        external
        nonReentrant
        callerIsUser
        whenNotPaused
    {
        require(isPreSaleOn(), "Shiniki: pre sale has not begun yet");
        require(
            (typeMint == TYPE_1 || typeMint == TYPE_2),
            "Shiniki: type input is invalid"
        );
        require(
            allowceInWhiteList(msg.sender) != 0,
            "Shiniki: caller is not in whiteList"
        );
        require(
            totalSupply() + quantity <= collectionSize,
            "Shiniki: reached max supply"
        );
        require(
            numberMinted(msg.sender) + quantity <=
                allowceInWhiteList(msg.sender),
            "Shiniki: can not mint greater than whiteList"
        );
        _safeMint(msg.sender, quantity, typeMint);
    }

    function publicSaleMint(bytes32 typeMint, uint256 quantity)
        external
        payable
        nonReentrant
        callerIsUser
        whenNotPaused
    {
        require(
            (typeMint == TYPE_1 || typeMint == TYPE_2),
            "Shiniki: type input is invalid"
        );
        require(isPublicSaleOn(), "Shiniki: public sale has not begun yet");
        require(
            totalSupply() + quantity <= collectionSize,
            "Shiniki: reached max supply"
        );
        require(
            numberPublicMint[msg.sender] + quantity <= maxPerAddressDuringMint,
            "Shiniki: can not mint this many"
        );
        numberPublicMint[msg.sender] += quantity;
        uint256 totalPrice = saleConfig.publicSalePrice * quantity;

        require(msg.value >= totalPrice, "Shiniki: insufficient balance");
        if (msg.value > totalPrice) {
            transferETH(msg.sender, msg.value - totalPrice);
        }

        _safeMint(msg.sender, quantity, typeMint);
    }

    function privateMint(uint256 quantity) public onlyOwner whenNotPaused {
        uint256 checkQuantity = quantity % 2;
        require(checkQuantity == 0, "Shiniki: quantity is even number");
        uint256 quantityType = quantity / 2;

        //mint with type 1
        _safeMint(msg.sender, quantityType, TYPE_1);

        //mint with type 2
        _safeMint(msg.sender, quantity - quantityType, TYPE_2);
    }

    // staking
    function lock(uint256[] memory tokenIds) public {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            TokenOwnership memory ownership = ownershipOf(tokenIds[i]);
            require(
                msg.sender == ownership.addr,
                "ERC721C: transfer caller is not owner nor approved"
            );
            _ownerships[tokenIds[i]].startTimestampLock = uint64(block.timestamp);
            nestingTransfer[tokenIds[i]] = true;

            emit Lock(msg.sender, tokenIds[i], ownership.startTimestampLock);
        }
    }

    // un-staking
    function unlock(uint256[] memory tokenIds) public {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            TokenOwnership memory ownership = ownershipOf(tokenIds[i]);
            require(
                msg.sender == ownership.addr,
                "ERC721C: transfer caller is not owner nor approved"
            );
            _ownerships[tokenIds[i]].startTimestampLock = 0;
            nestingTransfer[tokenIds[i]] = false;

            emit UnLock(msg.sender, tokenIds[i]);
        }
    }

    //claim reward
    function claim(
        address receiver,
        uint256 amount,
        uint256 nonce,
        bytes memory signature
    ) public {
        require(receiver != address(0), "Shiniki: receiver is not zero");
        require(
            SIGNATURE_VERIFIER.verifyClaim(receiver, amount, nonce, signature),
            "Shiniki: signature claim is invalid"
        );

        transferETH(receiver, amount);
    }

    function setBaseURI(bytes32[] memory _typeMint, string[] memory _baseURI)
        external
        onlyOwner
    {
        require(
            _typeMint.length == _baseURI.length,
            "Shiniki: input data is invalid"
        );
        for (uint256 i = 0; i < _typeMint.length; i++) {
            require(
                (_typeMint[i] == TYPE_1 || _typeMint[i] == TYPE_2),
                "Shiniki: type input is invalid"
            );
            _baseTokenURI[_typeMint[i]] = _baseURI[i];
        }
    }

    function setDefaultURI(string memory _defaultURI) external onlyOwner {
        defaultURI = _defaultURI;
    }

    function isPreSaleOn() public view returns (bool) {
        return
            block.timestamp >= saleConfig.preSaleStartTime &&
            block.timestamp <=
            saleConfig.preSaleStartTime + saleConfig.preSaleDuration;
    }

    function isPublicSaleOn() public view returns (bool) {
        return
            saleConfig.publicSalePrice != 0 &&
            block.timestamp >= saleConfig.publicSaleStartTime &&
            block.timestamp <=
            saleConfig.publicSaleStartTime + saleConfig.publicSaleDuration;
    }

    function allowceInWhiteList(address user) public view returns (uint256) {
        return whiteList[user];
    }

    function setPreSaleConfig(
        uint128 _preSaleStartTime,
        uint128 _preSaleDuration
    ) external onlyOwner {
        saleConfig.preSaleStartTime = _preSaleStartTime;
        saleConfig.preSaleDuration = _preSaleDuration;
    }

    function setPublicSaleConfig(
        uint128 _publicSaleStartTime,
        uint128 _publicSaleDuration,
        uint256 _publicSalePrice
    ) external onlyOwner {
        saleConfig.publicSaleStartTime = _publicSaleStartTime;
        saleConfig.publicSaleDuration = _publicSaleDuration;
        saleConfig.publicSalePrice = _publicSalePrice;
    }

    function addWhiteList(address[] memory addresses, uint256[] memory values)
        external
        onlyOwner
    {
        require(addresses.length == values.length, "Shiniki: input is invalid");
        for (uint256 i = 0; i < addresses.length; i++) {
            whiteList[addresses[i]] = values[i];
        }
    }

    function removeWhiteList(address[] memory addresses) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            whiteList[addresses[i]] = 0;
        }
    }

    function setMaxPerAddressDuringMint(uint256 _maxPerAddressDuringMint)
        external
        onlyOwner
    {
        maxPerAddressDuringMint = _maxPerAddressDuringMint;
    }

    // revealed
    function setRevealed(bool _revealed) external onlyOwner {
        revealed = _revealed;
    }

    function getConfig() public view returns (SaleConfig memory) {
        return saleConfig;
    }

    receive() external payable {}

    function transferETH(address receiver, uint256 amount) private {
        (bool success, ) = receiver.call{value: amount}("");
        require(success, "Shiniki: transfer failed.");
    }

    function withdraw(uint256 amount) external onlyOwner nonReentrant {
        transferETH(msg.sender, amount);
    }

    function withdrawAll() external onlyOwner nonReentrant {
        transferETH(msg.sender, address(this).balance);
    }

    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }

    function getOwnershipData(uint256 tokenId)
        external
        view
        returns (TokenOwnership memory)
    {
        return ownershipOf(tokenId);
    }

    /**
    @notice Transfer a token between addresses while the Shiniki is minting,
    thus not resetting the nesting period.
     */
    function safeTransferWhileNesting(
        address from,
        address to,
        uint256 tokenId
    ) external override {
        require(ownerOf(tokenId) == _msgSender(), "Shiniki: Only owner");
        if (nestingTransfer[tokenId]) {
            nestingTransfer[tokenId] = false;
            safeTransferFrom(from, to, tokenId);
            nestingTransfer[tokenId] = true;
        } else {
            safeTransferFrom(from, to, tokenId);
        }
    }

    /**
    @dev Block transfers while nesting.
     */
    function _beforeTokenTransfers(
        address from,
        address,
        uint256 tokenId,
        uint256
    ) internal view override {
        if (from != address(0)) {
            require(revealed, "Shiniki: not revealed");
            require(!nestingTransfer[tokenId], "Shiniki: nesting");
        }
    }

    /**
    @notice Sets the contract-wide royalty info.
     */
    function setRoyaltyInfo(address receiver, uint96 feeBasisPoints)
        external
        onlyOwner
    {
        _setDefaultRoyalty(receiver, feeBasisPoints);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721C, ERC2981Upgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

