    function burn(address to) external returns (uint256 amount0, uint256 amount1);

    function swap(
        uint256 amount0Out,
        uint256 amount1Out,
        address to,
        bytes calldata data
    ) external;

    function skim(address to) external;

    function sync() external;

    function initialize(address, address) external;
}

contract UniswapV2Helper {
    using SafeMath for uint256;

    function getPairsAndAllReserves(
        IUniswapV2Factory _contractFactory,
        uint256 _offset,
        uint256 _nbElements
    )
        external
        view
        returns (
            IUniswapV2Pair[] memory pairs,
            address[] memory token0,
            address[] memory token1,
            uint112[] memory reserves0,
            uint112[] memory reserves1
        )
    //uint112[] memory reserves0LastFictive,
    //uint112[] memory reserves1LastFictive
    {
        uint256 nbPairTotal = _contractFactory.allPairsLength();
        if (_offset >= nbPairTotal) {
            // _offset too high, return empty array
            return (
                pairs,
                token0,
                token1,
                reserves0,
                reserves1 /*, reserves0LastFictive, reserves1LastFictive*/
            );
        }

        if (_offset.add(_nbElements) > nbPairTotal) {
            // _nbElements too large: _nbElements is adjusted
            _nbElements = nbPairTotal.sub(_offset);
        }

        // declare return arrays
        pairs = new IUniswapV2Pair[](_nbElements);
        token0 = new address[](_nbElements);
        token1 = new address[](_nbElements);
        reserves0 = new uint112[](_nbElements);
        reserves1 = new uint112[](_nbElements);
        //reserves0LastFictive = new uint112[](_nbElements);
        //reserves1LastFictive = new uint112[](_nbElements);

        // fetch data and assign values
        for (uint256 i = 0; i < _nbElements; i++) {
            pairs[i] = IUniswapV2Pair(_contractFactory.allPairs(i + _offset));
            token0[i] = pairs[i].token0();
            token1[i] = pairs[i].token1();
            (reserves0[i], reserves1[i], ) = pairs[i].getReserves();
            // (reserves0LastFictive[i], reserves1LastFictive[i]) = pairs[i].getReservesLastFictive();
        }
    }
}