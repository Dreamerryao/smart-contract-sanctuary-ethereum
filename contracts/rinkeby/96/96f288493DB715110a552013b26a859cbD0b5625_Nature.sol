pragma solidity 0.8.15;

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "./interfaces/IMetarelics.sol";
import "./library/MintAllowance.sol";


/// @title Dummy nature NFT smart contract
/// @notice This contract is used for the purchase of the nature tokens at different prices
/// @dev All purchase function would be much better from the nature dApp
contract Nature is ERC721A, ERC2981, Pausable, Ownable, ReentrancyGuard {
  using Strings for string;
  using SafeCast for uint256;
  using MintAllowanceLib for uint256[];

  string public baseURI = "";

  uint256 public ghostProtocolStartTime;
  uint256 public ghostProtocolEndTime;

  uint256 public closedMintStartTime;
  uint256 public closedMintEndTime;

  bytes32 public discountedSneaksListMerkleRoot;
  bytes32 public sneaksListMerkleRoot;
  bytes32 public guaranteedListMerkleRoot;
  bytes32 public publicListMerkleRoot;

  struct PublicMintConfig { 
    uint16  maxTokensPerUser;
    bool    isWaitList;
  } 

  PublicMintConfig public publicMintConfig;

  mapping(uint16 => uint8) public relicPassTokenUsage;
  mapping(address => bool) public isDiscounderSneaksListUser;
  mapping(address => bool) public isSneaksListUsed;
  mapping(address => bool) public isGuaranteedAllowlistUsed;


  address immutable public relicPassToken;
  uint16 public constant MAX_TOKEN_SUPPLY = 3333;
  

  event TokenMint(address indexed receiver, string indexed mintType, uint256 tokenAmount); 

  constructor(string memory name, string memory symbol, address _relicPassToken, address royaltyAddress)
    ERC721A(name, symbol)
  {
    require(_relicPassToken != address(0), "Not a valid token address");

    relicPassToken = _relicPassToken;
    _setDefaultRoyalty(royaltyAddress, 1000);

  }

  /**     
  * @notice Relic Pass holders can use this method to purchase token form the contract. 
  * For each relic pass token a user have, the user get 1 free Nature token and 2 extra Nature token, 
  * 1 at RELIC_PASS_REDUCED_PRICE and 1 at RELIC_PASS_FULL_PRICE
  * @dev Mint a new token and update the used state of each relic pass token
  * @param receiver The holder of the relic pass token and the receiver of the newly minted nature token
  * @param tokenAmount The amount of paid nature token the sender wants to purchase. This free token is not included in the variable
  */ 
  function relicPassMint(address receiver, uint256 tokenAmount) external payable whenNotPaused nonReentrant {
    require(receiver != address(0), "Receiver not a valid token address");
    require(tokenAmount != 0, "Token amount can not be zero");

    require(block.timestamp > closedMintStartTime && block.timestamp < closedMintEndTime, "Mint period is either closed or not open");

    // Get an array of all the relic pass token the receiver have
    uint16[] memory relicPassIds = IMetarelics(relicPassToken).walletOfOwner(receiver);
    // We revert the transaction if receiver have zero relic pass token
    require(relicPassIds.length != 0, "No relic token found");

    uint8[] memory relicPassesUsage = getRelicPassIdUsage(relicPassIds);

    MintAllowanceLib.MintAllowance[] memory mints = MintAllowanceLib.mintAllowance(relicPassIds, false, false, false, relicPassesUsage, false, false, false);

    // Get the total ethers that will be spent by sender, this is calculated based on the tokenAmount.
    // We also get the allowed number of tokens a receiver is eligle to purchase, 
    // this might be lower than the  tokenAmount provided by users 
    // during purchase if it is greater than the current allowance of the receiver.
    (uint16 eligibleMintAmount, uint256 totalEtherUsed) = setRelicPassState(mints, tokenAmount);

    require(tokenAmount <= eligibleMintAmount, "Total amount can not be greater than eligble mint");

    // We revert this transaction if this mint is going to go beyond our max token supply
    require((_totalMinted() + tokenAmount) <= MAX_TOKEN_SUPPLY, "The amount of token you want to purchase plus the total token minted exceed the max supply");

    require(msg.value == totalEtherUsed, "Ether sent is less than total ether used");
   
    _safeMint(receiver, tokenAmount);

    emit TokenMint(receiver, "RELIC_PASS_MINT", tokenAmount);


  }


 /**     
  * @notice Waitlisted public users can use this method to purchase Nature token at SNEAKS_LIST_PRICE. 
  * @param proof An array of merkle proof of the sender, which is can be an array of zero bytes when waitlisting is off
  */ 
  function sneaksListMint(bytes32[] calldata proof) external payable whenNotPaused nonReentrant {
    require(!isSneaksListUsed[_msgSender()], "Allowance have been used");
    require(msg.value == MintAllowanceLib.SNEAKS_LIST_PRICE, "Price must be equal to the sneak list price");
    require(block.timestamp > closedMintStartTime && block.timestamp < closedMintEndTime, "Mint period is either closed or not open");

    require(_verify(_leaf(_msgSender()), proof, sneaksListMerkleRoot), "Invalid merkle proof");


    // We revert this transaction if this mint is going to go beyond our max token supply
    require((_totalMinted() + 1) <= MAX_TOKEN_SUPPLY, "The amount of token you want to purchase plus the total token minted exceed the max supply");
    isSneaksListUsed[_msgSender()] = true;
    _safeMint(_msgSender(), 1);

    emit TokenMint(_msgSender(), "SNEAKS_LIST", 1);
  }

  /**     
  * @notice Waitlisted public users can use this method to purchase Nature token at DISCOUNTED_SNEAKS_LIST_PRICE. 
  * @param proof An array of merkle proof of the sender
  */ 
  function discountedSneaksListMint(bytes32[] calldata proof) external payable whenNotPaused nonReentrant {
    require(!isDiscounderSneaksListUser[_msgSender()], "Allowance have been used");
    require(msg.value == MintAllowanceLib.DISCOUNTED_SNEAKS_LIST_PRICE, "Price must be equal to the sneak list price");
    require(block.timestamp > closedMintStartTime && block.timestamp < closedMintEndTime, "Mint period is either closed or not open");

    require(_verify(_leaf(_msgSender()), proof, discountedSneaksListMerkleRoot), "Invalid merkle proof");
    
    // We revert this transaction if this mint is going to go beyond our max token supply
    require((_totalMinted() + 1) <= MAX_TOKEN_SUPPLY, "The amount of token you want to purchase plus the total token minted exceed the max supply");
    isDiscounderSneaksListUser[_msgSender()] = true;
    _safeMint(_msgSender(), 1);

    emit TokenMint(_msgSender(), "DISCOUNTED_SNEAKS_LIST", 1);

  }

  /**     
  * @notice Waitlisted public users can use this method to purchase Nature token at GUARANTEED_ALLOWLIST_PRICE. 
  * @param proof An array of merkle proof of the sender
  */ 
  function guaranteedListMint(bytes32[] calldata proof) external payable whenNotPaused nonReentrant {
    require(!isGuaranteedAllowlistUsed[_msgSender()], "Allowance have been used");
    require(msg.value == MintAllowanceLib.GUARANTEED_ALLOWLIST_PRICE, "Price must be equal to the sneak list price");
    require(block.timestamp > closedMintStartTime && block.timestamp < closedMintEndTime, "Mint period is either closed or not open");

    require(_verify(_leaf(_msgSender()), proof, guaranteedListMerkleRoot), "Invalid merkle proof");

    // We revert this transaction if this mint is going to go beyond our max token supply
    require((_totalMinted() + 1) <= MAX_TOKEN_SUPPLY, "The amount of token you want to purchase plus the total token minted exceed the max supply");
    isGuaranteedAllowlistUsed[_msgSender()] = true;
    _safeMint(_msgSender(), 1);

    emit TokenMint(_msgSender(), "GUARANTEED_ALLOWLIST", 1);

  }

  /**     
  * @notice Waitlisted public users can use this method to purchase Nature token at RELIC_PASS_FULL_PRICE. 
  * The waitlisting feature of this method can be turned on or off by an Admin to allowed non waitlisted users
  * @dev To mint token when the waitlisting is enabled, you will have to calculate the merkle proof on the dApp 
  * or pass an array of zero bytes if the waitlisting is disabled
  * @param proof An array of merkle proof of the sender, which is can be an array of zero bytes when waitlisting is off
  * @param tokenAmount Number of token to mint
  */ 
  function publicMint(bytes32[] calldata proof, uint16 tokenAmount) external payable whenNotPaused nonReentrant {

    require(block.timestamp > ghostProtocolEndTime, "Mint period is not open");

    if (publicMintConfig.isWaitList) {
      require(_verify(_leaf(_msgSender()), proof, publicListMerkleRoot), "Invalid merkle proof");
    }

    uint16 newBalance = SafeCast.toUint16(balanceOf(_msgSender()) + tokenAmount);

    require(newBalance < publicMintConfig.maxTokensPerUser, "User max mint have been reached");

    // We revert this transaction if this mint is going to go beyond our max token supply
    require((_totalMinted() + tokenAmount) <= MAX_TOKEN_SUPPLY, "The amount of token you want to purchase plus the total token minted exceed the max supply");

    uint256 totalEtherUsed = tokenAmount * MintAllowanceLib.RELIC_PASS_FULL_PRICE;

    require(msg.value == totalEtherUsed, "Ether sent is less than total ether used");

    _safeMint(_msgSender(), tokenAmount);

    emit TokenMint(_msgSender(), "PUBLIC_MINT", tokenAmount);

  }

  //////////////////////////SETTERS/////////////////////////////////////////////////////////////////////////////////////////////////////

  /**     
  * @notice Set and Update a relic pass id usage state. 
  * @param mints An array of mint allowance.
  * @param tokenAmount The total token amount the receiver wants to purchase
  * @return eligibleMintAmount a user can mint.
  * @return totalEtherUsed to be collected from the users wallet
  */ 
  function setRelicPassState (MintAllowanceLib.MintAllowance[] memory mints, uint256 tokenAmount) internal returns (uint16 eligibleMintAmount, uint256 totalEtherUsed) {
    for (uint16 i = 0; i < mints.length; i++) {
      MintAllowanceLib.MintAllowance memory mint =  mints[i];
      if(!mint.isUsed) {
        relicPassTokenUsage[mint.relicPassId] = relicPassTokenUsage[mint.relicPassId]++;
        eligibleMintAmount++;
      }
    }
    // Calclutate and return the token at public price. 
    if (eligibleMintAmount != tokenAmount) {
      for (uint16 i = 0; i < mints.length; i++) {
        MintAllowanceLib.MintAllowance memory mint =  mints[i];
        if (mint.isUsed) {
          if ((mint.isPublicMintOnly && block.timestamp >= ghostProtocolEndTime) || !mint.isPublicMintOnly) {
            relicPassTokenUsage[mint.relicPassId] = relicPassTokenUsage[mint.relicPassId]++;
            eligibleMintAmount++;
            totalEtherUsed += mint.price;
          }
        }
        if (eligibleMintAmount == tokenAmount) {
          break;
        }
      }
    }
  }


  /**     
  * @notice Set the royalty receiver and fee
  * @dev The fee should be in base point, so 10% should 10 * 100 which is 1000
  * @param receiver The receiver of the fee.
  * @param fee The fee sent to receiver.
  */ 
  function setDefaultRoyalty(address receiver, uint96 fee) external whenNotPaused onlyOwner {
    _setDefaultRoyalty(receiver, fee);
  } 

  /**     
  * @notice Update the merkle root of the public waitlist
  * @param newMerkleRoot The new merkle root.
  */ 
  function setPublicListMerkleRoot(bytes32 newMerkleRoot) external whenNotPaused onlyOwner {
    publicListMerkleRoot = newMerkleRoot;
  } 

  /**     
  * @notice Update the merkle root of the sneaks list
  * @param newMerkleRoot The new merkle root.
  */ 
  function setSneaksListMerkleRoot(bytes32 newMerkleRoot) external whenNotPaused onlyOwner {
    sneaksListMerkleRoot = newMerkleRoot;
  } 

  /**     
  * @notice Update the merkle root of the discounted sneaks list
  * @param newMerkleRoot The new merkle root.
  */ 
  function setDiscountedSneaksListMerkleRoot(bytes32 newMerkleRoot) external whenNotPaused onlyOwner {
    discountedSneaksListMerkleRoot = newMerkleRoot;
  } 

  /**     
  * @notice Update the merkle root of the guaranteed sneaks list
  * @param newMerkleRoot The new merkle root.
  */ 
  function setGuaranteedListMerkleRoot(bytes32 newMerkleRoot) external whenNotPaused onlyOwner {
    guaranteedListMerkleRoot = newMerkleRoot;
  } 

  /**     
  * @notice Switch on or of the waitlisting feature for public mint
  * @param value The value of the waitlist.
  */ 
  function setWaitlistRequired(bool value) external whenNotPaused onlyOwner {
    publicMintConfig.isWaitList = value;
  }

  /**     
  * @notice Set the ghost protocol period. This is for allowing listing on exchanges
  * @param startTime The start time of the ghost protocol.
  * @param endTime The end time of the ghost protocol.
  */ 
  function setGhostProtocolPeriod(uint256 startTime, uint256 endTime) external whenNotPaused onlyOwner {
    require(startTime < endTime, "Start time must be less than end time");
    
    ghostProtocolStartTime = startTime;
    ghostProtocolEndTime = endTime;
  } 

  /**     
  * @notice Set closed mint start and end period
  * @param startTime The start time of the closed mint.
  * @param endTime The end time of the closed mint.
  */ 
  function setClosedMintPeriod(uint256 startTime, uint256 endTime) external whenNotPaused onlyOwner {
    require(startTime < endTime, "Start time must be less than end time");
    
    closedMintStartTime = startTime;
    closedMintEndTime = endTime;
  } 

  /**     
  * @notice Update the max mint token per user on the public mint
  * @param newMaxTokensPerUser The new max mint amount for an address.
  */ 
  function setMaxTokensPerUser(uint16 newMaxTokensPerUser) external whenNotPaused onlyOwner {
    publicMintConfig.maxTokensPerUser = newMaxTokensPerUser;
  }

  event WithdrawFunds(address indexed receiver, uint256 amount); 
  /**     
  * @notice Withdraw ethers from the contract, this can only be called by an admin
  * @param receiver The receiver of the ethers in the contract.
  */ 
  function withdrawFunds(address payable receiver) external onlyOwner nonReentrant {
    require(receiver != address(0), "Not a valid address");
    require(address(this).balance > 0, "Contract have zero balance");

    (bool sent, ) = receiver.call{value: address(this).balance}("");
    require(sent, "Failed to send ether");
    emit WithdrawFunds(receiver, address(this).balance);
  }  

  /**
  * @notice Change the base URI for the metadata
  * @param uri new base URI
  */
  function setBaseURI(string memory uri) external onlyOwner {
    baseURI = uri;
  }



  //////////////////////////GETTERS////////////////////////////////////////////////////////////////////////////////////////////////////

  /**
  * @notice Get total tokens minted
  */
  function mintedTokens() external view returns(uint256) {
    return _totalMinted();
  }

  /**
  * @notice Get mint allowance
  */
  function getMintAllowance(
    uint16[] calldata relicPassIds,
    bool isDiscountedSneaksList,
    bool isSneaksList,
    bool isGuaranteedAllowlist,
    uint8[] calldata relicPassesUsage,
    bool isDiscountedSneaksListUsed,
    bool isSneaksUsed,
    bool isGuaranteedlistUsed
  ) public pure returns (MintAllowanceLib.MintAllowance[] memory mints) {
    mints = MintAllowanceLib.mintAllowance(
      relicPassIds, 
      isDiscountedSneaksList, 
      isSneaksList, 
      isGuaranteedAllowlist, 
      relicPassesUsage,
      isDiscountedSneaksListUsed,
      isSneaksUsed,
      isGuaranteedlistUsed
    );
  }

  /**     
  * @notice Check if user have allowance for discounted sneaks
  */ 
  function getDiscountedSneaksAllowance(address user) external view returns (bool){
    return isDiscounderSneaksListUser[user];
  }

  /**     
  * @notice Check if user have allowance for guaranteed sneaks
  */ 
  function getGuaranteedSneaksAllowance(address user) external view returns (bool){
    return isGuaranteedAllowlistUsed[user];
  }

  /**     
  * @notice Check if user have allowance for sneaks list
  */ 
  function getSneaksListAllowance(address user) external view returns (bool){
    return isSneaksListUsed[user];
  }

  /**     
  * @notice Return the usage for a relic pass token
  */ 
  function getRelicPassIdUsage(uint16[] memory relicPassIds) internal view returns (uint8[] memory) {
    uint8[] memory relicPassesUsage = new uint8[](relicPassIds.length);
    for (uint16 i = 0; i < relicPassIds.length; i++) {
        uint16 passId = relicPassIds[i];
        relicPassesUsage[i] = relicPassTokenUsage[passId];
    }
    return relicPassesUsage;
  }

  /**     
  * @notice Return the value of the public waitlist
  */ 
  function isWaitlistRequired() external view returns (bool){
    return publicMintConfig.isWaitList;
  }

  /**     
  * @notice Check if contract is in a ghost protocol period
  */ 
  function isGhostProtocol() external view returns (bool){
    if (block.timestamp >= ghostProtocolStartTime && block.timestamp <= ghostProtocolEndTime) return true;
    return false;
  }

  /**     
  * @notice Check if contract is in a closed mint period
  */ 
  function isClosedMint() external view returns (bool){
    if (block.timestamp >= closedMintStartTime && block.timestamp <= closedMintEndTime) return true;
    return false;
  }

  /**     
  * @notice Get the max token per user on public mint
  */ 
  function getMaxTokensPerUser() external view returns (uint16){
    return publicMintConfig.maxTokensPerUser;
  }

  /**
  * @notice Get the 6 last letters from `addr`.   
  * @param addr The where the 6 letters are taken
  */
  function generateCode(address addr) internal pure returns(string memory) {
    return substring(addr, 36, 42);       
  }

  /**
  * @dev Get the substring of `addr`.   
  */
  function substring(address addr, uint startIndex, uint endIndex) public pure returns (string memory)  {
    string memory addrToStr = Strings.toHexString(uint256(uint160(addr)), 20);
    bytes memory strBytes = bytes(addrToStr);
    bytes memory result = new bytes(endIndex-startIndex);
    for(uint i = startIndex; i < endIndex; i++) {
      result[i-startIndex] = strBytes[i];
    }
    return string(result);
  }



  //////////////////////////OVERRIDES////////////////////////////////////////////////////////////////////////////////////////////////////

  /**
  * @notice Block the transfer of the token to another wallet when listing is turned off
  */
  function _beforeTokenTransfers(address from, address to, uint256 startTokenId, uint256 quantity) internal whenNotPaused override virtual {
    // Revert when listing is off
    if (from != address(0) && (block.timestamp >= ghostProtocolStartTime && block.timestamp <= ghostProtocolEndTime)) {
      revert(string.concat("HACKER: Check code ", generateCode(from)));
    }
    super._beforeTokenTransfers(from, to, startTokenId, quantity);
  }

  /**
  * @notice Block the listing of the token on exchanges when listing is turned off
  */
  function approve(address to, uint256 tokenId) public whenNotPaused virtual override {
    // Revert when listing is off
    if (block.timestamp >= ghostProtocolStartTime && block.timestamp <= ghostProtocolEndTime) {
      revert(string.concat("HACKER: Check code ", generateCode(_msgSender())));
    }
    super.approve(to, tokenId);
  }

  /**
  * @notice Block the listing of the token on exchanges when listing is turned off
  */
  function setApprovalForAll(address operator, bool approved) public whenNotPaused override {
    // Revert when listing is off
    if (block.timestamp >= ghostProtocolStartTime && block.timestamp <= ghostProtocolEndTime) {
      revert(string.concat("HACKER: Check code ", generateCode(_msgSender())));
    }
    super.setApprovalForAll(operator, approved);
  }

  /**
  * @dev Pause the contract
  */
  function pause() public onlyOwner {
    _pause();
  }

  /**
  * @dev Unpause the contract
  */
  function unpause() public onlyOwner {
    _unpause();
  }

  /**
  * @dev Get the base URI
  */
  function _baseURI() internal view virtual override returns (string memory) {
    return baseURI;
  }

  /**
  * @dev Return `account` hash in byte32
  */
  function _leaf(address account) internal pure returns (bytes32) {
    return keccak256(abi.encodePacked(account));
  }

  /**
  * @dev Verify leaf with the proof provided
  */
  function _verify(bytes32 leaf, bytes32[] memory proof, bytes32 root) internal pure returns (bool) {
    return MerkleProof.verify(proof, root, leaf);
  }

  /**
  * @dev Override support interface for ERC721A and ERC2981
  */
  function supportsInterface(bytes4 interfaceId) public view virtual override (ERC721A, ERC2981) returns (bool) {
    return super.supportsInterface(interfaceId);
  }


}

