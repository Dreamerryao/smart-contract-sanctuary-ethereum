pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "./Flipper.sol";

contract MockERC20 is ERC20 {
    constructor(string memory _name, string memory _symbol) ERC20(_name, _symbol) {}
    function mint(address _to, uint256 _amount) public {
        _mint(_to, _amount);
    }

    function burn(address _to, uint256 _amount) public {
        _burn(_to, _amount);
    }
}

contract OpenseaSharedStorefront is ERC1155 {
    uint256 public constant ALPHA_ACCESS_LIFETIME_MEMBERSHIP_TOKEN_ID = 0;
    uint256 public constant ALPHA_OG_ACCESS_LIFETIME_MEMBERSHIP_TOKEN_ID = 1;

    constructor() ERC1155("") {}

    function mintAlphaAccessLifetimeMembership(address _to) public {
        _mint(_to, ALPHA_ACCESS_LIFETIME_MEMBERSHIP_TOKEN_ID, 1, "");
    }

    function mintAlphaOGAccessLifetimeMembership(address _to) public {
        _mint(_to, ALPHA_OG_ACCESS_LIFETIME_MEMBERSHIP_TOKEN_ID, 1, "");
    }
}

contract MockOpenseaProxyRegistry is OpenseaProxyRegistry {
    constructor(address _proxyOfOwner) {
        proxies[msg.sender] = OpenseaOwnableDelegateProxy(_proxyOfOwner);
    }
}

contract PremintedFlipper is Flipper {
    constructor(
        string memory _tokenURI,
        string memory _contractURI,
        address _bankAddress,
        address _bankUniswapV2PairAddress,
        address _bankBalancerPoolAddress,
        address _openseaSharedStorefrontAddress,
        address _openseaProxyRegistryAddress,
        address _royaltyFeeRecipient,
        uint256[] memory _alphaAccessMembershipTokenIds,
        address[] memory _whitelistedAddresses,
        uint256 _premintAmount
    )
        Flipper(
            _tokenURI,
            _contractURI,
            _bankAddress,
            _bankUniswapV2PairAddress,
            _bankBalancerPoolAddress,
            _openseaSharedStorefrontAddress,
            _openseaProxyRegistryAddress,
            _royaltyFeeRecipient,
            _alphaAccessMembershipTokenIds,
            _whitelistedAddresses
        )
    {
        _mintCounter += _premintAmount;
        _mint(msg.sender, ACCESS_PASS_TOKEN_ID, _premintAmount, "");
    }
}

