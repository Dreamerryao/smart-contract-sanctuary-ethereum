pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./IERC2981.sol";

contract OpenseaOwnableDelegateProxy {}

contract OpenseaProxyRegistry {
    mapping(address => OpenseaOwnableDelegateProxy) public proxies;
}

/**
 * @title Flipper
 * Flipper - 2022 Access Pass
 */
contract Flipper is ERC1155, IERC2981, Ownable {
    string public name = "Flipper";
    string public symbol = "FLIP";
    uint256 public constant ACCESS_PASS_TOKEN_ID = 0;
    uint256[] private alphaAccessMembershipTokenIds;
    uint256 internal _mintCounter;
    IERC20 private Bank;
    IERC20 private BankUniswapV2Pair;
    IERC20 private BankBalancerPool;
    IERC1155 private OpenseaSharedStorefront;
    OpenseaProxyRegistry private openseaProxyRegistry;
    bool public public_sale_active = false;
    bool public bankless_presale_active = false;
    bool public general_presale_active = false;
    mapping(address => bool) private _whitelisted;
    mapping(address => bool) private _whitelistedProxy;
    string public contractURI;
    address private royaltyFeeRecipient = 0x6a024f521f83906671e1a23a8B6c560be7e980F4;

    constructor(
        string memory _tokenURI,
        string memory _contractURI,
        address _bankAddress,
        address _bankUniswapV2PairAddress,
        address _bankBalancerPoolAddress,
        address _openseaSharedStorefrontAddress,
        address _openseaProxyRegistryAddress,
        address _royaltyFeeRecipient,
        uint256[] memory _alphaAccessMembershipTokenIds,
        address[] memory _whitelistedAddresses
    ) ERC1155(_tokenURI) {
        contractURI = _contractURI;
        Bank = IERC20(_bankAddress);
        BankUniswapV2Pair = IERC20(_bankUniswapV2PairAddress);
        BankBalancerPool = IERC20(_bankBalancerPoolAddress);
        OpenseaSharedStorefront = IERC1155(_openseaSharedStorefrontAddress);
        alphaAccessMembershipTokenIds = _alphaAccessMembershipTokenIds;
        openseaProxyRegistry = OpenseaProxyRegistry(
            _openseaProxyRegistryAddress
        );
        royaltyFeeRecipient = _royaltyFeeRecipient;
        addWhitelist(_whitelistedAddresses);
    }

    function setTokenURI(string memory _tokenURI) public onlyOwner {
        _setURI(_tokenURI);
    }

    function setContractURI(string memory _contractURI) public onlyOwner {
        contractURI = _contractURI;
    }

    function mint() public payable {
        require(canMint(), "Sale is not active or wallet is not whitelisted");
        require(_mintCounter < 500, "All 500 passes were already minted");
        if (
            bankless_presale_active &&
            !public_sale_active &&
            !general_presale_active
        ) {
            require(
                _mintCounter < 400,
                "All 400 Bankless presale passes were already minted"
            );
        }
        require(
            msg.value == currentPrice(),
            "Value does not match current price"
        );
        _mintCounter += 1;
        _mint(msg.sender, ACCESS_PASS_TOKEN_ID, 1, "");
    }

    function totalSupply() public view returns (uint256) {
        return _mintCounter;
    }

    function canMint() public view returns (bool) {
        if (public_sale_active) {
            return true;
        }

        if (bankless_presale_active && isWhitelistedByBankBalance(msg.sender)) {
            return true;
        }

        if (
            general_presale_active &&
            (isWhitelistedByAlphaAccessMembership(msg.sender) ||
                isExplicitlyWhitelisted(msg.sender))
        ) {
            return true;
        }

        return false;
    }

    function isExplicitlyWhitelisted(address _toCheck)
        public
        view
        returns (bool)
    {
        return _whitelisted[_toCheck] == true;
    }

    function isWhitelistedByBankBalance(address _toCheck)
        public
        view
        returns (bool)
    {
        if (Bank.balanceOf(_toCheck) >= 35_000e18){
            return true;
        }

        if (BankUniswapV2Pair.balanceOf(_toCheck) >= 250e18){
            return true;
        }

        if (BankBalancerPool.balanceOf(_toCheck) >= 4_200e18){
            return true;
        }

        return false;
    }

    function isWhitelistedByAlphaAccessMembership(address _toCheck)
        public
        view
        returns (bool)
    {
        for (uint i = 0; i < alphaAccessMembershipTokenIds.length; i++) {
            if (
                OpenseaSharedStorefront.balanceOf(
                    _toCheck,
                    alphaAccessMembershipTokenIds[i]
                ) > 0
            ) {
                return true;
            }
        }

        return false;
    }

    function addWhitelist(address[] memory _toAdd) public onlyOwner {
        for (uint i = 0; i < _toAdd.length; i++) {
            _whitelisted[_toAdd[i]] = true;
        }
    }

    function removeWhitelist(address[] memory _toRemove) public onlyOwner {
        for(uint i = 0; i < _toRemove.length; i++) {
            delete _whitelisted[_toRemove[i]];
        }
    }

    function currentPrice() public view returns (uint256) {
        return 0.1 ether * (1 + (_mintCounter / 100));
    }

    function activatePublicSale() public onlyOwner {
        public_sale_active = true;
    }

    function deactivatePublicSale() public onlyOwner {
        public_sale_active = false;
    }

    function activateBanklessPresale() public onlyOwner {
        bankless_presale_active = true;
    }

    function deactivateBanklessPresale() public onlyOwner {
        bankless_presale_active = false;
    }

    function activateGeneralPresale() public onlyOwner {
        general_presale_active = true;
    }

    function deactivateGeneralPresale() public onlyOwner {
        general_presale_active = false;
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "Balance is 0");
        payable(msg.sender).transfer(balance);
    }

    // Implements ERC-2981
    function royaltyInfo(
        uint256,
        uint256 _salePrice
    ) external view returns (
        address receiver,
        uint256 royaltyAmount
    ) {
        return (royaltyFeeRecipient, _salePrice * 10 / 100);
    }

    // Register support for ERC-2981
    function supportsInterface(bytes4 interfaceId) public view virtual override (ERC1155, IERC165) returns (bool) {
       return interfaceId == type(IERC2981).interfaceId || super.supportsInterface(interfaceId);
    }

    // Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-free listings.
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        if (address(openseaProxyRegistry.proxies(_owner)) == _operator) {
            return true;
        }

        return ERC1155.isApprovedForAll(_owner, _operator);
    }
}

