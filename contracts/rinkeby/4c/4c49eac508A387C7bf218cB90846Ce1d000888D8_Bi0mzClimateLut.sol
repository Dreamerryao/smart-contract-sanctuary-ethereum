        hex""
    ];
    
    int256 constant TEMPERATURE_RESOLUTION = 45;
    int256 constant PRECIPITATION_RESOLUTION = 450;
    int256 constant TEMPERATURE_MIN = -15;

    function getBi0m(int256 temperatureC, int256 precipitationCm) public view returns (string memory) {
        int256 temperatureIndex = temperatureC - TEMPERATURE_MIN;
        int256 precipitationIndex = precipitationCm;
        uint256 clampedTemperatureIndex = temperatureIndex < 0 ? uint256(0):
            (temperatureIndex > TEMPERATURE_RESOLUTION - 1 ? uint256(TEMPERATURE_RESOLUTION- 1) : uint256(temperatureIndex));
        uint256 clampedPrecipitationIndex = precipitationIndex < 0 ? uint256(0):
            (precipitationIndex > PRECIPITATION_RESOLUTION - 1 ? uint256(PRECIPITATION_RESOLUTION - 1) : uint256(precipitationIndex));
        bytes1 rawTuple = climateLut[clampedTemperatureIndex][clampedPrecipitationIndex/2];
        if(clampedPrecipitationIndex%2 == 0) {
            rawTuple = (rawTuple >> 4);
        }
        uint256 index = uint(uint8(rawTuple & 0x0F));
        return bi0mz[index];
    }
}