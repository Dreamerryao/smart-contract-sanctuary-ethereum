struct ProptoVaultInfo {
    uint256 inVaultNum;
    address creator;
    address verifiedOwner;
    address contractAddress;
    uint256 predictedTokenId;
    bool settlementpaid;
}

/// @title The Core Contract of ProptoNFT
/// @notice Deploy PTokens, mint Propto Predict NFT and buy Propto Insurance
contract ProptoNFTCore is Ownable {
    using SafeMath for uint256;

    address public proptoVaultFactoryContract;

    address public proptoPredictContract;

    address public proptoInsuraceContract;

    address public uniswapV3Factory =
        0x1F98431c8aD98523631AE4a59f267346ea31F984;

    mapping(uint256 => ProptoVaultInfo) ProptoVaults;

    constructor(
        address _proptoVaultFactoryContract,
        address _proptoPredictFactoryContract,
        address _proptoInsuraceContract
    ) {
        proptoVaultFactoryContract = _proptoVaultFactoryContract;
        _proptoPredictFactoryContract;
        proptoInsuraceContract = _proptoInsuraceContract;
        proptoPredictContract = IProptoPredictFactory(
            _proptoPredictFactoryContract
        ).createNewProptoPredict("Propto Predict", "PPN");
    }

    /// @dev package transaction for create PToken
    /// @notice include deposit nfts create Vault Create PToken and open a sell in vending machine
    /// @param tokens nft contract addresses
    /// @param tokenIDs nft tokenIds
    /// @param vendingMachineAmount PToken amount for open sell in vending machine
    /// @param initialPrice initial price for open sell
    function easyCreateVault(
        address[] memory tokens,
        uint256[] memory tokenIDs,
        uint256 vendingMachineAmount,
        uint256 initialPrice,
        string memory name
    ) public {
        address vaultContract = IProptoVaultFactory(proptoVaultFactoryContract)
            .createVault(name, msg.sender);
        uint256 VaultId = IProptoVault(vaultContract).vaultId();

        require(tokens.length == tokenIDs.length, "args not fit");
        for (uint8 i = 0; i < tokens.length; i++) {
            require(
                ERC165(tokens[i]).supportsInterface(type(IERC721).interfaceId),
                "must support the ERC721 interface"
            );
            require(
                IERC721(tokens[i]).getApproved(tokenIDs[i]) == address(this) ||
                    IERC721(tokens[i]).isApprovedForAll(
                        msg.sender,
                        address(this)
                    ),
                "need token approve first"
            );
        }

        for (uint8 i = 0; i < tokens.length; i++) {
            IERC721(tokens[i]).safeTransferFrom(
                msg.sender,
                vaultContract,
                tokenIDs[i]
            );
        }

        IProptoVault(vaultContract).mintVToken();
        if (vendingMachineAmount > 0) {
            IProptoVault(vaultContract).openVendingMachine(
                vendingMachineAmount,
                initialPrice,
                IProptoInsurace(proptoInsuraceContract).reservesContract(),
                msg.sender
            );
        }

        IERC20(vaultContract).transfer(
            msg.sender,
            IProptoVault(vaultContract).totalSupply() - vendingMachineAmount
        );
        ProptoVaults[VaultId] = ProptoVaultInfo(
            IProptoVault(vaultContract).getInVaultNum(),
            msg.sender,
            address(0),
            vaultContract,
            0,
            false
        );
    }

    function createVault(string memory name) public {
        address vaultContract = IProptoVaultFactory(proptoVaultFactoryContract)
            .createVault(name, msg.sender);
        uint256 VaultId = IProptoVault(vaultContract).vaultId();

        IProptoVault(vaultContract).mintVToken();

        IERC20(vaultContract).transfer(
            msg.sender,
            IProptoVault(vaultContract).totalSupply()
        );
        ProptoVaults[VaultId] = ProptoVaultInfo(
            IProptoVault(vaultContract).getInVaultNum(),
            msg.sender,
            address(0),
            vaultContract,
            0,
            false
        );
    }

    function openVendingMachine(
        uint256 VaultId,
        uint256 amount,
        uint256 price
    ) public {
        require(
            IProptoVault(ProptoVaults[VaultId].contractAddress).creator() ==
                msg.sender,
            "you are not vault creator"
        );
        IProptoVault(ProptoVaults[VaultId].contractAddress).openVendingMachine(
            amount,
            price,
            IProptoInsurace(proptoInsuraceContract).reservesContract(),
            msg.sender
        );
    }

    function listVault(uint256 VaultId) public {
        require(
            ProptoVaults[VaultId].contractAddress == address(0),
            "already listed"
        );
        address vaultContract = IProptoVaultFactory(proptoVaultFactoryContract)
            .getVaultContract(VaultId);
        require(
            IProptoVault(vaultContract).creator() == msg.sender,
            "you are not vault creator"
        );
        require(
            IProptoVault(vaultContract).owner() == address(this),
            "need transfer owner to propto nft core contract to list"
        );
        require(
            IProptoVault(vaultContract).initialed() == true,
            "vault not initialed"
        );
        ProptoVaults[VaultId] = ProptoVaultInfo(
            IProptoVault(vaultContract).getInVaultNum(),
            msg.sender,
            address(0),
            vaultContract,
            0,
            false
        );
    }

    /**
        mint keychain for given VaultId and VRFMachine
     */
    function mintProptoPredict(
        uint256 VaultId,
        address PVRFContract,
        uint256 amount,
        uint256 startRange
    ) public {
        require(
            ProptoVaults[VaultId].contractAddress != address(0),
            "this vault is not listed yet"
        );
        (uint8 VStatus, ) = IProptoPredict(proptoPredictContract)
            .CustomIdPredictStatus(VaultId, address(0), 0);
        require(VStatus != 1, "Propto Vault already verified");

        uint256 valuation = getValuation(VaultId);
        console.log("valuation");
        console.log(valuation);

        uint256 payment = IProptoInsurace(proptoInsuraceContract)
            .calcBuyInsurancePaymeny(
                PVRFContract,
                IPVRF(PVRFContract).getPresentIndex(),
                amount,
                valuation
            );

        IERC20Metadata(
            IProptoInsurace(proptoInsuraceContract).reservesContract()
        ).transferFrom(msg.sender, address(this), payment);

        uint256 tokenId = IProptoPredict(proptoPredictContract).safeMint(
            msg.sender,
            PVRFContract,
            IPVRF(PVRFContract).getPresentIndex(),
            startRange,
            amount,
            VaultId
        );

        IERC20Metadata(
            IProptoInsurace(proptoInsuraceContract).reservesContract()
        ).approve(proptoInsuraceContract, payment);

        IProptoInsurace(proptoInsuraceContract).buyInsurance(
            proptoPredictContract,
            tokenId,
            valuation,
            ProptoVaults[VaultId].contractAddress,
            true
        );
    }

    function calcMintProptoPredictPayment(
        uint256 VaultId,
        address PVRFContract,
        uint256 amount
    ) public view returns (uint256) {
        uint256 payment = IProptoInsurace(proptoInsuraceContract)
            .calcBuyInsurancePaymeny(
                PVRFContract,
                IPVRF(PVRFContract).getPresentIndex(),
                amount,
                getValuation(VaultId)
            );
        return payment;
    }

    function getValuation(uint256 VaultId) public view returns (uint256) {
        uint256 price = getSettlementPrice(VaultId);
        return
            price
                .mul(
                    IProptoVault(ProptoVaults[VaultId].contractAddress)
                        .totalSupply()
                )
                .mul(125)
                .div(100)
                .div(
                    10 **
                        IProptoVault(ProptoVaults[VaultId].contractAddress)
                            .decimals()
                );
    }

    function getSettlementPrice(uint256 VaultId) public view returns (uint256) {
        uint256 uniprice = 0; // getUniPrice(VaultId);

        uint256 vendingMachinePrice;
        if (ProptoVaults[VaultId].contractAddress != address(0)) {
            vendingMachinePrice = IProptoVault(
                ProptoVaults[VaultId].contractAddress
            ).vendingMachinePrice();
        }
        //todo time check
        require(uniprice + vendingMachinePrice > 0, "no price found");
        uint256 price;
        if (uniprice > vendingMachinePrice) {
            price = uniprice;
        } else {
            price = vendingMachinePrice;
        }
        return price;
    }

    /**
        get the highest price of the PToken in 48 hours
     */
    function getUniPrice(uint256 VaultId) public view returns (uint256) {
        address tokenA = IProptoInsurace(proptoInsuraceContract)
            .reservesContract();
        address tokenB = ProptoVaults[VaultId].contractAddress;
        address poolContract = IUniswapV3Factory(uniswapV3Factory).getPool(
            tokenA,
            tokenB,
            3000
        );

        uint256 amount;
        if (poolContract == address(0)) {
            return amount;
        }

        uint32[] memory secends = new uint32[]((86400 / 900) + 1);
        uint32 start = 86400;
        uint32 i = 0;
        while (true) {
            secends[i] = start;
            if (start == 0) {
                break;
            }
            start -= 900;
            i++;
        }

        (int56[] memory tickCumulatives, ) = IUniswapV3Pool(poolContract)
            .observe(secends);
        for (i = 0; i < tickCumulatives.length; i++) {
            if (i + 1 >= tickCumulatives.length) {
                break;
            }
            uint256 quoteAmount = OracleLibrary.getQuoteAtTick(
                int24(tickCumulatives[i + 1] - tickCumulatives[i]),
                1000000,
                tokenA,
                tokenB
            );
            if (quoteAmount > amount) {
                amount = quoteAmount;
            }
        }
        return amount;
    }

    function getProptoVaultInfo(uint256 VaultId)
        public
        view
        returns (ProptoVaultInfo memory)
    {
        ProptoVaultInfo memory pvi = ProptoVaults[VaultId];

        (uint256 VStatus, uint256 tokenId) = IProptoPredict(
            proptoPredictContract
        ).CustomIdPredictStatus(VaultId, address(0), 0);
        if (VStatus == 1) {
            pvi.predictedTokenId = tokenId;
            ProptoPredictInfo memory ppi = IProptoPredict(proptoPredictContract)
                .getInfo(tokenId);
            insuranceData memory idata = IProptoInsurace(proptoInsuraceContract)
                .getInsuranceData(ppi.ProptoInsuranceIndex);
            pvi.settlementpaid = idata.settlement;
        }

        return pvi;
    }

    function updateVaultInfo(uint256 VaultId) public {
        ProptoVaultInfo memory pvi = ProptoVaults[VaultId];
        if (pvi.predictedTokenId == 0) {
            (uint256 VStatus, uint256 verifiedTokenId) = IProptoPredict(
                proptoPredictContract
            ).CustomIdPredictStatus(VaultId, address(0), 0);
            if (VStatus == 1) {
                ProptoVaults[VaultId].predictedTokenId = verifiedTokenId;
                pvi.predictedTokenId = verifiedTokenId;
            }
        }

        if (pvi.predictedTokenId != 0 && pvi.settlementpaid == false) {
            ProptoPredictInfo memory ppi = IProptoPredict(proptoPredictContract)
                .getInfo(pvi.predictedTokenId);
            IProptoInsurace(proptoInsuraceContract).collectInsurancePay(
                ppi.ProptoInsuranceIndex
            );
            ProptoVaults[VaultId].settlementpaid = true;
            insuranceData memory idata = IProptoInsurace(proptoInsuraceContract)
                .getInsuranceData(ppi.ProptoInsuranceIndex);
            IProptoVault(pvi.contractAddress).settled(
                idata.insuranceAmount,
                IProptoInsurace(proptoInsuraceContract).reservesContract()
            );
        }
    }

    /**
        Verify a suceed keychain and transfer the Vault ownership to keychain owner
     */
    function verifiProptoPredict(uint256 tokenId) public {
        address tokenOwner = IProptoPredict(proptoPredictContract).ownerOf(
            tokenId
        );
        require(tokenOwner == msg.sender, "not your prediction");
        ProptoPredictInfo memory ppi = IProptoPredict(proptoPredictContract)
            .getInfo(tokenId);
        updateVaultInfo(ppi.customId);
        ProptoVaultInfo memory pvi = ProptoVaults[ppi.customId];
        require(pvi.predictedTokenId == tokenId, "prediction not succeed");
        require(pvi.verifiedOwner == address(0), "vault already verified");
        ProptoVaults[ppi.customId].verifiedOwner = tokenOwner;
        IProptoVault(pvi.contractAddress).transferOwnership(tokenOwner);
    }

    function withdrawSettlement(uint256 VaultId) public {
        updateVaultInfo(VaultId);
        if (ProptoVaults[VaultId].settlementpaid) {
            IProptoVault(ProptoVaults[VaultId].contractAddress)
                .withdrawSettlementTo(msg.sender);
        }
    }
}

