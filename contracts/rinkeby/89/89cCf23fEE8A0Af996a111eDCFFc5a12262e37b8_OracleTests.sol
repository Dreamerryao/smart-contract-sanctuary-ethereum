    using Strings for uint256;

    string[] public sadStates = [
        'sadAngry',
        'sadDistress'
    ];

    string[] public happyStates = [
        'happyFlying',
        'happyJump'
    ];

    string[] public neutralStates = [
        'neutralBored',
        'neutralButterfly'
    ];

    string[] public superSadStates = [
        'superSadLayingDown',
        'superSadRainDesperate'
    ];

    string public baseURI = 'ipfs://QmTV4SdMX9Tfes79zyY9vWJSWn1jwGNU2iBbgBP514kt1h/';

    address[10] public oracles = [
        // TODO: RINKEBY
        0x8A753747A1Fa494EC906cE90E9f37563A8AF630e, // 0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419, // 0 eth
        // TODO: RINKEBY
        0xECe365B379E1dD183B20fc5f022230C044d51404, // 0xF4030086522a5bEEa4988F8cA5B36dbC97BeE88c, // 1 btc
        0xFF3EEb22B5E3dE6e705b44749C2559d704923FD7, // 2 avax
        0x2465CefD3b488BE410b941b1d4b2767088e2A028, // 3 doge
        // TODO: RINKEBY
        0xd8bD0a1cB028a31AA859A21A3758685a95dE4623, // 0x2c1d072e956AFFC0D435Cb7AC38EF18d24d9127c, // 4 link
        // TODO: RINKEBY
        0x7794ee502922e2b723432DDD852B3C30A911F021, // 0x7bAC85A8a13A4BcD8abb3eB7d6b4d632c5a57676, // 5 matic
        // TODO: RINKEBY
        0xf57FCa8B932c43dFe560d3274262b2597BCD2e5A, // 0x5239a625dEb44bF3EeAc2CD5366ba24b8e9DB63F, // 6 tezos
        // TODO: RINKEBY
        0xcf0f51ca2cDAecb464eeE4227f5295F2384F84ED, // 0x14e613AC84a31f709eadbdF89C6CC390fDc9540A, // 7 binance
        0x4ffC43a60e009B551865A93d232E33Fce9f01507, // 8 sol
        0xD6aA3D25116d8dA79Ea0246c4826EB951872e02e  // 9 uniswap
    ];

    // see getMetadataForTokenId for details.
    mapping(uint256 => bytes32) public tokenMetadata;

    // 0 = monthly, 1 = weekly, 2 = daily, 3 = twice daily, 4 = hourly
    uint256[5] public jumpSizes = [70, 10, 1, 1, 1];

    // todo: we don't really need this
    uint256 internal hour = 3600;

    /**
     * @notice Primary interface for finiliar ERC721 contract.
     */
    function tokenURI(uint256 tokenId) external view returns (string memory) {
        (uint256 frequency, uint256 family) = getMetadataForTokenId(tokenId);
        console.logUint(frequency);
        console.logUint(family);
        int256 mood = getMood(frequency, family);
        console.logInt(mood);
        return getURIForMood(mood, tokenId);
    }

    /**
     * @notice Get a mood (% delta) for a given frequency and family.
     */
    function getMood(uint256 frequency, uint256 family) public view returns (int256) {
        // first fetch the current price
        AggregatorV2V3Interface feed = AggregatorV2V3Interface(oracles[family]);
        int256 latestPrice = feed.latestAnswer();

        // next fetch the previous price
        int256 previousPrice = getPreviousPrice(frequency, family);

        // compare the two
        int256 mood = (latestPrice - previousPrice) / (previousPrice / 100);
        console.log('latest price');
        console.logInt(latestPrice);
        console.log('previous price');
        console.logInt(previousPrice);
        console.log('delta');
        console.logInt(mood);

        return mood;
    }

    /**
     * @notice Given a mood (% delta), return a URI.
     * @dev We use a single IPFS directory per mood for all finiliar (e.g.
     * happiness is a single directory of 10k items). So we don't need anything
     * finiliar-specific to know which IPFS hash to return.
     */
    function getURIForMood(int256 mood, uint256 tokenId) public view returns (string memory) {
        // neutral + fallback
        string memory path = neutralStates[pseudoRandom() % neutralStates.length];

        // sad
        if (mood < -3 && mood > -60) path = sadStates[pseudoRandom() % sadStates.length];
        if (mood <= -60) path = superSadStates[pseudoRandom() % superSadStates.length];

        // happy
        if (mood > 3 && mood < 60) path = happyStates[pseudoRandom() % happyStates.length];
        
        return string(abi.encodePacked(baseURI, path, '/', tokenId.toString()));
    }

    /**
     * @notice Recurse to find a round id with a timestamp within the given range.
     * @dev Oscillates back/forward in time, with halving "jump sizes", until found.
     * Any timestamp w/in the target range is accepted. We use larger jump sizes to
     * iterate roughly through more data (e.g. finding a price from a month ago).
     * If we overshoot we simply reverse directions and halve jump sizes. Gives up
     * and returns a neutral state if 100 iterations is reached, though this should
     * never happen.
     * @param frequency 0 = monthly, 1 = weekly, 2 = daily, 3 = twice daily, 4 = hourly
     * @param feed the oracle contract corresponding to the finiliar family
     * @param roundId the round id we should test for a timestamp
     * @param counter helps us prevent running out of gas
     * @param jumpSize how many round ids to skip as we iterate. halves each time
     * we overshoot.
     * @param jumpDirection whether our search is going backward or forward in time.
     * false = backwards, true = forwards. switch each time we overshoot.
     */
    function findRoundId(
        uint256 frequency,
        AggregatorV2V3Interface feed,
        uint80 roundId,
        uint256 counter,
        uint256 jumpSize,
        bool jumpDirection
    )
        public
        view
        returns (uint80)
    {

        // as a failsafe, if the counter has reached a certain point, give up
        // and return a generic state.
        if (counter > 100) {
            return uint80(feed.latestRound());
        }

        // get range for this timeframe
        (uint256 rangeStart, uint256 rangeEnd) = getRange(frequency);

        uint80 thisRoundId = uint80(jumpDirection ? (roundId + jumpSize) : (roundId - jumpSize));
        uint256 timestamp = feed.getTimestamp(thisRoundId);
        
        console.log('========================================');
        console.log('\n round id =>');
        console.logUint(thisRoundId);
        console.log('\n timestamp =>');
        console.logUint(timestamp);
        console.log('\n jumpsize =>');
        console.log(jumpSize);
        console.log('\n counter =>');
        console.logUint(counter);
        console.log('\n \n');

        bool tooEarly = timestamp < rangeStart;
        bool tooLate = timestamp > rangeEnd;

        // if we overshot range for an hourly, the price hasn't changed,
        // return the original roundId (i.e. the latest one), producing a neutral mood.
        if (tooEarly && frequency == 4) {
            console.log('overshot');
            return uint80(roundId);
        }

        // if we haven't gone far back enough in either direction, keep going
        if (
            (tooLate && !jumpDirection) || // still too late but heading backwards
            (tooEarly && jumpDirection) || // still too early but heading forwards
            timestamp == 0
        ) {
            return this.findRoundId(
                frequency,
                feed,
                thisRoundId,
                counter + 1,
                jumpSize,
                jumpDirection
            );
        }

        // if we overshot in either direction, switch directions and halve jumps
        if (
            (tooEarly && !jumpDirection) || // too early and heading back in time
            (tooLate && jumpDirection) // too late and heading forward in time
        ) {
            return this.findRoundId(
                frequency,
                feed,
                thisRoundId,
                counter + 1,
                (jumpSize / 2) == 0 ? 1 : (jumpSize / 2),
                !jumpDirection
            );
        }

        // if we made it out of here, we've found a timestamp within range
        return roundId;
    }

    /**
     * @notice Get the previous price for a given frequency and family.
     * @dev We'll use this previous price to compare to the current price and
     * generate a mood (% delta).
     * @return previous price
     */
    function getPreviousPrice(uint256 frequency, uint256 family) public view returns (int256) {
        AggregatorV2V3Interface feed = AggregatorV2V3Interface(oracles[family]);

        // grab with latest roundId
        uint256 latestRoundUint = feed.latestRound();
        uint80 latestRound = uint80(feed.latestRound());

        console.log('latest round');
        console.logUint(latestRound);
        console.logUint(latestRoundUint);
        console.logAddress(oracles[family]);

        // recurse to find the _roundId with the pricing info we need
        uint80 _roundId = findRoundId(
            frequency,
            feed,
            latestRound,
            0,
            jumpSizes[frequency],
            false
        );

        // roundId found, fetch info for it
        (
          ,
          int256 answer,
          ,
          ,
        ) = feed.getRoundData(_roundId);

        return answer;
    }

    /**
     * @notice Get target ranges per frequency.
     * @dev We use these as fuzziness in our price search algorithm. Required
     * because oracles won't have prices for an exact, arbitrary timestamp.
     * todo: should these be updateable?
     */
    function getRange(uint256 frequency) public view returns (uint256, uint256) {
        // monthly
        if (frequency == 0) {
            return (
                block.timestamp - (hour * 24 * 31), // 31 days ago
                block.timestamp - (hour * 24 * 29) // 29 days ago
            );
        }
        // weekly
        if (frequency == 1) {
            return (
                block.timestamp - (hour * 180), // 7.5 days ago
                block.timestamp - (hour * 156) // 6.5 days ago
            );
        }
        // daily
        if (frequency == 2) {
            return (
                block.timestamp - (hour * 26), // 26 hours ago
                block.timestamp - (hour * 22) // 22 hours
            );
        }
        // twice daily
        if (frequency == 3) {
            return (
                block.timestamp - (hour * 14), // 14 hours ago
                block.timestamp - (hour * 10) // 10 hours
            );
        }
        // hourly
        if (frequency == 4) {
            return (
                block.timestamp - (hour * 2), // 2 hours ago
                block.timestamp - (hour * 1) // 1 hour ago
            );
        }

        revert("Invalid frequency");
    }

    /**
     * @notice Fetch the frequency and family for a tokenId.
     * @dev We store both frequency and family values as a single byte (8 bits).
     * The tens place of the final uint256 is frequency, the ones place is family.
     * So if bytes32[0][0] = 00001001 (0x9, or 9), frequency is 0 and family is 9.
     * If bytes32[0][1] = 00011000 (0x18, or 24), frequency is 2 and family is 4.
     * @return frequency. 0 monthly, 1 weekly, 2 daily, 3 twice daily, 4 hourly
     * @return family. see feed orderings for mappings
     */
    function getMetadataForTokenId(uint256 tokenId) public view returns (uint256, uint256) {
        // grab the relevant byte. h/t @dievardump & @fiveoutofnine ❤️
        bytes1 data = bytes1(tokenMetadata[tokenId / 32] << (8 * (tokenId % 32)));

        // convert to uint and grab tens and ones place
        uint256 values = uint256(uint8(data));
        uint256 frequency = values / 10;
        uint256 family = values % 10;

        return (
            frequency,
            family
        );
    }

    /**
     * @notice Update token metadata (frequency and family) for tokenIds. Each
     * byte represents 32 token ids. See getMetadataForTokenId for more detail.
     */
    function updateMetadata(bytes32[] calldata data, uint256 startingIndex)
        public
        onlyOwner
    {
        for (uint256 i = startingIndex; i < data.length; i++) {
            tokenMetadata[i] = data[i];
        }
    }

    /**
     * @notice Add a IPFS path to a state type. These are randomly selected from
     * depending on mood.
     */
    function addState(uint256 stateType, string calldata path) external onlyOwner {
        if (stateType == 0) happyStates.push(path);
        if (stateType == 1) neutralStates.push(path);
        if (stateType == 2) sadStates.push(path);
        if (stateType == 3) superSadStates.push(path);
    }

    /**
     * @notice Delete a state path from the list of options
     * @dev We're copying the last item into the index of the deleted one, then
     * removing the last item, so we cover any gaps. Order doesn't matter.
     */
    function deleteState(uint256 stateType, uint256 index) external onlyOwner {
        if (stateType == 0) {
            happyStates[index] = happyStates[happyStates.length - 1];
            happyStates.pop();
        }
        if (stateType == 1) {
            neutralStates[index] = neutralStates[neutralStates.length - 1];
            neutralStates.pop();
        }
        if (stateType == 2) {
            sadStates[index] = sadStates[sadStates.length - 1];
            sadStates.pop();
        }
        if (stateType == 3) {
            superSadStates[index] = superSadStates[superSadStates.length - 1];
            superSadStates.pop();
        }
    }

    /**
     * @notice Get a pseudorandom value
     */
    function pseudoRandom() private view returns (uint) {
        // console.logUint(uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp))));
        return uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp)));
    } 

}

