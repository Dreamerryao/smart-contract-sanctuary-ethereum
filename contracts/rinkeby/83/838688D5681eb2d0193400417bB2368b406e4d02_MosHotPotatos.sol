    //Game
    function ExplodePotatos(uint NumberOfExplodes) public onlyOwner {

    for(uint i = 0; i < NumberOfExplodes; i++)
      {
        
        uint RandomNumber = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty, msg.sender))) % ArrayOfPotatoes.length;
        if(RandomNumber!=ArrayOfPotatoes.length-1)
        {
            ArrayOfPotatoes[RandomNumber]=ArrayOfPotatoes[ArrayOfPotatoes.length-1];
        }
        ArrayOfPotatoes.pop(); //will remove the very last element
      }

      Week=Week+1;
    uint RandomNumberTwo = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty, msg.sender))) % ArrayOfPotatoes.length;
    WeeklyWinner=ArrayOfPotatoes[RandomNumberTwo];
    if(Week!=5)
    {
    //address of winner
    (bool hs, ) = payable(ownerOf(WeeklyWinner)).call{value: address(this).balance * 10 / 100}("");
    require(hs);

    }
    else
    {
        (bool hs, ) = payable(ownerOf(WeeklyWinner)).call{value: address(this).balance * 50 / 100}("");
        require(hs);
        GameFinished = true;
    }

 }
