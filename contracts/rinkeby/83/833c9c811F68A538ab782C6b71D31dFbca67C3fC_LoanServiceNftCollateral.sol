pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

import "./LoanCommon.sol";

contract LoanServiceNftCollateral is Ownable {
    using SafeMath for uint256;

    /// @notice The loan oracle address
    address public loanOracleAddress;

    /// @notice Set the oracle address
    /// @param _address The new loan storage address
    function setLoanOracleAddress(address _address) public onlyOwner {
        loanOracleAddress = _address;
    }

    /// @notice internal function to get the loan oracle address
    function getLoanOracleAddress() internal view returns (address) {
        require (loanOracleAddress != address(0x0), "loan oracle address must be set");
        return loanOracleAddress;
    }

    modifier onlyServiceContractsOrOwner() {
        address loanServiceAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceAddress();

        require(
            msg.sender == loanServiceAddress || msg.sender == owner(),
            "caller must be a service contract or owner"
        );
        _;
    }

    /// @notice create loan collateral
    /// @param _loanId The ID of the loan
    /// @param _nftContracts A list of nft contracts
    /// @param _nftTokenIds A list of nft token IDs
    /// @param _nftTokenAmounts A list of token amounts
    /// @param _standards A list of token standards
    /// @param _nftEstimatedValue The borrower estimated value of the NFT
    function create(
        uint256 _loanId,
        address[] memory _nftContracts,
        uint256[] memory _nftTokenIds,
        uint256[] memory _nftTokenAmounts,
        LoanCommon.Standard[] memory _standards,
        uint256 _nftEstimatedValue
    ) public onlyServiceContractsOrOwner {

        address nftCollateralStorageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageNftCollateralAddress();
        LoanStorageNftCollateral_I(nftCollateralStorageAddress).create(
            _loanId,
            _nftContracts,
            _nftTokenIds,
            _nftTokenAmounts,
            _standards,
            _nftEstimatedValue);
    }

    /// @notice get collateral object by loan id
    /// @param _loanId The collateral id
    function collaterals(uint256 _loanId) public view returns(LoanCommon.NftCollateral memory) {
        address nftCollateralStorageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageNftCollateralAddress();
        return LoanStorageNftCollateral_I(nftCollateralStorageAddress).collaterals(_loanId);
    }

    /// @notice convenient method to get a list of nft collaterals
    /// @param _start The starting index
    /// @param _end The ending index
    function list(uint256 _start, uint256 _end) public view returns(LoanCommon.NftCollateral[] memory) {
        require(_end >= _start, "The end index must be greater than or equal to the start index");
        LoanCommon.NftCollateral[] memory collateralList = new LoanCommon.NftCollateral[](_end.sub(_start).add(1));

        uint256 index = 0;
        for (uint256 i = _start; i <= _end; i++) {
            collateralList[index] = collaterals(i);
            index++;
        }

        return collateralList;
    }

}

