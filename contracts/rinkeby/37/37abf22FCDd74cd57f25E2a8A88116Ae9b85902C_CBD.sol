// File: contracts/CBD.sol


pragma solidity ^0.8.0;









contract CBD is IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;


    mapping(address => uint256) public _rOwned;
    mapping(address => mapping(address => uint256)) private _allowances;

    bytes32 public _merkleRoot;
    mapping(uint256 => uint256) private claimedBitMap;

    EnumerableSet.AddressSet private _excludedFromBurn;

    address public _cashPool;

    uint256 public _tTotal = 100000000000000 * 10 ** 9;

    uint256 public constant MAX = ~uint256(0);

    uint256 public _burnRate = 27397;

    uint256 public _ratio = 10000000;

    uint256 public _rate = MAX / _burnRate / _tTotal;

    uint256 public _rateFix = _rate;

    uint256 public _nextBurnTime;

    uint256 private _airdropSupply = _tTotal * 50 / 100;

    address private _cashPoolAddress = 0x426e2cDF378A2a7cfEf51a6C62453aC1035823a9;
    uint256 private _cashPoolSupply = _tTotal * 25 / 100;

    address private _reservedAddress = 0x426e2cDF378A2a7cfEf51a6C62453aC1035823a9;
    uint256 private _reservedSupply = _tTotal * 25 / 100;


    string private _name = "CubicDao";
    string private _symbol = "CBD";
    uint8 private _decimals = 9;

    event Claim(address indexed claimant, uint256 amount);

    constructor (uint256 next_burn_time,bytes32 merkleRoot) {
        _nextBurnTime = next_burn_time;
        _merkleRoot = merkleRoot;
        _rOwned[address(this)] = _airdropSupply;
        _rOwned[_cashPoolAddress] = _cashPoolSupply;
        _rOwned[_reservedAddress] = _reservedSupply;

         IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
         address uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
         .createPair(address(this), _uniswapV2Router.WETH());

        EnumerableSet.insert(_excludedFromBurn, address(this));
        EnumerableSet.insert(_excludedFromBurn, uniswapV2Pair);
        EnumerableSet.insert(_excludedFromBurn, msg.sender);
        EnumerableSet.insert(_excludedFromBurn, _cashPoolAddress);
        EnumerableSet.insert(_excludedFromBurn, _reservedAddress);
        //emit Transfer(address(0), address(this), _airdropSupply);
        //emit Transfer(address(0), _cashPoolAddress, _cashPoolSupply);
        //emit Transfer(address(0), _reservedAddress, _reservedSupply);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        if (isExcludedFromBurn(account)) {
            return _rOwned[account];
        }
        return _rOwned[account].mul(_rate).div(_rateFix);
    }

    function claim(uint256 index, uint256 amount, bytes32[] calldata merkleProof) external {
        require(!isClaimed(index), 'CBD:already claimed');
        bytes32 node = keccak256(abi.encodePacked(index, msg.sender, amount));
        require(MerkleProof.verify(merkleProof, _merkleRoot, node), 'CBD: Valid proof required.');
        _setClaimed(index);
        _rOwned[address(this)] = _rOwned[address(this)].sub(amount);
        _rOwned[msg.sender] = _rOwned[msg.sender].add(amount);
        emit Claim(msg.sender, amount);
        emit Transfer(address(this), msg.sender, amount);
    }

    function _setClaimed(uint256 index) private {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        claimedBitMap[claimedWordIndex] = claimedBitMap[claimedWordIndex] | (1 << claimedBitIndex);
    }

    function isClaimed(uint256 index) public view returns (bool) {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        uint256 claimedWord = claimedBitMap[claimedWordIndex];
        uint256 mask = (1 << claimedBitIndex);
        return claimedWord & mask == mask;
    }


    function updateRate() public {
        _rate = _rate.sub(_rate.mul(_burnRate).div(_ratio));
        _nextBurnTime = _nextBurnTime.add(1 days);
        uint256 notBurnBalance = 0;
        if (getExcludedFromBurnLength() > 0) {
            for (uint256 i = 0; i < getExcludedFromBurnLength(); i++) {
                notBurnBalance = notBurnBalance.add(balanceOf(getExcludedFromBurn(i)));
            }
        }
        uint256 burnBalance = totalSupply().sub(notBurnBalance).mul(_burnRate).div(_ratio);
        burnBalance = burnBalance.add(balanceOf(address(this)).mul(_rate).div(_rateFix).mul(_burnRate).div(_ratio));
        _rOwned[_cashPool] = _rOwned[_cashPool].add(burnBalance);
        ICashPool(_cashPool).distributePool(burnBalance);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero"));
        return true;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        bool flag = false;
        if (block.timestamp > _nextBurnTime) {
            flag = true;
            _allocation();
        }
        uint256 rAmount = amount.mul(_rateFix).div(_rate);
        if (isExcludedFromBurn(from)) {
            _rOwned[from] = _rOwned[from].sub(amount);
        } else {
            if(flag && _rOwned[from] < rAmount){
                _rOwned[from] = 0;
            } else {
                _rOwned[from] = _rOwned[from].sub(rAmount);
            }
        }
        if (isExcludedFromBurn(to)) {
            _rOwned[to] = _rOwned[to].add(amount);
        } else {
            _rOwned[to] = _rOwned[to].add(rAmount);
        }
        emit Transfer(from, to, amount);
    }


    function _allocation() private {
        _rate = _rate.sub(_rate.mul(_burnRate).div(_ratio));
        _nextBurnTime = _nextBurnTime.add(1 days);
        uint256 notBurnBalance = 0;
        if (getExcludedFromBurnLength() > 0) {
            for (uint256 i = 0; i < getExcludedFromBurnLength(); i++) {
                notBurnBalance = notBurnBalance.add(balanceOf(getExcludedFromBurn(i)));
            }
        }
        uint256 burnBalance = totalSupply().sub(notBurnBalance).mul(_burnRate).div(_ratio);
        burnBalance = burnBalance.add(balanceOf(address(this)).mul(_rate).div(_rateFix).mul(_burnRate).div(_ratio));
        _rOwned[_cashPool] = _rOwned[_cashPool].add(burnBalance);
        ICashPool(_cashPool).distributePool(burnBalance);
    }


    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function addExcludedFromBurn(address[] memory _address) public onlyOwner {
        for (uint256 i = 0; i < _address.length; i++) {
            require(_address[i] != address(0), "manager is the zero address");
            EnumerableSet.insert(_excludedFromBurn, _address[i]);
        }
    }

    function delExcludedFromBurn(address _address) public onlyOwner returns (bool) {
        require(_address != address(0), "manager is the zero address");
        return EnumerableSet.remove(_excludedFromBurn, _address);
    }

    function getExcludedFromBurnLength() public view returns (uint256) {
        return EnumerableSet.length(_excludedFromBurn);
    }

    function getExcludedFromBurn(uint256 _index) public view returns (address){
        return EnumerableSet.at(_excludedFromBurn, _index);
    }

    function isExcludedFromBurn(address _address) public view returns (bool) {
        return EnumerableSet.contains(_excludedFromBurn, _address);
    }

    function setCashPoolAddress(address _address) public onlyOwner {
        _cashPool = _address;
    }

    function setBurnRate(uint256 burnRate,uint256 ratio) public onlyOwner {
        _burnRate = burnRate;
        _ratio = ratio;
    }

}