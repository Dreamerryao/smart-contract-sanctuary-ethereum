        transactionData storage transaction = transactions[_transactionID];
        uint256 templateId = 0; // yes/no question
        string memory question = string(abi.encodePacked("Was work at ", abi.encodePacked(transaction.buyer), " finished?\nEvidence1: ", abi.encodePacked(audits[_auditID].buyerEvidence), "\nEvidence2: ", abi.encodePacked(audits[_auditID].sellerEvidence) ));
        address arbitrator = address(this);                                     // TODO: set actual address
        uint32 timeout = uint32(block.timestamp + (_days * 86400));             // TODO: seconds, minutes, hours, ...
        uint32 openingTs;      // If set, the earliest time it should be possible to answer the question.
        uint256 nonce = block.timestamp;
        bytes32 questionId = realityContract.askQuestion(templateId, question, arbitrator, timeout, openingTs, nonce);
        */
    }
    

}