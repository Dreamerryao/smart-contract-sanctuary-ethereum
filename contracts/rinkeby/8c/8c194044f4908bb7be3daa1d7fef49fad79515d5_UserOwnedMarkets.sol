interface IERC20 {
    function balanceOf(address account) external returns (uint256);
    function transfer(address _to, uint256 _amount) external returns (bool);
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
}

interface RealityETH_v3_0 {
    function askQuestion(uint256 templateId, string memory question, address arbitrator, uint32 timeout, uint32 openingTs, uint256 nonce) external payable returns (bytes32);
}

contract UserOwnedMarkets is ERC721, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    address public paymentTokenAddress; //ERC20 payment token required to buy in (wETH/USDT?)
    address public delegate; //minion

    address[] public otherMarkets; //list of addresses of other UserOwnedMarkets that can participate here

    
    RealityETH_v3_0 public realityContract;

    struct transactionData {
        address buyer; 
        address seller;
        bytes32 spec; 
        address paymentTokenAddress; 
        uint paymentAmount; 
        uint expiry;
        bytes buyerSignature;
        bytes sellerSignature;
        uint[] auditIDs;

        bool finished;
    }

    struct assayerInput{
        bytes32 commit;
        bool revealed;
    }

    struct auditData{
        bytes32 buyerEvidence;
        bytes32 sellerEvidence;
        uint expiry;
        
        bool resolved; // set true if minion decides
        bool refund; // minions decision

        uint[] commits; //_tokenIds of all assayers who participated in commit
        bool[] verdicts; // false in favor of seller, true in favor of buyer - refund
        
        mapping(uint => assayerInput) scheme;//salted boolean commits, that need to be revealed with personal salt variable
        //mapping(uint => bool) revealedCommit;//tokenID => true //here to prevent multiple submissions

        // TODO array of hashes (not booleans) in the commitment scheme 
    }

    struct assayerData{ // ownerOf(tokenId) already returns address, here we would keep data od confirmed assayers not just NFT holders
        address _address;
    }

    uint public transactionID = 0;
    transactionData[] public transactions;
    
    uint public auditID = 0;
    auditData[] audits;
    
    uint public assayerID = 0;
    assayerData[] public assayers;
    
    
    
    uint public assayerPrice = 1000000000000000; // starting price 0.001 Eth
    uint8 public feePercent = 1;
    uint public burntTokens = 0;
    
    

    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        realityContract = RealityETH_v3_0( 0x93f8dddd876c7dBE3323723500e83E202A7C96CC ); //Reality ETH address
        paymentTokenAddress = 0x50EC5Ad07b8B0a6839DB03f6A0F5E9c22CABA2B1; //Test token - wETH
        transferOwnership(0x2c50837CFc5eA8fefD40Ba651eCe85ae94d1f0b1); //multisig contract / gnosis safe
    }

   

   /***
    * @dev Allows two users who have previously made an agreement to buy some good for some unit of demand (Token)
    * @param buyer: buyer's address
    * seller: seller's address
    
    * string denotating the good being bought
    * paymentToken: ERC20 interface for what unit of Demand
    * paymentAmount: amount of unit of Demand
    * contractLink: urlString to any attached agreement/contract
    * days: uint values of days for the transaction to go through, default 7 days/ 1 week
    */
    modifier validAddress(address addr) {
        require (addr != address(0));
        _;
        //might be more restrictions?
    }
    
    modifier validAmount(address buyer, address token, uint amount ) {
        require(IERC20(token).balanceOf(buyer) >= amount , "Not enough tokens.");
         _;
    }

    function createTransaction(address buyer, address seller, bytes32 spec, address _paymentTokenAddress, uint paymentAmount, uint _days, bytes memory buyerSignature, bytes memory sellerSignature)
        validAddress(buyer)
        validAddress(seller)
        validAmount(buyer, _paymentTokenAddress, paymentAmount)
    public payable returns (uint) {
        require(IERC20(_paymentTokenAddress).transferFrom(buyer, address(this), paymentAmount), "Token transfer failed.");  //buyer needs to approve transaction for this contract -- update with metatx procedure
        transactionData memory temp;
        temp.buyer = buyer;
        temp.seller = seller;
        temp.spec = spec;
        temp.paymentTokenAddress = paymentTokenAddress;
        temp.paymentAmount = paymentAmount;
        temp.expiry = block.timestamp + (_days * 86400); // TODO: seconds, minutes, hours, ...
        temp.buyerSignature = buyerSignature;
        temp.sellerSignature = sellerSignature;
        temp.finished = false;
        transactions.push(temp);
        transactionID++;
        
        
        return transactionID - 1;
        
    }

    function addAssayer() validAmount(msg.sender, paymentTokenAddress, assayerPrice*2**(_tokenIds.current() - burntTokens)) public payable {
        require(IERC20(paymentTokenAddress).transferFrom(msg.sender, address(this), assayerPrice*2**(_tokenIds.current() - burntTokens)), "Token transfer failed.");
        assayers.push(assayerData(msg.sender));
        assayerID++;
        
        _mint(msg.sender, _tokenIds.current());
        _tokenIds.increment();
    }

    function payWithFee(uint _transactionID) internal {
        uint fee=transactions[_transactionID].paymentAmount/100*feePercent;
        IERC20(transactions[_transactionID].paymentTokenAddress).transfer(msg.sender, transactions[_transactionID].paymentAmount - fee);
        IERC20(transactions[_transactionID].paymentTokenAddress).transfer(owner(), fee);
        transactions[_transactionID].finished = true;
    }

    /** 
    * @dev allows a user to withdraw funds from transaction
    */
    function withdraw(uint _transactionID) public payable {
        require(transactions[_transactionID].expiry < block.timestamp, "Time has not yet expired.");
        require(!transactions[_transactionID].finished, "Transaction has been completed.");
        if (msg.sender == transactions[_transactionID].seller) { //checking if withdraw was called by seller
            if (transactions[_transactionID].auditIDs.length == 0) { //checking if no audits were called
                payWithFee(_transactionID);
            } else { //checking if all audits were resolved and in favor of seller
                uint resolved;
                for (uint i = 0; i < transactions[_transactionID].auditIDs.length; i++) { // first we check if audit was ruled by delegate / minion
                    if (audits[transactions[_transactionID].auditIDs[i]].resolved && !audits[transactions[_transactionID].auditIDs[i]].refund) 
                        resolved++;
                    else { //then we check if all assayers voted in the favor of the seller
                        require(audits[transactions[_transactionID].auditIDs[i]].commits.length == audits[transactions[_transactionID].auditIDs[i]].verdicts.length , "Not all assayers have revealed their pick.");
                        uint verdicts;
                        for(uint j = 0; j < audits[transactions[_transactionID].auditIDs[i]].verdicts.length; i++){
                            if (!audits[transactions[_transactionID].auditIDs[i]].verdicts[j])
                                verdicts++;
                        }
                        if (verdicts != 0 && verdicts == audits[transactions[_transactionID].auditIDs[i]].verdicts.length) 
                        resolved++;
                    }
                }
                require(resolved == transactions[_transactionID].auditIDs.length, "Unresolved audit");
                payWithFee(_transactionID);
            }
        }
        else if (msg.sender == transactions[_transactionID].buyer && transactions[_transactionID].auditIDs.length > 0) {// checking if withdraw was called by buyer
            uint resolved;//checking if all audits were resolved and in favor of seller
            for (uint i = 0; i < transactions[_transactionID].auditIDs.length; i++) { // first we check if audit was ruled by delegate / minion
                if (audits[transactions[_transactionID].auditIDs[i]].resolved && audits[transactions[_transactionID].auditIDs[i]].refund) 
                    resolved++;
                else { //then we check if all assayers voted in the favor of the buyer
                    require(audits[transactions[_transactionID].auditIDs[i]].commits.length == audits[transactions[_transactionID].auditIDs[i]].verdicts.length , "Not all assayers have revealed their pick.");
                    uint verdicts;
                    for(uint j = 0; j < audits[transactions[_transactionID].auditIDs[i]].verdicts.length; i++){
                        if (audits[transactions[_transactionID].auditIDs[i]].verdicts[j])
                            verdicts++;
                    }
                    if (verdicts != 0 && verdicts == audits[transactions[_transactionID].auditIDs[i]].verdicts.length) 
                        resolved++;
                }
            }
            require(resolved == transactions[_transactionID].auditIDs.length, "Unresolved audit");
            payWithFee(_transactionID);
        }
    }

    function audit(uint _transactionID, uint _days) public payable {//require some payment?
        transactions[_transactionID].auditIDs.push(auditID);
        audits.push();
        audits[auditID].expiry = block.timestamp + (_days * 86400);
        auditID++;
    }

    modifier auditLinked(uint _transactionID, uint _auditID) {
        bool linked = false;
        for (uint i; i < transactions[_transactionID].auditIDs.length; i++) 
            if (transactions[_transactionID].auditIDs[i] == _auditID)
                linked=true;
        require(linked , "Transaction not linked with audit.");
        _;
    }

    function fillBuyerEvidence(uint _transactionID, uint _auditID, bytes32 evidence) auditLinked(_transactionID, _auditID) public payable {
        require(transactions[_transactionID].buyer == msg.sender, "You are not the buyer.");
        audits[_auditID].buyerEvidence = evidence;
    }

    function fillSellerEvidence(uint _transactionID, uint _auditID, bytes32 evidence) auditLinked(_transactionID, _auditID) public payable {
        require(transactions[_transactionID].seller == msg.sender, "You are not the seller.");
        audits[_auditID].sellerEvidence = evidence;
    }
    
    function resolveAudit(uint _auditID, bool refund) public onlyOwner{
        audits[_auditID].resolved = true;
        audits[_auditID].refund = refund;
    }
    
    modifier onlyDelegate(address _delegate){
        require(delegate==_delegate, "You are not the delegate.");
        _;
    }

    function burn(uint tokenID) public onlyDelegate(msg.sender) {
        _burn(tokenID);
        burntTokens++;
    }

    function _mint(uint numTokens) public onlyOwner{
        for (uint i = 0; i < numTokens; i++) {
            _mint(msg.sender, _tokenIds.current());
            _tokenIds.increment();
            
        }
    }
    
    /*function commitAssay(uint _auditID, bool decision, uint tokenID) public {
        require(ownerOf(tokenID)==msg.sender,"You are not an Asseyer!");
        //require(balanceOf(msg.sender)>0,"You are not an Asseyer!");
        //TODO check if already given assay for specific audit
        audits[_auditID].verdicts.push(decision);
        audits[_auditID].participatedAssayers.push(tokenID);
    }*/

    function commitAssay(uint _auditID, bytes32 saltedCommit, uint tokenID) public {
        require(ownerOf(tokenID)==msg.sender,"You are not an Asseyer!");
        require(audits[_auditID].scheme[tokenID].commit == bytes32(0), "Already commited!");
        audits[_auditID].scheme[tokenID].commit=saltedCommit;
        audits[_auditID].commits.push(tokenID);
        //audits[_auditID].revealedCommit[tokenID]=false; //not really needed
    }
    
    function revealAssay(uint _auditID, bool refund, bytes32 salt, uint tokenID) public {
        require(!audits[_auditID].scheme[tokenID].revealed,"Already revealed!");
        require(getSaltedHash(refund,salt)==audits[_auditID].scheme[tokenID].commit,"Revealed hash does not match commit");
        audits[_auditID].scheme[tokenID].revealed=true;
        audits[_auditID].verdicts.push(refund);
    }

    function assignDelegate(address _delegate) public onlyOwner{// assign delegate / minion address
        delegate=_delegate;
    }
    
    function getSaltedHash(bool refund,bytes32 salt) public view returns(bytes32){
        return keccak256(abi.encodePacked(address(this), refund, salt));
    }

    function updateOtherMarkets(address[] memory newSet) public {
        require(msg.sender ==owner() || msg.sender == delegate , "You dont have the permission.");
        otherMarkets = newSet;
    }

    function addOtherMarket(address newMarket) public{
        require(msg.sender ==owner() || msg.sender == delegate , "You dont have the permission.");
        otherMarkets.push(newMarket);
    }

    /** 
    * @dev allows a user to forward audit to external contract - reality.eth
    */
    function auditForward(uint _transactionID, uint _auditID, uint _days) auditLinked(_transactionID, _auditID) public payable {
        //require(audits[transactions[_transactionID].auditID].expiry < block.timestamp , "Time has not yet expired.");
