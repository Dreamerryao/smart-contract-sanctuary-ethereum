pragma solidity ^0.8.4;

import "ERC721A.sol";
import "Operators.sol";
import "IProp.sol";
import "INFT.sol";
import "Ownable.sol";

contract Staking is Ownable {
    INFT public nftContract;
    IProp public propToken;

    struct Staker {
        uint256[] tokenIds;
        mapping(uint256 => uint256) tokenStakingCoolDown;
        uint256 balance;
        uint256 lastUpdateTime;
    }

    /// @notice mapping of a staker to its wallet
    mapping(address => Staker) public stakers;

    /// @notice reward per second
    mapping(uint8 => uint) rates;

    /// @notice event emitted when a user has staked a nft
    event Staked(address owner, uint256 amount);

    /// @notice event emitted when a user has unstaked a nft
    event Unstaked(address owner, uint256 amount);

    /// @notice event emitted when a user claims reward
    event RewardPaid(address indexed user, uint256 reward);

    event RateChange(uint8 estateType, uint rate);

    constructor(
        INFT _nftContract,
        IProp _propToken)
    {
        nftContract = _nftContract;
        propToken = _propToken;

        _setRate(0, 100);
        _setRate(11, 500);
        _setRate(12, 700);
    }

    function setRate(uint8 estateType, uint rate) external onlyOwner {
        _setRate(estateType, rate);
    }

    function _setRate(uint8 estateType, uint rate) internal {
        rates[estateType] = rate;
        emit RateChange(estateType, rate);
    }

    function stake(uint256 tokenId) external {
        _stake(msg.sender, tokenId);
    }

    function stakeBatch(uint256[] memory tokenIds) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _stake(msg.sender, tokenIds[i]);
        }
    }

    function _updateReward(Staker storage staker) internal {
        uint ratesSum = 0;
        for (uint i = 0; i < staker.tokenIds.length; i++) {
            uint tokenId = staker.tokenIds[i];
            uint8 estateType = nftContract.getTokenInfo(tokenId);
            uint rate = rates[estateType];
            ratesSum += rate;
        }
        uint timeDelta = block.timestamp - staker.lastUpdateTime;
        uint income = ratesSum * timeDelta;
        staker.balance += income;
        staker.lastUpdateTime = block.timestamp;
    }

    function _stake(address _user, uint256 _tokenId) internal {
        require(
            nftContract.ownerOf(_tokenId) == _user,
            "user must be the owner of the token"
        );
        Staker storage staker = stakers[_user];
        if (staker.lastUpdateTime == 0) {
            staker.lastUpdateTime = block.timestamp;
        } else {
            _updateReward(staker);
        }

        staker.tokenIds.push(_tokenId);

        nftContract.transferFrom(_user, address(this), _tokenId);

        emit Staked(_user, _tokenId);
    }

    function claimReward(address _user) external {
        _claimReward(msg.sender);
    }

    function _claimReward(address _user) internal {
        Staker storage staker = stakers[_user];

        _updateReward(staker);
        require(staker.balance > 0 , "0 rewards yet");
        uint balance = staker.balance;
        staker.balance = 0;
        propToken.mint(_user, balance);

        emit RewardPaid(_user, balance);
    }

    function unstake(uint256 _tokenId) external {
        _claimReward(msg.sender);
        _unstake(msg.sender, _tokenId);
    }

    function _unstake(address _user, uint256 _tokenId) internal {
        Staker storage staker = stakers[_user];

        for (uint i = 0; i < staker.tokenIds.length; i++) {
            uint tokenId = staker.tokenIds[i];
            if (tokenId == _tokenId) {
                uint lastIndex = staker.tokenIds.length - 1;
                staker.tokenIds[i] = staker.tokenIds[lastIndex];
                staker.tokenIds.pop();

                nftContract.safeTransferFrom(address(this), _user, tokenId);
                emit Unstaked(_user, tokenId);

                return;
            }
        }
        require(false, "Token not found");
    }

    function _unstakeAll(address _user) internal {
        Staker storage staker = stakers[_user];

        for (uint i = 0; i < staker.tokenIds.length; i++) {
            uint tokenId = staker.tokenIds[i];
            nftContract.safeTransferFrom(address(this), _user, tokenId);
            emit Unstaked(_user, tokenId);
        }

        delete stakers[_user];
    }

}

