pragma solidity ^0.8.4;
import "./StringLib.sol";

library OracleUtils {
    
    function genRequestURL(uint16[] memory datetimeOfBirth, int16[] memory cityOfBirth) internal pure returns(string memory url){
        url = StringLib.concat("http://3.115.127.203/api/v1/calc-chart-data-degrees?houses=Placidus&zodiac=Krishnamurti&datetimeOfBirth=", convertDatetimeToString(datetimeOfBirth));
        url = StringLib.concat(StringLib.concat(url, "&cityOfBirth="), convertCityToString(cityOfBirth));
    }
    
    function convertDatetimeToString(uint16[] memory datetime) private pure returns (string memory res) {
        uint16 year = datetime[0]; uint16 month = datetime[1]; uint16 day = datetime[2]; uint16 hour = datetime[3]; uint16 minute = datetime[4]; uint16 second = datetime[5];
 
        res = string(abi.encodePacked(
            StringLib.uintToString(year), ",", StringLib.uintToString(month), "," , StringLib.uintToString(day), ",", StringLib.uintToString(hour), ",",  StringLib.uintToString(minute), ",", StringLib.uintToString(second))
        );
    }

    function convertCityToString(int16[] memory city) private pure returns(string memory res) {
        int16 longititude = city[0]; int16 latitude = city[1]; int16 timezone = city[2];
        
        res = string(abi.encodePacked(
            StringLib.intToString(longititude), ",", StringLib.intToString(latitude), ",",  StringLib.intToString(timezone)
        ));
    }

    
}

