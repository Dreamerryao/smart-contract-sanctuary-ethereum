pragma solidity ^0.8.4;


import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";
import "./libraries/SVGGenerator.sol";
import "./libraries/Base64.sol";
import "./libraries/AstroChartLib.sol";
import "./libraries/OracleUtils.sol";
import "./libraries/StringLib.sol";

contract AstroChart is ERC721, Ownable, ChainlinkClient {
    
    using Chainlink for Chainlink.Request;

    /***
     * ChainLink Block start
     */ 
    address private oracle;

    bytes32 private jobId;

    uint256 private fee;

    mapping (bytes32 => AstroChartResponse) private request2Response;

    struct AstroChartResponse {
        uint16[] cusps;
        uint16[] planets;
        bool exists;
    }

    /**
     * ChainLink Block end
     */

    // record tokenId to oracle request id
    mapping (uint256 => bytes32) public tokenId2OracleRequestId;

    /**
    Parami AstroChart Block End
     */

    event AstroChartBred(uint256 bredTokenId, uint256 fromTokenId, address bredTokenOwner, uint price);

    constructor()  ERC721("astro-chart", "ASTRO") {
        AstroChartLib._initNextTokenId();
       _transferOwnership(_msgSender());
    }

    ///
    /// setUpParams used by MetaAstro ERC-721 Token
    /// @param linkTokenAddress, Chainlink's ERC-20 token address,
    /// @param _oracle, oracle address
    /// @param _jobId, oracle jobId used to calculate astro data
    /// @param _feeInLink, fee that should paid to Oracle operator for each request
    /// @param _oracleGasFee, gas fee that operator use when submit data back to contract
    /// @param _salesStartTime, initial mint start time
    ///
    function setUpParams(
        address linkTokenAddress, address _oracle, bytes32 _jobId, 
        uint256 _feeInLink, uint256 _oracleGasFee, uint256 _salesStartTime
    ) external onlyOwner {
       oracle = _oracle;
       jobId = _jobId;
       fee = _feeInLink;
       setChainlinkToken(linkTokenAddress);
       AstroChartLib.setOracleGasFee(_oracleGasFee);
       AstroChartLib.setStartSalesTime(_salesStartTime);
    }

    /**
    withdraw initial deposit, only can be done by owner
    require amount <= initialDeposit, or else throw "withdraw amount must less than initialDeposit" as WAMLTI
     */
    function withdrawInitialDeposit(uint256 amount) external onlyOwner {
        require(amount <= AstroChartLib.initialDeposit(), "WAMLTI");
        payable(owner()).transfer(amount);
    }

    /**
    initial mint,
    require initialMintCount < INITIAL_MINT_LIMIT, or else throw "initial mint limit arrived" as "IMLA"
    require msg.value >= getPrice() + oracleGasFee, or else throw "initial mint should pay with initialMintPrice + oracleGasFee" as "IMSPWI+O"
    require salesStartTime > 0, or else throw sales start time should larger than zero as "SSTSLTZ"
    require libStorage().initialMintDate2Exists[dateToBytes32(datetimeOfBirth)] == false, or else throw initial mint date already exists as "IMDAE"
    _checkForDateAndCity's rule, ref _checkForDateAndCity's comment
     */
    function initialMint(address to, uint16[] calldata datetimeOfBirth, int16[] calldata cityOfBirth) 
    external payable {
        
        uint256 tokenId = AstroChartLib.initialMintDry(datetimeOfBirth, cityOfBirth);
        _safeMint(to, tokenId);

        //interactions
        sendRequestToOracle(tokenId, datetimeOfBirth, cityOfBirth);
    }

    function initalMintCount() external view returns ( uint256 ) {
        return AstroChartLib.initialMintCount();
    }

    function initialDeposit() external view returns ( uint256 ) {
        return AstroChartLib.initialDeposit();
    }

    function getPrice() external view returns ( uint256 ) {
        return AstroChartLib.getPrice();
    }

    /**
    checks:
      1. require ownerOf(tokenId) == msg.sender, guard only owner can change breed price, or else throw "Only owner can change breed price" as "OOCCBP"
    effects:
      1. update tokenId's breed price
     */
    function setBreedPrice(uint256 tokenId, uint256 breedPrice) external {
        //checks
        require(ownerOf(tokenId) == msg.sender, "OOCCBP");

        //effects
        AstroChartLib.setBreedPrice(tokenId, breedPrice);
    }

    /**
    require pendingWithdraws[msg.sender] > 0, or else throw "pending withdraw must large than zero" as PWMLTZ
     */
    function withdrawBreedFee() external {
        AstroChartLib.withdrawBreedFee();
    }

    /**
    checks:
        1. require msg.value >= token.breedPrice + oracleGasFee, or else throw "LTBP+O"
        2. require datetimeOfBirth[1] == astroDataOfParentToken.datetimeOfBirth[1], or else throw "month not equal" as "MNE"
        3. require datetimeOfBirth[2] == astroDataOfParentToken.datetimeOfBirth[2], or else throw "day not equal" as "DNE"
        4. require astroDataOfParentToken.alreadyBredCount < breedingLimitationOf(astroDataOfParentToken.generation), or else throw "beyond generation's breeding limitation" as "BGBL" 
    Effects:
        1. update pendinpendingWithdraws of fromToken's owner, as origin value + breedPrice
    Explainations:
    LTBP: less than breed price
     */
    function breedFrom(uint256 fromTokenId, uint16[] calldata datetimeOfBirth, int16[] calldata cityOfBirth) external payable {
        
        uint256 bredTokenId = AstroChartLib.breedFromDry(fromTokenId, datetimeOfBirth, cityOfBirth, ownerOf(fromTokenId));

        _safeMint(msg.sender, bredTokenId);

        // interactions
        sendRequestToOracle(bredTokenId, datetimeOfBirth, cityOfBirth);
        emit AstroChartBred(bredTokenId, fromTokenId, msg.sender, msg.value); 
    }

    //TODO: move to astro-data-consumer, do the test
    function breedingLimitationOf(uint32 generation) public pure returns (uint32 res) {
        return AstroChartLib.breedingLimitationOf(generation);
    }

    function getAstroArgsOf(uint256 tokenId) external view returns (AstroChartArgs memory) {
        return AstroChartLib.getAstroArgsOf(tokenId);
    }

    function getPendingWithdraw() external view returns (uint256) {
        return AstroChartLib.getPendingWithdraw();
    }

    /**
     Chainlink Relative Block start!!!!
     */

    

    function getLinkTokenAddress() external view returns (address) {
        return chainlinkTokenAddress();
    }
    /**
    Use to withdraw remain link from contract
     */
    function withdrawLink() external onlyOwner {
        LinkTokenInterface link = LinkTokenInterface(chainlinkTokenAddress());
        require(link.transfer(msg.sender, link.balanceOf(address(this))), "Unable to transfer");
    }

    /**
     * Create a Chainlink request to retrieve API response, find the target
     * data, then multiply by 1000000000000000000 (to remove decimal places from data).
     */
    function sendRequestToOracle(uint256 tokenId, uint16[] memory datetimeOfBirth, int16[] memory cityOfBirth) private returns(bytes32) {
        
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        
        string memory url = OracleUtils.genRequestURL(datetimeOfBirth, cityOfBirth);

        // Set the URL to perform the GET request on
        request.add("get", url);
        request.add("path_cusps", "cusps");
        request.add("path_planets", "planets");
        
        // Sends the request
        bytes32 requestId = sendChainlinkRequestTo(oracle, request, fee);

        tokenId2OracleRequestId[tokenId] = requestId;

        return requestId;
    }

    /**
     * Receive the response in the form of uint256
     */ 
    function fulfill(bytes32 _requestId, uint16[] calldata _cusps, uint16[] calldata _planets) external recordChainlinkFulfillment(_requestId)
    {
        request2Response[_requestId] = AstroChartResponse ({cusps: _cusps, planets: _planets, exists: true});
    }

    /**
    Chainlink Relative Block end!!!
     */

    /**
    ERC-721 tokenURI 
     */
    function tokenURI(uint256 tokenId) public view override returns (string memory) {

        bytes32 requestId = tokenId2OracleRequestId[tokenId];
        require(requestId != "0x0", "Token not minted");

        AstroChartResponse memory response = request2Response[requestId];

        string memory output = '<svg width="600" height="600" xmlns="http://www.w3.org/2000/svg"><text id="svg_6" font-size="10" y="311" x="301">generating...</text><svg>';

        if (response.exists) {
            AstroChartArgs memory args = AstroChartLib.getAstroArgsOf(tokenId);
            output = SVGGenerator.soGenSVG(response.cusps, response.planets, args.generation, tokenId);
        }

        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "Parami Astrology Chart #',
                        StringLib.uintToString(tokenId),
                        '", "description": "metaverse on-chain astro chart", "image": "data:image/svg+xml;base64,',
                        Base64.encode(bytes(output)),
                        '"}'
                    )
                )
            )
        );
        output = string(
            abi.encodePacked("data:application/json;base64,", json)
        );

        return output;
    }

    

    
}

