    function buyPresale(uint256 numberOfTokens, bytes32[] calldata _merkleProof) external payable {
    // Verify whitelist requirements
    require(whitelistMintEnabled, "The whitelist sale is not enabled!");
    require(!whitelistClaimed[msg.sender], "Address already claimed!");
    bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
    require(MerkleProof.verify(_merkleProof, merkleRoot, leaf), "Invalid proof!");
    require(numberOfTokens > 0 && numberOfTokens <= presaleMaxMintAmount, "Mint limit exceeded");
    require(msg.value >= price * numberOfTokens, "Insufficient funds!");
    require(numberOfTokens > 0 && numberOfTokens <= maxMintAmountPerTx, "Invalid mint amount!");
        // disallow contracts from buying
    require((!msg.sender.isContract() && msg.sender == tx.origin), "Contract buys not allowed");

    whitelistClaimed[msg.sender] = true;
    mint(msg.sender, numberOfTokens);
  }
   
    /// @notice Allows users to buy during presale, only whitelisted addresses may call this function.
    ///         Whitelisting is enforced by requiring a signature from the whitelistSigner address
    /// @dev Whitelist signing is performed off chain, via the cryptobatz website backend
    /// @param numberOfTokens number of NFTs to buy

    function buyPublic(uint256 numberOfTokens) external payable {
    require((totalSupply + numberOfTokens) <= maxSupply, "Purchase would exceed max supply");
    require(numberOfTokens <= maxSupply, "xxxxxxxxx");
    require(msg.value >= price * numberOfTokens, "Insufficient funds!");
    require(saleIsActive, "Sale must be active");
    require(numberOfTokens > 0 && numberOfTokens <= maxMintAmountPerTx, "Invalid mint amount!");
    // disallow contracts from buying
    require((!msg.sender.isContract() && msg.sender == tx.origin), "Contract buys not allowed");

    mint(msg.sender, numberOfTokens);
  }

