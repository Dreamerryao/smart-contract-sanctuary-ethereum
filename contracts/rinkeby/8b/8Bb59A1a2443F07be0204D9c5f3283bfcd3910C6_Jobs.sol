import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract RegularToken is ERC20, ERC20Burnable, Pausable, AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    uint _cap;

    constructor() ERC20("Regular Token", "REG") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _cap = 1000 * 1000000 * 10 ** decimals(); // 1B
        fundTreasury(10000 * 10 ** decimals());   // Initial Funding
    }

// ADMIN

    function drawTokens(address to, uint256 amount) public onlyRole(MINTER_ROLE) {
        if (!atCap(amount))
            _mint(to, amount);
        else 
            this.transfer(to,amount);
    }

    function sendFromTreasury(address to, uint256 amount) public onlyRole(MINTER_ROLE) {
        this.transfer(to,amount);
    }

    function fundTreasury(uint _amount) public onlyRole(MINTER_ROLE) {
        require(!atCap(_amount), "Capped!");
        _mint(address(this),_amount);
    }

    function pause() public onlyRole(MINTER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(MINTER_ROLE) {
        _unpause();
    }

// GETTERS

    function cap() public view returns (uint) {
        return _cap;
    }

    function treasuryBalance() public view returns (uint) {
        return balanceOf(address(this));
    }

// PRIVATE

    function atCap(uint _amount) private view returns (bool) { 
        return (totalSupply() + _amount >= _cap);
    }

// OVERRRIDE

    function _beforeTokenTransfer(address from, address to, uint256 amount) internal whenNotPaused override {
        super._beforeTokenTransfer(from, to, amount);
    }

// RINKEBY ONLY!!!! 

    function gimmeRinkeby() public {
        uint _faucetAmount = 140 * 10 ** decimals();
        this.transfer(msg.sender,_faucetAmount);
    }
}

