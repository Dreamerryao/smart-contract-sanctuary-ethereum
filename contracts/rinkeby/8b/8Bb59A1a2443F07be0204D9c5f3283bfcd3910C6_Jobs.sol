    function specialCountByCompany(uint _companyId) public view returns (uint) {
        return specials[_companyId].length;
    }

// Internal

    // check if a regular ID is special
    function isSpecial(uint _regTokenId) internal view returns (bool) {
        for (uint i = 0; i < numCompanies(); i++) {
            for (uint s = 0; s < specials[i].length;s++){
                if (specials[i][s] == _regTokenId)
                    return true;
            }
        }
        return false;
    }

    function getSpecialCompanyId(uint _tokenId) internal view returns (uint) {
        for (uint i = 0; i < numCompanies(); i++) {
            for (uint s = 0; s < specials[i].length;s++){
                if (specials[i][s] == _tokenId)
                    return i;
            }
        }
        revert("TokenId not special");
    }

    function getStartIndex(uint _companyId) internal view returns (uint) {
        return companies[_companyId].startIndex;
    }

}

