import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract IDO is Initializable, OwnableUpgradeable, PausableUpgradeable {
    enum Discount {
        FULL,   // 16.66%
        SEMI,   // 10%
        NO      // 0%
    }

    struct Purchase {
        // buy token (0 : UFARM, 1: USDT)
        uint8 buyToken;
        // total buyed amount with discount
        uint256 totalBuyedAmount;
        // buyed amount
        uint256 buyedAmount;
        // dollar value of buyed token
        uint256 buyedTokenUsdValue;
        // amount of sell token equivalent to buyedAmount deposited
        uint256 sellTokenAmount;
        // sell token conversation rate
        uint256 fixedSellTokenPrice;
        // sell token remaining
        uint256 amountLeft;
        // discount
        Discount discount;
        // to check insurance
        bool hasInsurance;
    }

    event Buyed(
        address ido,
        address buyer,
        uint256 amount,
        Discount discount,
        bool insurance
    );

    /// @notice purchases mapping
    mapping(address => Purchase) public purchases;

    /// @notice IDO token1 (UFARM)
    ERC20 public ido1;
    /// @notice no. of token1(in wei) per dollar
    uint256 public ido1Price;

    /// @notice IDO token2 (USDT)
    ERC20 public ido2;
    /// @notice no. of token1(in wei) per dollar
    uint256 public ido2Price;

    /// @notice default minimum alloction (in USD)
    uint256 public defaultMinAllocation;

    /// @notice default maximum allocation (in USD)
    uint256 public defaultMaxAllocation;

    /// @notice sell token
    address public sellToken;
    /// @notice no. of sell token(in wei) per dollar
    uint256 public sellTokenPricePremium;
    uint256 public sellTokenPriceNormal;

    /// @notice sale start time.
    uint256 public startTime;

    /// @notice sale end time
    uint256 public endTime;

    /// @notice sale purchase cap.
    uint256 public purchaseCap;

    /// @notice total amount raise.
    uint256 public totalRaised;

    /// @notice number of participants
    uint256 public participants;

    /// @notice number of participants having insurance
    uint256 public insuranceBuyers;

    /// @notice insurance wallet
    address public insuranceWallet;

    /// @notice insurance amount
    uint256 public insuranceAmount;

    // start - 9 sept (7am UTC)(12:30 pm IST)
    // IDO end - 12 sept (5 pm utc)
    // 14 sept(10am UTC) - 50% tge
    // 14 oct - 25% 1st unlock
    // 14 nov - 25% 2nd unlock

    uint256 public vestingStartTime;
    uint256 public constant CLIFF_DURATION = 147600;     // 41 hours
    uint256 public constant VESTING_DURATION = 60 days;     // 2 months
    uint256 public constant UNLOCK_DURATION  = 30 days;
    uint256 public constant TOTAL_EPOCHS = 2;
    // uint256 public constant CLIFF_DURATION = 600;
    // uint256 public constant VESTING_DURATION = 1200;
    // uint256 public constant UNLOCK_DURATION  = 600;
    // uint256 public constant TOTAL_EPOCHS = 2;


    /**
    * @notice construct the IDO contract.
    * @param _token1 IDO token address.
    * @param _token2 IDO token address.
    * @param _sellToken sell token address.
    * @param _purchaseCap maximum IDO purchase cap.
    * @param _startTime sale start time.
    * @param _duration sale epoch duration.
    * @param _minAllocation minimum public allocation.
    * @param _maxAllocation maximum public allocation.
    */

    function init(
        ERC20 _token1,
        ERC20 _token2,
        address _sellToken,
        uint256 _purchaseCap,
        uint256 _startTime,
        uint256 _duration,      // IDO duration
        uint256 _minAllocation,
        uint256 _maxAllocation,
        address _insuranceWallet,
        uint256 _insuranceAmount
    ) public initializer {
        require(_startTime >= block.timestamp, "IDO: INVALID_START_TIME");
        __Ownable_init();
        __Pausable_init();
        ido1 = _token1;
        ido2 = _token2;
        sellToken = _sellToken;
        purchaseCap = _purchaseCap;
        startTime = _startTime;
        endTime = startTime + _duration;
        defaultMinAllocation = _minAllocation;
        defaultMaxAllocation = _maxAllocation;
        insuranceWallet = _insuranceWallet;
        insuranceAmount = _insuranceAmount;
        vestingStartTime=  endTime;
    }

    function setSellTokenPrice(
        uint256 _sellTokenPricePremium,
        uint256 _sellTokenPriceNormal
    ) external onlyOwner {
        sellTokenPricePremium = _sellTokenPricePremium;
        sellTokenPriceNormal = _sellTokenPriceNormal;
    }

    /**
    * @dev validate sale duration.
    */

    function validateSale() internal view {
        require(block.timestamp >= startTime, "IDO: SALE_NOT_STARTED");
        require(block.timestamp < endTime, "IDO: SALE_ALREADY_ENDED");
    }

    /**
    * @notice used to buy allocate sale tokens.
    * @dev only whitelisted user can buy.
    * @dev sale automatically close after endTime reached.
    */

    function buy(
        uint8 idoToken,     // 0: UFARM, 1: USDT
        uint256 amount, 
        Discount discount, 
        bool insurance
    ) external whenNotPaused {
        address user = _msgSender();
        require(
            purchases[user].buyedAmount == 0,
            "IDO: ALREADY_BUYED"
        );
        require(idoToken == 0 || idoToken == 1, "IDO: INVALID TOKEN");

        ERC20 ido = idoToken == 0 ? ido1 : ido2;
        uint256 convRate = idoToken == 0 ? ido1Price : ido2Price;
        uint256 buyTokenUsdValue = getTokenUsdValue(convRate, amount, ido.decimals());

        uint256 totalBuyedAmount = amount;
        uint256 sellTokenPrice = sellTokenPriceNormal;
        if(discount == Discount.FULL && buyTokenUsdValue >= 200 * (10**18)) {
            totalBuyedAmount = 11666 * amount / 10000;
            sellTokenPrice = sellTokenPricePremium;
        }
        else if(discount == Discount.SEMI && buyTokenUsdValue >= 200 * (10**18)) {
            totalBuyedAmount = 110 * amount / 100;
            sellTokenPrice = sellTokenPricePremium;
        }
        
        uint256 totalBuyTokenUsdValue = getTokenUsdValue(convRate, totalBuyedAmount, ido.decimals());

        require(
            totalBuyTokenUsdValue >= defaultMinAllocation && totalBuyTokenUsdValue <= defaultMaxAllocation,
            "IDO: AMOUNT_RANGE_ERROR"
        );

        validateSale();
        
        require(totalRaised + totalBuyTokenUsdValue <= purchaseCap, "IDO: PURCHASE_LIMIT");

        totalRaised = totalRaised + totalBuyTokenUsdValue;
        participants++;

        if(insurance){
            // can buy insurance only using UFARM
            require(ido1.transferFrom(user, insuranceWallet, insuranceAmount), "INSURANCE: BUY_FAIL");
            insuranceBuyers++;
        }
        
        require(ido.transferFrom(user, address(this), totalBuyedAmount), "IDO: BUY_FAIL");
        
        uint256 sellTokenAmount = getSellTokensAmount(buyTokenUsdValue, sellTokenPrice);

        Purchase memory purchase = Purchase ({
            buyToken: idoToken,
            totalBuyedAmount: totalBuyedAmount,
            buyedAmount: amount,
            buyedTokenUsdValue: buyTokenUsdValue,
            sellTokenAmount: sellTokenAmount,
            fixedSellTokenPrice: sellTokenPrice,
            amountLeft: sellTokenAmount,
            discount: discount,
            hasInsurance: insurance
        });
        purchases[user] = purchase;

        emit Buyed( address(this), user, amount, discount, insurance);
    }

    function getTokenUsdValue(
        uint256 convRate,
        uint256 amount,
        uint8 decimals
    ) public view returns (uint256) {
        console.log("convRate: ", convRate, amount, decimals);
        return convRate * amount / (10 ** decimals);
    }

    function getSellTokensAmount(
        uint256 _usdValue,
        uint256 _sellTokenPrice
    ) public pure returns (uint256) {
        // considering usd has 18 decimals
        return _usdValue * (10 ** 18) / _sellTokenPrice;
    }

    function updateTokenPrice(
        uint256 _ido1Price,
        uint256 _ido2Price
    ) external onlyOwner {
        ido1Price = _ido1Price;
        ido2Price = _ido2Price;
    }

    /**
    * @notice sweep raised funds or rescue tokens.
    * @dev only current owner.
    */

    function sweep(ERC20 token, address to) external onlyOwner returns (bool) {
        uint256 balance = token.balanceOf(address(this));
        require(balance > 0 && block.timestamp >= endTime, "IDO: SWEEP_FAIL");
        token.transfer(to, balance);
        return true;
    }

    /**
    * @notice for security expect owner can pause/unpause the IDO
    * @dev only current owner
    */

    function pause() external onlyOwner {
        _pause();
    }

    /**
    * @notice for security expect owner can pause/unpause the IDO
    * @dev only current owner
    */

    function unpause() external onlyOwner {
        _unpause();
    }

    ///------------------------------ VESTING FUNCTIONS---------------------------------

    /// to redeem the vested tokens
    function redeem(
        uint8 tokenType     // 0: sell Token(Sportzchain), 1: USDT
    ) external {
        address account = _msgSender();
        require(purchases[account].amountLeft > 0, "TOKEN_NOT_VESTING");
        require(block.timestamp > vestingStartTime + CLIFF_DURATION, "IDO_RUNNING");
        require(tokenType == 1 ? purchases[account].hasInsurance : true, "NO_INSURANCE");

        uint256 unreleased = checkMaxRedeemableAmount(account);

        require(unreleased > 0, "NO_TOKENS_DUE");

        purchases[account].amountLeft -= unreleased;

        if(tokenType == 0) {
            // redeem SPN
            if(purchases[account].discount == Discount.FULL) {
                unreleased = 11666 * unreleased / 10000;  // 16.66% discount
            }
            else if(purchases[account].discount == Discount.SEMI) {
                unreleased = 110 * unreleased / 100;      // 10% discount
            }
            ERC20 sellTokenInstance = ERC20(sellToken);
            sellTokenInstance.transfer(_msgSender(), unreleased);
        }
        else {
            // redeem USDT
            ERC20 sellTokenInstance = ERC20(ido2);
            uint256 amount = getTokenUsdValue(purchases[account].fixedSellTokenPrice, unreleased, sellTokenInstance.decimals());
            console.log("AMOUNT: ", amount);
            sellTokenInstance.transfer(_msgSender(), amount);
        }
        // sellToken.mint(_msgSender(), sellToken.totalSupply() * unreleased / 10_000);
    }

    /// calculate how much a user can withdraw currently based on his position
    function checkMaxRedeemableAmount(
        address account
    ) public view returns (uint256) {
        uint256 releasedAmount = purchases[account].sellTokenAmount - purchases[account].amountLeft;
        return _getUnlockedAmount(account) - releasedAmount;
    }

    /// calculates the total amount of tokens vested till now
    function _getUnlockedAmount(
        address account
    ) private view returns (uint256) {
        uint256 totalAmount = purchases[account].sellTokenAmount;
        uint256 cliffAmount = 50 * totalAmount / 100;       /// 50% of total amount
        uint256 vestedAmount = totalAmount - cliffAmount;   /// 50% of total amount

        uint256 totalVestingDuration = CLIFF_DURATION + VESTING_DURATION;

        /// when cliff duration has not passed
        if (block.timestamp < vestingStartTime + CLIFF_DURATION) {
            return 0;
        } 
        /// when vesting is completed
        else if (block.timestamp >= vestingStartTime + totalVestingDuration) {
            return totalAmount;
        } 
        /// when cliff duration is over but vesting is not completed
        else {
            uint256 passedEpochs = (block.timestamp - vestingStartTime - CLIFF_DURATION) / UNLOCK_DURATION;
            return cliffAmount + (vestedAmount * passedEpochs / 2);
            // return cliffAmount + (vestedAmount * (block.timestamp - startTimestamp - CLIFF_DURATION) / VESTING_DURATION);
        }
    }
}

