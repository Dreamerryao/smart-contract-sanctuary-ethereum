    uint256[4] public /*constant*/ mintLimits = [200, 400, 600, 800];
    uint256[4] private /*constant*/ mintStartIdxes = [0, 200, 600, 1200];
    function _mintInternal(uint256 mintAmount) internal {
        for (uint256 i = 0; i < mintAmount; i++) {
            uint256 startCategoryIndex = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, mintedSupply + i))) % 3250;
            if (startCategoryIndex < 200) {
                startCategoryIndex = 0;
            } else if (startCategoryIndex < 400) {
                startCategoryIndex = 1;
            } else if (startCategoryIndex < 600) {
                startCategoryIndex = 2;
            } else {
                startCategoryIndex = 3;
            }
            uint256 currentCategoryIndex = startCategoryIndex;
            bool minted = false;
            do {
                if (mintedAmounts[currentCategoryIndex] < mintLimits[currentCategoryIndex]) {
                    _safeMint(msg.sender, mintStartIdxes[currentCategoryIndex] + mintedAmounts[currentCategoryIndex]);
                    mintedAmounts[currentCategoryIndex] += 1;
                    
                    minted = true;
                    break;
                }
                currentCategoryIndex = (currentCategoryIndex + 1) % 4;
            } while (currentCategoryIndex != startCategoryIndex);

            require(minted, "Mint Error");
        }
        mintedSupply += mintAmount;

        if (mintedSupply == maxSupply) {
            mintEnded = true;
        }
    }
    function mintWithETH(uint256 mintAmount) public payable {
