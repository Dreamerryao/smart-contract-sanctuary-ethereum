        require(randomWord == 0, "Random number has already came");
        require(block.number - randRequestBlockNumber > 50, "Not enough time passed yet"); // approximately 10 minutes

        randomWord = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, blockhash(block.number - 1))));
    }

    uint256[5] public winnersPlusOne = [0, 0, 0, 0, 0];
    uint256 public weekLPBalance = 0;
    function calculateWinners() external {
        require(randRequestBlockNumber != 0, "Random number hasn't been requested");
        require(randomWord != 0, "Random number hasn't been received");

        uint256[5] memory old_rands;

        uint256 limit = mintedAmounts[0] * 4 + mintedAmounts[1] * 3 + mintedAmounts[2] * 2 + mintedAmounts[3];
        uint256 mainRand = randomWord;
        for (uint256 i = 0; i < 5; i++) {
            uint256 rand = mainRand % (limit - i); // Fisher–Yates shuffle
            mainRand /= (limit - i); // use remaining bits for other randoms

            // check duplicate tickets
            for (uint256 j = 1; j <= i; j++) {
                if (rand >= old_rands[j - 1]) {
                    rand = (rand + 1) % limit;
                    if (rand == 0) { // %limit happened, look everything again
                        j = 0;
                    }
                }
            }
            old_rands[i] = rand;
            for (uint256 j = i; j >= 1; j--) {
                if (old_rands[j - 1] > old_rands[j]) {
                    uint256 tmp = old_rands[j - 1];
                    old_rands[j - 1] = old_rands[j];
                    old_rands[j] = tmp;
                } else {
                    break;
                }
            }

            if (rand < mintedAmounts[0] * 4) {
                winnersPlusOne[i] = (rand / 4) + 1;
            } else {
                rand -= mintedAmounts[0] * 4;

                if (rand < mintedAmounts[1] * 3) {
                    winnersPlusOne[i] = (rand / 3) + mintStartIdxes[1] + 1;
                } else {
                    rand -= mintedAmounts[1] * 3;

                    if (rand < mintedAmounts[2] * 2) {
                        winnersPlusOne[i] = (rand / 2) + mintStartIdxes[2] + 1;
                    } else {
                        winnersPlusOne[i] = (rand - mintedAmounts[2] * 2) + mintStartIdxes[3] + 1;
                    }
                }
            }
        }

        address _pairAddress = sushiSwapFactory.getPair(punksTokenAddress, wethTokenAddress);
        weekLPBalance = IERC20(_pairAddress).balanceOf(address(this));
        randRequestBlockNumber = 0;
        randomWord = 0;

        emit LotteryResult(lotteryWeek, winnersPlusOne[0] - 1, winnersPlusOne[1] - 1, winnersPlusOne[2] - 1, winnersPlusOne[3] - 1, winnersPlusOne[4] - 1);
    }

    function getReward(uint256 tokenID) external {
        require(ownerOf(tokenID) == msg.sender, "Not your token");

        bool found = false;
        for (uint256 i = 0; i < 5; i++) {
            if ((tokenID + 1) == winnersPlusOne[i]) {
                winnersPlusOne[i] = 0;

                uint256 divisor = (51 - lotteryWeek) * 5;
                uint256 your_share = weekLPBalance / divisor;

                address _pairAddress = sushiSwapFactory.getPair(punksTokenAddress, wethTokenAddress);
                IERC20(_pairAddress).safeTransfer(msg.sender, your_share);

                found = true;
                break;
            }
        }
        require(found, "This token didn't win this week");
    }

    function getWinnerAddresses() external view returns(address[5] memory) {
        address[5] memory result;
        for (uint256 i = 0; i < 5; i++) {
            if (winnersPlusOne[i] > 0) {
                result[i] = ownerOf(winnersPlusOne[i] - 1);
            }
        }
        return result;
    }





    // ADDING LIQUIDITY FUNCTION

    bool public liquidityAdded = false;
    function AddLiquidity() external {
        require(mintEnded, "Wait For Mint End");
        require(!liquidityAdded, "Liq. already added");

        IWETH(wethTokenAddress).deposit{ value: collectedPoolETH }();
        address token0Addr = punksTokenAddress;
        address token1Addr = wethTokenAddress;
        uint256 token0Amount = collectedPoolPUNKS;
        uint256 token1Amount = collectedPoolETH;

        address pairAddr = sushiSwapFactory.getPair(token0Addr, token1Addr);

        _approveToken(punksTokenAddress, address(sushiSwapRouter));
        _approveToken(wethTokenAddress, address(sushiSwapRouter));

        if (pairAddr == address(0)) { // pool is not created yet :)
            sushiSwapRouter.addLiquidity(
                token0Addr, token1Addr,
                token0Amount, token1Amount,
                token0Amount, token1Amount,
                address(this),
                block.timestamp + 60
            );
        } else { // pool is here :(
            IUniswapV2Pair pair = IUniswapV2Pair(pairAddr);
            (uint256 pairReserve0, uint256 pairReserve1, ) = pair.getReserves();

            {
                address pairToken0 = pair.token0();
                address pairToken1 = pair.token1();

                if (pairToken0 == token1Addr) { // swap inputs
                    address tmp = token0Addr;
                    token0Addr = token1Addr;
                    token1Addr = tmp;

                    uint256 tmpA = token0Amount;
                    token0Amount = token1Amount;
                    token1Amount = tmpA;
                }

                require(pairToken0 == token0Addr, "Faulty input");
                require(pairToken1 == token1Addr, "Faulty input");
            }

            uint256 token0Remaining = token0Amount;
            uint256 token1Remaining = token1Amount;
            if (pairReserve0 * token1Amount < pairReserve1 * token0Amount) {
                token0Remaining -= (pairReserve0 * token1Amount) / pairReserve1;
                token1Remaining = 0;
            } else {
                token1Remaining -= (pairReserve1 * token0Amount) / pairReserve0;
                token0Remaining = 0;
            }

            sushiSwapRouter.addLiquidity(
                token0Addr, token1Addr,
                token0Amount - token0Remaining, token1Amount - token1Remaining,
                (token0Amount - token0Remaining) * 9 / 10, (token1Amount - token1Remaining) * 9 / 10,
                address(this),
                block.timestamp + 60
            );

            if (token1Remaining > 0) {
                _addLiquidityUsingSingleToken(
                    token1Addr,
                    token1Remaining,
                    token0Addr,
                    token1Addr,
                    pair
                );
            } else if (token0Remaining > 0) {
                _addLiquidityUsingSingleToken(
                    token0Addr,
                    token0Remaining,
                    token0Addr,
                    token1Addr,
                    pair
                );
            }
        }

        // send 33% to multisig
        address _pairAddress = sushiSwapFactory.getPair(punksTokenAddress, wethTokenAddress);
        IERC20 lpToken = IERC20(_pairAddress);
        uint256 lpBalance = lpToken.balanceOf(address(this));
        lpToken.safeTransfer(multisigAddress, lpBalance / 3);

        liquidityAdded = true;
    }

    // If addLiq does not work, this is a backup function that sends all earnings to multisig
