        require(mintAmount <= singleMintLimit, "Tx mint limit exceeded");
        require(msg.value >= (mintPriceETHOnlyETH * mintAmount), "Not enough ethers");
        require((mintedSupply + mintAmount) <= maxSupply, "Mint limit reached");
        if (punksETHMintCount < 500) {
            require((mintedSupply + mintAmount + 500 - punksETHMintCount) <= maxSupply, "At least 500 tokens has to be minted with PUNKS&ETH");
        }

        collectedPoolETH += (mintPriceETHOnlyETH - mintOurETHShare) * mintAmount;
        _mintInternal(mintAmount);
    }

