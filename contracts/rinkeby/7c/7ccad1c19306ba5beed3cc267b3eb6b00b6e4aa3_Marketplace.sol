pragma solidity ^0.8.4;

import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";

contract Marketplace {
    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter private itemSold;
    CountersUpgradeable.Counter private itemIds;

    address payable marketplaceOwner;
    uint256 listingFee;

    struct MarketplaceItem {
        uint256 itemId;
        uint256 tokenId;
        address nftAddress;
        address payable owner;
        address payable seller;
        uint256 price;
        bool sold;
    }

    event MarketplaceItemCreated(
        uint256 itemId,
        uint256 tokenId,
        address nftAddress,
        address payable owner,
        address payable seller,
        uint256 price,
        bool sold
    );

    mapping(uint256 => MarketplaceItem) private mapMarketplaceItems;

    function initialize() public {
        marketplaceOwner = payable(msg.sender);
        listingFee = 0.01 ether;
    }

    /**
        return total items sold
     */
    function getItemSold() public view returns (uint256) {
        return itemSold.current();
    }

    /**
        return listing fee
     */
    function getListingFee() public view returns (uint256) {
        return listingFee;
    }

    /**
        - Desc: Publish item to market place 
        - Send listing fee from seller to contract
        - Transfer token from seller to market place
        - Params:   + nftAddress: nft contract address
                    + tokenUri: nft token uri
                    + price: price of item sale on market place
     */
    function createItem(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _price
    ) public payable {
        require(_price > 0, "Price must be more than 0");
        require(msg.value == listingFee, "Listing fee too low");

        itemIds.increment();
        uint256 itemId = itemIds.current();

        IERC721Upgradeable(_nftAddress).transferFrom(
            msg.sender,
            address(this),
            _tokenId
        );

        mapMarketplaceItems[itemId] = MarketplaceItem(
            itemId,
            _tokenId,
            _nftAddress,
            payable(address(0)),
            payable(msg.sender),
            _price,
            false
        );

        emit MarketplaceItemCreated(
            itemId,
            _tokenId,
            _nftAddress,
            payable(address(0)),
            payable(msg.sender),
            _price,
            false
        );
    }

    /***
        - Buy item on market place
        - Transfer item from market to buyer
        - Transfer listing fee from contract to contract owner address
        - Transfer ether from buyer to seller
     */
    function BuyItem(address _nftAddress, uint256 _tokenId) public payable {
        MarketplaceItem storage item = mapMarketplaceItems[_tokenId];
        require(item.price == msg.value, "Price must be equal nft price");

        itemSold.increment();
        item.sold = true;
        item.owner = payable(msg.sender);

        IERC721Upgradeable(_nftAddress).transferFrom(
            address(this),
            msg.sender,
            _tokenId
        ); // Transfer item from market to buyer
        item.seller.transfer(msg.value); // Transfer ether from buyer to seller
        marketplaceOwner.transfer(listingFee); // Transfer listing fee from contract to contract owner address
    }

    /**
        - Return unsold market place items
     */
    function fetchMarketItems() public view returns (MarketplaceItem[] memory) {
        uint256 unsoldItemSize = itemIds.current() - itemSold.current();
        if (unsoldItemSize == 0) {
            return new MarketplaceItem[](0);
        }
        MarketplaceItem[] memory unsoldItems = new MarketplaceItem[](
            unsoldItemSize
        );

        for (uint256 i = 0; i < itemIds.current(); i++) {
            MarketplaceItem storage item = mapMarketplaceItems[i + 1];
            if (!item.sold) {
                unsoldItems[i] = item;
            }
        }
        return unsoldItems;
    }

    // /**
    //     - Return list item sender bought
    //  */
    function fetchMyBoughtItems()
        public
        view
        returns (MarketplaceItem[] memory)
    {
        uint256 totalItemBought = 0;
        for (uint256 i = 0; i < itemIds.current(); i++) {
            MarketplaceItem memory item = mapMarketplaceItems[i + 1];
            if (item.owner == msg.sender) {
                totalItemBought += 1;
            }
        }

        MarketplaceItem[] memory totalItemsBought = new MarketplaceItem[](
            totalItemBought
        );
        for (uint256 i = 0; i < itemIds.current(); i++) {
            MarketplaceItem memory item = mapMarketplaceItems[i + 1];
            if (item.owner == msg.sender) {
                totalItemsBought[i] = item;
            }
        }
        return totalItemsBought;
    }

    function fetchItemsCreated()
        public
        view
        returns (MarketplaceItem[] memory)
    {
        uint256 totalCreatedItems = 0;
        for (uint256 i = 0; i < itemIds.current(); i++) {
            MarketplaceItem memory item = mapMarketplaceItems[i + 1];
            if (item.seller == msg.sender) {
                totalCreatedItems += 1;
            }
        }

        MarketplaceItem[] memory results = new MarketplaceItem[](
            totalCreatedItems
        );
        for (uint256 i = 0; i < totalCreatedItems; i++) {
            MarketplaceItem memory item = mapMarketplaceItems[i + 1];
            if (item.seller == msg.sender) {
                results[i] = item;
            }
        }
        return results;
    }
}

