pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "../lib/Blacklistable.sol";
import "../lib/Claimable.sol";
import "hardhat/console.sol";

struct OmegaKeys {
	bool active;
	bool burned; // this field is for control the Burn/Unburn process
	uint256 voxoId;
	address[] ownersKey;
}

struct UserKey {
	bool active;
	uint256[] voxoids;
}

//! TODO: after the first review, i will work in found Gas Efficient way to handle all data

/**
 * @title Omega Key Game Smart Contract
 * @dev All Method to permit handle onchain the Omega Key Game from
 * @dev https://voxodeus.notion.site/Omega-Key-Game-143fcf958a254295a5a1e2b344867fca
 */

contract OKG is Ownable, Pausable, Blacklistable, Claimable {
	using SafeMath for uint256;
	using Address for address;

	// Mapping for handle the Omega Keys
	mapping(uint256 => OmegaKeys) public omegaKeys;
	// Mapping for handle the UserKey
	mapping(address => UserKey) public userKey;
	// Event for Release Omega Key Game
	event ReleaseOmegaKey(uint256[] OmegaKey);
	// Event when a User Register the ownership of each Omega Key
	event RegisterOmegaKey(address indexed Owner, uint256 indexed OmegaKey);
	// Event when burnKey is called
	event BurnOmegaKey(address indexed Winner, uint256 indexed OmegaKey, uint256 indexed Tier, address[] allOwners);
	// Event when unBurnKey is called
	event UnBurnOmegaKey(address[] indexed oldOwners, uint256 indexed OmegaKey);
	// Event when Receive Amount of Ethereum for Rewards of Omega Key Game
	event Rewards(address indexed Winner, uint256 indexed Tier, uint256 Amount);
	// Address of the NFT contract
	address public nftContract;
	// Diamond Reward
	bool public diamondReward;
	// Gold Reward
	bool public goldReward;
	// Silver Reward
	bool public silverReward;
	// Bronze Reward
	bool public bronzeReward;


    constructor(address NFT_contract_address) {
    	nftContract = NFT_contract_address;
    }

	modifier existKey(uint256 _keyId) {
		require(isKey(_keyId), "VOXO: Omega Key don't exist");
		_;
	}

	modifier Registered(uint256 _keyId) {
		require(!isRegistered(_keyId), "VOXO: Omega Key already registered");
		_;
	}

	/**
     * @dev Implementation / Instance of paused methods() in the ERC20.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC20Pausable}.
     */
    function pause(bool status) public onlyOwner() {
        if (status) {
            _pause();
        } else {
            _unpause();
        }
    }

	/**
	 * @dev Method for Release (storage) of Voxo ids, who assign the Omega Key API assigned
	 * @param _id The Array of id of the voxo
	 */
	function release(uint256[] calldata _id) external
	 	onlyOwner()
		whenNotPaused()
	{
		for (uint i = 0; i < _id.length; i++) {
			require((_id[i] != uint(0)), "VOXO: Voxo Id Zero is wrong");
			require(!isKey(_id[i]), "VOXO: Omega Key exist");
		}

		for (uint i = 0; i < _id.length; i++) {
			OmegaKeys memory key = OmegaKeys({
				active: true,
				burned: false,
				voxoId: _id[i],
				ownersKey: new address[](0)
			});
			omegaKeys[_id[i]] = key;
		}
		// Emit Event when Release All Omega Key Game
		emit ReleaseOmegaKey(_id);
	}

	/**
	 * @dev Method for Register the ownership of each Omega Key
	 * @param _voxoId The Voxo Id of the user
	 */
	function register(uint256 _voxoId) public
		existKey(_voxoId)
		Registered(_voxoId)
		whenNotPaused()
	{
		IERC721 _token = IERC721(address(nftContract));
		require(_token.ownerOf(_voxoId) == _msgSender(), "VOXO: You are not the owner of this Omega Key");

		// register the ownership of the Omega Key
		omegaKeys[_voxoId].ownersKey.push(_msgSender());
		// register the winner of the Omega Key
		if (userKey[_msgSender()].active) {
			userKey[_msgSender()].voxoids.push(_voxoId);
		} else {
			userKey[_msgSender()].active = true;
			userKey[_msgSender()].voxoids.push(_voxoId);
		}
		// Emit Event when Register Each Omega Key Game
		emit RegisterOmegaKey(_msgSender(), _voxoId);
	}

	/**
	 * @dev Method for verify if _voxoId (Omega Key) Exist
	 * @param _voxoId The Voxo Id of the user
	 * @return True if the Omega Key exist, False if not
	 */
	function isKey(uint256 _voxoId) public view returns (bool) {
		return omegaKeys[_voxoId].active;
	}

	/**
	 * @dev Method for verify if _voxoId (Omega Key) is registered
	 * @param _voxoId The Voxo Id of the user
	 * @return registered True if the Omega Key is registered, False if not
	 */
	function isRegistered(uint256 _voxoId) internal view existKey(_voxoId) returns (bool registered) {
		registered = false;
		for (uint i = 0; i < omegaKeys[_voxoId].ownersKey.length; i++) {
			if (omegaKeys[_voxoId].ownersKey[i] == _msgSender()) {
				registered = true;
			}
		}
		return registered;
	}

	/**
	 * @dev Method to burn Omega Key
	 * @dev When burn the Omega Key, the method don't check again the ownership of the Voxo Id
	 * @dev Because is possible the Winner sell the Voxo after to Claim the Omega Key
	 * @param _keys Number of keys to burn
	 */
	function burnKey(uint256 _keys) private {
		for (uint i = 0; i < _keys; i++) {
			uint256 omegaKey = userKey[_msgSender()].voxoids[i];
			omegaKeys[omegaKey].active = false;
			omegaKeys[omegaKey].burned = true;
			address[] memory allOwners = omegaKeys[omegaKey].ownersKey;
			emit BurnOmegaKey(_msgSender(), omegaKey, _keys, allOwners);
		}
		delete userKey[_msgSender()];
	}

	/**
	 * @dev Method to unBurn Omega Key
	 * @param _omegaKey Number of keys to burn
	 */
	//! TODO: Is important remember that when unBurn the Omega Key, we don't get back the amount of the Reward
	//! TODO: Because is this occur after to the Winner Claim the Reward
	function unBurnKey(uint256 _omegaKey) public onlyOwner() whenNotPaused() {
		require ((omegaKeys[_omegaKey].burned && !omegaKeys[_omegaKey].active), "VOXO: Omega Key is not burned");
		address[] memory oldOwner = omegaKeys[_omegaKey].ownersKey;
		OmegaKeys memory key = OmegaKeys({
			active: true,
			burned: false,
			voxoId: _omegaKey,
			ownersKey: new address[](0)
		});
		omegaKeys[_omegaKey] = key;
		emit UnBurnOmegaKey(oldOwner, _omegaKey);
	}

	/**
	 * @dev Method for the Winner of the Omega Key Game, Claim the rewards
	 * @dev The UserKey must be have at least the amount of Keys to claim the rewards
	 * @dev Three for Tier Bronze, Four for Tier Silver, Six for Tier Gold and Eight for Tier Diamond
	 * @param price_tier The Amount of the Reward, that the Winner will claim
	 */
	function claim(uint256 price_tier) public whenNotPaused() {
		require(userKey[_msgSender()].active, "VOXO: Winner not exist");
		require(((price_tier == uint256(16)) || (price_tier == uint256(33)) || (price_tier == uint256(66)) || (price_tier == uint256(135))), "VOXO: Reward not exist");

		// Get the amount of the rewards
		uint256 amount;
		uint256 keys;
		// Tier Bronze
		if (price_tier == uint256(16)) {
			require(!bronzeReward, "VOXO: Bronze Reward already claimed");
			require(userKey[_msgSender()].voxoids.length >= 3, "VOXO: don't have enough keys for this Tier");
			amount = 16 ether;
			keys = 3;
			burnKey(keys);
			bronzeReward = true;
		}
		// Tier Silver
		if (price_tier == uint256(33))  {
			require(!silverReward, "VOXO: Silver Reward already claimed");
			require(userKey[_msgSender()].voxoids.length >= 4, "VOXO: don't have enough keys for this Tier");
			amount = 33 ether;
			keys = 4;
			burnKey(keys);
			silverReward = true;
		}
		// Tier Gold
		if (price_tier == uint256(66))  {
			require(!goldReward, "VOXO: Gold Reward already claimed");
			require(userKey[_msgSender()].voxoids.length >= 6, "VOXO: don't have enough keys for this Tier");
			amount = 66 ether;
			keys = 6;
			burnKey(keys);
			goldReward = true;
		}
		// Tier Diamond
		if (price_tier == uint256(135)) {
			require(!diamondReward, "VOXO: Diamond Reward already claimed");
			require(userKey[_msgSender()].voxoids.length >= 8, "VOXO: don't have enough keys for this Tier");
			amount = 135 ether;
			keys = 8;
			burnKey(keys);
			diamondReward = true;
		}
		// Send the amount of the rewards to the winner
		claimValues(address(0), _msgSender(), amount);
		// Emit Event when Send Reward to the Winner
		emit Rewards(_msgSender(), keys, amount);
	}

}

