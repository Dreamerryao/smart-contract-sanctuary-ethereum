pragma solidity ^0.8.0;

import "./interfaces/IStampingDatabaseDeployer.sol";

import "./StampingDatabase.sol";

contract StampingDatabaseDeployer is IStampingDatabaseDeployer {
    struct Parameters {
        address hub;
        address token;
    }

    /// @inheritdoc IStampingDatabaseDeployer
    Parameters public override parameters;

    /// @dev Deploys a database with the given parameters by transiently setting the parameters storage slot and then
    /// clearing it after deploying the database.
    /// @param hub The contract address of the Stamping Hub
    /// @param token The address of token for database
    function deploy(
        address hub,
        address token
    ) internal returns (address database) {
        parameters = Parameters({hub: hub, token: token});
        database = address(new StampingDatabase{salt: keccak256(abi.encode(token))}());
        delete parameters;
    }
}

