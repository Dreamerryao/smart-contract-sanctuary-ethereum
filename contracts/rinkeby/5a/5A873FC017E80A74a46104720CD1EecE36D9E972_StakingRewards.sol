pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";


import "hardhat/console.sol";

import "./RewardsDistributionRecipient.sol";
import "./DefiSpotToken.sol";

contract StakingRewards is RewardsDistributionRecipient, ReentrancyGuardUpgradeable {
    using SafeERC20Upgradeable for DefiSpotToken;

    /* ========== STATE VARIABLES ========== */

    DefiSpotToken public SPOT;

    // @TODO make sure all variables are private
    uint256 private totalStakedTokens;

    /* -------- Revenue -------- */
    // revenue rewards to airdrop every second.
    uint256 private revenueRewardPerSecond;

    // lastTimeRevenueRewardUpdated is the unix timestamp indicating the marker in current reward period OR the end of the current reward period.
    uint256 private lastTimeRevenueRewardUpdated;

    // revenueRewardPeriodEndTime is the timestamp for when the current revenue reward period ends.
    // It is defaulted to the current block timestamp when the contract is initiated.
    uint256 private revenueRewardPeriodEndTime;

    // @TODO verify the purpose of this variable
    // The rewardOdometer is the total amount of rewards given PER STAKED TOKEN in the history of the contract
    uint256 public revenueRewardOdometer;

    // allRevenueRewardIn holds all the revenue reward added to the contract.
    uint256 public allRevenueRewardIn;

    // allRevenueRewardOut holds all revenue reward but only for the users that have executed the contract. i.e. updateRewards(address)
    uint256 public allRevenueRewardOut;

    // @TODO Verify the purpose of this mapping data type
    mapping(address => uint256) public userRevenueRewardOdometer;
    
    mapping(address => uint256) public allUserRewards;
    

    // revenueRewards are the new revenue rewards to be paid out since the last payout.
    mapping(address => uint256) public revenueRewardsToPayout;

    /* -------- Minted -------- */
    uint256 public mintedRewardBalance;
    uint256 public lastTimeMintedRewardUpdated;
    uint256 public mintedRewardOdometer;
    // allMintedReward is the total SPOT that has been allocated for stakers. This includes SPOT that has been minted and
    // distributed AND the earned SPOT that has not been minted yet.
    uint256 public allEarnedMintedReward;
    // allMintedRewardPaid is the total mintedReward SPOT that has been minted and transferred to a Staker.
    uint256 public allMintedRewardPaid;
    uint256 public mintedRewardPerSecond;
    // uint256 public maxSupplyOfMintedReward;
    mapping(address => uint256) public userMintedRewardOdometer;
    // mintedRewards are the new minting rewards to be paid out since the last payout.
    mapping(address => uint256) public mintedRewardsToPayout;

    mapping(address => uint256) public userStakingTimestamp;

    mapping(address => WithdrawalRequest) public readyToWithdraw;

    mapping(address => uint256) private balances;

    uint256 public withdrawWaitTime;
    uint256 public penaltyFeesCollected;
    uint256 public penaltyDays;

    uint256 private constant OSCAR_TO_SPOT_RATIO =  1e18;
    //@TODO Rene made the max rate up. This needs to change!
    uint256 private constant MAX_MINTED_REWARD_RATE = 2 * OSCAR_TO_SPOT_RATIO; // OSCARS PER SECOND

    uint256 private constant FACTOR = 1 days;

    struct WithdrawalRequest {
        uint256 amount;
        uint256 time;
    }

    function initialize(
        address _rewardsDistribution, 
        address _spotAddress
    )
        public
        initializer
        onlyInitializing
    {
        // @TODO Confirm with Keiths and Oscar that rewards duration will be always one day.
        // @TODO Confirm that if rewards duration changes, then the code is not affected.
        SPOT = DefiSpotToken(_spotAddress);
        rewardsDistributionAddress = _rewardsDistribution;
        revenueRewardPeriodEndTime = block.timestamp;
        lastTimeRevenueRewardUpdated = block.timestamp;
        lastTimeMintedRewardUpdated = block.timestamp;
        // @ TODO add a function to modify the withdraw wait time.
        withdrawWaitTime = 10 days; // We added this
        // All the mintable SPOT that hasn't been minted. All goes to rewards.
        // maxSupplyOfMintedReward = SPOT.cap() - SPOT.totalSupply();
    }

    /* ========== EVENTS ========== */
    event LogRevenueRewardAdded(uint256 reward, uint256 rewardsDuration);
    event LogStaked(address indexed user, uint256 amount);
    event LogWithdrawn(address indexed user, uint256 amountToWithdraw, uint256 penaltyAmount);
    event LogWithdrawRequested(address indexed user, uint256 amount);
    event LogRewardPaid(address indexed user, uint256 revenueReward, uint256 mintedReward);
    event LogPenaltiesWithdrawn(address indexed user, uint256 penaltyFeesCollected);
    event LogPenaltyDaysUpdated(uint256 _days);
    event LogAddTokenReward(uint256 amount);

    /* ========== VIEWS ========== */
    // remove some duplicate views
    // Varaible should be private then...
    function getWithdrawAvailableTime() external view returns (uint256) {
        return readyToWithdraw[msg.sender].time;
    }

    function getWithdrawRequestedAmount() external view returns (uint256) {
        return readyToWithdraw[msg.sender].amount;
    }

    // @TODO change variable
    function getTotalStakedTokens() public view returns (uint256) {
        return totalStakedTokens;
    }
    // @TODO verify if we need to change to msg.sender, so only the caller can check its balance.
    function balanceOf(address _account) external view returns (uint256) {
        return balances[_account];
    }

    function lastTimeRevenueRewardApplicable(uint256 _periodFinish) public view returns (uint256) {
        return MathUpgradeable.min(block.timestamp, _periodFinish);
    }

    function getRevenueRewardsPerSecond() external view returns (uint) {
        return revenueRewardPerSecond;
    }

    /*
    * @notice This function will return an updated odometer reading. The odometer is the rewards per staked.
    */
    function calculateNewRevenueRewardOdometerAmount(
        uint256 _lastTimeRevenueRewardApplicable
        )
        internal
        view
        returns (uint256)
    {
        uint256 _stakedTokens = getTotalStakedTokens();

        if (_stakedTokens == 0) {
            return revenueRewardOdometer;
        }
        // durationSinceLastTimeRewardUpdated are the seconds between the last time the rewards were updated and the
        // end of the reward period. (or if the period hasn't ended then the current block time)
        
        uint256 durationSinceLastTimeRewardUpdated = _lastTimeRevenueRewardApplicable - lastTimeRevenueRewardUpdated;
        uint256 rewardsEarnedSinceLastTimeRewardUpdated = (durationSinceLastTimeRewardUpdated * revenueRewardPerSecond) / FACTOR;
        uint256 newRewardsEarnedPerToken = (rewardsEarnedSinceLastTimeRewardUpdated * OSCAR_TO_SPOT_RATIO) / _stakedTokens;
        // rewardOdometer is the value since last time the rewards were updated.
        return revenueRewardOdometer + newRewardsEarnedPerToken;
    }

    /*
    * @notice This function will return an updated odometer reading. i.e The total rewards generated per staked token up
    * until now. The odometer is the Rewards per Staked
    */
    function calculateNewMintedRewardOdometerAmount(
        uint256 _availableSpotToMint,
        uint256 _totalMintedRewardsEarnedSinceLastTimeUpdated
    )
        internal
        view
        returns (uint256)
    {
        uint256 _stakedTokens = getTotalStakedTokens();
        
        if (_stakedTokens == 0){
            return mintedRewardOdometer;
        }
        // If all the SPOT has been allocated then the odometer stops increasing. (i.e rewards/staked reaches the max  )
        if(_availableSpotToMint == 0){
            return mintedRewardOdometer;
        }
        // @TODO Test the scenerio when there is very little minting left. We need to make sure the stakers do not accumulate
        //       rewards that are not mintable.
        uint256 newRewardEarnedPerStake = _totalMintedRewardsEarnedSinceLastTimeUpdated * OSCAR_TO_SPOT_RATIO / _stakedTokens;
        return mintedRewardOdometer + newRewardEarnedPerStake;
    }


    /*
    * @notice The total rewards per staked token the user has earned so far.
    */
    function calculateNewUserRevenueRewardToPayout(
        uint256 _revenueRewardOdometer, //updated
        address _account
    )
        public
        view
        returns (uint256)
    {
        uint256 _userRevenueRewardOdometer = userRevenueRewardOdometer[_account];
        uint256 _userRevenueRewardToPayout = revenueRewardsToPayout[_account];
        uint256 _userBalance = balances[_account];
        
        uint256 rewardPerStakeDiff = _revenueRewardOdometer - _userRevenueRewardOdometer;
        
        return (_userBalance * rewardPerStakeDiff / OSCAR_TO_SPOT_RATIO) + _userRevenueRewardToPayout;
    }

    // @notice No reward will be given if all the SPOT has either been minted or unminted SPOT has been already reserved.
    // This happens with  _newMintedRewardOdometer and _userMintedRewardOdometer have the same value.
    
    function calculateUserMintedRewardForCurrentPeriod(
        uint256 _mintedRewardOdometer,
        address _account
    )
        public
        view
        returns (uint256)
    {
        
        uint256 _userMintedRewardOdometer =  userMintedRewardOdometer[_account];
        uint256 userBalance = balances[_account];
        uint256 rewardPerStakeDiff = _mintedRewardOdometer - _userMintedRewardOdometer;

        return userBalance * rewardPerStakeDiff / OSCAR_TO_SPOT_RATIO;
    }

    // @TODO What if the staking pool was 0 before this new stake was added?
    function calculateTotalMintedRewardsEarnedSinceLastTimeUpdated(
        uint256 _availableSpotToMint
    )
    public
    view
    returns (uint256)
    {
        // There is no rewards if no one is staking
        if(getTotalStakedTokens() == 0){
            return 0;
        }
        uint256 durationSinceLastTimeMintedRewardUpdated = block.timestamp - lastTimeMintedRewardUpdated;
        uint256 mintedRewardsEarnedSinceLastTimeRewardUpdated = durationSinceLastTimeMintedRewardUpdated * mintedRewardPerSecond;
        if(mintedRewardsEarnedSinceLastTimeRewardUpdated > _availableSpotToMint){
            return _availableSpotToMint;
        }
        return mintedRewardsEarnedSinceLastTimeRewardUpdated;
    }


    /* ========== MUTATIVE FUNCTIONS ========== */

    function stake(uint256 amountInOscars) external nonReentrant returns(bool) {
        require(amountInOscars > 0, "Cannot stake 0");
        // @TODO We should not allow 
        require(updateRevenueReward(msg.sender), " Updating revenue reward failed!");
        require(updateMintedReward(msg.sender), " Updating minted reward failed!");

        totalStakedTokens += amountInOscars;
        balances[msg.sender] += amountInOscars;
        
        userStakingTimestamp[msg.sender] = block.timestamp;

        require(SPOT.transferFrom(msg.sender, address(this), amountInOscars), "stake transfer failed!");

        emit LogStaked(msg.sender, amountInOscars);
        return true;
    }

    //@notice This function will submit a request from the staker to withdraw an amount from their staked [email protected]
    //        The user can withdraw the amount specified anytime after the request wait time is over.
    function requestToWithdraw(uint256 amount) external nonReentrant {

        require(amount > 0, "Cannot request 0");
        require(amount <= balances[msg.sender], "Not enough balance to withdraw!");
        //Make sure a request is not pending. both amount and time need to be zero.
        require(readyToWithdraw[msg.sender].amount == 0 && readyToWithdraw[msg.sender].time == 0, "Withdraw request pending!");

        require(updateRevenueReward(msg.sender), "Updating revenue reward failed!");
        require(updateMintedReward(msg.sender), "Updating minted reward failed!");

        balances[msg.sender] -= amount;

        totalStakedTokens -= amount;

        readyToWithdraw[msg.sender] = WithdrawalRequest(amount, block.timestamp + withdrawWaitTime);
        
        emit LogWithdrawRequested(msg.sender, amount);
    }

    // @notice A cancel can happen during and after a pending request.
    function cancelWithdraw() external nonReentrant {
        // @todo we need to be sure the user doesnt get rewards during the pending stage.
        require(readyToWithdraw[msg.sender].time > 0, "No request pending");
        require(readyToWithdraw[msg.sender].amount > 0, "No amount available");
        // Update the state variables

        balances[msg.sender] += readyToWithdraw[msg.sender].amount;
        totalStakedTokens += readyToWithdraw[msg.sender].amount;

        readyToWithdraw[msg.sender].time = 0;
        readyToWithdraw[msg.sender].amount = 0;
        
    }

    // @notice Can only withdraw if the amount has been requested.
    function withdraw() external nonReentrant {
        uint256 amountToWithdraw = readyToWithdraw[msg.sender].amount;

        require(amountToWithdraw > 0, "Should be greater than zero!");

        require(
            readyToWithdraw[msg.sender].time <= block.timestamp,
            "withdraw wait time not reached"
        );

        require(updateRevenueReward(msg.sender), "Updating revenue reward failed!");
        require(updateMintedReward(msg.sender), "Updating minted reward failed!");
        
        uint256 secondsSinceStaking = block.timestamp - userStakingTimestamp[msg.sender];
        uint256 daysSinceStaking = secondsSinceStaking / 1 days;
        uint256 penaltyAmount = 0;

        if (daysSinceStaking < penaltyDays) {
            penaltyAmount = (amountToWithdraw * (penaltyDays - daysSinceStaking)) / 100;
            amountToWithdraw -= penaltyAmount;
            penaltyFeesCollected += penaltyAmount;
        }
        
        readyToWithdraw[msg.sender].amount = 0;
        readyToWithdraw[msg.sender].time = 0;

        require(SPOT.transfer(msg.sender, amountToWithdraw), "withdraw funds failed!");

        emit LogWithdrawn(msg.sender, amountToWithdraw, penaltyAmount);
    }


    // @TODO at the moment there is no way to withdraw from the contract's balance except penalty fees.
    // @TODO Remove tx.origin as receiver of the penalty fees.
    function withdrawAllPenaltyFeesCollected() external onlyRewardsDistribution {
        require(penaltyFeesCollected > 0, "No penalty fees to collect");
        uint256 tempPenalty = penaltyFeesCollected;
        penaltyFeesCollected = 0;
        //validate require
        require(SPOT.transfer(rewardsDistributionAddress, tempPenalty), "withdraw penalty fees failed!");
        emit LogPenaltiesWithdrawn(rewardsDistributionAddress, tempPenalty);
    }

    // @notice
    //@TODO make sure that we need the nonReentrant modifier
    function getReward() external nonReentrant {
        require(updateRevenueReward(msg.sender), "Updating revenue reward failed!");
        require(updateMintedReward(msg.sender), "Updating minted reward failed!");
        
        uint256 _userRevenueRewardToPayout = revenueRewardsToPayout[msg.sender];
        
        if (_userRevenueRewardToPayout > 0) {
            allRevenueRewardOut += revenueRewardsToPayout[msg.sender];
            
            allUserRewards[msg.sender] += revenueRewardsToPayout[msg.sender];
            revenueRewardsToPayout[msg.sender] = 0;
            //require(SPOT.transfer(msg.sender, _userRevenueRewardToPayout), "get rewards failed!");
        }
        
        uint256 _userMintedRewardsToPayout = mintedRewardsToPayout[msg.sender];
        
        if (_userMintedRewardsToPayout > 0) {
            require(_userMintedRewardsToPayout <= mintedRewardBalance, "user rewards to payout is less than balance.");
            allMintedRewardPaid += _userMintedRewardsToPayout;
            allUserRewards[msg.sender] += _userMintedRewardsToPayout;
            mintedRewardsToPayout[msg.sender] = 0;
            mintedRewardBalance -= _userMintedRewardsToPayout;
            //require(SPOT.transfer(msg.sender, _userMintedRewardsToPayout), "minted transfer failed!");
        }
        uint256 totalReward = _userRevenueRewardToPayout + _userMintedRewardsToPayout;

        require(SPOT.transfer(msg.sender, totalReward), "get reward failed!");

        emit LogRewardPaid(msg.sender, _userRevenueRewardToPayout, _userMintedRewardsToPayout);
    }
    /* ========== RESTRICTED FUNCTIONS ========== */
    
    // @notice Each day represents 1 percent
    function setPenaltyDays(uint256 _days ) external onlyRewardsDistribution {
        //@TODO What if admin wants to disable penalties?  _days = 0?
        // require(_days > 0, "Penalty days should be > 0 days");
        require(_days <= 90, "Penalty days not allowed!");
        penaltyDays = _days;
        emit LogPenaltyDaysUpdated(_days);
    }

    
    // @notice Sets the unstaking wait time. This does not affect the current requests.
    //       The wait time can only be between 1 and 10 days.
    function setWithdrawWaitTime(uint256 newWaitTimeInSeconds) external onlyRewardsDistribution {
        require(newWaitTimeInSeconds >= 1 days, "Wait time needs to be > 1 day");
        require(newWaitTimeInSeconds <= 10 days, "Wait time needs to be < 10 days");
        withdrawWaitTime = newWaitTimeInSeconds;
    }

    function addTokenReward(uint256 rewardInOscars) external onlyRewardsDistribution {
        mintedRewardBalance += rewardInOscars;  //@TODO Do we need this?
        require(SPOT.transferFrom(msg.sender, address(this), rewardInOscars), "adding tokens reward failed!");
        emit LogAddTokenReward(rewardInOscars);
    }
    
    // @notice The admin will use notifyRewardAmount() to distribute Rewards to the Stakers.
    // @TODO should the owner have the ability to change the rewards duration?
    // function addRevenueReward(uint256 rewardInOscars, _rewardsDuration, rewardSchedule)  {
    function addRevenueReward(uint256 _rewardInOscars, uint256 _rewardsDuration) external onlyRewardsDistribution override {
        require(_rewardInOscars > 0, "Reward deposit should be > 0");
        require(updateRevenueReward(address(0)), "Updating revenue reward failed!");
        require(updateMintedReward(address(0)), "Updating minted reward failed!");
        
        if (block.timestamp >= revenueRewardPeriodEndTime) { //@todo make sure its >= ; might be >
            // Lack of precision
            // @ TODO check how to effectively calculate the factor, now is 1 days.
            revenueRewardPerSecond = (_rewardInOscars * FACTOR) / _rewardsDuration;
        } else {
            uint256 _remainingDuration = revenueRewardPeriodEndTime - block.timestamp;
            uint256 _remainingReward = (_remainingDuration * revenueRewardPerSecond)/FACTOR;
            revenueRewardPerSecond = ((_rewardInOscars + _remainingReward) * FACTOR) / _rewardsDuration;
        }
        
        allRevenueRewardIn += _rewardInOscars;

        // @TODO FIND OUT WHAT THIS IS!!!!
        // uint256 _rewardDiff = allRevenueRewardIn - allRevenueRewardOut;
        // Ensure the provided reward amount is not more than the balance in the contract.
        // This keeps the reward rate in the right range, preventing overflows due to
        // very high values of rewardRate in the earned and rewardsPerToken functions;
        // reward + remainingReward must be less than 2^256 / 10^18 to avoid overflow.
        // @TODO make sure that the following line is still valid!
        // require(revenueRewardPerSecond <= (_rewardDiff * 1 days / rewardsDuration), "Provided reward too high");

        lastTimeRevenueRewardUpdated = block.timestamp;  // revenueRewardPeriodTimeMarker
        revenueRewardPeriodEndTime = block.timestamp + _rewardsDuration;

        require(SPOT.transferFrom(msg.sender, address(this), _rewardInOscars), "add revenue reward failed!");

        emit LogRevenueRewardAdded(_rewardInOscars, _rewardsDuration);
    }
    // @TODO consider to add these functions to the abstract contract
    // @TODO Keith needs to approve the Max Minted Reward Rate
    function setMintedRewardRate(uint256 _mintedRewardRate) external onlyRewardsDistribution {
        require(_mintedRewardRate <= MAX_MINTED_REWARD_RATE, "Reward rate exceeds maximum rate");
        //@TODO Do we really need this?  updateRevenueReward(address(0))
        require(updateRevenueReward(address(0)), "revenue reward update failed!");
        require(updateMintedReward(address(0)), "minted reward update failed!");
        mintedRewardPerSecond = _mintedRewardRate;
    }

    function updateRevenueReward(address _account) internal returns (bool) {
        uint256 lastTimeRevenueRewardApplicable_ = lastTimeRevenueRewardApplicable(revenueRewardPeriodEndTime);

        uint256 updatedRevenueRewardOdometer = 
            calculateNewRevenueRewardOdometerAmount(lastTimeRevenueRewardApplicable_);
        
        if (_account != address(0)) {
            uint256 updatedRevenuePayout = calculateNewUserRevenueRewardToPayout(
                updatedRevenueRewardOdometer,
                _account
            ); 
            
            userRevenueRewardOdometer[_account] = updatedRevenueRewardOdometer;
            revenueRewardsToPayout[_account] = updatedRevenuePayout;
        }

        revenueRewardOdometer = updatedRevenueRewardOdometer;
        lastTimeRevenueRewardUpdated = lastTimeRevenueRewardApplicable_;

        return true;
    }

    function updateMintedReward(address _account) internal returns (bool) {     
        uint256 remainingMintedRewardToMint = allEarnedMintedReward - allMintedRewardPaid;
        uint256 availableSpotToMint = mintedRewardBalance - remainingMintedRewardToMint;

        uint256 totalMintedRewardsEarnedSinceLastTimeUpdated = calculateTotalMintedRewardsEarnedSinceLastTimeUpdated(
            availableSpotToMint
        );

        allEarnedMintedReward += totalMintedRewardsEarnedSinceLastTimeUpdated;

        uint256 updatedMintedRewardOdometer = calculateNewMintedRewardOdometerAmount(
            availableSpotToMint,
            totalMintedRewardsEarnedSinceLastTimeUpdated
        );
        
        if (_account != address(0)) {
            uint256 updatedMintedPayout = mintedRewardsToPayout[_account] + calculateUserMintedRewardForCurrentPeriod(
                updatedMintedRewardOdometer,
                _account
            );
            
            userMintedRewardOdometer[_account] = updatedMintedRewardOdometer;
            mintedRewardsToPayout[_account] = updatedMintedPayout;
        }
        mintedRewardOdometer = updatedMintedRewardOdometer;
        lastTimeMintedRewardUpdated = block.timestamp;
        return true;
    }

}

