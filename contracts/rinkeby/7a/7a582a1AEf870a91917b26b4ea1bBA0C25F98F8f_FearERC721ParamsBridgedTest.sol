pragma solidity 0.8.9;

import "./../common/ERC721/ERC721Enumerable.sol";
import "./../common/ERC721/IMRC721Metadata.sol";
import "./../common/access/AccessControl.sol";
import "./../common/Withdrawable.sol";
import "./../common/OpenSeaCompatible.sol";

/**
 * @dev FEAR NFT test.
 */
contract FearERC721ParamsBridgedTest is ERC721Enumerable, AccessControl, Withdrawable, OpenSeaCompatible, IMRC721Metadata {

    /**
    * @dev Minter role identifier.
    */
    bytes32 public constant MINTER_ROLE = "Minter";

    /**
    * @dev Bridger role identifier.
    */
    bytes32 public constant BRIDGER_ROLE = "Bridger";

    struct NftParams {
        bool set;
        uint256 param1;
        uint256 param2;
    }

    mapping (uint256 => NftParams) private items;

    uint256 private nextTokenId = 1;

    constructor() ERC721("FEAR ERC721 Params Bridged Test", "F721PTEST") {
        _setRoleAdmin(MINTER_ROLE, ROOT_ROLE);
        _setRoleAdmin(BRIDGER_ROLE, ROOT_ROLE);
        _registerInterface(type(IMRC721Metadata).interfaceId);
    }

    function _setItemParams(uint256 id, uint256 param1, uint256 param2) private {
        if (!items[id].set) // setting only once ?
            items[id] = NftParams(true, param1, param2);
    }

    function isApprovedForAll(address owner, address operator) public view override(ERC721, IERC721) returns (bool) {
        return _isOpenSeaProxy(owner, operator) || super.isApprovedForAll(owner, operator);
    }

    function totalSupply() public view virtual override(ERC721Enumerable, IMRC721) returns (uint256) {
        return ERC721Enumerable.totalSupply();
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "Query for nonexistent token");
        return OpenSeaCompatible._tokenURI(tokenId);
    }

    function getTokenParams(uint256 tokenId) public view returns (NftParams memory) {
        require(_exists(tokenId), "Query for nonexistent token");
        return items[tokenId];
    }

    function initialMint(address initialOwner) external onlyRole(MINTER_ROLE) {
        require(initialOwner != address(0), "Zero address");
        require(block.chainid == 4, "Initial minting available only on Rinkeby");

        // pseudo-random params
        uint256 param1 = block.timestamp % 100;
        uint256 param2 = (block.timestamp / param1) % 100;

        _safeMint(initialOwner, nextTokenId);
        _setItemParams(nextTokenId, param1, param2);
        nextTokenId++;
    }

    function encodeParams(uint256 id) public view returns(bytes memory){
        return abi.encode(
            items[id].param1,
            items[id].param2
        );
    }

    function encodeParams(uint256[] calldata ids) public view returns(bytes memory){
    	bytes[] memory params = new bytes[](ids.length);
    	for(uint i = 0; i < ids.length; i++)
    		params[i] = encodeParams(ids[i]);
    	return abi.encode(params);
    }

    // MUON Bridging functions

    function mint(address, uint256) external view onlyRole(BRIDGER_ROLE) {
        revert("Use mint with data");
    }

    function mint(address to, uint256 id, bytes calldata data) external onlyRole(BRIDGER_ROLE) {
        require(!_exists(id), "Cannot mint, token exists already");
        _safeMint(to, id);

        (uint256 param1, uint256 param2) = abi.decode(data, (uint256, uint256));
        _setItemParams(id, param1, param2);
    }

    function burn(uint256 tokenId) external onlyRole(BRIDGER_ROLE) {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "caller is not owner nor approved");
        _burn(tokenId);
    }

    function tokensOfOwner(address _owner) external view returns(uint256[] memory) {
        uint tokenCount = balanceOf(_owner);
        uint256[] memory tokensIds = new uint256[](tokenCount);
        for(uint i = 0; i < tokenCount; i++)
          tokensIds[i] = tokenOfOwnerByIndex(_owner, i);
        
        return tokensIds;
    }
}

