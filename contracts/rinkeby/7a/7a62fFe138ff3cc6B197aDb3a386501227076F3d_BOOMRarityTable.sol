/**
 * Toolset for deriving BOOM Town in-house NFT tokens rarities.
 * It assumes two periods: pre-reveal and post-reveal.
 * Pre-reveal period: NFT tokens are minted till the mint count cap is reached,
 * only in-house tokens mint events increase the mint counter. Token rarities cannot be derived.
 * Post-reveal period: The mint cap is reached. Token rarities can be derived.
 *
 * Upgrade: all the values (the rarity distrib table, entropy commitment and the entropy, blocknum and
 *          blockhash) are being transfered to the upgraded contract instance automatically
 * 
 * Dependencies:
 * - TagStorage
 *
 * Intructions: 
 * - after minting an in-house token, such token must be marked explicitly as the 
 *   'in-house' token by calling setInhouseNFT from BOOMTypeProcessor by minter
 * - the mint counter is set by setRevealCountLimit from BOOMTypeProcessor by minter
 * - the pre-reveal period is switched to the post-reveal one by calling fixBlocknum
 *   by minter. The fixBlocknum saves the block number when the mint cap has been reached.
 *   NOTE: the operator MUST call setBlockHash within 256 blocks after fixBlocknum was called.
 *   As the remedy of a late setBlockHash call, the operator may set explicitly the hash of the block
 *   when the post-reveal period started, though this scenario requires full trust towards the
 *   BOOM Town operator
 * - the BOOM Town operator's entropy is committed by calling init by the operator
 * - the BOOM Town operator's entropy is set/revealed by calling setEntropy by the operator
 * - the rarity distribution is set by setRarityTableLength, setRarityPopulationSize and setRarityTableElement
 *   by the operator
 */

pragma solidity ^0.8.0;

import "./common/BOOMBase.sol";
import "./TagStorage.sol";

contract BOOMRarityTable is BOOMBase {

    bytes32 public constant BOOM_RARITY_TABLE_GROUPID = keccak256(abi.encodePacked('BOOM_RARITY_TABLE_GROUPID'));

    TagStorage public tags;

    bytes32 public commitment;
    bytes32 public entropy;
    bytes32 public blockHash;
    uint256 public blockNum;

    uint256[] public tokenRarity;
    uint256   public tokenPopulationSize;

    event TAGLink(address addr);
    event Commit(bytes32 _commitment);
    event TableLength(uint256 _length);
    event TableElement(uint256 _index, uint256 _value);
    event PopulationSize(uint256 _value);
    event Entropy(bytes32 _value);
    event BlockNum(uint256 _value);
    event BlockHash(bytes32 _value);

    /**
     * Recovers all the values after the contract instance upgrade
     */
    constructor() {

    }

    function getId() override view public returns(bytes32) {
	return BOOM_RARITY_TABLE_CONTRACT_ID;
    }

    function getName() override view public returns(string memory) {
	return BOOM_RARITY_TABLE_CONTRACT_NAME;
    }

    function getVersion() override virtual view external returns(bytes32) {
	return keccak256(abi.encodePacked('v1.0')); // First release
    }

    function onListenAdded(bytes32 hname, address contractInstance, bool isNew) override public onlyManager {
	_onListenAdded(hname, contractInstance, isNew);
    }

    function _onListenAdded(bytes32 hname, address contractInstance, bool isNew) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
	    _linkTAG(contractInstance);
	    if(!isNew){
		if(commitment != bytes32(0))
		    _init(commitment);
		if(tokenRarity.length > 0){
		    _setRarityTableLength(tokenRarity.length);
		    for(uint i=0;i<tokenRarity.length;i++)
			_setRarityTableElement(i, tokenRarity[i]);
		}
		if(tokenPopulationSize > 0)
		    _setRarityPopulationSize(tokenPopulationSize);
		if(entropy != bytes32(0))
		    _setEntropy(entropy);
		if(blockNum > 0)
		    _fixBlocknum(blockNum);
		if(blockHash != bytes32(0))
		    _setBlockHash(blockHash);
	    }
	}else
	    super._onListenAdded(hname, contractInstance, isNew);
    }


    function onListenRemoved(bytes32 hname) override public onlyManager {
	_onListenRemoved(hname);
    }

    function _onListenRemoved(bytes32 hname) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
	    tags = TagStorage(address(0));
	}else
	super._onListenRemoved(hname);
    }


/**
     * @notice Sets link to Tag Storage smart contract
    **/
    function linkTAG(address tag_storage) public virtual onlyOwner {
        _linkTAG(tag_storage);
    }

    function _linkTAG(address tag_storage) internal {
        tags = TagStorage(tag_storage);

	bytes32 tag_key = _getCommitmentKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    commitment = tags.getByte32Value(tag_key);
	tag_key = _getRarityLengthKey();
	if(tags.getTagType(tag_key) != Util.StatType.None){
	    uint _length = _getRarityTableLength();
	    tokenPopulationSize = _getRarityPopulationSize();
	    for(uint8 i=0;i<_length;i++){
		uint256 _element = _getRarityTableElement(i);
		tokenRarity.push(_element);
	    }
	}
	tag_key = _getEntropyKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    entropy = tags.getByte32Value(tag_key);
	tag_key = _getBlockhashKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    blockHash = tags.getByte32Value(tag_key);
	tag_key = _getBlocknumKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    blockNum = tags.getIntValue(tag_key);

        emit TAGLink(tag_storage);
    }


    /**
     * Initializes the VRF with the BOOM Town operator entropy commitment.
     * The operator can commit the entropy just once
     *
     * @param _commitment Entropy commitment
     */
    function init(bytes32 _commitment) public virtual onlyOperator {
	_init(_commitment);
	commitment = _commitment;

	emit Commit(_commitment);
    }

    function _init(bytes32 _commitment) internal virtual {
	bytes32 tag_key = _getCommitmentKey();
	require(tags.getTagType(tag_key) == Util.StatType.None, 'The BOOM Town cannot have more than one commitment');
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, tag_key, _commitment);
    }

    /**
     * The operator sets the size of the rarity distribution table
     *
     * @param _length The size of the rarity distribution table.
     *        The operator should take care of the _length to correspond to the real number of elements in the table
     */
    function setRarityTableLength(uint8 _length) public virtual onlyOperator {
	_setRarityTableLength(_length);

	emit TableLength(_length);
    }

    function _setRarityTableLength(uint256 _length) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getRarityLengthKey(), _length);
    }

    /**
     * The operator sets the statistical population size
     *
     * @param _size The statistical population size
     */
    function setRarityPopulationSize(uint256 _size) public virtual onlyOperator {
	_setRarityPopulationSize(_size);
	tokenPopulationSize = _size;

	emit PopulationSize(_size);
    }

    function _setRarityPopulationSize(uint256 _size) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getRarityPopulationKey(), _size);
    }

    /**
     * The operator sets the statistical sample size
     *
     * @param index The element position in the table
     * @param value The size of the statistical sample
     */
    function setRarityTableElement(uint256 index, uint256 value) public virtual onlyOperator {
	_setRarityTableElement(index, value);
	tokenRarity[index] = value;

	emit TableElement(index, value);
    }

    function _setRarityTableElement(uint256 index, uint256 value) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getRarityElementKey(index), value);
    }


    /**
     * The operator sets the entropy. It can be done during the post-Ã¤reveal period only.
     * It can be done when the entropy has been commited first.
     * 
     * @param value The entropy
     */
    function setEntropy(bytes32 value) public virtual onlyOperator {
	require(blockNum != 0,'The reveal period must have been started');
	require(commitment == keccak256(abi.encodePacked(value)), 'Entropy and commitment do not match');
	_setEntropy(value);
	entropy = value;

	emit Entropy(value);
    }

    function _setEntropy(bytes32 value) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getEntropyKey(), value);
    }

    /**
     * The minter fixes the number of the first block (saves the currenty block number) of the post-reveal period.
     * This method is supposed to be called by the setInhouseNFT from BOOMTypeProcessor as soon as the mint cap is reached.
     * However, for whatever reason, the minter may call this method and switch to the post-reveal period prematurely.
     */
    function fixBlocknum() public virtual onlyVRFManager {
	require(blockNum == 0,'The reveal period must not have been started yet');
	blockNum = block.number;
	_fixBlocknum(blockNum);

	emit BlockNum(blockNum);
    }

    function _fixBlocknum(uint256 _value) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getBlocknumKey(), _value);
    }

    /**
     * The opertator sets the hash of the first block of the post-reveal period.
     * The method must be called within 256-block window after the start of the post-reveal period
     * (call of fixBlocknum method). Otherwise, the hash will not be set automatically and manual
     * trusted block hash setting will be required.
     */
    function setBlockHash() public virtual onlyOperator {
	require(blockNum>0,'The reveal period must have been started');
	blockHash = blockhash(blockNum);
	_setBlockHash(blockHash);

	emit BlockHash(blockHash);
    }

    /**
     * The opertator sets the hash of the first block of the post-reveal period manualy.
     * Here, we trust that the operator performed due diligence to acquire the hash of the
     * first block of the post-reveal period by some external means (like, from Etherscan
     * by knowing the first post-reveal block number)
     *
     * @param value The hash of the block
     */
    function setBlockHash(bytes32 value) public virtual onlyOperator {
	require(bytes32(blockHash) == 0,'The blockhash can be set just once');
	blockHash = value;
	_setBlockHash(value);

	emit BlockHash(value);
    }

    function _setBlockHash(bytes32 value) internal virtual {
	tags.setTag(BOOM_RARITY_TABLE_GROUPID, _getBlockhashKey(), value);
    }

    /**
     * Get the size of the rarity distribution table from the TagStorage
     *
     * @return The size of the rarity table
     */
    function _getRarityTableLength() public view virtual returns (uint256) {
	return tags.getIntValue(_getRarityLengthKey());
    }

    /**
     * Get the statistical population size from the TagStorage
     *
     * @return The size of the statistical population
     */
    function _getRarityPopulationSize() public view virtual returns (uint256) {
	return tags.getIntValue(_getRarityPopulationKey());
    }

    /**
     * Get the statistical sample element from the TagStorage
     *
     * @param  index Position of the sample element in the distribution table
     * @return The size of the statistical sample
     */
    function _getRarityTableElement(uint8 index) public view virtual returns (uint256) {
	return tags.getIntValue(_getRarityElementKey(index));
    }

    /**
     * Derive the rarity of the given NFT token from its ID based on the VRF, rarity distribution table
     * and statistical population size.
     *
     * @param  token_id NFT token ID.
     * @return The size of the statistical sample
     */
    function getTokenRarity(uint256 token_id) public view virtual returns (Util.NFTRarity) {
	uint256[] memory _tokenRarity;
	for(uint i=0;i<tokenRarity.length;i++)
	    _tokenRarity[i] = tokenRarity[i];
	return Util.NFTRarity(
	    Util.getRandomItem(
	    _getRandomValue(token_id),
	    _tokenRarity,
	    tokenPopulationSize
	    )
	);
    }

    /**
     * VRF extracting a random value from an NFT token ID, blockchain entropy (hash of the first block of the post-reveal period)
     * and the BOOM Town operator's entropy
     *
     * @return A random value
     */
    function _getRandomValue(uint256 token_id) internal view returns (uint256) {
	return uint256(keccak256(abi.encodePacked(blockHash,entropy,token_id)));
    }

    /**
     * Get the key of the commitment tag in the TagStorage
     *
     */
    function _getCommitmentKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_COMMITMENT_TAG'));
    }

    /**
     * Get the key of the distribution table's size tag in the TagStorage
     *
     */
    function _getRarityLengthKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_NFT_RARITY_TABLE_LENGTH_TAG'));
    }

    /**
     * Get the key of the statistical population size tag in the TagStorage
     *
     */
    function _getRarityPopulationKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_NFT_RARITY_TABLE_POPULATION_TAG'));
    }

    /**
     * Get the key of a statistical sample tag in the TagStorage
     *
     * @param  index Position of the sample element in the distribution table
     */
    function _getRarityElementKey(uint256 index) internal pure  returns (bytes32){
	return keccak256(abi.encodePacked('BOOM_TOWN_NFT_RARITY_TABLE_ELEMENT_TAG',index));
    }

    /**
     * Get the key of the entropy tag in the TagStorage
     *
     */
    function _getEntropyKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_ENTROPY_TAG'));
    }

    /**
     * Get the key of the hash of the first post-reveal block tag in the TagStorage
     *
     */
    function _getBlockhashKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_BLOCKHASH_TAG'));
    }

    /**
     * Get the key of the number of the first post-reveal block tag in the TagStorage
     *
     */
    function _getBlocknumKey() internal pure returns (bytes32) {
	return keccak256(abi.encodePacked('BOOM_TOWN_BLOCKNUM_TAG'));
    }

}

pragma solidity ^0.8;

import "./CrossContractManListener.sol";

abstract contract BOOMBase is CrossContractManListener {

    string  public constant MANAGER_CONTRACT_NAME = "MANAGER_CROSSCONTRACT";
    bytes32 public constant MANAGER_CONTRACT_ID = keccak256(abi.encodePacked(MANAGER_CONTRACT_NAME));

//    string  public constant ROLES_CONTRACT_NAME = "MANAGER_ROLES";
//    bytes32 public constant ROLES_CONTRACT_ID = keccak256(abi.encodePacked(ROLES_CONTRACT_NAME));

    string  public constant PROXY_REGISTRY_CONTRACT_NAME = "PROXY_REGISTRY";
    bytes32 public constant PROXY_REGISTRY_CONTRACT_ID = keccak256(abi.encodePacked(PROXY_REGISTRY_CONTRACT_NAME));

    string  public constant BOOM_RARITY_TABLE_CONTRACT_NAME = "BOOM_RARITY_TABLE";
    bytes32 public constant BOOM_RARITY_TABLE_CONTRACT_ID = keccak256(abi.encodePacked(BOOM_RARITY_TABLE_CONTRACT_NAME));

    string  public constant METADATA_CONTAINER_CONTRACT_NAME = "METADATA_CONTAINER";
    bytes32 public constant METADATA_CONTAINER_CONTRACT_ID = keccak256(abi.encodePacked(METADATA_CONTAINER_CONTRACT_NAME));

    string  public constant TAG_STORAGE_CONTRACT_NAME = "TAG_STORAGE";
    bytes32 public constant TAG_STORAGE_CONTRACT_ID = keccak256(abi.encodePacked(TAG_STORAGE_CONTRACT_NAME));

    string  public constant BOOM_NFT_CONTRACT_NAME = "BOOM_NFT";
    bytes32 public constant BOOM_NFT_CONTRACT_ID = keccak256(abi.encodePacked(BOOM_NFT_CONTRACT_NAME));

    string  public constant BOOM_FACTORY_CONTRACT_NAME = "BOOM_FACTORY";
    bytes32 public constant BOOM_FACTORY_CONTRACT_ID = keccak256(abi.encodePacked(BOOM_FACTORY_CONTRACT_NAME));

    string  public constant MINTER_CONTRACT_NAME = "MINTER";
    bytes32 public constant MINTER_CONTRACT_ID = keccak256(abi.encodePacked(MINTER_CONTRACT_NAME));

    bytes32 public constant BOOM_FACTORY_GROUPID = keccak256(abi.encodePacked('BOOM_FACTORY_GROUPID'));

    function onListenAdded(bytes32 hname, address contractInstance, bool isNew) override virtual public onlyManager {
	_onListenAdded(hname, contractInstance, isNew);
    }

    function _onListenAdded(bytes32 hname, address contractInstance, bool isNew) internal virtual {
	if(address(this) == contractInstance)return;
	if(hname == ROLE_MANAGER_CONTRACT_ID){
    	    _linkRoleManager(contractInstance);
	}
    }


    function onListenRemoved(bytes32 hname) override virtual public onlyManager {
	_onListenRemoved(hname);
    }

    function _onListenRemoved(bytes32 hname) internal virtual {
	if(hname == ROLE_MANAGER_CONTRACT_ID){
	    authManager = IAccessControl(address(0));
	}
    }


}

// SPDX-License-Identifier: MIT
/**
 * @title Key-value (Tag) generic storage
 * @notice Stores tags (key-value) pairs of byte32, string, uint256 and boolean types.
 * The storage is agnostic to what contract (editor) and what data are being stored. Tags are formed by 
 * the editor contracts. The storage provides authentication for adding/modifying tags.
 * 
 * Each tag is assigned an editors' group. An editor (contract or user) can be added to one or more groups.
 * An editor can add/modify only those tags that are assigned to a group which the editor belongs to.
 */

pragma solidity ^0.8.0;

//import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
//import "@openzeppelin/contracts/utils/Context.sol";
//import "@openzeppelin/contracts/access/Ownable.sol";

//import "./common/RoleManagerClient.sol";

import "./common/BOOMBase.sol";

import {Util} from "./common/Util.sol";


//contract TagStorage is Context, Ownable, RoleManagerClient, Util {
//contract TagStorage is Context, RoleManagerClient, Util {
contract TagStorage is BOOMBase {
    bytes32 public constant ADDER_ROLE = keccak256('ADDER_ROLE');

    mapping (bytes32 => bytes32) tagGroup;
    mapping (bytes32 => bytes32) tagByte32Value;
    mapping (bytes32 => string) tagStringValue;
    mapping (bytes32 => uint256) tagIntValue;
    mapping (bytes32 => bool) tagBooleanValue;
    mapping (bytes32 => Util.StatType) tagType;
//    mapping (bytes32 => StatType) tagType;
//    mapping (bytes32 => bool) groupMember;

    modifier onlyAdder() {
	require(hasRole(ADDER_ROLE, msg.sender),"TagStorage: The caller must have adder's priviledges");
	_;
    }

    modifier onlyAdmin() {
	require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender),"TagStorage: The caller must have admin's priviledges");
	_;
    }

    modifier authorizeEdit(bytes32 groupID, bytes32 key) {
	require(hasRole(groupID, msg.sender),"TagStorage: Caller needs to be group member of groupID");
	if(tagGroup[key]!=bytes32(0)){
	    require(hasRole(tagGroup[key], msg.sender),"TagStorage: Caller needs to be tag's group member");
//	    bytes32 groupMemberKey = keccak256(abi.encode(msg.sender,tagGroup[key]));
//	    require(groupMember[groupMemberKey],"TagStorage: Need to be tag's group member");
	}
	tagGroup[key] = groupID;
	_;
    }

    modifier byte32Tag(bytes32 key) {
	require((tagType[key] == Util.StatType.ByteArray), "TagStorage: The tag must be a byte array");
	_;
    }

    modifier stringTag(bytes32 key) {
	require((tagType[key] == Util.StatType.String), "TagStorage: The tag must be a string");
	_;
    }

    modifier intTag(bytes32 key) {
	require((tagType[key] == Util.StatType.Integer), "TagStorage: The tag must be an integer");
	_;
    }

    modifier booleanTag(bytes32 key) {
	require((tagType[key] == Util.StatType.Boolean), "TagStorage: The tag must be a boolean");
	_;
    }

    constructor() {
	super._setupRole(DEFAULT_ADMIN_ROLE,msg.sender);
    }

    function getId() override view public returns(bytes32) {
	return TAG_STORAGE_CONTRACT_ID;
    }

    function getName() override view public returns(string memory) {
	return TAG_STORAGE_CONTRACT_NAME;
    }

    function getVersion() override virtual view external returns(bytes32) {
	return keccak256(abi.encodePacked('v1.0')); // First release
    }

    function grantAdminRole(address entity) external virtual onlyOwner {
	super._setupRole(DEFAULT_ADMIN_ROLE,entity);
    }

    function revokeAdminRole(address entity) external virtual onlyOwner {
	super.revokeRole(DEFAULT_ADMIN_ROLE,entity);
    }

    function grantAdderRole(address entity) external virtual onlyAdmin {
	super._setupRole(ADDER_ROLE,entity);
    }

    function revokeAdderRole(address entity) external virtual onlyAdmin {
	super.revokeRole(ADDER_ROLE,entity);
    }

    function isAdmin(address entity) external view returns (bool) {
	return hasRole(DEFAULT_ADMIN_ROLE, entity);
    }

    function isAdder(address entity) external view returns (bool) {
	return hasRole(ADDER_ROLE, entity);
    }

    function isGroupMember(address entity, bytes32 groupID) external view returns (bool) {
	return hasRole(groupID, entity);
//	bytes32 groupMemberKey = keccak256(abi.encode(entity,groupID));
//	return groupMember[groupMemberKey];
    }

    /**
     * @notice Add editor to a group
     * 
     * @param entity  Editor's address. It can be contract or user
     * @param groupID The id of the group where to add the entity
    **/
    function addEditor2Group(address entity,bytes32 groupID) public virtual onlyAdmin {
	grantRole(groupID, entity);
//	bytes32 groupMemberKey = keccak256(abi.encode(entity,groupID));
//	groupMember[groupMemberKey] = true;
    }

    /**
     * @notice Remove editor from the group
     * 
     * @param entity  Editor's address. It can be contract or user
     * @param groupID The id of the group from where to remove the entity
    **/
    function removeEditorFromGroup(address entity,bytes32 groupID) public virtual onlyAdmin {
	revokeRole(groupID, entity);
//	bytes32 groupMemberKey = keccak256(abi.encode(entity,groupID));
//	groupMember[groupMemberKey] = false;
    }

    /**
     * @notice Set a byte array tag. The caller must have an adder role. 
     * The caller with the adder role can create new tag and assign it to any group id.
     * The caller may modify existing tag and assign it to a new group id if the caller belongs to the group currently associated to the tag.
     *
     * @param groupID The ID of the group for the tag
     * @param key     The key of the tag
     * @param value   The value (type byte32) of the tag
    **/
    function setTag(bytes32 groupID, bytes32 key, bytes32 value) public virtual onlyAdder authorizeEdit(groupID, key) {
	tagType[key] = Util.StatType.ByteArray;
	tagByte32Value[key] = value;
    }

    /**
     * @notice Set a string tag. The caller must have an adder role.
     * The caller with the adder role can create new tag and assign it to any group id.
     * The caller may modify existing tag and assign it to a new group id if the caller belongs to the group currently associated to the tag.
     *
     * @param groupID The ID of the group for the tag
     * @param key     The key of the tag
     * @param value   The value (type string) of the tag
    **/
    function setTag(bytes32 groupID, bytes32 key, string calldata value) public virtual onlyAdder authorizeEdit(groupID, key) {
	tagType[key] = Util.StatType.String;
	tagStringValue[key] = value;
    }

    /**
     * @notice Set an integer tag. The caller must have an adder role.
     * The caller with the adder role can create new tag and assign it to any group id.
     * The caller may modify existing tag and assign it to a new group id if the caller belongs to the group currently associated to the tag.
     *
     * @param groupID The ID of the group for the tag
     * @param key     The key of the tag
     * @param value   The value (type uint256) of the tag
    **/
    function setTag(bytes32 groupID, bytes32 key, uint256 value) public virtual onlyAdder authorizeEdit(groupID, key) {
	tagType[key] = Util.StatType.Integer;
	tagIntValue[key] = value;
    }

    /**
     * @notice Set a boolean tag. The caller must have an adder role.
     * The caller with the adder role can create new tag and assign it to any group id.
     * The caller may modify existing tag and assign it to a new group id if the caller belongs to the group currently associated to the tag.
     *
     * @param groupID The ID of the group for the tag
     * @param key     The key of the tag
     * @param value   The value (type boolean) of the tag
    **/
    function setTag(bytes32 groupID, bytes32 key, bool value) public virtual onlyAdder authorizeEdit(groupID, key) {
	tagType[key] = Util.StatType.Boolean;
	tagBooleanValue[key] = value;
    }

    /**
     * @notice Gets data type of the tag.
     *
     * @param  key The tag's key
     * @return The tag's value data type
    **/
    function getTagType(bytes32 key) public view returns (Util.StatType) {
	return tagType[key];
    }

    /**
     * @notice Gets tag's value of byte array type.
     *
     * @param key The tag's key
     * @return The tag's value of type bytes32
    **/
    function getByte32Value(bytes32 key) public view byte32Tag(key) returns (bytes32) {
	return tagByte32Value[key];
    }

    /**
     * @notice Gets tag's value of string type.
     *
     * @param key The tag's key
     * @return The tag's value of type string
    **/
    function getStringValue(bytes32 key) public view stringTag(key) returns (string memory) {
	return tagStringValue[key];
    }

    /**
     * @notice Gets tag's value of integer type.
     *
     * @param key The tag's key
     * @return The tag's value of type uint256
    **/
    function getIntValue(bytes32 key) public view intTag(key) returns (uint256) {
	return tagIntValue[key];
    }

    /**
