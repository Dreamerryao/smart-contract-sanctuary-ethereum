pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./VRFConsumerBaseV2.sol";
import "./ERC721AQueryableUpgradeable.sol";

interface Erc721Contract {
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;
}

contract ProjectP22Token is
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    ERC721AQueryableUpgradeable,
    VRFConsumerBaseV2
{
    bool public saleIsActive;

    uint256 public maxProjectP22;

    address public packContractAddress;

    string private _baseTokenURI;

    // Golden Ticket
    mapping(uint256 => bool) public isGoldenTicket;
    bool public goldenTicketRedeemActive;
    struct LinkRequest {
        uint256 tokenId;
        address userAddress;
    }
    struct GoldenTicketRedeemRequest {
        address userAddress;
        bool isBurned;
        bool isNftTransferred;
    }
    mapping(uint256 => LinkRequest) public LinkRequestMapping;
    mapping(uint256 => GoldenTicketRedeemRequest)
        public GoldenTicketRedeemRequestMapping;
    address public goldenTicketSetter;

    bool public isGoldenTicketTransferLocked;

    // CHAIN LINK
    VRFCoordinatorV2Interface COORDINATOR;
    LinkTokenInterface LINKTOKEN;
    // TODO: SET DEFAULT BEFORE DEPLOY
    struct ChainLinkConfig {
        uint64 chainLinkSubscriptionId;
        address chainLinkVrfCoordinator;
        address chainLinkLinkAddress;
        bytes32 chainLinkKeyHash;
        uint32 chainLinkCallbackGasLimit;
        uint16 chainLinkRequestConfirmations;
        uint256 numWords;
    }
    ChainLinkConfig public chainLinkConfig;

    struct GoldenTicketToken {
        uint256 tokenId;
        address contractAddress;
    }
    uint256 public goldenTicketTokenCount;
    uint256 public goldenTicketTokenMinAvailableIndex;
    uint256 public goldenTicketTokenMaxAvailableIndex;
    mapping(uint256 => GoldenTicketToken) public goldenTicketTokens;
    mapping(uint256 => uint256) public goldenTicketTokenIndex;
    mapping(uint256 => bool) public isGoldenTicketTokenIndexPicked;
    address public goldenTicketHolderWalletAddress;

    // Provenance
    struct ProvenanceRecord {
        string cardRang;
        string Provenance;
    }
    mapping(string => ProvenanceRecord[]) private provenanceRecord;
    uint256[] requests;

    error TransferGoldenTicketNotAllowed();

    // ############################# constructor #############################
    function initialize() external initializer {
        // HERE UPDATE DETAILS BEFORE LIVE
        __Ownable_init();
        __ERC721A_init("projectP22Token", "projectP22Token");
        chainLinkConfig = ChainLinkConfig(
            2411,
            0x6168499c0cFfCaCD319c818142124B7A15E857ab,
            0x01BE23585060835E02B77ef475b0Cc51aA1e0709,
            0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc,
            400000,
            3,
            1
        );
        __VRFConsumerBaseV2_init(0x6168499c0cFfCaCD319c818142124B7A15E857ab);
        COORDINATOR = VRFCoordinatorV2Interface(
            0x6168499c0cFfCaCD319c818142124B7A15E857ab
        );
        LINKTOKEN = LinkTokenInterface(
            0x01BE23585060835E02B77ef475b0Cc51aA1e0709
        );
        goldenTicketTokenCount = 0;
        goldenTicketTokenMinAvailableIndex = 0;
        goldenTicketTokenMaxAvailableIndex = 2;

        maxProjectP22 = 500000;
        saleIsActive = false;
        isGoldenTicketTransferLocked = true;
    }

    // ############################# function section #############################

    // ***************************** internal : Start *****************************

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function checkGoldenIndexIsAvailbale(uint256 randomNumber)
        internal
        returns (uint256)
    {
        uint256 j = 0;
        for (
            j = randomNumber;
            j <= goldenTicketTokenMaxAvailableIndex;
            j = j + 1
        ) {
            if (isGoldenTicketTokenIndexPicked[uint256(j)] != true) {
                return j;
            }
        }
        goldenTicketTokenMaxAvailableIndex = uint256(randomNumber - 1);
        return checkGoldenIndexIsAvailbale(randomNumber / 2);
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        GoldenTicketRedeemRequest
            memory goldenTicketRedeemRequestInstance = GoldenTicketRedeemRequestMapping[
                LinkRequestMapping[requestId].tokenId
            ];
        require(
            goldenTicketRedeemRequestInstance.isBurned == true &&
                goldenTicketRedeemRequestInstance.isNftTransferred == false,
            "Nft already transferred or token not burned"
        );

        uint256 rNumber = randomWords[0] %
            (goldenTicketTokenMaxAvailableIndex -
                goldenTicketTokenMinAvailableIndex +
                1);
        uint256 newIndex = checkGoldenIndexIsAvailbale(rNumber);
        isGoldenTicketTokenIndexPicked[uint256(newIndex)] = true;

        Erc721Contract erc721ContractInstance = Erc721Contract(
            goldenTicketTokens[uint256(newIndex)].contractAddress
        );
        erc721ContractInstance.transferFrom(
            goldenTicketHolderWalletAddress,
            LinkRequestMapping[requestId].userAddress,
            goldenTicketTokens[uint256(newIndex)].tokenId
        );

        GoldenTicketRedeemRequestMapping[
            LinkRequestMapping[requestId].tokenId
        ] = GoldenTicketRedeemRequest(msg.sender, true, true);
        requests.push(requestId);
    }

    function _beforeTokenTransfers(
        address from,
        address,
        uint256 tokenId,
        uint256 quantity
    ) internal view override {
        if (isGoldenTicketTransferLocked && from != address(0)) {
            for (uint256 i = 0; i < quantity; i++) {
                if (isGoldenTicket[uint256(tokenId)])
                    revert TransferGoldenTicketNotAllowed();
            }
        }
    }

    // ***************************** internal : End *****************************

    // ***************************** onlyOwner : Start *****************************

    function withdraw() public onlyOwner {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "Transfer failed.");
    }

    function startSale() external onlyOwner {
        require(!saleIsActive, "Public sale has already begun");
        saleIsActive = true;
    }

    function pauseSale() external onlyOwner {
        saleIsActive = false;
    }

    function setPackContractAddress(address _packContractAddress)
        external
        onlyOwner
    {
        packContractAddress = _packContractAddress;
    }

    function setCollectionSize(uint256 collectionSize_) external onlyOwner {
        maxProjectP22 = collectionSize_;
    }

    function setBaseURI(string calldata baseURI) external onlyOwner {
        _baseTokenURI = baseURI;
    }

    function setIsGoldenTicket(uint256 tokenId, bool isGT) external {
        require(
            goldenTicketSetter == msg.sender || owner() == msg.sender,
            "You are not authorized"
        );
        require(isGoldenTicket[tokenId] != isGT, "Same value already set");
        isGoldenTicket[tokenId] = isGT;
    }

    function setChainLinkSubscriptionId(uint64 newSubscriptionId)
        external
        onlyOwner
    {
        chainLinkConfig.chainLinkSubscriptionId = newSubscriptionId;
    }

    function setGoldenTicketConfigration(
        uint256 _goldenTicketTokenMinAvailableIndex,
        uint256 _goldenTicketTokenMaxAvailableIndex
    ) external onlyOwner {
        goldenTicketTokenMinAvailableIndex = _goldenTicketTokenMinAvailableIndex;
        goldenTicketTokenMaxAvailableIndex = _goldenTicketTokenMaxAvailableIndex;
    }

    function setChainLinkConfigration(
        address newVrfCoordinator,
        address newLinkAddress,
        bytes32 newKeyHash,
        uint32 newCallbackGasLimit,
        uint16 newRequestConfirmations
    ) external onlyOwner {
        chainLinkConfig.chainLinkVrfCoordinator = newVrfCoordinator;
        chainLinkConfig.chainLinkLinkAddress = newLinkAddress;
        chainLinkConfig.chainLinkKeyHash = newKeyHash;
        chainLinkConfig.chainLinkCallbackGasLimit = newCallbackGasLimit;
        chainLinkConfig.chainLinkRequestConfirmations = newRequestConfirmations;
        COORDINATOR = VRFCoordinatorV2Interface(newVrfCoordinator);
        LINKTOKEN = LinkTokenInterface(newLinkAddress);
    }

    function setChainLinkCallbackGasLimit(uint32 newCallbackGasLimit)
        external
        onlyOwner
    {
        chainLinkConfig.chainLinkCallbackGasLimit = newCallbackGasLimit;
    }

    function setGoldenTicketHolderWalletAddress(
        address _goldenTicketHolderWalletAddress
    ) external onlyOwner {
        goldenTicketHolderWalletAddress = _goldenTicketHolderWalletAddress;
    }

    function setGoldenTicketRedeemActive(bool isActive) external onlyOwner {
        require(isActive != goldenTicketRedeemActive, "same value already set");
        goldenTicketRedeemActive = isActive;
    }

    function withdrawGoldenTicketToken(uint256 tokenIndex) external onlyOwner {
        isGoldenTicketTokenIndexPicked[tokenIndex] = true;
    }

    function addGoldenTicket(address contractAddress, uint256 tokenId)
        external
        onlyOwner
    {
        goldenTicketTokens[goldenTicketTokenCount] = GoldenTicketToken(
            tokenId,
            contractAddress
        );
        goldenTicketTokenIndex[tokenId] = goldenTicketTokenCount;
        goldenTicketTokenCount += 1;
    }

    function setProvenance(
        string memory slabbed,
        string memory _rang,
        string memory _provenance
    ) external onlyOwner {
        provenanceRecord[slabbed].push(ProvenanceRecord(_rang, _provenance));
    }

    function setProvenance(
        string memory slabbed,
        uint256 index,
        string memory _rang,
        string memory _provenance
    ) external onlyOwner {
        provenanceRecord[slabbed][index] = ProvenanceRecord(_rang, _provenance);
    }

    function setGoldenTicketTransferLock(bool _lock) external onlyOwner {
        isGoldenTicketTransferLocked = _lock;
    }

    function setGoldenTicketSetter(address _goldenTicketSetter)
        external
        onlyOwner
    {
        goldenTicketSetter = _goldenTicketSetter;
    }

    // ***************************** onlyOwner : End *****************************

    // ***************************** public view : Start *************************

    function Mint(
        uint256 numberOfTokens,
        address toAddress,
        uint256 tokenStartIndex,
        uint256 randomNumber
    ) external {
        uint256 currentTotalSupply = _totalMinted();
        require(packContractAddress == msg.sender, "Invalid pack address");
        require(saleIsActive, "Sale must be active to mint ProjectP22");
        require(numberOfTokens < 21, "Can only mint 20 tokens at a time");
        require(
            currentTotalSupply + numberOfTokens <= maxProjectP22,
            "Purchase would exceed max supply of ProjectP22s"
        );
        _safeMint(toAddress, numberOfTokens, tokenStartIndex, randomNumber);
    }

    function numberMinted(address owner) external view returns (uint256) {
        return _numberMinted(owner);
    }

    function getOwnershipData(uint256 tokenId)
        external
        view
        returns (TokenOwnership memory)
    {
        return _ownershipOf(tokenId);
    }

    function getProvenance(string memory slabbed)
        external
        view
        returns (string memory)
    {
        string memory data = "";
        for (uint256 i = 0; i < provenanceRecord[slabbed].length; i++) {
            data = string(
                abi.encodePacked(
                    data,
                    provenanceRecord[slabbed][i].cardRang,
                    ": ",
                    provenanceRecord[slabbed][i].Provenance,
                    "  ||  "
                )
            );
        }
        return data;
    }

    function RedeemGoldenTicket(uint256 tokenId) external {
        require(goldenTicketRedeemActive, "Golden ticket redeem is not active");
        address owner = ERC721AUpgradeable.ownerOf(tokenId);
        GoldenTicketRedeemRequest
            memory goldenTicketRedeemRequestInstance = GoldenTicketRedeemRequestMapping[
                tokenId
            ];
        require(
            msg.sender == owner ||
                (goldenTicketRedeemRequestInstance.userAddress == msg.sender &&
                    goldenTicketRedeemRequestInstance.isBurned == true &&
                    goldenTicketRedeemRequestInstance.isNftTransferred ==
                    false),
            "You are not the owner of the entered token"
        );
        require(
            isGoldenTicket[tokenId] == true,
            "Selected token is not a golden ticket"
        );
        if (goldenTicketRedeemRequestInstance.isBurned == false) {
            _burn(tokenId);
            GoldenTicketRedeemRequestMapping[
                tokenId
            ] = GoldenTicketRedeemRequest(msg.sender, true, false);
        }
        uint256 requestId = COORDINATOR.requestRandomWords(
            chainLinkConfig.chainLinkKeyHash,
            chainLinkConfig.chainLinkSubscriptionId,
            chainLinkConfig.chainLinkRequestConfirmations,
            chainLinkConfig.chainLinkCallbackGasLimit,
            1
        );
        LinkRequestMapping[requestId] = LinkRequest(tokenId, msg.sender);
    }
}

