pragma solidity ^0.8.4;

import './interfaces/IRevAuction.sol';
import './interfaces/IRevSale.sol';
import './interfaces/IRevNFT.sol';
import './interfaces/IREV721.sol';
import './interfaces/ICommonStruct.sol';
import './interfaces/IMarketPlace.sol';
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import 'hardhat/console.sol';

contract MarketPlace is ReentrancyGuard, Pausable, Ownable, ICommonStruct, IMarketPlace {
   using Counters for Counters.Counter;

   mapping(address => mapping(uint256 => NFTInfo)) private tokenInfos;  // tokenAddress => tokenID => NFTInfo
   TokenInfo[] private tokenInfoList;
   Counters.Counter private infoCnt;

   mapping(address => bool) private blackList;  // walletAddress => blacked or not

   address private fundAddress;
   address private multisigWallet;

   uint16 private ROYALTY_MIN = 5 * 1e2;     // over 1e3, 500 means 0.5%
   uint16 private ROYALTY_MAX = 1 * 1e4;     // over 1e3, 10000 means 10%
   uint16 private TX_FEE = 2 * 1e3;          // over 1e3, 2000 means 2%

   uint16 private royaltyRate = ROYALTY_MIN;   // over 1e3, 500 means 0.5%

   uint8 private AUCTION_TYPE_SALE = 0;
   uint8 private AUCTION_TYPE_NORMAL = 1;
   uint8 private AUCTION_TYPE_TIME = 2;
   uint8 private AUCTION_TYPE_SILENT = 3;

   IRevAuction private auctionContract;
   IRevSale private saleContract;
   IRevNFT private revNFT;

   function checkNotScammer(address sender_) internal view {
      require (!blackList[sender_] && sender_ != address(0), 'scam address');
   }

   function checkMultiSigWallet(address sender_) internal view {
      require (sender_ == multisigWallet, 'not multisig wallet');
   }

   constructor(
      address auctionAddress_, 
      address saleAddress_,
      address revNFT_, 
      address multisigWallet_
   ) {
      auctionContract = IRevAuction(auctionAddress_);
      saleContract = IRevSale(saleAddress_);
      revNFT = IRevNFT(revNFT_);
      fundAddress = _msgSender();
      multisigWallet = multisigWallet_;
   }

   /**
   @dev set royalty of NFT. 0.5% <= royalty <= 10%
    */
   function setRoyalty(uint16 royalty_) external whenNotPaused onlyOwner {
      require (royalty_ >= ROYALTY_MIN && royalty_ <= ROYALTY_MAX, 'not proper rate');
      royaltyRate = royalty_;
   }

   function getRoyalty() external view returns(uint16) {
      return royaltyRate;
   }

   function addToBlackList(address scammerAddress_) external whenNotPaused {
      require (!blackList[scammerAddress_], 'already added');
      checkMultiSigWallet(_msgSender());
      blackList[scammerAddress_] = true;
   }

   function pause() external whenNotPaused {
      checkMultiSigWallet(_msgSender());
      _pause();
   }

   function mintNFT(string memory tokenURI_) 
   external payable 
   whenNotPaused 
   nonReentrant {
      checkNotScammer(_msgSender());
      string[] memory tokenURIs = new string[](1);
      tokenURIs[0] = tokenURI_;

      revNFT.createNFTTo{value: msg.value}(tokenURIs, _msgSender());
   }

   function listAuction(
      address tokenAddress_, 
      uint256[] memory tokenIDs_, 
      uint8 auctionType_, 
      uint256 price_, 
      uint256 timeLimit_
   ) external whenNotPaused {
      checkNotScammer(_msgSender());
      _checkCollection(_msgSender(), tokenAddress_, tokenIDs_, price_, false);
      auctionContract.listAuction(tokenAddress_, _msgSender(), tokenIDs_, auctionType_, timeLimit_, price_);
   }

   function bidAuction(uint256 auctionID_) external payable nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      auctionContract.bidAuction(_msgSender(), auctionID_, msg.value);

      if (auctionContract.getAuctionType(auctionID_) == AUCTION_TYPE_SALE) {
         _closeAuction(auctionContract.getAuctionOwner(auctionID_), auctionID_);
      }
   }

   function closeAuction(uint256 auctionID_) external nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      _closeAuction(_msgSender(), auctionID_);
   }

   function getAuctionList() external view returns(AuctionDispInfo[] memory) {
      checkNotScammer(_msgSender());
      return auctionContract.getAuctionList();
   }

   function getAuctionListByOwner(address owner_) external view returns(AuctionDispInfo[] memory) {
      checkNotScammer(_msgSender());
      return auctionContract.getAuctionListByOwner(owner_);
   }
   function getBidderInfo(uint256 auctionID_) external view returns(AuctionAskUserInfo[] memory) {
      checkNotScammer(_msgSender());
      return auctionContract.getBidderInfo(auctionID_);
   }

   function batchTransfer(
      address receive_, 
      TokenInfo[] memory tokens_
   ) external nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      for (uint256 i = 0; i < tokens_.length; i ++) {
         TokenInfo memory transferInfo = tokens_[i];
         require (tokenInfos[transferInfo.tokenAddress][transferInfo.tokenID].owner != address(0), 'not exist token');
         require (tokenInfos[transferInfo.tokenAddress][transferInfo.tokenID].curOwner == _msgSender(), 'not token owner');
      }
      for (uint256 i = 0; i < tokens_.length; i ++) {
         IREV721 token = IREV721(tokens_[i].tokenAddress);
         token.transferFrom(_msgSender(), receive_, tokens_[i].tokenID);
         tokenInfos[tokens_[i].tokenAddress][tokens_[i].tokenID].curOwner = receive_;
      }
   }

   function listNFTForSale(address tokenAddress_, uint256[] memory tokenIDs_, uint256 price_) external {
      _checkCollection(_msgSender(), tokenAddress_, tokenIDs_, price_, true);
      saleContract.listNFTsForSale(_msgSender(), tokenAddress_, tokenIDs_, price_);
   }

   function buyNFTs(uint256 sellID_) external payable nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      TokenSellInfo memory info = saleContract.buyNFTs(_msgSender(), sellID_, msg.value);
      _matchTransaction(info.owner, _msgSender(), info.tokenAddress, info.tokenIDs, info.price);
   }

   function getTokensForSale() external view returns (TokenSellInfo[] memory) {
      return saleContract.getTokensForSale();
   }
   function getSaleTokensByOwner(address owner_) external view returns(TokenSellInfo[] memory) {
      return saleContract.getSaleTokensByOwner(owner_);
   }

   function getAllTokens() external view returns(TokenDispInfo[] memory) {
      return _getTokens(address(0));
   }

   function getTokensByOwner(address owner_) external view returns(TokenDispInfo[] memory) {
      return _getTokens(owner_);
   }

   function getTokenDetail(address tokenAddress_, uint256 tokenID_) external view returns(TokenDetailInfo memory) {
      uint256 price = _getTradePrice(tokenAddress_, tokenID_);
      uint256 auctionID = auctionContract.getAuctionIDByToken(tokenAddress_, tokenID_);
      AuctionAskUserInfo[] memory bidders = auctionID == 0 ? new AuctionAskUserInfo[](0) : auctionContract.getBidderInfo(auctionID - 1);

      return TokenDetailInfo({
         tokenInfo: TokenInfo({
            tokenAddress: tokenAddress_,
            tokenID: tokenID_
         }),
         owner: tokenInfos[tokenAddress_][tokenID_].curOwner,
         price: price,
         offerCount: tokenInfos[tokenAddress_][tokenID_].offers.length,
         sellID: saleContract.getSaleID(tokenAddress_, tokenID_),
         offers: tokenInfos[tokenAddress_][tokenID_].offers,
         forAuction: bidders.length > 0,
         bidders: bidders,
         forSale: saleContract.getSaleStatus(tokenAddress_, tokenID_)
      });
   }

   /**
   @dev offerTokens_[0]: askToken, offerTokens_[1]: bidToken
   */
   function makeOffer(TokenInfo[] memory offerTokens_, uint256 askAmount_)
   external payable nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      require (offerTokens_.length > 0 && offerTokens_.length <= 2, 'not correct offer');
      require (offerTokens_.length == 2 || (offerTokens_.length == 1 && msg.value > 0 && askAmount_ == 0), 'not correct offer');
      for (uint256 i = 0; i < offerTokens_.length; i ++) {
         require (
            tokenInfos[offerTokens_[i].tokenAddress][offerTokens_[i].tokenID].owner != address(0), 
            'not listed token'
         );   
      }
      
      uint256 tradeAmount = msg.value;
      if (offerTokens_.length == 2) {
         IREV721 token = IREV721(offerTokens_[1].tokenAddress);
         require (
            token.getApproved(offerTokens_[1].tokenID) == address(this) || 
            token.isApprovedForAll(_msgSender(), address(this)), 'not approved'
         );
         tradeAmount += _getTradePrice(offerTokens_[1].tokenAddress, offerTokens_[1].tokenID);
         require (tradeAmount > askAmount_, 'not correct offer');
      }
      require (tokenInfos[offerTokens_[0].tokenAddress][offerTokens_[0].tokenID].curOwner != _msgSender(), 'same owner');

      tokenInfos[offerTokens_[0].tokenAddress][offerTokens_[0].tokenID].offers.push(OfferInfo({
         bider: _msgSender(),
         bidToken: offerTokens_.length == 1 ? TokenInfo({tokenAddress: address(0), tokenID: 0}) : offerTokens_[1],
         bidAmount: msg.value,
         askAmount: askAmount_,
         tradeAmount: tradeAmount - askAmount_
      }));
   }

   function acceptOffer(TokenInfo memory askToken_, uint256 acceptIndex_)
   external payable
   nonReentrant whenNotPaused {
      checkNotScammer(_msgSender());
      NFTInfo memory nftInfo = tokenInfos[askToken_.tokenAddress][askToken_.tokenID];
      require (
         (nftInfo.owner == _msgSender() && nftInfo.offers.length > acceptIndex_) && 
         msg.value >= nftInfo.offers[acceptIndex_].askAmount, 
         'wrong offer info'
      );

      uint256[] memory tokenArray = new uint256[](1);
      tokenArray[0] = askToken_.tokenID;

      delete tokenInfos[askToken_.tokenAddress][askToken_.tokenID].offers;
      saleContract.closeSale(askToken_.tokenAddress, askToken_.tokenID);

      _matchTransaction(
         _msgSender(), 
         nftInfo.offers[acceptIndex_].bider, 
         askToken_.tokenAddress,
         tokenArray,
         nftInfo.offers[acceptIndex_].bidAmount
      );

      if (nftInfo.offers[acceptIndex_].bidToken.tokenAddress != address(0)) {
         tokenArray[0] = nftInfo.offers[acceptIndex_].bidToken.tokenID;
         saleContract.closeSale(
            nftInfo.offers[acceptIndex_].bidToken.tokenAddress, 
            nftInfo.offers[acceptIndex_].bidToken.tokenID
         );
         _matchTransaction(
            nftInfo.offers[acceptIndex_].bider, 
            _msgSender(), 
            nftInfo.offers[acceptIndex_].bidToken.tokenAddress,
            tokenArray,
            nftInfo.offers[acceptIndex_].askAmount
         ); 
      }

      for (uint256 i = 0; i < nftInfo.offers.length; i ++) {
         if (i != acceptIndex_) {
            payable(nftInfo.offers[i].bider).transfer(nftInfo.offers[i].bidAmount);
         }
      }
   }
   // /**
   // @dev add new NFT to marketplace so that available for trading.
   // */
   function addCollection(address tokenAddress_, uint256[] memory tokenIDs_) 
   external whenNotPaused override {
      address sender = _msgSender(); 
      checkNotScammer(sender);
      require (tokenIDs_.length > 0, 'tokenID count should be greater than zero');

      IREV721 token = IREV721(tokenAddress_);
      for (uint256 i = 0; i < tokenIDs_.length; i ++) {
         require (tokenInfos[tokenAddress_][tokenIDs_[i]].owner == address(0), 'already added');
         if (sender != address(revNFT)) {
            require (token.ownerOf(tokenIDs_[i]) == sender, 'not owner');
         }
      }

      address tokenAddress = sender == address(revNFT) ? address(revNFT) : tokenAddress_;
      for (uint256 i = 0; i < tokenIDs_.length; i ++) {
         tokenInfos[tokenAddress][tokenIDs_[i]].owner = sender == address(revNFT) ? tokenAddress_ : sender;
         tokenInfos[tokenAddress][tokenIDs_[i]].curOwner = sender == address(revNFT) ? tokenAddress_ : sender;

         tokenInfoList.push(TokenInfo({
            tokenAddress: tokenAddress,
            tokenID: tokenIDs_[i]
         }));
         infoCnt.increment();
      }
   }

   /**
   @dev get trade trending list.
   */
   function getTrendingList() external view whenNotPaused returns(TrendingInfo[] memory) {
      TrendingInfo[] memory tempList = new TrendingInfo[](infoCnt.current());

      for (uint256 i = 0; i < infoCnt.current(); i ++) {
         tempList[i].tokenAddress = tokenInfoList[i].tokenAddress;
         tempList[i].tokenID = tokenInfoList[i].tokenID;
         tempList[i].tradePrice = tokenInfos[tokenInfoList[i].tokenAddress][tokenInfoList[i].tokenID].tradePrice;
         tempList[i].owner = tokenInfos[tokenInfoList[i].tokenAddress][tokenInfoList[i].tokenID].curOwner;
      }

      for (uint256 i = 0; i < infoCnt.current() - 1; i ++) {
         for (uint256 j = 0; j < infoCnt.current() - i - 1; j ++) {
            if (tempList[j].tradePrice < tempList[j + 1].tradePrice) {
               TrendingInfo memory temp = tempList[j];
               tempList[j] = tempList[j + 1];
               tempList[j + 1] = temp;
            }
         }
      }

      return tempList;
   }

   function getOffers(address tokenAddress_, uint256 tokenID_) external view returns(OfferInfo[] memory) {
      return tokenInfos[tokenAddress_][tokenID_].offers;
   }

   function getAuctionHistory(address tokenAddress_, uint256 tokenID_) external view returns (AuctionHistoryInfo[] memory) {
      return auctionContract.getAuctionHistory(tokenAddress_, tokenID_);
   }

   function getSaleHistory(address tokenAddress_, uint256 tokenID_) external view returns (SaleHistoryInfo[] memory) {
      return saleContract.getSaleHistory(tokenAddress_, tokenID_);
   }

   /**
   @dev withdraw BNB.
   */
   function withDraw() external whenPaused onlyOwner {
      uint256 balance = address(this).balance;
		require(balance > 0, "No fund to withdraw");
      payable(fundAddress).transfer(balance);
   }

   /**
   @dev set fund address that can withdraw BNB
   */
   function setFundAddress(address fundAddress_) external onlyOwner {
      require (fundAddress_ != address(0) && fundAddress_ != fundAddress, 'incorrect address');
      fundAddress = fundAddress_;
   }

   /**
   @dev refund money to unselected bidders.
   */
   function _refundMoney(AuctionAskUserInfo[] memory bidders_, address topBider_) internal {
      if (bidders_.length > 0) {
         for (uint256 i = 0; i < bidders_.length; i ++) {
            if (bidders_[i].askPerson != topBider_) {
               payable(bidders_[i].askPerson).transfer(bidders_[i].askPrice);
            }
         }
      }
   }

   function _matchTransaction(
      address seller_, 
      address buyer_, 
      address tokenAddress_,
      uint256[] memory tokenIDs_,
      uint256 price_
   ) internal {
      uint256 royaltyFee = price_ * royaltyRate / 1e5;
      uint256 txFee = price_ * TX_FEE / 1e5;
      payable(seller_).transfer(price_ - royaltyFee - txFee);
      payable(tokenInfos[tokenAddress_][tokenIDs_[0]].owner).transfer(royaltyFee);

      _transferCollection(seller_, buyer_, tokenAddress_, tokenIDs_, price_);
   }

   /**
   @dev transfer collection.
   */
   function _transferCollection(
      address owner_, address to_, 
      address tokenAddress_, 
      uint256[] memory tokenIDs_, 
      uint256 price_
   ) internal {
      IREV721 token = IREV721(tokenAddress_);
      uint256 perPrice = price_ / tokenIDs_.length;

      for (uint256 i = 0; i < tokenIDs_.length; i ++) {
         token.transferFrom(owner_, address(this), tokenIDs_[i]);
         token.approve(to_, tokenIDs_[i]);
         token.transferFrom(address(this), to_, tokenIDs_[i]);
         tokenInfos[tokenAddress_][tokenIDs_[i]].curOwner = to_;
         tokenInfos[tokenAddress_][tokenIDs_[i]].tradePrice = (tokenInfos[tokenAddress_][tokenIDs_[i]].tradePrice + perPrice);
         tokenInfos[tokenAddress_][tokenIDs_[i]].tradeCount ++;
      }
   }

   function _checkCollection(address owner_, address tokenAddress_, uint256[] memory tokenIDs_, uint256 price_, bool isSale) internal view {
      address originOwner = tokenInfos[tokenAddress_][tokenIDs_[0]].owner;
      IREV721 token = IREV721(tokenAddress_);

      require (originOwner != address(0), 'not added token');
      require (price_ > 0 && tokenIDs_.length > 0, 'incorrect info');
      for (uint256 i = 0; i < tokenIDs_.length; i ++) {
         bool saleStatus = saleContract.getSaleStatus(tokenAddress_, tokenIDs_[i]);
         bool auctionStatus = auctionContract.getAuctionStatus(tokenAddress_, tokenIDs_[i]);
         if (isSale == true && auctionStatus == true ||
             isSale == false && saleStatus == true) {
            revert ('can not list both sale and auction at once');
         }
         require (
            token.getApproved(tokenIDs_[i]) == address(this) || 
            token.isApprovedForAll(owner_, address(this)), 'not approved'
         );
         require (tokenInfos[tokenAddress_][tokenIDs_[i]].curOwner == owner_, 'not token owner');
         require (tokenInfos[tokenAddress_][tokenIDs_[i]].owner == originOwner, 'not same owner');
      }
   }

   function _closeAuction(address owner_, uint256 auctionID_) internal {
      (
         uint256 topPrice,
         uint256[] memory tokenIDs,
         address topBider,
         address auctionOwner,
         address tokenAddress,
         AuctionAskUserInfo[] memory bidders
      ) = auctionContract.closeAuction(owner_, auctionID_);

      if (topPrice != 0 && topBider != address(0)) {
         _matchTransaction(
            auctionOwner, topBider, 
            tokenAddress, 
            tokenIDs, 
            topPrice
         );
         _refundMoney(bidders, topBider);
      }
   }  

   function _getTokens(address owner_) internal view returns (TokenDispInfo[] memory) {
      
      uint256 index = 0;
      if (infoCnt.current() == 0) { return new TokenDispInfo[](0); }

      uint256 cnt = 0;
      for (uint256 i = 0; i < infoCnt.current(); i ++) {
         if (
            owner_ == address(0) || 
            tokenInfos[tokenInfoList[i].tokenAddress][tokenInfoList[i].tokenID].curOwner == owner_
         ) { cnt ++; }
      }
      if (cnt == 0) { return new TokenDispInfo[](0); }

      TokenDispInfo[] memory listedTokens = new TokenDispInfo[](cnt);
      for (uint256 i = 0; i < infoCnt.current(); i ++) {
         TokenInfo memory info = tokenInfoList[i];
         address tokenOwner = tokenInfos[tokenInfoList[i].tokenAddress][tokenInfoList[i].tokenID].curOwner;
         uint256 offerCount = tokenInfos[info.tokenAddress][info.tokenID].offers.length;
         if (owner_ == address(0) || owner_ == tokenOwner) {
            listedTokens[index] = TokenDispInfo({
               tokenAddress: info.tokenAddress,
               owner: tokenOwner,
               tokenID: info.tokenID,
               price: _getTradePrice(info.tokenAddress, info.tokenID),
               offerCount: offerCount,
               forSale: saleContract.getSaleStatus(info.tokenAddress, info.tokenID),
               forAuction: auctionContract.getAuctionStatus(info.tokenAddress, info.tokenID)
            });
            index ++;
         }
      }

      return listedTokens;
   }  

   function _getTradePrice(address tokenAddress_, uint256 tokenID_) internal view returns(uint256 price) {
      bool forSale = saleContract.getSaleStatus(tokenAddress_, tokenID_);
      if (forSale == true) {
         price = saleContract.getSalePrice(tokenAddress_, tokenID_);
      } else {
         price = tokenInfos[tokenAddress_][tokenID_].tradeCount == 0 ? 
         0 : 
         tokenInfos[tokenAddress_][tokenID_].tradePrice / tokenInfos[tokenAddress_][tokenID_].tradeCount;
      }
   }
}

