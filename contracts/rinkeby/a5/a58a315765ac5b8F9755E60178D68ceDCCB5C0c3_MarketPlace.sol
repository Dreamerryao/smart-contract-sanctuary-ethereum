pragma solidity ^0.8.4;

interface ICommonStruct {
   struct NFTInfo {
      address owner;
      address curOwner;
      uint256 tradePrice;
      uint256 tradeCount;
      uint256 offerCount;
      OfferInfo[] offers;
   }

   struct AuctionAskUserInfo {
      address askPerson;
      uint256 askPrice;
   }

   struct AuctionDetail {
      AuctionAskUserInfo[] bidders;
      uint256[] tokenIDs;
   }

   struct AuctionInfo {
      address owner;
      address topBider;
      address tokenAddress;
      uint256 timeLimit;
      uint256 price;
      uint256 topPrice;
      uint8 auctionType;
      bool forAuction;
   }

   struct CloseAuctionInfo {
      address owner;
      address tokenAddress;
      address topBider;
      uint256 topPrice;
      uint256[] tokenIDs;
      AuctionAskUserInfo[] bidders;
      uint8 auctionType;
   }

   struct AuctionHistoryInfo {
      address owner;
      address topBider;
      uint256 tradePrice;  
   }

   struct SaleHistoryInfo {
      address seller;
      address buyer;
      uint256 tradePrice;
   }

   struct TokenInfo {
      address tokenAddress;
      uint256 tokenID;
   }

   struct TokenDispInfo {
      address tokenAddress;
      address owner;
      uint256 tokenID;
      uint256 price;
      uint256 offerCount;
      bool forSale;
      bool forAuction;
   }

   struct TokenDetailInfo {
      TokenInfo tokenInfo;
      address owner;
      uint256 price;
      uint256 offerCount;
      uint256 sellID;
      AuctionAskUserInfo[] bidders;
      OfferInfo[] offers;
      bool forAuction;
      bool forSale;
   }

   struct TrendingInfo {
      address owner;
      address tokenAddress;
      uint256 tokenID;
      uint256 tradePrice;
   }

