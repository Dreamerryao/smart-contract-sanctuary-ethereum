pragma solidity ^0.8.4;
import "./ICommonStruct.sol";

interface IRevAuction is ICommonStruct {
   function listAuction(
       address tokenAddress_,
       address owner_,
       uint256[] memory tokenIDs_,
       uint8 auctionType_,
       uint256 timeLimit_,
       uint256 price_
   ) external;

   function bidAuction(address sender_, uint256 auctionID_, uint256 amount_) external;

   function closeAuction(
      address sender_, 
      uint256 auctionID_
   ) external returns(
      uint256 topPrice,
      uint256[] memory tokenIDs,
      address topBider,
      address owner,
      address tokenAddress,
      AuctionAskUserInfo[] memory bidders
   );

   function getBidderInfo(
      uint256 auctionID_
   ) external view returns(AuctionAskUserInfo[] memory);

   function getAuctionListByOwner(
      address owner_
   ) external view returns(AuctionDispInfo[] memory);

   function getAuctionList() external view returns(AuctionDispInfo[] memory);
   function getAuctionType(uint256 auctionID_) external view returns(uint8);
   function getAuctionOwner(uint256 auctionID_) external view returns(address);
   function getAuctionStatus(address tokenAddress_, uint256 tokenID_) external view returns(bool);
   function getBidCount(address tokenAddress_, uint256 tokenID_) external view returns(uint256);
   function getAuctionIDByToken(address tokenAddress_, uint256 tokenID_) external view returns(uint256);

   function getAuctionHistory(
      address tokenAddress_, 
      uint256 tokenID_
   ) external view returns(AuctionHistoryInfo[] memory);

   event BidAuction(
      uint256 auctionID,
      address bider,
      uint256 bidPrice,
      uint8 auctionType
   );
   event ListAuction(
      address tokenAddress, 
      address owner, 
      uint256[] tokenIDs, 
      uint256 price, 
      uint256 limitTime, 
      uint8 auctionType
   );
   event MatchedAuction(
      address tokenAddress,
      address owner,
      address topBider,
      uint256 finalPrice,
      uint8 auctionType
   );
   
}

