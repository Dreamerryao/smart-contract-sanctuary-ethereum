/// @title: WalletLimiter
/// @author: unrealaccelerator.io

/**
@notice A utility contract that provides functions for:
 - Enforcing a default wallet limit for all wallets || wallet limit for specific wallets
 - Providing the limit for a specific wallet
 - Providing a count for a specific wallet
 - Providing validation that a wallet transaction will not exceed it's limit
 - Note on Limitations:
 --- Does not allow for infinite limits
 --- Does not allow for multiple transaction types. In other words, you can keep up with
 --- the number of mints or claims, or mints and claims, but not mints and claims independently.
 - Note: it actually limits by account but everybody calls them wallets ¯\_(ツ)_/¯
 */

contract WalletLimiter {
    /**
    @dev A mapping to keep up with a wallets transaction count
    @notice Contracts implementing this utility need to call _incrementWalletCount()
    */
    mapping(address => uint256) private _walletCount;
    /**
    @dev A mapping to keep up with specific wallets transaction limits
    @notice Contracts implementing this utility need to call _incrementWalletCount()
    */
    mapping(address => uint256) private _walletLimit;
    /**
    @dev A default limit for all wallets without a specified limit
    @notice Contracts implementing this utility need to call _setWalletLimitDefault
    to update the limit from the default of 0
    */
    uint256 private _walletLimitDefault;

    function _setWalletLimitDefault(uint256 walletMintLimitDefault_) internal {
        _walletLimitDefault = walletMintLimitDefault_;
    }

    function _setWalletLimit(address account, uint256 walletMintLimit_)
        internal
    {
        _walletLimit[account] = walletMintLimit_;
    }

    /**
    @dev Gets the wallet limit
    @notice Contracts implementing this contract can call this function to
    get the current wallet limit
    @param account The account for which the current count is being requested
    */
    function _getWalletLimit(address account) internal view returns (uint256) {
        return
            (_walletLimit[account] > 0)
                ? _walletLimit[account]
                : _walletLimitDefault;
    }

    /**
    @dev Increments the wallet count
    @notice Contracts implementing this contract should call this function from
    transactions which are being limited for a specific wallet
    @param account The account for which the transaction is occuring
    @param amount The quantity of the thing that is being limited
    */
    function _incrementWalletCount(address account, uint256 amount) internal {
        _walletCount[account] += amount;
    }

    /**
    @dev Gets the wallet count
    @notice Contracts implementing this contract can call this function to
    get the current wallet count
    @param account The account for which the current count is being requested
    */
    function _getWalletCount(address account) internal view returns (uint256) {
        return _walletCount[account];
    }

    /**
    @dev Returns a boolean indicating whether the requested transaction should occur
    based on the wallet's limit
    @notice Returns true if the transaction does not violate the limit for the
    specified wallet. Returns false if the transaction will violate the limit for
    the specified wallet
    @param account The account for which the transaction is being verified
    @param amount The quantity of the thing that is being limited
    */
    function _validateWalletCount(address account, uint256 amount)
        internal
        view
        returns (bool)
    {
        return ((_walletCount[account] + amount) <=
            (
                (_walletLimit[account] > 0)
                    ? _walletLimit[account]
                    : _walletLimitDefault
            ));
    }
}

