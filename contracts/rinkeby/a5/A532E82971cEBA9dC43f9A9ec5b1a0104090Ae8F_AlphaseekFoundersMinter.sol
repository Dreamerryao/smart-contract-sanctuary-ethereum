pragma solidity ^0.8.9;

/// @title: AlphaseekFoundersMinter
/// @author: unrealaccelerator.io

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "./utils/SignedMinting.sol";
import "./utils/AllowListMinting.sol";
import "./utils/WalletLimiter.sol";
import "./finance/Provisioner.sol";
import "./access/AccessControlMinter.sol";
import "./interfaces/IMintableERC721.sol";
import "./interfaces/IMinterERC721.sol";

contract AlphaseekFoundersMinter is
    AccessControlMinter,
    ReentrancyGuard,
    SignedMinting,
    AllowListMinting,
    WalletLimiter,
    Provisioner,
    IMinterERC721
{
    IMintableERC721 public tokenContract;

    bool public mintingPaused;
    bool public privateMintAllowed;
    event PrivateMintStarted(bool indexed state);
    bool public waitlistMintAllowed;
    event WaitlistMintStarted(bool indexed state);
    bool public walletLimitRequired = true;
    bool public signatureRequired = true;
    address public promoter;
    bool public preminted;
    uint256 public constant MINTABLE_TOKENS = 1000;
    uint256 public constant PROMOTION_TOKENS = 111;
    uint256 public mintPrice = 3 ether;
    mapping(address => uint256) public minterCount;

    constructor(
        address _mintable,
        address _signer,
        address _administrator,
        address _developer,
        address _promoter,
        address[] memory _payees,
        uint256[] memory _shares
    )
        SignedMinting(_signer)
        Provisioner(_payees, _shares)
        AccessControlMinter()
    {
        require(
            ERC165Checker.supportsInterface(
                _mintable,
                type(IMintableERC721).interfaceId
            ),
            "Invalid token contract"
        );
        require(_administrator != address(0), "Invalid administrator address");
        require(_developer != address(0), "Invalid developer address");
        require(_promoter != address(0), "Invalid promoter address");
        promoter = _promoter;
        tokenContract = IMintableERC721(_mintable);
        _grantRole(DEFAULT_ADMIN_ROLE, _administrator);
        _grantRole(DEFAULT_ADMIN_ROLE, _developer);
        _setWalletLimitDefault(0);
    }

    /**
     * @dev premint of promotional tokens
     */
    function premint() external onlyAuthorized {
        require(!preminted, "Already preminted");
        _incrementWalletCount(promoter, PROMOTION_TOKENS);
        preminted = true;
        _mint(promoter, PROMOTION_TOKENS);
    }

    /**
     * @dev mint to an account
     * @notice For each phase of minting, the merkle proof and wallet limits
     * will be updated for the allowed addresses.
     */
    function mint(
        address _to,
        string memory _nonce,
        bytes memory _signature,
        bytes32[] calldata _merkleProof
    ) external payable callerIsUser onlyActiveSale {
        uint256 _amount = 1; // one per transaction
        require(mintPrice <= msg.value, "Wrong amount");
        require(
            !signatureRequired ||
                (signatureRequired &&
                    SignedMinting.validateSignature(
                        abi.encodePacked(_msgSender(), _nonce, _amount),
                        _signature
                    )),
            "Invalid signature"
        );
        require(_validateMerkleProof(_to, _merkleProof), "Not listed");
        require(
            !walletLimitRequired ||
                (walletLimitRequired && _validateWalletCount(_to, _amount)),
            "Wallet limit"
        );
        _incrementWalletCount(_to, _amount);
        minterCount[_msgSender()] += _amount; // for claims
        _mint(_to, _amount);
    }

    /**
     * @dev Admin mint an amount to an account
     */
    function adminMint(address _to, uint256 _amount) external onlyAuthorized {
        _incrementWalletCount(_to, _amount);
        _mint(_to, _amount);
    }

    /**
     * @dev Mints a new token
     */
    function _mint(address _to, uint256 _amount) internal nonReentrant {
        require(_to != address(0), "Cannot mint to 0 address");
        require(
            tokenContract.totalSupply() < MINTABLE_TOKENS + PROMOTION_TOKENS,
            "Exceeds supply"
        );
        tokenContract.mint(_to, _amount);
    }

    /**
     * @dev Get the total tokens minted from the mintable contract
     */
    function totalMinted() external view returns (uint256) {
        return tokenContract.totalSupply();
    }

    /**
     * @dev Set the mintable token contract
     */
    function setTokenContract(address _tokenContract) external onlyAuthorized {
        require(
            ERC165Checker.supportsInterface(
                _tokenContract,
                type(IMintableERC721).interfaceId
            ),
            "Invalid token contract"
        );
        tokenContract = IMintableERC721(_tokenContract);
    }

    /**
     * @dev Flag to control all minting
     */
    function setMintingPaused(bool _mintingPaused) external onlyAuthorized {
        mintingPaused = _mintingPaused;
    }

    /**
     * @dev Flag to control Private minting
     */
    function startPrivateMint(bytes32 privateMerkleRoot_)
        external
        onlyAuthorized
    {
        require(preminted, "Must premint before sale");
        require(!privateMintAllowed, "Private mint already started");
        _setAllowListMerkelRoot(privateMerkleRoot_);
        privateMintAllowed = true;
        waitlistMintAllowed = false;
        _setWalletLimitDefault(1);
        emit PrivateMintStarted(true);
    }

    /**
     * @dev Flag to control Waitlist minting
     */
    function startWaitlistMint(bytes32 waitlistMerkleRoot_)
        external
        onlyAuthorized
    {
        require(preminted, "Must premint before sale");
        require(!waitlistMintAllowed, "Waitlist mint already started");
        _setAllowListMerkelRoot(waitlistMerkleRoot_);
        privateMintAllowed = false;
        waitlistMintAllowed = true;
        _setWalletLimitDefault(2);
        emit WaitlistMintStarted(true);
    }

    /**
     * @dev Set the price for Minting
     */
    function setmintPrice(uint256 _mintPrice) external onlyAuthorized {
        mintPrice = _mintPrice;
    }

    /**
     * @dev Flag to control signature gated minting.
     */
    function setSignatureRequired(bool _signatureRequired)
        external
        onlyAuthorized
    {
        signatureRequired = _signatureRequired;
    }

    /**
     * @dev Flag to control wallet limit gated minting.
     */
    function setWalletLimitRequired(bool _walletLimitRequired)
        external
        onlyAuthorized
    {
        walletLimitRequired = _walletLimitRequired;
    }

    /**
     * @dev {See WalletLimiter-_setWalletLimitDefault}
     */
    function setWalletLimitDefault(uint256 walletMintLimitDefault_)
        external
        onlyAuthorized
    {
        _setWalletLimitDefault(walletMintLimitDefault_);
    }

    /**
     * @dev {See WalletLimiter-_setWalletLimit}
     */
    function setWalletLimit(address account, uint256 walletMintLimit_)
        external
        onlyAuthorized
    {
        _setWalletLimit(account, walletMintLimit_);
    }

    /**
     * @dev {See WalletLimiter-_getWalletLimit}. Useful for web3 interfaces
     */
    function getWalletLimit(address account) external view returns (uint256) {
        return _getWalletLimit(account);
    }

    /**
     * @dev {See WalletLimiter-_getWalletCount}. Useful for web3 interfaces
     */
    function getWalletCount(address account) external view returns (uint256) {
        return _getWalletCount(account);
    }

    /**
     * @dev Set the signer. {See SignedMinting-_setSigner}
     */
    function setSigner(address _signer) external onlyAuthorized {
        _setSigner(_signer);
    }

    /**
     * @dev Get the signer (for testing). {See SignedMinting-_getSigner}
     */
    function getSigner() external view returns (address) {
        return _getSigner();
    }

    /**
     * @dev Modifier to check that the caller is a user
     */
    modifier callerIsUser() {
        require(tx.origin == _msgSender(), "Caller is contract");
        _;
    }

    /**
     * @dev Modifier to check for active sale
     */
    modifier onlyActiveSale() {
        validateActiveSale();
        _;
    }

    function validateActiveSale() private view {
        require(!mintingPaused, "Minting paused");
        require(privateMintAllowed || waitlistMintAllowed, "Sale not started");
    }

    /**
     * @dev {See IERC165-supportsInterface}
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlMinter, IMinterERC721)
        returns (bool)
    {
        return
            AccessControlMinter.supportsInterface(interfaceId) ||
            type(IMinterERC721).interfaceId == interfaceId;
    }
}

