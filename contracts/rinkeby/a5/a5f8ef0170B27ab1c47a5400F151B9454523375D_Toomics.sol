import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/ERC20BurnableUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";

import "./TMRToken.sol";
import "./TMGToken.sol";
import "./interfaces/INFT.sol";

import "erc721a-upgradeable/contracts/IERC721AUpgradeable.sol";
import "erc721a-upgradeable/contracts/extensions/ERC721ABurnableUpgradeable.sol";
import "./Validator.sol";

contract Toomics is
    Initializable,
    AccessControlUpgradeable,
    ERC721HolderUpgradeable,
    Validator
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    address tmgToken;
    address tmrToken;

    // check if the nft contract is allowed for toomics contract
    mapping(address => bool) public nftAddressToAuthorized;

    event WithdrawToken(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        uint256 tmgAmount,
        uint256 tmrAmount
    );
    event DepositToken(
        address indexed requester,
        uint256 indexed userId,
        uint256 tmgAmount,
        uint256 tmrAmount
    );
    event WithdrawNFT(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        address indexed nftAddress,
        uint256 tokenId
    );
    event DepositNFT(
        address indexed requester,
        uint256 indexed userId,
        address indexed nftAddress,
        uint256 tokenId
    );
    event MintNFT(
        address indexed requester,
        uint256 nonce,
        uint256 indexed userId,
        uint256 localId,
        address indexed nftAddress
    );

  

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(
        address _tmgToken,
        address _tmrToken,
        address _nftCharacters,
        address _nftBadges,
        address _nftEquipments
    ) public initializer {
        __AccessControl_init();
        tmgToken = _tmgToken;
        tmrToken = _tmrToken;

        nftAddressToAuthorized[_nftCharacters] = true;
        nftAddressToAuthorized[_nftBadges] = true;
        nftAddressToAuthorized[_nftEquipments] = true;

        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        __EIP712_init("toomics", "1");
    }

    // Don't need proxy to call

    function depositToken(
        uint256 userId,
        uint256 tmgAmount,
        uint256 tmrAmount
    ) external {
        IERC20Upgradeable(tmgToken).safeTransferFrom(
            msg.sender,
            address(this),
            tmgAmount
        );

        IERC20Upgradeable(tmrToken).safeTransferFrom(
            msg.sender,
            address(this),
            tmrAmount
        );
        emit DepositToken(msg.sender, userId, tmgAmount, tmrAmount);
    }

    function depositNFT(
        uint256 userId,
        address nftAddress,
        uint256[] calldata tokenIds
    )
        external
        isOurNFTaddress(nftAddress)
        isValidArray(tokenIds)
    {
        //  require(tokenIds.length > 0 && tokenIds.length <= 5, "Invalid array");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721AUpgradeable(nftAddress).safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i]
            );
            emit DepositNFT(msg.sender, userId, nftAddress, tokenIds[i]);
        }
    }

    // Need proxy to call

    function withdrawToken(
        bytes[] calldata signature,
        uint256 nonce,
        uint256 userId,
        uint256 tmgAmount,
        uint256 tmrAmount
    ) external validatorPrecheck(signature, nonce) {
        require(
            _checkWithdrawToken(
                signature,
                nonce,
                msg.sender,
                userId,
                tmgAmount,
                tmrAmount
            ),
            "withdrawToken: invalid signature"
        );

        IERC20Upgradeable(tmgToken).safeTransfer(msg.sender, tmgAmount);

        if (IERC20Upgradeable(tmrToken).balanceOf(address(this)) < tmrAmount) {
            TMRToken(tmrToken).mint(msg.sender, tmrAmount);
        } else {
            IERC20Upgradeable(tmrToken).safeTransfer(msg.sender, tmrAmount);
        }
        prevNonce[msg.sender] = nonce;

        emit WithdrawToken(msg.sender, nonce, userId, tmgAmount, tmrAmount);
    }

    function withdrawNFT(
        bytes[] calldata signature,
        uint256 nonce,
        uint256 userId,
        address nftAddress,
        uint256[] calldata tokenIds
    )
        external
        isOurNFTaddress(nftAddress)
        validatorPrecheck(signature, nonce)
        isValidArray(tokenIds)
    {
        //  require(tokenIds.length > 0 && tokenIds.length <= 5, "Invalid array");
        require(
            _checkWithdrawNFT(
                signature,
                nonce,
                msg.sender,
                userId,
                nftAddress,
                tokenIds
            ),
            "withdrawNFT: invalid signature"
        );

        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721AUpgradeable(nftAddress).safeTransferFrom(
                address(this),
                msg.sender,
                tokenIds[i]
            );
            emit WithdrawNFT(
                msg.sender,
                nonce,
                userId,
                nftAddress,
                tokenIds[i]
            );
        }
        prevNonce[msg.sender] = nonce;
    }

    function mintNFT(
        bytes[] calldata signature,
        uint256 nonce,
        uint256 userId,
        uint256[] calldata localIds,
        address nftAddress
    )
        external
        isOurNFTaddress(nftAddress)
        validatorPrecheck(signature, nonce)
        isValidArray(localIds)
    {
        // require(localIds.length > 0 && localIds.length <= 5, "Invalid array");
        require(
            _checkMintNFT(
                signature,
                nonce,
                msg.sender,
                userId,
                localIds,
                nftAddress
            ),
            "mintNFT: invalid signature"
        );
        for (uint256 i = 0; i < localIds.length; i++) {
            INFT(nftAddress).mint(msg.sender, 1);
            emit MintNFT(msg.sender, nonce, userId, localIds[i], nftAddress);
        }
        prevNonce[msg.sender] = nonce;
    }

    //admin role

    function burnToken(uint256 tmgAmount, uint256 tmrAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        ERC20BurnableUpgradeable(tmgToken).burn(tmgAmount);
        ERC20BurnableUpgradeable(tmrToken).burn(tmrAmount);
    }

    function burnNFT(address nftAddress, uint256[] calldata tokenIds)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
        isOurNFTaddress(nftAddress)
    {
        require(tokenIds.length > 0, "Invalid array");

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                IERC721AUpgradeable(nftAddress).ownerOf(tokenIds[i]) ==
                    address(this),
                "Toomics/burnNFT: not the owner"
            );
            ERC721ABurnableUpgradeable(nftAddress).burn(tokenIds[i]);
        }
    }

    // fallback() external {
    //     revert("Toomics: Fallback called");
    // }

    modifier isOurNFTaddress(address nftAddress) {
        require(
            nftAddressToAuthorized[nftAddress],
            "Toomics: Not our NFT address"
        );
        _;
    }
    modifier isValidArray(uint256[] calldata array) {
        require(
            array.length > 0 && array.length <= 5,
            "Toomics: Invalid array"
        );
        _;
    }
}

