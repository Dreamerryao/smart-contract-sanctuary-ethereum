import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "hardhat/console.sol";
pragma solidity ^0.8.4;

contract Validator is EIP712Upgradeable {
    bytes32 public constant WITHDRAW_TOKEN =
        keccak256(
            "WithdrawToken(uint256 nonce,address userAddress,uint256 userId,uint256 tmgAmount,uint256 tmrAmount)"
        );
    bytes32 public constant WITHDRAW_NFT =
        keccak256(
            "WithdrawNFT(uint256 nonce,address userAddress,uint256 userId,address nftAddress,uint256[] tokenIds)"
        );

    bytes32 public constant MINT_NFT =
        keccak256(
            "MintNFT(uint256 nonce,address userAddress,uint256 userId,uint256[] localIds,address nftAddress)"
        );

    address[] private validators;
    mapping(address => uint256) prevNonce;

    event AddValidator(address validator);
    event RemoveValidator(address validator);
    event RejectTransaction(address requester, uint256 nonce);

    function getValidators() public view returns (address[] memory) {
        return validators;
    }

    function getChainID() public view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function addValidator(address _validator) public {
        require(_validator != address(0), "Null address");
        for (uint256 index = 0; index < validators.length; index++) {
            require(
                _validator != validators[index],
                "Validator: Already added"
            );
        }
        validators.push(_validator);
        emit AddValidator(_validator);
    }

    function removeValidator(address _validator) public {
        for (uint256 index = 0; index < validators.length; index++) {
            if (_validator == validators[index]) {
                validators[index] = validators[validators.length - 1];
                validators.pop();
                emit RemoveValidator(_validator);
                return;
            }
        }
        revert("Validator: Could not find validator to remove");
    }

    function rejectTransaction(uint256 nonce) public {
        require(prevNonce[msg.sender] < nonce, "Validator: wrong nonce");
        prevNonce[msg.sender] = nonce;
        emit RejectTransaction(msg.sender, nonce);
    }

    function _checkWithdrawToken(
        bytes[] calldata signature,
        uint256 nonce,
        address userAddress,
        uint256 userId,
        uint256 tmgAmount,
        uint256 tmrAmount
    ) internal view returns (bool) {
        // digest the data to transactionHash
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    WITHDRAW_TOKEN,
                    nonce,
                    userAddress,
                    userId,
                    tmgAmount,
                    tmrAmount
                )
            )
        );
        for (uint256 index = 0; index < signature.length; index++) {
            if (!_checkSignature(signature[index], digest)) return false;
        }
        return true;
    }

    function _checkWithdrawNFT(
        bytes[] calldata signature,
        uint256 nonce,
        address userAddress,
        uint256 userId,
        address nftAddress,
        uint256[] calldata tokenIds
    ) internal view returns (bool) {
        // digest the data to transactionHash
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    WITHDRAW_NFT,
                    nonce,
                    userAddress,
                    userId,
                    nftAddress,
                    keccak256(abi.encodePacked(tokenIds))
                )
            )
        );
        for (uint256 index = 0; index < signature.length; index++) {
            if (!_checkSignature(signature[index], digest)) return false;
        }
        return true;
    }

    function _checkMintNFT(
        bytes[] calldata signature,
        uint256 nonce,
        address userAddress,
        uint256 userId,
        uint256[] calldata localIds,
        address nftAddress
    ) internal view returns (bool) {
        // digest the data to transactionHash
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    MINT_NFT,
                    nonce,
                    userAddress,
                    userId,
                    keccak256(abi.encodePacked(localIds)),
                    nftAddress
                )
            )
        );
        for (uint256 index = 0; index < signature.length; index++) {
            if (!_checkSignature(signature[index], digest)) return false;
        }
        return true;
    }

    function _checkSignature(bytes memory signature, bytes32 digest)
        private
        view
        returns (bool)
    {
        address checkAdress = ECDSAUpgradeable.recover(digest, signature);

        for (uint256 index = 0; index < validators.length; index++) {
            if (checkAdress == validators[index]) {
                return true;
            }
        }
        return false;
    }

    modifier validatorPrecheck(bytes[] memory signature, uint256 nonce) {
        require(signature.length > 0, "validator(s) is empty");
        require(
            prevNonce[msg.sender] < nonce,
            "Validator: wrong nonce/already excuted"
        );

        // require(
        //     (signature.length * 100) / validators.length >= threshold,
        //     "Threshold not reached"
        // );

        if (signature.length >= 2) {
            for (uint256 i = 0; i < signature.length; i++) {
                for (uint256 j = i + 1; j < signature.length; j++) {
                    require(
                        keccak256(abi.encodePacked(signature[i])) !=
                            keccak256(abi.encodePacked(signature[j])),
                        "Can not be the same signature"
                    );
                }
            }
        }
        _;
    }
}

