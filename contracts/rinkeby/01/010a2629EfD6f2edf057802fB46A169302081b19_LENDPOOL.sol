        maximum=_maximum;
    }



    function setIntrest (uint _interest)public{
        interest=_interest;
    }

    function changePrice(uint256 _price) public {
        Price=_price;
    }




    function setLIQUIDATION_THRESHOLD(uint256 _liquidationThreshold)public{
        LIQUIDATION_THRESHOLD=_liquidationThreshold;

    }




