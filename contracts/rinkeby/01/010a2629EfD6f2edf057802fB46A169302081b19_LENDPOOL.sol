contract LENDPOOL is IMasterContract{
    using SafeMath for uint256;
    error No_masterContract();
    //init per clone
    ITestLUSDC public TestLUSDC;
    IERC20 public TestUSDC;
    IERC721 public TestBlueChipNFT;
    IDEBTNFT public DEBTNFT;
    IOracle public oracle;
    IAuction public auctionContract;
    bytes oracleData;
    uint public maxBorrow;
    address public ReserveInterestRateAddress;
    uint256 public COLLATERIZATION_RATE;
    uint256 public BORROW_OPENING_FEE;
    uint256 public totalLiquidity;
    uint256 public BORROW_OPENING_FEE_PRECISION;
    uint256 public LIQUIDATION_THRESHOLD;
    uint256 public maximum;
    uint256 public totalBorrow;
    
    uint public Price;
    uint public interest;
    uint public Debt;


 


    struct BorrowCap {
        uint128 total;
        uint128 borrowPartPerAddress;
    }

  
    BorrowCap public borrowLimit;

    /// @notice Mapping from clone contracts to their masterContract.
    mapping(address => address) public masterContractOf;
    mapping (address=>uint256) public userBorrowPart;
    mapping (address=>uint256) public userLpProvided;
    mapping (address=>uint256) public userDebt;
    mapping(address=>uint256) public userdeposit;


    event LogBorrow(address asset, address indexed to, uint256 amount);
    event LogRepay(address indexed to, uint256 amount, uint256 part);
    event LogFeeTo(address indexed newFeeTo);
    event LogDeploy(address indexed masterContract, bytes data, address indexed cloneAddress);
    event LogDeposited (address to, uint tokenId );
    event LogProvideLiquidity(address to, uint256 amount);
    event LogWithdrawLiquidity(address to, uint256 amount);
    
    

    function init(bytes calldata data) public payable  override{
        require(address(TestBlueChipNFT) == address(0), "already initialized");
        (TestUSDC, TestBlueChipNFT, DEBTNFT, TestLUSDC) = abi.decode(data, (IERC20, IERC721, IDEBTNFT,ITestLUSDC));
        require(address(TestBlueChipNFT) != address(0), " bad pair");

        //(, exchangeRate) = oracle.get(oracleData);
    }


   

    /// @notice Deploys a given master Contract as a clone.
    /// Any ETH transferred with this call is forwarded to the new clone.
    /// Emits `LogDeploy'.
    /// @param masterContract The address of the contract to clone.
    /// @param data Additional abi encoded calldata that is passed to the new clone via `IMasterContract.init`.
    /// @param useCreate2 Creates the clone by using the CREATE2 opcode, in this case `data` will be used as salt.
    /// @return cloneAddress Address of the created clone contract.
    function deploy(
        address masterContract,
        bytes calldata data,
        bool useCreate2
    ) public payable returns (address cloneAddress) {
        (masterContract != address(0), "No masterContract");
        bytes20 targetBytes = bytes20(masterContract); // Takes the first 20 bytes of the masterContract's address

        if (useCreate2) {
            // each masterContract has different code already. So clones are distinguished by their data only.
            bytes32 salt = keccak256(data);

            // Creates clone, more info here: https://blog.openzeppelin.com/deep-dive-into-the-minimal-proxy-contract/
            assembly {
                let clone := mload(0x40)
                mstore(clone, 0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000)
                mstore(add(clone, 0x14), targetBytes)
                mstore(add(clone, 0x28), 0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000)
                cloneAddress := create2(0, clone, 0x37, salt)
            }
        } else {
            assembly {
                let clone := mload(0x40)
                mstore(clone, 0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000)
                mstore(add(clone, 0x14), targetBytes)
                mstore(add(clone, 0x28), 0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000)
                cloneAddress := create(0, clone, 0x37)
            }
        }
        masterContractOf[cloneAddress] = masterContract;

        IMasterContract(cloneAddress).init{value: msg.value}(data);

        emit LogDeploy(masterContract, data, cloneAddress);
    }

    function setReserveInterestRateAddress( address rateAddress) external {
        ReserveInterestRateAddress=rateAddress;
    }


    function deposit ( address to, uint256 tokenId )payable external{
        TestBlueChipNFT.transferFrom(msg.sender, address (this), tokenId);
        DEBTNFT.safeMint(to, tokenId);
        userdeposit[msg.sender]=tokenId;
        emit LogDeposited (msg.sender, tokenId);
    }



    function provideLiquidity(uint256 _amount, address lprovider)payable external{
        uint shares;
        if (totalLiquidity == 0) {
            shares = _amount;
        } else {
            shares = (_amount * totalLiquidity) / TestUSDC.balanceOf(address(this));
        }
        TestUSDC.transferFrom(msg.sender, address(this), _amount);
        uint256 newLiquidityAdded = userLpProvided[msg.sender].add(_amount);
        userLpProvided[msg.sender] = newLiquidityAdded;
        TestLUSDC.mint(lprovider, _amount);
        totalLiquidity += _amount;
        emit LogProvideLiquidity(msg.sender, _amount);
    }

    function borrow(uint amount)payable external {
       // uint256 feeAmount = amount.mul(BORROW_OPENING_FEE) / BORROW_OPENING_FEE_PRECISION;
        //totalBorrow = totalBorrow.add(amount.add(feeAmount));
        uint toborrow=(maximum.div(1000)).mul(Price);
        //BorrowCap memory cap =  borrowLimit;
        
        //require(totalBorrow <= cap.total, "Borrow Limit reached");
        require (Price > amount,"cont borrow that much");
        require (Price >=toborrow, "borrow exceeds max borrow");
        require (userdeposit[msg.sender]!=0, "you have not provided any colleteral please provide TestBlueChipNFT to borrow");
        //require (amount<=(maximum.div(1000)).mul(Price), "");
        
        uint256 newBorrowPart = userBorrowPart[msg.sender].add(amount);
        Debt=userDebt[msg.sender].add(amount);
        //require(newBorrowPart <= cap.borrowPartPerAddress, "Borrow Limit reached");
        userBorrowPart[msg.sender] = newBorrowPart;
        TestUSDC.transfer(msg.sender,amount);
        delete userdeposit [msg.sender];
        emit LogBorrow(address(TestUSDC),msg.sender, amount);
    }




    function withdrawLiquidity(uint256 _shares) payable external{
        uint amount = (_shares * TestLUSDC.balanceOf(address(this))) / totalLiquidity;
        uint256 newLiquidityAdded = userLpProvided[msg.sender].sub(_shares);
        userLpProvided[msg.sender] = newLiquidityAdded;
        require(userLpProvided[msg.sender]>=_shares,"cant withdraw more than you deposited");
        TestLUSDC.transferFrom(msg.sender, address(this), _shares);
        TestLUSDC.burn(_shares);
        TestUSDC.transfer(msg.sender, amount);
        emit LogWithdrawLiquidity(msg.sender, _shares);
    }


    function redeem (uint256 tokenId)payable external{
        uint256 amount=userBorrowPart[msg.sender];
        DEBTNFT.transferFrom(msg.sender, address(this), tokenId);
        DEBTNFT.burn( tokenId);
        userBorrowPart[msg.sender]=0;
        TestUSDC.transferFrom(msg.sender, address(this ),amount);
        emit LogRepay(msg.sender, tokenId, amount);
    }

    function flashClaim() external {

    }

    function liquidate(uint tokenId)payable external returns (string memory cantLiquidate) {
        uint256  HEALTH_FACTOR=Price.mul(LIQUIDATION_THRESHOLD).div(Debt).add(interest);
        if (HEALTH_FACTOR<1){
          TestBlueChipNFT.transferFrom(address (this), address(auctionContract), tokenId); 
        }else{
            return
            "cannot be liquidated health factor >1";
        }
    }

