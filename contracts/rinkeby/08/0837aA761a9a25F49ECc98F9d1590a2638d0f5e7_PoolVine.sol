        totalFreeze[msg.sender] = totalFreeze[msg.sender] + tokenReward;
        if(isTokenAcceptance){
            tokenAcceptType = "ERC-20-COIN";
        }else{}
        emit regLevelEvent(msg.sender, userList[_referrerID], now,tokenAcceptType );
    }

