pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/draft-ERC20PermitUpgradeable.sol";

import "../SecurityToken.sol";
import "hardhat/console.sol";

contract SomaERC20Permit is ERC20PermitUpgradeable, SecurityToken {

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(address networkAddress) Guardable(networkAddress) {}

    function initialize(
        string memory name,
        string memory symbol,
        uint256 _totalSupply,
        address _mintTo
    ) public initializer virtual {
        __ERC20_init_unchained(name, symbol);
        __ERC20Permit_init(name);
        __ERC165_init_unchained();
        __NetworkAccess_init_unchained();

        if (_totalSupply > 0) {
            _mint(_mintTo, _totalSupply);
        }
    }

    /**
     * @dev See {IERC165-supportsInterface}.
