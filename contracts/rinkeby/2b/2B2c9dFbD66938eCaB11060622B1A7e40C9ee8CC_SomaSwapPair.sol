pragma solidity ^0.8.9;

import "./IGuardable.sol";

library GuardHelper {

    bytes32 internal constant DEFAULT_ACCESS = bytes32(uint256(2 ** 64 - 1));

    function requiredAccess(address account) internal view returns (bytes32 access) {
        try IGuardable(account).requiredAccess() returns (bytes32 requiredAccess_) {
            access = requiredAccess_;
        } catch(bytes memory) {
            access = DEFAULT_ACCESS;
        }
    }

    function mergeAccess(bytes32 access1, bytes32 access2) internal pure returns (bytes32) {
        return access1 | access2;
    }

    function mergeAccess(bytes32 access1, bytes32 access2, bytes32 access3) internal pure returns (bytes32) {
        return access1 | access2 | access3;
    }

    function switchOn(uint256[] memory ids, bytes32 base) internal pure returns (bytes32 result) {
        result = base;
        for (uint i = 0; i < ids.length; i++) {
            result = result | bytes32(2**ids[i]);
        }
    }

    function switchOff(uint256[] memory ids, bytes32 base) internal pure returns (bytes32 result) {
        result = base;
        for (uint i = 0; i < ids.length; i++) {
            result = result & bytes32(type(uint256).max - 2**ids[i]);
        }
        result = result | DEFAULT_ACCESS;
    }
}

