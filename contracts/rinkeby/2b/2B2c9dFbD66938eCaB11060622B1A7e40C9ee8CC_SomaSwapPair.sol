        return interfaceId == type(IGuardable).interfaceId || super.supportsInterface(interfaceId);
    }

    function updateRequiredAccess(bytes32 newRequiredAccess) public virtual override returns (bool) {
        require(
            hasRole(UPDATE_ACCESS_ROLE, _msgSender()) || hasRole(GLOBAL_UPDATE_ACCESS_ROLE, _msgSender()),
            'Guardable: you do not have the required roles to do this'
        );
        _updateRequiredAccess(newRequiredAccess);
        return true;
