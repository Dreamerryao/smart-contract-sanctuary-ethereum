    using NetworkLocations for ISomaNetwork;

    bytes32 private _requiredAccess;

    bytes32 public immutable UPDATE_ACCESS_ROLE;
    bytes32 public immutable DEFAULT_ACCESS;
    bytes32 public constant GLOBAL_UPDATE_ACCESS_ROLE = keccak256('Guardable:default-update-access');

    constructor(address networkAddress) NetworkAccessUpgradeable(networkAddress) {
        UPDATE_ACCESS_ROLE = keccak256(abi.encodePacked(address(this), "Guardable:update-access-role"));
        DEFAULT_ACCESS = _guard().DEFAULT_ACCESS();
        _updateRequiredAccess(DEFAULT_ACCESS);
    }

