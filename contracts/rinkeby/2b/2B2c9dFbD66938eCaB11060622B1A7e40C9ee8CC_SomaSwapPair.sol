pragma solidity ^0.8.9;

import "../SomaGuard/ISomaGuard.sol";
import "../SomaGuard/utils/Guardable.sol";
import "./ISecurityToken.sol";

abstract contract SecurityToken is Guardable, ISecurityToken {

    modifier onlyTransferApproved(address from, address to, uint256 value, bytes memory data) {
        require(canTransferFrom(from, to, value, data), 'required access not met');
        _;
    }

    function canTransfer(address _to, uint256 _value, bytes memory _data) external virtual override view returns (bool) {
        return canTransferFrom(_msgSender(), _to, _value, _data);
    }

    function canTransferFrom(address _from, address _to, uint256, bytes memory) public virtual override view returns (bool) {
        ISomaGuard guard = _guard();
        bytes32 _access = requiredAccess();
        return (_from == address(0) || guard.check(_from, _access)) &&
        (_to == address(0) || guard.check(_to, _access));
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(ISecurityToken).interfaceId || super.supportsInterface(interfaceId);
    }
}

