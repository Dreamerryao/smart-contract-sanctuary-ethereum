     * @param amount The amount to check.
     */
    function _assertEnoughERC20Allowance(
        address token,
        address owner,
        uint256 amount
    ) internal view {
        // Revert if the supplied owner hasn't given enough allowance to this contract.
        if (IERC20(token).allowance(owner, address(this)) < amount) {
            revert InsufficientERC20Allowance(token, owner, amount);
        }
    }

