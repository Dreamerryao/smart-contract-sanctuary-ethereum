pragma solidity ^0.8.0;

import "./lib/SSCVault.sol";
import "hardhat/console.sol";
import "./lib/SSCErrorsMessages.sol";

contract MSSC is SSCVault {
    // Events
    event RegisterCycle(bytes32 indexed cycleId, Instruction[] instructions);
    event ExecuteCycle(bytes32 indexed cycleId, Instruction[] instructions);

    mapping(bytes32 => SettlementCycle) private _cycles;

    /**
     * @notice Register settlementCycle, this function can only be perfomed by a Membrane wallet.
     *         Caller must transform obfuscatedId string to bytes32, pure strings are not supported.
     *
     * @param cycleId Cycle's bytes32 obfuscatedId to register.
     * @param instructions instructions to register.
     */
    function registerSettlementCycle(
        bytes32 cycleId,
        Instruction[] calldata instructions
    ) public {
        _assertCycleDoesNotExist(cycleId);
        Instruction[] storage newInstructions = _cycles[cycleId].instructions;

        // Retrieve the total number of instructions and place on the stack.
        uint256 totalInstructions = instructions.length;

        for (uint256 i = 0; i < totalInstructions; ) {
            Instruction memory instruction = instructions[i];

            _assertValidInstruction(instruction);

            newInstructions.push(instruction);

            // Skip overflow check as for loop is indexed starting at zero.
            unchecked {
                ++i;
            }
        }

        emit RegisterCycle(cycleId, instructions);
    }

    /**
     * @notice Execute instructions in a SettlementCycle, anyone can call this function as long as it
     *         meets some requirements, further call to {isReadyToExecute} is recommended before performing
     *         this function to ensure that the SettlementCycle meets the requirements to execute instructions.
     *
     * @param cycleId Cycle's bytes32 obfuscatedId to execute.
     */
    function executeInstructions(bytes32 cycleId) external {
        _assertCycleExists(cycleId);

        _assertCycleIsNotExecuted(cycleId);

        _cycles[cycleId].executed = true;
        Instruction[] memory instructions = _cycles[cycleId].instructions;

        // Retrieve the total number of instructions and place on the stack.
        uint256 totalInstructions = instructions.length;

        for (uint256 i = 0; i < totalInstructions; ) {
            Instruction memory instruction = instructions[i];

            DepositItem memory depositItem = DepositItem({
                depositType: instruction.asset == address(0)
                    ? DepositType.NATIVE
                    : DepositType.ERC20,
                sender: instruction.sender,
                token: instruction.asset,
                instructionId: instruction.id,
                cycleId: cycleId
            });

            TransferDepositedItem memory data = TransferDepositedItem({
                depositItem: depositItem,
                to: instruction.receiver,
                amount: instruction.amount
            });

            _transferDeposited(data);

            // Skip overflow check as for loop is indexed starting at zero.
            unchecked {
                ++i;
            }
        }
        emit ExecuteCycle(cycleId, instructions);
    }

    /**
     * @notice Make deposits (Native coin or ERC20 tokens) to a existent cycle, {msg.sender} must be a
     *         sender of any of the instructions in the Settlement Cycle.
     *
     * @param cycleId Cycle to allocate received ETH.
     */
    function deposit(
        bytes32 cycleId,
        bytes32 instructionId,
        uint256 amount
    ) public payable {
        _assertCycleExists(cycleId);
        _assertAccountIsSender(msg.sender, cycleId);

        Instruction memory instruction;
        Instruction[] memory instructions = _cycles[cycleId].instructions;

        // Retrieve the total number of instructions and place on the stack.
        uint256 totalInstructions = instructions.length;

        // get instruction to deposit
        for (uint256 i = 0; i < totalInstructions; ) {
            if (instructions[i].id == instructionId) {
                instruction = instructions[i];
            }

            unchecked {
                ++i;
            }
        }

        DepositItem memory depositItem = DepositItem({
            depositType: instruction.asset == address(0)
                ? DepositType.NATIVE
                : DepositType.ERC20,
            sender: instruction.sender,
            token: instruction.asset,
            instructionId: instruction.id,
            cycleId: cycleId
        });

        _deposit(
            depositItem,
            depositItem.depositType == DepositType.NATIVE ? msg.value : amount
        );
    }

    /**
     * @notice Withdraw ETH from a cycle.
     *
     * @param cycleId Cycle to withdraw deposited ETH from.
     */
    function withdraw(bytes32 cycleId, bytes32 instructionId) external {
        Instruction memory instruction;
        Instruction[] memory instructions = _cycles[cycleId].instructions;

        // Retrieve the total number of instructions and place on the stack.
        uint256 totalInstructions = instructions.length;

        // get instruction to deposit
        for (uint256 i = 0; i < totalInstructions; ) {
            if (instructions[i].id == instructionId) {
                instruction = instructions[i];
            }

            unchecked {
                ++i;
            }
        }

        DepositItem memory depositItem = DepositItem({
            depositType: instruction.asset == address(0)
                ? DepositType.NATIVE
                : DepositType.ERC20,
            sender: instruction.sender,
            token: instruction.asset,
            instructionId: instruction.id,
            cycleId: cycleId
        });
        _withdraw(depositItem);
    }

    function canRegister(bytes32 cycleId, Instruction[] calldata instructions)
        external
        view
        returns (bool, string memory)
    {
        if (_exist(cycleId)) return (false, ERR_MSG_REGISTERED);

        // check that instructions are valid
        (bool result, string memory errMsg) = _checkInstructions(instructions);
        if (!result) return (false, errMsg);

        return (true, "yes");
    }

    function isReadyToExecute(bytes32 cycleId)
        external
        view
        returns (bool, string memory)
    {
        if (!_exist(cycleId)) return (false, ERR_MSG_NOT_REGISTERED);
        if (_cycles[cycleId].executed) return (false, ERR_MSG_EXECUTED);

        // check valid balances and allowance
        (bool result, string memory errMsg) = _checkBalancesFromCycle(cycleId);
        if (!result) return (false, errMsg);

        return (true, "yes");
    }

    /**
     * @notice View function to get the instructions in a settlement cycle.
     *
     * @param cycleId Cycle to check.
     */
    function getSettlementInstructions(bytes32 cycleId)
        external
        view
        returns (Instruction[] memory)
    {
        _assertCycleExists(cycleId);
        return _cycles[cycleId].instructions;
    }

    /**
     * @notice View function to check if a cycle has been executed.
     *
     * @param cycleId Cycle to check.
     */
    function executed(bytes32 cycleId) external view returns (bool) {
        _assertCycleExists(cycleId);
        return _cycles[cycleId].executed;
    }

    /**
     * @notice View function to get deposited ETH by an account.
     *
     * @param account Account to get deposited ETH from.
     * @param cycleId Cycle to get deposited ETH from.
     */
    function deposits(
        address account,
        bytes32 cycleId,
        bytes32 instructionId
    ) external view returns (uint256) {
        bytes32 slot = keccak256(
            abi.encodePacked(account, cycleId, instructionId)
        );
        return _getDeposits(slot);
    }

    /*//////////////////////////////////////////////////////////////
                             ASSERTIONS
    //////////////////////////////////////////////////////////////*/

    // Check if an address is a sender of any instruction in the SettleMentCycle.
    function _assertAccountIsSender(address account, bytes32 cycleId)
        private
        view
    {
        //gas savings
        Instruction[] memory instructions = _cycles[cycleId].instructions;

        // Retrieve the total number of instructions and place on the stack.
        uint256 totalInstructions = instructions.length;

        for (uint256 i; i < totalInstructions; ) {
            if (instructions[i].sender == account) return;

            // Skip overflow check as for loop is indexed starting at zero.
            unchecked {
                ++i;
            }
        }
        revert NotASender();
    }

    // Ensure that {cycleId} is registered.
    function _assertCycleExists(bytes32 cycleId) private view {
        if (!_exist(cycleId)) {
            revert NoCycle();
        }
    }

    // Ensure that {cycleId} is NOT registered.
    function _assertCycleDoesNotExist(bytes32 cycleId) private view {
        if (_exist(cycleId)) {
            revert CycleAlreadyRegistered();
        }
    }

    // Ensure that cycle hasn't been executed before.
    function _assertCycleIsNotExecuted(bytes32 cycleId) private view {
        if (_cycles[cycleId].executed) {
            revert CycleAlreadyExecuted();
        }
    }

    /*//////////////////////////////////////////////////////////////
                             CHECKERS 
    //////////////////////////////////////////////////////////////*/

    // Check that cycleId is registered by looking at instructions length, this function may change its logic later
    function _exist(bytes32 cycleId) private view returns (bool) {
        return _cycles[cycleId].instructions.length > 0;
    }

    // check that instructions have valid properties.
    function _checkInstructions(Instruction[] calldata instructions)
        private
        view
        returns (bool, string memory)
    {
        for (uint256 i; i < instructions.length; i++) {
            Instruction memory instruction = instructions[i];
            if (
                instruction.sender == address(0) ||
                instruction.receiver == address(0)
            ) return (false, ERR_MSG_NO_ZERO_ADDRESS);

            // check that asset is a contract or zero address
            if (
                instruction.asset != address(0) &&
                instruction.asset.code.length <= 0
            ) return (false, ERR_MSG_INVALID_ASSET);

            if (instruction.amount <= 0) return (false, ERR_MSG_NO_ZERO_AMOUNT);
        }

        return (true, "");
    }

    // check that senders provided valid allowances and deposits.
    function _checkBalancesFromCycle(bytes32 cycleId)
        private
        view
        returns (bool, string memory)
    {
        Instruction[] memory instructions = _cycles[cycleId].instructions;
        for (uint256 i = 0; i < instructions.length; i++) {
            Instruction memory instruction = instructions[i];

            bytes32 slot = keccak256(
                abi.encodePacked(instruction.sender, cycleId, instruction.id)
            );

            if (_getDeposits(slot) < instruction.amount) {
                return (false, ERR_MSG_NOT_ENOUGH_DEPOSIT);
            }
        }
        return (true, "");
    }

    // calculate the total amount of an asset a sender address will transfer in a single SettlementCycle
    function _totalSenderAmount(
        address sender,
        bytes32 cycleId,
        address asset
    ) private view returns (uint256 total) {
        Instruction[] memory instructions = _cycles[cycleId].instructions;
        for (uint256 i; i < instructions.length; i++) {
            if (
                instructions[i].sender == sender &&
                asset == instructions[i].asset
            ) {
                total += instructions[i].amount;
            }
        }
    }
}

