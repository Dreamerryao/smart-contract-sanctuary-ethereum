    function _assertEnoughERC20Balance(
        address token,
        address account,
        uint256 amount
    ) internal view {
        // Revert if the supplied account has insufficient balance of given token.
        if (IERC20(token).balanceOf(account) < amount) {
            revert InsufficientERC20Balance(token, account, amount);
        }
    }

    /**
     * @dev Internal view function to ensure that a given account has approved enough
     *      tokens to this contract.
     *
     * @param token  The ERC20 contract to check.
