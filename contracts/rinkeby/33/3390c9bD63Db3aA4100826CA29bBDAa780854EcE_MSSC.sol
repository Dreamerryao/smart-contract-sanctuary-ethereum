pragma solidity >=0.8.4;

import "./SSCStructs.sol";
import "./Transferrer.sol";

/**
 * @title  Vault
 * @notice Vault contains logic for making deposits and withdrawals of Ether
 *         to and from Settlement Cycles.
 */
contract SSCVault is Transferrer {
    // Events
    event Deposit(address indexed account, bytes32 cycleId);
    event Withdraw(address indexed account, bytes32 cycleId);

    // Track deposited allocated to various cycles.
    mapping(bytes32 => uint256) private _deposits;

    /**
     * @notice Internal function to deposit and allocate ETH to a cycle.
     *
     * @param item Struct data to perform the deposit.
     */
    function _deposit(DepositItem memory item, uint256 amount) internal {
        bytes32 slot = keccak256(
            abi.encodePacked(item.sender, item.cycleId, item.instructionId)
        );
        _deposits[slot] += amount;
        if (item.depositType == DepositType.ERC20) {
            _transferERC20(item.token, item.sender, address(this), amount);
        }
        emit Deposit(msg.sender, item.cycleId);
    }

    /**
     * @notice Internal function to withdraw ETH from a cycle.
     *
     * @param item Struct data to perform the withdraw.
     */
    function _withdraw(DepositItem memory item) internal {
        bytes32 slot = _hashSlot(item.sender, item.cycleId, item.instructionId);
        uint256 amount = _getDeposits(slot);
        _cutDeposits(slot, amount);

        if (item.depositType == DepositType.ERC20) {
            _transferERC20(item.token, address(this), item.sender, amount);
        } else {
            _transferETH(payable(msg.sender), amount);
        }

        emit Withdraw(item.sender, item.cycleId);
    }

    /**
     * @notice Transfer allocated ETH to another account.
     *
     * @param item Source of the transfer.
     */
    function _transferDeposited(TransferDepositedItem memory item) internal {
        DepositItem memory depositItem = item.depositItem;
        bytes32 slot = _hashSlot(
            depositItem.sender,
            depositItem.cycleId,
            depositItem.instructionId
        );

        _assertEnoughAllocatedDeposit(depositItem.sender, slot, item.amount);

        _cutDeposits(slot, item.amount);
        if (depositItem.depositType == DepositType.ERC20) {} else {
            _transferETH(payable(item.to), item.amount);
        }
    }

    /**
     * @notice Cut deposits to a cycle.
     *
     * @param slot Hashed Slot to cut deposited amount from
     */
    function _cutDeposits(bytes32 slot, uint256 amount) internal {
        _deposits[slot] -= amount;
    }

    /**
     * @notice Get deposited ETH to a cycle.
     *
     * @param slot Hashed Slot to get deposited amount from.
     */
    function _getDeposits(bytes32 slot) internal view returns (uint256) {
        return _deposits[slot];
    }

    /**
     * @notice Ensure that a given {account} has enought allocated ETH.
     *
     * @param amount Amount to compare deposited ETH with.
     */
    function _assertEnoughAllocatedDeposit(
        address account,
        bytes32 slot,
        uint256 amount
    ) private view {
        if (_getDeposits(slot) < amount) {
            revert InsufficientSuppliedDeposit(account, amount);
        }
    }

    function _hashSlot(
        address account,
        bytes32 cycleId,
        bytes32 instructionId
    ) private pure returns (bytes32 slot) {
        slot = keccak256(abi.encodePacked(account, cycleId, instructionId));
    }
}

