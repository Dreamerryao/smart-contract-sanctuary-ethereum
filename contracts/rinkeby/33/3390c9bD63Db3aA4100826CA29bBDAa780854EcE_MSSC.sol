interface MSSCErrors {
    /**
     * @dev Revert with an error when trying to register an existent Settlement Cycle.
     */
    error CycleAlreadyRegistered();

    /**
     * @dev Revert with an error when executing a previously executed Settlement Cycle.
     */
    error CycleAlreadyExecuted();

    /**
     * @dev Revert with an error when attempting to interact with a cycle that
     *      does not yet exist.
     */
    error NoCycle();

    /**
     * @dev Revert with an error when a non-zero address being called as an assumed
     *      ERC20 contract does not have code.
     *
     * @param instruction The instruction that contain the invalid asset.
     */
    error InvalidAsset(bytes32 instruction);

    /**
     * @dev Revert with an error when attempting to register an account (sender or recipient)
     *      and supplying the null address.
     *
     * @param instruction The instruction that contain the zero address.
     */
    error AccountIsZeroAddress(bytes32 instruction);

    /**
     * @dev Revert with an error when insufficient ether is deposited for a cycle
     *      to execute.
     *
     * @param account    The source of the attempted transfer.
     * @param amount     The amount for the attempted transfer.
     */
    error InsufficientSuppliedDeposit(address account, uint256 amount);

    /**
     * @dev Revert with an error when a cycle has no enough approvals from senders
     *      in order to execute it.
     */
    error InsufficientApprovals();

    /**
     * @dev Revert with an error when an account is not a sender of any of the instructions in a cycle.
     */
    error NotASender();
}