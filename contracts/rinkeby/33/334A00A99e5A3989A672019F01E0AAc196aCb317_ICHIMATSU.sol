pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";

struct RGB {
    uint24 red;
    uint24 green;
    uint24 blue;
}

library svgData {
    using Strings for uint256;

    bytes16 private constant _HEX_SYMBOLS = "0123456789abcdef";
    uint24 private constant _filter = 32;

    function getRGBfromAddress(address addr) public pure returns (RGB memory rgb) {
        string memory str = Strings.toHexString(uint160(addr), 20);

        rgb.red = _hexToInt(_substring(str, 2, 4));
        rgb.green = _hexToInt(_substring(str, 4, 6));
        rgb.blue = _hexToInt(_substring(str, 6, 8));
    }

    function getRGBAtRandom() public view returns (RGB memory rgb) {
        uint256 rand = uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, msg.sender)));

        rgb.red = uint24((rand % 255) + 1);
        rgb.green = uint24(((rand / 1000) % 255) + 1);
        rgb.blue = uint24((((rand / 1000) / 1000) % 255) + 1);
    }

    function _substring(string memory str, uint startIndex, uint endIndex) private pure returns (string memory) {
        bytes memory strBytes = bytes(str);
        bytes memory result = new bytes(endIndex - startIndex);
        for(uint i = startIndex; i < endIndex; i++) {
            result[i - startIndex] = strBytes[i];
        }
        return string(result);
    }

    function _hexToInt(string memory hexStr) private pure returns (uint24) {
        bytes memory hexBytes = bytes(hexStr);
        uint24 ret = 0;

        for (uint24 l = 0; l < hexBytes.length; l++) {
            uint24 p = 0;
            for (uint24 i = 0; i < 16; i++) {
                if (keccak256(abi.encodePacked(hexBytes[l])) == keccak256(abi.encodePacked(_HEX_SYMBOLS[i]))) {
                    p = i;
                    break;
                }
            }
            ret = ret * 16 + p;
        }
        return ret;
    }

    function _RGBToHex(RGB memory rgb) private pure returns (string memory) {
        uint24 value = (rgb.red << 16) | (rgb.green << 8) | rgb.blue;

        bytes memory buffer = new bytes(6);
        for (uint256 i = 0; i < 6; i++) {
            buffer[i] = _HEX_SYMBOLS[(value >> (4 * (5 - i))) & 0xf];
        }
        return string(buffer);
    }

    function _blendColor(RGB memory rgb, uint24 _r, uint24 _g, uint24 _b) private pure returns (RGB memory ret) {
        if (rgb.red + _r >= 255) {ret.red = 255;} else {ret.red = rgb.red + _r;}
        if (rgb.green + _g >= 255) {ret.green = 255;} else {ret.green = rgb.green + _g;}
        if (rgb.blue + _b >= 255) {ret.blue = 255;} else {ret.blue = rgb.blue + _b;}
    }

    function tokenURI(uint256 tokenId, RGB memory rbg, address addr,uint256 transCount) public pure returns (string memory) {
        string memory bg1 = _RGBToHex(rbg);
        string memory bg2 = _RGBToHex(_blendColor(rbg, _filter, _filter, _filter));

        bytes memory attributes = abi.encodePacked(
            '{"trait_type": "bg1 color", "value": "',
                bg1,
            '"},',
            '{"trait_type": "bg2 color", "value": "',
                bg2,
            '"},',
            '{"trait_type": "transfer count", "value": "',
                transCount.toString(),
            '"}'
        );

        bytes memory head = abi.encodePacked(
            '<svg width="600" height="600" viewBox="0 0 600 600" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:svg="http://www.w3.org/2000/svg">'
        );

        bytes memory style = abi.encodePacked(
            '<style>',
            '.bg1  { fill: #',
                bg1,
            ' }',
            '.bg2  { fill: #',
                bg2,
            ' }',
            '.gold { fill: #E0B34E }',
            '.gray { fill: #888E7E }',
            '</style>'
        );

        bytes memory owner = abi.encodePacked(
            '<text id="owner">',
                _substring(Strings.toHexString(uint160(addr), 20), 0, 10),
            '...</text>'
        );

        bytes memory body = bytes(abi.encodePacked(
            '<defs>',
                '<pattern id="bg" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">',
                    '<path d="M0 0 v100 h100 v-100 h-100 z" class="bg1" />',
                    '<path d="M50 0 v50 h50 v-50 h-50 z M0 50 v50 h50 v-50 h-50 z" class="bg2" />',
                '</pattern>',
            '</defs>',
            '<symbol id="glass" viewBox="0 0 200 200">',
                '<path d="M0 20 v30 h10 v-20 h20 v30 h60 v-30 h10 v30 h60 v-60 h-60 v20 h-10 v-20 h-60 v20 h-30 z" />',
                '<path d="M40 10 v40 h40 v-40 h-40 z M110 10 v40 h40 v-40 h-40 z" fill="white"/>',
                '<path d="M60 10 v40 h20 v-40 h-20 z M130 10 v40 h20 v-40 h-20 z" fill="black"/>',
            '</symbol>',
            '<symbol id="name" viewBox="0 -20 600 50">',
                '<rect  x="0" y="0" width="600" height="50" class="gray" />',
                '<circle cx="50" cy="25" r="40" fill="white" stroke-width="5" stroke="#E0B34E"></circle>',
                '<use href="#owner" x="100" y="10" width="500"  height="100" font-size="48" fill="#E0B34E" stroke-width="0.5" stroke="white" dominant-baseline="central"  />',
            '</symbol>',
            '<rect width="100%" height="100%" fill="url(#bg)" fill-opacity="0.5"/>',
            '<use href="#glass" x="50" y="200" width="100%" height="100%" class="gray" fill-opacity="0.5" transform="rotate(-5 200 200)" />',
            '<use href="#name" x="0" y="530" width="600" height="70" />',
            '<use href="#glass" x="30" y="575" width="50" height="50" class="gold" />',
            '</svg>'
    ));

        bytes memory svg = bytes(abi.encodePacked(head, style, owner, body));

        bytes memory description = 'ICHIMATSU';
        bytes memory metadata =
            abi.encodePacked(
                '{"name": "ICHIMATSU #',
                tokenId.toString(),
                '", "description": "',
                description,
                '", "image": "data:image/svg+xml;base64,',
                Base64.encode(svg),
                '", "attributes": [',
                attributes,
                ']}'
            );

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(metadata)
                )
            );
    }
}

