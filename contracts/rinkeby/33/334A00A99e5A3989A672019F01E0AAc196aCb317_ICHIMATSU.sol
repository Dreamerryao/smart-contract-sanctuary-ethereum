        require(_exists(tokenId), "mint approved query for nonexistent token");
    
         return _mintApprovals[tokenId];
     }

    function _clearMintApprove(address owner, uint256 tokenId) internal virtual {
        if (_mintApprovals[tokenId] != address(0)) {
            _mintApprove(owner, address(0), tokenId);
        }
    }

    function setMintApprovalForAll(address operator, bool approved) public virtual override {
        _setMintApprovalForAll(_msgSender(), operator, approved);
    }

    function _setMintApprovalForAll(address owner, address operator, bool approved) internal virtual {
        require(owner != operator, "ERC5058: lock approve to caller");
         _mintOperatorApprovals[owner][operator] = approved;
         emit MintApprovalForAll(owner, operator, approved);
    }

    function isMintApprovedForAll(address owner, address operator) public view virtual override returns (bool) {
         return _mintOperatorApprovals[owner][operator];
     }

    function isMintApprovedOrOwner(address spender, uint256 tokenId) public view virtual override returns (bool) {
        return _isMintApprovedOrOwner(spender, tokenId);
    }

    function _isMintApprovedOrOwner(address spender, uint256 tokenId) internal view virtual returns (bool) {
        require(_exists(tokenId), "ERC5058: lock operator query for nonexistent token");
        address owner = ERC721.ownerOf(tokenId);
        return (spender == owner || isMintApprovedForAll(owner, spender) || getMintApproved(tokenId) == spender);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(IERC165, ERC721) returns (bool) {
        return interfaceId == type(IERC721L).interfaceId || super.supportsInterface(interfaceId);
    }
}

