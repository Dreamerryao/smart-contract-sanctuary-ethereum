abstract contract ERC721L is IERC721L, ERC721 {

    mapping(uint256 => address) private _mintApprovals;
    mapping(address => mapping(address => bool)) private _mintOperatorApprovals;

    function mintApprove(address to, uint256 tokenId) public virtual override {
        address owner = ERC721.ownerOf(tokenId);
        require(to != owner, "mint approval to current owner");
        require(_msgSender() == owner || isMintApprovedForAll(owner, _msgSender()), "mint approve caller is not owner nor approved for all");

        _mintApprove(owner, to, tokenId);
    }

    function _mintApprove(address owner, address to, uint256 tokenId) internal virtual {
        _mintApprovals[tokenId] = to;
        emit MintApproval(owner, to, tokenId);
    }

