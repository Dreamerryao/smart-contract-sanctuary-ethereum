pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./library/ERC721L.sol";
import "./library/svgData.sol";

contract ICHIMATSU is ERC721L, Pausable, Ownable {
    using Strings for uint256;
    
    uint256 public totalSupply;
    
    // constant
    uint256 constant private MAX_SUPPLY = 99999;
    uint256 constant private CLAIM_MAX_PER_WALLET = 10;

    // token
    mapping(uint256 => RGB) private _layerData;
    mapping(uint256 => uint256) private _transCount;

    // user
    mapping(address => uint256) private _addrClaimed;

    constructor(string memory _name,string memory _symbol) ERC721(_name, _symbol) {}

    function getMintApprovedOrOwnerIdxs(address addr) public view returns(uint256[] memory) {
        uint256[] memory tokenIdxs = new uint256[](totalSupply);
        uint256 currentIndex = 0;
        
        for (uint256 i = 1; i <= totalSupply; i++) {
            if (isMintApprovedOrOwner(addr, i)) {tokenIdxs[currentIndex] = i; currentIndex++;}
        }

        return tokenIdxs;
    }

    function mint(uint256 mintAmount) public payable whenNotPaused {
        uint256 supply = totalSupply;
        _mintCheck(mintAmount, supply);
  
        for (uint256 i = 1; i <= mintAmount; i++) {
            unchecked { ++totalSupply; }
            _safeMint(msg.sender, supply + i);
            unchecked { _addrClaimed[msg.sender]++; }
            _layerData[supply + i] = svgData.getRGBfromAddress(msg.sender);
        }
    }

    function storageWalletMint(address addr, uint256 mintAmount) public payable whenNotPaused {
        require(addr != msg.sender, "equal address");
        require(addr != address(0), "address 0");
        uint256 supply = totalSupply;
        _mintCheck(mintAmount, supply);
  
        for (uint256 i = 1; i <= mintAmount; i++) {
            unchecked { ++totalSupply; }
            _safeMint(addr, supply + i);
            unchecked { _addrClaimed[msg.sender]++; }
            _layerData[supply + i] = svgData.getRGBfromAddress(msg.sender);
            _mintApprove(addr, msg.sender, supply + i);
        }
    }

    function _mintCheck(uint256 mintAmount, uint256 supply) private view {
        require(mintAmount > 0, "Mint amount cannot be zero");
        require(supply + mintAmount <= MAX_SUPPLY, "MAXSUPPLY over");
        require(_addrClaimed[msg.sender] + mintAmount <= CLAIM_MAX_PER_WALLET, "Already claimed max");
    }

    function _beforeTokenTransfer(address from, address to,uint256 tokenId) internal virtual override whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId);
        require(from != to, "equal address");

        if (from != address(0)) {
            _clearMintApprove(from, tokenId);
            _layerData[tokenId] = svgData.getRGBAtRandom();
            _transCount[tokenId]++;
        }
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "URI query for nonexistent token");
        return svgData.tokenURI(tokenId, _layerData[tokenId], ownerOf(tokenId), _transCount[tokenId]);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function withdraw() external onlyOwner {
        Address.sendValue(payable(owner()), address(this).balance);
    }

    function renounceOwnership() public override onlyOwner {}   
}

