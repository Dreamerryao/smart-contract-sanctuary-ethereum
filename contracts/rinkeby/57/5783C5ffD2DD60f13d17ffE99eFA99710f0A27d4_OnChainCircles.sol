pragma solidity ^0.8.4;

import "ERC721.sol";
import "ERC721URIStorage.sol";
import "Ownable.sol";
import "Counters.sol";

// import other helper contracts 
import {SVG} from "SVG.sol";
import {Base64} from "Base64.sol";
import "IERC4907.sol";


contract OnChainCircles is ERC721, ERC721URIStorage, Ownable, SVG, IERC4907 {
    using Strings for uint256;
    using Counters for Counters.Counter;
   
    struct UserInfo 
    {
        address user;   // address of user role
        uint64 expires; // unix timestamp, user expires
    }
    mapping (uint256  => UserInfo) private _users;
   
    Counters.Counter private _tokenIdCounter;
    mapping (uint256 => string) public magicKey;
    uint256 public totalMinted;

    address public contractOwner;

    event Minted(uint256 indexed TokenId, address indexed MintedTo, uint256 indexed TotalMinted );

    constructor() ERC721("OnChainCircles", "OCC") SVG(){
        totalMinted = 0;
        contractOwner = msg.sender;
    }

    function mintNFT(address mintAddress) public returns (uint256){
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();

        bytes32 hashed = keccak256(abi.encodePacked(totalMinted, msg.sender, mintAddress, block.difficulty, block.timestamp, block.number, block.gaslimit));
        // address _magicKey = address(uint160(bytes20(hashed)));
        magicKey[tokenId] = Strings.toHexString(uint256(hashed));

        _safeMint(mintAddress, tokenId);
        _setTokenURI(tokenId, generateFinalMetaJson(magicKey[tokenId], tokenId, mintAddress) );

        totalMinted+=1;
        emit Minted(tokenId, mintAddress, totalMinted);
        return tokenId;
    }

    function OwnerMintNFT(address mintAddress, string memory _magicKey) public returns (uint256){
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();

        magicKey[tokenId] = _magicKey;

        _safeMint(mintAddress, tokenId);
        _setTokenURI(tokenId, generateFinalMetaJson(_magicKey, tokenId, mintAddress) );
        

        totalMinted+=1;
        require(msg.sender == contractOwner, "Ownable: Only ContractOwner allowed");
        emit Minted(tokenId, mintAddress, totalMinted);
        return tokenId;
    }

    // The following functions are overrides required by Solidity.
    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    // function burn(uint256 tokenId) external {
    //     _burn(tokenId);
    // }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return  generateFinalMetaJson(magicKey[tokenId], tokenId, ownerOf(tokenId));
    }

    function generateSVGFromKey(uint256 tokenId) public virtual view returns (string memory) {
        return super.generateSVGFromKey(magicKey[tokenId], tokenId, ownerOf(tokenId));
    }

    // Renting Functions

    // @notice set the user and expires of a NFT
    function setUser(uint256 tokenId, address user, uint64 duration) public override virtual{
        require(userOf(tokenId)==address(0),"User already assigned");
        require(_isApprovedOrOwner(msg.sender, tokenId),"ERC721: transfer caller is not owner nor approved");
        require(duration >= 1 days, "Deficit of Min Renting Time ( 1 day )");
        require(duration <= 90 days, "Exceding Max Renting Time ( 90 days )");
        uint64 expires = uint64(block.timestamp + duration) ;
        UserInfo storage info =  _users[tokenId];
        info.user = user;
        info.expires = expires;
        emit UpdateUser(tokenId,user,expires);
    }

    // @notice Get the user address of an NFT
    function userOf(uint256 tokenId)public view override virtual returns(address){
        if( uint256(_users[tokenId].expires) >=  block.timestamp){
            return _users[tokenId].user; 
        }
        else{
            return address(0);
        }
    }

    // @notice Get the user expires of an NFT
    function userExpires(uint256 tokenId) public view override virtual returns(uint256){
        return _users[tokenId].expires;
    }

   
}

