import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Bean.sol";
import "./JellyzFactory.sol";


contract Traits is Ownable {
    bool public traitItemsUploaded = false;
    string[][3][9] public traitsItems;  // empty array traits
    string[9] public traitTypes = ["background", "clothes", "eyes", "flavor", "glasses", "hair", "headphones", "mouth", "tattoo"]; // default trait types
    string baseTokenURI;
    JellyzFactory jellyzFactory;

    function setJellyzFactory(address factoryAddress) public onlyOwner {
        jellyzFactory = JellyzFactory(factoryAddress);
    }

    function setTraitItems(string[] memory _items, uint8 _rarity, uint8 _traitId) public onlyOwner {
        require(_rarity <= traitsItems.length, "Rarity index out of range");
        traitsItems[_traitId][_rarity] = _items;
        traitItemsUploaded = true;
    }

    function compileAttributes(Bean memory b) private view returns (string memory) {
        uint8 tokenRarity = b.rarity;
        string memory _traits = string(abi.encodePacked(
            generateTraitMetadata(tokenRarity, 0, b.background),
            generateTraitMetadata(tokenRarity, 1, b.clothes),
            generateTraitMetadata(tokenRarity, 2, b.eyes),
            generateTraitMetadata(tokenRarity, 3, b.flavor),
            generateTraitMetadata(tokenRarity, 4, b.glasses),
            generateTraitMetadata(tokenRarity, 5, b.hair),
            generateTraitMetadata(tokenRarity, 6, b.headphones),
            generateTraitMetadata(tokenRarity, 7, b.mouth),
            generateTraitMetadata(tokenRarity, 8, b.tattoo)
        ));

        uint level = b.level;
        if(b.stage == 3) {
            level = jellyzFactory.getTokenLevel(b.id);
        }
        uint isStacked = jellyzFactory.getTokenState(b.id);

        return string(abi.encodePacked(
            '[',
            _traits,
            '{"trait_type":"Generation","value":',
            Strings.toString(b.generation),
            '},{"trait_type":"Stage","value":',
            Strings.toString(b.stage),
            '},{"trait_type":"Level","value":',
            Strings.toString(level),
            '},{"trait_type":"Rarity","value":',
            Strings.toString(b.rarity),
            '},{"trait_type":"Stacked","value":',
            Strings.toString(isStacked),
            '}]'
        ));
    }

    function generateTraitMetadata(uint8 _tokenRarity, uint8 _traitType, uint8 _traitId) private view returns (string memory) {
        uint8 tmpTokenRarity = _tokenRarity - 1;
        uint256 countTraits = traitsItems[_traitType][tmpTokenRarity].length;
        
        while (countTraits == 0 && tmpTokenRarity >= 0) {
            tmpTokenRarity -= 1;
            countTraits = traitsItems[_traitType][tmpTokenRarity].length;
        }

        return string(abi.encodePacked(
            '{"trait_type": "', traitTypes[_traitType], '" ,"value": "',
                baseTokenURI,
                Strings.toString(_traitType),
                '/',
                Strings.toString(_tokenRarity),
                '/',
                traitsItems[_traitType][tmpTokenRarity][_traitId % countTraits],
            '"},'
        ));
    }

    function setBaseURI(string memory _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
    }

    function structToString(Bean memory b) private pure returns (bytes memory) {
        return
            abi.encodePacked(
                b.id,
                b.background,
                b.flavor,
                b.eyes,
                b.hair,
                b.tattoo,
                b.mouth,
                b.headphones,
                b.clothes,
                b.glasses,
                b.stage,
                b.level,
                b.rarity
            );
    }

    function tokenURI(Bean memory b) public view returns (string memory) {
        string memory metadata = string(abi.encodePacked(
            '{"name": "Bean #',
            Strings.toString(b.id),
            '", "description": "Some Description", "image": "',
            baseTokenURI,
            Base64.encode(structToString(b)),
            '", "attributes": ',
            compileAttributes(b),
            '}'
        ));

        return string(abi.encodePacked(
            "data:application/json;base64,",
            Base64.encode(bytes(metadata))
        ));
    }
}

