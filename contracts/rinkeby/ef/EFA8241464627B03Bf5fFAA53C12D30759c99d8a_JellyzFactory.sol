pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC721Receiver.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./Bean.sol";
import "./JELL.sol";
import "./Jellyz.sol";


contract JellyzFactory is Ownable, Pausable {
    struct Stake {
        uint48 timestamp;
        uint48 timetotal;
        address owner;
    }

    Jellyz jellyz;
    JELL jell;

    mapping(uint => Stake) public stakes;

    uint[] public levelRanges;
    uint[] public levelRates;

    event TokenStaked(address owner, uint tokenId, uint timestamp);
    event TokenUnstaked(uint tokenId, uint timestamp);
    event JELLClaimed(address owner, uint earned);

    constructor(address _jellyz, address _jell) {
        jellyz = Jellyz(_jellyz);
        jell = JELL(_jell);
        levelRanges = [1, 30, 60, 100, 160, 220, 280, 340, 400, 460, 520, 580, 640, 735];
        levelRates = [5 ether, 10 ether, 15 ether, 20 ether, 25 ether, 30 ether, 35 ether, 40 ether, 45 ether, 50 ether, 55 ether, 70 ether, 85 ether, 100 ether];
    }

    function stake(uint16[] calldata tokenIds) public whenNotPaused {
        for (uint i = 0; i < tokenIds.length; i++) {
            require(jellyz.ownerOf(tokenIds[i]) == _msgSender(), "Not an owner!");
            require(jellyz.getTokenInfo(tokenIds[i]).stage == 3, "Only super NFT!");
            jellyz.transferFrom(_msgSender(), address(this), tokenIds[i]);
            stakes[tokenIds[i]].owner = _msgSender();
            stakes[tokenIds[i]].timestamp = uint48(block.timestamp);
            emit TokenStaked(_msgSender(), tokenIds[i], block.timestamp);
        }
    }

    function claimAndUnstake(uint16[] calldata tokenIds, bool unstake) public whenNotPaused {
        uint owed = 0;
        for (uint i = 0; i < tokenIds.length; i++) {
            uint16 tokenId = tokenIds[i];
            require(stakes[tokenId].owner == _msgSender(), "Not an owner");
            if (unstake) {
                jellyz.safeTransferFrom(address(this), _msgSender(), tokenId, "");
                owed += pendingJELL(tokenId);
                stakes[tokenIds[i]].timestamp = 0;
                emit TokenUnstaked(tokenId, block.timestamp);
            } else {
                owed += pendingJELL(tokenId);
                stakes[tokenId].timestamp = uint40(block.timestamp);
            }
            stakes[tokenIds[i]].timetotal += uint48(block.timestamp) - stakes[tokenIds[i]].timestamp;
        }
        if (owed != 0) {
            jell.mint(_msgSender(), owed);
            emit JELLClaimed(_msgSender(), owed);
        }
    }

    function getTokenLevel(uint tokenId) view public returns (uint) {
        uint totalTime = stakes[tokenId].timetotal; 
        if(stakes[tokenId].timestamp != 0) {
            totalTime += block.timestamp - stakes[tokenId].timestamp;
        }
        return getLevel(totalTime);
    }

    function getTokenState(uint tokenId) view public returns (uint) {
        if (stakes[tokenId].timestamp > 0)
            return 1;
        return 0;
    }

    function getLevel(uint time) pure private returns (uint) {
        return time / 12 hours + 1;
    }

    function getLevelIndex(uint level) view private returns (uint) {
        uint i = levelRanges.length - 1;
        while(i > 0 && level <= levelRanges[i]) {
            i -= 1;
        }
        return i;
    }

    function pendingJELL(uint tokenId) view public returns (uint amount) {
        if(stakes[tokenId].timestamp == 0) {
            return 0;
        }
        uint timeIndex = stakes[tokenId].timestamp;
        uint level = getLevel(stakes[tokenId].timetotal);
        uint levelIndex = getLevelIndex(level);
        uint toNextLevel = 12 hours - stakes[tokenId].timetotal % 12 hours;
        
        while (timeIndex + toNextLevel < block.timestamp) {
            amount += toNextLevel * levelRates[levelIndex] / 12 hours;
            level += 1;
            if (levelIndex != levelRanges.length - 1 && levelRanges[levelIndex + 1] <= level) {
                levelIndex += 1;
            }
            timeIndex += toNextLevel;
            toNextLevel = 12 hours;
        }
        amount += (block.timestamp - timeIndex) * levelRates[levelIndex] / 12 hours;  
    }
}

