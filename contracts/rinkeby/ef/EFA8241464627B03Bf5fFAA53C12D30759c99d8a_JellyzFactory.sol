pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./Bean.sol";
import "./JELL.sol";
import "./Traits.sol";


contract Jellyz is VRFConsumerBaseV2, ERC721Enumerable, Pausable, Ownable {
    JELL jell;
    Traits traits;
  
    VRFCoordinatorV2Interface immutable COORDINATOR;
    bytes32 internal vrfKeyHash;
    uint64 vrfSubscriptionId;
    uint32 vrfCallbackGasLimit;
    uint32 vrfNumWords =  1;
    uint16 vrfRequestConfirmations = 1;

    event RandomnessRequested(uint256);
    event BreddingToken1Burned(uint256);
    event BreddingToken2Burned(uint256);

    // struct to store each tokens breeds
    struct Breed {
        address owner;
        uint32 token1;
        uint32 token2;
    }
    address jellyzFactory; // contract address of jellyz factory

    uint16 public constant maxSupply = 10_000; // max token supply
    uint16 public minted = 0; // count minted tokens
    uint8 public batchLimit = 20; // mint amount limit
    uint8 public whitelistBatchLimit = 5; // mint amount limit for white list
    uint8 public bronzelistBatchLimit = 5; // mint amount limit for bronze list
    uint public constant mintPrice = 0.11 ether; // mint token price in ETH
    uint public constant levelupPrice = 200 ether; // levelup price in $JELL
    uint public constant evolutionPrice = 400 ether; // evolution price in $JELL
    uint public constant birthPrize = 10_000 ether; // birth prize in $JELL
    uint public constant breedingPrice = 600 ether; // breeding price if $JELL 
    uint public constant breedingCooldownResetPrice = 600 ether; // breeding cooldown reset price in $JELL
    bool public mintStarted = false; // is mint started flag
    bool public mintWhitelistStarted = false; // if is mint for white list started flag
    bool public mintBronzelistStarted = false; // if is mint for bronze list started flag
    bytes32 public whitelistMerkleRoot; // root of Merkle tree for only white list
    bytes32 public bronzelistMerkleRoot; // root of Merkle tree for only white list

    mapping(address => bool) public whitelistMinted; // store if sender is already minted from white list
    mapping(address => bool) public brozelistMinted; // store if sender is already minted from bronze list

    mapping(uint256 => Breed) private breedTokens; // map of struct breed tokens 
    mapping(uint256 => Bean) private beans; // minted tokens

    constructor(
        address _jell,
        address _traits,
        address _vrfCoordinator,
        uint64 _subscriptionId,
        bytes32 _keyHash,
        uint32 _callbackGasLimit
    ) ERC721("Jellyz", "Jellyz") VRFConsumerBaseV2(_vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        jell = JELL(_jell);
        traits = Traits(_traits);
        vrfKeyHash = _keyHash;
        vrfSubscriptionId = _subscriptionId;
        vrfCallbackGasLimit = _callbackGasLimit;
    }

    /**
     * inreasing the token level by token id if it possible
     * @param tokenId the id of the token to level up
    */ 
    function levelup(uint32 tokenId) public {
        require(_exists(tokenId) && (ownerOf(tokenId) == msg.sender), "Not your NFT!");
        require(jell.balanceOf(msg.sender) >= levelupPrice, "Not enough JELL");
        require(beans[tokenId].stage < 3, "Can not levelup super NFT");
        require(beans[tokenId].level < 10, "Already at max level");
                 
        jell.burn(msg.sender, levelupPrice);
        beans[tokenId].level += 1;
    }

    /**
     * evolve the token to a new token stage and rarity
     * @param tokenId the id of the token to evolve
    */ 
    function evolve(uint32 tokenId) public {
        require(_exists(tokenId) && (ownerOf(tokenId) == msg.sender), "Not your NFT!");
        require(jell.balanceOf(msg.sender) >= evolutionPrice, "Not enough JELL");
        require(beans[tokenId].stage < 3, "Can not levelup super NFT");
        require(beans[tokenId].level > 3, "Too low level to evolve");

        uint8 newRarity = 1;
        if(beans[tokenId].level == 10) {
            newRarity = 3;
        } else if (beans[tokenId].level > 6) {
            newRarity = 2;
        }
                 
        jell.burn(msg.sender, evolutionPrice);
        beans[tokenId].level = 1;
        beans[tokenId].stage += 1;
        beans[tokenId].rarity = newRarity;
    }

    /**
     * sent requests to breed two minted tokens to a new Baby NFT token
     * @param tokenId1 the id of the token to breed
     * @param tokenId2 the id of the token to breed
    */ 
    function breeding(uint32 tokenId1, uint32 tokenId2) public {
        require(tokenId1 != tokenId2, "Can't breed same NFT");
        require(_exists(tokenId1) && ownerOf(tokenId1) == msg.sender, "Not your NFT!");
        require(_exists(tokenId2) && ownerOf(tokenId2) == msg.sender, "Not your NFT!");
        require(jell.balanceOf(msg.sender) >= breedingPrice, "Not enough JELL");
        require(beans[tokenId1].stage == 2 && beans[tokenId2].stage == 2, "Token stage isn't adult");
        require(uint48(block.timestamp) >= (beans[tokenId1].timestamp + beans[tokenId1].breedCooldown), "Breed timer isn't over");
        require(uint48(block.timestamp) >= (beans[tokenId2].timestamp + beans[tokenId2].breedCooldown), "Breed timer isn't over");

        jell.burn(msg.sender, breedingPrice);
        requestRandomWords(tokenId1, tokenId2);
    }

    /**
     * resets breeding cooldown to the minimal wait time
     * @param tokenId the id of the token to reset breeding cooldown
    */
    function resetBreedingCooldown(uint32 tokenId) public {
        require(_exists(tokenId), "Token not exists");
        require(jell.balanceOf(msg.sender) >= breedingCooldownResetPrice, "Not enough JELL");

        jell.burn(msg.sender, breedingCooldownResetPrice);
        beans[tokenId].breedCooldown = 30 seconds;
    }

    /**
     * mint tokens to the sender address 
     * @param mintAmount amount of tokens to mint
    */
    function mint(uint8 mintAmount) public payable {
        require(mintStarted, "Mint is not started");
        require(mintAmount <= batchLimit, "Not in batch limit");
        require(mintPrice * mintAmount == msg.value, "Wrong amount of ETH");
        require(minted + mintAmount <= maxSupply, "Too much tokens to mint");

        mintInternal(mintAmount);
    }

    /**
     * white list mint tokens to the sender address
     * @param merkleProof Merkle tree data to verify is address in white list
     * @param mintAmount amount of tokens to mint
    */
    function whitelistMint(bytes32[] calldata merkleProof, uint8 mintAmount) public payable {
        require(mintWhitelistStarted, "Mint for whitelist is not started");
        require(mintAmount <= whitelistBatchLimit, "Not in batch limit");
        require(mintPrice * mintAmount == msg.value, "Wrong amount of ETH");
        require(minted + mintAmount <= maxSupply, "Too much tokens to mint");
        require(!whitelistMinted[msg.sender], "Already minted from whitelist.");
        require(
            MerkleProof.verify(
                merkleProof,
                whitelistMerkleRoot,
                keccak256(abi.encodePacked(msg.sender))
            ),
            "Failed to verify proof."
        );

        whitelistMinted[msg.sender] = true;
        mintInternal(mintAmount);
    }

        /**
     * bronze list mint tokens to the sender address
     * @param merkleProof Merkle tree data to verify is address in white list
     * @param mintAmount amount of tokens to mint
    */
    function bronzelistMint(bytes32[] calldata merkleProof, uint8 mintAmount) public payable {
        require(mintBronzelistStarted, "Mint for bronzelist is not started");
        require(mintAmount <= bronzelistBatchLimit, "Not in batch limit");
        require(mintPrice * mintAmount == msg.value, "Wrong amount of ETH");
        require(minted + mintAmount <= maxSupply, "Too much tokens to mint");
        require(!brozelistMinted[msg.sender], "Already minted from bronzelist.");
        require(
            MerkleProof.verify(
                merkleProof,
                bronzelistMerkleRoot,
                keccak256(abi.encodePacked(msg.sender))
            ),
            "Failed to verify proof."
        );

        brozelistMinted[msg.sender] = true;
        mintInternal(mintAmount);
    }

    /**
     * generate and mint tokens to the sender address
     * @param mintAmount amount of tokens to mint
    */
    function mintInternal(uint8 mintAmount) private {
        for (uint8 i = 1; i <= mintAmount; i++) {
            uint32 id = minted + i;
            Bean memory b = generate(id, random(id));
            beans[id] = b;
            _mint(msg.sender, id);
        }
        minted += mintAmount;
        jell.mint(msg.sender, mintAmount * birthPrize);
    }

    /**
     * withdraw funds from contract to owner
    */
    function withdraw() public onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    /**
     * set mint state for white list mint
     * @param mintState boolean state to set
    */
    function setWhitelistMintState(bool mintState) external onlyOwner {
        mintWhitelistStarted = mintState;
    }

    /**
     * set mint state for bronze list mint
     * @param mintState boolean state to set
    */
    function setBronzelistMintState(bool mintState) external onlyOwner {
        mintBronzelistStarted = mintState;
    }

    /**
     * set mint state for mint
     * @param mintState boolean state to set
    */
    function setMintState(bool mintState) external onlyOwner {
        mintStarted = mintState;
    }

    /**
     * set Merkle tree root for white list minting
     * @param _merkleRoot bytes data of Merkle root
    */
    function setWhitelistRoot(bytes32 _merkleRoot) public onlyOwner {
        whitelistMerkleRoot = _merkleRoot;
    }

    /**
     * set Merkle tree root for bronze list minting
     * @param _merkleRoot bytes data of Merkle root
    */
    function setBronzeListRoot(bytes32 _merkleRoot) public onlyOwner {
        bronzelistMerkleRoot = _merkleRoot;
    }

    /**
     * returns struct token traits
     * @param tokenId the id of the token to get information
    */
    function getTokenInfo(uint256 tokenId) public view returns (Bean memory) {
        return beans[tokenId];
    }

    /**
     * request random value from ChainlinkVRF coorinator (only for breeding function)
     * @param tokenId1 the id of the token to breed
     * @param tokenId2 the id of the token to breed
    */
    function requestRandomWords(uint32 tokenId1, uint32 tokenId2) private {
        uint256 requestId = COORDINATOR.requestRandomWords(
            vrfKeyHash,
            vrfSubscriptionId,
            vrfRequestConfirmations,
            vrfCallbackGasLimit,
            vrfNumWords
        );
        breedTokens[requestId] = Breed(msg.sender, tokenId1, tokenId2);
        emit RandomnessRequested(requestId);
    }

    /**
     * request callback from ChainlinkVRF coorinator to get random value (only for breeding function)
     * @param requestId ChainLink request id, provided by send request
     * @param randomWords array of integer random values
    */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        uint256 randomness = randomWords[0];
        uint32 tokenId1 = breedTokens[requestId].token1;
        uint32 tokenId2 = breedTokens[requestId].token2;
        uint32 babyTokenId = minted + 1;

        beans[tokenId1].timestamp = uint48(block.timestamp);
        beans[tokenId2].timestamp = uint48(block.timestamp);
        beans[tokenId1].breedCooldown *= beans[tokenId1].torpor;
        beans[tokenId2].breedCooldown *= beans[tokenId2].torpor;

        Bean memory b = generateBaby(babyTokenId, beans[tokenId1], beans[tokenId2], randomness);
        _mint(breedTokens[requestId].owner, babyTokenId);
        beans[babyTokenId] = b;
        minted += 1;
        
        if ((randomness & 0xFFFF) % 100 <= 18) {
            _burn(tokenId1);
            emit BreddingToken1Burned(tokenId1);
        }
        randomness >>= 16;
        if ((randomness & 0xFFFF) % 100 <= 18) {
            _burn(tokenId2);
            emit BreddingToken2Burned(tokenId2);
        }
    }

    /**
     * generates traits for a specific Adult token, checking to make sure it's unique
     * @param tokenId the id of the token to generate traits for
     * @param seed a pseudorandom 256 bit number to derive traits from
     * @return b - a struct of traits for the given token ID
    */ 
    function generate(uint32 tokenId, uint256 seed) internal view returns (Bean memory b) {           
        b = selectTraits(seed);
        b.id = uint32(tokenId);
        b.generation = 0;
        b.level = 1;
        b.stage = 2;
        b.timestamp = uint48(block.timestamp);
        b.breedCooldown = 30 seconds;
        
        uint8 rarityPercentage = uint8(seed % 100);
        if (rarityPercentage <= 5) {
            b.rarity =  3;  // rare
        } else if (rarityPercentage <= 15) {
            b.rarity = 2;  // epic
        }
        else {
            b.rarity = 1; // common
        }

        return b;
    }

    /**
     * generates traits for a specific Baby token, checking to make sure it's unique
     * @param tokenId the id of the token to generate traits for
     * @param father a struct of father token baby NFT
     * @param mother a struct of mother token baby NFT
     * @param seed a pseudorandom 256 bit number to derive traits from (generated by chainlink)
     * @return b - a struct of traits for the given token ID
    */ 
    function generateBaby(uint32 tokenId, Bean memory father, Bean memory mother, uint256 seed) internal pure returns (Bean memory b) {  
        b = selectBabyTraits([father.flavor, father.eyes], [mother.hair, mother.tattoo], seed);
        b.id = tokenId;
        b.generation = (father.generation >= mother.generation ? father.generation : mother.generation) + 1;
        b.level = (father.rarity  + mother.rarity) / 2;
    }

    /**
     * selects the species and all of its traits based on the seed value
     * @param seed a pseudorandom 256 bit number to derive traits from
     * @return b -  a struct of randomly selected traits
    */
