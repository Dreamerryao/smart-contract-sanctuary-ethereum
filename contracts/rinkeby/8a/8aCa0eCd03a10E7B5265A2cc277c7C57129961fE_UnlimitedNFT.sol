    function buyItem(uint256 _id, uint256 _amount)
        external
        payable
        nonReentrant
    {
        _checkCanBuy(_id);

        Item storage item = items[_id];

        
        uint256 _amountToPay = _amount;

        require(!item.flags.auctionable, "no auctionable");
        require(_amount == items[_id].price, "Below price");
        require(_amountToPay == msg.value, "invalid eth");

        // Process the payment
        _processPayment(_id, item.price);

        uint256 tokenId;
        if (item.flags.mintable) {
            _tokenIds.increment();
            tokenId = _tokenIds.current();

            // Mint token directly to buyer
            _safeMint(_msgSender(), tokenId);
            // Set the tokens metadata
            _setTokenURI(tokenId, item.uri);
            // Store creator of item
            _creators[tokenId] = _msgSender();
        } else {
            // Transfer ERC721 to buyer
            tokenId = item.tokenId;
            IERC721(item.collection).safeTransferFrom(
                address(this),
                _msgSender(),
                item.tokenId
            );
        }

        // Reduce available NFTs
        item.count = item.count.sub(1);
        if (item.count == 0) {
            item.flags.isSold = true;
        }

        emit ItemSold(_msgSender(), tokenId, _id, _amount);
    }

    function getLogTime() external view returns (uint256) {
        console.log("block.timestamp", block.timestamp);
        return block.timestamp;
    }

    function placeBid(uint256 _id, uint256 _amount)
        external
        payable
        nonReentrant
    {
        _checkCanBuy(_id);

        Item storage item = items[_id];
        (, uint256 amount, ) = lastBid(_id);
        uint256 _amountToPay = _amount; //calcBidAmount(_id, _amount);

        require(item.flags.auctionable, "no auctionable");
        require(_amount == _amountToPay, "invalid eth");
        require(_amount > amount && _amount > item.price, "insufficient");

        _refundLastBid(_id);

        Bid memory newBid;
        newBid.itemId = _id;
        newBid.bidder = _msgSender();
        newBid.amount = _amount;
        newBid.isActive = true;

        itemBids[_id].push(newBid);

        emit BidAddedToItem(_msgSender(), _id, _amount);
    }

