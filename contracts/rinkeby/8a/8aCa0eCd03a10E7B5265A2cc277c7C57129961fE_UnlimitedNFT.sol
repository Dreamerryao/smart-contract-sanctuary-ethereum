    function createItem(
        string memory _uri,
        uint256 _count,
        uint256 _price,
        bool _auctionable,
        uint256 _startTime,
        uint256 _endTime,
        address _currency
    ) external returns (uint256) {
        require(!_auctionable || _count == 1, "can mint only 1");
        // require(_price > 0, "invalid price");
        // require(isSupportedToken(_currency), "invalid payment currency");

        _itemIds.increment();
        uint256 currentId = _itemIds.current();
        Item storage item = items[currentId];

        {
            item.id = currentId;
            item.collection = address(this);
            item.creator = _msgSender();
            item.uri = _uri;
            item.count = _auctionable ? 1 : _count;
            item.price = _price;
            item.currency = _currency;
            item.phrase = "";
            item.auction.startTime = _auctionable ? _startTime : 0;
            item.auction.endTime = _auctionable ? _endTime : 0;
            item.flags.mintable = true;
            item.flags.isSold = false;
            item.flags.auctionable = _auctionable;
        }
        // console.log(block.timestamp);
        emit ItemCreated(currentId);
        return currentId;
    }

