    /**
     * @notice Ends the bidding and accepts the winning bid
     */
    function acceptWinningBid(uint256 _id)
        external
        onlyItemCreator(_id)
        nonReentrant
    {
        require(items[_id].flags.auctionable, "no auction");
        require(!items[_id].flags.isSold, "already sold");
        require(block.timestamp > items[_id].auction.endTime, "running now");

        Item storage item = items[_id];
        uint256 bidsLength = itemBids[_id].length;

        if (bidsLength == 0) {
            // cancelAuction(_id);
            // transfer from this contract to item creator
            if (!item.flags.mintable) {
                IERC721(item.collection).safeTransferFrom(
                    address(this),
                    item.creator,
                    item.tokenId
                );
            }

            item.flags.isSold = true;
            item.count = 0;
            emit AuctionCancelled(_id);
        } else {
            Bid storage _lastBid = itemBids[_id][bidsLength - 1];

            address buyer = _lastBid.bidder;
            // Process the payment
            uint256 amountToPay = _lastBid.amount; //calcBidAmount(_id, _lastBid.amount);
            _processPayment(_id, amountToPay);

            uint256 tokenId;
            if (item.flags.mintable) {
                _tokenIds.increment();
                tokenId = _tokenIds.current();

                // Mint token directly to buyer
                _safeMint(buyer, tokenId);
                // Set the tokens metadata
                _setTokenURI(tokenId, item.uri);
                // Store creator of item
                _creators[tokenId] = buyer;
            } else {
                // Transfer ERC721 to buyer
                tokenId = item.tokenId;
                IERC721(item.collection).safeTransferFrom(
                    address(this),
                    buyer,
                    item.tokenId
                );
            }

            // Reduce available NFTs
            item.count = item.count.sub(1);
            if (item.count == 0) {
                item.flags.isSold = true;
            }

            emit ItemSold(_msgSender(), tokenId, _id, _lastBid.amount);
        }
    }

    /**
     * Check _msgSender() can buy this item
     */
    function _checkCanBuy(uint256 _id) internal view {
        require(_id <= _itemIds.current(), "invalid item id");
        Item memory item = items[_id];

        require(!item.flags.isSold, "sold out");
        require(item.count > 0, "sold out");

        if (item.flags.auctionable) {
            require(block.timestamp >= item.auction.startTime, "not started");
            require(block.timestamp <= item.auction.endTime, "auction over");
        }

        if (!item.flags.mintable) {
            require(
                IERC721(item.collection).ownerOf(item.tokenId) == _msgSender(),
                "insufficient"
            );
        }

        require(item.creator != _msgSender(), "insufficient");
    }

    function _processPayment(uint256 _id, uint256 _amount) internal {
        Item memory item = items[_id];

        uint256 _total = _amount; //calcBidAmount(_id, _amount);
        uint256 _commissionValue = _total.mul(info.adminFee).div(1000);
        uint256 _royalties = _total
            .mul(
                item.collection == address(this) &&
                    _creators[item.tokenId] != address(0x0)
                    ? info.royalty
                    : 0
            )
            .div(1000);
        uint256 _sellerValue = _total.sub(_commissionValue).sub(_royalties);

        _safeTransferEth(item.creator, _sellerValue);
        if (_commissionValue > 0)
            _safeTransferEth(info.feeAddress, _commissionValue);
        if (_royalties > 0)
            _safeTransferEth(_creators[item.tokenId], _royalties);
    }

    /**
     * @notice Refunds all other bidders on acceptance of winning bid
     */
    function _refundLastBid(uint256 _id) internal {
        uint256 bidsLength = itemBids[_id].length;
        if (bidsLength > 0) {
            Bid storage _lastBid = itemBids[_id][bidsLength - 1];

            if (
                _lastBid.isActive &&
                _lastBid.amount > 0 &&
                _lastBid.bidder != address(0)
            ) {
                uint256 amountToRefund = _lastBid.amount; //calcBidAmount(_id, _lastBid.amount);
                _safeTransferEth(_lastBid.bidder, amountToRefund);
                _lastBid.isActive = false;
            }
        }
    }

    function lastBid(uint256 _id)
        public
        view
        returns (
            address,
            uint256,
            bool
        )
    {
        if (_id <= _itemIds.current()) {
            uint256 bidsLength = itemBids[_id].length;
            if (bidsLength > 0) {
                Bid memory _lastBid = itemBids[_id][bidsLength - 1];

                return (_lastBid.bidder, _lastBid.amount, _lastBid.isActive);
            }
        }
        return (address(0x0), 0, false);
    }

    function itemURI(uint256 _id) public view returns (string memory) {
        return items[_id].uri;
    }

    function _safeTransferEth(address to, uint256 value)
        internal
        returns (bool)
    {
        (bool success, ) = to.call{value: value}(new bytes(0));
        return success;
    }

    receive() external payable {}

    /**
        For Admin
     */
    function addSupportedToken(address _address) public onlyOwner {
        _supportedTokens.add(_address);
        // emit UpdatedSupportCurrency(_address, true);
    }

    function isSupportedToken(address _address) public view returns (bool) {
        return _supportedTokens.contains(_address);
    }

