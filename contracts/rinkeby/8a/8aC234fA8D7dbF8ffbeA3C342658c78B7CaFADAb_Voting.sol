pragma solidity ^0.8.4;
import "hardhat/console.sol";

contract Voting {

    address owner;

    struct candidate {                  /// struct of the candidates 
        address candidateAddress;
        uint votesNumber;
    }

    struct voting {                                 /// struct of the votings that 
        candidate[] candidates;                     /// stores all the information 
        mapping (address => bool) voters;           /// about sertain voting
        uint creationTime;
        uint prizePull;
        bool ended;
    }

    uint comission;                                 /// comission saved on the platform
    uint charge = 10000000000000000;                /// fee to pay
    uint public contractBalance = address(this).balance;    

    voting[] votings;                               /// array of votings
    uint public votingsNumber;                      /// number of votings to interact with voting ids


    event votingCreated (
        uint votingIndex
    );

    modifier ownerOnly{
        require(msg.sender == owner, "Not an owner");
        _;
    }

    constructor(){
        owner = msg.sender;
    }

    function createVoting()                         /// basicaly we just create a new voting and add into our array
    external
    ownerOnly
    {
        uint votingIndex = votingsNumber;
        votings.push();
        votingsNumber++;
        voting storage v = votings[votingsNumber-1];
        v.creationTime = block.timestamp;
        emit votingCreated (votingIndex);
        // for (uint i; i < _candidatesNumber; ++i){
        //     v.candidates.push(candidate({
        //         candidateAddress : _candidatesFile[i],
        //         votesNumber : 0
        //     }));
        // }
        
    }

    function showNumber()
    external    
    view
    returns (uint)
    {
        uint number = votingsNumber;
        return (number);
    }
    
    function addCandidate (uint _votingIndex, address _candidateAddress)        /// using votingindex we add candidates to some voting
    external
    ownerOnly
    {

        votings[_votingIndex].candidates.push(candidate({
            candidateAddress : _candidateAddress,
            votesNumber :0
        }) );

    }

    // function votingInfo (uint _votingIndex)                             /// here we observe amount of candidates to vote
    // external
    // view
    // returns (memory voting)
    // {
    //     require(_votingIndex < votings.length, "Voting is not found");
    //     return votings[_votingIndex];
    // }

    function candidateInfo (uint _votingIndex, uint _candidateIndex)        /// here we observe vote count of some candidate at some voting
    external
    view
    returns (uint)
    {
        uint count = votings[_votingIndex].candidates[_candidateIndex].votesNumber;
        return (count);
    }

    function doVote (uint _votingIndex, uint _candidateIndex)              /// function of voting that checks if you've already voted and checks the transaction
    external
    payable
    {
        require(votings[_votingIndex].voters[msg.sender] == false, "Already voted");
        require (charge == msg.value, "Transfer 0.01 ETH to vote");

        votings[_votingIndex].candidates[_candidateIndex].votesNumber += 1;
        votings[_votingIndex].voters[msg.sender] = true;
        votings[_votingIndex].prizePull = charge*9/10;
        comission += charge*1/10;
    }

    function endVoting (uint _votingIndex)                          /// function to end some voting
    external
    payable
    {
       require(block.timestamp >= votings[_votingIndex].creationTime + 259200, "It's not time yet");
       require(votings[_votingIndex].ended == false, "Voting already ended");
       
       uint maxVotesNumber;
       uint winnerIndex;
       uint winnersAmount;

       for (uint i; i < votings[_votingIndex].candidates.length; ++i) {
            if (votings[_votingIndex].candidates[i].votesNumber > maxVotesNumber){
                maxVotesNumber = votings[_votingIndex].candidates[i].votesNumber;
                winnerIndex = i;
                winnersAmount = 1;
            }
            else if (votings[_votingIndex].candidates[i].votesNumber == maxVotesNumber) winnersAmount += 1; 
        }

        if (winnersAmount == 1){
            payable(votings[_votingIndex].candidates[winnerIndex].candidateAddress).transfer(votings[_votingIndex].prizePull);
        }

        else if (winnersAmount > 1){
           uint _prize = (votings[_votingIndex].prizePull)*(1/winnersAmount);
           for (uint i; i < votings[_votingIndex].candidates.length; ++i) {
                
                if (votings[_votingIndex].candidates[i].votesNumber == maxVotesNumber){
                    payable(votings[_votingIndex].candidates[winnerIndex].candidateAddress).transfer(_prize);
                }
            }
        }

    }

    function comissionWithdraw(address payable _to)                         /// function to transfer assets from the platform to so address
    external
    payable
    ownerOnly
    {
        if (comission == 0) revert ("No comission on the platform");
        _to.transfer(comission);
        comission = 0;
    }
}

