pragma solidity 0.8.10;
import "hardhat/console.sol";
import "./ERC721EnumB.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract BabyDragons is ERC721EnumB, ReentrancyGuard, Ownable, Pausable {
    string public baseUri;
    string public extension = ".json";
    uint256 public supply = 40000;
    address private teamAddress;
    address private lpAddress;
    address private stakingAddress;
    bool isBondingCurve = true;

    uint256 private burnPercentage = 20;
    uint256 private teamPercentage = 5;
    uint256 private lpPercentage = 35;
    uint256 private stakingPercentage = 40;
    using Strings for uint256;

    struct AcceptedToken {
        bool isAccepted;
        uint256 basePrice;
        uint256 rate;
    }
    mapping(ERC20Burnable => AcceptedToken) acceptedTokens;

    constructor(address _lp, address _team, address _staking) ERC721B("BabyDragons", "BD") {
        setLP(_lp);
        setTeam(_team);
        setStaking(_staking);
        pause();
    }

    /**
        @dev on mint supply the token you want to use
        burn 20%
        send to staking 40%
        send to LP 35%
        send to team 5%
        @param currency should be an ERC20 token that has been approved by owner
     */
    function mint(ERC20Burnable currency) external nonReentrant whenNotPaused {
        uint256 s = totalSupply();
        require(s + 1 <= supply, "Cannot exceed");
        uint256 required = getPrice(currency);
        require(currency.balanceOf(_msgSender()) >= required, "Not enough");
        _safeMint(_msgSender(), s + 1);
        currency.burnFrom(_msgSender(), getScaledPercentage(required, burnPercentage));
        transferToStaking(currency, required);
        transferToLP(currency, required);
        transferToTeam(currency, required);
    }

    /**
        @dev approve spending token for minting process
     */
    function approveSpendingToken(ERC20Burnable token, uint rawAmount) external returns (bool) {
        uint256 amount;
        if (rawAmount ==  type(uint256).max) {
            amount = type(uint256).max;
        } else {
            amount = safe256(rawAmount, "amount exceeds 256 bits");
        }
        token.approve(address(this), amount);
        return true;
    }

    /**
        @dev used to get a % amount from the currency
        num should be 18 decimals
        e.g. 500e16 * 40 / 100 * 1e18 = 500e16 * 40 * 1e16
     */
    function getScaledPercentage(uint256 num, uint256 percentage) internal pure returns (uint256) {
        uint256 scaled = num * (percentage) * 1e16;
        return scaled / (1e18);
    }

    function transferToStaking(ERC20Burnable currency, uint256 amount) internal {
        currency.transferFrom(_msgSender(), stakingAddress, getScaledPercentage(amount, stakingPercentage));
    }

    function transferToTeam(ERC20Burnable currency, uint256 amount) internal {
        currency.transferFrom(_msgSender(), teamAddress, getScaledPercentage(amount, teamPercentage));
    }

    function transferToLP(ERC20Burnable currency, uint256 amount) internal {
        currency.transferFrom(_msgSender(), lpAddress, getScaledPercentage(amount, lpPercentage));
    }

    /** VIEW */

    function safe256(uint n, string memory errorMessage) internal pure returns (uint256) {
        require(n < 2**96, errorMessage);
        return uint256(n);
    }

    /**
        @dev check if it is accepted token
        get base price by multiplying by the discountRate 
        if its not a bonding curve return normal price
        otherwise return the basePrice x bonding curve
     */
    function getPrice(ERC20Burnable currency) public view returns (uint256) {
        AcceptedToken memory token = acceptedTokens[currency];
        require(token.isAccepted, "Not approved token");
        uint256 s = totalSupply();        
        uint256 tokenBase = getScaledPercentage(token.basePrice, token.rate);
        if(!isBondingCurve) return tokenBase;
        if(s == 0) return tokenBase;
        uint256 precision = 8;
        if(s < 8) precision = s;  
        return fracExp(tokenBase, 33333, s, precision);
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
	    require(_exists(tokenId), "ERC721Metadata: Nonexistent token");
	    string memory currentBaseURI = baseUri;
	    return bytes(currentBaseURI).length > 0	? string(abi.encodePacked(currentBaseURI, tokenId.toString(), extension)) : "";
	}

    /*** UTILITIES */
    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    /**
        @dev k is the multiplier, 
             q is the denominator for percentage,
             n is the total
             p is the precision

        e.g. k * (1 + 1/q)^n
        8 + 1.01^5
        k = 8
        q = 100
        n = 5
     */
    function fracExp(uint k, uint q, uint n, uint p) internal pure returns (uint) {
        uint s = 0;
        uint N = 1;
        uint B = 1;
        for (uint i = 0; i < p; ++i){
            s += k * N / B / (q**i);
            N  = N * (n-i);
            B  = B * (i+1);
        }
        return s;
    }

    /*** SETTERS */
    function setSupply(uint256 _supply) public onlyOwner {
        supply = _supply;
    }

    function setAcceptedToken(ERC20Burnable currency, bool _accepted, uint256 _basePrice, uint256 _rate) public onlyOwner {
        AcceptedToken memory token = acceptedTokens[currency];
        token.isAccepted = _accepted;
        token.basePrice = _basePrice;
        token.rate = _rate;
        acceptedTokens[currency] = token;
    }

    function setBondingCurve(bool _bonding) public onlyOwner {
        isBondingCurve = _bonding;
    }

    function setTeam(address _team) public onlyOwner {
        teamAddress = _team;
    }
    
    function setLP(address _lp) public onlyOwner {
        lpAddress = _lp;
    }

    function setStaking(address _staking) public onlyOwner {
        stakingAddress = _staking;
    }

    function setTeamPercentage(uint256 _team) public onlyOwner {
        teamPercentage = _team;
    }

    function setBurnPercentage(uint256 _burn) public onlyOwner {
        burnPercentage = _burn;
    }
    
    function setLPPercentage(uint256 _lp) public onlyOwner {
        lpPercentage = _lp;
    }

    function setStakingPercentage(uint256 _staking) public onlyOwner {
        stakingPercentage = _staking;
    }

    function setExtensions(string memory _extension) public onlyOwner {
        extension = _extension;
    }
}

