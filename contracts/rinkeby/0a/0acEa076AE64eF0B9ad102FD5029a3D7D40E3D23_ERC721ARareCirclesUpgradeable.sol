pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/IERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "./Mint721AValidator.sol";
import "./ERC721AURIUpgradeable.sol";
import "./LibERC721AMint.sol";
import "./RoyaltyUpgradeable.sol";
import "./ProcessorUpgradeable.sol";
import "./ERC721AEnumerableUpgradeable.sol";
import "./OwnablePausableUpgradeable.sol";

contract ERC721ARareCirclesUpgradeable is
    ContextUpgradeable,
    AccessControlUpgradeable,
    UUPSUpgradeable,
    OwnablePausableUpgradeable,
    ERC721AUpgradeable,
    Mint721AValidator,
    ERC721AURIUpgradeable,
    ERC721AEnumerableUpgradeable,
    RoyaltyUpgradeable,
    ProcessorUpgradeable
{
    using SafeMathUpgradeable for uint256;

    event CreateERC721ARareCircles(address indexed owner, string name, string symbol);
    event Payout(address indexed to, uint256 amount);
    event Fee(address indexed to, uint256 amount);
    event Mint(uint256 amount, address indexed to);
    event BaseURI(string uri);
    event PlaceholderHolderURI(string uri);
    event MerchantTreasury(address treasury);
    event RarecirclesTreasury(address treasury);
    event MaxSupply(uint256 maxSupply);
    event Name(string name);
    event Symbol(string symbol);

    // =============================================================
    //                           CONSTANTS
    // =============================================================

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    // Mask of an entry in packed address data.
    uint256 private constant _BITMASK_ADDRESS_COUNT_ENTRY = (1 << 64) - 1;
    // The bit position of `numberWhitelist` in packed address counts.
    uint256 private constant _BITPOS_NUMBER_WHITELISTED = 64;
    // The bit position of `numberMinted` in packed address counts.
    uint256 private constant _BITPOS_NUMBER_MINTED = 128;
    // The bit position of `numberSignature` in packed address counts.
    uint256 private constant _BITPOS_NUMBER_SIGNATURE = 192;

    // =============================================================
    //                            STORAGE
    // =============================================================

    // Total max supply of contract
    uint256 private _maxSupply;

    // RareCircles Treasury
    address private _RCTreasury;

    // Merchant Treasury
    address private _merchantTreasury;

    // Indicates that the contract has been setup after creation.
    bool private _setup;

    // Mapping owner address to address counts.
    //
    // Bits Layout:
    // - [0..63]    `numberAirdropped`
    // - [64..127]  `numberWhitelisted`
    // - [128..191] `numberMinted`
    // - [192..255] `numberSignature`
    mapping(address => uint256) private _packedAddressCounts;

    // Record of already-used signatures.
    mapping(bytes => bool) public usedSigs;

    // =============================================================
    //                          INITIALIZATION
    // =============================================================
    function __ERC721ARareCircles_init(string memory _name, string memory _symbol, string memory _baseURI, string memory _placeholderURI) external virtual {
        __ERC721ARareCircles_init_unchained(_name, _symbol, _baseURI, _placeholderURI);

        emit CreateERC721ARareCircles(_msgSender(), _name, _symbol);
    }

    function __ERC721ARareCircles_init_unchained(string memory _name,
        string memory _symbol,
        string memory _baseURI,
        string memory _placeholderURI) internal initializer {
        _setBaseURI(_baseURI);
        _setPlaceholderURI(_placeholderURI);
        _setup = false;

        __Context_init_unchained();
        __AccessControl_init_unchained();
        __UUPSUpgradeable_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();
        __ERC721AURI_init_unchained();
        __Mint721AValidator_init_unchained();
        __ERC165_init_unchained();
        __ERC721A_init_unchained(_name, _symbol);
        __Royalty_init_unchained();
        __Processor_init_unchained();
    }

    // =============================================================
    //                          UPGRADEABLE
    // =============================================================

    function _authorizeUpgrade(address) internal override onlyOperatorOrOwner {}

    // =============================================================
    //                            IERC165
    // =============================================================
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721AUpgradeable, ERC721AEnumerableUpgradeable, AccessControlUpgradeable, RoyaltyUpgradeable)
        returns (bool)
    {
        return
            ERC721AUpgradeable.supportsInterface(interfaceId) ||
            ERC721AEnumerableUpgradeable.supportsInterface(interfaceId) ||
            RoyaltyUpgradeable.supportsInterface(interfaceId) ||
            AccessControlUpgradeable.supportsInterface(interfaceId);
    }

    // =============================================================
    //                    ADDRESS COUNT OPERATIONS
    // =============================================================

    /**
     * Returns the number of tokens airdropped by `owner`.
     */
    function numberAirdropped(address owner) public view returns (uint256) {
        return _packedAddressCounts[owner] & _BITMASK_ADDRESS_COUNT_ENTRY;
    }

    /**
     * Returns the number of tokens whitelisted by `owner`.
     */
    function numberWhitelisted(address owner) public view returns (uint256) {
        return (_packedAddressCounts[owner] >> _BITPOS_NUMBER_WHITELISTED) & _BITMASK_ADDRESS_COUNT_ENTRY;
    }

    /**
     * Returns the number of tokens minted via public sale by the `owner`.
     */
    function numberMinted(address owner) public view returns (uint256) {
        return (_packedAddressCounts[owner] >> _BITPOS_NUMBER_MINTED) & _BITMASK_ADDRESS_COUNT_ENTRY;
    }

    /**
    * Returns the number of signatures submitted by the `owner`.
    */
    function numberSignature(address owner) public view returns (uint256) {
        return (_packedAddressCounts[owner] >> _BITPOS_NUMBER_SIGNATURE) & _BITMASK_ADDRESS_COUNT_ENTRY;
    }

    /**
    * Returns the number of total minted by the `owner`.
    */
    function totalMinted(address owner) internal view returns (uint256) {
        return _numberMinted(owner);
    }
    
    // =============================================================
    //                        MINT OPERATIONS
    // =============================================================


    function airdrop(LibERC721AMint.Mint721AData memory data) public payable {
        _validateUnsafeData(data);
        _validateSignature(data);
        _canAirdrop(data.recipient, data.limit, data.amount);
        require(data.cost == 0, "Expected to cost for an airdrop");
        _mintAndTransfer(data);
        // Overflows are incredibly unrealistic. `count` have a maximum limit of 2**64.
        unchecked {
            _packedAddressCounts[data.recipient] += data.amount * (1);
        }
        useSignature(data.recipient, data.signature);
    }

    function whitelist(LibERC721AMint.Mint721AData memory data) public payable {
        _validateUnsafeData(data);
        _validateSignature(data);
        _canWhitelist(data.recipient, data.limit, data.amount);
        _mintAndTransfer(data);
        // Overflows are incredibly unrealistic. `count` have a maximum limit of 2**64.
        unchecked {
            _packedAddressCounts[data.recipient] += data.amount * (1 << _BITPOS_NUMBER_WHITELISTED);
        }
        useSignature(data.recipient, data.signature);
    }

    function mint(LibERC721AMint.Mint721AData memory data) public payable {
        _validateUnsafeData(data);
        _validateSignature(data);
        _canMint(data.recipient, data.limit, data.amount);
        _mintAndTransfer(data);
        // Overflows are incredibly unrealistic. `count` have a maximum limit of 2**64.
        unchecked {
            _packedAddressCounts[data.recipient] += data.amount * (1 << _BITPOS_NUMBER_MINTED);
        }
        useSignature(data.recipient, data.signature);
    }

    function processor(address _to, uint256 quantity_) public payable onlyProcessorOrOwner {
        require(_getProcessorConfig().enabled, "Processor checkout is not enabled");
        require((_getProcessorConfig().mintPrice * quantity_) == msg.value, "Incorrect ETH value sent");
        _canMint(_to, _getProcessorConfig().limit, quantity_);
        _mintTo(quantity_, _to);
    }

    function mintTo(uint256 _amount, address _to) public onlyMinterOrOwner {
        _mintTo(_amount, _to);
    }

    // Minting functions
    function _mintAndTransfer(LibERC721AMint.Mint721AData memory data) internal {
        _mintTo(data.amount, data.recipient);
        uint256 payout = msg.value - data.fee;
        if (payout > 0 && _merchantTreasury != address(0)) {
            payable(_merchantTreasury).transfer(payout);
            emit Payout(_merchantTreasury, payout);
        }
        if (data.fee > 0 && _RCTreasury != address(0)) {
            payable(_RCTreasury).transfer(data.fee);
            emit Fee(_RCTreasury, data.fee);
        }
    }


    function _mintTo(uint256 _amount, address _to) internal whenNotPaused {
        // Max Supply
        if (_maxSupply > 0) {
            require(totalSupply() + _amount <= _maxSupply, "cannot exceed max supply");
        }
        _safeMint(_to, _amount);
        emit Mint(_amount, _to);
    }


    // =============================================================
    //                        VALIDATION OPERATIONS
    // =============================================================
    /**
    @dev valid the mint input data, at this point we do not know if data's signature
    is valid
     */
    function _validateUnsafeData(LibERC721AMint.Mint721AData memory data) private {
        require(data.fee <= msg.value, "Application fee must be less then or equal to ETH sent");
        require(data.amount > 0, "Cannot mint 0 tokens");
        require(data.cost == msg.value, "Insufficient amount");
        require(data.recipient == _msgSender(), "Invalid caller");
    }

    function _validateSignature(LibERC721AMint.Mint721AData memory data) private view {
        // We make sure that this has been signed by the contract owner
        bytes32 _hash = LibERC721AMint.hash(data);
        // validate(owner(), hash, data.signature);
        address signer = getValidator(_hash, data.signature);
        require(signer == owner() || hasRole(MINTER_ROLE, signer), "Signature verification error");
        require(!usedSigs[data.signature], "Signature already used");
    }

    function _canAirdrop(address owner, uint256 limit, uint256 quantity) private view {
        require((limit == 0) || ((numberAirdropped(owner) + quantity) <= limit), "cannot exceed airdrop limit");
    }

    function _canMint(address owner, uint256 limit, uint256 quantity) private  view {
        require((limit == 0) || ((numberMinted(owner) + quantity) <= limit), "cannot exceed mint limit");
    }

    function _canWhitelist(address owner, uint256 limit, uint256 quantity) private  view {
        require((limit == 0) || ((numberWhitelisted(owner) + quantity) <= limit), "cannot exceed whitelist limit");
    }

    // =============================================================
    //                        Signature
    // =============================================================

    function useSignature(address owner, bytes memory signature) private {
        usedSigs[signature] = true;
        // Overflows are incredibly unrealistic. `count` have a maximum limit of 2**64.
        unchecked {
            _packedAddressCounts[owner] += (1 << _BITPOS_NUMBER_SIGNATURE);
        }
    }

    // =============================================================
    //                        RBAC
    // =============================================================

    function setMinter(address _minter) external onlyOwner {
        _setupRole(MINTER_ROLE, _minter);
    }

    function setOperator(address _operator) external onlyOwner {
        _setupRole(OPERATOR_ROLE, _operator);
    }

    // =============================================================
    //                        METADATA
    // =============================================================
    // TokenURI
    function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override(ERC721AUpgradeable, ERC721AURIUpgradeable)
    returns (string memory)
    {
        return ERC721AURIUpgradeable.tokenURI(tokenId);
    }

    function setTokenURI(uint256 _tokenId, string memory _tokenURI)
        public
        onlyOperatorOrOwner
    {
        return ERC721AURIUpgradeable._setTokenURI(_tokenId, _tokenURI);
    }

    function setBaseTokenURI(string memory baseTokenURI_) public onlyOperatorOrOwner {
        _setBaseURI(baseTokenURI_);
        emit BaseURI(baseTokenURI_);
    }

    function baseTokenURI() public view returns (string memory) {
        return super.baseURI();
    }

    function setPlaceholderURI(string memory placeholderURI_) public onlyOperatorOrOwner {
        _setPlaceholderURI(placeholderURI_);
        emit PlaceholderHolderURI(placeholderURI_);
    }

    function placeholderURI() public view override returns (string memory) {
        return super.placeholderURI();
    }

    // Token Enumeration
    function tokenOfOwnerByIndex(address owner, uint256 index)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return super.tokenOfOwnerByIndex(owner, index);
    }

    function tokensOfOwner(address owner)
        public
        view
        virtual
        override
        returns (uint256[] memory)
    {
        return super.tokensOfOwner(owner);
    }

    function tokenByIndex(uint256 index)
    public
    view
    virtual
    override
    returns (uint256)
    {
        return super.tokenByIndex(index);
    }

    // =============================================================
    //                     Royalty
    // =============================================================
    function setRoyalty(address creator_, uint256 amount_) public onlyOperatorOrOwner {
        _saveRoyalty(LibPart.Part(creator_, amount_));
    }

    // =============================================================
    //                     Processor
    // =============================================================
    function setProcessorConfig(bool enabled_, address processor_, uint256 price_, uint256 limit_) public onlyOperatorOrOwner {
        _saveProcessorConfig(ProcessorUpgradeable.ProcessorConfig(enabled_, processor_, price_, limit_));
    }

    // =============================================================
    //                     Limits And Supply
    // =============================================================
    function totalSupply() public view virtual override(ERC721AEnumerableUpgradeable, ERC721AUpgradeable) returns (uint256) {
        return ERC721AEnumerableUpgradeable.totalSupply();
    }

    function setMaxSupply(uint256 maxSupply_) public onlyOperatorOrOwner {
        _maxSupply = maxSupply_;
        emit MaxSupply(maxSupply_);
    }

    function maxSupply() public view returns (uint256) {
        return _maxSupply;
    }

    // =============================================================
    //                     TREASURY ADDRESS
    // =============================================================
    function setMerchantTreasury(address treasury_) public onlyOperatorOrOwner {
        _merchantTreasury = treasury_;
        emit MerchantTreasury(treasury_);
    }

    function merchantTreasury() public view returns (address) {
        return _merchantTreasury;
    }

    function setRCTreasury(address treasury_) public onlyOperatorOrOwner {
        _RCTreasury = treasury_;
        emit RarecirclesTreasury(treasury_);
    }

    function RCTreasury() public view returns (address) {
        return _RCTreasury;
    }

//    // =============================================================
//    //                     METADATA OVERRIDE
//    // =============================================================
//    function setName(string memory name_) public onlyOperatorOrOwner {
//        _setName(name_);
//        emit Name(name_);
//    }

    // =============================================================
    //                     SETUP
    // =============================================================
    function setup(address creator_, uint256 amount_, uint256 maxSupply_,address minter_, address operator_) public onlyOwner {
        require(!_setup,"ERC721: contract is already setup");
        _setup = true;
        _saveRoyalty(LibPart.Part(creator_, amount_));
        setMaxSupply(maxSupply_);
        _setupRole(MINTER_ROLE, minter_);
        _setupRole(OPERATOR_ROLE, operator_);
    }

    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal override(ERC721AUpgradeable, ERC721AEnumerableUpgradeable) {
        super._beforeTokenTransfers(from, to, startTokenId, quantity);
    }

    modifier onlyMinterOrOwner() {
        require(hasRole(MINTER_ROLE, _msgSender()) || owner() == _msgSender(), "Caller is not a minter nor owner");
        _;
    }

    modifier onlyProcessorOrOwner() {
        require((_getProcessorConfig().account == _msgSender()) || owner() == _msgSender(), "Caller is not a processor nor owner");
        _;
    }

    modifier onlyOperatorOrOwner() {
        require(hasRole(OPERATOR_ROLE, _msgSender()) || owner() == _msgSender(), "Caller is not a operator nor owner");
        _;
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[44] private __gap;
}

