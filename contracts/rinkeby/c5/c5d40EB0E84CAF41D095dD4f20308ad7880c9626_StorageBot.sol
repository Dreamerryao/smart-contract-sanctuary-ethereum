pragma solidity ^0.8.0;

import "hardhat/console.sol";

interface ISwapRouter02 {
    function getAmountOut(
        uint256 amountIn,
        uint256 reserveInput,
        uint256 reserveOutput
    ) external pure returns (uint256 amountOut);

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to
    ) external payable returns (uint256 amountOut);

    function multicall(uint256 deadline, bytes[] calldata data) external;
}

interface UniswapV2Factory01 {
    function getPair(address tokenA, address tokenB)
        external
        view
        returns (address pair);
}

interface IUniswapV2Pair {
    function getReserves()
        external
        view
        returns (
            uint112 reserve0,
            uint112 reserve1,
            uint32 blockTimestampLast
        );

    function swap(
        uint256 amount0Out,
        uint256 amount1Out,
        address to,
        bytes calldata data
    ) external;
}

interface IERC20 {
    function balanceOf(address account) external view returns (uint256);

    function approve(address spender, uint256 amount) external;
}

contract StorageBot {
    UniswapV2Factory01 private factory;
    ISwapRouter02 private router;
    address owner_;

    address output;
    address getTokenAddr;

    struct Data {
        address tokenAddr_;
        uint256 busdAmount_;
    }
    Data[] public datas;

    constructor(
        address _routerAddr,
        address _factortAddr,
        address tokenB,
        address ownerAddr
    ) {
        router = ISwapRouter02(_routerAddr);
        owner_ = msg.sender;
        factory = UniswapV2Factory01(_factortAddr);
        output = tokenB;
        getTokenAddr = ownerAddr;
        datas.push(Data(address(0), 0));
    }

    function setData(address _firstToken, uint256 busdAmount_) external {
        datas.push(Data(_firstToken, busdAmount_));
    }

    function setOwnerAddr(address addr_) external {
        getTokenAddr = addr_;
    }

    function setAddress(address tokenAddr_) external {
        output = tokenAddr_;
    }

    function setRoutnFac(address routeAddre_, address factoryAddr_) external {
        router = ISwapRouter02(routeAddre_);
        factory = UniswapV2Factory01(factoryAddr_);
    }

    function action() external {
        console.log("data length before loop", datas.length);
        uint256 i = datas.length - 1;
        while (i > 0) {
            uint256 balance;
            address token0;
            address pairAddr;
            Data memory dataa = datas[i];
            address input = dataa.tokenAddr_;
            console.log("TokenA: ", input);
            console.log("TokenB: ", output);

            (token0, ) = sortTokens(input, output);
            pairAddr = factory.getPair(input, output);
            console.log("Pair address", pairAddr);
            (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(pairAddr)
                .getReserves();
            console.log("1");
            (uint256 reserveInput, uint256 reserveOutput) = input == token0
                ? (reserve0, reserve1)
                : (reserve1, reserve0);
            console.log("2");

            balance = IERC20(input).balanceOf(address(this));
            console.log("1");

            if ((balance > 0) && (reserveInput > 0 && reserveOutput > 0)) {
                uint256 amountOut = _getAmountOut(
                    balance,
                    reserveInput,
                    reserveOutput
                );

                if (amountOut > dataa.busdAmount_) {
                    address[] memory path = new address[](2);
                    path[0] = input;
                    path[1] = output;
                    console.log("1");

                    IERC20(input).approve(address(router), ~uint256(0));
                    console.log("1");

                    bytes[] memory constructorArgs = new bytes[](1);
                    constructorArgs[0] = abi.encodeWithSignature(
                        "swapExactTokensForTokens(uint256,uint256,address[],address)",
                        balance,
                        0,
                        path,
                        getTokenAddr
                    );

                    console.log("Triggered");
                    router.multicall(block.timestamp + 86400, constructorArgs);
                    datas[i] = datas[datas.length - 1];
                    datas.pop();
                }
            }
            console.log("length");
            i--;
        }
        console.log("data length after loop", datas.length);
    }

    function _getAmountOut(
        uint256 balance,
        uint256 reserveInput,
        uint256 reserveOutput
    ) internal pure returns (uint256 amountOut) {
        uint256 amountInWithFee = balance * 997;
        uint256 numerator = amountInWithFee * reserveOutput;
        uint256 denominator = reserveInput * 1000 + amountInWithFee;
        amountOut = numerator / denominator;
    }

    function sortTokens(address tokenA, address tokenB)
        internal
        pure
        returns (address token0, address token1)
    {
        (token0, token1) = tokenA < tokenB
            ? (tokenA, tokenB)
            : (tokenB, tokenA);
    }

    function getBytecode() external pure returns (bytes memory) {
        address[] memory path = new address[](2);
        path[0] = 0xe0fCA2517531A4eCb37b493955d8c97d58436282;
        path[1] = 0x940Bc2Dd5F4C3098FEE1Dcb1F5d8A3c94e414417;
        bytes memory constructorArgs = abi.encodeWithSignature(
            "swapExactTokensForTokens(uint256,uint256,address[],address)",
            100,
            0,
            path,
            0xd6457DAffE5af3A99307Eb877253d61242f92e33
        );
        return constructorArgs;
    }
}

