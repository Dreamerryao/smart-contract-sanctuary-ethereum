pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract ERC20 is ERC20Upgradeable,OwnableUpgradeable{

    using SafeMath for uint256;
    using SafeMath for uint16;
   
     uint256 nftId;
     address nftOwner;
     address erc20_addr;
     uint256 dex_rp;
     uint256 p2p_rp;
     uint256 no_of_artbits;        
    address public gaxAdmin;
    
    uint256 public whiteList_count;
    uint256 public dexList_count;
    uint256 private totalCommission;
    uint256 private totalRoyalty; 

    mapping(address => uint256) private _balances; 
    mapping (address => bool) private whiteList_addresses;
    mapping (address => bool) private dex_addresses;
    

    function initialize(string memory _name,string memory _symbol,
    uint256 _initialSupply, address _gaxAdmin) initializer public returns (bool){

        __Ownable_init_unchained();
        __ERC20_init(_name,_symbol);
        mint(_gaxAdmin,_initialSupply);
        gaxAdmin = _gaxAdmin;
        return true;
    }

    modifier onlyAdmin {
        require(msg.sender == gaxAdmin, "Sender is not Admin.");
        _;
    }

    function changeOwnership(address _newOwner) external onlyAdmin{
         gaxAdmin = _newOwner;
    } 

   function balance(address account) public view returns(uint){
       return  balanceOf(account);
    } 

   function mint(address account,uint256 amount) public returns(bool){
         _mint(account,amount);
         return true;
    }

    function setArtbitAsset(uint256 _nftId, address _nftOwner, address _erc20Addr, uint256 _dex_rp, uint256 _p2p_rp, uint256 _artbits) public {
         nftId= _nftId;
         nftOwner = _nftOwner; 
         erc20_addr = _erc20Addr;
         dex_rp = _dex_rp;
         p2p_rp = _p2p_rp;
         no_of_artbits = _artbits;
    }

    function addWhiteListAddresses(address _whiteListAddr) external onlyAdmin {
         whiteList_count++;
         whiteList_addresses[_whiteListAddr] = true;
    }

    function removeWhiteListAddresses(address _whiteListAddr) external onlyAdmin {
         whiteList_count--;
         whiteList_addresses[_whiteListAddr] = false;
    }

    function addDexAddresses(address _dexAddr) external onlyAdmin {
         dexList_count++;
         dex_addresses[_dexAddr] = true;
    }

    function removeDexAddresses(address _dexAddr) external onlyAdmin {
         dexList_count--;
         dex_addresses[_dexAddr] = false;
    }

    function getTotCommissionAmt() external view returns (uint256){
         return totalCommission;
    }

     function getTotRoyaltyAmt() external view returns (uint256){
         return totalRoyalty;
    }

     function isWhitelisted(address _whitelistedAddr) external view returns (bool){
           return whiteList_addresses[_whitelistedAddr];
    }

    function isDexAddress(address _dexAddress) external view returns (bool){
         return dex_addresses[_dexAddress];
    } 

    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        transferArtbits(_msgSender(), recipient, amount);
        return true;
    }

     function transferArtbits(address sender, address recipient,uint256 amount) internal virtual {                          
        uint256 transferAmount;
        // whitelist transaction no commission fee
        if((whiteList_addresses[sender] == true) || (whiteList_addresses[recipient] == true) 
                || recipient == gaxAdmin || sender == gaxAdmin){
             _transfer(_msgSender(), recipient, amount);
        }

        // DEX transaction NFT_Asset owner receive royalty
        else if ((dex_addresses[sender] == true) || (dex_addresses[recipient] == true)){
            uint256 royalty_amt = (dex_rp.mul(amount)).div(100); // admin royalty amt
            _transfer(_msgSender(), nftOwner, royalty_amt); 
            totalRoyalty = totalRoyalty.add(royalty_amt); // add royalty amount to NFT_Asset owner wallet

            transferAmount = amount.sub(royalty_amt); // AB sent to client            
            _transfer(_msgSender(), recipient, transferAmount);                  
        }  
          
        else {          
            uint256 commission = (p2p_rp.mul(amount)).div(100); // admin commission amt
            _transfer(_msgSender(), nftOwner, commission); 

            transferAmount = amount.sub(commission); // AB sent to client
            totalCommission = totalCommission.add(commission); // add commission amount to NFT_Asset owner wallet
            _transfer(_msgSender(), recipient, transferAmount);   
        } 

        emit Transfer(sender, recipient, amount);

        _afterTokenTransfer(sender, recipient, amount);  
    }
}

