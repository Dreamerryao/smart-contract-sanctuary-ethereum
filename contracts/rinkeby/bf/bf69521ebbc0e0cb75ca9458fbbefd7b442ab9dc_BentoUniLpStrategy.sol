interface IBentoAccessControls {
    function hasAdminRole(address _address) external view returns (bool);
    function hasSmartContractRole(address _address) external view returns (bool);
}

abstract contract BentoBaseStrategy is Pausable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    /// @notice AccessControls address
    IBentoAccessControls public accessControls;
    /// @notice Staking token address
    address public want;
    /// @notice Reward token address
    address public rewardToken;
    /// @notice Staked balances
    mapping(address => uint256) public balances;

    /// @dev Public Method for deposit token
    function deposit(address _sender) external payable virtual whenNotPaused {
        // TODO: Only Clash Contract
        require(_sender != address(0x0), "BentoBaseStrategy.deposit: Depositer is invalid.");

        _deposit(_sender);
    }

    /// @dev Public Method for withdraw all
    function withdrawAll(address payable _recipient) external whenNotPaused {
        // TODO: Only Clash Contract
        require(_recipient != address(0x0), "BentoBaseStrategy.withdrawAll: Recipient is invalid.");

        _withdrawAll(_recipient);
    }

    /// @dev Reset approval and approve exact amount
    function _approveHelper(
        address token,
        address recipient,
        uint256 amount
    ) internal {
        IERC20(token).safeApprove(recipient, 0);
        IERC20(token).safeApprove(recipient, amount);
    }

    /// @notice Get the balance of want held idle in the Strategy
    function balanceOfWant() public view returns (uint256) {
        return IERC20(want).balanceOf(address(this));
    }

    /// Abstract Functions
    /// @dev internal deposit method
    function _deposit(address _sender) internal virtual;

    /// @dev internal withdraw all method
    function _withdrawAll(address payable _recipient) internal virtual;

    /// @dev internal withdraw method
    //function _withdraw() internal virtual;

    /// @dev First token address
    function tokenA() public virtual view returns (address);

    /// @dev Second token address
    function tokenB() public virtual view returns (address);
}

contract BentoUniLpStrategy is BentoBaseStrategy, ReentrancyGuard {

    /// @notice First token address
    address public token0;
    /// @notice Second token address
    address public token1;
    /// @notice Uniswap router
    UniswapV2Router02 public uniswapRouter;

    uint256 public periodFinish = 0;
    uint256 public rewardRate = 0;
    uint256 public rewardsDuration = 365 days;
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;
    uint256 private _totalSupply;
    uint256 private _slippage = 950;

    mapping(address => uint256) public userRewardPerTokenPaid;
    mapping(address => uint256) public rewards;

    constructor(IBentoAccessControls _accessControls, UniswapV2Router02 _uniswapRouter, address _stakingToken, address _rewardToken, address _token0, address _token1) public {
        accessControls = _accessControls;
        uniswapRouter = _uniswapRouter;
        want = _stakingToken;
        rewardToken = _rewardToken;
        token0 = _token0;
        token1 = _token1;
    }

    /// Internal core implementations
    /// @dev deposit method
    function _deposit(address _sender) internal override updateReward(_sender) {
        address token = token0;
        if (token == uniswapRouter.WETH()) {
            token = token1;
        }

        // Zap ETH into LP token
        uint256 amountIn = msg.value.div(2);
        (uint reserveA, uint reserveB) = UniswapV2Library.getReserves(uniswapRouter.factory(), uniswapRouter.WETH(), token);
        uint256 amountOut = uniswapRouter.getAmountOut(amountIn, reserveA, reserveB);
        address[] memory path = new address[](2);
        path[0] = uniswapRouter.WETH();
        path[1] = token;
        uniswapRouter.swapExactETHForTokens{value:amountIn}(amountOut.mul(_slippage).div(1000), path, address(this), uint(~0));
        (uint amountToken, uint amountETH, uint liquidity) = uniswapRouter.addLiquidityETH(token, amountOut, uint(0), uint(0), address(this), uint(~0));

        _totalSupply = _totalSupply.add(liquidity);
        balances[_sender] = balances[_sender].add(liquidity);
    }

    function _harvest(address _user) internal returns (uint256) {
        uint256 reward = rewards[_user];
        if (reward > 0) {
            rewards[_user] = 0;
            IERC20(rewardToken).safeTransfer(_user, reward);
        }
        return reward;
    }

    function _withdrawAll(address payable _user) internal override updateReward(_user) {
        uint256 amount = balances[_user];
        _withdraw(_user, amount);
        _harvest(_user);
    }

    function _withdraw(address payable _user, uint256 _amount) internal {
        _totalSupply = _totalSupply.sub(_amount);
        balances[_user] = balances[msg.sender].sub(_amount);
        // Zap LP token to ETH
        address token = token0;
        if (token == uniswapRouter.WETH()) {
            token = token1;
        }
        (uint amountToken, uint amountETH) = uniswapRouter.removeLiquidityETH(token, _amount, uint(0), uint(0), address(this), uint256(~0));
        (uint reserveA, uint reserveB) = UniswapV2Library.getReserves(uniswapRouter.factory(), token, uniswapRouter.WETH());
        uint256 amountOut = uniswapRouter.getAmountOut(amountToken, reserveA, reserveB);
        address[] memory path = new address[](2);
        path[0] = token;
        path[1] = uniswapRouter.WETH();
        uniswapRouter.swapExactTokensForETH(amountToken, amountOut.mul(_slippage).div(1000), path, address(this), uint256(~0));
        _user.transfer(address(this).balance); // TODO: Double check
    }

    function harvest(address _user) external nonReentrant updateReward(_user) returns (uint256) {
        uint256 reward = _harvest(_user);
        return reward;
    }

    function withdraw(address payable _user, uint256 _amount) external nonReentrant updateReward(_user) {
        require(_amount > 0, "Cannot withdraw 0");
        _withdraw(_user, _amount);
        _harvest(_user);
    }

    /* ========== VIEWS ========== */

    function lastTimeRewardApplicable() public view returns (uint256) {
        return Math.min(block.timestamp, periodFinish);
    }

    function tokenA() public override view returns (address) {
        return token0;
    }

    function tokenB() public override view returns (address) {
        return token1;
    }

    function rewardPerToken() public view returns (uint256) {
        if (_totalSupply == 0) {
            return rewardPerTokenStored;
        }
        return
            rewardPerTokenStored.add(
                lastTimeRewardApplicable().sub(lastUpdateTime).mul(rewardRate).mul(1e18).div(_totalSupply)
            );
    }

    function earned(address account) public view returns (uint256) {
        return balances[account].mul(rewardPerToken().sub(userRewardPerTokenPaid[account])).div(1e18).add(rewards[account]);
    }

    function getRewardForDuration() external view returns (uint256) {
        return rewardRate.mul(rewardsDuration);
    }

    /* ========== ADMIN =========== */

    function notifyRewardAmount(uint256 reward) external updateReward(address(0)) {
        require(
            accessControls.hasAdminRole(msg.sender) || accessControls.hasSmartContractRole(msg.sender),
            "StakingRewards.notifyRewardAmount: Sender must be admin role or smart contract role");
        
        if (block.timestamp >= periodFinish) {
            rewardRate = reward.div(rewardsDuration);
        } else {
            uint256 remaining = periodFinish.sub(block.timestamp);
            uint256 leftover = remaining.mul(rewardRate);
            rewardRate = reward.add(leftover).div(rewardsDuration);
        }

        // Ensure the provided reward amount is not more than the balance in the contract.
        // This keeps the reward rate in the right range, preventing overflows due to
        // very high values of rewardRate in the earned and rewardsPerToken functions;
        // Reward + leftover must be less than 2^256 / 10^18 to avoid overflow.
        uint balance = IERC20(rewardToken).balanceOf(address(this));
        require(rewardRate <= balance.div(rewardsDuration), "Provided reward too high");

        lastUpdateTime = block.timestamp;
        periodFinish = block.timestamp.add(rewardsDuration);
    }

    // End rewards emission earlier
    function updatePeriodFinish(uint timestamp) external updateReward(address(0)) {
        require(accessControls.hasAdminRole(msg.sender), "StakingRewards.updatePeriodFinish: Sender must be admin role");

        periodFinish = timestamp;
    }

    // Added to support recovering LP Rewards from other systems such as BAL to be distributed to holders
    function recoverERC20(address tokenAddress, uint256 tokenAmount) external {
        require(accessControls.hasAdminRole(msg.sender), "StakingRewards.recoverERC20: Sender must be admin role");

        require(tokenAddress != want, "Cannot withdraw the staking token");
        IERC20(tokenAddress).safeTransfer(msg.sender, tokenAmount);
    }

    function setRewardsDuration(uint256 _rewardsDuration) external {
        require(accessControls.hasAdminRole(msg.sender), "StakingRewards.setRewardsDuration: Sender must be admin role");

        require(
            block.timestamp > periodFinish,
            "Previous rewards period must be complete before changing the duration for the new period"
        );
        rewardsDuration = _rewardsDuration;
    }

    /* ========== MODIFIERS ========== */

    modifier updateReward(address account) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = earned(account);
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }
}