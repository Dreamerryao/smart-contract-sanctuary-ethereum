// SPDX-License-Identifier: MIT
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./interfaces/AuctionExchange.sol";
import "./interfaces/ICollectionRoyalty.sol";
import "./interfaces/IERC1155Tradable.sol";
import "hardhat/console.sol";

contract Marketplace is AuctionExchange, Initializable {
    using SafeMath for uint256;

    address public marketPayee;
    uint256 public marketPercent;

    function initialize(address _payee, uint256 _percent) public initializer {
        marketPercent = _percent;
        marketPayee = _payee;
    }

    function setMarketPayee(address _payee) public {
        require(msg.sender == marketPayee, "only market payee");
        marketPayee = _payee;
    }

    function setMarketPercent(uint256 _percent) public {
        require(msg.sender == marketPayee, "only market payee");
        marketPercent = _percent;
    }

    function payout(
        address _collection,
        uint256 _id,
        address payable _seller,
        uint256 _value
    ) internal override {
        uint256 balance = _value;

        if (isRoyalCollection(_collection)) {
            ICollectionRoyalty collection = ICollectionRoyalty(_collection);
            address payable creator = payable(collection.getCreator(_id));
            uint256 royalty = collection.getRoyalty(_id);
            uint256 decimal = collection.getDecimal();
            uint256 creatorRevenue = _value.mul(royalty).div(100**decimal);
            payable(creator).transfer(creatorRevenue);
            balance = balance.sub(creatorRevenue);
        }

        uint256 marketRevenue = _value.mul(marketPercent).div(100**2);
        payable(marketPayee).transfer(marketRevenue);
        balance = balance.sub(marketRevenue);

        _seller.transfer(balance);
    }

    function isRoyalCollection(address _collection) public view returns (bool) {
        return
            IERC165(_collection).supportsInterface(
                type(ICollectionRoyalty).interfaceId
            );
    }
}

