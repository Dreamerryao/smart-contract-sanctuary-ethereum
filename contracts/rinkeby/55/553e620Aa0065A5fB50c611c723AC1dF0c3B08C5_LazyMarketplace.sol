pragma solidity ^0.8.0;
// SPDX-License-Identifier: MIT
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "./Exchange.sol";

abstract contract AuctionExchange is Exchange {
    using SafeMath for uint256;

    struct Auction {
        uint256 amount;
        uint256 maxBid;
        address maxBidder;
        uint256 endTime;
        bool ended;
    }

    // token address => token ID => seller => value
    mapping(address => mapping(uint256 => mapping(address => Auction)))
        public auctionItems;

    mapping(address => uint256) public pendingWithdraws;

    event BidAdded(
        address _address,
        uint256 _id,
        address _seller,
        address _bidder,
        uint256 _value
    );
    event Withdrawn(address _bidder, uint256 _value);
    event AuctionEnded(
        address _address,
        uint256 _id,
        address _seller,
        address _maxBidder,
        uint256 _maxBid,
        uint256 _amount,
        bool _success
    );

    function putOnAuction(
        address _address,
        uint256 _id,
        uint256 _amount,
        uint256 _minPrice,
        uint256 _endTime
    )
        external
        HasEnoughToken(_address, _id, _amount)
        HasTransferApproval(_address, _id, msg.sender)
    {
        require(
            fixedItems[_address][_id][msg.sender].amount == 0,
            "in fixed sale"
        );
        saleType[_address][_id][msg.sender] = SaleType.AUCTION;

        auctionItems[_address][_id][msg.sender] = Auction(
            _amount,
            _minPrice.sub(1),
            address(0),
            _endTime,
            false
        );
        emit ItemUpdated(
            _address,
            _id,
            msg.sender,
            _amount,
            _minPrice,
            SaleType.AUCTION,
            _endTime
        );
    }

    function putOnSale(
        address _address,
        uint256 _id,
        uint256 _amount,
        uint256 _price
    )
        public
        override
        HasEnoughToken(_address, _id, _amount)
        HasTransferApproval(_address, _id, msg.sender)
    {
        bool isInAuction = (auctionItems[_address][_id][msg.sender].ended !=
            true) && (auctionItems[_address][_id][msg.sender].amount > 0);
        require(isInAuction == false, "in auction sale");
        super.putOnSale(_address, _id, _amount, _price);
    }

    function bid(
        address _address,
        uint256 _id,
        address _seller
    ) external payable {
        require(
            saleType[_address][_id][_seller] == SaleType.AUCTION,
            "not in auction"
        );
        require(
            auctionItems[_address][_id][_seller].ended != true,
            "auction ended"
        );
        require(
            block.timestamp <= auctionItems[_address][_id][_seller].endTime,
            "auction expired"
        );

        require(
            auctionItems[_address][_id][_seller].maxBid < msg.value,
            "already is a higher bid"
        );

        pendingWithdraws[
            auctionItems[_address][_id][_seller].maxBidder
        ] = pendingWithdraws[auctionItems[_address][_id][_seller].maxBidder]
            .add(auctionItems[_address][_id][_seller].maxBid);

        auctionItems[_address][_id][_seller].maxBid = msg.value;
        auctionItems[_address][_id][_seller].maxBidder = msg.sender;

        emit BidAdded(_address, _id, _seller, msg.sender, msg.value);
    }

    function endAuction(
        address _address,
        uint256 _id,
        address _seller
    ) external {
        IERC1155 tokenContract = IERC1155(_address);
        require(
            saleType[_address][_id][_seller] == SaleType.AUCTION,
            "not in auction"
        );
        require(
            auctionItems[_address][_id][_seller].ended != true,
            "auction ended"
        );
        require(
            tokenContract.balanceOf(_seller, _id) >=
                auctionItems[_address][_id][_seller].amount,
            "seller not enough token"
        );

        tokenContract.safeTransferFrom(
            _seller,
            auctionItems[_address][_id][_seller].maxBidder,
            _id,
            auctionItems[_address][_id][_seller].amount,
            ""
        );
        payout(
            _address,
            _id,
            payable(_seller),
            auctionItems[_address][_id][_seller].maxBid
        );
        auctionItems[_address][_id][_seller].ended = true;
        emit AuctionEnded(
            _address,
            _id,
            _seller,
            auctionItems[_address][_id][_seller].maxBidder,
            auctionItems[_address][_id][_seller].maxBid,
            auctionItems[_address][_id][_seller].amount,
            true
        );
    }

    function withdraw() external {
        uint256 amount = pendingWithdraws[msg.sender];
        pendingWithdraws[msg.sender] = 0;
        payable(msg.sender).transfer(amount);
        emit Withdrawn(msg.sender, pendingWithdraws[msg.sender]);
    }

    function takeOffAuction(address _address, uint256 _id) public {
        require(
            auctionItems[_address][_id][msg.sender].ended == false,
            "not on auction"
        );
        auctionItems[_address][_id][msg.sender].ended = true;
        pendingWithdraws[
            auctionItems[_address][_id][msg.sender].maxBidder
        ] = pendingWithdraws[auctionItems[_address][_id][msg.sender].maxBidder]
            .add(auctionItems[_address][_id][msg.sender].maxBid);
        emit AuctionEnded(_address, _id, msg.sender, address(0), 0, 0, false);
    }
}

