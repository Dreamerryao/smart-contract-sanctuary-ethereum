pragma solidity ^0.8.7;


import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "./Betterticket.sol";
import "./BetterticketEvent.sol";
import "./BetterticketController.sol";

contract BetterticketDeployer is ERC721URIStorage, BetterticketEvent {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    enum TICKETS {
        GENERAL,
        VIP
    }

    address controller;
    mapping(address => bool) creators;

    Betterticket[] private _tickets;

    //submissionPrice represents how much it costs to deploy a new ticket, while ticketCost represents how much it costs per ticket.
    uint256 public submissionPrice = 0.01 ether;
    uint256 public ticketCost = 0.001 ether;

    mapping(address => mapping(TICKETS => uint256[])) nftIds;

    constructor(address _owner) ERC721("", "") {
        //Setup access role
        owner = _owner;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    function setBetterticketController(address _controller) public onlyOwner {
        controller = _controller;
    }

    

    function createTicket(
        uint256 _ticketPrice,
        uint256 _totalSaleAmount,
        string[] memory _eventNFTDetails,
        string[] memory _otherInfoURIs,
        uint256 _sellCooDown

    ) public payable {
        require(
            msg.value >= getTicketFee(1, _eventNFTDetails.length + _otherInfoURIs.length)
        );

        mintMultipleTickets(_eventNFTDetails, _otherInfoURIs);

        //Create Ticket
        _tickets.push(
            new Betterticket(
                _ticketPrice,
                _totalSaleAmount,
                _otherInfoURIs.length,
                _eventNFTDetails.length,
                msg.sender,
                owner,
                controller
            )
        );

        creators[msg.sender] = true;

        BetterticketController(controller).addToDistributors(
            _sellCooDown
        );
    }

    function setsubmissionPrice(uint256 _submissionPrice) public onlyOwner {
        submissionPrice = _submissionPrice;
    }

    function setNFT(uint256 _ticketCost) public onlyOwner {
        ticketCost = _ticketCost;
    }

    function getTicketFee(uint256 _contractsToDeploy, uint256 _nftsToDeploy)
        public
        view
        returns (uint256)
    {
        return submissionPrice * _contractsToDeploy + ticketCost * _nftsToDeploy;
    }

    function mintMultipleTickets(
        string[] memory _eventNFTDetails,
        string[] memory _otherInfoURIs
    ) internal returns (uint256[] memory, uint256[] memory) {
        for (uint256 i = 0; i < _eventNFTDetails.length; i++) {
            uint256 id = mintEventTicket(_eventNFTDetails[i]);
            nftIds[address(this)][TICKETS.VIP].push(id);
        }

        for (uint256 i = 0; i < _otherInfoURIs.length; i++) {
            uint256 id = mintEventTicket(_otherInfoURIs[i]);
            nftIds[address(this)][TICKETS.GENERAL].push(id);
        }

        return (
            nftIds[address(this)][TICKETS.GENERAL],
            nftIds[address(this)][TICKETS.VIP]
        );
    }

    function mintEventTicket(string memory _nftURI) internal returns (uint256) {
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        _safeMint(address(this), newItemId);
        _setTokenURI(newItemId, _nftURI);
        return newItemId;
    }


    function transferToWinners(
        address[] memory _GENERALWinners,
        address[] memory _topWinners
    ) public {
        //add check if creator
        require(
            msg.sender == owner ||
                msg.sender == controller ||
                creators[msg.sender]
        );

        for (uint256 i = 0; i < _GENERALWinners.length; i++) {
            safeTransferFrom(
                address(this),
                _GENERALWinners[i],
                nftIds[msg.sender][TICKETS.GENERAL][i]
            );
        }

        for (uint256 i = 0; i < _topWinners.length; i++) {
            safeTransferFrom(
                address(this),
                _topWinners[i],
                nftIds[msg.sender][TICKETS.VIP][i]
            );
        }
    }
}

