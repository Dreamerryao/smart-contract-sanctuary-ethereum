import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
pragma solidity ^0.8.7;

contract BetterticketEvent {
    uint256 public counter;
    uint16 public platformCut = 2;
    address owner;

    struct Event {
        uint16 totalOfAttendees;
        uint16 remainingTickets; 
        address organizer; 
        uint256 createTime;
        uint256 initialPrice;
        uint256 dueTime;
        uint256 attendees;
        uint16 organizerCut;
        bool notSentBack; 
        mapping (address => bool) participants; 
        address[] attendants;
        mapping (address => bool) checkedIn;
        string metadata;
        address[] contractAddresses;
        mapping (string => address) userName2Address;
    }

    event EventCreated (
        uint16 totalOfAttendees,
        uint16 remainingTickets, 
        address organizer, 
        uint256 createTime,
        uint256 initialPrice,
        uint256 dueTime,
        uint256 attendees,
        uint16 organizerCut,
        bool notSentBack, 
        address[] attendants,
        string metadata,
        address[] contractAddresses
        
    );

    event CreateEvent(
        uint256 eventId,
        uint exprTime
    );
    event SignUp(
        uint256 eventId,
        address participant
    );
    event CheckIn(
        uint256 eventId,
        address attendant
    );
    event Release(uint256 eventId);

    mapping (uint256 => Event) public allEvents;
    mapping (address => uint256) public eventSignUp;
    mapping (address => mapping(uint256 => uint256)) public eventIDsSignUp;
    mapping (address => uint256) public eventShowList;
    mapping (address => mapping(uint256 => uint256)) public eventIDsShowUp;
    mapping (address => uint256) public eventCreated;
    mapping (address => mapping(uint256 => uint256)) public eventIDs;

    constructor() {
        owner = msg.sender;
        counter = 0;
    }

    function createEvent(uint256 initialPrice, uint256 dueTime, uint16 totalOfAttendees, uint16 organizerCut, string calldata metadata, address[] calldata contractAddresses) external returns (uint256) {
        require(totalOfAttendees > 0, "Invalid count");
        require(initialPrice >= 0, "Invalid initialPrice");
        require(dueTime > 0, "Invalid Due Time");
        require(organizerCut >= 0 && organizerCut < 100, "Invalid Organizer Cut Percentage");

        uint256 eventId = counter;
        Event storage newEvent = allEvents[eventId];
        newEvent.totalOfAttendees = totalOfAttendees;
        newEvent.remainingTickets = totalOfAttendees;
        newEvent.organizer = msg.sender;
        newEvent.createTime = block.timestamp;
        newEvent.initialPrice = initialPrice * 1e15; // 1 ether = 1000000000000000000 wei
        newEvent.dueTime = dueTime;
        newEvent.attendees = 0;
        newEvent.notSentBack = true;
        newEvent.attendants = new address[](totalOfAttendees);
        newEvent.metadata = metadata;
        newEvent.organizerCut = organizerCut;
        newEvent.contractAddresses = contractAddresses;
        counter = counter + 1;

        eventIDs[msg.sender][eventCreated[msg.sender]] = eventId;
        eventCreated[msg.sender] += 1;
    
        emit CreateEvent(
            eventId, 
            newEvent.createTime+(dueTime*1 days)
        );
        return eventId;
    }


    function checkUserWhoSignup(uint256 eventId, address user) external view returns (bool) {
        return allEvents[eventId].participants[user];
    }


    function showAllAttendees(uint256 eventId) external view returns (address[] memory) {
        return allEvents[eventId].attendants;
    }

    function signUp(uint256 eventId, string calldata userName) external payable returns (bool) {
        require(allEvents[eventId].organizer != address(0), "Invalid ID");
	    require(allEvents[eventId].remainingTickets > 0, "No Space left");
	    require(block.timestamp < allEvents[eventId].createTime+(allEvents[eventId].dueTime*1 days), "Due Date Passed");
        require(allEvents[eventId].organizer != msg.sender, "organizer Cannot Signup");
        require(allEvents[eventId].initialPrice == msg.value, "ETH Amount Not Correct");
        require(!allEvents[eventId].participants[msg.sender], "User Already Signed Up");
        require(bytes(userName).length > 0, "invalid user name");
        require(allEvents[eventId].userName2Address[userName] == address(0), "user name used");
        require(allEvents[eventId].notSentBack, "Event Fund Already Released");
        if(allEvents[eventId].contractAddresses.length > 0){   
            for(uint i = 0; i < allEvents[eventId].contractAddresses.length; i++){
                IERC721 token = IERC721(allEvents[eventId].contractAddresses[i]);
                uint count = token.balanceOf(msg.sender);
                require(count > 0, "User NFT holdings doen't meet the requirements");
            }
        }

        eventIDsSignUp[msg.sender][eventSignUp[msg.sender]] = eventId;
        eventSignUp[msg.sender] += 1;
        allEvents[eventId].remainingTickets -= 1;
        allEvents[eventId].userName2Address[userName] = msg.sender;
        allEvents[eventId].participants[msg.sender] = true;
        emit SignUp(eventId, msg.sender);
	    return true;
    }

    function checkIn(uint256 eventId, address attendant) external returns (bool) {
        require(allEvents[eventId].organizer != address(0), "Invalid ID");
        require(allEvents[eventId].organizer == msg.sender, "Not Event organizer");
        require(allEvents[eventId].participants[attendant], "Not Such Participant");
        require(!allEvents[eventId].checkedIn[attendant], "Already checked in");
        require(allEvents[eventId].notSentBack, "Event Fund Already Released");
        
        allEvents[eventId].attendants[allEvents[eventId].attendees] = attendant;
        allEvents[eventId].attendees += 1;
        eventIDsShowUp[attendant][eventShowList[attendant]] = eventId;
        eventShowList[attendant] += 1;
        allEvents[eventId].checkedIn[attendant] = true;
        emit CheckIn(eventId, attendant);
        return true;
    }

    function releaseDeposit(uint256 eventId) external payable returns (uint256){
        require(allEvents[eventId].organizer != address(0), "Invalid ID");
        require(allEvents[eventId].organizer == msg.sender, "Not Event organizer");
        require(allEvents[eventId].notSentBack, "Event Fund Already Released");
        allEvents[eventId].notSentBack = false;

        uint256 noShowAmount = allEvents[eventId].initialPrice * (allEvents[eventId].totalOfAttendees-allEvents[eventId].remainingTickets-allEvents[eventId].attendees);
        uint256 platformAmount = noShowAmount / 100 * platformCut;
        uint256 organizerAmount = noShowAmount / 100 * allEvents[eventId].organizerCut;
        uint256 attendantsAmount = noShowAmount - platformAmount - organizerAmount;

        uint256 eachAttendantAmount = attendantsAmount / allEvents[eventId].attendees + allEvents[eventId].initialPrice;
        for(uint i = 0; i < allEvents[eventId].attendees; i++){
            address attendant = allEvents[eventId].attendants[i];
            require(payable(attendant).send(eachAttendantAmount), "Transfer ETH to attendent failed");
        }
        if(organizerAmount != 0){
            require(payable(allEvents[eventId].organizer).send(organizerAmount), "Transfer ETH to organizer failed");
        }
        if(platformAmount != 0){
            require(payable(owner).send(platformAmount), "Transfer ETH to platform failed");
        }
        emit Release(eventId);
        return platformAmount;
    }

    function showUserlistOfSignUp(address user) external view returns (uint256[] memory) {
        uint256[] memory result = new uint256[](eventSignUp[user]);
        for(uint i = 0; i < eventSignUp[user]; i++){
            result[i] = eventIDsSignUp[user][i];
        }
        return result;
    }

    function showUserEventList(address user) external view returns (uint256[] memory) {
        uint256[] memory result = new uint256[](eventShowList[user]);
        for(uint i = 0; i < eventShowList[user]; i++){
            result[i] = eventIDsShowUp[user][i];
        }
        return result;
    }

    function showUserEvent(address user) external view returns (uint256[] memory) {
        uint256[] memory result = new uint256[](eventCreated[user]);
        for(uint i = 0; i < eventCreated[user]; i++){
            result[i] = eventIDs[user][i];
        }
        return result;
    }


}

