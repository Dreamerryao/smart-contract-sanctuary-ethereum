pragma solidity ^0.8.7;

import "./Betterticket.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract BetterticketController is KeeperCompatibleInterface, AccessControl {
    address[] public tickets;
    mapping(address => uint256) public ticketsDelay;
    mapping(address => uint256) public ticketsStart;

    bytes32 public constant CREATOR_ROLE = keccak256("CREATOR_ROLE");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    constructor(address _owner) {
        //Setup access role
        _setupRole(OWNER_ROLE, _owner);
        _setupRole(CREATOR_ROLE, _owner);
        _setRoleAdmin(CREATOR_ROLE, OWNER_ROLE);
    }

    function addToOrganizers() public {
        require(
            hasRole(OWNER_ROLE, msg.sender),
            "caller is not authorized to add organizers"
        );

        grantRole(CREATOR_ROLE, msg.sender);
    }

    function addToDistributors(uint256 _timeInMinutes) public {
        require(
            hasRole(OWNER_ROLE, msg.sender) ||
                hasRole(CREATOR_ROLE, msg.sender),
            "caller is not authorized to add to distributors"
        );

        tickets.push(msg.sender);
        ticketsDelay[msg.sender] = _timeInMinutes;
        ticketsStart[msg.sender] = block.timestamp;
    }

    function checkUpkeep(
        bytes calldata /* checkData */
    )
        external
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /* performData */
        )
    {
        upkeepNeeded = false;

        for (uint256 i = 0; i < tickets.length; i++) {
            if (
                (block.timestamp - ticketsStart[tickets[i]]) >=
                ticketsDelay[tickets[i]]
            ) {
                //rewards not distributed
                if (!Betterticket(tickets[i]).rewardsGiven()) {
                    //rewards should be distributed
                    if (
                        tickets[i].balance >=
                        Betterticket(tickets[i]).totalSaleAmount()
                    ) {
                        upkeepNeeded = true;
                        break;
                    }
                }
            }
        }
    }

    function performUpkeep(
        bytes calldata /* performData */
    ) external override {

        for (uint256 i = 0; i < tickets.length; i++) {
            if (
                (block.timestamp - ticketsStart[tickets[i]]) >=
                ticketsDelay[tickets[i]]
            ) {
                if (!Betterticket(tickets[i]).rewardsGiven()) {
                   
                    if (
                        tickets[i].balance >=
                        Betterticket(tickets[i]).totalSaleAmount()
                    ) {
                        // perform upkeep
                        Betterticket(tickets[i]).selectWinner();

                        //remove from tracking
                        ticketsStart[tickets[i]] = 0;
                        ticketsDelay[tickets[i]] = 0;
                        burn(tickets, i);
                    }
                }
            }
        }
    }

    function burn(address[] storage array, uint256 index) internal {
        require(index < array.length);
        array[index] = array[array.length - 1];
        array.pop();
    }
}

