pragma solidity ^0.8.7;

import "./BetterticketDeployer.sol";

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

contract Betterticket is VRFConsumerBaseV2 {
    //Betterticket Data
    uint256 public counter;
    uint16 public platformCut = 10;
    string public ipfsStorage;
    uint256 ticketPrice;
    uint256 public totalSaleAmount;
    uint256 otherTickets;
    uint256 largerTickets;
    bool public rewardsGiven = false;

    //Roles
    address owner;
    address creator;
    address controller;

    //Raffle / Donation Data
    address[] public buyers;
    mapping(address => uint256) public entries;
    address[] public randomWinners;
    address[] public biggestWinners;

    //Chainlink VRF
    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId = 4776;
    address vrfCoordinator = 0x6168499c0cFfCaCD319c818142124B7A15E857ab;
    bytes32 keyHash =
        0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;
    uint32 callbackGasLimit = 100000;
    uint16 requestConfirmations = 3;
    uint256[] public randomWords;
    uint256 public requestId;

    //Price feeds
    AggregatorV3Interface internal priceFeed;
    address ethToUsdRinkeby = 0x8A753747A1Fa494EC906cE90E9f37563A8AF630e;


    constructor(
        uint256 _ticketPrice,
        uint256 _totalSaleAmount,
        uint256 _otherTickets,
        uint256 _largerTickets,
        address _eventOrganizer,
        address _owner,
        address _controller

    ) VRFConsumerBaseV2(vrfCoordinator) {
        //Set Data
        ticketPrice = _ticketPrice;
        totalSaleAmount = _totalSaleAmount;
        otherTickets = _otherTickets;
        largerTickets = _largerTickets;

        //Attach VRF Coodinator
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);

        //Price feeds
        priceFeed = AggregatorV3Interface(ethToUsdRinkeby);

        //Setup access roles
        // _setupRole(CREATOR_ROLE, _eventOrganizer);
        // _setupRole(OWNER_ROLE, _owner);
        owner = _owner;
        creator = _eventOrganizer;
        controller = _controller;
        counter = 0;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    modifier onlyCreator() {
        require(msg.sender == creator);
        _;
    }

    modifier onlyController() {
        require(msg.sender == controller);
        _;
    }

    event CreateEvent(uint256 eventId, uint exprTime);
    
    event SignUp(uint256 eventId, address participant);

    event CheckIn(uint256 eventId, address attendant);

    event Release(uint256 eventId);

    function smallerTickets() public view returns (uint256) {
        (, int256 latestPrice, , , ) = priceFeed.latestRoundData(); //decimal of 8
        uint256 newPrice = uint256(latestPrice) * 10**10;
        return (ticketPrice * 10**18) / newPrice;
    }

    function buyTicket() public payable {
        require(msg.value >= smallerTickets());

        //add to buyers if nothing was sent yet
        if (entries[msg.sender] == 0) {
            buyers.push(msg.sender);
        }
        //track tickets amounts
        entries[msg.sender] += msg.value;
    }

    function refundbuyers() public onlyOwner {
        for (uint256 i = 0; i < buyers.length; i++) {
            payable(buyers[i]).transfer(entries[buyers[i]]);
        }
        selfdestruct(payable(msg.sender));
    }

    function releaseFunds(uint256 _amount) public onlyCreator {
        require(address(this).balance >= totalSaleAmount);
        require(_amount <= address(this).balance);

        payable(msg.sender).transfer(_amount);
    }

    function selectWinner() public onlyController {
        require(address(this).balance >= totalSaleAmount);
        require(!rewardsGiven);

        //make sure it works and account for failure
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            uint32(otherTickets)
        );

        rewardsGiven = true;
    }

    function burnAddress(address[] storage array, uint256 index) internal {
        require(index < array.length);
        array[index] = array[array.length - 1];
        array.pop();
    }

    function fulfillRandomWords(
        uint256, /* requestId */
        uint256[] memory _randomWords
    ) internal override {
        randomWords = _randomWords;
        address[] storage allbuyers = buyers;

        //get tickets winners
        for (uint256 i = 0; i < largerTickets; i++) {
            uint256 largest = 0;
            address largerTicketBuyer;
            uint256 largestJ;

            for (uint256 j = 0; j < allbuyers.length; j++) {
                if (entries[allbuyers[j]] > largest) {
                    largest = entries[allbuyers[j]];
                    largerTicketBuyer = allbuyers[j];
                    largestJ = j;
                }
            }

            biggestWinners.push(payable(largerTicketBuyer));
            burnAddress(allbuyers, largestJ);
        }

        //find random winner for each tickets
        for (uint256 i = 0; i < otherTickets; i++) {
            uint256 latestWinnerIndex = randomWords[i] % allbuyers.length;
            randomWinners.push(payable(allbuyers[latestWinnerIndex]));
            burnAddress(allbuyers, latestWinnerIndex);
        }

        //distribute nft to winners
        BetterticketDeployer(owner).transferToWinners(
            randomWinners,
            biggestWinners
        );
    }
}

