pragma solidity ^0.8.6;
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "hardhat/console.sol";
import "./LionWorldLawa.sol";

interface MultiSig {
    function checkUserAuthority(string memory level, address targetSender) external view returns (bool result);
    function getUserAddress(string memory name) external view returns (address);
}
abstract contract LionworldPropBase is ERC1155 {
  uint256 public totalSupply; // Total amount of tokens
  uint256 public totalWithdraw; // The total amount of liquidated damages that can be withdrawn
  address public platformAccount; // Platform account
  address public adminAccount; //Administrator account
  address public officialAccount; // Official account
  bool public _castAuth=true; // Casting permissions open
  uint256 public cliamRatio = 8000; // The proportion of liquidated damages 80 / 100 *10000
  uint256 public platformRatio = 250; // Platform handling fee charging ratio 2.5 /100 * 10000
  uint256 public delayTime = 300; // Over time auction 5minute
  uint256 constant _minPrice = 10000000000000000;
  MultiSig public verifyAddress; // Authorized contract
  LionWorldLawa public lawa; // Lawa contract
    // struct Nft
    struct Token {
        address owner; // owner
        uint256 ratio; // 分成比例
        uint256 amount; //数量
    }
    // struct bale
    struct Bale {
        uint256[] tokenIds; // nft的唯一ID(铸造时产生)
        uint256[] ratios; // nft的价格比例
        uint256[] amounts; // nft 数量
    }
    // struct trade
    struct TokenMode{
      uint256 mode; // 交易类型 1定价 2拍卖
      uint256 orderId; // 上架NFT id
      uint256 state; // 1selling/biding 2sellend/bidend 3action success 4auction failed
      uint256 cashRate; // 押金比例
      uint256 expirationTime; // 拍卖出价截至时间
      uint256 paymentTime; // 支付尾款截止时间
      uint256 price; // 初始价格(拍卖为起拍价)
      uint256 bidPrice; // 当前的出价金额
      uint256 payPrice;// 已支付的金额
      uint256 addPrice; // 每次加价金额
      address owner; // 上架人
      address to; // 出价人(默认0地址无人出价)
    }
    // tokenId => Token
    mapping(uint256=>Token) public tokens;
    // tokenId => bool
    mapping(uint256=>bool) public tokenKeys;
    // orderId => Bale
    mapping(uint256 => Bale) bales;
    // orderId => bool
    mapping(uint256 => bool) public orders;
    // orderId => TokenMode
    mapping(uint256 => TokenMode) public tokenModes;
    // address => tokenId => amount
    mapping(address => mapping(uint256 =>uint256)) public userLocked;
    // events
    event lwMintBatchLog(address indexed _from, uint256[] _tokenIds);
    event lwMintLog(address indexed _from, uint256 _tokenId, uint256 _ratio, uint256 _amount);
    event lwPreSellLog(address indexed _from, uint256 _orderId, uint256 _price);
    event lwUnSellLog(address indexed _from, uint256 _orderId);
    event lwTradeLog(address indexed _from, address indexed _to, uint256 _orderId, uint256 _price);
    event lwAuctionLog(address indexed _from, address indexed _to, uint256 _orderId, uint256 _minPrice, uint256 _addPrice, uint256 _cashRate,uint256 _expirationTime,uint256 _paymentTime);
    event lwBidLog(address indexed _from, uint256 _orderId, uint256 _price);
    event lwAuctionCancelLog(address indexed _from, uint256 _orderId);
    event lwPaymentLog(address indexed _from, address indexed _to, uint256 _orderId, uint256 _price,uint256 _fee);
    event lwAuctionResultLog(address indexed _from, uint256 _orderId, uint256 _result);
    event feeLog(uint256 fee);
    event lwTransferPropLog(address indexed _from, address indexed _to, uint256[] _orderIds, uint256[] _amounts);
    // modifier
    modifier onlyMulSig(string memory _user) {
        require(verifyAddress.checkUserAuthority(_user, msg.sender),"Only authority user can use this function");
        _;
    }
    modifier onlyAdmin() {
      require(msg.sender == adminAccount ,"invalid admin account");
      _;
    }
    modifier onlyOrder(uint256 _orderId){
      require(_orderId > 0, "invalid orderId");
      require(orders[_orderId], "orderId is not exist");
      _;
    }
    modifier castAuth(){
      if(!_castAuth){
          require(officialAccount == msg.sender, "invalid caster");
      }
      _;
    }
    modifier official(){
      require(officialAccount == msg.sender, "invalid official caster");
      _;
    }
    function setMulsig(address _address) public onlyMulSig("Owner") returns (bool){
        verifyAddress = MultiSig(_address);
        return true;
    }
    function setLawa(address _address) external onlyMulSig("Owner") returns(bool){
      lawa = LionWorldLawa(_address);
      return true;
    }
    function setURI(string memory newuri) external  onlyMulSig("Owner")  {
        _setURI(newuri);
    }
    function setCliamRatio(uint256 _cliamRatio) external onlyMulSig("Owner") returns(bool){
      require(_cliamRatio > 0, "invalid cliamRatio");
      cliamRatio = _cliamRatio;
      return true;
    }
    function setPlatformRatio(uint256 _platformRatio) external onlyMulSig("Owner") returns(bool){
      require(_platformRatio > 0, "invalid platformRatio");
      platformRatio = _platformRatio;
      return true;
    }
    function setDelayTime(uint256 _delayTime) external onlyMulSig("Owner") returns(bool){
      require(_delayTime > 0, "invalid delayTime");
      delayTime = delayTime;
      return true;
    }
    function setAdminAccount(address _adminAccount) external onlyMulSig("Owner") returns(bool){
      require(_adminAccount != address(0), "invalid admin account");
      adminAccount = _adminAccount;
      return true;
    }
    function setPlatformAccount(address _platformAccount) external onlyMulSig("Owner") returns(bool){
      require(_platformAccount != address(0), "invalid platform account");
      platformAccount = _platformAccount;
      return true;
    }
    function setOfficialAccount(address _officialAccount) external onlyMulSig("Owner") returns(bool){
      require(_officialAccount != address(0), "invalid official account");
      officialAccount = _officialAccount;
      return true;
    }
    function setCastAuth(bool _state) external onlyMulSig("Owner") returns(bool){
      _castAuth = _state;
      return true;
    }
    function getAddress(string memory _user) public view returns (address) {
      require( verifyAddress.getUserAddress(_user) != address(0),"zero address");
      return verifyAddress.getUserAddress(_user);
    }
    function onERC1155Received(address,address,uint256,uint256,bytes memory) public virtual returns (bytes4) {
        return this.onERC1155Received.selector;
    }
    function onERC1155BatchReceived(address,address,uint256[] calldata ,uint256[] calldata,bytes memory) public virtual returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }
    function getRatio(uint256[] memory _ratios) internal pure returns(uint256){
      uint256 _allRatio;
      for(uint256 i=0; i< _ratios.length; i++){
        _allRatio += _ratios[i];
      }
      return _allRatio;
    }
}
contract LionworldProp is LionworldPropBase {    
  constructor(address mulSigAddress)ERC1155("https://ipfs.lionworld.io/item/{id}.json") {
    verifyAddress = MultiSig(mulSigAddress);
  }
  // platform fee
  function _platformFee(uint256 _value)  private returns (uint256){
      require(_value > 0, "value is zero");
      require(platformAccount != address(0), "platform account is zero");
      uint256 _fee = (_value * platformRatio) / 10000;
      require(lawa.Ltransfer(address(this), platformAccount, _fee),"invalid lawa transfer");
      emit feeLog(_fee);
      return _fee;
  }
  // beneficiary fee
  function _beneficiaryFee(uint256 _orderId, uint256 _value, address _from) private returns (uint256) {
      require(_value > 0, "value is zero");
      uint256 _fees;
      Bale storage _bale = bales[_orderId];
      for (uint256 i = 0; i < _bale.tokenIds.length; i++) {
        userLocked[_from][_bale.tokenIds[i]] -= _bale.amounts[i];
        Token storage token = tokens[_bale.tokenIds[i]];
        // 分成=总价*参与分成物品的价格比例*物品创作者的分成比例
        uint256 _fee = (_value * _bale.ratios[i] * token.ratio)/100000000;
        _fees += _fee;
        require(lawa.Ltransfer(address(this), token.owner, _fee),"invalid lawa transfer");
        emit feeLog(_fee);
      }
      this.safeBatchTransferFrom(address(this),msg.sender,_bale.tokenIds,_bale.amounts,"");
      return _fees;
  }

  function _unBale(uint256 _orderId,address _to) private returns(bool){
    Bale storage _bale = bales[_orderId];
    for (uint256 i = 0; i < _bale.tokenIds.length; i++) {
      userLocked[_to][_bale.tokenIds[i]] -= _bale.amounts[i];
    }
    this.safeBatchTransferFrom(address(this),_to,_bale.tokenIds,_bale.amounts,"");
    return true;
  }
  function _makeBale(uint256 _orderId,Bale calldata _params) private returns(bool){
    uint256[] memory _tokenIds = _params.tokenIds;
    uint256[] memory _ratios = _params.ratios;
    uint256[] memory _amounts = _params.amounts;
    require(_tokenIds.length > 0 && _tokenIds.length < 21 && _tokenIds.length == _ratios.length && _tokenIds.length == _amounts.length, "params length mismatch");
    require(getRatio(_ratios) == 10000, "invalid ratios");
    for (uint256 i = 0; i < _tokenIds.length; i++) {
      require(balanceOf(msg.sender, _tokenIds[i]) >= _amounts[i], "token is low");
      userLocked[msg.sender][_tokenIds[i]] += _amounts[i];
    }
    setApprovalForAll(address(this),true);
    safeBatchTransferFrom(msg.sender, address(this),_tokenIds,_amounts,"");
    bales[_orderId] = _params;
    return true;
  }
  function lwMintBatch(uint256[] calldata _tokenIds,uint256[] calldata _ratios, uint256[] calldata _amounts) external official returns(bool){
    require(msg.sender != address(0), "mint to the zero address");
    require(_tokenIds.length > 0 && _tokenIds.length <= 50 && _tokenIds.length == _amounts.length && _tokenIds.length == _ratios.length, "params length mismatch");
    for(uint256 i=0; i<_tokenIds.length; i++){
      require(!tokenKeys[_tokenIds[i]],"token already minted");
      require(_amounts[i] > 0 && _amounts[i] <= 200,"invalid mint amount");
      tokenKeys[_tokenIds[i]] = true;
      tokens[_tokenIds[i]] = Token(msg.sender, _ratios[i], _amounts[i]);
    }
    totalSupply += _tokenIds.length;
    _mintBatch(msg.sender, _tokenIds, _amounts, "");
    emit lwMintBatchLog(msg.sender, _tokenIds);
    return true;
  }
  function lwMint(uint256 _tokenId,uint256 _ratio, uint256 _amount) external castAuth returns(bool){
      require(_tokenId > 0, "invalid tokenId");
      require(_ratio >= 0, "invalid ratio");
      require(_amount > 0 && _amount <= 200, "invalid mint amount");
      require(!tokenKeys[_tokenId], "token already minted");
      tokenKeys[_tokenId] = true;
      tokens[_tokenId] = Token(msg.sender, _ratio, _amount);
      totalSupply += 1;
      _mint(_msgSender(), _tokenId,_amount, "");
      emit lwMintLog(msg.sender, _tokenId, _ratio, _amount);
      return true;
  }
  function lwPreSell(uint256 _orderId, Bale calldata _params,  uint256 _price) external returns(bool){
      require(_orderId > 0, "invalid orderId");
      require(!orders[_orderId], "orderId is exist");
      require(_price >= _minPrice, "invalid price");
      orders[_orderId]= true;
      require(_makeBale(_orderId, _params), "invalid package");
      tokenModes[_orderId] = TokenMode({
        mode: 1,
        orderId:_orderId,
        state: 1,
        cashRate: 0,
        expirationTime: 0,
        paymentTime: 0,
        price: _price,
        bidPrice: 0,
        payPrice: 0,
        addPrice: 0,
        owner: msg.sender,
        to: address(0)
      });
      emit lwPreSellLog(msg.sender,_orderId, _price);
      return true;
  }
  function lwUnSell(uint256 _orderId) external onlyOrder(_orderId) returns(bool){
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.owner == msg.sender, "account is not owner");
    require(_mode.state == 1, "invalid trading state");
    require(_mode.mode == 1, "invalid trading mode");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    _mode.state=2;
    require(_unBale(_orderId,_mode.owner),"unsell failed");
    emit lwUnSellLog(msg.sender,_orderId);
    return true;
  }
  function lwTrade(uint256 _orderId) external onlyOrder(_orderId) returns(bool){
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.state == 1, "invalid trading state");
    require(_mode.mode == 1, "invalid trading mode");
    require(lawa.balanceOf(msg.sender) >= _mode.price, "balance is low");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    _mode.state=2;
    _mode.to=msg.sender;
    require(lawa.Ltransfer(msg.sender, address(this), _mode.price),"invalid lawa transfer");
    // platformFee
    uint256 restValue = _mode.price - _platformFee(_mode.price);
    // beneficiaryFee
    restValue = restValue - _beneficiaryFee(_orderId, _mode.price,_mode.owner);
    // The amount that the seller deserves it
    require(lawa.Ltransfer(address(this), _mode.owner, restValue), "invalid lawa transfer");
    emit lwTradeLog(_mode.owner, msg.sender, _orderId, _mode.price);
    return true;
  }
  function lwAuction(uint256 _orderId,  Bale calldata _params, uint256 _price,uint256 _addPrice,uint256 _cashRate,uint256 _expirationTime,uint256 _paymentTime) external returns(bool){
    require(_orderId > 0, "invalid orderId");
    require(!orders[_orderId], "orderId is exist");
    require(_price >= _minPrice, "invalid price");
    require(_addPrice >= _minPrice, "invalid addPrice");
    require(_cashRate >= 100, "invalid cashRate"); 
    require(_paymentTime > _expirationTime,"paymentTime gt expirationTime");
    require(_expirationTime > block.timestamp, "invalid expirationTime");
    require(_paymentTime > block.timestamp, "invalid paymentTime");
    orders[_orderId]= true;
    require(_makeBale(_orderId, _params), "invalid package");
    tokenModes[_orderId] = TokenMode({
      mode: 2,
      orderId:_orderId,
      state: 1,
      cashRate: _cashRate,
      expirationTime: _expirationTime,
      paymentTime: _paymentTime,
      price: _price,
      bidPrice: 0,
      payPrice: 0,
      addPrice: _addPrice,
      owner: msg.sender,
      to: address(0)
    });
    emit lwAuctionLog(msg.sender,address(0), _orderId,_minPrice,_addPrice,_cashRate, _expirationTime, _paymentTime);
    return true;
  }
  function lwBid(uint256 _orderId,uint256 _bidPrice) external onlyOrder(_orderId) returns(bool) {
    require(_bidPrice >= _minPrice, "invalid bidPrice");
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.state == 1, "invalid trading state");
    require(_mode.mode == 2, "invalid trading mode");
    require(block.timestamp <= _mode.expirationTime, "expirationTime end");
    require(_mode.orderId == _orderId, "invalid trading order");
    address prevAddr = _mode.to;
    uint256 prevBidPrice = _mode.bidPrice;
    uint256 prevPay = _mode.payPrice;
    uint256 cash = (_bidPrice * _mode.cashRate) / 10000; //deposit
    require(lawa.balanceOf(msg.sender) >= cash, "balance is low");
    if (_mode.to != address(0)) {
        require(_bidPrice >= prevBidPrice + _mode.addPrice,"current bidPrice lt bidPrice add addPrice");
        // return prev deposit
        require(lawa.Ltransfer(address(this),prevAddr, prevPay),"invalid lawa transfer");
    } else {
        require(_bidPrice >= _mode.price, "bidPrice lt minPrice");
    }
    _mode.to = msg.sender;
    _mode.bidPrice = _bidPrice;
    _mode.payPrice = cash;
    // pay deposit
    require(lawa.Ltransfer(msg.sender, address(this), cash),"invalid lawa transfer");
    //set new delay time
    if (block.timestamp >= _mode.expirationTime - delayTime) {
        _mode.expirationTime += delayTime;
        _mode.paymentTime += delayTime;
    }
    emit lwBidLog(msg.sender, _orderId, _bidPrice);
    return true;
  }
  function lwPayment(uint256 _orderId) external onlyOrder(_orderId) returns(bool){
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.state == 2, "invalid trading state");
    require(_mode.mode == 2, "invalid trading mode");
    uint256 _payPrice =  (_mode.bidPrice * (10000 - _mode.cashRate)) / 10000; //margin cash
    require(lawa.balanceOf(_msgSender()) >= _payPrice, "balance is low");
    require(block.timestamp > _mode.expirationTime,"auction is not over");
    require(block.timestamp <= _mode.paymentTime, "paymentTime end");
    require(_mode.to == msg.sender, "invalid payer");
    _mode.payPrice += _payPrice;
    require(_mode.payPrice == _mode.bidPrice, "invalid pay price");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    _mode.state = 3; // bid succ
    // accept payment amount
    require(lawa.Ltransfer(msg.sender, address(this), _payPrice),"invalid lawa transfer");
    // platformFee
    uint256 restValue = _mode.bidPrice  - _platformFee(_mode.bidPrice);
    // beneficiaryFee
    restValue = restValue - _beneficiaryFee(_orderId, _mode.bidPrice , _mode.owner);
    // The amount that the seller deserves it
    require(lawa.Ltransfer(address(this), _mode.owner, restValue), "invalid lawa transfer");
    emit lwPaymentLog(msg.sender, _mode.owner, _orderId, _mode.bidPrice,restValue);
    return true;
  }
  function lwAuctionCancel(uint256 _orderId) external onlyOrder(_orderId) returns(bool) {
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.owner == msg.sender, "account is not owner");
    require(block.timestamp <= _mode.expirationTime,"expirationTime end");
    require(_mode.state ==1, "invalid trading state");
    require(_mode.mode == 2, "invalid trading mode");
    require(_mode.to == address(0), "in the auction");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    _mode.state= 4;
    require(_unBale(_orderId,_mode.owner),"unsell failed");
    emit lwAuctionCancelLog(msg.sender, _orderId);
    return true;
  }
  function lwAuctionResult(uint256 _orderId, uint256 _result) external onlyAdmin onlyOrder(_orderId) returns (bool){
    require(_result > 1, "invalid result");
    TokenMode storage _mode = tokenModes[_orderId];
    require(_mode.state >= 1, "invalid trading state");
    require(block.timestamp > _mode.expirationTime,"auction is not over");
    require(_mode.mode==2, "invalid trading mode");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    if (block.timestamp > _mode.paymentTime) { 
      _mode.bidPrice > _mode.payPrice ?  _result = 4 :  _result = 3;
    }
    _mode.state = _result;
    if (block.timestamp > _mode.paymentTime) { 
      if (_mode.bidPrice==0 || _mode.bidPrice > _mode.payPrice){
        require(_unBale(_orderId,_mode.owner),"unsell failed");
      }
    }
    emit lwAuctionResultLog(msg.sender, _orderId, _result);
    return true;
  }
  function lwAuctionCliam(uint256 _orderId) external onlyAdmin onlyOrder(_orderId) returns(bool){
    TokenMode storage _mode = tokenModes[_orderId];
    require(block.timestamp > _mode.paymentTime, "invalid paymentTime");
    require(_mode.state == 4 && _mode.payPrice > 0,"invalid auction result or payPrice");
    require(_mode.orderId == _orderId, "invalid trading orderId");
    uint256 _cliam = (_mode.payPrice * cliamRatio) / 10000;
    // nft owner damages for breach of contract
    require(lawa.Ltransfer(address(this), _mode.owner, _cliam),"invalid lawa transfer");
    // platform damages for breach of contract
    require(lawa.Ltransfer(address(this), platformAccount, _mode.payPrice - _cliam),"invalid lawa transfer");
    totalWithdraw += _mode.payPrice - _cliam;
    return true;
  }
  function lwTransferProp(uint256[] calldata _tokenIds,uint256[] calldata _amounts, address _to) external returns(bool){
      require(_tokenIds.length > 0 && _tokenIds.length == _amounts.length, "invalid orderId length");
      require(_to != address(0), "invalid receiver address");
      for(uint256 i=0; i<_tokenIds.length; i++){
        require(balanceOf(msg.sender, _tokenIds[i]) >= _amounts[i], "token is low");
      }
      safeBatchTransferFrom(msg.sender, _to, _tokenIds,_amounts,'');
      emit lwTransferPropLog(msg.sender, _to, _tokenIds,_amounts);
      return true;
  }
}

