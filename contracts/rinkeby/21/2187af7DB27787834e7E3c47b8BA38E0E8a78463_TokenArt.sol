// File: contracts/TokenArt.sol


pragma solidity ^0.8.6;










contract TokenArt is Ownable, ITokenArt, ReentrancyGuard {

    using SafeMath for uint256;
    
    ////////////////////////////////
    //          Variables         //
    ////////////////////////////////
    uint256 public LAW_FORK_PRICE = 0.1 ether;
    uint256 public lockedTA;

    address public widthdrawalWallet;
    address public signatureWallet;

    struct ContractLicense {
        uint licenseId;
        uint version;
    }

    struct LawForkRequest {
        address requester;
        ContractLicense license;
        uint256 lockedTA;
        uint256 lockedOwner;
        bool initialized;
        bool granted;
    }

    mapping (address => mapping(uint256 => ContractLicense)) public contractTokenLicenses;
    mapping (address => ContractLicense) public contractLicenses;
    mapping (address => bool) public whitelistedProxies;
    
    mapping (uint256 => string) public licenses;
    mapping (uint256 => mapping(uint256 => string)) public licenseLinks;
    mapping (string => uint256) public licenseIds;
    mapping (address => mapping(uint256 => LawForkRequest)) public lawForkRequests;
    mapping (address => uint256) public lockedForUsers;
    mapping (address => uint256) public senderSignIndeces;

    Counters.Counter licenseCounter;

    ////////////////////////////////
    //        Constructor         //
    ////////////////////////////////
    constructor(
        address _widthdrawalWallet, 
        address _signerWallet
    ){
        widthdrawalWallet = _widthdrawalWallet;
        signatureWallet = _signerWallet;
    }

    ////////////////////////////////
    //          Modifiers         //
    ////////////////////////////////
    modifier canAddLicense(address _contract, uint256 _license, uint256 _version) {
        require(
            bytes(licenseLinks[_license][_version]).length > 0,
            "License does not exist"
        );
        require(
            _contract != address(0),
            "can't add 0 address"
        );
        _;
    }

    modifier isOwnerOrProxy() {
        require(
            msg.sender == owner() || whitelistedProxies[msg.sender],
            "Unauthorized"
        );
        _;
    }

    modifier requestIsOngoing(
        address _contract,
        uint256 _tokenId
    ) {
        require(lawForkRequests[_contract][_tokenId].initialized, "No law fork");
        require(!lawForkRequests[_contract][_tokenId].granted, "Already settled");
        _;
    }

    ////////////////////////////////
    //      Admin Functions       //
    ////////////////////////////////
    function whitelistProxy(address _proxy) public onlyOwner {
        whitelistedProxies[_proxy] = true;
    }

    function revokeProxy(address _proxy) public onlyOwner {
        whitelistedProxies[_proxy] = false;
    }

    function addLicense(string calldata _license) public onlyOwner {
        uint256 licenseId = Counters.current(licenseCounter);
        licenses[licenseId] = _license;
        licenseIds[_license] = licenseId;
        console.log(Counters.current(licenseCounter));
        Counters.increment(licenseCounter);
    }

    function addLicenseLink(uint256 _licenseId, uint256 _version, string calldata _link) public onlyOwner {
        licenseLinks[_licenseId][_version] = _link;
    }

    function setLawForkPrice(uint256 _lawForkPrice) public onlyOwner {
        LAW_FORK_PRICE = _lawForkPrice;
    }

    function setWithdrawalWallet(address _withdrawalWallet) public onlyOwner {
        widthdrawalWallet = _withdrawalWallet;
    }

    function setSignatureWallet(address _signerWallet) public onlyOwner {
        signatureWallet = _signerWallet;
    }

    ////////////////////////////////
    //      Getter Functions      //
    ////////////////////////////////
    function totalLicenses() public view returns(uint256) {
        return Counters.current(licenseCounter) + 1;                                                                                                                                                                                                                                     
    }

    ////////////////////////////////
    //     License Functions      //
    ////////////////////////////////
    function _addLicenseForTokenBase (
        address _contract,
        uint256 _tokenId,
        ContractLicense memory _license
    ) internal {
        contractTokenLicenses[_contract][_tokenId] = _license;
    }

    function _addLicenseForToken (
        address _contract,
        uint256 _tokenId,
        uint256 _license,
        uint256 _version
    ) internal canAddLicense(
        _contract,
        _license,
        _version
    ){
        _addLicenseForTokenBase(
            _contract,
            _tokenId,
            ContractLicense({
                licenseId: _license,
                version: _version
            })
        );
    }

    function _addLicenseForContractBase(
        address _contract,
        ContractLicense memory _license
    ) internal {
        contractLicenses[_contract] = _license;
    }

    function _addLicenseForContract(
        address _contract,
        uint256 _license,
        uint256 _version
    ) internal canAddLicense(
        _contract,
        _license,
        _version
    ){
        _addLicenseForContractBase(
            _contract,
            ContractLicense({
                licenseId: _license,
                version: _version
            })
        );
    }

    function addLicenseForToken(
        address _contract,
        uint256 _tokenId,
        uint256 _license,
        uint256 _version
    ) public override isOwnerOrProxy {
        _addLicenseForToken(_contract, _tokenId, _license, _version);
    }

    function addLicenseForTokenFromContract(
        address _contract,
        uint256 _tokenId,
        uint256 _license,
        uint256 _version,
        address _sender,
        bytes calldata _signature
    ) external override {
        require(_signatureWallet(_signature, _sender) == signatureWallet, "unauthorized");
        _addLicenseForToken(_contract, _tokenId, _license, _version);
        senderSignIndeces[_sender] += 1;
    }

    function addLicenseForContract(
        address _contract,
        uint256 _license,
        uint256 _version
    ) public override isOwnerOrProxy {
        _addLicenseForContract(_contract, _license, _version);
    }

    function clearLicenseForContract (
        address _contract
    ) public isOwnerOrProxy {
        require (contractLicenses[_contract].licenseId > 0, "not registered");
        delete contractLicenses[_contract];
    }

    function clearLicenseForToken (
        address _contract,
        uint256 _tokenId
    ) public isOwnerOrProxy {
        require (contractTokenLicenses[_contract][_tokenId].licenseId > 0, "not registered");
        delete contractTokenLicenses[_contract][_tokenId];
    }

    function requestLawFork(
        address _contract, 
        uint256 _tokenId, 
        uint256 _license, 
        uint256 _version, 
        uint256 amountTA,
        uint256 amountOwner
    ) public payable {
        require (IERC721(_contract).ownerOf(_tokenId) == msg.sender, "this token does not belong to you");
        require (!lawForkRequests[_contract][_tokenId].initialized, "Request already pending");
        require (amountTA + amountOwner == msg.value);
        require (amountTA >= LAW_FORK_PRICE, "not enough money");
        lawForkRequests[_contract][_tokenId] = LawForkRequest({
            requester: msg.sender,
            license: ContractLicense({
                licenseId: _license,
                version: _version
            }),
            lockedTA: amountTA,
            lockedOwner: amountOwner,
            initialized: true,
            granted: false
        });
    }

    function grantRequest(
        address _contract,
        uint256 _tokenId
    ) public {
        require (Ownable(_contract).owner() == msg.sender, "not authorized");
        require(lawForkRequests[_contract][_tokenId].initialized, "No law fork");
        require(!lawForkRequests[_contract][_tokenId].granted, "Already settled");
        LawForkRequest memory lawForkRequest = lawForkRequests[_contract][_tokenId];
        lawForkRequest.granted = true;
        lockedTA += lawForkRequest.lockedTA;
        (bool success, ) = msg.sender.call{value:lawForkRequest.lockedOwner}("");
        if(!success) {
            lockedForUsers[msg.sender] += lawForkRequest.lockedOwner;
        }
        _addLicenseForTokenBase(
            _contract,
            _tokenId,
            lawForkRequest.license
        );
    }

    function _cancelLawForkRequest(
        address _contract,
        uint256 _tokenId
    ) internal requestIsOngoing (_contract, _tokenId) {
        LawForkRequest memory lawForkRequest = lawForkRequests[_contract][_tokenId];
        lawForkRequest.initialized = false;
        _returnFundsToRequester(
            lawForkRequest.requester, 
            lawForkRequest.lockedOwner + lawForkRequest.lockedTA
        );
    }

    function _returnFundsToRequester(
        address _requester,
        uint256 amount
    ) internal {
        (bool success, ) = _requester.call{
            value:amount
        }("");
        if(!success) {
            lockedForUsers[_requester] += amount;
        }
    }

    function denyRequest (
        address _contract,
        uint256 _tokenId
    ) public requestIsOngoing(_contract, _tokenId) {
        require (Ownable(_contract).owner() == msg.sender, "not authorized");
        _cancelLawForkRequest(_contract, _tokenId);
    }

    function cancelRequest (
        address _contract,
        uint256 _tokenId
    ) public requestIsOngoing(_contract, _tokenId) {
        require (lawForkRequests[_contract][_tokenId].requester == msg.sender, "not authorized");
        _cancelLawForkRequest(_contract, _tokenId);
    }

    //////////////////////
    //    Withdrawal    //
    //////////////////////
    function TAWithdraw() public onlyOwner {
        require(widthdrawalWallet != address(0), "withdrawal wallet not set");
        (bool success, ) = widthdrawalWallet.call{
            value: lockedTA
        }("");
        require(success, 'Failed to send funds');
    }

    function userWithdraw() public nonReentrant{
        require(lockedForUsers[msg.sender] > 0, "No funds to withdraw");
        (bool success, ) = msg.sender.call{
            value: lockedForUsers[msg.sender]
        }("");
        require(success, 'Failed to send funds');
    }

    //////////////////////
    //     Signature    //
    //////////////////////
    function _signatureWallet(bytes memory _signature, address _sender) internal view returns(address) {
        return ECDSA.recover(
            ECDSA.toEthSignedMessageHash(
                keccak256(abi.encodePacked(
                    _sender,
                    senderSignIndeces[_sender],
                    address(this)
                )
            )
        ), _signature);
    }

}