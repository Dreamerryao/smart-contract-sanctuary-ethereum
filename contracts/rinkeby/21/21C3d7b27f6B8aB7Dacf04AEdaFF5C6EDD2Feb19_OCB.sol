   function getColorCSS(uint256 coloridx) public view returns(string memory) {
        uint256 paletteIdx = coloridx*3;
        uint256 r;
        uint256 g;
        uint256 b;
        uint256 a;
        if(paletteIdx>=colorPalette.length) {
            //outside of colorPalette -> alphaPalette
            uint256 fixedColorIdx = coloridx-colorPalette.length/3;
            paletteIdx = fixedColorIdx*4;
            require(paletteIdx < alphaPalette.length, "color index out of bound");
            r = uint256(alphaPalette[paletteIdx]);
            g = uint256(alphaPalette[paletteIdx+1]);
            b = uint256(alphaPalette[paletteIdx+2]);
            a = uint256(alphaPalette[paletteIdx+3]);
            //a = a*100;
            //uint256 predec = a/255;
            //uint256 predec = a/255;
            uint256 dec = a*100/255;
            return string(abi.encodePacked("rgba(", Strings.toString(r), ",", Strings.toString(g), ",", Strings.toString(b), ", 0.", Strings.toString(dec), ")"));
        } else {
            r = uint256(colorPalette[paletteIdx]);
            g = uint256(colorPalette[paletteIdx+1]);
            b = uint256(colorPalette[paletteIdx+2]);
            return string(abi.encodePacked("rgb(", Strings.toString(r), ",", Strings.toString(g), ",", Strings.toString(b), ")"));
        }
    }

