contract VRYNT_NFT is ERC1155 {
      
     struct details{
         uint256 maxNft;
         uint256 currentMintedNft;
     }
     constructor(address _trustedForwarder) {
    trustedForwarder = _trustedForwarder;
  }
 event changeForwarder(address indexed newForwarder, address indexed oldForwarder);

//  function to change the forwarder

  function changeTrustedForwarder(address newForwarder) external  onlyOwner() {
     require(newForwarder != address(0x0) && newForwarder != trustedForwarder, "Msg : Invlaid Address");
     address _oldForwarder = trustedForwarder;
     trustedForwarder = newForwarder;
     emit changeForwarder(newForwarder,_oldForwarder);
  
    }
    mapping(string => details) public collections;
    
    function mint(
        address to,
        string memory id,
        uint256 value,
        string memory _tokenURI,
        string memory data,
        string memory collectionId,
        uint256 maxNFTToBeMinted
    ) public {
        if(collections[collectionId].maxNft != 0){
         require(collections[collectionId].currentMintedNft < collections[collectionId].maxNft,"collection limit Exceeds,cannot mint your NFT");
        require(minters[msg.sender],"you are not the minter" );
        _mint(to, id, value, _tokenURI, data);
        collections[collectionId].currentMintedNft++;
        }else{
        require(minters[msg.sender],"you are not the minter" );
        collections[collectionId].maxNft = maxNFTToBeMinted;
        collections[collectionId].currentMintedNft =  0;
        _mint(to, id, value, _tokenURI, data);
        collections[collectionId].currentMintedNft++;
    }
    }

    function mintBatch(
        address to,
        string[] memory ids,
        uint256[] memory values,
        string[] memory _tokenURIs,
        string memory data,
        string memory collectionId
    ) public {
        require(minters[msg.sender],"you are not the minter" );
        _mintBatch(to, ids, values, _tokenURIs,data);
        collections[collectionId].currentMintedNft++;
    }

    function burn(
        address owner,
        string memory id,
        uint256 value,
        string memory collectionId
    ) public  {
      require(NFTOwner[id] == msg.sender,"only NFT owner can burn the NFT");
        _burn(owner, id, value);
        collections[collectionId].currentMintedNft--;
    }

    function burnBatch(
        address owner,
        string[] memory ids,
        uint256[] memory values,
        string memory collectionId
    ) public {
        for (uint256 i = 0; i < ids.length; i++) {
            string memory id = ids[i];
        require(NFTOwner[id] == msg.sender,"only NFT owner can burn the NFT");
        _burnBatch(owner, ids, values);
        collections[collectionId].currentMintedNft--;
    }
    }

}