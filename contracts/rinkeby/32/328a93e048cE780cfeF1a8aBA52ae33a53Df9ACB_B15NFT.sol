  uint256 private _userReservedMaxCount = 0;
  uint256 private _mintMaxCount = 0;
  uint256 private _mintCount = 0;


    constructor(string memory name, string memory symbol, uint256 ownerReservedMaxCount,uint256 userReservedMaxCount,uint256  mintMaxCount,string memory baseuri)
    {
      nftName = name;
      nftSymbol = symbol;
      _ownerReservedMaxCount = ownerReservedMaxCount;
      _userReservedMaxCount = userReservedMaxCount;
      _mintMaxCount = mintMaxCount;
      maxSupply =  ownerReservedMaxCount + userReservedMaxCount + mintMaxCount;
      baseURI = baseuri;
      isSaleActive = true;
      tokenSupply = 0;
    }

    function setPrice(uint256 _newPrice) external onlyOwner {
        _price = _newPrice;
    }

    function getPrice() public view returns (uint256){
        return _price;
    }


    function flipSaleAtive() external onlyOwner {
          isSaleActive = !isSaleActive;
    }



    function strConcat(string memory  _a, string memory _b) internal pure returns ( string memory ){
            bytes memory _ba = bytes(_a);
            bytes memory _bb = bytes(_b);
            string memory ret = new string(_ba.length + _bb.length);
            bytes memory bret = bytes(ret);
            uint k = 0;
            uint i = 0;
            for (i = 0; i < _ba.length; i++) bret[k++] = _ba[i];
            for (i = 0; i < _bb.length; i++) bret[k++] = _bb[i];
            return string(ret);
    }  



  function mint( uint256 _count) external  payable {

    require(_count > 0, "You cannot mint less than 1 Tokens at once.");
    require(_count  <= (_mintMaxCount - _mintCount) , "Purchase would exceed max supply");


    if (owner != msg.sender)
    {
        require(_count * _price <= msg.value, "Inconsistent amount sent!");

        require(isSaleActive, "Sale must be active to mint");
    }


    for (uint256 i = 1; i <= _count; i++) {
          _mintCount++;
          tokenSupply = _mintCount + _userReservedCount + _ownerReservedCount;

          uint256 tokenid =  _ownerReservedMaxCount + _userReservedMaxCount + _mintCount;

          super._mint(msg.sender, tokenid);


            string memory uri = strConcat(baseURI,tokenid.toString());
            super._setTokenUri(tokenid, uri);

        }

  }



      function claimOwnerReserved(uint256 _count ) external onlyOwner {

          require(_count > 0, "You cannot mint less than 1 Tokens at once.");
          require(_count <= (_ownerReservedMaxCount- _ownerReservedCount), "That would exceed the owner max reserved.");

          for (uint256 i = 1; i <= _count; i++) {
            
            _ownerReservedCount++;
            tokenSupply = _mintCount + _userReservedCount + _ownerReservedCount;
            uint256 tokenid = _ownerReservedCount;

            super._mint(owner, tokenid);

            string memory uri = strConcat(baseURI,tokenid.toString());
            super._setTokenUri(tokenid, uri);

          }

          
      }



     function claimUserReservedByID(uint256 _tokenId, address _receiver) external onlyOwner {

        require((_tokenId > _ownerReservedMaxCount) &&  (_tokenId <= (_ownerReservedMaxCount + _userReservedMaxCount)), "The tokenId out of range");
        require(idToOwner[_tokenId] == address(0), "Current item has been minted");

        _userReservedCount++;   
        tokenSupply = _mintCount + _userReservedCount + _ownerReservedCount;

        super._mint(_receiver, _tokenId);

        string memory uri = strConcat(baseURI,_tokenId.toString());
        super._setTokenUri(_tokenId, uri);

        
    }






  

  function withdraw() public onlyOwner {
        uint256 _balance = address(this).balance;

        require(payable(owner).send(_balance));
    }


    function getUserReservedLeft() public view returns (uint256) {
        return  (_userReservedMaxCount -  _userReservedCount);
    }
    

  function getUserReservedMaxCount() public view returns (uint256) {
          return  _userReservedMaxCount ;
      }



    function getOwnerReservedLeft() public view returns (uint256) {
        return  (_ownerReservedMaxCount -  _ownerReservedCount);
    }
    

  function getOwnerReservedMaxCount() public view returns (uint256) {
          return  _ownerReservedMaxCount ;
      }



  function getMintLeft() public view returns (uint256) {

        return  (_mintMaxCount - _mintCount);
    }




function supportsInterface(bytes4 interfaceId)
        public
        view
        override(HoleTokenMetadata)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }


}