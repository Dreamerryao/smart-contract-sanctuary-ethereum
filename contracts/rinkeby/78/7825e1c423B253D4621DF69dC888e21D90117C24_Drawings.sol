pragma solidity 0.8.4;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "./DrawingStructs.sol";

contract Drawings is IERC721Receiver, VRFConsumerBaseV2 {
  using Counters for Counters.Counter;
  Counters.Counter public drawingCount;

  VRFCoordinatorV2Interface internal immutable COORDINATOR;
  LinkTokenInterface internal immutable LINKTOKEN;

  bytes32 internal immutable KEY_HASH;
  uint256 public constant GIVEAWAY_FEE = 0.01 ether;
  address public immutable TREASURY;
  address public immutable ERC721_DIVDENDS_CONTRACT;
  address public immutable ERC20_DIVIDENDS_CONTRACT;

  /* chainlink request ID => drawing index */
  mapping(uint256 => uint256) private _drawingIndex;
  /* drawing index => drawing instance */
  mapping(uint256 => DrawingStructs.Drawing) public drawing;
  /* drawing index => drawing config */
  mapping(uint256 => DrawingStructs.DrawingConfig) public drawingConfig;
  /* staker => asset contract => token id => true if staked */
  mapping(address => mapping(address => mapping(uint256 => bool))) public staked;
  /* asset contract => token id => drawingIndex */
  mapping(address => mapping(uint256 => uint256)) public drawingIndex;
  /* drawing index => player address => number of tickets */
  mapping(uint256 => mapping(address => uint256)) public tickets;
  /* drawing index => token address => token id  => player */
  mapping(uint256 => mapping(address => mapping(uint256 => address))) public winnerOf;
  /* drawing index => prizes */
  mapping(uint256 => DrawingStructs.NFT[][]) public prizes;
  /* drawing index => entries */
  mapping(uint256 => address[]) public entries;
  /* drawing index => ETH balance */
  mapping(uint256 => uint256) public drawingBalance;
  /* drawing index => chainlink random result(s) */
  mapping(uint256 => uint256[]) public randomness;

  constructor(
    address treasury,
    address vrfCoordinator,
    address link,
    bytes32 keyHash,
    address erc20DividendsContract,
    address erc721DividendsContract
  ) VRFConsumerBaseV2(vrfCoordinator) {
    COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    LINKTOKEN = LinkTokenInterface(link);
    KEY_HASH = keyHash;
    ERC20_DIVIDENDS_CONTRACT = erc20DividendsContract;
    ERC721_DIVDENDS_CONTRACT = erc721DividendsContract;
    TREASURY = treasury;
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Registers the drawing instance &
   * @notice verifies that the `_prizes` are staked
   * @notice and adds the staked `_prizes` to drawing struct instance.
   *
   * @param _config | Drawing config
   * @param _nfts | NFTs that will be awarded to winners
   */
  function register(DrawingStructs.DrawingConfig memory _config, DrawingStructs.NFT[][] memory _nfts) public payable {
    // must include at least 1 prize
    require(_nfts[0].length > 0, "missing prizes");

    // max tickets must be greater than or equal to reserve
    if (_config.maxTickets > 0) {
      require(_config.maxTickets >= _config.reserve, "max tickets < reserve");
    }

    // giveway drawings conditional fee
    if (_config.ticketPrice == 0 && GIVEAWAY_FEE > 0) {
      require(msg.value >= GIVEAWAY_FEE, "incorrect value");
    }

    drawingCount.increment();

    uint256 idx = drawingCount.current();
    DrawingStructs.Drawing storage _drawing = drawing[idx];
    DrawingStructs.NFT[][] storage _prizes = prizes[idx];

    for (uint256 i = 0; i < _nfts.length; i++) {
      require(_nfts[i].length > 0, "empty prize");

      _prizes.push();

      for (uint256 j = 0; j < _nfts[i].length; j++) {
        address tokenAddress = _nfts[i][j].tokenAddress;
        uint256 tokenId = _nfts[i][j].tokenId;

        // verify prize has been staked
        require(staked[msg.sender][tokenAddress][tokenId], "token not staked");

        // verify that the prize is not currently associated with another active drawing
        require(drawingIndex[tokenAddress][tokenId] == 0, "not allowed: duplicate");

        _prizes[i].push();
        drawingIndex[tokenAddress][tokenId] = idx;

        _prizes[i][j] = _nfts[i][j];
      }
    }

    // Require valid giveway configuration
    if (_config.ticketPrice == 0) {
      // giveaways do not expire
      require(_config.expiration == 0, "not allowed: expiration");
      if (_config.reserve > 0) {
        // duration not allowed
        require(_config.duration == 0, "not allowed: duration");
      } else {
        // duration is required
        require(_config.duration >= 24 * 60 * 60, "invalid: min duration");
        require(_config.duration <= 90 * 24 * 60 * 60, "invalid: max duration");
      }
      // Require valid raffle configuration
    } else {
      if (_config.reserve > 0) {
        // duration not allowed
        require(_config.duration == 0, "not allowed: duration");
        // expiration is required
        require(_config.expiration >= 24 * 60 * 60, "invalid: min expiration");
        require(_config.expiration <= 90 * 24 * 60 * 60, "invalid: max expiration");
      } else {
        // expiration not allowed
        require(_config.expiration == 0, "not allowed: expiration");
        // drawing time is required
        require(_config.duration >= 24 * 60 * 60, "invalid: min duration");
        require(_config.duration <= 90 * 24 * 60 * 60, "invalid: max duration");
      }
    }

    drawingConfig[idx] = _config;
    drawingConfig[idx].beneficiary = _config.beneficiary == address(0) ? msg.sender : _config.beneficiary;

    _drawing.expiresAt = _config.ticketPrice > 0 && _config.reserve > 0 ? block.timestamp + _config.expiration : 0;
    _drawing.drawingAt = _config.duration > 0 ? block.timestamp + _config.duration : 0;
    _drawing.provider = msg.sender;

    emit Create({ drawingIndex: idx, provider: _drawing.provider });
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Removes token stake
   * @dev Tokens can be "unstaked" if they have not been added to a
   * @dev drawing OR if the drawing associated with the NFT has expired.
   * @param token | the NFT to be unstaked
   */
  function unstake(DrawingStructs.NFT memory token) public {
    // check stake map for existing stake
    require(staked[msg.sender][token.tokenAddress][token.tokenId], "nothing to unstake");

    // Check if NFT is controlled by a drawing
    uint256 idx = drawingIndex[token.tokenAddress][token.tokenId];

    if (idx > 0) {
      require(drawing[idx].drawingAt == 0, "drawing results pending");
      require(drawing[idx].expiresAt > 0 && drawing[idx].expiresAt < block.timestamp, "wait for expiration");
    }

    // remove from stake map
    staked[msg.sender][token.tokenAddress][token.tokenId] = false;

    // transfer token to sender
    IERC721(token.tokenAddress).safeTransferFrom({ from: address(this), to: msg.sender, tokenId: token.tokenId });

    // clean up
    drawingIndex[token.tokenAddress][token.tokenId] = 0;

    emit Unstake({ nft: token, stakeholder: msg.sender });
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Enter a Drawing.
   * @dev May or may not require value.
   * @param idx | drawing index
   */
  function enter(uint256 idx, uint256 numberOfTickets) public payable {
    DrawingStructs.Drawing storage _drawing = drawing[idx];
    DrawingStructs.DrawingConfig memory _config = drawingConfig[idx];

    require(!_drawing.randomnessRequested, "drawing has ended");
    require(_drawing.provider != address(0), "invalid idx");

    if (_config.ticketPrice == 0) {
      require(msg.value == 0, "no value required");
    }

    require(_config.ticketPrice * numberOfTickets <= msg.value, "not enough ETH");

    // Drawing ~ might ~ be permissioned
    address[] memory eligible = _config.eligible;
    for (uint256 i = 0; i < eligible.length; i++) {
      require(IERC721(eligible[i]).balanceOf(msg.sender) > 0, "ineligible entry");
    }

    // If this is a raffle AND it has a reserve, check the expiration
    if (drawing[idx].expiresAt > 0) {
      require(drawing[idx].expiresAt > block.timestamp, "drawing is expired");
    }

    // If maxTickets > 0, make sure number of entries does not exceed maximum
    if (drawingConfig[idx].maxTickets > 0) {
      require(entries[idx].length + numberOfTickets <= _config.maxTickets, "exceeds max tickets");
    }

    // Limit one ticket per address for free drawings
    if (_config.ticketPrice == 0) {
      require(tickets[idx][msg.sender] + numberOfTickets == 1, "exceeds balance limit");
    }

    for (uint256 i = 0; i < numberOfTickets; i++) {
      tickets[idx][msg.sender] += 1;
      entries[idx].push(msg.sender);
    }

    drawingBalance[idx] += msg.value;

    // if there is a reserve, and it has been "met", set drawing time if it has not been set
    if (_config.reserve > 0 && entries[idx].length >= _config.reserve && _drawing.drawingAt == 0) {
      _drawing.drawingAt = block.timestamp + 24 hours;
      _drawing.expiresAt = 0; // the drawing can no longer expire
      emit ReserveMet({ drawingIndex: idx });
    }

    // return surplus value
    uint256 returnValue = msg.value - _config.ticketPrice * numberOfTickets;
    if (returnValue > 0) {
      (bool success, ) = payable(msg.sender).call{ value: returnValue }("");
      require(success, "unable to send value");
    }

    emit Enter({ entrant: msg.sender, drawingIndex: idx, numberOfTickets: numberOfTickets });
  }

  /**
   * *** PUBLIC ***
   *
   * @notice process refund.
   * @dev drawing must be "expired".
   * @param idx | drawing index
   */
  function processRefund(uint256 idx) public {
    // drawing must not be successful
    require(!drawing[idx].randomnessRequested, "drawing complete");

    // drawing time must be zero
    require(drawing[idx].drawingAt == 0, "drawing results pending");

    // must be a paid drawing
    require(drawingConfig[idx].ticketPrice > 0, "action not available for type");

    // drawing expiration must be set and expired
    require(drawing[idx].expiresAt > 0 && drawing[idx].expiresAt < block.timestamp, "wait for expiration");

    // drawing must have balance
    require(drawingBalance[idx] > 0, "drawing has zero balance");

    // sender must have tickets
    require(tickets[idx][msg.sender] > 0, "sender has no tickets");

    uint256 refundAmount = tickets[idx][msg.sender] * drawingConfig[idx].ticketPrice;

    drawingBalance[idx] -= refundAmount;
    tickets[idx][msg.sender] = 0;

    (bool success, ) = payable(msg.sender).call{ value: refundAmount }("");
    require(success, "unable to send value");
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Transfers NFT prize to `msg.sender`.
   * @param idx | drawing index
   * @param prize | NFT to be claimed
   */
  function claimPrize(uint256 idx, DrawingStructs.NFT memory prize) public {
    require(msg.sender == winnerOf[idx][prize.tokenAddress][prize.tokenId], "must be winner");

    // clean up
    drawingIndex[prize.tokenAddress][prize.tokenId] = 0;
    winnerOf[idx][prize.tokenAddress][prize.tokenId] = address(0);

    // transfer token
    IERC721(prize.tokenAddress).safeTransferFrom({ from: address(this), to: msg.sender, tokenId: prize.tokenId });
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get registered NFT award at `index` for a drawing by index.
   * @param tokenAddress | token address
   * @param tokenId | token id
   * @return index
   */
  function getDrawingIndex(address tokenAddress, uint256 tokenId) public view returns (uint256) {
    return drawingIndex[tokenAddress][tokenId];
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get drawing config by drawing index
   * @param idx | drawing index
   * @return config
   */
  function getDrawingConfig(uint256 idx) public view returns (DrawingStructs.DrawingConfig memory) {
    return drawingConfig[idx];
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get drawing provider by drawing index
   * @param idx | drawing index
   * @return provider
   */
  function getDrawingProvider(uint256 idx) public view returns (address) {
    return drawing[idx].provider;
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get all registered NFT prizes for a drawing by index
   * @param idx | drawing index
   * @return prizes
   */
  function getPrizes(uint256 idx) public view returns (DrawingStructs.NFT[][] memory) {
    return prizes[idx];
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get registered prize at `index` for a drawing by index.
   * @param index | the prize at index; may include one or more nfts.
   * @param idx | drawing index
   * @return prize
   */
  function getPrize(uint256 idx, uint256 index) public view returns (DrawingStructs.NFT[] memory) {
    return prizes[idx][index];
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get all addresses permissioned for drawing by index.
   * @param idx | drawing index
   * @return permissioned addresses
   */
  function getEligibile(uint256 idx) public view returns (address[] memory) {
    return drawingConfig[idx].eligible;
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Get all entries for a drawing by index.
   * @param idx | drawing index
   * @return entries
   */
  function getEntries(uint256 idx) public view returns (address[] memory) {
    return entries[idx];
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Requests randomness from Chainlink VRF
   * @dev Randomness can only be requested one time.
   * @dev Drawing `drawingAt` must not be zero and be less than the current block timestamp if expiration
   * @dev has not been reached.
   *
   * @param idx | drawing index
   */
  function getRandomness(uint256 idx, uint64 subscriptionId) public {
    // must not have already requested randomness
    require(drawing[idx].randomnessRequested == false, "randomness already requested");

    // must be time to draw
    require(drawing[idx].drawingAt > 0 && drawing[idx].drawingAt <= block.timestamp, "must be time to draw");

    // must not be expired
    if (drawing[idx].expiresAt > 0) {
      require(drawing[idx].expiresAt > block.timestamp, "drawing is expired");
    }

    // prevent reentrancy
    drawing[idx].randomnessRequested = true;

    uint256 requestId = COORDINATOR.requestRandomWords(
      KEY_HASH,
      subscriptionId,
      5, // request confirmations
      200000, // callback gas limit
      uint32(prizes[idx].length) // number of random words
    );

    _drawingIndex[requestId] = idx;

    emit GetRandomness({ drawingIndex: idx });
  }

  /**
   * *** INTERNAL ***
   *
   * @notice Fulfill Chainlink VRF randomness request
   * @dev At time of writing, VRF fulfillment tx is limited to 200k gas
   */
  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    uint256 idx = _drawingIndex[requestId];
    drawing[idx].randomnessReceived = true;
    randomness[idx] = randomWords;

    emit RandomnessReceived({ drawingIndex: idx });
  }

  /**
   * *** PUBLIC ***
   *
   * @dev select winners
   * @param idx | drawing index
   */
  function selectWinners(uint256 idx) public {
    require(drawing[idx].randomnessReceived, "randomness not received");

    if (entries[idx].length == 0) {
      entries[idx].push(drawing[idx].provider);
    }

    for (uint256 i = 0; i < randomness[idx].length; i++) {
      uint256 winningIndex = randomness[idx][i] % entries[idx].length;
      address winningAddress = entries[idx][winningIndex];

      DrawingStructs.NFT[] memory prize = prizes[idx][i];

      for (uint256 j = 0; j < prize.length; j++) {
        winnerOf[idx][prize[j].tokenAddress][prize[j].tokenId] = winningAddress;

        // remove NFT from stake map
        staked[drawing[idx].provider][prize[j].tokenAddress][prize[j].tokenId] = false;

        emit SelectWinner({ entrant: winningAddress, drawingIndex: idx, prize: prize[j] });
      }
    }
  }

  /**
   * *** PUBLIC ***
   *
   * @notice Withdraw funds
   *
   * @param idx | drawing index
   */
  function withdraw(uint256 idx) public {
    address beneficiary = drawingConfig[idx].beneficiary;
    uint256 balance = drawingBalance[idx];

    require(balance > 0, "no balance");
    require(drawing[idx].randomnessRequested == true, "drawing is not over");

    drawingBalance[idx] = 0;

    // Fees: 6.942%
    uint256 fees = (balance * 6942) / 100000;

    // Beneficiary: 93.058%
    (bool beneficiarySuccess, ) = beneficiary.call{ value: balance - fees }("");
    require(beneficiarySuccess, "beneficiary: send error");

    // 40% to ERC20 Dividends
    (bool erc20DividendsSuccess, ) = ERC20_DIVIDENDS_CONTRACT.call{ value: (fees * 40) / 100 }("");
    require(erc20DividendsSuccess, "token dividends: send error");

    // 40% to ERC721 Dividends
    (bool erc721DividendsSuccess, ) = ERC721_DIVDENDS_CONTRACT.call{ value: (fees * 40) / 100 }("");
    require(erc721DividendsSuccess, "nftDividends: send error");

    // 20% to Treasury
    (bool treasurySuccess, ) = TREASURY.call{ value: fees - ((fees * 80) / 100) }("");
    require(treasurySuccess, "treasury: send error");
  }

  /**
   * *** PRIVATE ***
   *
   * @dev This function is called from onERC721Received
   * @dev NFT has not been associated with a reward yet
   * @param stakeholder | the address in control of the NFT
   * @param token | the NFT transferred to contract
   */
  function _stake(address stakeholder, DrawingStructs.NFT memory token) private {
    staked[stakeholder][token.tokenAddress][token.tokenId] = true;
    emit Stake({ nft: token, stakeholder: stakeholder });
  }

  /**
   * @dev See {IERC721Receiver-onERC721Received}.
   *
   * Always returns `IERC721Receiver.onERC721Received.selector`.
   */
  function onERC721Received(
    address operator,
    address,
    uint256 tokenId,
    bytes memory
  ) public virtual override returns (bytes4) {
    DrawingStructs.NFT memory token = DrawingStructs.NFT({ tokenAddress: msg.sender, tokenId: tokenId });
    _stake(operator, token);
    return this.onERC721Received.selector;
  }

  event Create(uint256 drawingIndex, address provider);

  event Enter(address entrant, uint256 drawingIndex, uint256 numberOfTickets);

  event Stake(DrawingStructs.NFT nft, address stakeholder);

  event Unstake(DrawingStructs.NFT nft, address stakeholder);

  event GetRandomness(uint256 drawingIndex);

  event RandomnessReceived(uint256 drawingIndex);

  event SelectWinner(address entrant, uint256 drawingIndex, DrawingStructs.NFT prize);

  event ReserveMet(uint256 drawingIndex);
}

