// File contracts/Genesis.sol

pragma solidity ^0.8.4;






contract Genesis is IERC1155MetadataURI {
    using Address for address;
    using Strings for uint256;

    // Balances
    mapping(address => mapping(uint => uint)) internal _balances;

    // Owner => Spender => Approved
    mapping(address => mapping(address => bool)) internal _approvals;
    
    constructor() {
        console.logBytes4(type(IERC165).interfaceId);
    }

    function mint(uint256 id, uint256 amount) external {
        _balances[msg.sender][id] += amount;
    }
    
    //================================== ERC1155MetadataURI =======================================/

    function uri(uint256 id) external pure override returns (string memory) {
        return string(abi.encodePacked("https://opensea-creatures-api.herokuapp.com/api/creature/", id.toString()));
    }

    //================================== ERC165 =======================================/


    function supportsInterface(bytes4 interfaceId) external pure override returns (bool) {
        return 
            interfaceId == type(IERC165).interfaceId || 
            interfaceId == type(IERC1155).interfaceId ||
            interfaceId == type(IERC1155MetadataURI).interfaceId;
    }

    //================================== ERC1155 =======================================/


    function balanceOf(address account, uint256 id) public view override returns (uint256) {
        return _balances[account][id];
    }

    function balanceOfBatch(address[] calldata accounts, uint256[] calldata ids) external view override returns (uint256[] memory) {
        require(accounts.length == ids.length, "Incorrect input");
        
        uint[] memory result = new uint[](accounts.length);

        for (uint i = 0; i < accounts.length; i++) {
            address account = accounts[i];
            uint id = ids[i];
            result[i] = balanceOf(account, id);
        }

        return result;
    }

    function setApprovalForAll(address operator, bool approved) external override {
        _approvals[msg.sender][operator] = approved;
    }

    function isApprovedForAll(address account, address operator) external view override returns (bool) {
        return _approvals[account][operator];
    }

    function safeTransferFrom(address from, address to, uint256 id, uint256 amount, bytes calldata data) public override {
        require(to != address(0), "Transfer to zero");
        require(_approvals[from][msg.sender] == true || msg.sender == from, "Non-approved sender");
        require(_balances[from][id] >= amount, "Not enough owned");
        require(from != to, "Sending to itself");

        _transfer(from, to, id, amount);

        emit TransferSingle(msg.sender, from, to, id, amount);

        if (to.isContract()) {
            require(IERC1155Receiver(to).onERC1155Received(msg.sender, from, id, amount, data) == 0xf23a6e61, "Transfer refused");
        }
    }

    function safeBatchTransferFrom(address from, address to, uint256[] calldata ids, uint256[] calldata amounts, bytes calldata data) external override {
        require(ids.length == amounts.length, "Incorrect input");
        require(_approvals[from][msg.sender] == true || msg.sender == from, "Non-approved sender");

        for (uint i = 0; i < ids.length; i++) {
            uint id = ids[i];
            uint amount = amounts[i];
            require(_balances[from][id] >= amount, "Not enough owned");
            _transfer(from, to, id, amount);
        }

        emit TransferBatch(msg.sender, from, to, ids, amounts);

        if (to.isContract()) {
            require(IERC1155Receiver(to).onERC1155BatchReceived(msg.sender, from, ids, amounts, data) == 0xbc197c81, "Transfer refused");
        }
    }

    function _transfer(address from, address to, uint256 id, uint256 amount) private {
        _balances[from][id] -= amount;
        _balances[to][id] += amount;
    }
}


