pragma solidity ^0.8.0;

import {CreatorExtensionBasic} from "@manifoldxyz/creator-core-solidity/contracts/extensions/CreatorExtensionBasic.sol";
import {IERC721CreatorCore} from "@manifoldxyz/creator-core-solidity/contracts/core/IERC721CreatorCore.sol";

import {Renderer} from "./Renderer.sol";
import {Base64} from "./Base64.sol";

/**
 * An extension for Manifold contracts that add pay to mint functionalities
 */

contract EndGameExtension is CreatorExtensionBasic, Renderer {
    //
    // Errors
    //

    //
    // Library
    //

    using Base64 for bytes;

    //
    // State
    //

    // Counter of minted tokens
    uint256 public tokenAmountTracker;
    // address of the core contract also refered as creator on the manifold nomenclature
    address public immutable creator;

    constructor(address creator_) {
        creator = creator_;
    }

    //
    // External
    //

    /**
     * Pay to mint a token on the creator contract
     */
    function mint() external payable {
        IERC721CreatorCore(creator).mintExtension(msg.sender);

        string memory svgEncoded = bytes(render(1)).encode();
        string memory uri = string(
            abi.encodePacked("data:image/svg+xml;base64,", svgEncoded)
        );

        IERC721CreatorCore(creator).setTokenURIExtension(
            tokenAmountTracker,
            uri
        );

        unchecked {
            ++tokenAmountTracker;
        }
    }

    //
    // Queries
    //

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(CreatorExtensionBasic)
        returns (bool)
    {
        return CreatorExtensionBasic.supportsInterface(interfaceId);
    }
}

