pragma solidity ^0.8.11;

import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "./Multimedia.sol";
import "./MultimediaSchema.sol";
import "hardhat/console.sol";

contract MultimediaVRF is VRFConsumerBaseV2 {
  VRFCoordinatorV2Interface public vrfCoordinator;
  LinkTokenInterface public linkToken;
  bytes32 public keyHash;
  uint64 public subscriptionId;

  address public deployerAddress;
  address public multimediaAddress;
  bool public initialized = false;
  uint256 public bulkRandomizeMax = 10;

  mapping(uint256 => uint256) public vrfToDropId;

  constructor(
    address _vrfCoordinator,
    address _linkToken,
    bytes32 _keyHash,
    uint64 _subscriptionId
  ) VRFConsumerBaseV2(_vrfCoordinator) {
    deployerAddress = msg.sender;
    vrfCoordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
    linkToken = LinkTokenInterface(_linkToken);
    keyHash = _keyHash;
    subscriptionId = _subscriptionId;
  }

  modifier onlyDeployer() {
    require(msg.sender == deployerAddress, "Not allowed");
    _;
  }

  modifier onlyDAO() {
    require(msg.sender == Multimedia(multimediaAddress).daoAddress(), "Not allowed");
    _;
  }

  function initVRF(address _contractAddress) external onlyDeployer() {
    require(!initialized, "Already initialized");
    multimediaAddress = _contractAddress;
    initialized = true;
  }

  function changeVRF(address _vrfCoordinator, address _linkToken, bytes32 _keyHash, uint64 _subscriptionId) external onlyDAO() {
    vrfCoordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
    linkToken = LinkTokenInterface(_linkToken);
    keyHash = _keyHash;
    subscriptionId = _subscriptionId;
  }

  function changeBulkRandomizeMax(uint256 newBulkRandomizeMax) external onlyDAO() {
    bulkRandomizeMax = newBulkRandomizeMax;
  }

  function resetVRF(uint256 dropId) external onlyDAO() {
    Multimedia(multimediaAddress).setVrfResult(dropId, 0, 0, "reset");
  }

  // Called by public or keeper
  function requestVRF(uint256 dropId) external {
    (,MultimediaSchema.DropData memory dropData) = Multimedia(multimediaAddress).getDrop(dropId);

    require(!dropData.vrfPending &&
        dropData.reserveMintCompleted &&
        dropData.tokenIdCounter - dropData.randomizedTraitsCounter > 0, 'VRF not needed');

    uint256 requestId = vrfCoordinator.requestRandomWords(keyHash, subscriptionId, 7, 300000, 1);

    vrfToDropId[requestId] = dropId;
    Multimedia(multimediaAddress).setVrfResult(dropId, 0, 0, "requested");
  }

  // Chainlink VRF Callback
  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    uint256 dropId = vrfToDropId[requestId];
    Multimedia(multimediaAddress).setVrfResult(dropId, randomWords[0] + 1, bulkRandomizeMax, "fulfilled");
  }
}

