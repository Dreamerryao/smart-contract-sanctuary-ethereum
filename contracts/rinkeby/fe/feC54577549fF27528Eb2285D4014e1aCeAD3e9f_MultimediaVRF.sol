pragma solidity ^0.8.11;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./MultimediaSchema.sol";

import "../HelperFunctions.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

library MultimediaDrop {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.universe.multimedia.storage");
  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly {
      ds.slot := position
    }
  }

  modifier onlyDAO() {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msg.sender == ds.daoAddress, "Wrong address");
    _;
  }

  modifier onlyVRF() {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msg.sender == ds.vrfAddress, "Wrong address");
    _;
  }


  function setVrfAddress(address _vrfAddress) public onlyDAO {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    ds.vrfAddress = _vrfAddress;
  }


  function createDrop(MultimediaSchema.Drop memory drop) external onlyDAO returns (uint256 dropCounter) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    require(drop.traits.length == drop.traitChances.length, 'Trait mismatch');

    ds.drops[ds.dropCounter] = drop;
    ds.dropData[ds.dropCounter] = MultimediaSchema.DropData({
      tokenIdCounter: drop.reserveQuantity + 1,
      reserveIdCounter: 1,
      randomizedTraitsCounter: 1,
      useVRF: drop.traits.length > 0,
      vrfPending: false,
      reserveMintCompleted: !(drop.reserveQuantity > 0),
      saleActive: false
    });

    ds.dropCounter = ds.dropCounter.add(1);
    return ds.dropCounter;
  }


  function startOrPauseSale(uint256 dropId, bool isStart) external onlyDAO {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    ds.dropData[dropId].saleActive = isStart;
  }


  function adjustPrice(uint256 dropId, uint256 price) public onlyDAO {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    ds.drops[dropId].salePrice = price;
  }


  // Check if minting with incorrect value drains user's funds
  function mint(uint256 dropId, uint256 quantity, uint256 ethValue, uint256 tokenValue) external returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    require(ds.dropData[dropId].saleActive, 'Cannot purchase');

    uint256 tokenIdCounter = ds.dropData[dropId].tokenIdCounter;
    if (ds.drops[dropId].totalQuantity > 0)
      require(tokenIdCounter.add(quantity).sub(1) <= ds.drops[dropId].totalQuantity, 'Sold out');
    if (ds.drops[dropId].saleToken == address(0))
      require(tokenValue == ds.drops[dropId].salePrice.mul(quantity), "Incorrect value"); 
    else {
      IERC20 token = IERC20(ds.drops[dropId].saleToken);

      require(ethValue <= token.balanceOf(msg.sender), "Not enough balance");
      require(ethValue == ds.drops[dropId].salePrice.mul(quantity), "Incorrect value");

      token.transfer(ds.daoAddress, ethValue);
    }

    uint256 tokenId = uint256(dropId.mul(1000000000)) + uint256(tokenIdCounter);
    ds.dropData[dropId].tokenIdCounter = tokenIdCounter.add(quantity);

    return tokenId;
  }


  // Check if minting with incorrect value drains user's funds
  function mintReserve(uint256 dropId, uint256 quantity) external onlyDAO returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    require(ds.dropData[dropId].saleActive, 'Cannot purchase');

    uint256 reserveIdCounter = ds.dropData[dropId].reserveIdCounter;
    require(ds.drops[dropId].reserveQuantity > 0, 'None reserved');
    require(reserveIdCounter.add(quantity).sub(1) <= ds.drops[dropId].reserveQuantity, 'All minted');

    uint256 tokenId = uint256(dropId.mul(1000000000)) + uint256(reserveIdCounter);
    ds.dropData[dropId].reserveIdCounter = reserveIdCounter.add(quantity);

    if (ds.dropData[dropId].reserveIdCounter >= ds.drops[dropId].reserveQuantity) {
      ds.dropData[dropId].reserveMintCompleted = true;
    }

    return tokenId;
  }

  // Returned random word from Chainlink VRF
  function setVrfResult(uint256 dropId, uint256 randomWord, uint256 maxBulk, string memory action) external onlyVRF {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    if (keccak256(bytes(action)) == keccak256(bytes('requested'))) {
      ds.dropData[dropId].vrfPending = true;
    } else if (keccak256(bytes(action)) == keccak256(bytes('reset'))) {
      ds.dropData[dropId].vrfPending = false;
    } else if (keccak256(bytes(action)) == keccak256(bytes('fulfilled'))) {
      uint256 tokenCounter = ds.dropData[dropId].tokenIdCounter;
      uint256 remainder = tokenCounter.sub(ds.dropData[dropId].randomizedTraitsCounter);
      remainder = remainder > maxBulk ? maxBulk : remainder;

      if (ds.dropData[dropId].vrfPending &&
          ds.dropData[dropId].reserveMintCompleted &&
          remainder > 0) {
        ds.randomizedTraits[ds.dropData[dropId].randomizedTraitsCounter] = randomWord;
        ds.dropData[dropId].randomizedTraitsCounter = ds.dropData[dropId].randomizedTraitsCounter.add(remainder);
        ds.dropData[dropId].vrfPending = false;
      }
    }
  }
}

