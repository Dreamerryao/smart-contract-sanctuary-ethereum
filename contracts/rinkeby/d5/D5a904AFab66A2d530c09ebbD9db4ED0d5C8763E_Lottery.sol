pragma solidity ^0.8.0;

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "hardhat/console.sol";
import "./TicketInterface.sol";

contract Lottery is VRFConsumerBase, Ownable {
    // chainlink rng params
    address private vrfCoordinator;
    bytes32 private keyHash;
    uint256 private fee;

    TicketInterface private ticketContract;

    // start and end token ids for current lottery in progress
    uint256 private tokenIdStart;
    uint256 private tokenIdEnd;
    using Counters for Counters.Counter;
    Counters.Counter private lotteryId;

    uint256 private currentLotteryPot;
    uint256 private settlementPot;

    enum State {
        STARTED,
        DRAW_START,
        DRAW_END
    }
    State state;

    // reward params
    uint8 private DECIMALS_REWARD = 6;
    uint8 private INVALID_WINNING_COMBO_ID = 25;
    // array maintaining the reward probabilities for each of the 9 possible winning combinations
    uint256[9] private winningComboRewards;
    mapping(uint256 => uint256) private tokenIdToReward;
    mapping(uint256 => bool) private tokenIdToHasClaimed;
    mapping(uint256 => TicketInterface.TicketAttributes)
        private lotteryIdToWinningTicket;

    event LotteryStateChanged(State from, State to);
    event LotteryDrawn(
        uint256 lotteryId,
        uint8[] winningWhites,
        uint8 winningPowerNumber
    );
    event RewardClaimed(uint256 tokenId, uint256 reward, address holder);

    constructor(
        address _vrfCoordinator,
        address _linkToken,
        bytes32 _keyHash,
        uint256 _fee,
        address _ticketContractAddr
    )
        VRFConsumerBase(
            _vrfCoordinator, // VRF Coordinator
            _linkToken // LINK Token
        )
    {
        console.log("Lottery contract initialized.");

        vrfCoordinator = _vrfCoordinator;
        keyHash = _keyHash;
        fee = _fee;
        ticketContract = TicketInterface(_ticketContractAddr);
        tokenIdStart = 1;
        tokenIdEnd = 0;
        state = State.STARTED;

        // start lottery ids from 1
        lotteryId.increment();

        /**

            Reward is inversely proportional to the probability of getting the combination
            num_whites = 20, max_power = 5
            5_WHITES_POWER =  0.788297       1 in 77520
            5_WHITES =        0.197074       1 in 19380           (5c5 x 4c1)            4 / 77520
            4_WHITES_POWER =  0.010510       1 in 1033.6          (5c4 x 15c1 x 1)       75 / 77520
            4_WHITES =        0.003333       1 in 258.4           (5c4 x 15c1 x 4c1)     300 / 77520
            3_WHITES_POWER =  0.000952       1 in 73.8285         (5c3 x 15c2 x 1)       1050 / 77520
            3_WHITES =        0.000238       1 in 18.4571         (5c3 x 15c2 x 4c1)      4200 / 77520
            2_WHITES_POWER =  0.000219       1 in 17.0373         (5c2 x 15c3 x 1)       4550 / 77520
            1_WHITE_POWER =   0.000146       1 in 11.3582         (5c1 x 15c4 x 1)       6825 / 77520
            POWER =           0.000332       1 in 25.8141         (15c5 x 1)/ 77520       3003 / 77520

            TOTAL = 98338.4952
         */

        // todo: compute the probablities based on numWhites, maxWhite, maxPower from TicketInterface
        winningComboRewards = [
            788297,
            197074,
            10510,
            3333,
            952,
            238,
            219,
            146,
            332
        ];
    }

    function draw() public payable onlyOwner returns (bytes32 requestId) {
        require(state == State.STARTED, "Lottery state is not in STARTED");
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        setState(State.DRAW_START);
        requestId = requestRandomness(keyHash, fee);
        // take a snapshot of last token id
        tokenIdEnd = ticketContract.getCurrentTokenId();
        currentLotteryPot = ticketContract.getLotteryPot();
        console.log("Randomness request executed");
    }

    // todo: make this function more lightweight to avoid errors during callback
    // Callback function used by VRF Coordinator once random number is generated
    function fulfillRandomness(bytes32, uint256 randomness) internal override {
        require(
            msg.sender == vrfCoordinator,
            "Fulfilment only permitted by Coordinator"
        );
        TicketInterface.TicketAttributes memory ticketAttr = ticketContract
            .generatePowerBlockNumbers(randomness);
        lotteryIdToWinningTicket[lotteryId.current()] = ticketAttr;
        emit LotteryDrawn(
            lotteryId.current(),
            ticketAttr.whiteNumbers,
            ticketAttr.powerNumber
        );
        setState(State.DRAW_END);
    }

    // update the reward for each tokenId based on the pot and assert that winningComboId is correct
    // todo: directly pass in tokenIdToReward map here to reduce gas
    function updateTokenIdToReward(
        uint256[][9] calldata winningComboIdToTokenIds
    ) public onlyOwner {
        require(state == State.DRAW_END, "Lottery has not been drawn yet");
        uint256 totalReward = 0;
        // iterate over all winning combos
        for (uint8 i = 0; i < 9; i++) {
            uint256 numWinners = winningComboIdToTokenIds[i].length;
            uint256 rewardPercent = winningComboRewards[i];
            for (uint256 j = 0; j < numWinners; j++) {
                uint256 tokenId = winningComboIdToTokenIds[i][j];
                uint8 winningComboId = getWinningComboId(
                    tokenId,
                    lotteryId.current()
                );
                require(winningComboId == i, "Invalid data passed");
                require(
                    tokenId < tokenIdEnd && tokenId >= tokenIdStart,
                    "Invalid token id for current lottery in progress"
                );
                // todo: use safe math here to avoid overflow
                tokenIdToReward[tokenId] =
                    ((rewardPercent * currentLotteryPot) /
                        (10**DECIMALS_REWARD)) /
                    numWinners;
                totalReward += tokenIdToReward[tokenId];
            }
        }
        ticketContract.reduceLotteryPot(totalReward);
        settlementPot += totalReward;

        // ends current lottery and changes state to prepare for next draw
        tokenIdStart = tokenIdEnd;
        currentLotteryPot = 0;
        lotteryId.increment();
        setState(State.STARTED);
    }

    function claim(uint256 tokenId) public payable returns (uint256) {
        uint256 reward = _claim(tokenId);
        if (reward != 0) {
            ticketContract.payReward(payable(msg.sender), reward);
        }
        return reward;
    }

    function claimBatch(uint256[] memory tokenIds)
        public
        payable
        returns (uint256)
    {
        uint256 reward = 0;
        for (uint8 i = 0; i < tokenIds.length; i++) {
            reward += _claim(tokenIds[i]);
        }
        if (reward != 0) {
            ticketContract.payReward(payable(msg.sender), reward);
        }
        return reward;
    }

    /**
    9 different ways to win the power ball
    https://www.powerball.com/games/home#:~:text=Select%20five%20numbers%20from%201,the%209%20Ways%20to%20Win.

    Get the index of winning combination based on the ticket attributes. This is used to assert that we are not setting the 
    reward for incorrect winning combination
     */
    function getWinningComboId(uint256 _tokenId, uint256 _lotteryId)
        public
        view
        returns (uint8 winningComboId)
    {
        TicketInterface.TicketAttributes memory ticketAttr = ticketContract
            .getTicketAttributes(_tokenId);
        uint8[] memory whites = ticketAttr.whiteNumbers;
        uint8 powerBall = ticketAttr.powerNumber;
        TicketInterface.TicketAttributes
            memory winningTicket = lotteryIdToWinningTicket[_lotteryId];
        uint8[] memory winningWhites = winningTicket.whiteNumbers;
        uint8 winningPowerBall = winningTicket.powerNumber;

        uint256 matches = 0;
        for (uint256 i = 0; i < winningWhites.length; i++) {
            for (uint256 j = 0; j < whites.length; j++) {
                if (winningWhites[i] == whites[j]) {
                    matches++;
                    break;
                }
            }
        }
        bool isPowerBallMatch = winningPowerBall == powerBall;

        if (matches == 5 && isPowerBallMatch) {
            winningComboId = 0;
        } else if (matches == 5) {
            winningComboId = 1;
        } else if (matches == 4 && isPowerBallMatch) {
            winningComboId = 2;
        } else if (matches == 4) {
            winningComboId = 3;
        } else if (matches == 3 && isPowerBallMatch) {
            winningComboId = 4;
        } else if (matches == 3) {
            winningComboId = 5;
        } else if (matches == 2 && isPowerBallMatch) {
            winningComboId = 6;
        } else if (matches == 1 && isPowerBallMatch) {
            winningComboId = 7;
        } else if (isPowerBallMatch) {
            winningComboId = 8;
        } else {
            winningComboId = INVALID_WINNING_COMBO_ID;
        }
    }

    function _claim(uint256 tokenId) internal returns (uint256) {
        require(
            (tokenId >= tokenIdEnd || tokenId < tokenIdStart),
            "Lottery has not been drawn for this ticket yet"
        );
        require(
            msg.sender == ticketContract.ownerOf(tokenId),
            "Claim can only be made by owner"
        );
        // todo: consider burning all invalid token ids or enforce rewards to be claimed within claim window
        require(
            tokenIdToHasClaimed[tokenId] == false,
            "Invalid duplicate claim"
        );
        uint256 reward = tokenIdToReward[tokenId];
        if (reward != 0) {
            settlementPot -= reward;
            tokenIdToHasClaimed[tokenId] = true;
            emit RewardClaimed(tokenId, reward, msg.sender);
        }
        return reward;
    }

    function setState(State _newState) internal {
        State prevState = state;
        state = _newState;
        emit LotteryStateChanged(prevState, _newState);
    }

    /**
    getter functions to read lottery model params
     */

    function getLotteryId() public view returns (uint256) {
        return lotteryId.current();
    }

    function getTokenIdStart() public view returns (uint256) {
        return tokenIdStart;
    }

    function getTokenIdEnd() public view returns (uint256) {
        return tokenIdEnd;
    }

    function getState() public view returns (State) {
        return state;
    }

    function getReward(uint256 tokenId) public view returns (uint256) {
        return tokenIdToReward[tokenId];
    }

    function hasClaimedReward(uint256 tokenId) public view returns (bool) {
        return tokenIdToHasClaimed[tokenId];
    }

    function getWinningComboRewards() public view returns (uint256[9] memory) {
        return winningComboRewards;
    }

    function getSettlementPot() public view returns (uint256) {
        return settlementPot;
    }

    function getCurrentLotteryPot() public view returns (uint256) {
        return currentLotteryPot;
    }

    function getWinningTicket(uint256 _lotteryId)
        public
        view
        returns (TicketInterface.TicketAttributes memory)
    {
        return lotteryIdToWinningTicket[_lotteryId];
    }

    /**
    setter functions to tweak lottery model params which could be set by governance
    */

    function setKeyhash(bytes32 _keyhash) external onlyOwner {
        keyHash = _keyhash;
    }

    function setFee(uint256 _fee) external onlyOwner {
        fee = _fee;
    }
}

