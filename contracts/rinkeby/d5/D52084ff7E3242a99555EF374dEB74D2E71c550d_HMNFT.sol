    }
    
    // Total amount of tokens minted
    function totalTokensMinted() public view returns (uint256) {
        return _tokenIds.current();
    }

    function mintNFT(string memory _name) public payable {
        uint totalMinted = _tokenIds.current();

        // Check if all 7777 tokens are minted
        if (totalMinted > 7776) {
            revert('The collection is already minted');
        }
        // Check if the tx value is correct
        if (msg.value != 0.01 ether) {
            revert('Tx value is not correct');
        }
        // Check if sender has reached the limit
        if (balanceOf(msg.sender) > 9) {
            revert("Max 10 NFTs per wallet");
        }
        
        _tokenIds.increment();
        uint newTokenID = _tokenIds.current();
        _safeMint(msg.sender, newTokenID);
    }
    
    function withdraw() public payable onlyOwner {
        uint balance = address(this).balance;
        require(balance > 0, "No ether left to withdraw");

        (bool success, ) = (msg.sender).call{value: balance}("");
        require(success, "Transfer failed.");
    }

