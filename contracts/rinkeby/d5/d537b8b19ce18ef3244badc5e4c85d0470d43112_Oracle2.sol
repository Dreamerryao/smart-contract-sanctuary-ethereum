contract KittyPuppy is IKittyPuppy, ERC721Enumerable, Ownable, Pausable {
    using SafeMath for uint256;
    uint256 public constant MINT_PRICE = .01 ether;
    uint256 public MAX_TOKENS = 80000;
    uint256 public PAID_TOKENS = 10000;
    uint16 public minted;

    mapping(uint256 => KittyOrPuppy) public tokenTraits;
    mapping(uint256 => uint256) public existingCombinations;

    uint8[][16] public rarities;
    uint8[][16] public aliases;

    Fish public fish;
    IPond public pond;
    IOracle public oracle;

    // 需要删除
    function modifyMinted(uint16 _minted) external onlyOwner {
        minted = _minted;
    }

    constructor(address _fish, address _oracle) ERC721('KittyPuppy Game', 'KITTYPUPPY') {
        fish = Fish(_fish);
        oracle = IOracle(_oracle);

        // kitty
        // fur
        rarities[0] = [15, 50, 200, 250, 255];
        aliases[0] = [4, 4, 4, 4, 4];
        // head
        rarities[1] = [190, 215, 240, 100, 110, 135, 160, 185, 80, 210, 235, 240, 80, 80, 100, 100, 100, 245, 250, 255];
        aliases[1] = [1, 2, 4, 0, 5, 6, 7, 9, 0, 10, 11, 17, 0, 0, 0, 0, 4, 18, 19, 19];
        // ears
        rarities[2] = [255, 30, 60, 60, 150, 156];
        aliases[2] = [0, 0, 0, 0, 0, 0];
        // eyes
        rarities[3] = [221, 100, 181, 140, 224, 147];
        aliases[3] = [1, 2, 5, 0, 1, 7];
        // nose
        rarities[4] = [175, 100, 40, 250, 115, 100, 185, 175, 180, 255];
        aliases[4] = [3, 0, 4, 6, 6, 7, 8, 8, 9, 9];
        // mouth
        rarities[5] = [80, 225, 227, 228, 112, 240, 64, 160, 167, 217, 171, 64, 240, 126, 80, 255];
        aliases[5] = [1, 2, 3, 8, 2, 8, 8, 9, 9, 10, 13, 10, 13, 15, 13, 15];
        // neck
        rarities[6] = [255];
        aliases[6] = [0];
        // alphaIndex
        rarities[7] = [8, 160, 73, 255];
        aliases[7] = [2, 3, 3, 3];

        // puppy
        // fur
        rarities[8] = [210, 90, 9, 9, 9, 150, 9, 255, 9];
        aliases[8] = [5, 0, 0, 5, 5, 7, 5, 7, 5];
        // head
        rarities[9] = [255];
        aliases[9] = [0];
        // ears
        rarities[10] = [255];
        aliases[10] = [0];
        // eyes
        rarities[11] = [135, 177, 219, 141, 183];
        aliases[11] = [1, 2, 3, 4, 5];
        // nose
        rarities[12] = [255];
        aliases[12] = [0];
        // mouth
        rarities[13] = [239, 244, 249, 234, 234, 234, 234, 234, 234, 234, 130, 255, 247];
        aliases[13] = [1, 2, 11, 0, 11, 11, 11, 11, 11, 11, 11, 11, 11];
        // neck
        rarities[14] = [75, 180, 165, 120, 60, 150, 105, 195, 45, 225, 75, 45, 195, 120, 255];
        aliases[14] = [1, 9, 0, 0, 0, 0, 0, 0, 0, 12, 0, 0, 14, 12, 14];
        // alphaIndex
        rarities[15] = [8, 160, 73, 255];
        aliases[15] = [2, 3, 3, 3];
    }

    function mint(uint256 amount, bool stake) external payable whenNotPaused {
        require(tx.origin == _msgSender(), 'Only EOA');
        require(minted + amount <= MAX_TOKENS, 'All tokens minted');
        require(!stake || minted > PAID_TOKENS / 10, 'Stake disabled');
        require(amount > 0 && amount <= 10, 'Invalid mint amount');
        if (minted < PAID_TOKENS) {
            require(minted + amount <= PAID_TOKENS, 'All tokens on-sale already sold');
            require(amount * MINT_PRICE == msg.value, 'Invalid payment amount');
        } else {
            require(msg.value == 0);
        }

        uint256 totalFishCost = 0;
        uint16[] memory tokenIds = stake ? new uint16[](amount) : new uint16[](0);
        uint256 seed;
        for (uint256 i = 0; i < amount; i++) {
            minted++;
            seed = random(minted);
            generate(minted, seed);
            address recipient = selectRecipient(seed);
            if (!stake || recipient != _msgSender()) {
                _safeMint(recipient, minted);
            } else {
                _safeMint(address(pond), minted);
                tokenIds[i] = minted;
            }
            if (minted > 10000 && minted <= 30000) {
                uint256 fishPricePerUsd = oracle.getFishPricePerUsd();
                totalFishCost += fishPricePerUsd.mul(120);
            } else if (minted > 30000) {
                uint256 fishPricePerUsd = oracle.getFishPricePerUsd();
                totalFishCost += fishPricePerUsd.mul(150);
            }
        }
        if (totalFishCost > 0) fish.burn(_msgSender(), totalFishCost);
        if (stake) pond.addManyToPondAndYard(_msgSender(), tokenIds);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        if (_msgSender() != address(pond))
            require(_isApprovedOrOwner(_msgSender(), tokenId), 'ERC721: transfer caller is not owner nor approved');
        _transfer(from, to, tokenId);
    }

    function generate(uint256 tokenId, uint256 seed) internal returns (KittyOrPuppy memory t) {
        t = selectTraits(seed);
        if (existingCombinations[structToHash(t)] == 0) {
            tokenTraits[tokenId] = t;
            existingCombinations[structToHash(t)] = tokenId;
            return t;
        }
        return generate(tokenId, random(seed));
    }

    function selectRecipient(uint256 seed) internal view returns (address) {
        if (minted <= PAID_TOKENS || ((seed >> 245) % 10) != 0) return _msgSender();
        address thief = pond.randomPuppyOwner(seed >> 144);
        if (thief == address(0x0)) return _msgSender();
        return thief;
    }

    function selectTraits(uint256 seed) internal view returns (KittyOrPuppy memory t) {
        t.isKitty = (seed & 0xFFFF) % 10 != 0;
        uint8 shift = t.isKitty ? 0 : 8;
        seed >>= 16;
        t.fur = selectTrait(uint16(seed & 0xFFFF), 0 + shift);
        seed >>= 16;
        t.head = selectTrait(uint16(seed & 0xFFFF), 1 + shift);
        seed >>= 16;
        t.ears = selectTrait(uint16(seed & 0xFFFF), 2 + shift);
        seed >>= 16;
        t.eyes = selectTrait(uint16(seed & 0xFFFF), 3 + shift);
        seed >>= 16;
        t.nose = selectTrait(uint16(seed & 0xFFFF), 4 + shift);
        seed >>= 16;
        t.mouth = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
        seed >>= 16;
        t.neck = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
        seed >>= 16;
        t.alphaIndex = selectTrait(uint16(seed & 0xFFFF), 7 + shift);
    }

    function selectTrait(uint16 seed, uint8 traitType) internal view returns (uint8) {
        uint8 trait = uint8(seed) % uint8(rarities[traitType].length);
        if (seed >> 8 < rarities[traitType][trait]) return trait;
        return aliases[traitType][trait];
    }

    function structToHash(KittyOrPuppy memory s) internal pure returns (uint256) {
        return
            uint256(bytes32(abi.encodePacked(s.isKitty, s.fur, s.head, s.eyes, s.mouth, s.neck, s.ears, s.alphaIndex)));
    }

    function random(uint256 seed) internal view returns (uint256) {
        return oracle.random(seed);
    }

    function getTokenTraits(uint256 tokenId) external view override returns (KittyOrPuppy memory) {
        return tokenTraits[tokenId];
    }

    function setOracle(address _oracle) external onlyOwner {
        oracle = IOracle(_oracle);
    }

    function setPond(address _barn) external onlyOwner {
        pond = IPond(_barn);
    }

    function setPaidTokens(uint256 _paidTokens) external onlyOwner {
        PAID_TOKENS = _paidTokens;
    }

    function getPaidTokens() external view override returns (uint256) {
        return PAID_TOKENS;
    }

    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), 'ERC721Metadata: URI query for nonexistent token');
        return super.tokenURI(tokenId);
    }
}


