// File: contracts\mocks\MockOracle.sol

pragma solidity 0.8.9;



contract MockOracle is IPriceOracle, AccessControl {
    mapping(ITToken => uint256) public prices;

    modifier onlyOwner() {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "Access is denied");
        _;
    }

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function setPriceOfUnderlying(ITToken tToken, uint256 priceOfUnderlying)
        external
        onlyOwner
    {
        prices[tToken] = priceOfUnderlying;
    }

    /**
     * @notice Get the underlying price of a tToken asset
     * @param tToken The tToken to get the underlying price of
     * @return The underlying asset price mantissa (scaled by 1e18).
     *  Zero means the price is unavailable.
     */
    function getUnderlyingPrice(ITToken tToken)
        external
        view
        override
        returns (uint256)
    {
        return prices[tToken];
    }
}