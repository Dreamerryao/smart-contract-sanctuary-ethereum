pragma solidity ^0.8.0;

import {IERC165} from "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import {ERC165} from "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {IERC721} from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import {IERC1155} from "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./IContractWallet.sol";

contract FeeSplitContractWallet is IContractWallet, Ownable, ERC165 {
  using SafeMath for uint256;

  // 10000 => 100% (1% == 100 での計算)
  uint16 private constant _MAX_PERCENT = 10000;
  address private _sbinftFeeAddress;
  uint16 private _sbinftFeePercent;
  bool private _canRegister = true;

  //収益分配情報
  mapping(address => SplitInfo) private _splitInfo;
  mapping(address => bool) private _ownershipTransferRight;

  struct SplitInfo {
    address[] receivers;
    uint16[] receiversPercent;
  }

  /**
   * @dev ソースコードの利用の権利的なSBINFTのFeeを2.5%(250) deploy時に定義しておく
   *
   * Requirements:
   *
   * Emits a {WalletInitialized} event.
   * Emits a {SplitInfomationRegistered} event.
   */
  function initialize(
    address newOwner,
    address sbinftFeeAddress,
    uint16 sbinftFeePercent,
    address[] calldata receivers,
    uint16[] calldata receiversPercent
  ) external override onlyOwner {
    require(
      _canRegister,
      "FeeSplitContractWallet: Parameters set is Completed"
    );
    // 50%以上はsbinftFeePercentを指定できない
    require(
      sbinftFeePercent > 0 && sbinftFeePercent <= (_MAX_PERCENT / 2),
      "FeeSplitContractWallet: Platform fee in unacceptable"
    );

    _sbinftFeeAddress = payable(sbinftFeeAddress);
    _sbinftFeePercent = sbinftFeePercent;
    _registerSplitInfo(newOwner, receivers, receiversPercent);

    transferOwnership(newOwner);
    _canRegister = false;

    emit WalletInitialized(newOwner, sbinftFeeAddress, sbinftFeePercent);
  }

  /**
   * @dev ロイヤルティ分配情報の登録 1% == 100 での計算で配列の要素登録
   *
   * Requirements:
   *
   * Emits a {SplitInfomationRegistered} event.
   */
  function _registerSplitInfo(
    address newOwner,
    address[] calldata receivers,
    uint16[] calldata receiversPercent
  ) internal {
    uint256 totalPercent = 0;
    for (uint256 idx = 0; idx < receiversPercent.length; idx++) {
      totalPercent = totalPercent.add(receiversPercent[idx]);
    }

    require(
      receivers.length == receiversPercent.length,
      "FeeSplitContractWallet: Not matching Element num"
    );
    require(
      totalPercent == _MAX_PERCENT,
      "FeeSplitContractWallet: The total is not 100 Percent"
    );

    SplitInfo memory splitInfo = SplitInfo({
      receivers: receivers,
      receiversPercent: receiversPercent
    });
    _splitInfo[newOwner] = splitInfo;

    emit SplitInfomationRegistered(
      splitInfo.receivers,
      splitInfo.receiversPercent
    );
  }

  /**
   * @dev 移転権利のあるアドレスを登録(鍵紛失時用)
   */
  function updateSigner(address newSigner) external onlyOwner {
    _ownershipTransferRight[newSigner] = true;
  }

  /**
   * @dev 移転権利のあるアドレスを削除(鍵紛失時用)
   */
  function deleteSigner(address newSigner) external onlyOwner {
    delete _ownershipTransferRight[newSigner];
  }

  /**
   * @dev アドレスに移転権利があるかを確認(鍵紛失時用)
   */
  function getSigner(address newSigner) public view returns (bool) {
    return _ownershipTransferRight[newSigner];
  }

  /**
   * @dev 鍵紛失時のコントラクトオーナー権の書き換え
   *
   * Requirements:
   *
   */
  function transferWalletOwnership(address newOwner) external {
    require(
      _ownershipTransferRight[_msgSender()],
      "FeeSplitContractWallet: Don't have permission to execute"
    );
    _splitInfo[newOwner] = _splitInfo[owner()];
    _transferOwnership(newOwner);
  }

  /**
   * @dev onERC721Received
   */
  function onERC721Received(
    address operator,
    address from,
    uint256 tokenId,
    bytes calldata data
  ) external pure override returns (bytes4) {
    return IERC721Receiver.onERC721Received.selector;
  }

  /**
   * @dev onERC1155Received
   */
  function onERC1155Received(
    address,
    address,
    uint256,
    uint256,
    bytes memory
  ) public pure override returns (bytes4) {
    return IERC1155Receiver.onERC1155Received.selector;
  }

  /**
   * @dev onERC1155BatchReceived
   */
  function onERC1155BatchReceived(
    address operator,
    address from,
    uint256[] calldata ids,
    uint256[] calldata values,
    bytes calldata data
  ) external pure override returns (bytes4) {
    return IERC1155Receiver.onERC1155BatchReceived.selector;
  }

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(ERC165, IERC165)
    returns (bool)
  {
    return
      interfaceId == type(IContractWallet).interfaceId ||
      super.supportsInterface(interfaceId);
  }

  /**
   * @dev depositNativeToken
   *
   * Emits a {NativeTokenDeposited} event.
   */
  function depositNativeToken() external payable override onlyOwner {
    emit NativeTokenDeposited(_msgSender(), address(this), msg.value);
  }

  /**
   * @dev depositERC20
   */
  function depositERC20(address tokenAddress, uint256 amount)
    external
    override
    onlyOwner
  {
    IERC20(tokenAddress).transferFrom(_msgSender(), address(this), amount);
  }

  /**
   * @dev depositERC721
   */
  function depositERC721(address tokenAddress, uint256 tokenId)
    external
    override
    onlyOwner
  {
    IERC721(tokenAddress).safeTransferFrom(
      _msgSender(),
      address(this),
      tokenId
    );
  }

  /**
   * @dev depositERC1155
   */
  function depositERC1155(
    address tokenAddress,
    uint256 tokenId,
    uint256 amount
  ) external override onlyOwner {
    bytes memory data;
    IERC1155(tokenAddress).safeTransferFrom(
      _msgSender(),
      address(this),
      tokenId,
      amount,
      data
    );
  }

  /**
   * @dev withdrawNativeTokenForAllBalance
   *
   * Requirements:
   *
   * Emits a {AllSplitNativeTokenWithdrawn} event.
   */
  function withdrawNativeTokenForAllBalance()
    external
    payable
    override
    onlyOwner
  {
    uint256 tokenAmount = address(this).balance;
    SplitInfo memory splitInfo = _splitInfo[_msgSender()];
    require(
      tokenAmount != 0,
      "FeeSplitContractWallet: Don't have NativeToken, Balance error!"
    );
    require(
      splitInfo.receivers.length != 0,
      "FeeSplitContractWallet: Not Registed SplitInfomation,"
    );

    uint256 sbinftFee = tokenAmount.mul(_sbinftFeePercent).div(_MAX_PERCENT);
    uint256 totalRoyaltySplitFee = tokenAmount.sub(sbinftFee);

    // SBINFTへ送金
    payable(_sbinftFeeAddress).transfer(sbinftFee);
    // ロイヤリティ送金
    for (uint256 idx = 0; idx < splitInfo.receivers.length; idx++) {
      uint256 splitFee = totalRoyaltySplitFee
        .mul(splitInfo.receiversPercent[idx])
        .div(_MAX_PERCENT);
      payable(splitInfo.receivers[idx]).transfer(splitFee);
    }

    emit AllSplitNativeTokenWithdrawn(
      address(this),
      splitInfo.receivers,
      tokenAmount
    );
  }

  /**
   * @dev withdrawERC20TokenForAllBalance
   *
   * Requirements:
   *
   * Emits a {AllSplitERC20Withdrawn} event.
   */
  function withdrawERC20TokenForAllBalance(address tokenAddress)
    external
    override
    onlyOwner
  {
    uint256 tokenAmount = IERC20(tokenAddress).balanceOf(address(this));
    SplitInfo memory splitInfo = _splitInfo[_msgSender()];
    require(
      tokenAmount != 0,
      "FeeSplitContractWallet: Don't have NativeToken, Balance error!"
    );
    require(
      splitInfo.receivers.length != 0,
      "FeeSplitContractWallet: Not Registed SplitInfomation,"
    );
    
    uint256 sbinftFee = tokenAmount.mul(_sbinftFeePercent).div(_MAX_PERCENT);
    uint256 totalRoyaltySplitFee = tokenAmount.sub(sbinftFee);

    // SBINFTへ送金
    IERC20(tokenAddress).transfer(_sbinftFeeAddress, sbinftFee);
    // ロイヤリティ送金
    for (uint256 idx = 0; idx < splitInfo.receivers.length; idx++) {
      uint256 splitFee = totalRoyaltySplitFee
        .mul(splitInfo.receiversPercent[idx])
        .div(_MAX_PERCENT);
      IERC20(tokenAddress).transfer(splitInfo.receivers[idx], splitFee);
    }
    if(IERC20(tokenAddress).balanceOf(address(this)) != 0){
      IERC20(tokenAddress).transfer(
        splitInfo.receivers[splitInfo.receivers.length-1],
        IERC20(tokenAddress).balanceOf(address(this))
      );
    }

    emit AllSplitERC20Withdrawn(
      address(this),
      splitInfo.receivers,
      tokenAmount
    );
  }

  /**
   * @dev withdrawERC721Token
   */
  function withdrawERC721Token(address tokenAddress, uint256 tokenId)
    external
    override
    onlyOwner
  {
    IERC721(tokenAddress).safeTransferFrom(
      address(this),
      _msgSender(),
      tokenId
    );
  }

  /**
   * @dev withdrawERC1155Token
   */
  function withdrawERC1155Token(
    address tokenAddress,
    uint256 tokenId,
    uint256 amount
  ) external override onlyOwner {
    bytes memory data;
    IERC1155(tokenAddress).safeTransferFrom(
      address(this),
      _msgSender(),
      tokenId,
      amount,
      data
    );
  }
}

