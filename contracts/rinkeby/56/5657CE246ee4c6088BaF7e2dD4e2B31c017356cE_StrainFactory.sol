pragma solidity ^0.8.0;

import "./StrainContract.sol";
import "./StrainAdmin.sol";
import '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';

contract StrainFactory is StrainContract, StrainAdmin {
    uint256 public constant CREATION_LIMIT_GEN0 = 65535;
    uint256 public constant DNA_LENGTH = 16;
    uint256 public constant RANDOM_DNA_THRESHOLD = 7;
    uint256 internal _gen0Counter;

    bytes32 public constant MINT_ROLE = keccak256("STRAIN_MINT_ROLE");

    using SafeERC20 for IERC20;

    event StrainCreated(
        address owner,
        uint256 strainId,
        uint256 genes
    );

    event StrainUpdated(
        address owner,
        uint256 strainId
    );

    /// @dev rarity score
    uint256[4] public rarityScores = [
        0,
        20 * 10**9,
        50 * 10**9,
        100 * 10**9
    ];

    uint256 public rewardPerNFT;
    uint256 public rewardPerStage;

    address stakingContractAddr;
    address treasuryAddr;

    IERC20 strnToken;

    uint256 MAX_NFT_COUNT = 20;

    mapping(address => bool) paidUserList;

    struct StrainData {
        uint256 tokenId;
        string tokenURIString;
        uint256 genes;
        uint256 rarityScore; 
        uint64 createdTime;
        uint8 stage;
    }

    constructor(
        address _strnTokenAddr,
        address _mintRoleManager,
        address _treasuryAddr
    ) StrainContract() {
        require(_strnTokenAddr != address(0), "Zero address");
        require(_mintRoleManager != address(0), "Zero address");
        require(_treasuryAddr != address(0), "Zero address");
       
        rewardPerNFT = 2100;
        rewardPerStage = 700;        
        strnToken = IERC20(_strnTokenAddr);
        treasuryAddr = _treasuryAddr;

        _setupRole(MINT_ROLE, _mintRoleManager);
    }

    function strainsOf(address _owner) public view returns (uint256[] memory) {
        // get the number of strains owned by _owner
        uint256 ownerCount = ownerStrainCount[_owner];
        if (ownerCount == 0) {
            return new uint256[](0);
        }

        // iterate through each strainId until we find all the strains
        // owned by _owner
        uint256[] memory ids = new uint256[](ownerCount);
        uint256 i = 1;
        uint256 count = 0;
        while (count < ownerCount || i < strains.length) {
            if (strainToOwner[i] == _owner) {
                ids[count] = i;
                count += 1;
            }
            i += 1;
        }

        return ids;
    }

    function getOwnedStrains(address _owner) public view returns (StrainData[] memory) {
        // get the number of strains owned by _owner
        uint256 ownerCount = ownerStrainCount[_owner];
        if (ownerCount == 0) {
            return new StrainData[](0);
        }

        // iterate through each strainId until we find all the strains
        // owned by _owner
        StrainData[] memory strainDataList = new StrainData[](ownerCount);
        uint256 i = 1;
        uint256 count = 0;
        while (count < ownerCount || i < strains.length) {
            if (strainToOwner[i] == _owner) {
                Strain memory strain = strains[count];
                strainDataList[count] = StrainData({
                    tokenId: count,
                    tokenURIString: tokenURI(count),
                    genes: strain.genes,
                    rarityScore: strain.rarityScore, 
                    createdTime: strain.createdTime,
                    stage: strain.stage
                });
                count += 1;
            }
            i += 1;
        }

        return strainDataList;
    }

    function getGen0Count() public view returns (uint256) {
        return _gen0Counter;
    }

    /// @dev throws if @param _genes is valid genes
    modifier onlyValidGenes(uint256 _genes) {
        require(_genes/10**30==0, "Invalid genes");
        _;
    }

    modifier onlyStakingContract(address _addr) {
        require(stakingContractAddr != address(0) && stakingContractAddr == _addr, "Invalid Access Role");
        _;
    }

    function createStrainGen0(uint256 _genes)
        public
        onlyStrainCreator
        onlyValidGenes(_genes)
        returns (uint256)
    {
        require(_gen0Counter < CREATION_LIMIT_GEN0, "gen0 limit exceeded");

        _gen0Counter = _gen0Counter + 1;
        return _createStrain(msg.sender, _genes, "");
    }

    function payETH() public payable {
        require(!paidUserList[msg.sender], "Already Paid User");

        //  1000 FTM msg.value
        //  To test on rinkeby, set it as 0.01 eth
        require(msg.value >= 1e15, "Not enough value");
        payable(treasuryAddr).transfer(msg.value);

        paidUserList[msg.sender] = true;
    }

    function createStrain(
        address _owner,
        uint256 _genes,
        string memory _tokenURI
    )   
        public 
        onlyRole(MINT_ROLE)
    returns (uint256) {
        require(paidUserList[_owner], "No paid user");
        paidUserList[_owner] = false;
        
        return _createStrain(_owner, _genes, _tokenURI);
    }

    function _createStrain(
        address _owner,
        uint256 _genes,
        string memory _tokenURI  
    ) 
        internal
        onlyValidGenes(_genes)
        returns (uint256) 
    {
        uint8 stage = 0;
        uint256 strainCount = ownerStrainCount[_owner];
        require(strainCount <= MAX_NFT_COUNT, "Strain token count exceeded");

        uint256 rarityScore = calculateRarityScore(_genes, stage);

        Strain memory strain = Strain({
            genes: _genes,
            createdTime: uint64(block.timestamp),
            stage: stage,
            rarityScore: rarityScore
        });

        strains.push(strain);
        uint256 newStrainId = strains.length - 1;
        emit StrainCreated(_owner, newStrainId, _genes);

        _transfer(address(0), _owner, newStrainId);
        strnToken.transfer(_owner, rewardPerNFT);

        setTokenURI(newStrainId, _tokenURI);

        return newStrainId;
    }

    function getStrainCount(address _addr) public view returns (uint256 count) {
        count = ownerStrainCount[_addr];
    }

    function calculateRarityScore (
        uint256 _genes,
        uint8 _stage
    ) private view returns (uint256) {
        uint number = _genes;
        uint256 raritySum = rarityScores[_stage];
        while (number > 0) {
            uint8 digit = uint8(number % 100);
            raritySum += digit;
            number = number / 100;
        }

        return raritySum;
    }

    function setStakingContract(address _addr) 
        public 
        onlyOwner 
    {
        require(_addr != address(0), "Invalid Address");

        stakingContractAddr = _addr;
    }

    function upgradeStage (
        uint256 _genes,
        uint256 _strainId,
        address _owner,
        string memory _tokenURI
    )   public
        onlyValidGenes(_genes)
        onlyStakingContract(msg.sender)
    {
        Strain storage strain = strains[_strainId];

        require(strain.stage < 3, "Cannot Upgrade Bud");

        strain.genes = _genes;
        strain.stage += 1;
        strain.rarityScore = calculateRarityScore(_genes, strain.stage);

        setTokenURI(_strainId, _tokenURI);

        emit StrainUpdated(_owner, _strainId);
    }
}

