pragma solidity ^0.8.0;
import "@openzeppelin/contracts/access/Ownable.sol";

contract StrainAdmin is Ownable {
    mapping(address => uint256) addressToStrainCreatorId;
    address[] strainCreators;

    event StrainCreatorAdded(address creator);
    event StrainCreatorRemoved(address creator);

    constructor() {
        // placeholder to reserve ID zero as an invalid value
        _addStrainCreator(address(0));

        // the owner should be allowed to create strains
        _addStrainCreator(owner());
    }

    modifier onlyStrainCreator() {
        require(isStrainCreator(msg.sender), "must be a strain creator");
        _;
    }

    function isStrainCreator(address _address) public view returns (bool) {
        return addressToStrainCreatorId[_address] != 0;
    }

    function addStrainCreator(address _address) external onlyOwner {
        require(_address != address(this), "contract address");
        require(_address != address(0), "zero address");

        _addStrainCreator(_address);
    }

    function _addStrainCreator(address _address) internal {
        addressToStrainCreatorId[_address] = strainCreators.length;
        strainCreators.push(_address);

        emit StrainCreatorAdded(_address);
    }

    function removeStrainCreator(address _address) external onlyOwner {
        uint256 id = addressToStrainCreatorId[_address];
        delete addressToStrainCreatorId[_address];
        delete strainCreators[id];

        emit StrainCreatorRemoved(_address);
    }

    function getStrainCreators() external view returns (address[] memory) {
        return strainCreators;
    }
}

