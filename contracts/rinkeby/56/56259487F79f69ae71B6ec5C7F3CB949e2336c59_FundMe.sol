/** @title A contract for crowd funding
 *  @author Muhammad Shumas
 *  @notice This contract is to demo a simple funding contract
 *  @dev    This implements price feed as our library

 */
contract FundMe {
    // When we are using solhint, It will give us warning if we dont specifiy visiblity explicitly.
    // Even though Internal is the default visibility for state variables, but solhint wants everything to be clear
    // uint num;

    // This will attach all the methods of PriceConvertor library to uint256

    // 6) Type Declarations
    using PriceConverter for uint256;

    // Minimum value of USD we want to allow
    // This value is in USD but solidity uses wei as a unit. So we will need to use decentralized oracle to convert ether / wei to USD
    // constant keyword means that a variable can not be reassigned a value
    // Also, variable marked as constant dont take storage spot saving gas
    // By convention, we use snake casing with all caps for constants (screaming snake case)
    uint256 public constant MINIMUM_USD = 50;

    // If we return 18 decimal places price from getPrice method then we will have to multiply minimum price to 1e18 as well
    // uint256 public MINIMUM_USD = 50 * 1e18;

    // uint256 public number = 5;
    // When writing a smart contract, you need to ensure that money is being sent to the contract and out of the contract as well. Payable does this for you, any function in Solidity with the modifier Payable ensures that the function can send and receive Ether.
    // Every time we deploy a contract they get an address and smart contract addresses can hold funds as well just like wallets

    // Array of address to keep track of all the funders
    // Its best practice to append s_ to storage variables
    address[] private s_funders;

    // Address to actual fund amount mapping
    mapping(address => uint256) private s_addressToAmountFunded;

    // Owner address
    // immutable variables are similar to constants but they can be assigned new value only inside constructors while constants can never be reassigned in any case
    // convention is to use snake casing with i appended to variable to indicate that its immutable
    address private immutable i_owner;

    AggregatorV3Interface private s_priceFeed;

    modifier onlyOwner() {
        // require(msg.sender == i_owner, "Sender is not i_owner");

        // We are using revert keyword which does same as require i.e revert transaction but with revert we throw custom error and its gas efficient than require. Because in require we use string which needs to be stored
        if (msg.sender != i_owner) {
            revert FundMe__NotOwner();
        }
        _;
    }

    /* 
    Order of Functions

    Ordering helps readers identify which functions they can call and to find the constructor and fallback definitions easier.

    Functions should be grouped according to their visibility and ordered:

    1) constructor

    2) receive function (if exists)

    3) fallback function (if exists)

    4) external

    5) public

    6) internal

    7) private
    
    */

    constructor(address priceFeedAddress) {
        // msg.sender will refer to address who was deployed this contract
        i_owner = msg.sender;
        console.log(msg.sender);
        s_priceFeed = AggregatorV3Interface(priceFeedAddress);
    }

    receive() external payable {
        fund();
    }

    fallback() external payable {
        fund();
    }

    /**
    *  @notice This function funds this contract
    *  @dev    This implements price feed as our library

    */
    function fund() public payable {
        // number = 10;
        // Want to be able to set a minimum amount in USD
        // Unit used in calculcations is wei
        // Lets say we want to set a minimum of 1 ether which can be sent when funding
        // Here we are making share that transaction value is greater than or equal to 1 ether
        // If the condition specified in require statment is not met then transaction is revered
        // What is reverting?
        // Undo any action before and send remaining gas back
        //  This means that in this function we are setting value of number = 10, if this require condition is not met then number will rever back to its previous value. Also the gas spend on setting value of 5 will NOT be reversed, only remaining gas will be sent back
        // Get conversion rate will convert ethers (which are in wei) to USD
        // require(
        //     getConversionRate(msg.value) >= MINIMUM_USD,
        //     "Didn't send enough funds"
        // ); // 1e18 == 1 * 10 ** 18 == 1000000000000000000

        // Because we have attached PriceConvertor library to uint 256. We can now use its methods
        // Will be same as getConversionRate(msg.value), getConverstionRate and all other methods inside PriceConvertor library will get the object to which its attached (in this case uint256) as first argument
        // So here msg.value.getConversionRate() will get msg.value as its first argument. If we pass any argument then it will be second not first

        // POSSIBLE IMPROVEMENT:
        // We can convert this require to rever with some error like we did above because here we are storing a string which costs gas

        require(
            msg.value.getConversionRate(s_priceFeed) >= MINIMUM_USD,
            "Didn't send enough funds"
        ); // 1e18 == 1 * 10 ** 18 == 1000000000000000000

        // msg.value will have 18 decimal places because wei is used as unit in calculations and 1 ether = 1000000000000000000

        // Adding successful funder to the list. msg.sender will be address of the wallet which made the transaction
        s_funders.push(msg.sender);

        // Adding address to amount mapping
        s_addressToAmountFunded[msg.sender] += msg.value;
    }

    // We have added onlyOwner modifier to the withdraw function (check below for details about modifiers)
    function withdraw() public payable onlyOwner {
        for (
            uint256 funderIndex = 0;
            funderIndex < s_funders.length;
            funderIndex++
        ) {
            address funder = s_funders[funderIndex];
            s_addressToAmountFunded[funder] = 0;
        }

        // Resetting the array
        s_funders = new address[](0);

        payable(msg.sender).transfer(address(this).balance);

        // Send
        // It returns a boolean wether funds were transferred succesfully or not
        // This transfer method is also not recommended because it is also capped at 2300 gas
        bool sendSuccess = payable(msg.sender).send(address(this).balance);
        // Since send returns a boolean we add a require so that transaction is reverted if it fails
        require(sendSuccess, 'Send failed');

        (bool callSuccess, ) = payable(msg.sender).call{
            value: address(this).balance
        }('');
        require(callSuccess, 'Call failed');
    }

    function cheaperWithdraw() public payable onlyOwner {
        // Reading s_funders once so that we dont have to read from stroage inside the loop
        address[] memory funders = s_funders;
        // Mappings cant be stored in memory

        for (
            uint256 funderIndex = 0;
            funderIndex < funders.length;
            funderIndex++
        ) {
            address funder = funders[funderIndex];
            s_addressToAmountFunded[funder] = 0;
        }
        // Resetting the array
        s_funders = new address[](0);

        // msg.sender or i_owner will be same address as we are using only owner modifier in this function
        (bool callSuccess, ) = i_owner.call{value: address(this).balance}('');
        require(callSuccess, 'Call failed');
    }

    // View / Pure

    function getOwner() public view returns (address){
         return i_owner;
    }
    function getFunder(uint256 index) public view returns (address){
         return s_funders[index];
    }
    function getAddressToAmountFunded(address funder) public view returns (uint256){
        return s_addressToAmountFunded[funder];
    }
    function getPriceFeed() public view returns(AggregatorV3Interface){
        return s_priceFeed;
    }
}



