pragma solidity ^0.8.7;

// In remix we were refercing it using github but in hardhat we need to install @chainlink/contracts before using it
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";


// Libraries are similar to smart contracts but they differ in some ways
// 1) Libraries can not have state variables
// 2) They can not send ethers
// 3) All the functions need to be internal

library PriceConverter {
    function getPrice(AggregatorV3Interface priceFeed) internal view returns (uint256) {
        // To get the price of ether in USD we will interact with chainlink oracle network via a refernce smart contract
        // And to interact with a smart contract that is not part of our project we need 2 things
        // ABI of smart contract
        // Address of smart contract (0x8A753747A1Fa494EC906cE90E9f37563A8AF630e)

        // Getting address is easier, we can get address from here (just go to network (in our case its rinkby and look for desired pair)):
        // https://docs.chain.link/docs/ethereum-addresses/

        // To get the abi we can use interfaces. If we compile interface of some contract we get its ABI. chainlink provides an npm package for this and we can use this (check solidity import statement we are using https://docs.chain.link/docs/get-the-latest-price/)

        // This will give us AggreatorV3Interface contract ABI with which we can interact with AggreatorV3 contract and get price ether price in USD
        // AggregatorV3Interface priceFeed = AggregatorV3Interface(
        //     0x8A753747A1Fa494EC906cE90E9f37563A8AF630e
        // );

        // Check interface to find out more (https://github.com/smartcontractkit/chainlink/blob/develop/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol)

        // This latestRoundData method returns many variables (we are kind of desctructuring like we do in JS)
        // It returns 5 variables so we are only specifying what we need and leaving others by comma
        (, int256 price, , , ) = priceFeed.latestRoundData();

        //    The reason we are * 1 ^ 10 is because the price will already be with 8 decimal places. We need to convert it into wei which has 18 decimal places so 10 + 8 will be 18 hence we multiply it by 10
        // We need to convert (or typecast) price from int256 to uint256 because msg.value is also uint 256

        // This will return price of 1 ether to usd (with 18 decimal places)
        return uint256(price * 1e10); // (1e10 means 1^10 or 1 ** 10 = 10000000000)
    }

    function getConversionRate(uint256 ethAmount,AggregatorV3Interface priceFeed)
        internal
        view
        returns (uint256)
    {
        uint256 ethPrice = getPrice(priceFeed);
        // we are dividing by 1 ^ 18 because getPrice returns 18 decimal places number to us and also ethAmount is with 18 decimal places (wei)
        // For example 1 ether = 3000 USD

        // Then get price will give us 3000_000000000000000000 (It wont have _ but . we have just replaced for udnerstanding it better) so it will have 18 decimal places

        // Now the price will match msg.value since it will be in wei and will have 18 decimal places as well
        // But we want to compare it with USD but this amount without dividing by 1e36or 1^36 will have addiiton 36 (because both ethAmount and ethPrice have addition 18 deimcal places 18+18 = 36) decimals then the actual USD value, thats why we are diving it


        uint256 ethAmountInUSD = (ethAmount * ethPrice) / 1e36;
        // alternatively we can do this:
        // When we do this we will have to multiply minimumUSD to 1^18 or 1 ** 18 because ethAmountInUSD will then have 18 decimal places
        // uint256 ethAmountInUSD = (ethAmount * ethPrice) / 1e18;

        return ethAmountInUSD;
    }
}

