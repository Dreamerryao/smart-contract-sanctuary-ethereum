import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "./interfaces/IMember.sol";
import "./interfaces/IMembership.sol";
import "./interfaces/IMembershipFactory.sol";
import "./structs/MembershipStruct.sol";
//DEV
import "hardhat/console.sol";

contract Membership is
    IMembership,
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable
{
    // Support multiple ERC20 tokens

    // Membership Information of a specific user
    mapping(address => MembershipStruct) private membershipInfoOfAddress;

    address private IMemberAddress;
    address private IMembershipFactoryAddress;

    event membershipUpdated(
        address membershipContractAddress,
        address user,
        string uid,
        uint256 membershipCreatedDate,
        uint256 membershipEndDate,
        uint256 membershipId,
        uint256 updatesPerYear,
        address collectionAddress
    );

    modifier checkIfMemberIsUser(address _user) {
        MembershipStruct memory membership = membershipInfoOfAddress[_user];
        require(
            membership.user != address(0),
            "Only the user with the membership can top up updates"
        );
        _;
    }

    /**
     * @dev initialize - Initializes the function for Ownable and Reentrancy.
     */
    function initialize(
        string memory uid,
        address _memberContractAddress,
        address _membershipFactoryAddress,
        address _userAddress,
        uint256 _membershipStartDate,
        uint256 _membershipEndedDate,
        uint256 _membershipId,
        uint256 updatesPerYear,
        uint256 _membershipPayedAmount,
        address nftCollection
    ) public initializer {
        __Context_init_unchained();
        __Ownable_init();
        __ReentrancyGuard_init();
        IMemberAddress = _memberContractAddress;
        IMembershipFactoryAddress = _membershipFactoryAddress;
        if (
            (keccak256(
                abi.encodePacked((IMember(IMemberAddress).getMember(uid).uid))
            ) != keccak256(abi.encodePacked((""))))
        ) {
            MembershipStruct memory _membership = MembershipStruct(
                _userAddress,
                _membershipStartDate,
                _membershipEndedDate,
                _membershipPayedAmount,
                true,
                _membershipId,
                updatesPerYear,
                nftCollection,
                uid
            );
            membershipInfoOfAddress[_userAddress] = _membership;
        } else {
            IMember(IMemberAddress).createMember(uid, _userAddress);
            MembershipStruct memory _membership = MembershipStruct(
                _userAddress,
                _membershipStartDate,
                _membershipEndedDate,
                _membershipPayedAmount,
                true,
                _membershipId,
                updatesPerYear,
                nftCollection,
                uid
            );
            membershipInfoOfAddress[_userAddress] = _membership;
        }
    }

    // Function to return membership information of the user
    function getMembership(address _user)
        external
        view
        returns (MembershipStruct memory)
    {
        return membershipInfoOfAddress[_user];
    }

    // Function to check of membership is active for the user
    function checkIfMembershipActive(address _user)
        public
        view
        checkIfMemberIsUser(_user)
        returns (bool status)
    {
        MembershipStruct storage _membership = membershipInfoOfAddress[_user];
        if (block.timestamp >= _membership.membershipEnded) {
            status = false;
        } else {
            status = true;
        }
    }

    // renewmembership Function to renew membership of the user
    function renewMembership()
        external
        payable
        checkIfMemberIsUser(msg.sender)
    {
        MembershipStruct storage _membership = membershipInfoOfAddress[
            msg.sender
        ];
        membershipPlan memory _membershipPlan = IMembershipFactory(
            IMembershipFactoryAddress
        ).getMembershipPlan(_membership.membershipId);
        require(
            _membershipPlan.active == true,
            "Membership plan is no longer active"
        );
        require(
            msg.value == _membershipPlan.costOfMembership,
            "User has not provided sufficient funds"
        );

        IMembershipFactory(IMembershipFactoryAddress).transferToPool{
            value: msg.value
        }();

        _membership.membershipEnded =
            block.timestamp +
            _membershipPlan.membershipDuration;
        _membership.payedAmount = msg.value;
        _membership.updatesPerYear =
            _membership.updatesPerYear +
            _membershipPlan.updatesPerYear;

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }

    // renewmembershipNFT - Function to renew membership for users that have NFTs
    function renewMembershipNFT(
        address _contractAddress,
        string memory _NFTType,
        uint256 tokenId
    ) external payable checkIfMemberIsUser(msg.sender) {
        IMember(IMemberAddress).checkIfWalletHasNFT(
            _contractAddress,
            _NFTType,
            tokenId,
            msg.sender
        );
        MembershipStruct storage _membership = membershipInfoOfAddress[
            msg.sender
        ];
        membershipPlan memory _membershipPlan = IMembershipFactory(
            IMembershipFactoryAddress
        ).getMembershipPlan(_membership.membershipId);
        require(
            _membershipPlan.active == true,
            "Membership plan is no longer active"
        );
        require(
            msg.value == _membershipPlan.costOfMembership,
            "User has not provided sufficient funds"
        );

        IMembershipFactory(IMembershipFactoryAddress).transferToPool{
            value: msg.value
        }();

        _membership.membershipEnded =
            block.timestamp +
            _membershipPlan.membershipDuration;
        _membership.payedAmount = msg.value;
        _membership.updatesPerYear =
            _membership.updatesPerYear +
            _membershipPlan.updatesPerYear;

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }

    // Function to top up updates
    function topUpUpdates() external payable checkIfMemberIsUser(msg.sender) {
        MembershipStruct storage _membership = membershipInfoOfAddress[
            msg.sender
        ];
        uint256 _updateCost = IMembershipFactory(IMembershipFactoryAddress)
            .getUpdatesPerYearCost();
        require(
            msg.value == _updateCost,
            "User needs to send sufficient amount to topUp"
        );

        IMembershipFactory(IMembershipFactoryAddress).transferToPool{
            value: msg.value
        }();

        _membership.updatesPerYear = _membership.updatesPerYear + 1;

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }

    // Function to change the users membership plan
    function changeMembershipPlan(uint256 membershipId)
        external
        payable
        checkIfMemberIsUser(msg.sender)
    {
        membershipPlan memory _membershipPlan = IMembershipFactory(
            IMembershipFactoryAddress
        ).getMembershipPlan(membershipId);
        require(
            msg.value == _membershipPlan.costOfMembership,
            "User needs to send sufficient value to pay for this plan"
        );
        require(
            _membershipPlan.active == true,
            "Membership plan is not active"
        );

        MembershipStruct storage _membership = membershipInfoOfAddress[
            msg.sender
        ];

        IMembershipFactory(IMembershipFactoryAddress).transferToPool{
            value: msg.value
        }();

        _membership.membershipId = _membershipPlan.membershipId;
        _membership.membershipEnded =
            block.timestamp +
            _membershipPlan.membershipDuration;
        _membership.updatesPerYear =
            _membership.updatesPerYear +
            _membershipPlan.updatesPerYear;
        _membership.payedAmount = msg.value;

        IMembershipFactory(IMembershipFactoryAddress).setUserForMembershipPlan(
            msg.sender,
            _membershipPlan.membershipId
        );

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }

    // Function to change membership plan to an NFT based plan
    function changeMembershipPlanNFT(
        uint256 membershipId,
        address _contractAddress,
        string memory _NFTType,
        uint256 tokenId
    ) external payable checkIfMemberIsUser(msg.sender) {
        IMember(IMemberAddress).checkIfWalletHasNFT(
            _contractAddress,
            _NFTType,
            tokenId,
            msg.sender
        );
        membershipPlan memory _membershipPlan = IMembershipFactory(
            IMembershipFactoryAddress
        ).getMembershipPlan(membershipId);
        require(
            msg.value == _membershipPlan.costOfMembership,
            "User needs to send sufficient value to pay for this plan"
        );
        require(
            _membershipPlan.active == true,
            "Membership plan is not active"
        );

        MembershipStruct storage _membership = membershipInfoOfAddress[
            msg.sender
        ];

        IMembershipFactory(IMembershipFactoryAddress).transferToPool{
            value: msg.value
        }();

        _membership.membershipId = _membershipPlan.membershipId;
        _membership.membershipEnded =
            block.timestamp +
            _membershipPlan.membershipDuration;
        _membership.updatesPerYear =
            _membership.updatesPerYear +
            _membershipPlan.updatesPerYear;
        _membership.payedAmount = msg.value;

        IMembershipFactory(IMembershipFactoryAddress).setUserForMembershipPlan(
            msg.sender,
            _membershipPlan.membershipId
        );

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }

    // Function to set new member address
    function setMemberAddress(address _newMemberAddress) public {
        require(
            msg.sender == IMembershipFactoryAddress,
            "Only the factory can change the member address"
        );
        IMemberAddress = _newMemberAddress;
    }

    // Function to get member address
    function getMemberAddress() public view returns (address) {
        return IMemberAddress;
    }

    function redeemUpdate(address _user) public {
        checkIfMembershipActive(_user);
        MembershipStruct storage _membership = membershipInfoOfAddress[_user];
        _membership.updatesPerYear = _membership.updatesPerYear - 1;

        emit membershipUpdated(
            address(this),
            _membership.user,
            _membership.uid,
            _membership.membershipStarted,
            _membership.membershipEnded,
            _membership.membershipId,
            _membership.updatesPerYear,
            _membership.nftCollection
        );
    }
}

