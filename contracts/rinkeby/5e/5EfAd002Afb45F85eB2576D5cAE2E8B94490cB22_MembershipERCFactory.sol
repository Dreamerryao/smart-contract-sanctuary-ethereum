pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@uniswap/v3-periphery/contracts/interfaces/IQuoter.sol";
import "../Membership.sol";
import "../structs/MembershipPlansStruct.sol";
//DEV
import "hardhat/console.sol";

contract MembershipERCFactory is
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable
{
    // storing all addresses of membership plans
    address[] private membershipPlanAddresses;

    // Member contract address
    address private IMemberAddress;

    // WETH Address
    address private wETHAddress;

    // Pool Address
    address payable private _poolAddress;

    address private DEXAddress;

    address[] private supportingERC20Tokens;

    // Variable to provide membershipId for user
    uint256 private membershipId;

    // Fixed cost for updatesPerYear
    uint256 private updatesPerYearCost;

    // Storing all membership factory states
    Membership[] private memberships;

    // storing all membership plans
    membershipPlan[] private membershipPlans;

    //  Mapping specific user to a membership plan id ; each user can have only one membership plan
    mapping(address => uint256) private UserToMembershipPlan;

    // Mapping specific plan to a membershipID
    mapping(uint256 => membershipPlan) private membershipIdtoPlan;

    // Mapping user to factory address of membership
    mapping(address => address) private UserToMembershipContract;

    event MembershipContractCreated(
        address membershipContractAddress,
        address user,
        string uid,
        uint256 membershipCreatedDate,
        uint256 membershipEndDate,
        uint256 membershipId,
        uint256 updatesPerYear,
        address collectionAddress
    );

    /**
     * @dev initialize - Initializes the function for Ownable and Reentrancy.
     */
    function initialize(
        address _memberContractAddress,
        address poolAddress,
        address _dexAddress,
        address wrappedEthAddress
    ) public initializer {
        __Context_init_unchained();
        __Ownable_init();
        __ReentrancyGuard_init();
        IMemberAddress = _memberContractAddress;
        DEXAddress = _dexAddress;
        _poolAddress = payable(poolAddress);
        membershipId = 0;
        updatesPerYearCost = 3e17;
        wETHAddress = wrappedEthAddress;
    }

    // Function to return users membership contract address
    function getUserMembershipAddress(address _user)
        external
        view
        returns (address)
    {
        return UserToMembershipContract[_user];
    }

    //
    // Function to createMembership by deploying membership contract for a specific member
    function createMembershipWithDex(
        string calldata uid,
        uint256 _membershipId,
        address token
    ) external payable {
        IMember(IMemberAddress).createMember(uid, msg.sender);
        require(
            UserToMembershipContract[msg.sender] == address(0),
            "User already has a membership contract"
        );
        require(
            (keccak256(
                abi.encodePacked((IMember(IMemberAddress).getUID(msg.sender)))
            ) == keccak256(abi.encodePacked((uid)))),
            "User does not exist"
        );
        membershipPlan memory _membershipPlan = membershipIdtoPlan[
            _membershipId
        ];
        require(
            _membershipPlan.active == true,
            "Membership plan is no longer active"
        );

        if (token != address(0)) {
            payERC20Dex(token, msg.value, _membershipPlan.costOfMembership);
        } else {
            require(
                msg.value == _membershipPlan.costOfMembership,
                "Membership cost send is not sufficient"
            );
            _poolAddress.transfer(msg.value);
        }

        uint256 _createdDate = block.timestamp;
        uint256 _endedDate = block.timestamp +
            _membershipPlan.membershipDuration;
        Membership _membership = new Membership();
        _membership.initialize(
            uid,
            IMemberAddress,
            address(this),
            msg.sender,
            _createdDate,
            _endedDate,
            _membershipPlan.membershipId,
            _membershipPlan.updatesPerYear,
            msg.value,
            _membershipPlan.nftCollection
        );
        memberships.push(_membership);
        UserToMembershipPlan[msg.sender] = membershipId;
        UserToMembershipContract[msg.sender] = address(_membership);
        membershipPlanAddresses.push(address(_membership));
        IMember(IMemberAddress).setIMembershipAddress(
            msg.sender,
            address(_membership),
            address(this)
        );
        emit MembershipContractCreated(
            address(_membership),
            msg.sender,
            uid,
            _createdDate,
            _endedDate,
            _membershipId,
            _membershipPlan.updatesPerYear,
            address(0)
        );
    }


    // Function to pay with other erc20 tokens 
    function payERC20Dex(
        address token,
        uint256 amount,
        uint256 costOfMembership
    ) internal {
        bool supportingToken = false;
        for (uint256 i = 0; i < supportingERC20Tokens.length; i++) {
            if (supportingERC20Tokens[i] == token) {
                supportingToken = true;
            }
        }
        if (!supportingToken) {
            revert(
                "Token sent is not supported. Please use a supporting token."
            );
        }

        uint256 ethValue = IQuoter(DEXAddress).quoteExactInputSingle(
            token,
            wETHAddress,
            500,
            amount,
            0
        );
        require(
            ethValue >= costOfMembership,
            "User needs to send more tokens in order to purchase the membership"
        );
        IERC20(token).approve(address(this), amount);
        IERC20(token).transfer(_poolAddress, amount);
    }

    // Function to create Membership for a member with supporting NFTs
    function createMembershipSupportingNFT(
        string calldata uid,
        address _contractAddress,
        string memory _NFTType,
        uint256 tokenId
    ) external payable {
        require(
            UserToMembershipContract[msg.sender] == address(0),
            "User already has a membership contract"
        );
        require(
            (keccak256(
                abi.encodePacked((IMember(IMemberAddress).getUID(msg.sender)))
            ) == keccak256(abi.encodePacked((uid)))),
            "User does not exist"
        );

        for (uint256 i = 0; i < membershipPlans.length; i++) {
            if (membershipPlans[i].nftCollection == _contractAddress) {
                IMember(IMemberAddress).checkIfWalletHasNFT(
                    _contractAddress,
                    _NFTType,
                    tokenId,
                    msg.sender
                );

                membershipPlan memory _membershipPlan = membershipIdtoPlan[
                    membershipPlans[i].membershipId
                ];
                require(
                    _membershipPlan.active == true,
                    "Membership plan is no longer active"
                );
                require(
                    msg.value == _membershipPlan.costOfMembership,
                    "Membership cost send is not sufficient"
                );

                _poolAddress.transfer(msg.value);

                uint256 _createdDate = block.timestamp;
                uint256 _endedDate = block.timestamp +
                    _membershipPlan.membershipDuration;

                Membership _membership = new Membership();
                _membership.initialize(
                    uid,
                    IMemberAddress,
                    address(this),
                    msg.sender,
                    _createdDate,
                    _endedDate,
                    _membershipPlan.membershipId,
                    _membershipPlan.updatesPerYear,
                    msg.value,
                    _membershipPlan.nftCollection
                );
                memberships.push(_membership);
                UserToMembershipPlan[msg.sender] = _membershipPlan.membershipId;
                UserToMembershipContract[msg.sender] = address(_membership);
                membershipPlanAddresses.push(address(_membership));
                emit MembershipContractCreated(
                    address(_membership),
                    msg.sender,
                    uid,
                    _createdDate,
                    _endedDate,
                    _membershipPlan.membershipId,
                    _membershipPlan.updatesPerYear,
                    _contractAddress
                );
                break;
            }
        }
    }

    // Function set member contract addresses for all deployed contracts
    function setMemberContractAddress(address _membercontractAddress)
        external
        onlyOwner
    {
        IMemberAddress = _membercontractAddress;
        for (uint256 i = 0; i < membershipPlanAddresses.length; i++) {
            Membership(membershipPlanAddresses[i]).setMemberAddress(
                _membercontractAddress
            );
        }
    }

    // Function to get Member Contract Address of Contracts
    function getMemberContractAddress()
        external
        view
        onlyOwner
        returns (address)
    {
        return IMemberAddress;
    }

    //
    // Function to create a membership plan with an NFT or without - If no collection provide address(0) for _collection
    function createMembershipPlan(
        uint256 _duration,
        uint256 _updatesPerYear,
        uint256 _cost,
        address _collection
    ) external onlyOwner {
        if (_collection == address(0)) {
            membershipPlan memory _membershipPlan = membershipPlan(
                _duration,
                _cost,
                _updatesPerYear,
                address(0),
                ++membershipId,
                true
            );
            membershipIdtoPlan[membershipId] = _membershipPlan;
            membershipPlans.push(_membershipPlan);
        } else {
            membershipPlan memory _membershipPlan = membershipPlan(
                _duration,
                _cost,
                _updatesPerYear,
                _collection,
                ++membershipId,
                true
            );
            membershipIdtoPlan[membershipId] = _membershipPlan;
            membershipPlans.push(_membershipPlan);
        }
    }

    // function to make membership plan active/inactive
    function setMembershipPlanActive(bool _active, uint256 _membershipId)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < membershipPlans.length; i++) {
            if (membershipPlans[i].membershipId == _membershipId) {
                membershipPlans[i].active = _active;
            }
        }
    }

    // function to get active/inactive status of membershipplan
    function getMembershipPlanActive(uint256 _membershipId)
        external
        view
        onlyOwner
        returns (bool)
    {
        for (uint256 i = 0; i < membershipPlans.length; i++) {
            if (membershipPlans[i].membershipId == _membershipId) {
                return membershipPlans[i].active;
            }
        }
    }

    // function to get all membership plans
    function getAllMembershipPlans()
        external
        view
        returns (membershipPlan[] memory)
    {
        return membershipPlans;
    }

    // function to getCostOfMembershipPlan
    function getMembershipPlan(uint256 _membershipId)
        external
        view
        returns (membershipPlan memory)
    {
        return membershipIdtoPlan[_membershipId];
    }

    // Function to get updates per year cost
    function getUpdatesPerYearCost() external view returns (uint256) {
        return updatesPerYearCost;
    }

    // Function to set new updates per year cost
    function setUpdatesPerYearCost(uint256 _newCost) external onlyOwner {
        updatesPerYearCost = _newCost;
    }

    // Function to set new membership plan for user
    function setUserForMembershipPlan(address _user, uint256 _membershipId)
        external
    {
        UserToMembershipPlan[_user] = _membershipId;
    }

    // Functino to get pool Address
    function getPoolAddress() external view returns (address) {
        return _poolAddress;
    }

    // Function to set new pool address
    function setPoolAddress(address _pool) external onlyOwner {
        _poolAddress = payable(_pool);
    }

    // Function to transfer eth to specific pool
    function transferToPool() external payable {
        _poolAddress.transfer(msg.value);
    }

    // Function to get DEX address
    function getDEXAddress() public view returns (address) {
        return DEXAddress;
    }

    // Function to get DEX address
    function setDEXAddress(address _DexAddress) external onlyOwner {
        DEXAddress = _DexAddress;
    }


    // Function to get WETH Address
    function getWETHAddress() public view returns (address) {
        return wETHAddress;
    }

    // Function to set WETH address
    function setWETHAddress(address _ethAddress) external onlyOwner {
        wETHAddress = _ethAddress;
    }

    // Function to get supporting erc20tokens
    function getSupportingERC20Tokens()
        external
        view
        returns (address[] memory)
    {
        return supportingERC20Tokens;
    }

    // function to set new supporting erc20 tokens
    function setSupportingERC20Tokens(address _token) external {
        supportingERC20Tokens.push(_token);
    }
}

