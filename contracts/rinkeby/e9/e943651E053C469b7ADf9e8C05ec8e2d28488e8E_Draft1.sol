pragma solidity ^0.8.8;
import "./ERC721DraftBase.sol";
import "./IERC721Draft1.sol";

contract Draft1 is ERC721DraftBase, IERC721Draft1 {
    // Functions that define ownership

    constructor(string memory name, string memory symbol) {
        _name = name;
        _symbol = symbol;

    }

    function tokensOfOwnerByIndex(address _owner, uint256 _index)
        external
        view
        returns (uint256 tokenId) {
            return 0;
        }

    function transferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) external {
        require(_msgSender() == _tokenApprovals[_tokenId] || _msgSender() == _owners[_tokenId], "Sender is not owner or receiver does not have approval");
        address owner = _owners[_tokenId];
        _balances[owner] -= 1;
        _balances[_to] += 1;
        _owners[_tokenId] = _to;

        emit Transfer(owner, _to, _tokenId);
    }
}

