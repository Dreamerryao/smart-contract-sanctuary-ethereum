  mapping (uint256 => uint256) private _overallTokenIndex;

  mapping (uint256 => address) private _tokenOwner;
  
  mapping (uint256 => address) private _tokenApproval;
  
  mapping (address => mapping (address => bool)) private _tokenOperator;
  
