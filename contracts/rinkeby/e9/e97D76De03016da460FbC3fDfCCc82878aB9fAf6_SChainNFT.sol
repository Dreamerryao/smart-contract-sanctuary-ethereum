        require(
          _isTokenOwner(msg.sender, _tokenId) ||
            _isTokenOperator(msg.sender, _tokenId) ||
            _isApproved(msg.sender, _tokenId)
          , "ERROR: not token authorized"
        );
        _;
    }
    
