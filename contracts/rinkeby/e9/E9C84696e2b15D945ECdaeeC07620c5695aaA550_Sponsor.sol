import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./Badge.sol";

error PctsTooHigh();
error ReservedIsMoreThanSupply();
error WrongAmount(uint256 amount);
error ExceededReserved();
error CantMintAboveSupply();
error InsufficientFunds();
error AgentIsBanned();
error TokenBadgeMismatch(uint256 tokenId);
error AgentNotPaid();
error PlatformFeeNotPaid();
error CantBeAgent();
error URIFrozen();
error UserNotWhitelisted();
error PresaleTurnedOff();
error PublicSaleTurnedOff();
error NotPlatformOwner();

contract Sponsor is ERC721A, Ownable, ReentrancyGuard {
    using Strings for uint256;
    using ECDSA for bytes32;

    uint256 constant public MAX_TX_LIMIT = 20;

    // Immutables
    Badge   public immutable badgeNFT;
    GnosisSafe public immutable gnosisSafe;
    bytes32 public immutable salt;
    uint256 public immutable maxSupply;
    uint256 public immutable maxReserved;

    // booleans
    bool public isURIFrozen;
    bool public revealed;
    bool public isPublicSale;
    bool public isPresale;

    // mint costs
    uint256 public publicCost;
    uint256 public presaleCost;

    // urls
    string private sponsorBaseURI;
    string public unrevealedURI;
    string public baseExtension = ".json";

    // TBI settings(in one slot...)
    address public platformOwner;
    uint64 public platformFee;

    // other
    uint256 private reserved;
    address public signatureVerifier;

    mapping(address => bool) public blacklist;
    mapping(address => bool) public preSaleList;

    uint8[5] public commissions;

    struct Config {
        string name;
        string symbol;
        string baseURI;
        string baseExtension;
        string unrevealedURI;
        bool isRevealed;
    }

    struct Setting {
        address platformOwner; // The TBI Treasury Address
        address signatureVerifier; // Used in case presale works with discord bot...
        uint64 platformFee; // The TBI Treasury Fee
        uint maxSupply;
        uint maxReserved;
        uint publicCost;
        uint presaleCost;
        bytes32 salt;
        uint8[5] commissions;
    }
    
    constructor(
        Config memory cfg,
        Setting memory stg,
        Badge _badgeNFT
    ) ERC721A(cfg.name, cfg.symbol) {
        // One time set, can not be changed..
        for(uint256 i = 0; i < stg.commissions.length; i++) {
            if(stg.platformFee + stg.commissions[i] > 100) revert PctsTooHigh();
            commissions[i] = stg.commissions[i];
        }

        if(stg.maxReserved > stg.maxSupply) revert ReservedIsMoreThanSupply();

    
        // set url configs
        sponsorBaseURI = cfg.baseURI;
        unrevealedURI = cfg.unrevealedURI;
        revealed = cfg.isRevealed;

        if(bytes(cfg.baseExtension).length > 0) {
            baseExtension = cfg.baseExtension;
        }

        // set settings config
        maxSupply = stg.maxSupply;
        maxReserved = stg.maxReserved;
        publicCost = stg.publicCost;
        presaleCost = stg.presaleCost;
        platformOwner = stg.platformOwner;
        platformFee = stg.platformFee;
        signatureVerifier = stg.signatureVerifier;
        salt = stg.salt;

        // set badge contract...
        badgeNFT = _badgeNFT;

        gnosisSafe = new GnosisSafe();
        address[] memory addr = new address[](2);
        addr[0] = platformOwner;
        addr[1] = msg.sender;
    }

    function mint(address agentAddress, uint amount) public payable {
        if(!isPublicSale) revert PublicSaleTurnedOff();
        if(amount == 0 || amount > MAX_TX_LIMIT) revert WrongAmount(amount);
        if(totalSupply() + amount > maxSupply) revert CantMintAboveSupply();
        if(amount * publicCost > msg.value) revert InsufficientFunds();

        _startMint(agentAddress, amount);
    }
    
    function presaleMint(address agentAddress, uint256 amount, bytes calldata signature) public payable {
        if(!isPresale) revert PresaleTurnedOff();
        if(amount == 0 || amount > MAX_TX_LIMIT) revert WrongAmount(amount);
        if(totalSupply() + amount > maxSupply) revert CantMintAboveSupply();
        if(amount * presaleCost > msg.value) revert InsufficientFunds();

        if(!isUserWhitelisted(msg.sender, signature)) revert UserNotWhitelisted();
        
        _startMint(agentAddress, amount);
    }

    // owner functions
    function reserve(uint256 amount, address destination) external onlyOwner {
        if(amount == 0 || amount > MAX_TX_LIMIT) revert WrongAmount(amount);
        if(totalSupply() + amount > maxSupply) revert CantMintAboveSupply();
        if(reserved + amount > maxReserved) revert ExceededReserved();

        reserved += amount;
        
        _safeMint(destination, amount);
    }

    // add or remove agents from blacklist 
    function blacklistAgents(address[] calldata agents, bool state) external {
        if(msg.sender != platformOwner) revert NotPlatformOwner();
        for(uint256 i = 0; i < agents.length; i++) {
            blacklist[agents[i]] = state;
        }
    }

    function whitelistUsers(address[] calldata users, bool state) external onlyOwner {
         for(uint256 i = 0; i < users.length; i++) {
            preSaleList[users[i]] = state;
        }
    }

    function changePlatformOwner(address _newOwner) external {
        if(msg.sender != platformOwner) revert NotPlatformOwner();

        platformOwner = _newOwner;
    }
    
    function setPublicCost(uint256 _cost) external onlyOwner {
        publicCost = _cost;
    }

    function setPresaleCost(uint256 _cost) external onlyOwner {
        presaleCost = _cost;
    }

    function togglePreSale() external onlyOwner {
        isPresale = !isPresale;
    }

    function togglePublicSale() external onlyOwner {
        isPublicSale = !isPublicSale;
    }

    function setUnrevealedURI(string calldata _unrevealedURI) external onlyOwner {
        unrevealedURI = _unrevealedURI;
    }

    function setSignatureVerifier(address _signatureVerifier) external onlyOwner {
        signatureVerifier = _signatureVerifier;
    }

    function reveal() external onlyOwner {
        if(isURIFrozen) revert URIFrozen();
        revealed = !revealed;
    }

    function setBaseExtension(string calldata _newBaseExtension) external onlyOwner {
        if(isURIFrozen) revert URIFrozen();
        baseExtension = _newBaseExtension;
    }

    function setBaseURI(string calldata newURI) external onlyOwner {
        if(isURIFrozen) revert URIFrozen();
        sponsorBaseURI = newURI;
    }

    function freezeURI() external onlyOwner {
        isURIFrozen = true;
    }

    function withdraw() public onlyOwner {
        (bool os, ) = payable(owner()).call{value: address(this).balance}("");
        require(os);
    }

    // view/internal functions

    function _startMint(address agentAddress, uint256 amount) private {
        if(blacklist[agentAddress]) revert AgentIsBanned();

        // if sender is himself an agent, he can't mint.
        (uint tokenId, ) = badgeNFT.users(msg.sender);
        if(tokenId != 0) revert CantBeAgent();

        // check if agentAddres is actually an agent.
        (tokenId, ) = badgeNFT.users(agentAddress);
        if(tokenId < 1 || tokenId > 5) revert TokenBadgeMismatch({ tokenId: tokenId });

        bool ok;

        // pay the agent its commission
        (ok, ) = payable(agentAddress).call{value: commissions[tokenId - 1] * msg.value / 100}("");
        if(!ok) revert AgentNotPaid();

        // pay the platform fee
        (ok, ) = payable(platformOwner).call{value: platformFee * msg.value / 100}("");
        if(!ok) revert PlatformFeeNotPaid();

        _safeMint(msg.sender, amount);      
    }


    function isUserWhitelisted(address _user, bytes calldata _signature) public view returns(bool) {
        if(_signature.length != 0) {
            bytes32 messageHash = hashMessage(_user);
            return messageHash.recover(_signature) == signatureVerifier;
        } else {
            return preSaleList[_user];
        }
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        if(!_exists(tokenId)) revert URIQueryForNonexistentToken();
        
        if(revealed == false) {
            return unrevealedURI;
        }

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0 
            ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
            : "";
    }

    function hashMessage(address sender) internal view returns(bytes32) {
        bytes32 payloadHash = keccak256(abi.encode(sender, salt));
        bytes32 messageHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", payloadHash));

        return messageHash;
    }

    function _baseURI() internal view override returns (string memory) {
        return sponsorBaseURI;
    }

    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }
}

