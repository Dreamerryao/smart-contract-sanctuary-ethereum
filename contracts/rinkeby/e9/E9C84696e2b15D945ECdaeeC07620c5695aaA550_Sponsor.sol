pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";

import "./CBB.sol";

contract Badge is ERC1155, Ownable {
    using Strings for uint256;

    CBB public cbbNFT;

    struct MintedBadge {
        uint256 tokenId;
        bool minted;
    }

    mapping(uint256 => bool) private validBadgeTypes;
    mapping(address => MintedBadge) public users;

    string public baseURI = "https://tbiagency.mypinata.cloud/ipfs/QmaisGjANJ8TWSYYowuMoDb4sizd95kR4C1aEzBrQfLkiX/";

    constructor(CBB _cbbNFT) ERC1155(baseURI) {
        cbbNFT = _cbbNFT;

        validBadgeTypes[1] = true;
        validBadgeTypes[2] = true;
        validBadgeTypes[3] = true;
        validBadgeTypes[4] = true;
        validBadgeTypes[5] = true;
    }

    function mint() external {
        // If user already minted, not allow to mint again, even though
        // his badge might have been burnt.
        require(!users[msg.sender].minted, "Mint not possible after getting a badge");

        // if no token on genesis, don't allow to mint.
        uint balanceOf = cbbNFT.balanceOf(msg.sender);
        require(balanceOf > 0, "Balance can't be 0");

        uint tokenId = getBadgeType(balanceOf);
        
        // save cost by property by property initialization
        users[msg.sender].tokenId = tokenId;
        users[msg.sender].minted = true;

        _mint(msg.sender, tokenId, 1, "");
    }
    
    function burn(
        address from, 
        address to, 
        uint balanceFrom, 
        uint balanceTo
    ) external {    
        require(msg.sender == address(cbbNFT), "CBB: caller not Genesis contract");

        if(from != address(0)) {
            burnAndMintByCBB(from, balanceFrom);
        }

        if(to != address(0)) {
            burnAndMintByCBB(to, balanceTo);
        }
    }

    function uri(uint256 typeId) public view override returns (string memory) {
        require(
            validBadgeTypes[typeId],
            "URI requested for invalid badge type"
        );
        return bytes(baseURI).length > 0
            ? string(abi.encodePacked(baseURI, typeId.toString()))
            : baseURI;
    }

    // ================================== Owner Functions ==================================
    
    function setcbbNFT(CBB _cbbNFT) external onlyOwner {
        cbbNFT = _cbbNFT;
    }

    function setBaseURI(string calldata _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }

    // ================================== Private Functions ==================================

    /**
        @param user the user address for which we should burn the current badge and mint new one.
        @param balance the user's updated CBB balance caused by mint or transferFrom on CBB
    */
    function burnAndMintByCBB(address user, uint balance) private {
        // If user never minted a badge, don't activate burn/mint 
         if(!users[user].minted) {
            return;
        }
        
        uint nextTokenId = getBadgeType(balance);
        uint currentTokenId = users[user].tokenId;

        // if user's current badge and next possible badge are equal,
        // no need to burn/mint as it would cause the same result.
        // example: user had 5 CBB(hence black ops) and he got 1 more CBB
        // in which case he still should own black ops.
        if(nextTokenId == currentTokenId) {
            return;
        }
        
        // If user minted and his current badge isn't burnt, burn it
        // Necessary for the next step to give a new rank badge.
        if(currentTokenId != 0) {
            _burn(user, currentTokenId, 1);
        }

        // if balance is not 0, user's updated balance requires its corresponding
        // badge rank, so mint it, otherwise, user doesn't have CBB anymore,
        // delete it and refund some of the gas costs.
        if(balance != 0) {            
            users[user].tokenId = nextTokenId;
            
            _mint(user, nextTokenId, 1, "");
        } else {
            delete users[user].tokenId;
        }
    }

    function getBadgeType(uint balanceOf) private pure returns(uint tokenId)  {
        return balanceOf <= 5 ? balanceOf : 5;
    }

    // Overrides
    function _beforeTokenTransfer(
        address /* operator */,
        address  from,
        address  to,
        uint256[] memory /* ids */,
        uint256[] memory /* amounts */,
        bytes memory /* data */
    ) internal pure override {
        // If mint or burn happens, allow, otherwise
        // reject any transfer of badges.
        if(from != address(0) && to != address(0)) {
            require(false, "Badge Transfer Not Possible");
        }
    } 
}

