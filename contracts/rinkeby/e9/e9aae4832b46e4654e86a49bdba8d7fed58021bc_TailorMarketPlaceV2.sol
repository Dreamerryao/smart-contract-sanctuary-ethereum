pragma solidity ^0.8.9;

/**
 * @author Thomas Boot - Tailor
 * @title OrderTypes
 * @notice Inspired by library of LooksRare exchange.
 */
library OrderTypes {
    bytes32 internal constant NFT_ORDER_HASH =
        keccak256(
            "NFTOrder(bool isSellOrder,address collection,address strategy,address payable signer,uint256 price,uint256 tokenId, address payable feesAddress,TicketState status,PurchaseState sellstatus,uint256 startTime,uint256 endTime)"
        );

    enum PurchaseState {
        SELLABLE,
        NONSELLABLE
    }

    enum NFTState {
        LISTED, // 0
        AUCTIONABLE, // 1
        TRADABLE, // 2
        PENDING, // 3 : PENDING : for the case that the ticket is being transferred -> CHECK THIS for every transfer (if user doesn't sign transfer -> infinte loop and tickets aren't transferable)
        TRADED, // 4
        SOLD, // 5
        NOTLISTED // 6
    }

    // NFTOrder are buy/sell orders - Can be filled with EIP-712 standard (nonce, signature, hashTx)
    struct NFTOrder {
        bool isSellOrder; // true -> sell (passive order) | false -> buy (passive order)
        address collection; // NFT collection contract address
        address payable strategy; // address of the strategy choosen (english auction, sale at fixed price) -> should be whitelisted
        address payable signer; // address of signer of the order
        uint256 tokenId; // id Token of the order
        address payable feesAddress; // Address of royalty trade owner
        uint96 secondaryMarketFee;
        address payable royaltyFeeRecipient;
        uint96 royaltyFeeAmount;
        NFTState status; // NFT status
        PurchaseState sellstatus; // Selling status
        uint256 price; // Price of the item
        address highestbidder; // highest bidder address
        uint256 minBid; // minimum bid
        uint256 startTime; // startTime in timestamp
        uint256 endTime; // endTime in timestamp
        address currencyToken;
        // uint256 nonce;					// Signer nonce
        // uint8 v; 						// v: parameter (27 or 28)
        // bytes32 r; 						// r: parameter
        // bytes32 s; 						// s: parameter
    }

    //  are buy/sell orders that are stored ON-CHAIN
    struct TakerOrder {
        bool isSellOrder; // true -> execute order to sell against buy Order | false -> execute order to buy against sell Order
        address taker; // address of the msg.sender
        uint256 price; // final purchase price
        uint256 tokenId;
        address currencyToken; // Id of token
    }

    struct ParamsVariable {
        address to;
        uint256 amount;
    }
    struct TransferParams {
        address from;
        address token;
        ParamsVariable[3] variables;
    }

    function getAuctionVariables(NFTOrder memory nftOrder, uint256 bidPrice)
        internal
        pure
        returns (ParamsVariable[3] memory)
    {
        return [
            ParamsVariable(
                nftOrder.royaltyFeeRecipient,
                (nftOrder.royaltyFeeAmount * bidPrice) / 10000
            ),
            ParamsVariable(
                nftOrder.feesAddress,
                (nftOrder.secondaryMarketFee * bidPrice) / 10000
            ),
            ParamsVariable(
                nftOrder.signer,
                bidPrice -
                    (((nftOrder.secondaryMarketFee +
                        nftOrder.royaltyFeeAmount) * bidPrice) / 10000)
            )
        ];
    }

    function getPaymentParams(NFTOrder memory nftOrder, address buyer)
        internal
        pure
        returns (TransferParams memory)
    {
        return
            TransferParams(
                buyer,
                nftOrder.currencyToken,
                [
                    ParamsVariable(
                        nftOrder.royaltyFeeRecipient,
                        (nftOrder.royaltyFeeAmount * nftOrder.price) / 10000
                    ),
                    ParamsVariable(
                        nftOrder.feesAddress,
                        (nftOrder.secondaryMarketFee * nftOrder.price) / 10000
                    ),
                    ParamsVariable(
                        nftOrder.signer,
                        nftOrder.price -
                            (((nftOrder.secondaryMarketFee +
                                nftOrder.royaltyFeeAmount) * nftOrder.price) /
                                10000)
                    )
                ]
            );
    }

    function hash(NFTOrder memory nftOrder) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    NFT_ORDER_HASH,
                    nftOrder.isSellOrder,
                    nftOrder.collection,
                    nftOrder.strategy,
                    nftOrder.signer,
                    nftOrder.price,
                    nftOrder.tokenId,
                    nftOrder.feesAddress,
                    nftOrder.status,
                    nftOrder.sellstatus,
                    nftOrder.startTime,
                    nftOrder.endTime
                    // ticketOrder.nonce
                )
            );
    }
}

