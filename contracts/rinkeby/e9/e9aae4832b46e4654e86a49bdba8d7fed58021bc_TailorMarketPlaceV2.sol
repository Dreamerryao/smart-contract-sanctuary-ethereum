/**
 * Should look the stacks of the auction in this contract
 */
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "../libraries/OrderTypesV2.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract AuctionStrategy is Initializable, Context, ReentrancyGuardUpgradeable {
    using SafeERC20 for IERC20;
    address private initialOwner;
    address private lastbidder;
    address public WETH;
    address public marketplace;
    enum AuctionStatus {
        OPEN, //0
        CLOSE //1
    }
    AuctionStatus public status;
    address currenthighestbidder;
    using OrderTypes for OrderTypes.ParamsVariable;

    function isEnded() public view returns (bool) {
        return status == AuctionStatus.CLOSE;
    }

    function initialize(
        address signer,
        address _WETH,
        address _marketplace
    ) external initializer {
        initialOwner = signer;
        WETH = _WETH;
        marketplace = _marketplace;
    }

    modifier onlySeller() {
        require(_msgSender() == initialOwner);
        _;
    }

    modifier isFinalOwner() {
        require(msg.sender != address(0), "not final owner");
        _;
    }
    modifier onlyMarketplace() {
        require(msg.sender == marketplace);
        _;
    }

    function executeTransfer(OrderTypes.ParamsVariable memory variables)
        private
        returns (bool)
    {
        if (variables.to != address(0) && variables.amount != 0) {
            IERC20(WETH).safeTransfer(variables.to, variables.amount);
            return true;
        }
        return false;
    }

    function withdrawBalance(
        OrderTypes.ParamsVariable[3] memory recipients,
        address signer
    ) external returns (bool) {
		 require(
            msg.sender == marketplace,
            "this function has to be called by the marketplace"
        );
		require(
            isEnded(),
            "Can not withdraw balance unfinished auction"
        );
        require(signer == initialOwner, "the signer has to be the seller");

        require(
            IERC20(WETH).balanceOf(address(this)) > 0,
            "nothing to withdraw"
        );
        for (uint8 u = 0; u < 3; u++) {
            executeTransfer(recipients[u]);
        }

        return (true);
    }

    function destruct() external {
        require(msg.sender == marketplace, "has to be called by mp");
        require(
            IERC20(WETH).balanceOf(address(this)) == 0,
            "the contract isnt empty"
        );
        selfdestruct(payable(initialOwner));
    }

    function getBalance() public view returns (uint256) {
        return IERC20(WETH).balanceOf(address(this));
    }

    function reimburseLastBidder(address highestbidder) external nonReentrant {
        require(msg.sender == marketplace, "call from marketplace");
        if (highestbidder == address(0)) {
            return;
        }

        IERC20(WETH).safeTransfer(
            highestbidder,
            IERC20(WETH).balanceOf(address(this))
        );
    }

    function changeAuctionStatus(AuctionStatus state) public {
        require(msg.sender == marketplace);
        status = state;
    }

    receive() external payable {}

    fallback() external {}
}

