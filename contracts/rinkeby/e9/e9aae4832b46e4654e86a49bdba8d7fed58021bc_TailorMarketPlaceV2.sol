contract TailorMarketPlaceV2 is
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable,
    TailorControlAccess
{
    // @custom:oz-upgrades-unsafe-allow delegatecall
    uint256[500] private __placeholder;
    using EnumerableSet for EnumerableSet.AddressSet;
    EnumerableSet.AddressSet acceptedCurrencies;
    using OrderTypes for OrderTypes.NFTOrder;
    using OrderTypes for OrderTypes.TakerOrder;
    using OrderTypes for OrderTypes.TransferParams;
    // using ERC165Checker for address;
    uint96 internal _secondaryMarketFee;
    address payable internal _feesAddress;

    address internal _strategyFactory;
    address internal _custodyFactory;
    EnumerableSet.AddressSet private _whitelistedStrategies;

    mapping(uint256 => OrderTypes.NFTOrder) private marketNFTData;
    mapping(address => address payable) private openEscrow;

    event NFTListed(
        address collection, // address of the NFT contract
        address indexed signer, // sender of the sell TakerOrder
        address indexed feesAddress, // sender of the sell TakerOrder
        address strategy,
        uint256 tokenId // token Id of the item
    );
    event EscrowCreated(address signer, address escrow);
    event NFTAuctioned(
        address collection, // address of the NFT contract
        address indexed signer, // sender of the sell TakerOrder
        address indexed feesAddress, // sender of the sell TakerOrder
        uint256 tokenId // token Id of the item
    );
    event NFTSold(
        address collection, // address of the NFT contract
        address indexed signer, // sender of the sell TakerOrder
        address indexed feesAddress, // sender of the sell TakerOrder
        uint256 feesAmount,
        address royaltyAddress,
        uint256 royaltyfeesAmount,
        address indexed prevOwner, // sender of the sell TakerOrder
        uint256 tokenId, // token Id of the item
        uint256 price
    );
    event NewBidNFT(
        address collection, // address of the NFT contract
        address indexed signer, // sender of the sell TakerOrder
        address indexed highestbidder, // sender of the sell TakerOrder
        address indexed feesAddress, // sender of the sell TakerOrder
        uint256 tokenId, // token Id of the item
        uint256 price,
        uint256 prevBid
    );
    event ModifiedListing(
        address collection,
        address indexed sender,
        uint256 tokenId
    );

    event CancelListing(
        address collection,
        address indexed sender,
        uint256 tokenId
    );
    event CreationStrategy(
        address collection,
        uint256 tokenId,
        address strategy
    );
    event StrategyBalanceWithdrawn(
        address collection,
        uint256 tokenId,
        address strategy,
        uint256 amount
    );
    event WithdrawEscrowFunds(address to, uint256 amount);
    modifier onlySellableCollection(
        bool _openForSaleBeforeEvent,
        bool _openForSaleAfterEvent
    ) {
        require(
            _openForSaleBeforeEvent || _openForSaleAfterEvent,
            "Event sales are not open"
        );
        _;
    }

    modifier onlyNFTOwner(
        address collection,
        address signer,
        uint256 tokenId
    ) {
        address owner = ITailorNFTCollectible(collection).ownerOf(tokenId);
        require(owner == signer, "Not NFT Owner");
        _;
    }

    modifier notBiddedOn(uint256 tokenId) {
        require(
            marketNFTData[tokenId].highestbidder == address(0),
            "Already a current bid"
        );
        _;
    }

    modifier notListed(uint256 tokenId) {
        require(!_listed(tokenId), "Item already listed");
        _;
    }
    modifier acceptedCurrency(address currency) {
        require(isCurrencyAccepted(currency), "currency not accepted");
        _;
    }
    modifier isListed(uint256 tokenId) {
        require(_listed(tokenId), "Item not listed");
        _;
    }

    function _listed(uint256 tokenId) private view returns (bool) {
        return marketNFTData[tokenId].signer != address(0);
    }

    function initialize(
        address feesAddress,
        uint96 secondaryMarketFee,
        address _WETH
    ) external initializer {
        __ReentrancyGuard_init();
        __setFee(feesAddress, secondaryMarketFee);
        __TailorControlAccess_init();
        _addCurrency(address(0)); //payments in ether
        _addCurrency(_WETH);
    }

    function __addAuctionStrategies(address _address) external onlyAdmin {
        _strategyFactory = _address;
        _whitelistedStrategies.add(_address);
    }

    function __addEscrowStrategies(address _address) external onlyAdmin {
        _custodyFactory = _address;
        _whitelistedStrategies.add(_address);
    }

    function __setFee(address feesAddress, uint96 secondaryMarketFee) private {
        _feesAddress = payable(feesAddress);
        _secondaryMarketFee = secondaryMarketFee;
    }

    function isClonedAuction(address _test) external view returns (bool) {
        return ICloneFactory(_strategyFactory).isCloned(_test);
    }

    function _addCurrency(address currency) private {
        // require(IERC165(currency).supportsInterface(type(IERC20).interfaceId),"unsupported interface");
        acceptedCurrencies.add(currency);
    }

    function addCurrency(IERC20 currency) external onlyAdmin {
        // log(currency,"currency address");
        // log(type(IERC20).interfaceId==0x36372b07,"isgood id");
        // bytes4 IERC20InterfaceId = 0x36372b07;
        // bytes4[] memory tab = new bytes4[](1);
        // tab[0]=IERC20InterfaceId;
        // require(currency.getSupportedInterfaces(tab)[0],"this address doens't support the ierc20 interface");

        // require(IERC165(currency).supportsInterface(type(IERC20).interfaceId),"unsupported interface");

        _addCurrency(address(currency));
    }

    //function _createSell
    function isCurrencyAccepted(address currency) public view returns (bool) {
        return acceptedCurrencies.contains(currency);
    }

    function _createListing(OrderTypes.NFTOrder memory NFTOrder) private {
        //,typeListing typesell

        NFTOrder.status == OrderTypes.NFTState.LISTED
            ? _buyListing(NFTOrder)
            : _auctionListing(NFTOrder);

        marketNFTData[NFTOrder.tokenId] = NFTOrder;
    }

    function _buyListing(OrderTypes.NFTOrder memory NFTOrder) private {
        if (openEscrow[NFTOrder.signer] == address(0)) {
            require(
                _custodyFactory != address(0),
                "the custody factory is not initialized"
            );
            require(
                _custodyFactory != address(0),
                "_custodyFactory not initialized"
            );
            openEscrow[NFTOrder.signer] = ICloneFactory(_custodyFactory)
                .createEscrowClone(NFTOrder.signer, address(this));
            emit EscrowCreated(NFTOrder.signer, openEscrow[NFTOrder.signer]);
        }
        emit NFTListed(
            NFTOrder.collection,
            NFTOrder.signer,
            _feesAddress,
            openEscrow[NFTOrder.signer],
            NFTOrder.tokenId
        );
    }

    function _auctionListing(OrderTypes.NFTOrder memory NFTOrder) private {
        NFTOrder.strategy = payable( //function à la place
            ICloneFactory(_strategyFactory).createAuctionClone(
                NFTOrder.signer,
                NFTOrder.currencyToken,
                address(this)
            )
        );

        emit NFTAuctioned(
            NFTOrder.collection,
            NFTOrder.signer,
            _feesAddress,
            NFTOrder.tokenId
        );
    }

    /**
     * @dev Allows ticket to be LISTED on secondary Market
     * Check if tokenID owner is the owner that calls the function
     * Temporary add onlyAdmin
     */
    function secondaryMarketListing(
        OrderTypes.NFTOrder calldata NFTOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
        onlyNFTOwner(NFTOrder.collection, NFTOrder.signer, NFTOrder.tokenId)
        notListed(NFTOrder.tokenId)
        onlyAdmin
        acceptedCurrency(NFTOrder.currencyToken)
    {
        _validateOrder(NFTOrder);
        _createListing(NFTOrder);
    }

    /**
     * @dev Allows to bid on LISTED tickets
     * Verify that CALL contains msg.value >= item price
     * Verify that tickets is LISTED and SELLABLE
     * Temporary add onlyAdmin
     */
    function getEscrow(address user) external view returns (address) {
        return openEscrow[user];
    }

    function secondaryMarketBuying(
        OrderTypes.TakerOrder calldata takerOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        payable
        nonReentrant
        onlyAdmin
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
    {
        OrderTypes.NFTOrder storage ticketNFT = marketNFTData[
            takerOrder.tokenId
        ];

        require(
            ticketNFT.status == OrderTypes.NFTState.LISTED,
            "NFT does not have the status LISTED"
        );
        require(
            ticketNFT.sellstatus == OrderTypes.PurchaseState.SELLABLE,
            "NFT does not have the status SELLABLE"
        );
        require(
            ticketNFT.signer != takerOrder.taker,
            "Owner NFT can not buy his own NFT"
        );
        require(
            openEscrow[ticketNFT.signer] != address(0),
            "no custody contract"
        );
        require(ticketNFT.price == takerOrder.price, "incorrect price");

        require(
            !(ticketNFT.currencyToken != address(0) &&
                IERC20(ticketNFT.currencyToken).allowance(
                    takerOrder.taker,
                    openEscrow[ticketNFT.signer]
                ) <
                takerOrder.price),
            "you don't have enough token"
        );
        require(
            !(ticketNFT.currencyToken != address(0) &&
                takerOrder.taker.balance < takerOrder.price),
            "you don't have enough matic"
        );
        require(
            ticketNFT.currencyToken == takerOrder.currencyToken,
            "tokens don't match"
        );
        OrderTypes.TransferParams memory params = ticketNFT.getPaymentParams(
            takerOrder.taker
        );
        uint256 balanceEscrow = openEscrow[ticketNFT.signer].balance;
        bool balanceWithdrawn = IEscrowUser(openEscrow[ticketNFT.signer])
            .transferFeesAndFunds{value: msg.value}(params);
        if (balanceWithdrawn)
            emit WithdrawEscrowFunds(ticketNFT.signer, balanceEscrow);
        // require(takerOrder.taker != address(0), "Buyer must be not address(0)");
        // require(success, "could not withdraw");

        address originalOwner = ticketNFT.signer;
        ticketNFT.status = OrderTypes.NFTState.PENDING;

        // 1. Transfer NFT
        _transferNFT(
            ticketNFT.collection,
            ticketNFT.signer,
            takerOrder.taker,
            ticketNFT.tokenId
        );

        // CAUTION : use msgSender();
        ticketNFT.signer = payable(takerOrder.taker);
        ticketNFT.status = OrderTypes.NFTState.SOLD;
        ticketNFT.sellstatus = OrderTypes.PurchaseState.NONSELLABLE;

        emit NFTSold(
            ticketNFT.collection,
            takerOrder.taker,
            _feesAddress,
            params.variables[1].amount,
            params.variables[0].to,
            params.variables[0].amount,
            originalOwner,
            ticketNFT.tokenId,
            ticketNFT.price
        );
		delete marketNFTData[ticketNFT.tokenId];
        // if(ba)
    }

    //set a balance limit that if
    /**
     * @dev Allows to bid on AUCTIONNABLE tickets
     * Verify that CALL contains msg.value >= minBid
     * Verify that tickets is AUCTIONNABLE and SELLABLE
     */
    function secondaryMarketBidding(
        OrderTypes.TakerOrder calldata takerOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        payable
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
        onlyAdmin
    {
        OrderTypes.NFTOrder storage ticketNFT = marketNFTData[
            takerOrder.tokenId
        ];

        require(
            ticketNFT.signer != takerOrder.taker,
            "NFTOwner can't bid on his own NFT"
        );
        require(
            ticketNFT.status == OrderTypes.NFTState.AUCTIONABLE,
            "NFT does not have the status AUCTIONABLE"
        );
        require(
            ticketNFT.sellstatus == OrderTypes.PurchaseState.SELLABLE,
            "NFT does not have the status SELLABLE"
        );
        require(
            block.timestamp < ticketNFT.endTime,
            "Auction must not be closed"
        );
        require(
            takerOrder.price >= ((ticketNFT.minBid) * 12) / 10,
            "Price < minimum bid price + condition"
        );
        // require(ITailorNFTCollectible(ticketNFT.collection)._exists(takerOrder.tokenId), "NFT token doesn't exist");
        IAuctionStrategy(ticketNFT.strategy).reimburseLastBidder(
            ticketNFT.highestbidder
        );

        // 1. If reserve Price is reached
        if (_hasMatchedPrice(ticketNFT.price, takerOrder.price)) {
            executeBidPrice(takerOrder.taker, takerOrder.tokenId);
        } else {
            //(bool success, bytes memory result) =ticketNFT.strategy.delegatecall(abi.encodeWithSignature("receiveToken(OrderTypes.NFTOrder),ticketNFT")) ;
            // 2. Place normal bid
            transferAuctionBid(ticketNFT, takerOrder);
        }
    }

    function transferAuctionBid(
        OrderTypes.NFTOrder storage listedOrder,
        OrderTypes.TakerOrder calldata takerOrder
    ) internal nonReentrant {
        require(
            (listedOrder.isSellOrder) && !(takerOrder.isSellOrder),
            "transfer can not be done"
        );
        IERC20(listedOrder.currencyToken).transferFrom(
            takerOrder.taker,
            listedOrder.strategy,
            takerOrder.price
        );
        uint256 originalMinBid = listedOrder.minBid;
        listedOrder.highestbidder = takerOrder.taker;
        listedOrder.minBid = takerOrder.price;

        emit NewBidNFT(
            listedOrder.collection,
            listedOrder.signer,
            listedOrder.highestbidder,
            listedOrder.feesAddress,
            takerOrder.tokenId,
            takerOrder.price,
            originalMinBid
        );
    }

    /**
     * @notice Transfer fees and funds to royalty recipient, protocol, and seller
     */

    function _calculateFees(uint256 amount) internal view returns (uint256) {
        uint256 fees = viewSecondaryMarketFee();
        return (amount * fees) / 10000;
    }

    function updateRoyaltyAndFees(
        uint256 tokenId //faire ça offchain
    ) public returns (OrderTypes.NFTOrder memory) {
        OrderTypes.NFTOrder memory order = marketNFTData[tokenId];

        // if bid placed -> return bid to sender
        // payable(msg.sender).transfer(amount);

        // 1. Protocol fees
        order.feesAddress = _feesAddress;
        order.secondaryMarketFee = _secondaryMarketFee; //_calculateFees(order.price);

        // 2. Royalty fees
        (
            order.royaltyFeeRecipient,
            order.royaltyFeeAmount
        ) = _calculateRoyaltyFee(order.collection, order.tokenId);

        marketNFTData[order.tokenId] = order;
        return (order);
    }

    function _calculateRoyaltyFee(address collection, uint256 tokenId)
        internal
        view
        returns (address payable, uint96)
    {
        // 1. Check if there is royalty info
        (uint128 collectionId, , ) = ITailorNFTCollectible(collection)
            .getNFTinfo(tokenId);

        (address royaltyReceiver, uint96 royaltyAmount) = ITailorNFTCollectible(
            collection
        ).getCollectionRoyalty(collectionId);
        return (payable(royaltyReceiver), royaltyAmount);
    }

    function _transferNFT(
        address collection,
        address from,
        address to,
        uint256 tokenId
    ) internal {
        ITailorNFTCollectible(collection).transferFrom(from, to, tokenId);

        /// @custom:oz-upgrades-unsafe-allow delegatecall
        // (bool success, bytes memory error) = address(collection).call(abi.encodeWithSignature("transferFrom(address,address,uint256)", from, to, tokenId));
        // require(success, "Delegate call transferFrom failed");
    }

    function _validateOrder(OrderTypes.NFTOrder calldata ticketOrder)
        internal
        view
    {
        // verify the nonce of the tx is executed
        // verify the signature of the order

        // verify the signer is not address(0)
        require(
            ticketOrder.signer != address(0),
            "Seller must be not address(0)"
        );

        // verify that the strategy is whitelisted
        require(
            _isStrategyWhiteListed(ticketOrder.strategy),
            "Strategy address is not listed"
        );
    }

    function _isStrategyWhiteListed(address strategy)
        internal
        view
        returns (bool)
    {
        return _whitelistedStrategies.contains(strategy);
    }

    function _isAuctionEnded(OrderTypes.NFTOrder storage ticketNFT)
        internal
        view
        returns (bool)
    {
        return IAuctionStrategy(ticketNFT.strategy).isEnded();
    }

    function executeAuction(uint256 tokenID) public onlyAdmin {
        OrderTypes.NFTOrder storage ticketNFT = marketNFTData[tokenID];
        require(
            _hasReachedDeadline(ticketNFT.endTime) ||
                _isAuctionEnded(ticketNFT),
            "Auction is not finished"
        );
        require(
            ticketNFT.status == OrderTypes.NFTState.AUCTIONABLE,
            "NFT does not have the status AUCTIONABLE"
        );

        address originalOwner = ticketNFT.signer;
        ticketNFT.status = OrderTypes.NFTState.PENDING;

        // 1. Transfer NFT
        transferAndChangeProperties(ticketNFT, originalOwner);
		
    }

    function endAuction(OrderTypes.NFTOrder calldata ticketNFT)
        external
        onlyAdmin
    {

        IAuctionStrategy(ticketNFT.strategy).changeAuctionStatus(1);
        executeAuction(ticketNFT.tokenId);
        
    }

    function withdrawlastBid(OrderTypes.NFTOrder calldata listedOrder)
        external
    {
        uint256 balance = IERC20(listedOrder.currencyToken).balanceOf(
            listedOrder.strategy
        );
        OrderTypes.ParamsVariable[3] memory auctionParams = listedOrder
            .getAuctionVariables(balance);

        // require(msg.sender == listedOrder.signer || hasRole(TAILOR_ADMIN, _msgSender()),"the signer has to be the seller");
        // // require(signer == initialOwner, "the signer has to be the seller");
        // require(
        //     listedOrder.status == OrderTypes.NFTState.SOLD,
        //     "Can not withdraw balance unfinished auction"
        // );

        // require(IAuctionStrategy(listedOrder.strategy).isEnded(), "the auction isn't ended yet");
        require(
            IAuctionStrategy(listedOrder.strategy).withdrawBalance(
                auctionParams,
                msg.sender
            ),
            "could not withdraw"
        );
        IAuctionStrategy(listedOrder.strategy).destruct();

        emit StrategyBalanceWithdrawn(
            listedOrder.collection,
            listedOrder.tokenId,
            listedOrder.strategy,
            balance
        );
		
    }

    function transferAndChangeProperties(
        OrderTypes.NFTOrder storage ticketNFT,
        address originalOwner
    ) private {
        _transferNFT(
            ticketNFT.collection,
            ticketNFT.signer,
            ticketNFT.highestbidder,
            ticketNFT.tokenId
        );

        ticketNFT.signer = payable(ticketNFT.highestbidder);
        ticketNFT.status = OrderTypes.NFTState.SOLD;
        ticketNFT.sellstatus = OrderTypes.PurchaseState.NONSELLABLE;
         delete marketNFTData[ticketNFT.tokenId];
        emit NFTSold(
            ticketNFT.collection,
            ticketNFT.signer,
            _feesAddress,
            (ticketNFT.price * ticketNFT.secondaryMarketFee) / 10000,
            ticketNFT.royaltyFeeRecipient,
            (ticketNFT.price * ticketNFT.royaltyFeeAmount) / 10000,
            originalOwner,
            ticketNFT.tokenId,
            ticketNFT.price
        );
    }

    function executeBidPrice(address to, uint256 tokenId) private onlyAdmin {
        OrderTypes.NFTOrder storage ticketNFT = marketNFTData[tokenId];
        require(to != address(0), "Highest bidder can not be address(0)");
        IAuctionStrategy(getMarketListing(tokenId).strategy)
            .changeAuctionStatus(1);

        address originalOwner = ticketNFT.signer;
        ticketNFT.status = OrderTypes.NFTState.PENDING;
        ticketNFT.highestbidder = to;
        IERC20(ticketNFT.currencyToken).transferFrom(
            to,
            ticketNFT.strategy,
            ticketNFT.price
        );
        // 1. Transfer NFT
        transferAndChangeProperties(ticketNFT, originalOwner);
    }

    function _hasReachedDeadline(uint256 deadline) private view returns (bool) {
        return block.timestamp >= deadline;
    }

    function _hasMatchedPrice(uint256 askPrice, uint256 price)
        private
        pure
        returns (bool)
    {
        return askPrice == price;
    }

    function cancelListing(OrderTypes.NFTOrder memory NFTOrder)
        external
        onlyNFTOwner(NFTOrder.collection, NFTOrder.signer, NFTOrder.tokenId)
        isListed(NFTOrder.tokenId)
        notBiddedOn(NFTOrder.tokenId)
        onlyAdmin
    {
        marketNFTData[NFTOrder.tokenId].status = OrderTypes.NFTState.NOTLISTED;
        marketNFTData[NFTOrder.tokenId].sellstatus = OrderTypes
            .PurchaseState
            .NONSELLABLE;
        emit CancelListing(
            NFTOrder.collection,
            NFTOrder.signer,
            NFTOrder.tokenId
        );
    }

    function modifyListing(OrderTypes.NFTOrder memory NFTOrder)
        external
        onlyNFTOwner(NFTOrder.collection, NFTOrder.signer, NFTOrder.tokenId)
        isListed(NFTOrder.tokenId)
        notBiddedOn(NFTOrder.tokenId)
        onlyAdmin
        acceptedCurrency(NFTOrder.currencyToken)
    {
        marketNFTData[NFTOrder.tokenId] = NFTOrder;
        emit ModifiedListing(
            NFTOrder.collection,
            NFTOrder.signer,
            NFTOrder.tokenId
        );
    }

    function toBN(uint256 number) internal pure returns (uint256) {
        return (number * 10**18);
    }

    function setPendingStatus(uint128 tokenID) external onlyAdmin {
        OrderTypes.NFTOrder storage ticketNFT = marketNFTData[tokenID];
