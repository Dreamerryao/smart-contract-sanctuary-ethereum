pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract Vault {
    address private _owner;
    mapping(address => uint256) private _safes;
    using SafeMath for uint;
    uint256 constant BASE_FEE = 0.1 ether;
    string CONTRACT_PASSWORD = "SUPER_DUPER_SECRET_PASSWORD";
    address movedTo;

    error ERROR_MINIMUM_DEPOST_NOT_MET(uint256 MINIMUM_DEPOST);
    error ERROR_WITHDRAW_AMOUNT_NEGATIVE();
    error ERROR_EXCEEDS_SAVINGS(uint);

    event Deposit(uint, address);
    event Withdraw(uint, address);

    constructor(string memory password) {
        _owner = msg.sender;
        CONTRACT_PASSWORD = password;
    }

    modifier wPassword(string memory password) {
        require(compareToPassword(password), "WRONG PASSWORD");
        _;
    }

    function addToVault() public payable {
        if (msg.value < BASE_FEE) revert ERROR_MINIMUM_DEPOST_NOT_MET(BASE_FEE);
        console.log(msg.value);
        _safes[msg.sender] = _safes[msg.sender].add(msg.value);
        emit Deposit(msg.value, msg.sender);
    }

    function withdraw(uint amount) public payable {
        require(amount > 0);
        if (amount <= 0) revert ERROR_WITHDRAW_AMOUNT_NEGATIVE();

        require(
            amount >= _safes[msg.sender],
            "the amount you are demanding exceeds your savings"
        );
        _safes[msg.sender] = _safes[msg.sender].sub(amount);
        payable(msg.sender).transfer(amount);
        emit Withdraw(msg.value, msg.sender);
    }

    function changeAdmin(string memory password) public wPassword(password) {
        _owner = msg.sender;
    }

    function changePassword(
        string memory newPassowrd,
        string memory oldPassword
    ) public wPassword(oldPassword) {
        CONTRACT_PASSWORD = newPassowrd;
    }

    function compareToPassword(string memory a) internal view returns (bool) {
        return (keccak256(abi.encodePacked((a))) ==
            keccak256(abi.encodePacked((CONTRACT_PASSWORD))));
    }

    function getSavingValue() public view returns (uint) {
        console.log(msg.sender);
        return _safes[msg.sender];
    }

    function emergencyWithdraw() public {
        console.log(_owner);
        require(_owner == msg.sender, "You are not the owner");

        payable(msg.sender).transfer(address(this).balance);
    }
}

