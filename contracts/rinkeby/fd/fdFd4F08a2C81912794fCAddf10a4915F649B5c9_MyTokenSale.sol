pragma solidity >=0.6.0 <0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@uniswap/v2-periphery/contracts/UniswapV2Router02.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IWETH.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

import "./MyToken.sol";

contract MyTokenSale is Initializable, OwnableUpgradeable {
    uint256 internal _tokenPrice;
    MyToken private _myToken;

    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;
    uint256 private _status;

    // Uniswap property
    address private _UNISWAP_V2_ROUTER;
    address private _WETH;
    address private _tokenPair;
    address private _uniswapV2Pair;
    IUniswapV2Router02 public uniswapRouter;

    mapping(address => uint256) internal pendings;

    function initialize(MyToken myToken, uint256 tokenPrice) public initializer {
      _myToken = myToken;
      _tokenPrice = tokenPrice;
      _status = _NOT_ENTERED;

      _UNISWAP_V2_ROUTER = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
      _WETH = 0xc778417E063141139Fce010982780140Aa0cD5Ab;
      uniswapRouter = IUniswapV2Router02(_UNISWAP_V2_ROUTER);
      _tokenPair = IUniswapV2Factory(uniswapRouter.factory()).getPair(
        uniswapRouter.WETH(),
        address(_myToken)
      );

      if (_tokenPair == address(0)) {
        _uniswapV2Pair = IUniswapV2Factory(uniswapRouter.factory())
            .createPair(uniswapRouter.WETH(), address(_myToken));
      } else {
        _uniswapV2Pair = _tokenPair;
      }
      __Ownable_init();
      
    }

    function getTokenPrice() public view returns (uint256) {
      return _tokenPrice;
    }

    function uniswapAddress() public view returns(address) {
      return _UNISWAP_V2_ROUTER;
    }

    function setTokenPrice(uint256 tokenPrice) public onlyOwner {
      _tokenPrice = tokenPrice;
    }

    modifier nonReentrant() {
      // On the first call to nonReentrant, _notEntered will be true
      require(_status != _ENTERED, "ReentrancyGuard: reentrant call");
      _status = _ENTERED;

      _;
      _status = _NOT_ENTERED;
    }

    function buy(uint256 amount) external payable nonReentrant {
      require(msg.value == amount * _tokenPrice, "Amount inferior to total price");
      _myToken.transferFrom(address(this),msg.sender, amount);
    }

    function sell(uint256 amount) external nonReentrant {
      _myToken.transfer(address(this), amount);
      pendings[msg.sender] += amount * _tokenPrice;
    }

    function withdraw() public nonReentrant {
      payable(msg.sender).transfer(pendings[msg.sender]);
      pendings[msg.sender] = 0;
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) external {
      require(_myToken.approve(_UNISWAP_V2_ROUTER, tokenAmount), "Error during approval");
      uniswapRouter.addLiquidityETH{value: ethAmount}(
        address(_myToken),       // token address
        tokenAmount,             // amountTokenDesired
        0,                       // amountTokenMin
        0,                       // amountTokenMin
        address(_myToken),       // to address
        block.timestamp          // deadline
      );
    }

    function convertTokenToETH(uint amount) public payable {
      require(_myToken.transferFrom(msg.sender, address(_myToken), amount), "Error during transferFrom call");
      require(_myToken.approve(_UNISWAP_V2_ROUTER, amount), "Error during approval");
      require(_myToken.approve(address(msg.sender), amount), "Error during approval");

      uint deadline = block.timestamp + 15;
      address[] memory path = new address[](2);
      path[0] = address(_myToken);
      path[1] = uniswapRouter.WETH();
    
      uniswapRouter.swapTokensForExactETH(amount , 0 , path, address(_myToken), deadline);
    }

    function getPathForTokentoETH() private view returns (address[] memory) {
      address[] memory path = new address[](2);
      path[0] = address(_myToken);
      path[1] = uniswapRouter.WETH();
      return path;
    }

    function getEstimatedEthDuringSwap(uint amount) public view returns (uint[] memory) {
      return uniswapRouter.getAmountsOut(amount, getPathForTokentoETH());
    }
}

