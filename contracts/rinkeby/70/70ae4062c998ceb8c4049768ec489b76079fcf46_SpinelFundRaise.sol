interface IClubManager {
  function isSupportedToken(address token) external view returns (bool);
}
contract SpinelFundRaise is
  Initializable,
  UUPSUpgradeable,
  OwnableUpgradeable
{
  using SafeMath for uint256;

  uint256 fee;
  uint256 providerFee;
  uint256 constant capFee = 10000;
  uint8 constant nativeDecimal = 18;
  uint256 constant divident = 1000000;
  address public treasury;
  address public manager;
  mapping(uint256 => SharedStructs.PhaseDetail[]) phases;

  event FundProvided(address owner, uint256 clubId, uint256 amount, uint256 fee, uint256 token, uint256 phaseId);
  event PhaseCreated(uint256 clubId, string[] phaseDetail, address fundToken, uint256[] data);
  event PhaseCancel(uint256 clubId, uint256 phaseId);

 modifier authority(uint256 _clubId, string memory _action) {
    ITreasury(treasury).gainAccess(_clubId, msg.sender, _action);
    _;
  }

  function initialize(address _manager, address _treasury)
    public
    // ExtendUpgradeableERC20 token_
    initializer
  {
    manager = _manager;
    treasury = _treasury;
    fee = 0;
    providerFee = 0;

    __Ownable_init();
  }
  function _authorizeUpgrade(address) internal override onlyOwner {}

  function setManagerAddress(address _manager) public onlyOwner {
     manager = _manager;
  }
  function setTreasuryAddress(address _treasury) public onlyOwner {
     treasury = _treasury;
  }

  function provideFund(uint256 _clubId, uint256 amount, uint256 _phaseId) public payable {
    SharedStructs.TreasuryData memory treasuryData = ITreasury(treasury).getTreasuryData(_clubId);
    require(treasuryData.owner != address(0), "not existed");
    require(ITreasury(treasury).isMember(_clubId, msg.sender), "member only");
    require(phases[_clubId].length > _phaseId, "invalid phase");
    if(phases[_clubId][_phaseId].phaseType == 1) {
      address[] memory wl = phases[_clubId][_phaseId].whitelist;
      bool isWl = false;
      for (uint256 i = 0; i < wl.length; i++) {
        if(wl[i] == msg.sender) {
          isWl = true;
        }
      }
      require(isWl, "not in whitelist"); 
    }

    SharedStructs.PhaseDetail storage rs = phases[_clubId][_phaseId];
    uint256 fundDecimal = nativeDecimal;
    bool raiseInEther = false;
    if(rs.fundToken == address(0)) {
      require(amount == msg.value, "amount not match");
      raiseInEther = true;
    } else {
      fundDecimal = ERC20Upgradeable(rs.fundToken).decimals();
    }
    require(fundDecimal <= 18, "decimal exceed limit");
    
    require(amount >= rs.minBuy, "Amount too small");
    require(amount <= rs.maxBuy, "Amount exceed limit");
    require(!rs.isCancel, "phase has been cancel");
    require(rs.startBlock <= block.number && rs.endBlock >= block.number, "invalid time");

    uint256 totalFee = fee.mul(amount).div(capFee);
    uint256 remainingBalance = amount.sub(totalFee, "SafeMath: subtraction overflow");
    
      if(raiseInEther) {
        if(totalFee > 0) {
          payable(treasuryData.feeAddress).transfer(totalFee);
        }
        payable(treasuryData.treasuryAddress).transfer(1000);
      } else {
        if(totalFee > 0) {
          bool transferFeeResult = IERC20(rs.fundToken).transferFrom(
              msg.sender,
              treasuryData.feeAddress,
              totalFee
            );
          require(transferFeeResult, "fee move fail");
        }

        bool transferResult = IERC20(rs.fundToken).transferFrom(
          msg.sender,
          treasuryData.treasuryAddress,
          remainingBalance
        );
        require(transferResult, "Deposit token fail");
      }
    

    uint256 decimal = ERC20Upgradeable(address(treasuryData.votePower)).decimals() - fundDecimal;
    uint256 offset = 10**decimal;
    uint256 tokenAmount = remainingBalance.mul(offset).mul(rs.price).div(divident);
    require(treasuryData.votePower.totalSupply() + tokenAmount < treasuryData.totalSupply, "Club reach max raise");
    IVotePower(address(treasuryData.votePower)).mint(msg.sender, tokenAmount);
    rs.tokenSold += tokenAmount;
    require(rs.tokenSold <= rs.tokenAmount, "exceed fund");
    emit FundProvided(msg.sender, _clubId, amount, totalFee, tokenAmount, _phaseId);
  }
// 0: tokenAmount
// 1: price
// 2: startBlock
// 3: endBlock
// 4: phaseType
function createPhase(uint256 _clubId, string memory _phaseName, string memory _phaseDetail, address _fundToken, uint256[] memory _input, address[] memory _whitelist) public authority(_clubId, "createPhase"){
    SharedStructs.TreasuryData memory treasuryData = ITreasury(treasury).getTreasuryData(_clubId);
    require(treasuryData.owner == msg.sender);
    require(_input[2] < _input[3] && _input[2] > block.number, "invalid block");
    require(IClubManager(manager).isSupportedToken(_fundToken), "invalid token");
    require(_input[5] < _input[6], "invalid limit");
    phases[_clubId].push();
    SharedStructs.PhaseDetail storage rs = phases[_clubId][phases[_clubId].length -1];
    rs.tokenAmount = _input[0];
    rs.price = _input[1];
    rs.blocknum = block.number;
    rs.startBlock = _input[2];
    rs.endBlock = _input[3];
    rs.index = phases[_clubId].length -1;
    rs.phaseName = _phaseName;
    rs.phaseType = _input[4];
    rs.whitelist = _whitelist;
    rs.fundToken = _fundToken;
    rs.phaseDetail = _phaseDetail;
    rs.minBuy = _input[5];
    rs.maxBuy = _input[6];
    string[] memory phaseDetail = new string[](2);
    phaseDetail[0] = _phaseName;
    phaseDetail[1] = _phaseDetail;
    uint256[] memory data = new uint256[](8);
    data[0] = _input[0];
    data[1] = _input[1];
    data[2] = _input[2];
    data[3] = _input[3];
    data[4] = _input[4];
    data[5] = _input[5];
    data[6] = _input[6];
    data[7] = rs.index;
    emit PhaseCreated(_clubId, phaseDetail, _fundToken, data);
}
  
  function cancelPhaseDetail(uint256 _clubId, uint256 _id) public authority(_clubId, "createPhase"){
    SharedStructs.TreasuryData memory treasuryData = ITreasury(treasury).getTreasuryData(_clubId);
    require(treasuryData.owner == msg.sender);
    require(phases[_clubId].length > _id, "invalid phase");
    require(!phases[_clubId][_id].isCancel, "phase was cancel already");
    phases[_clubId][_id].isCancel = true;
    emit PhaseCancel(_clubId, _id);
  }
  
  function getPhaseDetail(uint256 _clubId, uint256 _phaseId) public view returns (SharedStructs.PhaseDetail memory){
    return phases[_clubId][_phaseId];
  }

  function getPhases(uint256 _clubId) public view returns (SharedStructs.PhaseDetail[] memory){
    return phases[_clubId];
  }


  function migratePhase(SharedStructs.PhaseDetail[] memory datas) external {
      require(msg.sender == manager, "manager only");
      for (uint256 i = 0; i < datas.length; i++) {
        SharedStructs.PhaseDetail memory data = datas[i];
        phases[data.clubId].push(data);
        require(data.index == phases[data.clubId].length - 1, "invalid index");

        uint256 phaseIndex = data.index;
        if(phases[data.clubId][phaseIndex].isCancel) {
            emit PhaseCancel(data.clubId, phaseIndex);
        } else {
          string[] memory phaseDetail = new string[](2);
          phaseDetail[0] = phases[data.clubId][phaseIndex].phaseName;
          phaseDetail[1] = phases[data.clubId][phaseIndex].phaseDetail;
          uint256[] memory eventData = new uint256[](8);
          eventData[0] = phases[data.clubId][phaseIndex].tokenAmount;
          eventData[1] = phases[data.clubId][phaseIndex].price;
          eventData[2] = phases[data.clubId][phaseIndex].startBlock;
          eventData[3] = phases[data.clubId][phaseIndex].endBlock;
          eventData[4] = phases[data.clubId][phaseIndex].phaseType;
          eventData[5] = phases[data.clubId][phaseIndex].minBuy;
          eventData[6] = phases[data.clubId][phaseIndex].maxBuy;
          eventData[7] = phases[data.clubId][phaseIndex].index;
          emit PhaseCreated(data.clubId, phaseDetail, phases[data.clubId][phaseIndex].fundToken, eventData);
        }
      }
  }

    receive() external payable {}
}

