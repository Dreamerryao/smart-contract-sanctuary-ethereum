pragma solidity ^0.8.4;

import "hardhat/console.sol";

contract SenryuContract {
    //川柳のデータの塊（＝構造体）Senryuを宣言
  struct Senryu {
    //Senryuを作成したアドレス
    address owner;
    //Senryuのid(0から始まる)
    uint256 id;
    //Senryuを読んだ柳人名。　例：松尾芭蕉
    string poet;
    //初句。　例：古池や
    string first;
    //二句。　例：蛙飛び込む
    string second;
    //結句。　例：水の音
    string end;
    //いいね数
    uint256 likeCount;
  }
  //Senryuを配列senryusに格納
  Senryu[] public senryus;

  //キー(address型)を渡すとバリュー(uint型)を返す関数
  //msg.sender(=自分)が作成したSenryuのID（複数可）を取得する
  mapping(address => uint256[]) public ownerToSenryu;

  //作られたSenryuの数を取得する関数
  function getLength() public view returns (uint256) {
    //senryus.length = senryusの長さ（＝数）  
    return senryus.length;
 }

  //Senryuを作成する関数
  function readSenryu(
    //Senryuに入力する値 = 引数
    string memory _first, 
    string memory _second, 
    string memory _end, 
    string memory _poet
  ) public {
    //ownerにmsg.sender(関数を実行したアドレス)を入力
    address owner = msg.sender;
    //idにsenryus.length(=最初0でSenryuが追加される度1づつ増える)を入力
    uint256 id = senryus.length;
    //likeCountに初期値0を入力
    uint256 likeCount = 0;
    //Senryu構造体からnewSenryuを作り、owner, id, likeCountと引数を構造体の順番通り渡す
    Senryu memory newSenryu = Senryu(owner, id, _poet, _first, _second, _end, likeCount);
    //senryus配列に追加する
    senryus.push(newSenryu);
    //ownerToSenryuマッピングに追加する
    ownerToSenryu[owner].push(id);
    console.log(_first, _second, _end, _poet);
  }
  //id(_id)のSenryuのlikeCountを+1する関数
  function addLikeCount(uint256 _id) public {
    senryus[_id].likeCount++;
  }

  function getSenryu(address _owner) public view returns(uint256[] memory){
      uint numOfSenryu = ownerToSenryu[_owner].length;
      uint[] memory ids = new uint[](numOfSenryu);

      for(uint i=0; i < numOfSenryu; i++) {
            uint id = ownerToSenryu[_owner][i];
            ids[i] = (senryus[id].id);
        }
        return ids;
  }
}

