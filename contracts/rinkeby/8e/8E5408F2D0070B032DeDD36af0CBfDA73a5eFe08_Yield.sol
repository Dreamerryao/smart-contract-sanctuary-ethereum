pragma solidity ^0.8.0;


import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@forbitswap/v3-core/contracts/libraries/TickMath.sol";
import "./libraries/LiquidityAmounts.sol";
import "./libraries/Math.sol";
import "./IYield.sol";

contract Yield is Ownable, IYield {
    using SafeMath for uint256;
    using SafeERC20 for ERC20;


//----------------------------------------------------------------------------
//      VARIABLE
//----------------------------------------------------------------------------

    ERC20 public forbitspace;
    ERC721 public NFTAddress;
    uint256 public bonusEndBlock;
    uint256 public FBSPerBlock;
    address public walletFBS;
    uint256 public constant BONUS_MULTIPLIER = 10;

    mapping(uint256 => PoolInfo) public poolInfo;
    mapping(bytes32 => mapping (address => UserInfo)) public userInfo;
    uint256 public totalAllocPoint = 0;
    uint256 public startBlock;
    uint256 public poolCount;

//----------------------------------------------------------------------------
//      CONSTRUCTOR
//----------------------------------------------------------------------------

    constructor(
        address _FBSToken,
        address _NFT_address,
        uint256 _FBSPerBlock,
        uint256 _startBlock,
        uint256 _bonusEndBlock
    ) {
        forbitspace = ERC20(_FBSToken);
        NFTAddress = ERC721(_NFT_address);
        FBSPerBlock = _FBSPerBlock;
        startBlock = _startBlock;
        bonusEndBlock = _bonusEndBlock;
    }

//----------------------------------------------------------------------------
//      FUNCTION
//---------------------------------------------------------------------------- 
    function poolLength() external view override returns (uint256) {
        return poolCount;
    }

    function initialize(address tokenA, address tokenB, uint256 _allocPoint, bool _update) public override onlyOwner {
        if(_update) {
            updateAllPool();
        }
        (address token0, address token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        bytes32 pool = keccak256(abi.encodePacked(token0, token1));
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo[poolCount] = PoolInfo(
            poolCount,
            pool,
            token0,
            token1,
            0,
            _allocPoint,
            lastRewardBlock,
            0
        );
        poolCount++;
    }

    function configPool(uint256 _pid, uint256 _allocPoint, bool _update) public override onlyOwner {
        if(_update) {
            updateAllPool();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
    }

    function updateAllPool() override public {
        uint256 length = poolCount;
        for(uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        } 
    }
    // Import from IYield.sol
    function updatePool(uint256 _pid) public override returns (PoolInfo memory pool) {
       pool = poolInfo[_pid];
        if (block.number > pool.lastRewardBlock) {
            uint256 amountLP = pool.totalAmount;
            if (amountLP > 0) {
                uint256 blocks = block.number.sub(pool.lastRewardBlock);
                uint256 reward = blocks.mul(FBSPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
                pool.accFBSPerShare = pool.accFBSPerShare.add(reward / amountLP);
            }

            pool.lastRewardBlock = block.number;
            poolInfo[_pid] = pool;
        }
    }

    function pendingFBS(uint256 _pid, address _user) public override view returns(uint256 pending) {
        PoolInfo memory Pool = poolInfo[_pid];
        UserInfo memory user = userInfo[Pool.pool][_user];
        uint256 accFBSPerShare = Pool.accFBSPerShare;
        uint256 lpSupply = Pool.totalAmount;
        if(block.number > Pool.lastRewardBlock && lpSupply > 0) {
            uint256 multiplier = getMultiplier(Pool.lastRewardBlock, block.number);
            uint256 FBSReward = multiplier.mul(FBSPerBlock).mul(Pool.allocPoint).div(totalAllocPoint);
            accFBSPerShare = accFBSPerShare.add(FBSReward.mul(1e12).div(lpSupply));
        }
        pending = user.amount.mul(accFBSPerShare).div(1e12) < user.rewardDebt ? 
            0 
            : user.amount.mul(accFBSPerShare).div(1e12).sub(user.rewardDebt);
    }

    function getMultiplier(uint256 _from, uint256 _to) public view override returns (uint256) {
        if(_to <= bonusEndBlock) {
            return _to.sub(_from).mul(BONUS_MULTIPLIER);
        } else if (_from >= bonusEndBlock) {
            return _to.sub(_from);
        } else {
            return bonusEndBlock.sub(_from).mul(BONUS_MULTIPLIER).add(
                _to.sub(bonusEndBlock)
            );
        }
    }

    function calculateAmount(
        uint160 sqrtPriceX96,
        int24 tickLower,
        int24 tickUpper,
        uint128 liquidity
    ) public pure override returns (uint256 amount0, uint256 amount1){

        // Calculate sqrt form tick
        uint160 sqrtRatioAX96 = TickMath.getSqrtRatioAtTick(tickLower);
        uint160 sqrtRatioBX96 = TickMath.getSqrtRatioAtTick(tickUpper);

        // Get amount0, amount1 from sqrtPrice
        (amount0, amount1) = LiquidityAmounts.getAmountsForLiquidity(sqrtPriceX96, sqrtRatioAX96, sqrtRatioBX96, liquidity);
    }

    function deposit(depositParams calldata params) override public {
        PoolInfo storage Pool = poolInfo[params.pid];
        UserInfo storage user = userInfo[Pool.pool][msg.sender];
        updatePool(params.pid);
        (uint256 amount0, uint256 amount1) = calculateAmount(
            params.sqrtPriceX96,
            params.tickLower,
            params.tickUpper,
            params.liquidity
        );
        uint256 LP = Math.sqrt(amount0.mul(amount1));
        // Transfer NFT from sender to contract
        NFTAddress.transferFrom(msg.sender, address(this), params.tokenId);
        
        // Update data user
        user.amount = user.amount.add(LP);
        Pool.totalAmount = Pool.totalAmount.add(LP);
        emit Deposit(msg.sender, Pool.pool, LP);
    }

    function harvest(uint256 _pid) public override {
        PoolInfo storage Pool = poolInfo[_pid];
        UserInfo storage user = userInfo[Pool.pool][msg.sender];

        require(user.amount > 0, 'Not thing to harvest');

        uint256 calculateReward = user.amount.mul(Pool.accFBSPerShare);
        uint256 pendingReward = calculateReward.sub(user.rewardDebt); // calculate amount token reward from block
        user.rewardDebt = calculateReward;

        safeFBSTransfer(msg.sender, pendingReward);

        emit Harvest(msg.sender, Pool.pool, pendingReward);
    }

    function withdraw(withdrawParams calldata params) public override {
        PoolInfo storage Pool = poolInfo[params.pid];
        UserInfo storage user = userInfo[Pool.pool][msg.sender];

        require(user.amount > 0, 'Not thing to withdraw');
        updatePool(params.pid);
        (uint256 amount0, uint256 amount1) = calculateAmount(
            params.sqrtPriceX96,
            params.tickLower,
            params.tickUpper,
            params.liquidity
        );
        uint256 LP = Math.sqrt(amount0.mul(amount1));

        user.amount = user.amount.sub(LP);

        NFTAddress.safeTransferFrom(address(this), msg.sender, params.tokenId);

        emit Withdraw(msg.sender, Pool.pool, LP);
    }

    function harvestAndWithdraw(withdrawParams calldata params) public override {
        PoolInfo storage Pool = poolInfo[params.pid];
        UserInfo storage user = userInfo[Pool.pool][msg.sender];

        require(user.amount > 0, 'Not thing to harvest & withdraw');
        
        (uint256 amount0, uint256 amount1) = calculateAmount(
            params.sqrtPriceX96,
            params.tickLower,
            params.tickUpper,
            params.liquidity
        );
        uint256 LP = Math.sqrt(amount0.mul(amount1)).mul(1e18);

        uint256 calculateReward = user.amount.mul(Pool.accFBSPerShare); // calculate token reward harvest
        uint256 pendingReward = calculateReward.sub(user.rewardDebt);

        user.amount = user.amount.sub(LP);
        user.rewardDebt = calculateReward;

        safeFBSTransfer(msg.sender, pendingReward);
        NFTAddress.safeTransferFrom(address(this), msg.sender, params.tokenId);

        emit HarvestAndWithdraw(msg.sender, Pool.pool, LP);
    }

    function safeFBSTransfer(address _to, uint256 amount) private {
        uint256 FBSBalance = forbitspace.balanceOf(address(this));
        if(amount > FBSBalance) {
            forbitspace.transfer(_to, FBSBalance);
        } else {
            forbitspace.transfer(_to, amount);
        }
    }

    function getOrders() view public returns(PoolInfo[] memory p){
        p = new PoolInfo[](poolCount);
        for(uint256 i = 0; i < poolCount; i++) {
            p[i] = poolInfo[i];
        }
    }
}

