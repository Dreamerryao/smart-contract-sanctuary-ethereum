pragma solidity ^0.8.14;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import '@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol';
import "./IOps.sol";
import "./IBabyDogeRouter.sol";

//fixme
import "hardhat/console.sol";

//todo: nonreentrant?
//todo: swap custom errors
contract BbdSwapLimitOrder is EIP712 {
    /* An ECDSA signature. */
    struct Sig {
        /* v parameter */
        uint8 v;
        /* r parameter */
        bytes32 r;
        /* s parameter */
        bytes32 s;
    }

    //todo: cancel order

    struct OrderInfo {
        address account;
        uint64 deadline;
        bytes32 gelatoHash;
    }

    struct Order {
        address account;
        address receiver;
        uint256 deadline;
        address tokenIn;
        address tokenOut;
        uint256 amountIn;
        uint256 amountOutMin;
    }

    bytes32 constant ORDER_TYPEHASH = keccak256(
        "Order(address account,address receiver,uint256 deadline,address tokenIn,address tokenOut,uint256 amountIn,uint256 amountOutMin)"
    );

    address public immutable ops;
    address payable public immutable gelato;
    address public immutable WETH;
    IBabyDogeRouter public immutable router;

    address private constant GELATO_ETH = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;

    mapping(bytes32 => OrderInfo) public orders;

    //todo: events
    event LimitOrderCreated(
        address indexed account,
        address receiver,
        uint256 deadline,
        bytes32 indexed orderHash,
        bytes32 gelatoHash,
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 amountOutMin
    );

    event SwapTokenToToken(
        address account,
        address receiver,
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 amountOut,
        bytes32 gelatoHash
    );

    event OrderCancelled(
        address indexed account,
        bytes32 indexed orderHash
    );

    modifier onlyOps() {
        require(msg.sender == ops, "OpsReady: onlyOps");
        _;
    }

    /**
     * @param _ops - Gelato Ops address
     * @param _router - BBD Router address
     */
    constructor(
        address _ops,
        IBabyDogeRouter _router
    ) EIP712("BbdSwapLimitOrder","1") {
        ops = _ops;
        gelato = IOps(_ops).gelato();

        router = _router;
        WETH = _router.WETH();
    }


    /**
     * @notice Creates limit order
     * @param order - Order, signed by user
     * @param sig - Signature of signing the order
     * @param path - Swap path
     * @param feePath - Empty if this is ETH<>Token or Token<>ETH swap. Or TokenIn -> ETH path for gelato fee payment
     * @dev feePath[0] must be either input or output token of the main path, But it can't be taxed token
     */
    function createLimitOrder(
        Order calldata order,
        Sig calldata sig,
        address[] calldata path,
        address[] calldata feePath
    ) external payable {
        address _WETH = WETH;
        bytes32 orderHash = buildOrderHash(order);

        {
            (address recoveredAddress,) = ECDSA.tryRecover(orderHash, sig.v, sig.r, sig.s);
            require(msg.sender == recoveredAddress, 'Bad signature');
        }

        if (order.tokenIn != _WETH && order.tokenOut != _WETH) {
            require(
                (feePath[0] == order.tokenIn || feePath[0] == order.tokenOut)
                && feePath[feePath.length - 1] == _WETH,
                "Invalid fee path"
            );
        }

        //todo: gelatoHash
        bytes32 gelatoHash;
        if (order.tokenIn == _WETH) {
            //todo: msg.value check
            gelatoHash = IOps(ops).createTaskNoPrepayment(
                address(this),
                this.swapTokensForETH.selector,
                address(this),
                abi.encodeWithSelector(
                        this.check.selector,
                            order,
                            sig,
                            path,
                            feePath
                    ),
                GELATO_ETH
            );
        } else if (order.tokenOut == _WETH) {
            gelatoHash = IOps(ops).createTaskNoPrepayment(
                address(this),
                this.swapETHForTokens.selector,
                address(this),
                abi.encodeWithSelector(
                    this.check.selector,
                        order,
                        sig,
                        path,
                        feePath
                ),
                GELATO_ETH
            );
        } else {
            gelatoHash = IOps(ops).createTaskNoPrepayment(
                address(this),
                this.swapTokensForTokens.selector,
                address(this),
                abi.encodeWithSelector(
                    this.check.selector,
                        order,
                        sig,
                        path,
                        feePath
                ),
                GELATO_ETH
            );
        }

        _createOrder(order, orderHash, gelatoHash);
    }


    /**
     * @notice Internal function for order creation
     * @param order Order data
     * @param orderHash Order hash
     * @param gelatoHash Gelato hash of created task
     * @dev Removes "Stack too deep" error
     */
    function _createOrder(
        Order calldata order,
        bytes32 orderHash,
        bytes32 gelatoHash
    ) private {
        orders[orderHash] = OrderInfo({
            account: msg.sender,
            deadline: uint64(order.deadline),
            gelatoHash: gelatoHash
        });


        emit LimitOrderCreated(
            msg.sender,
            order.receiver,
            order.deadline,
            orderHash,
            gelatoHash,
            order.tokenIn,
            order.tokenOut,
            order.amountIn,
            order.amountOutMin
        );
    }


    /**
     * @notice Internal function for order creation
     * @param order Signed order data
     * @param sig Signature
     * @dev used by Gelato smart contracts to check execution
     */
    function check (
        Order calldata order,
        Sig calldata sig,
        address[] calldata path,
        address[] calldata feePath
    )
        external
        view
        returns (bool canExec, bytes memory execPayload)
    {
        bytes32 orderHash = buildOrderHash(order);

        if (orders[orderHash].account == address(0)) {
            return (false, "");
        }

        address _WETH = WETH;

        // ETH -> TOKEN
        if (order.tokenIn == _WETH) {
            (uint256[] memory amounts) = router.getAmountsOut(
                order.amountIn,
                path
            );

            if (amounts[amounts.length - 1] >= order.amountOutMin) {
                execPayload = abi.encodeWithSelector(
                    this.swapETHForTokens.selector,
                    order,
                    sig,
                    orders[orderHash].gelatoHash,
                    path
                );

                return(true, execPayload);
            }

        }
        // TOKEN -> ETH
        else if (order.tokenOut == _WETH) {
            (uint256[] memory amounts) = router.getAmountsOut(
                order.amountIn,
                path
            );

            if (amounts[amounts.length - 1] >= order.amountOutMin) {
                execPayload = abi.encodeWithSelector(
                    this.swapTokensForETH.selector,
                    order,
                    sig,
                    orders[orderHash].gelatoHash,
                    path
                );

                return(true, execPayload);
            }
        }
        // TOKEN -> TOKEN
        else {
            (uint256[] memory amounts) = router.getAmountsOut(
                order.amountIn,
                path
            );

            if (amounts[amounts.length - 1] >= order.amountOutMin) {
                execPayload = abi.encodeWithSelector(
                    this.swapTokensForTokens.selector,
                    order,
                    sig,
                    orders[orderHash].gelatoHash,
                    path,
                    feePath
                );

                return(true, execPayload);
            }

            return (false, "");
        }

        return (false, "");
    }


    function swapTokensForTokens(
        Order calldata order,
        Sig calldata sig,
        bytes32 gelatoHash,
        address[] calldata path,
        address[] calldata feePath
    ) external onlyOps {
        {
            bytes32 orderHash = buildOrderHash(order);
            OrderInfo memory orderInfo = orders[orderHash];

            require(orderInfo.account != address(0), "Order is not active");
            require(orderInfo.deadline > block.timestamp, "Over deadline");
            delete orders[orderHash].account;
            (address recoveredAddress, ) = ECDSA.tryRecover(orderHash, sig.v, sig.r, sig.s);
            require(orderInfo.account == recoveredAddress, 'Bad signature');

            IERC20(order.tokenIn).transferFrom(orderInfo.account, address(this), order.amountIn);
        }

        uint256 amountIn = order.amountIn;
        uint256 initialBalance = IERC20(order.tokenOut).balanceOf(order.receiver);
        bool swapTokenInToFee = order.tokenIn == feePath[0];
        if (swapTokenInToFee) {
            // If tokenIn is not taxed token we swap it to ETH end pay the fee
            uint256 swappedAmount = _swapTokenToGelatoFee(feePath, amountIn);
            amountIn -= swappedAmount;
        }

        {
            address swapReceiver = swapTokenInToFee ? order.receiver : address(this);
            if (IERC20(path[0]).allowance(address(this), address(router)) < amountIn) {
                IERC20(path[0]).approve(address(router), type(uint256).max);
            }
            router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amountIn,
                order.amountOutMin,
                path,
                swapReceiver,
                block.timestamp + 1200
            );
        }

        if (!swapTokenInToFee) {
            // If tokenOut is not taxed token we swap it to ETH end pay the fee
            require(order.tokenOut == feePath[feePath.length - 1], "Invalid feePath");
            _swapTokenToGelatoFee(feePath, order.amountOutMin);
            IERC20(order.tokenOut).transfer(order.receiver, order.amountOutMin);
        }


        uint256 amountOut = IERC20(order.tokenOut).balanceOf(order.receiver) - initialBalance;
        require(amountOut >= order.amountOutMin, "Below amountOutMin");

        // Cancelling Gelato order
        IOps(ops).cancelTask(gelatoHash);

        emit SwapTokenToToken(
            order.account,
            order.receiver,
            order.tokenIn,
            order.tokenOut,
            order.amountIn,
            amountOut,
            gelatoHash
        );
    }


    function swapETHForTokens(
        Order calldata order,
        Sig calldata sig,
        address[] calldata path
    ) external onlyOps {

    }


    function swapTokensForETH(
        Order calldata order,
        Sig calldata sig,
        address[] calldata path
    ) external onlyOps {

    }


    function cancelLimitOrder(
        bytes32 orderHash
    ) external {
        require(msg.sender == orders[orderHash].account, "Not allowed");

        bytes32 gelatoHash = orders[orderHash].gelatoHash;
        delete orders[orderHash];
        IOps(ops).cancelTask(gelatoHash);

        emit OrderCancelled(
            msg.sender,
            orderHash
        );
    }


    /**
     * @notice Function builds hash according to hashing typed data standard V4 (EIP712)
     * @param order - Limit order info
     * @dev May be used on off-chain to build order hash
     */
    function buildOrderHash(Order memory order) public view returns (bytes32){
        return _hashTypedDataV4(keccak256(abi.encode(
                ORDER_TYPEHASH,
                order.account,
                order.receiver,
                order.deadline,
                order.tokenIn,
                order.tokenOut,
                order.amountIn,
                order.amountOutMin
            )));
    }

    //todo: cancelExpiredOrders([])


    /**
     * @notice Transfers fees for Gelato execution
     */
    function _transferGelatoFee() private {
        (uint256 feeAmount, ) = IOps(ops).getFeeDetails();
        (bool success, ) = gelato.call{value: feeAmount}("");
        require(success, "_transfer: ETH transfer failed");
    }


    /**
     * @notice Swaps token to ETH to pay for Gelato execution
     * @param feePath - Token -> ETH swap path
     * @param amountInMax - Max amount of tokens to spend
     */
    function _swapTokenToGelatoFee(
        address[] calldata feePath,
        uint256 amountInMax
    ) private returns(uint256){
        if (IERC20(feePath[0]).allowance(address(this), address(router)) < amountInMax) {
            IERC20(feePath[0]).approve(address(router), type(uint256).max);
        }

        (uint256 feeAmount, ) = IOps(ops).getFeeDetails();
        (uint256[] memory amounts) = router.swapTokensForExactETH(
            feeAmount,
            amountInMax,
            feePath,
            gelato,
            block.timestamp + 1200
        );

        return amounts[0];
    }

}

