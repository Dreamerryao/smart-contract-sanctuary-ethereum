interface AggregatorV3Interface {
    function decimals() external view returns (uint8);

    function description() external view returns (string memory);

    function version() external view returns (uint256);

    function getRoundData(uint80 _roundId)
        external
        view
        returns (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        );

    function latestRoundData()
        external
        view
        returns (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        );
}

contract NFTLending is ERC1155Holder, Ownable {
    // using EnumerableSet for EnumerableSet.AddressSet;

    uint256 collateralRate = 80; // 80/100
    uint256 interestRate = 50; // 50/1000

    uint256 minDuration = 0;
    uint256 maxDuration = 60 * 60 * 24 * 360;

    bool isPaused;

    AggregatorV3Interface internal priceFeed;

    // EnumerableSet.AddressSet internal NFTAddresses;

    IERC20 ERN;

    struct BorrowerInfo {
        address NFTAddress;
        uint256 tokenID;
        uint256 borrowAmount;
        uint256 borrowTimestamp;
        uint256 durationSeconds;
    }

    struct NFTInfo {
        uint256 priceUSD;
    }

    mapping(address => mapping(bytes32 => BorrowerInfo)) public borrowers;
    mapping(address => mapping(uint256 => NFTInfo)) public NFTs;
    mapping(address => bool) userLocks;

    modifier IsLive() {
        require(!isPaused, "Lending is paused");
        _;
    }

    modifier UserNotLocked() {
        require(!userLocks[msg.sender], "User is locked");
        _;
    }

    event Lent(
        address indexed borrower,
        address indexed NFTAddress,
        uint256 tokenID,
        bytes32 loanID,
        uint256 lendAmount
    );
    event Repayed(bytes32 indexed loanID);
    event NFTAddressSet(
        address indexed NFTAddress,
        uint256 tokenID,
        uint256 priceUSD
    );

    constructor(address ERNAddress) {
        // 0x0a87e12689374A4EF49729582B474a1013cceBf8
        priceFeed = AggregatorV3Interface(
            0xf625b9b6A78D6cE751820a1cb8d7EC6594B7e04a
        );

        ERN = IERC20(ERNAddress);
    }

    function borrow(
        address NFTAddress,
        uint256 tokenID,
        uint256 durationSeconds
    ) public IsLive UserNotLocked {
        // require(
        //     NFTAddresses.contains(NFTAddress),
        //     "Borrow: NFT address is not valid"
        // );
        require(
            NFTs[NFTAddress][tokenID].priceUSD > 0,
            "Borrow: NFT address or tokenID is not valid"
        );
        require(
            durationSeconds > minDuration && durationSeconds < maxDuration,
            "Borrow: Duration should be between min&max"
        );

        userLocks[msg.sender] = true;

        IERC1155(NFTAddress).safeTransferFrom(
            msg.sender,
            address(this),
            tokenID,
            1,
            ""
        );

        uint256 NFTPriceUSD = NFTs[NFTAddress][tokenID].priceUSD;
        // require(NFTPriceUSD > 0, "Borrow: Token ID is not valid");
        uint256 ERNPriceUSD = uint256(getLatestERNPrice());

        uint256 priceFeedDecimal = 10**8;

        uint256 lendAmount = (((NFTPriceUSD * priceFeedDecimal) / ERNPriceUSD) *
            (collateralRate)) / 100;

        ERN.transfer(msg.sender, lendAmount);

        bytes32 loanID = hashLoan(lendAmount, block.timestamp);

        borrowers[msg.sender][loanID] = BorrowerInfo(
            NFTAddress,
            tokenID,
            lendAmount,
            block.timestamp,
            durationSeconds
        );

        userLocks[msg.sender] = false;

        emit Lent(msg.sender, NFTAddress, tokenID, loanID, lendAmount);
    }

    function repay(bytes32 loanID) public {
        BorrowerInfo memory borrower = borrowers[msg.sender][loanID];

        require(
            borrower.borrowAmount > 0,
            "Repay: User doesn't have a loan in progress"
        );
        require(
            block.timestamp <=
                borrower.borrowTimestamp + borrower.durationSeconds,
            "Repay: User has exceeded allowed time to repay"
        );

        uint256 interest = (interestRate *
            borrower.borrowAmount *
            (block.timestamp - borrower.borrowTimestamp)) / (360 days * 1000);

        ERN.transferFrom(
            msg.sender,
            address(this),
            borrower.borrowAmount + interest
        );

        IERC1155(borrower.NFTAddress).safeTransferFrom(
            address(this),
            msg.sender,
            borrower.tokenID,
            1,
            ""
        );

        _resetUserLoan(msg.sender, loanID);

        emit Repayed(loanID);
    }

    function resetUserLoan(address user, bytes32 loanID) public onlyOwner {
        _resetUserLoan(user, loanID);
    }

    function _resetUserLoan(address user, bytes32 loanID) private {
        borrowers[user][loanID] = BorrowerInfo(address(0), 0, 0, 0, 0);
    }

    function pause() public onlyOwner {
        isPaused = true;
    }

    function resume() public onlyOwner {
        isPaused = false;
    }

    function setMinDuration(uint256 _minDuration) public onlyOwner {
        minDuration = _minDuration;
    }

    function setMaxDuration(uint256 _maxDuration) public onlyOwner {
        maxDuration = _maxDuration;
    }

    function setInterestRate(uint256 _interestRate) public onlyOwner {
        interestRate = _interestRate;
    }

    function setCollateralRate(uint256 _collateralRate) public onlyOwner {
        collateralRate = _collateralRate;
    }

    function setNFTPrice(
        address NFTAddress,
        uint256 tokenID,
        uint256 priceUSD
    ) public onlyOwner {
        // require(
        //     !NFTAddresses.contains(NFTAddress),
        //     "SetNFTPrice: NFT address is already added"
        // );

        require(
            priceUSD / 10**18 > 0,
            "SetNFTPrice: Price should be 18 decimals"
        );
        // NFTAddresses.add(NFTAddress);

        NFTs[NFTAddress][tokenID].priceUSD = priceUSD;

        emit NFTAddressSet(NFTAddress, tokenID, priceUSD);
    }

    function setBatchNFTPrice(
        address[] memory _NFTAddresses,
        uint256[] memory tokenIDs,
        uint256[] memory pricesUSD
    ) public onlyOwner {
        require(
            _NFTAddresses.length == tokenIDs.length &&
                tokenIDs.length == pricesUSD.length,
            "setBatchNFTPrice: Lengths should be the same"
        );

        for (uint256 i; i < _NFTAddresses.length; i++) {
            setNFTPrice(_NFTAddresses[i], tokenIDs[i], pricesUSD[i]);
        }
    }

    function removeNFT(address NFTAddress, uint256 tokenID) public onlyOwner {
        delete NFTs[NFTAddress][tokenID];
    }

    // function addNFTAddress(
    //     address memory NFTAddress
    // ) public onlyOwner {
    //     require(
    //         !NFTAddresses.contains(NFTAddress),
    //         "addNFTAddress: NFT address is already added"
    //     );

    //     NFTAddresses.add(NFTAddress);

    //     emit NFTAddressSet(NFTAddress, priceUSD);
    // }

    // function getNFTAddresses()
    //     public
    //     view
    //     returns (EnumerableSet.AddressSet memory)
    // {
    //     return NFTAddresses;
    // }

    function getNFTPrice(address NFTAddress, uint256 tokenID)
        public
        view
        returns (uint256)
    {
        return NFTs[NFTAddress][tokenID].priceUSD;
    }

    function getLatestERNPrice() public view virtual returns (int256) {
        (
            uint80 roundID,
            int256 price,
            uint256 startedAt,
            uint256 timeStamp,
            uint80 answeredInRound
        ) = priceFeed.latestRoundData();
        return price;
    }

    function getLoan(address borrower, bytes32 loanID)
        public
        view
        returns (BorrowerInfo memory)
    {
        return borrowers[borrower][loanID];
    }

    function hashLoan(uint256 lendAmount, uint256 timestamp)
        public
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(lendAmount, timestamp));
    }
}

