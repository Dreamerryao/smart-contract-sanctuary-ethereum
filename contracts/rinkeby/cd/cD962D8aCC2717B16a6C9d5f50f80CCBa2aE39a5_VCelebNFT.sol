pragma solidity ^0.8.4;

import "./Operatable.sol";
import "./common/meta-transactions/ContentMixin.sol";
import "./common/meta-transactions/NativeMetaTransaction.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "erc721a/contracts/ERC721A.sol";

// Proxy for Opensea
// https://docs.opensea.io/docs/1-structuring-your-smart-contract

contract OwnableDelegateProxy {

}

contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

contract VCelebNFT is ERC721A, ContextMixin, NativeMetaTransaction, Ownable, Operatable {
    uint8 constant MINT_LIMIT = 10; // for gas limit
    uint256 public presaleMintPrice = 0.1 ether;
    uint256 public mintPrice = 0.2 ether;
    uint16 public presaleSupply = 2000;
    uint16 public maxSupply = 10000;

    bool public onPresale = false;
    bool public onPublicSale = false;

    string public baseTokenURI;

    address public proxyRegistryAddress;

    mapping(address => uint8) private presaleAllowedList;

    event ProxyRegistryAddressChanged(address newProxyRegistryAddress);
    event BaseURIChanged(string baseURI);
    event Withdraw(uint256 amount);
    event PresaleMintPriceChanged(uint256 newPrice);
    event MintPriceChanged(uint256 newPrice);
    event PresaleSupplyChanged(uint256 newSupply);
    event MaxSupplyChanged(uint256 newSupply);
    event StartPresale();
    event StopPresale();
    event StartPublicSale();
    event StopPublicSale();
    event PresaleAllowed(address account, uint8 amount);

    error InvalidParametersLength();
    error NotOnPresale();
    error NotAllowedToMint(uint256 allowedAmount);
    error InvalidQuantity(uint8 quantity, uint8 allowedQuantity);
    error NoMoreSupply();
    error InvalidPrice(uint256 value, uint256 mintValue);
    error NotOnPublicSale();

    constructor(string memory baseURI, address openseaProxyRegistryAddress) ERC721A("vCeleb", "VCELEB") {
        _setBaseURI(baseURI);
        proxyRegistryAddress = openseaProxyRegistryAddress;
        _initializeEIP712("vCeleb");
    }

    function mint(uint256 quantity) external payable {
        // _safeMint's second argument now takes in a quantity, not a tokenId.
        _safeMint(msg.sender, quantity);
    }

    function setProxyRegistryAddress(address newProxyRegistryAddress) external onlyOwner {
        proxyRegistryAddress = newProxyRegistryAddress;
        emit ProxyRegistryAddressChanged(newProxyRegistryAddress);
    }

    function setBaseURI(string memory baseURI) external onlyOwner {
        _setBaseURI(baseURI);
    }

    function _setBaseURI(string memory baseURI) internal {
        baseTokenURI = baseURI;
        emit BaseURIChanged(baseURI);
    }

    function withdraw(uint256 amount) external onlyOwner {
        Address.sendValue(payable(owner()), amount);
        emit Withdraw(amount);
    }

    function setPresaleMintPrice(uint256 newPrice) external onlyOwner {
        presaleMintPrice = newPrice;
        emit PresaleMintPriceChanged(newPrice);
    }

    function setMintPrice(uint256 newPrice) external onlyOwner {
        mintPrice = newPrice;
        emit MintPriceChanged(newPrice);
    }

    function setPresaleSupply(uint16 newSupply) external onlyOwner {
        presaleSupply = newSupply;
        emit PresaleSupplyChanged(newSupply);
    }

    function setMaxSupply(uint16 newSupply) external onlyOwner {
        maxSupply = newSupply;
        emit MaxSupplyChanged(newSupply);
    }

    function startPresale() external onlyOperator {
        onPresale = true;
        emit StartPresale();
    }

    function stopPresale() external onlyOperator {
        onPresale = false;
        emit StopPresale();
    }

    function startPublicSale() external onlyOperator {
        onPublicSale = true;
        emit StartPublicSale();
    }

    function stopPublicSale() external onlyOperator {
        onPublicSale = false;
        emit StopPublicSale();
    }

    function setPresaleAllowedAmounts(address[] memory accounts, uint8[] memory amounts) external onlyOperator {
        if (accounts.length != amounts.length) revert InvalidParametersLength();
        for (uint256 i = 0; i < accounts.length; i++) {
            presaleAllowedList[accounts[i]] = amounts[i];
            emit PresaleAllowed(accounts[i], amounts[i]);
        }
    }

    function getPresaleAllowedAmount(address account) external view returns (uint8) {
        return presaleAllowedList[account];
    }

    function mintPresale(uint8 quantity) external payable {
        address msgSender = _msgSender();
        uint8 allowedQuantity = presaleAllowedList[msgSender];
        uint256 previousTokenId = totalSupply();
        if (!onPresale) revert NotOnPresale();
        if (allowedQuantity <= 0) revert NotAllowedToMint(allowedQuantity);
        if (quantity <= 0 || quantity > allowedQuantity) revert InvalidQuantity(quantity, allowedQuantity);
        if (previousTokenId + quantity > presaleSupply) revert NoMoreSupply();
        if (msg.value < presaleMintPrice * quantity) revert InvalidPrice(msg.value, presaleMintPrice * quantity);

        presaleAllowedList[msgSender] = allowedQuantity - quantity;

        _safeMint(msgSender, quantity);
    }

    function mintPublicSale(uint8 quantity) external payable {
        address msgSender = _msgSender();
        uint256 previousTokenId = totalSupply();
        if (!onPublicSale) revert NotOnPublicSale();
        if (quantity <= 0 || quantity > MINT_LIMIT) revert InvalidQuantity(quantity, MINT_LIMIT);
        if (previousTokenId + quantity > maxSupply) revert NoMoreSupply();
        if (msg.value < mintPrice * quantity) revert InvalidPrice(msg.value, mintPrice * quantity);

        _safeMint(msgSender, quantity);
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-less listings.
     */
    function isApprovedForAll(address owner, address operator) public view override(ERC721A) returns (bool isOperator) {
        // Whitelist OpenSea proxy contract for easy trading.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (address(proxyRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender() internal view override returns (address sender) {
        return ContextMixin.msgSender();
    }

    function renounceOperator() public onlyOwner {
        _transferOperator(address(0));
    }

    function transferOperator(address newOperator) public onlyOwner {
        require(newOperator != address(0), "Operatable: new operator is the zero address");
        _transferOperator(newOperator);
    }
}

