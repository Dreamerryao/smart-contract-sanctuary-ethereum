pragma solidity ^0.8.0;

abstract contract Pausable {
    mapping(string => bool) private _paused;
    string internal constant FOSSIL_PAUSE = "FOSSIL_PAUSE";
    string internal constant COMMON_PAUSE = "COMMON_PAUSE";
    string internal constant GENESIS_PAUSE = "GENESIS_PAUSE";

    constructor() {
        _paused[FOSSIL_PAUSE] = true;
        _paused[COMMON_PAUSE] = true;
        _paused[GENESIS_PAUSE] = false;
    }

    function paused(string memory module) public view virtual returns (bool) {
        return _paused[module];
    }

    modifier whenNotPaused(string memory module) {
        require(!paused(module), "Pausable: paused");
        _;
    }

    modifier whenPaused(string memory module) {
        require(paused(module), "Pausable: not paused");
        _;
    }

    function _pause(string memory module) internal virtual whenNotPaused(module) {
        _paused[module] = true;
    }

    function _unpause(string memory module) internal virtual whenPaused(module) {
        _paused[module] = false;
    }
}

