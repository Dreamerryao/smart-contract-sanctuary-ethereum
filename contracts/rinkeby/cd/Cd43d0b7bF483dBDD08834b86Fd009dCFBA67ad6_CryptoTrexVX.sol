pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./Fossil.sol";
import "./lib/Pausable.sol";
import "./ERC721Tradeable.sol";


contract CryptoTrexVX is ERC721Tradeable, Pausable {
    uint private maxSupply;
    uint private constant MAX_PER_TX = 20;

    // Metadata
    string internal _tokenURI;

    // Mint related
    uint public etherPrice;
    uint public fossilPrice;
    address payable public receiver;

    Fossil private fossilContract;

    IERC721 private genesisContract;

    constructor(
        address _proxyRegistry,
        address _fossilContract,
        address _genesisContract,
        string memory _uri
    ) ERC721Tradeable("CryptoTrexVX", "TREXVX", _proxyRegistry) {
        _tokenURI = _uri;
        etherPrice = 8 ether / 100;
        fossilPrice = 50 ether;
        fossilContract = Fossil(_fossilContract);
        genesisContract = IERC721(_genesisContract);
        maxSupply = 13333;
    }

    function genesisMint(uint256[] calldata tokenIds) external whenNotPaused(GENESIS_PAUSE) nonReentrant {
        require(tokenIds.length > 0, "nothing to claim");
        uint256[] memory ids = new uint256[](tokenIds.length);
        for(uint i; i < tokenIds.length; i++) {
            uint _tokenId = tokenIdMap(tokenIds[i]);
            require(_tokenId != 0, "invalid token");
            require(genesisContract.ownerOf(tokenIds[i]) == msg.sender, "not authorized");
            ids[i] = _tokenId;
        }
        _genesisMint(msg.sender, ids);
    }

    function isGenesisMinted(uint256[] memory tokenIds) external view returns (bool[] memory) {
        bool[] memory result = new bool[](tokenIds.length);
        for(uint i; i<tokenIds.length; i++) {
            uint _tokenId = tokenIdMap(tokenIds[i]);
            result[i] = _exists(_tokenId);
        }
        return result;
    }

    function mint(uint amount) external payable whenNotPaused(COMMON_PAUSE) nonReentrant {
        require(amount < MAX_PER_TX + 1, "amount can't exceed 20");
        require(amount > 0, "amount too little");
        require(msg.value == etherPrice * amount, "insufficient fund");
        require(totalSupply() + amount < maxSupply + 2, "exceed max supply");
        _mint(msg.sender, amount);
    }

    function fossilMint(uint amount) external payable whenNotPaused(FOSSIL_PAUSE) nonReentrant {
        require(amount < MAX_PER_TX + 1, "amount can't exceed 20");
        require(amount > 0, "amount too little");
        require(totalSupply() + amount < maxSupply + 2, "exceed max supply");
        require(fossilContract.balanceOf(msg.sender) >= fossilPrice * amount, "insufficient fund");
        require(genesisContract.balanceOf(msg.sender) > 0, "must own a genesis rex");

        _mint(msg.sender, amount);
        fossilContract.transferFrom(msg.sender, address(this), fossilPrice * amount);
    }

    function airdrop(address to, uint256 amount) external onlyOwner {
        require(totalSupply() + amount <= maxSupply + 1, "exceed max supply");
        _mint(to, amount);
    }

    function owned(address owner) external view returns (uint256[] memory) {
        uint balance = balanceOf(owner);
        uint256[] memory tokenIds = new uint256[](balance);
        for(uint i=0;i<balance;i++){
            tokenIds[i] = tokenOfOwnerByIndex(owner, i);
        }
        return tokenIds;
    }

    // Stake
    bool internal _stakingIsActive;
    mapping(address => uint256) private _lastUpdated;
    mapping(address => uint256) private _rewards;
    uint256 private _stakingOriginalBeginTime;

    function setStakingStatus(bool value) external onlyOwner {
        _stakingIsActive = value;
    }

    function setStakingStartTimestamp() external onlyOwner {
        _stakingOriginalBeginTime = block.timestamp;
    }

    function rewards(address owner) public view returns (uint256) {
        uint since = _lastUpdated[owner];
        if(since == 0) {
            since = _stakingOriginalBeginTime;
        }
        uint256 totalRewards = balanceOf(owner) * ((block.timestamp - since) * 1 ether) / (3 days);
        return totalRewards + _rewards[owner];
    }

    function _currentRewards(address owner) internal view returns (uint256) {
        uint since = _lastUpdated[owner];
        if(since == 0) {
            since = _stakingOriginalBeginTime;
        }
        uint256 totalRewards = balanceOf(owner) * ((block.timestamp - since) * 1 ether) / (3 days);
        return totalRewards;
    }

    function claimRewards() external {
        require(_stakingIsActive, "staking not active");
        uint256 totalRewards = rewards(msg.sender);
        _lastUpdated[msg.sender] = block.timestamp;
        _rewards[msg.sender] = 0;
        require(totalRewards > 0, "nothing to claim");
        fossilContract.mint(msg.sender, totalRewards);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Tradeable) {
        if(from != address(0)) {
            _rewards[from] = rewards(from);
            _lastUpdated[from] = block.timestamp;
        }
        if (to != address(0)) {
            _rewards[to] = rewards(to);
            _lastUpdated[to] = block.timestamp;
        }
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // Pausable
    function setPause(string calldata module, bool value) external onlyOwner {
        if(value) {
            _pause(module);
        } else {
            _unpause(module);
        }
    }

    // Minting fee
    function setEtherPrice(uint amount) external onlyOwner {
        etherPrice = amount;
    }
    function setFossilPrice(uint amount) external onlyOwner {
        fossilPrice = amount;
    }
    function setReceiver(address payable _receiver) external onlyOwner {
        receiver = _receiver;
    }
    function setMaxSupply(uint amount) external onlyOwner {
        require(amount > 1111, "less than genesis count");
        maxSupply = amount;
    }
    function claim() external onlyOwner {
        require(receiver != address(0), "receiver not set");
        receiver.transfer(address(this).balance);
    }

    // Metadata
    function setTokenURI(string calldata _uri) external onlyOwner {
        _tokenURI = _uri;
    }
    function baseTokenURI() external view returns (string memory) {
        return _tokenURI;
    }
    
    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        return string(abi.encodePacked(
            _tokenURI,
            "/",
            Strings.toString(_tokenId),
            ".json"
        ));
    }

    // Utility
    function tokenIdMap(uint256 _tokenId) public pure returns(uint) {
        if (_tokenId >> 96 != 0x000000000000000000000000D640CF88F763CEE6B243F1CAB3D0BEFDAFC47B9E){
            return 0;
        }
		if (_tokenId & 0x000000000000000000000000000000000000000000000000000000ffffffffff != 1){
			return 0;
        }
		uint256 id = (_tokenId & 0x0000000000000000000000000000000000000000ffffffffffffff0000000000) >> 40;
        if(id < 59) { // prevent underflow
            return 0;
        }
        id = id - 59;
        if(id > 1117 || id == 104 || id == 107 || id == 206 || id == 610 || id == 611 || id == 612 || id == 613) {
            return 0;
        }
        uint reducer = 0;
        if(id > 104) reducer++;
        if(id > 107) reducer++;
        if(id > 206) reducer++; 
        if(id > 610) reducer++;
        if(id > 611) reducer++;
        if(id > 612) reducer++;
        if(id > 613) reducer++;
        return id - reducer + 1;
    }
}

