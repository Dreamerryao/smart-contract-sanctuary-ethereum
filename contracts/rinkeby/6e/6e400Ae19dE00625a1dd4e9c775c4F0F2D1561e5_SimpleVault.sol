pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./IVault.sol";

contract SimpleVault is Initializable, AccessControlUpgradeable, IVault {
  using Counters for Counters.Counter;

  bytes32 public constant BROKER_ROLE = keccak256("CUSTODY_BROKER_ROLE");
  bytes32 public constant WITHDRAW_TYPE = keccak256("CUSTODY_WITHDRAW_TYPE");

  struct WithdrawAsset {
    address asset;
    uint256 amount;
  }

  string private _name;
  address private _broker;
  Counters.Counter private _ledgerId;

  modifier onlyValidMovingFundParams(address account) {
    require(account != address(0), "Vault: account is zero address");
    _;
  }

  function initialize(string memory name_, address broker_) public initializer {
    _name = name_;
    _broker = broker_;
    console.log("[Vault]", "(initialize)", "name:", _name);
    __AccessControl_init();
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    console.log("[Vault]", "(initialize)", "add_admin:", msg.sender);
    _setupRole(BROKER_ROLE, _broker);
    console.log("[Vault]", "(initialize)", "add_broker:", _broker);
  }

  function name() public view virtual returns (string memory) {
    return _name;
  }

  function changeBroker(address newBroker) external onlyRole(DEFAULT_ADMIN_ROLE) {
    revokeRole(BROKER_ROLE, _broker);
    _setupRole(BROKER_ROLE, newBroker);
    _broker = newBroker;
    console.log("[Vault]", "(changeBroker)", "broker:", _broker);
  }

  function getLastId() override external view returns (uint256) {
    return _ledgerId.current();
  }

  function deposit(address asset, uint256 amount) public payable returns (bool) {
    return _deposit(msg.sender, asset, amount);
  }

  function _deposit(address account, address asset, uint256 amount) internal onlyValidMovingFundParams(account) returns (bool) {
    console.log("[Vault]", "(deposit)", "account:", account);
    console.log("[Vault]", "(deposit)", "asset:", asset);
    console.log("[Vault]", "(deposit)", "amount:", amount);
    
    _transferAssetFrom(asset, account, amount);

    _ledgerId.increment();
    emit Deposited(_ledgerId.current(), account, asset, amount);
    return true;
  }

  function withdraw(bytes calldata payload, bytes memory signature) public returns (bool) {
    return _withdraw(msg.sender, payload, signature);
  }

  function _withdraw(address account, bytes calldata payload, bytes memory signature) internal onlyValidMovingFundParams(account) returns (bool) {
    console.log("[Vault]", "(withdraw)", "account:", account);

    {
      bytes32 digest = ECDSA.toEthSignedMessageHash(
        keccak256(abi.encodePacked(WITHDRAW_TYPE, payload))
      );
      address recovered = ECDSA.recover(digest, signature);
      console.log("[Vault]", "(withdraw)", "recovered:", recovered);
      require(recovered == _broker, "Vault: invalid signature");
    }

    require(hasRole(BROKER_ROLE, _broker), "Vault: invalid broker");

    (
      bytes32 id,
      uint64 deadline,
      address destination,
      WithdrawAsset[] memory withdrawAssets
    ) = abi.decode(payload, (bytes32, uint64, address, WithdrawAsset[]));

    console.log("[Vault]", "(withdraw)", "id:", uint256(id));
    console.log("[Vault]", "(withdraw)", "destination:", destination);
    console.log("[Vault]", "(withdraw)", "deadline", deadline);
    console.log("[Vault]", "(withdraw)", "block.timestamp", block.timestamp);
    require(deadline > block.timestamp, "Vault: request is expired");
    require(account == destination, "Vault: invalid request");

    for (uint i = 0; i < withdrawAssets.length; i++) {
      console.log("[Vault]", "(withdraw)", "asset", withdrawAssets[i].asset);
      console.log("[Vault]", "(withdraw)", "amount", withdrawAssets[i].amount);
      require(withdrawAssets[i].amount > 0, "Vault: amount is zero");
    }
    for (uint i = 0; i < withdrawAssets.length; i++) {
      _transferAssetTo(withdrawAssets[i].asset, account, withdrawAssets[i].amount);
      _ledgerId.increment();
      emit Withdrawn(_ledgerId.current(), account, withdrawAssets[i].asset, withdrawAssets[i].amount);
    }

    return true;
  }

  function _transferAssetFrom(
    address asset,
    address from,
    uint256 amount
  ) internal {
    if (asset == address(0)) {
      require(msg.value == amount, "Vault: Incorrect msg.value");
    } else {
      // require successful deposit before updating holdings (protect against reentrancy)
      require(
        IERC20(asset).transferFrom(from, address(this), amount),
        "Vault: Could not deposit ERC20"
      );
    }
  }

  /**
    * @notice Transfers the given amount of this AssetHolders's asset type to a supplied ethereum address.
    * @dev Transfers the given amount of this AssetHolders's asset type to a supplied ethereum address.
    * @param destination ethereum address to be credited.
    * @param amount Quantity of assets to be transferred.
    */
  function _transferAssetTo(
    address asset,
    address destination,
    uint256 amount
  ) internal {
    if (asset == address(0)) {
      (bool success, ) = destination.call{value: amount}(""); //solhint-disable-line avoid-low-level-calls
      require(success, "Vault: Could not transfer ETH");
    } else {
      IERC20(asset).transfer(destination, amount);
    }
  }
}

