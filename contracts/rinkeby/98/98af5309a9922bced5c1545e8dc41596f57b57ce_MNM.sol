    // Set status of public sale
    function setPublicActive(bool state) external onlyOwner {
        isPublicMintActive = state;
    }
    // Set status of white list sale
    function setWhiteListActive(bool state) external onlyOwner {  
        isWhiteListMintActive = state;
    }

    // Adding address to white list
    function addWhiteList(address[] calldata addresses) external onlyOwner{
        for(uint256 i = 0; i < addresses.length; i++){
            whiteList[addresses[i]] = true;
        }
    }
    
    // White list mint
    function whiteListClaim() public nonReentrant {
        require(isWhiteListMintActive, "White list sale status must be active to mint");
        require(whiteList[msg.sender], "You are not in the white list!");
        require(numClaimed >= 0 && numClaimed < 1501, "Invalid claim");
        //require(msg.value >= mintPrice, "Mint price is 0.15 Eth");   
        
        internalMint();
    }

    // Public mint
   function claim() public nonReentrant {
        require(isPublicMintActive, "Mint sale status must be active to mint");
        require(numClaimed >= 0 && numClaimed < 3001, "Invalid claim");
        //require(msg.value >= mintPrice, "Mint price is 0.15 Eth");

        internalMint();
    }
    
    // This is to prevent duplicate code for the mint function
    function internalMint() internal {
        
        ItemInd memory itemInd;
        ItemStr memory itemStr;

        // This might have to be moved to mint instead
        if (tryToken % randR == 0){
            itemStr.unique = oneClass[oneClass.length-1];
            itemStr.isUsed = "False";
            itemInd.isUnique = true;
            oneClass.pop();
        }

        else {
            uint256 token;

            while(token == 0){
            
                (itemInd, itemStr) = genItem(tryToken);

                // Create DNA
                string memory dnaStr = string(abi.encodePacked(itemStr.trait1, itemStr.trait2, itemStr.trait3, itemStr.trait4, itemStr.trait5, itemStr.trait6));
                uint256 dnaHash = uint256(keccak256(abi.encodePacked(dnaStr)));
                dna = dnaHash % (10 ** 8);
                
                if (tokens[dna] == false) {
                    tokens[dna] = true;
                    tryToken ++;
                    token = tryToken;
                }

                else {
                    tryToken ++;
                }
            }
        }
        

        itemInds[numClaimed] = itemInd;
        itemStrs[numClaimed] = itemStr;
        _mint(_msgSender(), numClaimed, 1, "");
        numClaimed += 1;
    }

    function withdraw() public onlyOwner{
        uint balance = address(this).balance;
        payable(owner()).transfer(balance);
    }

    function batchBurn(uint256[] memory a, uint256[] memory b) public nonReentrant onlyOwner {
        _burnBatch(_msgSender(), a, b);

    }

