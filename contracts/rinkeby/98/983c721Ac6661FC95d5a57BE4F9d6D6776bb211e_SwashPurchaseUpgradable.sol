pragma solidity ^0.8.6;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "./IERC677Receiver.sol";

import "hardhat/console.sol";

contract SwashPurchaseUpgradable is OwnableUpgradeable {

    event LicenseAddedOrUpdated(uint256 duration, uint256[] prices);

    event UserLicenseAdded(address _wallet, string _type, uint256 _count, uint256 _startTime, uint256 _endTime);
    event UserLicenseUpdated(address _wallet, string _oldType, string _type, uint256 _count, uint256 _startTime, uint256 _endTime);

    event TokenAdded(string name, address tokenAddress, bool isNative);
    event TokenRemoved(string name);

    event Purchased(address sender, uint256 duration);
    event PurchasedWithNativeToken(address sender, uint256 duration, uint256 price, string _type);
    event Withdrawn(address reciever, uint256 amount, address tokenAddress);
    event WithdrawnNative(address reciever, uint256 amount);

    struct LicenseStruct {
        uint256 duration;
        uint256[] priceList;
    }

    struct TokenStruct {
        string name;
        address tokenAddress;
        bool isNative;
    }

    struct UserLicenseStruct {
        address wallet;
        string licenseType;
        uint256 count;
        uint256 startTime;
        uint256 endTime;
    }

    struct LicenseToBuy {
        string tokenName;
        address tokenAddress;
        bool isNative;
        uint256 price;
    }

    address public swashTokenAddress;
    address public baseTokenAddress;

    string[] public licenseModelNameList;
    mapping(string => uint) public licenseModelNameOrder;

    string[] public tokenList;
    mapping(string => TokenStruct) public tokenMap;

    LicenseStruct public license;

    address[] public userLicenseList;
    mapping(address => UserLicenseStruct) public userLicenseMap;

    event Deposit(address indexed user, uint256 indexed poolId, uint256 amount);

    address public swashPurchaseUniswapWrapper;
    address uniswapRouter;
    uint256 SLIPPAGE;
    uint256 timeWindow;

    constructor(){}

    modifier onlyWrapper(){
        require(msg.sender == swashPurchaseUniswapWrapper, "Use wrapper");
        _;
    }
    function initialize(
        address _swashTokenAddress,
        address _baseTokenAddress
    ) public initializer {
        __Ownable_init();
        swashTokenAddress = _swashTokenAddress;
        baseTokenAddress = _baseTokenAddress;
    }

    function getLicense() public view returns (LicenseStruct memory){
        return license;
    }

    function getTokenList() public view returns (string[] memory){
        return tokenList;
    }

    function setSwashTokenAddress(address _swashTokenAddress) public onlyOwner {
        swashTokenAddress = _swashTokenAddress;
    }

    function getLicenseModels() public view returns (string[] memory){
        return licenseModelNameList;
    }

    function setLicenseModels(string[] memory licenseModelNames) public onlyOwner {
        for (uint idx = 0; idx < licenseModelNames.length; ++idx) {
            licenseModelNameOrder[licenseModelNames[idx]] = idx;
        }
        licenseModelNameList = licenseModelNames;
    }

    function setSlippage(uint256 slippage) public onlyOwner {
        SLIPPAGE = slippage;
    }

    function setTimeWindow(uint256 _timeWindow) public onlyOwner {
        timeWindow = _timeWindow;
    }

    function setBaseTokenAddress(address _baseTokenAddress) public onlyOwner {
        baseTokenAddress = _baseTokenAddress;
    }

    function setSwashPurchaseUniswapWrapper(address _swashPurchaseUniswapWrapper) public onlyOwner {
        swashPurchaseUniswapWrapper = _swashPurchaseUniswapWrapper;
    }

    function resetTokenList() public {
        for (uint idx = tokenList.length-1 ; idx >=0 ; --idx) {
            delete tokenMap[tokenList[idx]];
            tokenList.pop();
        }
    }

    function addTokenList(
        string[] memory _nameList,
        address[] memory _addressList,
        bool[] memory _isNativeList
    ) external onlyOwner {
        require(
            _nameList.length == _addressList.length,
            "name list length not equals to address list length"
        );

        require(
            _nameList.length == _isNativeList.length,
            "name list length not equals to isNative list length"
        );

        for (uint idx = 0; idx < _nameList.length; ++idx)
        {
            addToken(_nameList[idx], _addressList[idx], _isNativeList[idx]);
        }

    }

    function addToken(
        string memory _name,
        address _address,
        bool isNative
    ) public onlyOwner {
        require(
            keccak256(abi.encodePacked(_name)) != keccak256(abi.encodePacked("")),
            "Token name can't be empty address"
        );

        require(
            tokenMap[_name].tokenAddress == address(0),
            "token is already available"
        );

        tokenList.push(_name);
        tokenMap[_name] = TokenStruct(_name, _address, isNative);

        emit TokenAdded(_name, _address, isNative);
    }

    function removeToken(
        string memory _name
    ) external onlyOwner {
        require(
            tokenMap[_name].tokenAddress != address(0),
            "token is not available"
        );

        tokenList.push(_name);
        uint256 len = tokenList.length;
        uint8 idx = 0;
        for (; idx < len - 1; ++idx) {
            if (keccak256(abi.encodePacked(tokenList[idx])) == keccak256(abi.encodePacked(_name))) {
                break;
            }
        }
        tokenList[idx] = tokenList[len - 1];
        tokenList.pop();

        delete tokenMap[_name];

        emit TokenRemoved(_name);
    }

    function getTokenOb(string memory tokenName) public view returns (TokenStruct memory){
        return tokenMap[tokenName];
    }

    function setLicense(
        uint256 _duration,
        uint256[] memory _prices
    ) external onlyOwner {
        require(
            licenseModelNameList.length == _prices.length,
            "priceNames and prices length are not equal "
        );

        license = LicenseStruct(_duration, _prices);

        emit LicenseAddedOrUpdated(_duration, _prices);
    }

    function setUniswapRouter(address _uniswapRouter) public onlyOwner {
        uniswapRouter = _uniswapRouter;
    }

    function _uniswapPathToBaseToken(address fromCoin) internal view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = fromCoin;
        path[1] = baseTokenAddress;
        return path;
    }

    //****************************************** UserLicense ***********************************************************

    function addOrUpdateUserLicenseInternal(
        address _wallet,
        string memory _type,
        uint256 _count,
        uint256 _startTime
    ) internal {
        UserLicenseStruct memory userLicensePurchased = userLicenseMap[_wallet];

        require(
            license.duration > 0,
            "License is not available"
        );

        uint256 endTime = _startTime + (license.duration * _count);
        if (userLicensePurchased.startTime != 0) {

            require(keccak256(abi.encodePacked(_type)) == keccak256(abi.encodePacked(userLicensePurchased.licenseType)) ||
                _count != userLicensePurchased.count, "Already have this License");
            require(getLicenseTypeOrder(_type) >= getLicenseTypeOrder(userLicensePurchased.licenseType), "type should not be downgraded");
            require(_count >= userLicensePurchased.count, "count should not be downgraded");

            endTime = userLicensePurchased.startTime + (license.duration * _count);

            string memory oldType = userLicensePurchased.licenseType;
            userLicenseMap[_wallet].licenseType = _type;
            userLicenseMap[_wallet].count = _count;
            userLicenseMap[_wallet].startTime = _startTime;
            userLicenseMap[_wallet].endTime = endTime;

            emit UserLicenseUpdated(_wallet, oldType, _type, _count, _startTime, endTime);
            return;

        }

        userLicenseList.push(_wallet);
        userLicenseMap[_wallet] = UserLicenseStruct(_wallet, _type, _count, _startTime, endTime);
        emit UserLicenseAdded(_wallet, _type, _count, _startTime, endTime);

    }

    function getSelectedPrice(string memory _type) public view returns (uint256){
        uint256 selectedPrice = license.priceList[licenseModelNameOrder[_type]];
        return selectedPrice;
    }

    function getLicenseTypeOrder(string memory _type) public view returns (uint){
        return licenseModelNameOrder[_type];
    }

    function getUserLicense(address _wallet) public view returns (UserLicenseStruct memory){
        return userLicenseMap[_wallet];
    }

    function buyWithUniswapEth(uint256 count, string memory _type, address[] calldata path) public payable {
        buyWithUniswapErc20(count, 'native', msg.value, _type, path);
    }

    function getAmountsIn(uint amountOut, address[] calldata path) internal view returns (uint amount){
        return IUniswapV2Router02(uniswapRouter).getAmountsIn(amountOut, path)[0];
    }

    function checkIsEnoughTokenReceived(uint256 swashNeeded, uint256 amount, string memory from_token_name,
        address[] calldata path) public {

        TokenStruct memory tokenOb = getTokenOb(from_token_name);
        uint256 neededToken = getAmountsIn(swashNeeded, path);
        require(neededToken >= amount, "Not enough token received");
    }

    function priceInSwash(uint256 priceInBaseToken) public view returns (uint256){
        return IUniswapV2Router02(uniswapRouter).getAmountsIn(priceInBaseToken, _uniswapPathToBaseToken(swashTokenAddress))[0];
    }

    function buyWithUniswapErc20(uint256 count, string memory from_token_name, uint256 amount, string memory _type, address[] calldata path) public {

        uint timeWindow = 600;

        TokenStruct memory tokenOb = getTokenOb(from_token_name);

        require(
            tokenOb.tokenAddress != address(0) ||
            keccak256(abi.encodePacked(from_token_name)) == keccak256(abi.encodePacked('native')),
            "from token not valid");
        console.log('--passed--');

        address from_token = tokenOb.tokenAddress == address(0) ? IUniswapV2Router02(uniswapRouter).WETH() : tokenOb.tokenAddress;

        if (tokenOb.tokenAddress != address(0)) {
            IERC20(tokenOb.tokenAddress).transferFrom(msg.sender, address(this), amount);
        }

        uint256 selectedPrice = getSelectedPrice(_type);
        uint256 remainedMoney = getRemainedMoneyFromPreviousVersion(msg.sender);

        uint swashNeeded = priceInSwash((selectedPrice * count) - remainedMoney);
        checkIsEnoughTokenReceived(swashNeeded, amount, from_token_name, path);

        changeTokenAndPurchase(_type, count, from_token, amount, swashNeeded, path);
    }

    function getRemainedMoneyFromPreviousVersion(address _wallet) public view returns (uint256){

        require(
            license.duration > 0,
            "License is not available"
        );

        UserLicenseStruct memory userLicense = getUserLicense(_wallet);

        if (userLicense.startTime != 0) {


            uint256 totalPrice = getSelectedPrice(userLicense.licenseType);

            return totalPrice * (userLicense.endTime - block.timestamp) / license.duration;
        }
        return 0;
    }

    function changeTokenAndPurchase(string memory _type, uint count, address from_token, uint256 amount,
        uint256 swashNeeded, address[] calldata path) internal {

        uint256 deadline = block.timestamp + timeWindow;
        uint256 minSwashAmount = swashNeeded - swashNeeded * SLIPPAGE / 1000;

        if (from_token == address(IUniswapV2Router02(uniswapRouter).WETH())) {
            IUniswapV2Router02(uniswapRouter).swapExactETHForTokens{value : amount}(minSwashAmount, path, address(this), deadline)[1];
        }
        else if (from_token != address(swashTokenAddress)) {
            IUniswapV2Router02(uniswapRouter).swapExactTokensForTokens(amount, minSwashAmount, path, address(this), deadline)[1];
        }
        purchaseLicense(msg.sender, count, _type);
        //        callPurchase(received_swash, count, _type);
    }

    function purchaseLicense(address _wallet, uint256 _count, string memory _type) internal {
        //addLicense
        addOrUpdateUserLicenseInternal(_wallet, _type, _count, block.timestamp);
        emit Purchased(msg.sender, _count);

    }

    //    function purchaseLicenseWithNativeToken(address _wallet, uint256 _count, string memory _type) public payable {
    //        require(
    //            license.duration > 0,
    //            "License is not available"
    //        );
    //
    //        uint256 remainedMoney = getRemainedMoneyFromPreviousVersion(_wallet);
    //        uint256 selectedPrice = getSelectedPrice(_type);
    //
    //        uint256 totalPrice = priceInSwash((selectedPrice * _count)- remainedMoney);
    //        console.log('msg.value');
    //        console.log(msg.value);
    //        console.log(totalPrice);
    //        require(
    //            msg.value == totalPrice,
    //            "sent amount is not equals to totalPrice"
    //        );
    //
    //        addOrUpdateUserLicenseInternal(_wallet, _type, _count, block.timestamp);
    //        emit PurchasedWithNativeToken(_wallet, _count, totalPrice, _type);
    //
    //    }

    function withdraw(address _tokenAddress) external onlyOwner {
        uint256 balance = IERC20(_tokenAddress).balanceOf(address(this));
        IERC20(_tokenAddress).transfer(msg.sender, balance);
        emit Withdrawn(msg.sender, balance, _tokenAddress);
    }

    function withdrawNative() external onlyOwner payable {
        uint256 balance = address(this).balance;
        //        bool sent = msg.sender.send(balance);
        (bool sent, bytes memory data) = msg.sender.call{value : balance}("");

        require(sent, "Failed to send native token");

        emit WithdrawnNative(msg.sender, balance);
    }

    function getAvailableBuyList(string memory _type, string[] memory tokenListIn, address[][] calldata paths) external view returns (LicenseToBuy[] memory){

        string[] memory tokenNameList = tokenListIn;

        LicenseToBuy[] memory retList = new LicenseToBuy[](tokenNameList.length);
        LicenseToBuy memory licenseToBuy;

        for (uint idx = 0; idx < tokenNameList.length; ++idx) {
            TokenStruct memory tokenOb = getTokenOb(tokenNameList[idx]);
            address[] calldata path = paths[idx];
            uint256 swashPrice = getSelectedPrice(_type);
            uint256 price = getAmountsIn(priceInSwash(swashPrice), path);
            licenseToBuy.tokenName = (tokenOb.name);
            licenseToBuy.tokenAddress = (tokenOb.tokenAddress);
            licenseToBuy.isNative = (tokenOb.isNative);
            licenseToBuy.price = (price);
            retList[idx] = licenseToBuy;
        }
        return retList;
    }


}

