// CHANGE whitelist_mint to whitelistMint

contract StakeBears is IERC721Receiver, Ownable {
    using EnumerableSet for EnumerableSet.UintSet;
    using ABDKMath64x64 for int128;

    address public ERC20_CONTRACT = 0x2a3E525B4b53A832A3c3CCC23A2e4363e28aE083;
    address public ERC721_CONTRACT = 0x4061Ba38fEd8CaBd4f318E0620f20B35db298Fbf;
    address public DAO_VAULT = 0x33d5E481295920cB1cE12Ab1Cf557e93482038e1;
    address public ROYALTIES_ADDRESS = 0xAA32Ed4376bDB9645a9059d9EAb32ae5dDE34E1D;

    mapping(address => EnumerableSet.UintSet) private _deposits;
    mapping(address => mapping(uint256 => uint256)) public depositBlocks;
    mapping(address => uint256) public claimedRewards;

    function totalFROST() public view returns (uint256) {
        uint256 total;
        total = IFROST(ERC20_CONTRACT).balanceOf(address(this));
        return total;
    }

    bool started = false;
    
    // Only set once
    uint256 public startingBlock = 0;
    // Factor of deflation: 0.1
    uint256 public deflationFactor = 100000000000000000;
    // Number of blocks between deflations
    uint256 public deflationLength = 10;
    
    function toggleStart() public onlyOwner() {
        started = !started;
        if (startingBlock == 0) {
            startingBlock = block.number;
        }
    }

    function setDeflationFactor(uint256 _factor) public onlyOwner() {
        // Used to change deflation factor if needed
        deflationFactor = _factor;
    }

    function setDeflationLength(uint256 _length) public onlyOwner() {
        // Used to change deflation length if needed
        deflationLength = _length;
    }

    function setDAOAddress(address _vaultAddress) public onlyOwner() {
        // Used to change vault address if needed
        DAO_VAULT = _vaultAddress;
    }

    function setRoyaltiesAddress(address _royaltiesAddress) public onlyOwner() {
        // Used to change royalties address if needed
        ROYALTIES_ADDRESS = _royaltiesAddress;
    }

    function setTokenAddress(address _tokenAddress) public onlyOwner() {
        // Used to change rewards token if needed
        ERC20_CONTRACT = _tokenAddress;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function deflate(uint256 amount) public view returns (uint256) {
        uint256 period = (block.number - startingBlock) / deflationLength;
        if (startingBlock == 0) {
            period = 0;
        }
        return ABDKMath64x64.mulu (
            ABDKMath64x64.pow (
            ABDKMath64x64.sub (
                ABDKMath64x64.fromUInt (1), 
                ABDKMath64x64.divu (
                deflationFactor,
                10**18)),
            period),
            amount);
    }

    function depositsOf(address account) external view returns (uint256[] memory) {
        EnumerableSet.UintSet storage depositSet = _deposits[account];
        uint256[] memory tokenIds = new uint256[](depositSet.length());

        for (uint256 i; i < depositSet.length(); i++) {
            tokenIds[i] = depositSet.at(i);
        }

        return tokenIds;
    }

    function calculateRewards(address account, uint256[] memory tokenIds) public view returns (uint256[] memory rewards) {
        rewards = new uint256[](tokenIds.length);
        uint256 total = totalFROST();

        for (uint256 i; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];

            rewards[i] =
                (((total / 10000) * (_deposits[account].contains(tokenId) ? 1 : 0)
                * (block.number - depositBlocks[account][tokenId]))
                / (block.number - startingBlock));
        }
    }

    function claimRewards(uint256[] calldata tokenIds) public {
        uint256 reward;

        uint256[] memory rewards = calculateRewards(msg.sender, tokenIds);

        for (uint256 i; i < tokenIds.length; i++) {
            reward += rewards[i];
        }

        reward -= claimedRewards[msg.sender];

        if (reward > 0) {
            IFROST(ERC20_CONTRACT).transfer(msg.sender, reward);
            claimedRewards[msg.sender] += reward;
        }
    }

    function mintFROSTWithRoyalties() public payable {
        require(msg.sender == ROYALTIES_ADDRESS);
        uint256 amount = deflate(msg.value);
        IFROST(ERC20_CONTRACT).whitelist_mint(address(this), amount);
        payable(DAO_VAULT).transfer(msg.value);
    }

    function buyFROST() public payable {
        uint256 amount = deflate(msg.value);
        IFROST(ERC20_CONTRACT).whitelist_mint(msg.sender, amount);
        payable(DAO_VAULT).transfer(msg.value);
    }

    function viewPeriod() public view returns (uint256) {
        uint256 period = (block.number - startingBlock) / deflationLength;
        return period;
    }

    function deposit(uint256[] calldata tokenIds) external {
        require(started, 'StakeBears: Staking contract not started yet');
        
        for (uint256 i; i < tokenIds.length; i++) {
            IERC721(ERC721_CONTRACT).safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i],
                ''
            );
            _deposits[msg.sender].add(tokenIds[i]);
            depositBlocks[msg.sender][tokenIds[i]] = block.number;
        }
    }

    function adminDeposit(uint256[] calldata tokenIds) onlyOwner() external {
        
        for (uint256 i; i < tokenIds.length; i++) {
            IERC721(ERC721_CONTRACT).safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i],
                ''
            );
            _deposits[msg.sender].add(tokenIds[i]);
            depositBlocks[msg.sender][tokenIds[i]] = block.number;
        }
    }

    function withdraw(uint256[] calldata tokenIds) external {
        claimRewards(tokenIds);

        for (uint256 i; i < tokenIds.length; i++) {
            require(
                _deposits[msg.sender].contains(tokenIds[i]),
                'StakeBears: Token not deposited'
            );

            _deposits[msg.sender].remove(tokenIds[i]);

            IERC721(ERC721_CONTRACT).safeTransferFrom(
                address(this),
                msg.sender,
                tokenIds[i],
                ''
            );
        }
    }

    function emergencyWithdraw(uint256[] calldata tokenIds) external {
      
        for (uint256 i; i < tokenIds.length; i++) {
              require(
                  _deposits[msg.sender].contains(tokenIds[i]),
                  'StakeBears: Token not deposited'
              );

              _deposits[msg.sender].remove(tokenIds[i]);

              IERC721(ERC721_CONTRACT).safeTransferFrom(
                  address(this),
                  msg.sender,
                  tokenIds[i],
                  ''
              );
          }
      }
}