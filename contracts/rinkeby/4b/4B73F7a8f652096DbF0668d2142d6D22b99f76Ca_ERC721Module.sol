pragma solidity 0.8.9;

// TODO EIP712 sign typed data

import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "../interfaces/ITransferProxy.sol";
import "../interfaces/IWETH.sol";
import "../interfaces/ISubModule.sol";
import "../interfaces/IERC721Module.sol";
import "../base/OrderInvalidator.sol";
import "../libs/LibOrder.sol";
import "../libs/LibEthSigHash.sol";
import "../base/FeePusher.sol";
import "hardhat/console.sol";

/**
 * @title Merito's ERC721 Module Contract
 * @author Merit Circle
 * @notice Contract that provides the logic to match and fill both buy and sell orders.
 * @dev Manages the matching of orders in batches or individual buy or sell orders Grants and revokes to add/remove submodules (Open Zeppelin's AccessControlEnumerable).
 */
contract ERC721Module is IERC721Module, OrderInvalidator, AccessControlEnumerable, FeePusher {
    using SafeERC20 for IERC20;

    error OrderArrayLengthInvalid();
    error BuyOrderInvalidated();
    error FeeTooHighError();
    error SellOrderInvalidated();
    error BuyOrderVerificationFailedError();
    error SellOrderVerificationFailedError();
    error NFTAddressMismatchError();
    error NFTIdMismatchError();
    error PriceMismatchError();
    error TokenMismatchError();
    error BuyOrderExpired();
    error SellOrderExpired();
    error BuyOrderTypeInvalid();
    error SellOrderTypeInvalid();
    error BuyOrderTakerMismatch();
    error SellOrderTakerMismatch();
    error FeeMismatchError();
    error InsufficientETHError();
    error TokenNotWETHError();
    error CallNotSuccessful();
    error SenderIsNotWETH();

    error InvalidSenderError();

    error NotSubModuleAdderError();
    error NotSubModuleRemoverError();
    error SubModuleAlreadyAddedError(address submodule);
    error SubModuleDoesNotExistError(address submodule);
    error SubModuleAlreadyActivatedError(address submodule);
    error SubModuleAlreadyDeactivatedError(address submodule);

    event SubModuleAdded(address indexed submodule);
    event SubModuleRemoved(address indexed submodule);
    event SubModuleActivated(address indexed submodule);
    event SubModuleDeactivated(address indexed submodule);
    event UserSubmoduleActivated(address indexed user, address indexed submodule);
    event UserSubmoduleDeactivated(address indexed user, address indexed submodule);

    event OrderFilled(address indexed maker, uint256 indexed orderId, bool indexed orderType);

    address public immutable feeReceiver;
    address public immutable WETH;
    ITransferProxy public immutable transferProxy;

    mapping(address => bool) public isSubModule; // Possibly replace this with accesControl by OpenZeppelin
    mapping(address => mapping(address => bool)) public isUserSubModule; // User activated modules

    bytes32 public constant SUBMODULE_ADDER_ROLE = keccak256("SUBMODULE_ADDER_ROLE");
    bytes32 public constant SUBMODULE_REMOVER_ROLE = keccak256("SUBMODULE_REMOVER_ROLE");

    /**
     * @notice Lets only submodule adders.
     * @dev Reverts with error in case the sender does not have the submodule adder role.
     */
    modifier onlySubModuleAdder() {
        if (!hasRole(SUBMODULE_ADDER_ROLE, msg.sender)) {
            revert NotSubModuleAdderError();
        }
        _;
    }

    /**
     * @notice Lets only submodule remover.
     * @dev Reverts with error in case the sender does not have the submodule remover role.
     */
    modifier onlySubModuleRemover() {
        if (!hasRole(SUBMODULE_REMOVER_ROLE, msg.sender)) {
            revert NotSubModuleRemoverError();
        }
        _;
    }

    /**
     * @notice Sets the main external variables.
     * @dev On deployments sets the parent smart contract to manage the transactions, WETH contract to manage unwrapping,
     * @param _feeReceiver Address of the receiver of the fees of the transactions.
     * @param _transferProxy Address of Transfer Proxy contract.
     * @param _WETH Address of wrapped ether.
     */
    constructor(
        address _feeReceiver,
        address _transferProxy,
        address _WETH
    ) {
        feeReceiver = _feeReceiver;
        transferProxy = ITransferProxy(_transferProxy);
        WETH = _WETH;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender); // Should be another address?
    }

    /**
     * @notice Fallback to receive ETH from WETH contract.
     * @dev Only receives ETH from the WETH smart contract to prevent users from sending it accidentally.
     */
    receive() external payable {
        if (msg.sender != WETH) {
            revert SenderIsNotWETH();
        }
    }

    /**
     * @notice Adds submodules for future usage.
     * @dev Adds the submodule to the isSubModule mapping. Emits event with the address of the submodule.
     * @param _submodule The submodule to be added.
     */
    function addSubModule(address _submodule) external onlySubModuleAdder {
        if (isSubModule[_submodule]) {
            revert SubModuleAlreadyAddedError(_submodule);
        }

        isSubModule[_submodule] = true;
        emit SubModuleAdded(_submodule);
    }

    /**
     * @notice Removes submodules to prevent its usage.
     * @dev Removes the submodule from the isSubModule mapping. Emits event with the address of the submodule.
     * @param _submodule The module to be removed.
     */
    function removeSubModule(address _submodule) external onlySubModuleRemover {
        if (!isSubModule[_submodule]) {
            revert SubModuleDoesNotExistError(_submodule);
        }

        isSubModule[_submodule] = false;
        emit SubModuleRemoved(_submodule);
    }

    /**
     * @notice Activates submodules for users.
     * @dev Adds the submodule to the isUserSubModule mapping for the msg.sender. Emits event with the address of the submodule.
     * @param _submodule The submodule to be activated.
     */
    function activateUserSubModule(address _submodule) external {
        if (isUserSubModule[msg.sender][_submodule]) {
            revert SubModuleAlreadyActivatedError(_submodule);
        }

        isUserSubModule[msg.sender][_submodule] = true;
        emit UserSubmoduleActivated(msg.sender, _submodule);
    }

    /**
     * @notice Deactivates submodules for users.
     * @dev Removes the submodule from the isUserSubModule mapping for the msg.sender. Emits event with the address of the submodule.
     * @param _submodule The submodule to be deactivated.
     */
    function deactivateUserSubModule(address _submodule) external {
        if (!isUserSubModule[msg.sender][_submodule]) {
            revert SubModuleAlreadyDeactivatedError(_submodule);
        }

        isUserSubModule[msg.sender][_submodule] = false;
        emit UserSubmoduleDeactivated(msg.sender, _submodule);
    }

    // TODO consider removing most sellorderdata from calldata and copying it over
    /**
     * @notice Matches orders in pairs.
     * @dev Function that matches buy orders with sell orders.
     * @param _orders Array of orders to be matched. The array should have even length, each buy order should have an even index and each sell order an odd index. Emits one detailed event for the sell order and one for the buy order.
     */
    function batchMatchOrders(
        LibOrder.VerifiedOrder[] calldata _orders,
        bool _revertOnFail
    ) external {
        if (_orders.length % 2 != 0) {
            revert OrderArrayLengthInvalid();
        }

        for (uint256 i = 0; i < _orders.length; i += 2) {
            LibOrder.VerifiedOrder memory buyOrder = _orders[i];
            LibOrder.VerifiedOrder memory sellOrder = _orders[i + 1];

            // If we allow matches to fail call into the contract and check for success
            if(!_revertOnFail) {
                (bool success,) = address(this).call(abi.encodeWithSelector(this.validateMatchAndTransfer.selector, buyOrder, sellOrder, msg.sender));
                // if failed go to next loop iteration
                if(!success) {
                    continue;
                }
            } else {
                _validateMatchAndTransfer(buyOrder, sellOrder, msg.sender);(buyOrder, sellOrder, msg.sender);
            }
            
        }
    }

    function validateMatchAndTransfer(
        LibOrder.VerifiedOrder memory _buyOrder,
        LibOrder.VerifiedOrder memory _sellOrder,
        address _sender
    ) external {
        if(msg.sender != address(this)) {
            revert InvalidSenderError();
        }
        _validateMatchAndTransfer(_buyOrder, _sellOrder, _sender);
    }

    function _validateMatchAndTransfer(
        LibOrder.VerifiedOrder memory _buyOrder,
        LibOrder.VerifiedOrder memory _sellOrder, address _sender
    ) internal {
        _validateMatch(_buyOrder, _sellOrder, _sender);
        _matchInvalidateOrders(_buyOrder, _sellOrder);
        _matchTransfer(_buyOrder, _sellOrder);
        _matchEvents(_buyOrder, _sellOrder);
    }

    function _matchInvalidateOrders(
        LibOrder.VerifiedOrder memory _buyOrder,
        LibOrder.VerifiedOrder memory _sellOrder
    ) internal {
        // Only invalidate if needed.
        if (_buyOrder.order.orderId != 0) {
            _setOrderInvalidated(_buyOrder.order.maker, _buyOrder.order.orderId);
        }
        // Only invalidate if needed.
        if (_sellOrder.order.orderId != 0) {
            _setOrderInvalidated(_sellOrder.order.maker, _sellOrder.order.orderId);
        }
    }

    function _matchEvents(LibOrder.VerifiedOrder memory _buyOrder, LibOrder.VerifiedOrder memory _sellOrder) internal {
        emit OrderFilled(_sellOrder.order.maker, _sellOrder.order.orderId, _sellOrder.order.orderType);
        emit OrderFilled(_buyOrder.order.maker, _buyOrder.order.orderId, _buyOrder.order.orderType);
    }   

    function _matchTransfer(
        LibOrder.VerifiedOrder memory _buyOrder,
        LibOrder.VerifiedOrder memory _sellOrder
    ) internal {
        // INTERACTIONS
            uint256 fee = (_sellOrder.order.tokenAmount * _sellOrder.order.feeFraction) / 1e18;

            bytes memory nftTransferCall = abi.encodeWithSelector(
                transferProxy.transferNFT.selector,
                _buyOrder.order.nft,
                _sellOrder.order.maker,
                _buyOrder.order.maker,
                _sellOrder.order.nftId
            );

            // If sellOrder was ETH transfer ETH by unwrapping WETH
            if (_sellOrder.order.token == address(0)) {
                bytes[] memory transferCalls = new bytes[](2);
                // NFT transfer
                transferCalls[0] = nftTransferCall;
                // Transfer WETH to this address
                transferCalls[1] = abi.encodeWithSelector(
                    transferProxy.transferToken.selector,
                    _buyOrder.order.token,
                    _buyOrder.order.maker,
                    address(this),
                    _sellOrder.order.tokenAmount
                );
                transferProxy.batch(transferCalls, true);

                // Withdraw WETH
                IWETH(WETH).withdraw(_sellOrder.order.tokenAmount - fee);
                (bool success, ) = payable(_sellOrder.order.maker).call{value: _sellOrder.order.tokenAmount - fee}("");
                if (!success) {
                    revert CallNotSuccessful();
                }
            } else {
                bytes[] memory transferCalls = new bytes[](3);
                // NFT transfer
                transferCalls[0] = nftTransferCall;
                // transfer amount to seller minus fee
                transferCalls[1] = abi.encodeWithSelector(
                    transferProxy.transferToken.selector,
                    _buyOrder.order.token,
                    _buyOrder.order.maker,
                    _sellOrder.order.maker,
                    _sellOrder.order.tokenAmount - fee
                );
                // transfer fee
                transferCalls[2] = abi.encodeWithSelector(
                    transferProxy.transferToken.selector,
                    _buyOrder.order.token,
                    _buyOrder.order.maker,
                    address(this),
                    fee
                );
                
                transferProxy.batch(transferCalls, true);
            }
    }

    function _validateMatch(
        LibOrder.VerifiedOrder memory _buyOrder,
        LibOrder.VerifiedOrder memory _sellOrder,
        address _sender
    ) internal {
        if (orderInvalidated(_buyOrder.order.maker, _buyOrder.order.orderId) == true) {
            revert BuyOrderInvalidated();
        }

        if (orderInvalidated(_sellOrder.order.maker, _sellOrder.order.orderId) == true) {
            revert SellOrderInvalidated();
        }

        // CHECKS
        if (!validateOrderVerification(_buyOrder, _sender)) {
            revert BuyOrderVerificationFailedError();
        }

        if (!validateOrderVerification(_sellOrder, _sender)) {
            revert SellOrderVerificationFailedError();
        }

        if (_buyOrder.order.feeFraction != _sellOrder.order.feeFraction) {
            revert FeeMismatchError();
        }

        // Only need to check one side
        if (_buyOrder.order.feeFraction > 1e18) {
            revert FeeTooHighError();
        }

        if (_buyOrder.order.nft != _sellOrder.order.nft) {
            revert NFTAddressMismatchError();
        }

        // Check if tokens match or buy order token is WETH and sell order token is ETH
        if (
            !(_buyOrder.order.token == _sellOrder.order.token) &&
            !(_buyOrder.order.token == WETH && _sellOrder.order.token == address(0))
        ) {
            revert TokenMismatchError();
        }

        if (_buyOrder.order.nftId != _sellOrder.order.nftId) {
            revert NFTIdMismatchError();
        }

        if (_sellOrder.order.tokenAmount > _buyOrder.order.tokenAmount) {
            revert PriceMismatchError();
        }

        if (_buyOrder.order.validTill < block.timestamp) {
            revert BuyOrderExpired();
        }

        if (_sellOrder.order.validTill < block.timestamp) {
            revert SellOrderExpired();
        }

        if (_buyOrder.order.orderType != LibOrder.BUY_TYPE) {
            revert BuyOrderTypeInvalid();
        }

        if (_sellOrder.order.orderType != LibOrder.SELL_TYPE) {
            revert SellOrderTypeInvalid();
        }

        if (_buyOrder.order.taker != address(0) && _sellOrder.order.maker != _buyOrder.order.taker) {
            revert BuyOrderTakerMismatch();
        }

        if (_sellOrder.order.taker != address(0) && _buyOrder.order.maker != _sellOrder.order.taker) {
            revert SellOrderTakerMismatch();
        }
    }

    /**
     * @notice Fills a sell order.
     * @dev Fills a sell order using ETH (can unwrap WETH from buyer) or any ERC20 token.
     * @param _order Sell VerifiedOrder struct to be filled (Includes signature and verification method). Emits an event for the order filled.
     * @param _receiver Address that receives the NFT.
     */
    function fillSell(LibOrder.VerifiedOrder memory _order, address _receiver) external payable {
        if (_order.order.taker != address(0) && _order.order.taker != msg.sender) {
            revert SellOrderTakerMismatch();
        }

        if (_order.order.validTill < block.timestamp) {
            revert SellOrderExpired();
        }

        if (_order.order.orderType != LibOrder.SELL_TYPE) {
            revert SellOrderTypeInvalid();
        }

        if (!validateOrderVerification(_order, msg.sender)) {
            revert SellOrderVerificationFailedError();
        }

        if (orderInvalidated(_order.order.maker, _order.order.orderId) == true) {
            revert SellOrderInvalidated();
        }

        if (_order.order.orderId != 0) {
            _setOrderInvalidated(_order.order.maker, _order.order.orderId);
        }

        if (_order.order.feeFraction > 1e18) {
            revert FeeTooHighError();
        }

        uint256 fee = (_order.order.tokenAmount * _order.order.feeFraction) / 1e18;

        // Handle ETH payout
        if (_order.order.token == address(0)) {
            if (msg.value != 0) {
                // Not enough ETH send
                if (msg.value != 0 && msg.value < _order.order.tokenAmount) {
                    revert InsufficientETHError();
                }
                (bool success, ) = payable(_order.order.maker).call{value: _order.order.tokenAmount - fee}("");
                if (!success) {
                    revert CallNotSuccessful();
                }
            } else {
                // unwrap WETH and send it
                // pull WETH
                transferProxy.transferToken(WETH, msg.sender, address(this), _order.order.tokenAmount);
                // Withdraw WETH
                IWETH(WETH).withdraw(_order.order.tokenAmount - fee);
                (bool success, ) = payable(_order.order.maker).call{value: _order.order.tokenAmount - fee}("");
                if (!success) {
                    revert CallNotSuccessful();
                }
            }
        } else {
            // handle token payout
            if (msg.value > 0) {
                // Check if token is WETH, otherwise revert
                if (_order.order.token != WETH) {
                    revert TokenNotWETHError();
                }
                // If not enough WETH send
                if (msg.value < _order.order.tokenAmount) {
                    revert InsufficientETHError();
                }

                payable(WETH).call{ value: msg.value }("");
                IERC20(WETH).safeTransfer(_order.order.maker, _order.order.tokenAmount - fee);
            } else {
                // Pull fee
                transferProxy.transferToken(_order.order.token, msg.sender, address(this), fee);
                // Send token to seller
                transferProxy.transferToken(
                    _order.order.token,
                    msg.sender,
                    _order.order.maker,
                    _order.order.tokenAmount - fee
                );
            }
        }

        transferProxy.transferNFT(_order.order.nft, _order.order.maker, _receiver, _order.order.nftId);
        emit OrderFilled(_order.order.maker, _order.order.orderId, _order.order.orderType);
    }

    /**
     * @notice Fills a buy order.
     * @dev Fills a buy order using ETH (unwraps WETH from buyer) or any ERC20 token.
     * @param _order Buy VerifiedOrder struct to be filled (Includes signature and verification method). Emits an event for the order filled.
     * @param _wantEth Bool expressing if seller accepts ETH.
     * @param _receiver Address that receives the ETH or tokens.
     */
    function fillBuy(
        LibOrder.VerifiedOrder memory _order,
        bool _wantEth,
        address _receiver
    ) external {
        if (_order.order.taker != address(0) && _order.order.taker != msg.sender) {
            revert BuyOrderTakerMismatch();
        }

        if (_order.order.validTill < block.timestamp) {
            revert BuyOrderExpired();
        }

        if (_order.order.orderType != LibOrder.BUY_TYPE) {
            revert BuyOrderTypeInvalid();
        }

        if (!validateOrderVerification(_order, msg.sender)) {
            revert BuyOrderVerificationFailedError();
        }

        if (orderInvalidated(_order.order.maker, _order.order.orderId) == true) {
            revert BuyOrderInvalidated();
        }

        if (_order.order.orderId != 0) {
            _setOrderInvalidated(_order.order.maker, _order.order.orderId);
        }

        if (_order.order.feeFraction > 1e18) {
            revert FeeTooHighError();
        }

        uint256 fee = (_order.order.tokenAmount * _order.order.feeFraction) / 1e18;

        // If buy order == weth and seller wants ETH
        if (_order.order.token == WETH && _wantEth) {
            transferProxy.transferToken(WETH, _order.order.maker, address(this), _order.order.tokenAmount);
            IWETH(WETH).withdraw(_order.order.tokenAmount);
            (bool success, ) = payable(_receiver).call{value: _order.order.tokenAmount - fee}("");
            if (!success) {
                revert CallNotSuccessful();
            }
        } else {
            transferProxy.transferToken(
                _order.order.token,
                _order.order.maker,
                _receiver,
                _order.order.tokenAmount - fee
            );
            transferProxy.transferToken(_order.order.token, _order.order.maker, address(this), fee);
        }

        // Transfer nft
        transferProxy.transferNFT(_order.order.nft, msg.sender, _order.order.maker, _order.order.nftId);
        emit OrderFilled(_order.order.maker, _order.order.orderId, _order.order.orderType);
    }

    /**
     * @notice Verifies if the order is valid for a sender.
     * @dev Verifies the integrity of the signature, both from an order sender or a contract.
     * @param _order VerifiedOrder struct to be validated (Includes signature and verification method).
     * @param _sender Address from the sender of the order.
     * @return Whether or not the order is valid for the given sender.
     */
    function validateOrderVerification(LibOrder.VerifiedOrder memory _order, address _sender) internal returns (bool) {
        // if msg.sender is filling the order there is no need to check the signature
        if (_order.order.maker == _sender) {
            return true;
        }

        if (_order.verificationMethod == LibOrder.SIGNATURE_VERIFICATION) {
            // https://github.com/0xProject/protocol/blob/24397c51a8c7bf704948c8fc6874843bccd5d244/contracts/zero-ex/contracts/src/features/libs/LibSignature.sol#L90-L96
            bytes32 orderHash = keccak256(abi.encode(_order.order, address(this)));
            bytes32 ethSigHash = LibEthSigHash.hash(orderHash);
            return SignatureChecker.isValidSignatureNow(_order.order.maker, ethSigHash, _order.signature);
        } else if (_order.verificationMethod == LibOrder.CONTRACT_VERIFICATION) {
            // module signature format format
            //
            //  {
            //      address - address of the verification contract
            //      bytes - arbitrary data
            //  }
            (address module, bytes memory data) = abi.decode(_order.signature, (address, bytes));
            // check if is global module or user specific module
            if (isSubModule[module] || isUserSubModule[_order.order.maker][module]) {
                return ISubModule(module).validateAndHandleOrder(_order.order, data);
            }

            return false;
        }

        return false;
    }

    /**
     * @notice Hashes orders.
     * @dev Produces a keccak256 hash after encoding the order struct.
     * @param _order Order struct to be encoded and hashed.
     * @return Keccak256 hash of the order.
     */
    function hashOrder(LibOrder.Order memory _order) external view returns (bytes32) {
        return keccak256(abi.encode(_order, address(this)));
    }
}

