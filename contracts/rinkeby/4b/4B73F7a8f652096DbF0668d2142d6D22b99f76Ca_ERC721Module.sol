pragma solidity 0.8.9;

/**
 * @title Merito's ETH Signature Hash Library Contract
 * @author Merit Circle
 * @notice Library that computes the special Ethereum hash signature.
 * @dev This contract produces the hash necessary to sign and recover signatures in Ethereum.
 */
library LibEthSigHash {
    /**
     * @notice Produces a hash that can be used for signatures in Ethereum.
     * @dev Generates the hash from an input using the special constants that Ethereum needs to comply with the protocol signature standard.
     * @param _hash Hash of information to be signed.
     * @return Ethereum hash of input information.
     */
    function hash(bytes32 _hash) internal pure returns (bytes32) {
        // https://github.com/0xProject/protocol/blob/24397c51a8c7bf704948c8fc6874843bccd5d244/contracts/zero-ex/contracts/src/features/libs/LibSignature.sol#L90-L96
        bytes32 ethSigHash;
        assembly {
            // Use scratch space
            mstore(0, 0x19457468657265756d205369676e6564204d6573736167653a0a333200000000) // length of 28 bytes
            mstore(28, _hash) // length of 32 bytes
            ethSigHash := keccak256(0, 60)
        }
        return ethSigHash;
    }
}

pragma solidity 0.8.9;

import "@merit-cirlce/merkle-orchard/contracts/IMerkleOrchard.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

/**
 * @title Merito's Fee Pusher Contract
 * @author Merit Circle
 * @notice Contract for pushing fees into channels.
 * @dev The contract manages the functions for setting orchard contracts. Then provides the methods for setting channels and fund them with tokens.
 */
// Inheriting contract must set DEFAULT_ADMIN_ROLE
abstract contract FeePusher is AccessControlEnumerable {
    error NotAdminError();
    error ChannelIdDoesNotExistError();

    event ChannelIdSet(uint256 indexed channelId);
    event OrchardContractSet(address indexed orchardContract);
    event TokenPushed(address indexed token, uint256 amount, address caller);

    uint256 public channelId;
    IMerkleOrchard public orchardContract;

    /**
     * @notice Sets a channel id.
     * @dev Sets the channel id into storage. Emits event with the id of the channel.
     * @param _channelId Id of the channel to be set.
     */
    function setChannelId(uint256 _channelId) external {
        if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) {
            revert NotAdminError();
        }

        try orchardContract.ownerOf(_channelId) {
            channelId = _channelId;
            emit ChannelIdSet(_channelId);
        } catch {
            revert ChannelIdDoesNotExistError();
        }

    }

    /**
     * @notice Sets the orchard contract.
     * @dev An address with admin role sets a contract of the orchard contract for usage in other methods. Emits event with the address of the contract.
     * @param _orchardContract Address of the contract to be set.
     */
    function setOrchardContract(address _orchardContract) external {
        if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) {
            revert NotAdminError();
        }

        orchardContract = IMerkleOrchard(_orchardContract);
        emit OrchardContractSet(_orchardContract);
    }

    /**
     * @notice Funds channel with tokens.
     * @dev Uses the balance of different tokens of this smart contract to fund a channel.
     * @param _tokens Array of address of the tokens used to fund channels.
     */
    function pushTokens(address[] calldata _tokens) external {
        for (uint256 i = 0; i < _tokens.length; i++) {
            IERC20 token = IERC20(_tokens[i]);
            uint256 balance = token.balanceOf(address(this));

            token.approve(address(orchardContract), balance);
            orchardContract.fundChannel(channelId, _tokens[i], balance);

            emit TokenPushed(_tokens[i], balance, msg.sender);
        }
    }

    /**
     * @notice Funds channel with ETH.
     * @dev Uses the balance of this smart contract to fund a channel.
     */
    function pushEth() external {
        uint256 balance = address(this).balance;
        orchardContract.fundChannelWithEth{ value: balance }(channelId);
        emit TokenPushed(address(0), balance, msg.sender);
    }
}

