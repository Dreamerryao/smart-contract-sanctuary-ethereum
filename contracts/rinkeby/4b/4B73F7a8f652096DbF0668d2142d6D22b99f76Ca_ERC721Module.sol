pragma solidity 0.8.9;

import "@openzeppelin/contracts/utils/structs/BitMaps.sol";

/**
 * @title Merito's Order Invalidator Contract
 * @author Merit Circle
 * @notice Contract that invalidates orders and keep track of them.
 * @dev Invalidates orders and stores that information into a mapping for keeping track of the past orders.
 */
contract OrderInvalidator {
    using BitMaps for BitMaps.BitMap;

    error InvalidateZeroOrderError();
    event OrderInvalidated(address indexed sender, uint256 indexed orderId);

    mapping(address => BitMaps.BitMap) internal _orderInvalidated;

    /**
     * @notice Invalidates an order.
     * @dev Uses the id of the order and the address of the sender as the keys to invalidate the order. Emits an event with the address and order id invalidated.
     * @param _orderId Id of the order to be invalidated.
     */
    function invalidateOrder(uint256 _orderId) external {
        if (_orderId == 0) {
            revert InvalidateZeroOrderError();
        }

        _orderInvalidated[msg.sender].set(_orderId);
        emit OrderInvalidated(msg.sender, _orderId);
    }

    function _setOrderInvalidated(address _address, uint256 _orderId) internal {
        _orderInvalidated[_address].set(_orderId);
    }

    function orderInvalidated(address _address, uint256 _orderId) public view returns (bool) {
        return _orderInvalidated[_address].get(_orderId);
    }
}

