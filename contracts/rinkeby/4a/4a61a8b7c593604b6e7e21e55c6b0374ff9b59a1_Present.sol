pragma solidity ^0.8.10;

import {ChannelProxyStorage} from "./ChannelProxyStorage.sol";
import {ERC721AUpgradeable} from "erc721a-upgradeable/ERC721AUpgradeable.sol";
import {ChannelStorageV1} from "./ChannelStorageV1.sol";
import {IChannelSnapshot} from "../channelSnapshot/IChannelSnapshot.sol";
import {IPresent} from "../IPresent.sol";

/// @title Channel
/// @author foliose.eth <0xdead8325cfCC31618f9f3Ae19F11DA88C52FadBa>
/// @notice This contract is a live and ever-updating broadcast for subscribers to view from their wallets
contract Channel is ChannelProxyStorage, ERC721AUpgradeable, ChannelStorageV1 {
    event MintingStatusUpdated(bool enabled);
    event TokenURIUpdated(string tokenURI);
    event ChannelRemintPriceUpdated(uint256 price);
    event NewSubscriber(address indexed subscriber, uint256 tokenID);
    event Remint(address indexed subscriber, uint256 tokenID);

    /// @notice initializes the channel
    /// @param _name the channel name
    /// @param _symbol the channel token symbol
    /// @param _initialTokenURI the channel's initial metadata
    function initialize(
        string memory _name,
        string memory _symbol,
        string memory _initialTokenURI
    ) public initializerERC721A {
        __ERC721A_init(_name, _symbol);

        mintingEnabled = true;
        baseTokenURI = _initialTokenURI;
    }

    /// @notice Token URI for a given token
    /// @param _tokenID The ID of the token
    function tokenURI(uint256 _tokenID) public view override returns (string memory) {
        require(_exists(_tokenID), "Channel: URI query for nonexistent token");

        return baseTokenURI;
    }

    /// @notice enables or disables re-minting for a channel
    /// @param _enabled whether to enable or disable re-minting
    function setMintingEnabled(bool _enabled) external onlyOwner {
        mintingEnabled = _enabled;

        emit MintingStatusUpdated(_enabled);
    }

    /// @notice updates the channel broadcast
    /// @param _tokenURI the URI of the new broadcast
    function setTokenURI(string memory _tokenURI) external onlyOwner {
        baseTokenURI = _tokenURI;

        emit TokenURIUpdated(baseTokenURI);
    }

    /// @notice initializes the channel snapshot proxy for re-mints
    /// @param _snapshot the deployed snapshot proxy to use for re-mints
    function initializeChannelSnapshotProxy(address _snapshot) external {
        require(msg.sender == presentAddr, "setChannelSnapshot: only present addr");
        channelSnapshot = _snapshot;
    }

    /// @notice sets the remint price for the channel
    /// @param _price the ETH price for remints
    function setChannelRemintPrice(uint256 _price) external onlyOwner {
        remintPrice = _price;

        emit ChannelRemintPriceUpdated(_price);
    }

    /// @notice Subscribes a user to a channel
    /// @param _to The user to subscribe
    function subscribe(address _to) external {
        uint256 tokenId = totalSupply();

        _mint(_to, 1);

        emit NewSubscriber(_to, tokenId);
    }

    /// @notice Snapshots the channel and mints a frozen NFT for the user.
    /// @param _to the address to send the NFT to.
    function remint(address _to) external payable {
        require(mintingEnabled, "remint: minting must be enabled");
        require(balanceOf(msg.sender) != 0, "remint: must be a Channel holder");
        require(msg.value == remintPrice, "remint: must pay remintPrice");

        uint256 tokenId = IChannelSnapshot(channelSnapshot).mint(_to, baseTokenURI);

        emit Remint(_to, tokenId);
    }

    /// @notice withdraws fees from re-mints for the owner
    /// @param _to the address to send ETH fees to
    // TODO: fix reentrancy attack here
    function withdraw(address _to) external onlyOwner {
        uint16 feeBps = IPresent(presentAddr).getWithdrawFeeBps(address(this));
        uint256 fee = (address(this).balance * feeBps) / 10000;
        _to.call{value: address(this).balance - fee}("");
        IPresent(presentAddr).getWithdrawFeeRecipient().call{value: fee}("");
    }
}

