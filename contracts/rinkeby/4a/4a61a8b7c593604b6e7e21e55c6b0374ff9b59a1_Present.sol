pragma solidity ^0.8.10;

import {ERC721AUpgradeable} from "erc721a-upgradeable/ERC721AUpgradeable.sol";
import {ERC721} from "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import {IERC721} from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import {ChannelProxy} from "./channel/ChannelProxy.sol";
import {Channel} from "./channel/Channel.sol";
import {ChannelSnapshotProxy} from "./channelSnapshot/ChannelSnapshotProxy.sol";
import {ChannelSnapshot} from "./channelSnapshot/ChannelSnapshot.sol";
import {IPresent} from "./IPresent.sol";
import {IERC721TokenURI} from "./common/IERC721TokenURI.sol";
import {PresentStorageV1} from "./PresentStorageV1.sol";
import {PresentProxyStorage} from "./PresentProxyStorage.sol";

/// @title Present
/// @author foliose.eth <0xdead8325cfCC31618f9f3Ae19F11DA88C52FadBa>
/// @notice This contract allows anyone to create a channel, live broadcasts for subscribers.
contract Present is PresentProxyStorage, ERC721AUpgradeable, PresentStorageV1, IPresent {
    event ChannelCreated(address indexed channel, address creator, string name, string symbol);
    event Received(address sender, uint256 amount, uint256 balance);
    event MetadataUpdated(address indexed newMetadata);
    event OwnerUpdated(address indexed newOwner);

    function initialize(
        address _channelImplementation,
        address _channelSnapshotImplementation,
        address _metadata,
        uint16 _defaultWithdrawFeeBps
    ) public initializerERC721A {
        // Setup ERC721A
        __ERC721A_init("PRSNT", "PRSNT");

        channelImplementation = _channelImplementation;
        channelSnapshotImplementation = _channelSnapshotImplementation;
        metadata = _metadata;
        defaultWithdrawFeeBps = _defaultWithdrawFeeBps;
    }

    /// @notice Ensures the given upgrade path is valid and does not overwrite existing storage slots
    /// @param _newImpl The proposed implementation address
    /// @param _currentImpl The current implementation address
    function isValidChannelUpgradePath(address _newImpl, address _currentImpl) external view returns (bool) {
        return _validChannelUpdatePaths[_newImpl][_currentImpl];
    }

    /// @notice Ensures the given upgrade path is valid and does not overwrite existing storage slots
    /// @param _newImpl The proposed implementation address
    /// @param _currentImpl The current implementation address
    function isValidChannelSnapshotUpgradePath(address _newImpl, address _currentImpl) external view returns (bool) {
        return _validChannelSnapshotUpdatePaths[_newImpl][_currentImpl];
    }

    /// @notice Registers a new safe upgrade path for a channel implementation
    /// @param _newImpl The new implementation
    /// @param _supportedPrevImpls Safe implementations that can upgrade to this new implementation
    function registerNewChannelUpdate(address _newImpl, address[] calldata _supportedPrevImpls) external onlyOwner {
        for (uint256 i = 0; i < _supportedPrevImpls.length; i++) {
            _validChannelUpdatePaths[_newImpl][_supportedPrevImpls[i]] = true;
        }
    }

    /// @notice Registers a new safe upgrade path for a channel snapshot implementation
    /// @param _newImpl The new implementation
    /// @param _supportedPrevImpls Safe implementations that can upgrade to this new implementation
    function registerNewChannelSnapshotUpdate(address _newImpl, address[] calldata _supportedPrevImpls) external onlyOwner {
        for (uint256 i = 0; i < _supportedPrevImpls.length; i++) {
            _validChannelSnapshotUpdatePaths[_newImpl][_supportedPrevImpls[i]] = true;
        }
    }

    /// @notice Sets the owner for the contract
    /// @param _owner The new owner
    function setOwner(address _owner) external onlyOwner {
        owner = _owner;

        emit OwnerUpdated(owner);
    }

    /// @notice Sets the channel implementation for future channel creations
    /// @param _channelImplementation The deployed channel implementation to use
    function setChannelImplementation(address _channelImplementation) external onlyOwner {
        channelImplementation = _channelImplementation;
    }

    /// @notice Sets the channel snapshot implementation for future channel creations
    /// @param _channelSnapshotImplementation The deployed channel snapshot implementation to use
    function setChannelSnapshotImplementation(address _channelSnapshotImplementation) external onlyOwner {
        channelSnapshotImplementation = _channelSnapshotImplementation;
    }

    /// @notice Sets the default withdrawal fee for remint funds
    /// @param _bps The basis points representing the withdrawal fee
    function setDefaultWithdrawFeeBps(uint16 _bps) external onlyOwner {
        defaultWithdrawFeeBps = _bps;
    }

    /// @notice Sets the metadata for PRSNT token URIs
    /// @param _metadata the address to query for token URIs
    function setMetadata(address _metadata) external onlyOwner {
        _setMetadata(_metadata);
    }

    /// @notice Creates a new channel
    /// @param _name The name of the channel
    /// @param _symbol The token symbol of the channel
    /// @param _initialTokenURI The initial token URI for the braodcasted channel
    function createChannel(
        string memory _name,
        string memory _symbol,
        string memory _initialTokenURI
    ) public returns (address) {
        _mint(msg.sender, 1);
        uint256 tokenId = totalSupply() - 1;

        // Deploy ERC-721 and mint to msg.sender
        ChannelProxy proxy = new ChannelProxy(channelImplementation, tokenId);
        Channel channel = Channel(address(proxy));
        channel.initialize(_name, _symbol, _initialTokenURI);
        channel.subscribe(msg.sender);

        // Deploy snapshot ERC-721 and configure it for channel
        ChannelSnapshotProxy sProxy = new ChannelSnapshotProxy(channelSnapshotImplementation, address(channel), address(this), tokenId);
        ChannelSnapshot channelSnapshot = ChannelSnapshot(address(sProxy));
        channelSnapshot.initialize(string(abi.encodePacked("PAST: ", _name)), string(abi.encodePacked("PAST-", _symbol)));
        channel.initializeChannelSnapshotProxy(address(channelSnapshot));

        tokenIdToChannel[tokenId] = address(channel);

        emit ChannelCreated(address(channel), msg.sender, _name, _symbol);

        return address(channel);
    }

    /// @notice Token URI for a given token
    /// @param _tokenID The ID of the token
    function tokenURI(uint256 _tokenID) public view override returns (string memory) {
        require(_exists(_tokenID), "ERC721Metadata: URI query for nonexistent token");
        require(metadata != address(0), "ERC721Metadata: no metadata address");

        return IERC721TokenURI(metadata).tokenURI(_tokenID);
    }

    function getWithdrawFeeBps(address _channelAddr) public view returns (uint16) {
        return defaultWithdrawFeeBps;
    }

    function getWithdrawFeeRecipient() public view returns (address) {
        return owner;
    }

    function _setMetadata(address _metadata) private {
        metadata = _metadata;

        emit MetadataUpdated(_metadata);
    }

    receive() external payable {
        emit Received(msg.sender, msg.value, address(this).balance);
    }
}

