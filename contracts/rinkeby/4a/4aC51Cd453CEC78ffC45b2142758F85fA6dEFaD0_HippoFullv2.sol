// SPDX-License-Identifier: MIT
// An example of a consumer contract that relies on a subscription for funding.
pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "./IERC20.sol";
import "./SafeMath.sol";
import "hardhat/console.sol";

contract HippoFullv2 is VRFConsumerBaseV2 {
    using SafeMath for uint256;
    VRFCoordinatorV2Interface COORDINATOR;
    LinkTokenInterface LINKTOKEN;

    // Your subscription ID.
    uint64 s_subscriptionId;

    // Rinkeby coordinator. For other networks,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    address vrfCoordinator = 0x6168499c0cFfCaCD319c818142124B7A15E857ab;

    // Rinkeby LINK token contract. For other networks,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    address link = 0x01BE23585060835E02B77ef475b0Cc51aA1e0709;

    // The gas lane to use, which specifies the maximum gas price to bump to.
    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    bytes32 keyHash =
        0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 callbackGasLimit = 100000; //2M

    // The default is 3, but you can set this higher.
    uint16 requestConfirmations = 3;

    // For this example, retrieve 2 random values in one request.
    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
    uint32 numWords = 1;

    uint256[] public s_randomWords;
    uint256 public s_requestId;
    address s_owner;

    mapping(uint256 => address) public s_rollers; //make it private
    mapping(address => uint256) public s_results; //make it private

    uint256 private constant SLOT_IN_PROGRESS = 42;

    uint256 public tokenAmount = 5 * 10**18;

    struct Reward {
        uint256 percentage; // 00001 = 0.0001%
        uint256 reward; // 100 = 100x
    }

    Reward[] public rewards;

    uint256 public highestReward;

    event SlotStarted(uint256 indexed requestId, address indexed user);
    event SlotStopped(address indexed user);

    IERC20 private orareToken;

    constructor(uint64 subscriptionId, IERC20 _orareToken)
        VRFConsumerBaseV2(vrfCoordinator)
    {
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        LINKTOKEN = LinkTokenInterface(link);
        s_owner = msg.sender;
        s_subscriptionId = subscriptionId;
        orareToken = IERC20(_orareToken);
    }

    function setTokenAmount(uint256 _amount) public onlyOwner returns (bool) {
        require(_amount > 0, "Token amount cannot be 0");
        tokenAmount = _amount;
        return true;
    }

    function addReward(uint256 _percentage, uint256 _reward) public onlyOwner {
        if (rewards.length > 0) {
            if (_reward > highestReward) {
                highestReward = _reward;
            }
        } else {
            highestReward = _reward;
        }
        rewards.push(Reward(_percentage, _reward));
    }

    function removeReward(uint256 _index) public onlyOwner returns (bool) {
        delete rewards[_index];
        return true;
    }

    modifier allowPlay() {
        require(
            orareToken.balanceOf(address(this)) >=
                (tokenAmount.mul(highestReward)),
            "Pool amount is less than highest prize. Please re-fill"
        );
        _;
    }

    function play() public allowPlay returns (uint256 requestId) {
        //check allowance
        require(
            orareToken.allowance(msg.sender, address(this)) >= tokenAmount,
            "User allowance is not enough"
        );
        console.log("allowance done");
        //transfer tokenAmount
        orareToken.transferFrom(msg.sender, address(this), tokenAmount);
        console.log("transfer done");
        require(s_results[msg.sender] == 0, "Slot already started");
        console.log("slot done");
        s_requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        console.log("request done", s_requestId);
        s_rollers[s_requestId] = msg.sender;
        s_results[msg.sender] = SLOT_IN_PROGRESS;
        emit SlotStarted(s_requestId, msg.sender);

        return s_requestId;
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        s_randomWords = randomWords;
        uint256 rand = s_randomWords[0] % 1000000;
        address user = s_rollers[requestId];
        require(s_results[user] != 0, "Please start the slot machine first!");
        uint256 prize = 0;

        for (uint256 i = 0; i < rewards.length; i++) {
            if (rand < rewards[i].percentage) {
                prize = rewards[i].reward;
            }
        }

        uint256 totalPrize = tokenAmount.mul(prize);
        require(
            orareToken.balanceOf(address(this)) >= totalPrize,
            "Pool amount is less than prize. Please re-fill"
        );
        orareToken.transfer(user, totalPrize);

        s_results[user] = 0;
        emit SlotStopped(user);
    }

    modifier onlyOwner() {
        require(msg.sender == s_owner);
        _;
    }
}

