        }
        if (_operator == address(0)) {
            revert('need to set an operator');
        }
        currentContractURI = _currentContractURI;
        operator = _operator;
        _initializeEIP712(_name);
    }

    modifier onlyOwnerOrOperator() {
        require(owner() == _msgSender() || operator == _msgSender(), "caller is not the owner nor the operator");
        _;
    }

    function setOperator(address _operator) public onlyOwner {
        if (_operator != address(0)) {
            revert('need to set an operator');
        }
        operator = _operator;
    }

    function getProxyRegistryAddress() internal view returns (address) {
        if (block.chainid == 1) { 
            return 0xa5409ec958C83C3f309868babACA7c86DCB077c1;
        }
        if (block.chainid == 4) { 
            return 0xF57B2c51dED3A29e6891aba85459d600256Cf317;
        }
        revert("chain id has no default proxyRegistryAddress. Provide it on constructor.");
    }

    
    function contractURI() public view returns (string memory) {
      return currentContractURI;
    }

    function _mintAndSetUri(address _to, uint256 _tokenId, string memory _uri) internal
    {
        _mint(_to, _tokenId);
        _setTokenUri(_tokenId, _uri);
    }

    
    function mintTo(address _to, uint256 _tokenId, string memory _uri) public onlyOwnerOrOperator {
        _mintAndSetUri(_to, _tokenId, _uri);
    }

    function _mintOnAvailable(address _to, uint256 _tokenIdHint, string memory _uri) internal
    {
        uint256 tokenId = _tokenIdHint;
        while (_exists(tokenId)) tokenId++;
        _mintAndSetUri(_to, tokenId, _uri);
    }

    
    function mintTo(address _to, uint256 _tokenId, bool _isTokenIdHint, string memory _uri) public onlyOwnerOrOperator {
        if (!_isTokenIdHint) {
            mintTo(_to, _tokenId, _uri);
        } else {
            _mintOnAvailable(_to, _tokenId, _uri);
        }
    }

    
    function mintTo(address _to, string memory _uri) public onlyOwnerOrOperator {
        _mintOnAvailable(_to, 0, _uri);
    }

    
    function isApprovedForAll(address owner, address operator)
        override
        public
        view
        returns (bool)
    {
        
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (address(proxyRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }
    
    
    function tokenURI(
        uint256 _tokenId
    )
        public
        override
        view
        returns (string memory)
    {
        return _tokenURI(_tokenId);
    }

    
    function _tokenURI(
        uint256 _tokenId
    )
        internal
        virtual
        view
        returns (string memory)
    {
        return idToUri[_tokenId];
    }
    
    
    function _setTokenUri(
        uint256 _tokenId,
        string memory _uri
    )
        internal
    {
        idToUri[_tokenId] = _uri;
    }

    
    function unMint(uint256 _tokenId) public onlyOwnerOrOperator {
        super._unMint(_tokenId);
    }
}

contract AriaOpenSeaNFT is BaseInfiniteOpenSeaNFT {
    using SafeMath for uint256;
    constructor(
        string memory _name,
        string memory _symbol,
        address _operator,
        string memory _currentContractURI,
        address _proxyRegistryAddress
    ) BaseInfiniteOpenSeaNFT(_name, _symbol, _operator, _currentContractURI, _proxyRegistryAddress) {}
}