pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

interface IERC721 {
    function balanceOf(address owner) external view returns (uint256 balance);
}

contract COKTreasury is Ownable {
    struct Competition {
        uint256 id;
        uint256 rewardAmount;
        uint256 totalWeight;
        bytes32 merkleRoot;
        bool activeStatus;
        mapping(uint256 => uint256) claimedBitMap;
    }

    struct WeightContract {
        address contractAddr;
        uint256 weight;
        string name;
    }

    WeightContract[] public weightInfo;
    mapping(uint256 => Competition) public taskList;
    uint256[] public taskIDList;
    uint256 public currentID;
    
    mapping(address => bool) public isAdmin;

    // @ Events
    event Received(address, uint256);
    event Claim(address, uint256, uint256);
    event CompentitionStart(uint256);

    constructor() {
        isAdmin[_msgSender()] = true;
    }

    modifier onlyAdmin() {
        require(isAdmin[_msgSender()] , "Ownable: caller is not the owner");
        _;
    }

    function addAdmin(address admin) external onlyOwner{
        isAdmin[admin] = true;
    }

    function removeAdmin(address admin) external onlyOwner{
        isAdmin[admin] = false;
    }

    // Receive ETH
    receive() external payable {
        emit Received(_msgSender(), msg.value);
    }

    // Add NFT Contract & weights
    function addNFTContract(
        address _addr,
        uint256 _weight,
        string memory _name
    ) external onlyOwner {
        weightInfo.push(WeightContract(_addr, _weight, _name));
    }

    // Update NFT Contract Weights using Name
    function updateNFTWeight(address _addr, uint256 _weight)
        external
        onlyOwner
    {
        uint256 index = getContractIndexByAddress(_addr);
        require(index > 0, "Contract Name Not Found");
        WeightContract storage nftInfo = weightInfo[index - 1];
        nftInfo.weight = _weight;
    }

    // Get Contract Index using Address
    function getContractIndexByAddress(address _addr) public view returns (uint256) {
        for (uint256 i = 0; i < weightInfo.length; i++) {
            if (weightInfo[i].contractAddr == _addr) {
                return i + 1;
            }
        }
        return 0;
    }

    // Add Compentition by Owner
    function startCompetition() public onlyAdmin {
        uint256 currentBalance = address(this).balance;

        for (uint256 i = 0; i < taskIDList.length; i++) {
            require(
                currentBalance > taskList[taskIDList[i]].rewardAmount,
                "Not enough reward in the pool"
            );
            currentBalance -= taskList[i].rewardAmount;
        }
        
        Competition storage task = taskList[currentID];
        task.rewardAmount = currentBalance;
        task.totalWeight = 0;
        task.merkleRoot = "";
        task.activeStatus = true;

        taskIDList.push(currentID);
        emit CompentitionStart(currentID);
        
        currentID += 1;
    }

    function endCompetition(
        uint256 _id,
        uint256 _weight,
        bytes32 _root
    ) public onlyAdmin {
        require(_weight > 0, "Weight should be bigger than zero");

        Competition storage task = taskList[_id];
        task.merkleRoot = _root;
        task.totalWeight = _weight;
        task.activeStatus = false;

        removeTaskIDList(_id);
    }

    function removeTaskIDList(uint256 _taskID) internal {
        for (uint i = 0 ; i < taskIDList.length ; i ++) {
            if(taskIDList[i] == _taskID) {
                delete taskIDList[i];
                return;
            }
        }
    }

    function getWeightInfo(address[] memory _users)
        external
        view
        returns (uint256[] memory)
    {
        uint256 count = _users.length;
        uint256[] memory _weights = new uint256[](count);
        for (uint256 i = 0; i < count; i++) {
            _weights[i] = getWeightByAddress(_users[i]);
        }

        return (_weights);
    }

    // Get weight by user address
    function getWeightByAddress(address _address)
        public
        view
        returns (uint256)
    {
        uint256 _weight = 0;
        for (uint256 i = 0; i < weightInfo.length; i++) {
            uint256 balance = IERC721(weightInfo[i].contractAddr).balanceOf(
                _address
            );
            _weight += balance * weightInfo[i].weight;
        }
        return _weight;
    }

    // Process Payout from the owner address
    function payout(
        address[] memory _users,
        uint256[] memory _indexs,
        uint256[] memory _weights,
        uint256 _taskID
    ) public onlyAdmin {
        uint256 count = _users.length;
        
        require(
            taskList[_taskID].activeStatus == false,
            "Competition was not ended yet"
        );

        uint256 distributeBalance = taskList[_taskID].rewardAmount;
        uint256 _totalWeight = taskList[_taskID].totalWeight;
        uint256 claimedAmount = 0;
        uint256 claimedWeight = 0;

        require(distributeBalance > 0, "Already claimed all");
        require(weightInfo.length > 0, "Not added any contract yet");

        uint256 weightPoint = distributeBalance / _totalWeight;

        for (uint256 i = 0; i < count; i++) {
            if(!isClaimed(_taskID, _indexs[i])) {
                uint256 payBalance = weightPoint * _weights[i];

                (bool sent, bytes memory data) = payable(_users[i]).call{
                    value: payBalance
                }("");

                claimedWeight += _weights[i];
                claimedAmount += payBalance;

                emit Claim(_users[i], payBalance, _taskID);
                _setClaimed(_taskID, _indexs[i]);

                require(sent, "Failed to send Ether");
            }
        }
        taskList[_taskID].rewardAmount -= claimedAmount;
        taskList[_taskID].totalWeight -= claimedWeight;
    }

    function claimPayout(
        uint256 _taskID,
        uint256 _index,
        uint256 _weight,
        bytes32[] calldata merkleProof
    ) external {
        require(!isClaimed(_taskID, _index), "User already claimed.");

        Competition storage task = taskList[_taskID];

        bytes32 node = keccak256(
            abi.encodePacked(_index, _msgSender(), _weight)
        );

        require(
            MerkleProof.verify(merkleProof, task.merkleRoot, node),
            "Invalid proof."
        );

        uint256 reward = (task.rewardAmount * _weight) / task.totalWeight;

        (bool sent, bytes memory data) = payable(_msgSender()).call{ value: reward }("");
        require(sent, "Failed to claim Ether");

        taskList[_taskID].rewardAmount -= reward;
        taskList[_taskID].totalWeight -= _weight;

        _setClaimed(_taskID, _index);

        emit Claim(_msgSender(), reward, _taskID);
    }

    function isClaimed(uint256 taskID, uint256 index)
        public
        view
        returns (bool)
    {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        uint256 claimedWord = taskList[taskID].claimedBitMap[claimedWordIndex];
        uint256 mask = (1 << claimedBitIndex);
        return claimedWord & mask == mask;
    }

    function _setClaimed(uint256 taskID, uint256 index) private {
        uint256 claimedWordIndex = index / 256;
        uint256 claimedBitIndex = index % 256;
        Competition storage task = taskList[taskID];
        task.claimedBitMap[claimedWordIndex] =
            task.claimedBitMap[claimedWordIndex] |
            (1 << claimedBitIndex);
    }
}

