pragma solidity ^0.8.6;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";
import {PegaERC20Token} from "../PegaERC20.sol";
import {IProxyRegistry} from "../opensea/IProxyRegistry.sol";

contract PegaPal is ERC721, Ownable {
  using SafeMath for uint256;
  using Counters for Counters.Counter;
  using Strings for uint256;

  event MinterUpdated(address minter);

  // pegapal created
  event PegaPalCreated(uint256 indexed tokenId);

  //if pegapal doesnt sell it's burrned
  event PegaPalBurned(uint256 indexed tokenId);

  Counters.Counter private _tokenIds;
  string public baseuri;
  mapping(uint256 => uint256) public tokenIdToLevel;
  mapping(uint256 => string) private _tokenURIs;
  mapping(uint256 => string) private _levelToURI;

  // cost to mint using task
  uint256 public _mintingPrice;
  //cost to upgrade one nft a level
  uint256 public _upgradePrice;
  address public tokenAddress;
  uint256 public tokenValue;
  address public mintingTokenAddress;

  IProxyRegistry public immutable proxyRegistry;

  // from nouns dao
  address public minter;
  bool public isMinterLocked;
  uint256 private _currentPegaId;

  event NFTMinted(uint256 tokenId, address owner);
  event LevelUp(uint256 tokenID, address owner);

  /**
   * @notice Require that the minter has not been locked.
   */
  modifier whenMinterNotLocked() {
    require(!isMinterLocked, "Minter is locked");
    _;
  }

  /**
   * @notice Require that the sender is the minter.
   */
  modifier onlyMinter() {
    require(msg.sender == minter, "Sender is not the minter");
    _;
  }

  constructor(
    address _contractAddress,
    address _minter,
    IProxyRegistry _proxyRegistry
  ) ERC721("PegaPal", "PEGA") {
    baseuri = "https://gateway.pinata.cloud/ipfs/";
    tokenAddress = _contractAddress;
    tokenValue = 0;
    _mintingPrice = 1;
    _upgradePrice = 500 * 10**9;
    _levelToURI[1] = "QmRapPMV7MNBWeVS6Z2TQpjH1NSuG31fu1DRJAUErf4DYB/";
    _levelToURI[2] = "QmdXTZok251HgEkQh2yAExeUuW83uMRJEDA98sZLAWDqjH/";
    proxyRegistry = _proxyRegistry;

    minter = _minter;
    for (uint256 i = 1; i < 4; i++) {
      _levelToURI[i] = string(abi.encodePacked("level", i.toString(), "/"));
    }
  }

  /**
   * @notice Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-less listings.
   */
  function isApprovedForAll(address owner, address operator)
    public
    view
    override
    returns (bool)
  {
    // Whitelist OpenSea proxy contract for easy trading.
    if (proxyRegistry.proxies(owner) == operator) {
      return true;
    }
    return super.isApprovedForAll(owner, operator);
  }

  function baseURI() public view returns (string memory) {
    return baseuri;
  }

  function setBaseURI(string memory newBaseURI) public onlyOwner {
    baseuri = newBaseURI;
  }

  function _setTokenURI(uint256 tokenId, string memory _tokenURI) internal {
    require(_exists(tokenId), "ERC721URIStorage: URI set of nonexistent token");
    _tokenURIs[tokenId] = _tokenURI;
  }

  /**
   * @notice Burn a noun.
   */
  function burn(uint256 pegasusId) public onlyMinter {
    _burn(pegasusId);
    emit PegaPalBurned(pegasusId);
  }

  /**
   * @notice Set the token minter.
   * @dev Only callable by the owner when not locked.
   */
  function setMinter(address _minter) external onlyOwner whenMinterNotLocked {
    minter = _minter;

    emit MinterUpdated(_minter);
  }

  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(
      _exists(tokenId),
      "ERC721URIStorage: URI query for nonexistent token"
    );

    string memory _tokenURI = _tokenURIs[tokenId];
    string memory base = baseURI();
    string memory _levelURI = _levelToURI[tokenIdToLevel[tokenId]];

    // If there is no base URI, return the token URI.
    if (bytes(base).length == 0) {
      return _tokenURI;
    }
    // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
    if (bytes(_tokenURI).length > 0) {
      return string(abi.encodePacked(base, _levelURI, _tokenURI));
    }
    return super.tokenURI(tokenId);
  }

  function setLevelURI(uint256 level, string memory newLevelURI)
    public
    onlyOwner
    returns (uint256)
  {
    _levelToURI[level] = newLevelURI;
    return level;
  }

  function levelURI(uint256 level) public view returns (string memory) {
    return _levelToURI[level];
  }

  function _mintNFThelper(address recipient) internal returns (uint256) {
    uint256 newItemId = _tokenIds.current();
    _mint(recipient, newItemId);
    _currentPegaId++;
    _tokenIds.increment();
    string memory newTokenURI = string(
      abi.encodePacked("pegapal", newItemId.toString(), ".json")
    );

    tokenValue = tokenValue.add(500 * (10**9));
    tokenIdToLevel[newItemId] = 1;
    _setTokenURI(newItemId, newTokenURI);
    emit PegaPalCreated(newItemId);
    return newItemId;
  }

  function mintNFT(address recipient) public returns (uint256) {
    require(
      PegaERC20Token(mintingTokenAddress).burnFrom(_msgSender(), _mintingPrice)
    );
    return _mintNFThelper(recipient);
  }

  function safeMint(address recipient) public onlyMinter returns (uint256) {
    return _mintNFThelper(recipient);
  }

  function setMintingPrice(uint256 newMintPrice) public onlyOwner {
    _mintingPrice = newMintPrice;
  }

  function setUpgradePrice(uint256 newUpgradePrice) public onlyOwner {
    _upgradePrice = newUpgradePrice;
  }

  function levelUp(uint256 tokenId) public {
    require(
      _isApprovedOrOwner(_msgSender(), tokenId),
      "ERC721: transfer caller is not owner nor approved"
    );
    require(PegaERC20Token(tokenAddress).burnFrom(_msgSender(), _upgradePrice));

    tokenIdToLevel[tokenId] = tokenIdToLevel[tokenId].add(1);
    tokenValue = tokenValue.add(500 * (10**9));
    emit LevelUp(tokenId, msg.sender);
  }

  function getLevel(uint256 tokenId) public view returns (uint256) {
    return tokenIdToLevel[tokenId];
  }
}

