pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "hardhat/console.sol";
import "./interfaces/IDToken.sol";
import "./interfaces/IDao.sol";
import "./interfaces/IOracle.sol";

contract Dao is Initializable, OwnableUpgradeable, IDao {
    using SafeMathUpgradeable for uint256;

    uint256 public constant BASE_DENOMINATOR = 100;
    uint256 public constant INTEREST_FACTOR = 60; //60% minted token transfer to lender
    uint256 public constant baseDTokenMintRate = 110; //Base rate to mint DToken (11 CDT per 0.1 USD => 110 CDT per 1 USD)
    uint256 public constant liquidityToAdded = 1_000_000_000_000_000_000; //Initial liquidity for DAO

    IDToken public dToken;
    address public lendingPool;
    mapping(address => uint256) pendingDToken;
    mapping(address => IOracle) tokenOracle;
    bool isLiquidityAdded;

    /**
     * @notice Function initializer take 3 parameters {_dToken}, {_lendingPool} and {_ethOracle}
     * The first 2 params represent the 2 address of the DToken contract and LendingPool contract
     * The last param take the oracle address of ETH
     */
    function initialize(address _dToken, address _ethOracle) public initializer {
        __Ownable_init_unchained();
        isLiquidityAdded = false;
        dToken = IDToken(_dToken);
        tokenOracle[address(0)] = IOracle(_ethOracle);

    }

    function setLendingPool(address _lendingPool) public onlyOwner {
        lendingPool = _lendingPool;
    }

    //Fallback function to receive ethers from lending pool
    fallback() external payable {
        if(msg.sender != lendingPool) {
            (bool sent, ) = msg.sender.call{value: msg.value}(""); // send ETH back to user if sender isn't lending pool
            require(sent, "Coindeed: Transfer failed");
        }
    }

    function addOracleForToken(address _token, address _tokenOracle) public override onlyOwner {
        tokenOracle[_token] = IOracle(_tokenOracle);
    }

    /**
     * @notice This function is temporary and could be removed in the later version. (We can use DToken contract to send token directly to this contract instead of using this function)
     * @dev This function create the liquidity for this contract with the amount of 1,000,000,000 DToken
     * Only owner of this contract can execute this function and it could only be execute once
     * DToken contract need to provide MINTER ROLE for this contract in order to execute this function
     */
    function createDTokenLiquidity() public override onlyOwner {
        require(!isLiquidityAdded, "Coindeed: Liquidity already added");
        dToken.mint(address(this), liquidityToAdded.mul(1e18)); //Provide the liquidity for DAO contract
        isLiquidityAdded = true;
    }

    modifier onlyLendingPool () {
        require(msg.sender == lendingPool, "Coindeed: Only lending pool can call this function");
        _;
    }

    function getPendingDToken(address _userAddress) external override view returns(uint256) {
        return pendingDToken[_userAddress];
    }

    /**
     * @notice This function will receive the amount of token that lending pool transfer to
     * @dev It will convert the token to USDT and add it to user account to claim
     * This function take 2 parameters {_to} and {_amount}
     * The default value of amount should be in wei (1e18)
     */
    function receiveRewardTokenFromPool(address _userAddress, address _tokenAddress, uint256 _amount) external override onlyLendingPool {
        _receiveRewardTokenFromPool(_userAddress, _tokenAddress, _amount);
    }

    // Function to claim DToken reward
    function claimDToken(address _to) external override onlyLendingPool {
         _claimDToken(_to);
    }

    function _receiveRewardTokenFromPool(address _userAddress, address _tokenAddress, uint256 _amount) private {
        require(_amount > 0, "Coindeed: amount must be greater than zero");
        uint256 amountUSDTconverted = _exchangeRewardToken(_tokenAddress, _amount); 
        uint256 amountDTokenPending = baseDTokenMintRate.mul(amountUSDTconverted.mul(INTEREST_FACTOR).div(BASE_DENOMINATOR));      
        pendingDToken[_userAddress] = pendingDToken[_userAddress].add(amountDTokenPending);
    }

    function _claimDToken(address _to) private {
        require(pendingDToken[_to] > 0, "Coindeed: You don't have any reward");
        uint256 amountToClaim = pendingDToken[_to];
        dToken.transfer(_to, amountToClaim);
        emit Transfer(_to, amountToClaim);
    }

    // This function is used to exchange token price to USD
    function _exchangeRewardToken(address _tokenAddress, uint256 _amount) private returns(uint256) {
        uint256 tokenPrice = uint256(tokenOracle[_tokenAddress].latestAnswer());
        uint256 rewardInUSDT = _amount.mul(tokenPrice);

        return rewardInUSDT;
    }

    function getBalance() public view returns (uint) {
        return address(this).balance;
    }
}

