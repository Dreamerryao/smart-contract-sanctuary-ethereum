    function getPunkCID(uint256 punkId) public view returns(string memory) {
        require(!_isStrEmpty(_punkCID[punkId]), "This punkId does not exist.");
        return _punkCID[punkId];
    }

    // Buy a punk in an auction.
    function buy() public payable onlyInAuction {
        require(msg.value == getPunkPrice(), "The value is incorrect!");
        uint nextIndex = getNextIndex();
        mint(_msgSender(), nextIndex);
    }

    // Get the price of the next punk
    function getPunkPrice() public view returns(uint256) {
        uint256 nextIndex = getNextIndex();
        return _getPunkPrice(nextIndex);
    }

    function _getPunkPrice(uint256 punkIndex) internal pure returns(uint256) {
        uint256 punkPrice;
        if (punkIndex < 2) {
            punkPrice = 100000000000000000;
        } else
        if (punkIndex < 4) {
            punkPrice = 200000000000000000;
        } else
        if (punkIndex < 6) {
            punkPrice = 300000000000000000;
        } else
        if (punkIndex < 8) {
            punkPrice = 400000000000000000;
        } else
        if (punkIndex < 10) {
            punkPrice = 500000000000000000;
        }
        return punkPrice;
    }

    function mint(address to, uint256 punkId) public onlyInAuction {
        _mint(to, punkId);
    }

    /**
     * @dev Safely mints `tokenId` and transfers it to `to`.
     *
     * Requirements:
     *
     * - `tokenId` must not exist.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
     */
    function _safeMint(address to, uint256 punkId) internal virtual {
        _safeMint(to, punkId, "");
    }

    /**
     * @dev Same as {xref-ERC721-_safeMint-address-uint256-}[`_safeMint`], with an additional `data` parameter which is
     * forwarded in {IERC721Receiver-onERC721Received} to contract recipients.
     */
    function _safeMint(address to, uint256 punkId, bytes memory _data) internal virtual {
        _mint(to, punkId);
        require(_checkOnERC721Received(address(0), to, punkId, _data), "ERC721: transfer to non ERC721Receiver implementer");
    }

    /**
     * @dev Mints `tokenId` and transfers it to `to`.
     *
     * WARNING: Usage of this method is discouraged, use {_safeMint} whenever possible
     *
     * Requirements:
     *
     * - `tokenId` must not exist.
     * - `to` cannot be the zero address.
     *
     * Emits a {Transfer} event.
     */
    function _mint(address to, uint256 punkId) internal {
        require(to != address(0), "ERC721: mint to the zero address");
        require(!_exists(punkId), "ERC721: token already minted");

        _beforeTokenTransfer(address(0), to, punkId);

        _balances[to] += 1;  // owner balance
        _ownerToPunks[to].add(punkId);  // address to id
        _punkToOwners.set(punkId, to);  // id to address

        Punk memory thisPunk = Punk(punkId, 0, 0, false, address(0));

        _punkIdDetails[punkId] = thisPunk;

        _updateMintedBeforeReveal(punkId);

        _incrementIndex();

        emit Transfer(address(0), to, punkId);
    }

    function _exists(uint256 punkId) internal view returns (bool) {
        return _punkToOwners.contains(punkId);
    }

    // This should be inherited.
    // TODO: once a transfer of a punk is triggered, the information for 
    // this punk is reset.
    function transfer(address to, uint256 punkId) public returns(bool) {
        _transfer(_msgSender(), to, punkId);
        
        return true;
    }

    function transferFrom(address from, address to, uint256 punkId) public override {
        require(_isApprovedOrOwner(_msgSender(), punkId), "ERC721: transfer caller is not owner nor approved");

        _transfer(from, to, punkId);

    }

    function _isApprovedOrOwner(address spender, uint256 tokenId) internal view virtual returns (bool) {
        require(_exists(tokenId), "ERC721: operator query for nonexistent token");
        address owner = ownerOf(tokenId);
        return (spender == owner || getApproved(tokenId) == spender || isApprovedForAll(owner, spender));
    }

    function _transfer(address from, address to, uint256 punkId) internal {
        require(from != address(0), "ERC721: transfer from the zero address");
        require(to != address(0), "ERC721: transfer to the zero address");

        _beforeTokenTransfer(from, to, punkId);

        // Clear approvals from the previous owner
        _approve(address(0), punkId);

        _ownerToPunks[from].remove(punkId);
        _ownerToPunks[to].add(punkId);
        _punkToOwners.set(punkId, to);

        _balances[from]--;
        _balances[to]++;

        Punk memory thisPunk = _punkIdDetails[punkId];

        // If the punk is not sent to the current bidder
        // for any reason, the _maxBidPrice would be
        // available for refund.
        if(to != thisPunk._bidder) {
            _pendingWithdrawals[thisPunk._bidder] += thisPunk._maxBidPrice;
        }

        _punkIdDetails[punkId] = _reset(thisPunk);

        emit Transfer(from, to, punkId);
    }

    function _safeTransfer(address from, address to, uint256 punkId, bytes memory _data) internal virtual {
        _transfer(from, to, punkId);
        require(_checkOnERC721Received(from, to, punkId, _data), "ERC721: transfer to non ERC721Receiver implementer");
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(address from, address to, uint256 punkId) public virtual override {
        safeTransferFrom(from, to, punkId, "");
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(address from, address to, uint256 punkId, bytes memory _data) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), punkId), "ERC721: transfer caller is not owner nor approved");
        _safeTransfer(from, to, punkId, _data);
    }

    function _checkOnERC721Received(address from, address to, uint256 punkId, bytes memory _data)
        private returns (bool) {
        if (to.isContract()) {
            try IERC721Receiver(to).onERC721Received(_msgSender(), from, punkId, _data) returns (bytes4 retval) {
                return retval == IERC721Receiver(to).onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert("ERC721: transfer to non ERC721Receiver implementer");
                } else {
                    // solhint-disable-next-line no-inline-assembly
                    assembly {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } else {
            return true;
        }
    }

    /**
     * @dev Approve `to` to operate on `tokenId`
     *
     * Emits a {Approval} event.
     */
    function _approve(address to, uint256 punkId) internal virtual {
        _punkApprovals[punkId] = to;
        emit Approval(ownerOf(punkId), to, punkId);
    }

    // reset a punk to default values except punkId
    function _reset(Punk memory punk) internal pure returns(Punk memory) {
        return Punk(punk._punkId, 0, 0, false, address(0));
    }

    function getPunkInfo(uint256 punkId) public view returns(Punk memory) {
        return _punkIdDetails[punkId];
    }

    // ask for a price by the owner
    // A punk can only be asked once in this function.
    function ask(uint256 punkId, uint256 askPrice) public returns(bool) {
        require(_msgSender() == ownerOf(punkId), "Not the owner of this punk.");
        Punk memory thisPunk = _punkIdDetails[punkId];
        require(!thisPunk._isOnSale, "This punk is already on sale.");

        if (askPrice <= thisPunk._maxBidPrice) {
            _transfer(_msgSender(), thisPunk._bidder, punkId);

            // update balances
            _pendingWithdrawals[_msgSender()] += thisPunk._maxBidPrice;

        } else {
            thisPunk._offerPrice = askPrice;
            thisPunk._isOnSale = true;
            _punkIdDetails[punkId] = thisPunk;
        }


        return true;
    }

    function toggleOnSale(uint256 punkId) public returns (bool) {
        if (_punkIdDetails[punkId]._isOnSale) {
            _punkIdDetails[punkId]._isOnSale = false;
        } else {
            _punkIdDetails[punkId]._isOnSale = true;
        }
        return _punkIdDetails[punkId]._isOnSale;
    }

    // Set a bid price for a punk
    // This is not an ERC20 logic. This is an ETH logic.
    // msg.value should be used instead of uint256.
    function bid(uint256 punkId) public payable {
        require(_msgSender() != ownerOf(punkId), "Already the owner of this punk.");
        require(msg.value > _punkIdDetails[punkId]._maxBidPrice, "Bid price is not enough.");
        require(_msgSender() != _punkIdDetails[punkId]._bidder, "Current bidder call inincreaseBid.");

        Punk memory thisPunk = _punkIdDetails[punkId];

        if (msg.value >= thisPunk._offerPrice && thisPunk._isOnSale) {
            address punkOwner = _punkToOwners.get(punkId);
            
            // transfer ownership
            _transfer(punkOwner, _msgSender(), punkId);
            
            // Update balances
            _pendingWithdrawals[punkOwner] += msg.value;
            
        } else {
            // Update withdrawal amount for the existing bidder first.
            if (thisPunk._maxBidPrice != 0) {
                _pendingWithdrawals[thisPunk._bidder] += thisPunk._maxBidPrice;
            }

            // Update bidding information for the new bid.
            thisPunk._maxBidPrice = msg.value;
            thisPunk._bidder = _msgSender();
            _punkIdDetails[punkId] = thisPunk;
        }
    }


    function increaseBid(uint256 punkId) public payable returns(bool) {
        require(_punkIdDetails[punkId]._bidder == _msgSender(), "Only current bidder can increase bid.");
        
        Punk memory thisPunk = _punkIdDetails[punkId];
        
        if (msg.value + thisPunk._maxBidPrice >= thisPunk._offerPrice) {
            address punkOwner = _punkToOwners.get(punkId);
            _transfer(punkOwner, _msgSender(), punkId);
            
            thisPunk = _reset(thisPunk);
            
            _pendingWithdrawals[punkOwner] += msg.value;
        } else {
            thisPunk._maxBidPrice += msg.value;
        }
        
        _punkIdDetails[punkId] = thisPunk;
        
        return true;
    }

    function withdraw() public payable returns(bool) {
        require(_pendingWithdrawals[_msgSender()] > 0, "No positive balance to withdraw.");
        (bool success, ) = payable(_msgSender()).call{value: _pendingWithdrawals[_msgSender()]}(" ");
        require(success, "withdraw is not successful.");
        return success;
    }
    
    // This is not redundant since, owner can bid/ask punks as well.
    function ownerWithdraw() public payable onlyOwner returns(bool) {
        (bool success, ) = payable(_msgSender()).call{value: address(this).balance}(" ");
        require(success, "withdraw is not successful.");
        return success;
    }

    function getNextIndex() public view returns(uint256) {
        return _punkIndex;
    }

    function _incrementIndex() internal {
        _punkIndex++;
    }

    /**
     * @dev Returns if the NFT has been minted before reveal phase
     */
    function isMintedBeforeReveal(uint256 index) public view override returns (bool) {
        return _mintedBeforeReveal[index];
    }

    function _updateMintedBeforeReveal(uint256 index) internal {
        _mintedBeforeReveal[index] = true;
    }

    function _beforeTokenTransfer(address from, address to, uint256 punkId) internal virtual {
        require(from != to, "ERC721: transfer to the sender address");
        require(punkId < _totalSupply, "ERC721: punkId is beyond the limit.");
        
    }
    
    function beforeDestroy() public onlyOwner {
        _selfDestructUnlock = true;
    }
    
    function destroySmartContract() public onlyOwner {
        require(_selfDestructUnlock, "Locked for self destruct.");
        selfdestruct(payable(owner()));
    }
    
}