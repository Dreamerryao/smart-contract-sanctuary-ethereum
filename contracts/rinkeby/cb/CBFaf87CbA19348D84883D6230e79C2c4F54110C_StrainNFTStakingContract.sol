pragma solidity 0.8.4;

import '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import './interface/IStrainFactory.sol';
import 'hardhat/console.sol';

contract StrainNFTStakingContract is ERC721Holder {
    address public constant UTILITY_DISTRIBUTION_ADDRESS = 0x1d2582DB948C201b10A0E75BcB1e09B8Be4E9770;
    uint256 public constant UPGRADE_STAGE_LIMIT = 14;

    using SafeERC20 for IERC20;

    struct StakeInfo {
        address owner;
        uint256 stakeDate;
        uint256 lastWateredTime;
        uint256 lastFertilisedTime;
        uint64 waterCount;
        uint64 fertiliserCount;
        uint8 stage;
    }

    uint256[4] public fertBonusPerInteraction = [
        1428571429,
        2142857143,
        3571428571,
        0
    ];

    mapping(address => uint256[]) stakeStrainIds;

    // Strain NFT token to stake
    IStrainFactory strainNftToken;

    // Strain ERC-20 token address
    IERC20 strnToken;
    IERC20 wtrToken;
    IERC20 frtToken;

    //  The rewards will be transferred from this address
    address rewarderAddr;

    //  It's used to store all stake history
    mapping(uint256 => StakeInfo) public stakeHistory;

    //  Reward History
    mapping(address => uint256) public rewardHistory;
    
    //  Events
    event Staked(
        address staker,
        uint256 _tokenId
    );

    event Unstakeed(
        address unstaker,
        uint256 _tokenId
    );

    event EmergencyUnstakeed(
        address unstaker,
        uint256 _tokenId
    );

    event StrainUpdated(
        address owner,
        uint256 strainId
    );

    /**
     * @dev constructor
     * @param _nftTokenAddr the address of token to stake
     * @param _rewarder the address of rewarder
     */
    constructor(
        address _nftTokenAddr,
        address _strnTokenAddr,
        address _wtrTokenAddr,
        address _frtTokenAddr,
        address _rewarder
    ) {
        strainNftToken = IStrainFactory(_nftTokenAddr);
        strnToken = IERC20(_strnTokenAddr);
        wtrToken = IERC20(_wtrTokenAddr);
        frtToken = IERC20(_frtTokenAddr);
        rewarderAddr = _rewarder;
    }

    modifier isOwner(uint256 _tokenId, address _owner) {
        StakeInfo storage stakeInfo = stakeHistory[_tokenId];
        require(stakeInfo.owner == _owner, "Not Strain Owner");
        _;
    }

    /**
     * @dev the function to stake strain NFT token
     * @param _tokenId the strain NFT token Id to unstake
     */
    function stake(uint256 _tokenId, uint256 _stage) public {
        require(strainNftToken.ownerOf(_tokenId) == msg.sender, "Not Strain Owner");
        require(!isStaked(_tokenId, msg.sender), "Already Staked");

        stakeStrainIds[msg.sender].push(_tokenId);
        stakeHistory[_tokenId] = StakeInfo({
            owner: msg.sender,
            stakeDate: block.timestamp,
            lastWateredTime: block.timestamp - 1 days,
            lastFertilisedTime: block.timestamp - 1 days,
            waterCount: 0,
            fertiliserCount: 0,
            stage: uint8(_stage)
        });

        //  Stake token
        strainNftToken.safeTransferFrom(msg.sender, address(this), _tokenId);
        emit Staked(msg.sender, _tokenId);
    }

    function isStaked(uint256 _tokenId, address _owner) public view returns (bool) {
        uint256[] memory stakedIds = stakeStrainIds[_owner];
        for (uint i = 0; i < stakedIds.length; i++) {
            if (stakedIds[i] == _tokenId) {
                return true;
            }
        }
        return false;
    }

    /**
     * @dev the function to unstake the staked strain NFT token in the history
     * @param _tokenId the strain NFT token Id to unstake
     */
    function unstake(uint256 _tokenId) public {
        removeStakedStrainId(_tokenId, msg.sender);
        // Transfer token to the staker and update its balance
        strainNftToken.transferFrom(address(this), msg.sender, _tokenId);
        emit Unstakeed(msg.sender, _tokenId);
    }

    /**
     * @dev the function to unstake the staked token without reward in the history
     * @param _tokenId the strain NFT token Id to unstake
     */
    function emergencyUnstake(uint256 _tokenId) public {
        removeStakedStrainId(_tokenId, msg.sender);
        // Transfer token to the staker and update its balance
        strainNftToken.transferFrom(address(this), msg.sender, _tokenId);
        emit EmergencyUnstakeed(msg.sender, _tokenId);
    }

    function removeStakedStrainId(uint256 _tokenId, address _owner)
        private
        isOwner(_tokenId, _owner)
    {
        StakeInfo storage stakeInfo = stakeHistory[_tokenId];
        require(stakeInfo.owner == _owner, "Not Strain Owner");

        delete stakeHistory[_tokenId];

        uint256[] storage stakedIds = stakeStrainIds[_owner];
        for (uint256 i; i<stakedIds.length; i++) {
            if (stakedIds[i] == _tokenId) {
                stakedIds[i] = stakedIds[stakedIds.length - 1];
                stakedIds.pop();
                break;
            }
        }
    }

    /**
     * @dev the function to claim reward
     */
    function claimReward() public {
        uint256 reward = rewardHistory[msg.sender];

        require(reward > 0, "No Reward");
        require(strnToken.balanceOf(rewarderAddr) >= reward, "Insufficient balance in rewarder address");

        // Transfer the reward to the staker
        rewardHistory[msg.sender] = 0;
        strnToken.safeTransferFrom(rewarderAddr, msg.sender, reward);
    }

    function isStageUpgradable (
        uint256 _strainId
    )   public
        view
        returns (bool)
    {
        StakeInfo memory stakeInfo = stakeHistory[_strainId];
        return stakeInfo.waterCount >= UPGRADE_STAGE_LIMIT;
    }

    function watered(
        uint256 _strainId
    ) 
        public
        isOwner(_strainId, msg.sender)
    {
        StakeInfo storage stakeInfo = stakeHistory[_strainId];

        require(stakeInfo.lastWateredTime + uint64(1 days) <= block.timestamp, "Strain Watering on cooldown");
        require(block.timestamp - stakeInfo.lastWateredTime < 14 days, "Staking Completed");

        stakeInfo.lastWateredTime = uint64(block.timestamp);
        stakeInfo.waterCount += 1;

        wtrToken.safeTransferFrom(msg.sender, UTILITY_DISTRIBUTION_ADDRESS, 1);
        emit StrainUpdated(msg.sender, _strainId);
    }

    function fertilized(
        uint256 _strainId
    ) 
        public
        isOwner(_strainId, msg.sender)
    {
        StakeInfo storage stakeInfo = stakeHistory[_strainId];

        require(stakeInfo.lastFertilisedTime + uint64(1 days) <= block.timestamp, "Strain Fertilizing on cooldown");
        require(block.timestamp - stakeInfo.lastFertilisedTime < 14 days, "Staking Completed");
        require(stakeInfo.fertiliserCount <= UPGRADE_STAGE_LIMIT, "Useless Fertilizing");

        stakeInfo.lastFertilisedTime = uint64(block.timestamp);
        stakeInfo.fertiliserCount += 1;
        // strain.rarityScore += fertBonusPerInteraction[strain.stage];

        frtToken.safeTransferFrom(msg.sender, UTILITY_DISTRIBUTION_ADDRESS, 1);
        emit StrainUpdated(msg.sender, _strainId);
    }

    function upgradeStage(
        uint256 _strainId,
        uint256 _genes
    ) 
        public 
        isOwner(_strainId, msg.sender)
    {
        require(isStageUpgradable(_strainId), "Impossible to upgrade");

        StakeInfo storage strain = stakeHistory[_strainId];
        strain.fertiliserCount = 0;
        strain.lastFertilisedTime = block.timestamp;
        strain.lastWateredTime = block.timestamp;
        strain.waterCount = 0;
        strain.stage++;

        rewardHistory[msg.sender] += 3 * 1e20;

        strainNftToken.upgradeStage(_genes, _strainId, msg.sender);
    }

    function getStakeInfo(uint256 _tokenId) public view returns (StakeInfo memory stakeInfo) {
        stakeInfo = stakeHistory[_tokenId];
    }

    function getAvailableReward(address _owner) public view returns (uint256 reward) {
        reward = rewardHistory[_owner];
    }
}

