import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/IAccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";

import "./IAdminControl.sol";
import "./IVerifiedArtists.sol";

contract VerifiedArtists is ERC165, OwnableUpgradeable {
    using AddressUpgradeable for address;

    /**
    * @dev Struct for Artist
    * @param referrer is the referrer that sent the artist over
    * @param reporter is the referrer that reported the artist (only filled if banned)
    * @param metadata link
    */
    struct Artist {
        address referrer;
        address reporter;
        string metadata;
    }

    /**
    * @dev Struct for Referrer
    * @param identifier is the name of the referrer
    * @param referral is number of referrals
    * @param banned is number of referrals banned that is associated with this referrer
    */
    struct Referrer {
        string identifier;
        string metadataBase;
        uint256 referrals;
        uint256 artistBanned;
        bool referrerBanned;
    }

    IAdminControl public adminControl;

    mapping (address => Artist) artistRegistry;
    mapping (address => Referrer) referrerRegistry;

    event ReferrerAdded(address referrerWallet, string identifier, string metadataBase);
    event ReferrerBanned(address referrerWallet, string identifier);
    event ReferrerUpdated(address referrerWallet, string identifier, uint256 referrals, uint256 artistBanned);

    event ArtistAdded(address referrerWallet, address artistWallet, string metadata);
    event ArtistInfoUpdated(address senderWallet, address artistWallet, string metadata);

    event ArtistBanned(address senderWallet, address artistWallet, bool banned);
    event ArtistUnBanned(address artistWallet, bool banned);
    event ArtistReset(address senderWallet, address artistWallet);
    event AdminContractUpdated(address senderWallet, address adminContract);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     * @param _adminContract to declare the admin contract we are using
     */
    function initialize(IAdminControl _adminContract) public initializer {
        __Ownable_init_unchained();
        adminControl = _adminContract;
    }

    /**
     * @dev Get the list of assigned admins
     */
    function getAdmins() external view returns (address[] memory) {
        return adminControl.getAdmins();
    }
    /**
     * @dev Check if an address is admin
     * @param _admin is the address of the admin we wish to check
     */
    function isAdmin(address _admin) external view returns (bool) {
        return adminControl.isAdmin(_admin);
    }


    //          REFERRER FUNCTIONS              //
    /**
     * @dev Add referrer to the system. This can be done by any admin
     * @param _wallet is the address of the referrer
     * @param _identifier is the identifier of the referrer e.g. Foundation
     * @param _metadataBase is the base URL of the referrer to read their referral metadata
     */
    function addReferrer(address _wallet, string calldata _identifier, string calldata _metadataBase) public {
        require(adminControl.isAdmin(_msgSender()), "VerifiedArtists: This function requires admin level access");
        require(bytes(referrerRegistry[_wallet].identifier).length == 0, "VerifiedArtists: This referrer address has already been added");
        referrerRegistry[_wallet].identifier = _identifier;
        referrerRegistry[_wallet].metadataBase = _metadataBase;
        emit ReferrerAdded(_wallet, _identifier, _metadataBase);
    }

    /**
     * @dev Ban a referrer. This can be done by any admin
     * @param _wallet is the address of the referrer
     */
    function banReferrer(address _wallet) external {
        require(adminControl.isAdmin(_msgSender()), "VerifiedArtists: This function requires admin level access");
        require(bytes(referrerRegistry[_wallet].identifier).length != 0, "VerifiedArtists: This referrer address has not been added");
        require(referrerRegistry[_wallet].referrerBanned == false, "VerifiedArtists: This referrer is already banned");
        referrerRegistry[_wallet].referrerBanned = true;
        emit ReferrerBanned(_wallet, referrerRegistry[_wallet].identifier);
    }

    /**
     * @dev Update a referrer. This can be done by any admin
     * @param _wallet is the address of the referrer
     * @param _wallet is the address of the referrer
     * @param _referrals is the address of the referrer
     * @param _artistBanned is the address of the referrer
     */
    function updateReferrer(address _wallet, string calldata _identifier, string calldata _metadataBase, uint256 _referrals, uint256 _artistBanned) external {
        require(adminControl.isAdmin(_msgSender()), "VerifiedArtists: This function requires admin level access");
        require(bytes(referrerRegistry[_wallet].identifier).length != 0, "VerifiedArtists: This referrer address has not been added");
        referrerRegistry[_wallet] = Referrer(_identifier, _metadataBase, _referrals, _artistBanned, false);
        emit ReferrerUpdated(_wallet, _identifier, _referrals, _artistBanned);
    }

    /**
     * @dev Modified built so to only allows approved referrers to call the specified function
     */
    modifier referrerRequired() {
        require(bytes(referrerRegistry[_msgSender()].identifier).length != 0, "VerifiedArtists: This function requires referrer");
        require(referrerRegistry[_msgSender()].referrerBanned == false, "VerifiedArtists: This referrer is banned");
        _;
    }   

    /**
     * @dev Get referrers identifier
     * @param _referrer is the wallet of the referrer you are checking
     */
         function isReferrerBanned(address _referrer) public view returns (bool) {
        return referrerRegistry[_referrer].referrerBanned;
    }
    /**
     * @dev Get referrers identifier
     * @param _referrer is the wallet of the referrer you are checking
     */
         function getReferrerId(address _referrer) public view returns (string memory) {
        return referrerRegistry[_referrer].identifier;
    }
    /**
     * @dev Get referrers metadata base URL
     * @param _referrer is the wallet of the referrer you are checking
     */
    function getReferrerBaseURL(address _referrer) public view returns (string memory) {
        return referrerRegistry[_referrer].metadataBase;
    }
    /**
     * @dev Get number of artists referred by referrer
     * @param _referrer is the wallet of the referrer you are checking
     */
    function getReferrerNumbers(address _referrer) external view returns (uint256) {
        return referrerRegistry[_referrer].referrals;
    }
    /**
     * @dev Get number of artists banned by referrer
     * @param _referrer is the wallet of the referrer you are checking
     */
    function getReferrerArtistBanned(address _referrer) external view returns (uint256) {
        return referrerRegistry[_referrer].artistBanned;
    }

    //          ARTIST FUNCTIONS              //
    /**
     * @dev To add artist info. Needs admin authority.
     * @param _wallet is the wallet of the Artist
     * @param _metadata is the metadata of the artist such as their usernames etc.
     */
    function addArtist(address _wallet, string calldata _metadata) external referrerRequired {
        require(artistRegistry[_wallet].referrer == address(0), "VerifiedArtists: This artist address has already been added");
        artistRegistry[_wallet].referrer = _msgSender();
        artistRegistry[_wallet].metadata =  _metadata;
        referrerRegistry[_msgSender()].referrals++;
        emit ArtistAdded(_msgSender(), _wallet, _metadata);
    }

    /**
     * @dev To update artist info. Needs referrer or artists authority.
     * @param _wallet is the wallet of the Artist
     * @param _metadata is the metadata of the artist such as their usernames etc.
     */
    function updateArtistInfoByRef(address _wallet, string calldata _metadata) external {
        require(artistRegistry[_wallet].referrer == _msgSender(), "VerifiedArtists: Not referrer wallet");       
        artistRegistry[_wallet].metadata =  _metadata;
        emit ArtistInfoUpdated(_msgSender(), _wallet, _metadata);
    }

    /**
     * @dev To ban an Artist. Needs referrer authority.
     * @param _wallet is the wallet of the Artist
     */
    function banArtist(address _wallet) external referrerRequired {
        require(artistRegistry[_wallet].referrer != address(0), "VerifiedArtists: This artist address has not yet been added");
        require(artistRegistry[_wallet].reporter == address(0), "VerifiedArtists: Artist already banned");
        artistRegistry[_wallet].reporter = _msgSender();        
        referrerRegistry[_msgSender()].artistBanned++;
        emit ArtistBanned(_msgSender(), _wallet, true);
    }

    /**
     * @dev To unban an Artist. Needs owner authority.
     * @param _wallet is the wallet of the Artist
     */
    function unbanArtist(address _wallet) external {
        require(adminControl.isAdmin(_msgSender()), "VerifiedArtists: This function requires admin or owner");
        require(artistRegistry[_wallet].reporter != address(0), "VerifiedArtists: This artist address has not been banned");
        artistRegistry[_wallet].reporter = address(0);        
        emit ArtistUnBanned(_wallet, false);
    }

    /**
     * @dev Delete an Artist to remove any record or fix mistakes.
     * @param _wallet is the wallet of the Artist
     */
    function resetArtist(address _wallet) external {
        require(adminControl.isAdmin(_msgSender()), "VerifiedArtists: This function requires admin or owner");
        require(artistRegistry[_wallet].referrer != address(0), "VerifiedArtists: This artist address has not yet been added");
        artistRegistry[_wallet] = Artist(address(0), address(0), '');
        emit ArtistReset(_msgSender(), _wallet);
    }

    /**
     * @dev Get artists identifier
     * @param _wallet is the wallet of the artist you are checking
     */
    function getArtistMetadata(address _wallet) external view returns (string memory) {
        return artistRegistry[_wallet].metadata;
    }
    /**
     * @dev Get number of artists referred by referrer
     * @param _wallet is the wallet of the artist you are checking
     */
    function getArtistReferrer(address _wallet) external view returns (string memory) {
        return getReferrerId(artistRegistry[_wallet].referrer);
    }
    /**
     * @dev Get number of artists banned by referrer
     * @param _wallet is the wallet of the artist you are checking
     */
    function isArtistBanned(address _wallet) external view returns (bool) {
        return artistRegistry[_wallet].reporter != address(0)? true : false;
    }

    //          UPGRADE FUNCTIONS              //
    /**
     * @dev For owner to update the admin contract if needed
     * @param _adminContract is the new contract address
     */
    function updateAdminContract(address _adminContract) external onlyOwner {
        adminControl = IAdminControl(_adminContract);
        emit AdminContractUpdated(_msgSender(), _adminContract);
    }
}

