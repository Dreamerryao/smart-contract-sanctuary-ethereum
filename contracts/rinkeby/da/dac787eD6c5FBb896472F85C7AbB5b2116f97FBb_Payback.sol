contract Payback is Ownable {

    using BoringMath for uint256;

    uint256 public launchingBlock;
    uint256 public claimed;
    uint256 public accNeuronPerShare;
    
    uint256 public deflation1st;
    uint256 public deflation2nd;
    uint256 public deflation3rd;
    uint256 public deflation4th;
    uint256 public deflation5th;

    uint256 public def1stNeuronPerShare;
    uint256 public def2ndNeuronPerShare;
    uint256 public def3rdNeuronPerShare;
    uint256 public def4thNeuronPerShare;
    uint256 public def5thNeuronPerShare;

    uint256[] public defNeuronPerShare;
    /*

    */
    uint256 public totalToken;
    address public neuronToken;
    address public routerAddress;
    event Claim(uint256 cliamed);

    modifier claimableAddress() {
    // router지정된 주소만가능
        require(msg.sender == routerAddress || msg.sender == owner );
        _;
    }

    constructor(address _neuronToken, uint256 _accNeuronPerShare) public {
        uint ctr = 5; 
        uint val = 42277080162354100;

        neuronToken = _neuronToken;
        accNeuronPerShare = _accNeuronPerShare;

        for (uint i = 0; i < ctr; i++) {
            if (i == 0) {
                defNeuronPerShare.push(_accNeuronPerShare.sub(val));
            } else {
                defNeuronPerShare.push(defNeuronPerShare[i - 1]);
            }
        }
        
        def1stNeuronPerShare = _accNeuronPerShare.sub(42277080162354100);
        def2ndNeuronPerShare = def1stNeuronPerShare.sub(42277080162354100);
        def3rdNeuronPerShare = def2ndNeuronPerShare.sub(42277080162354100);
        def4thNeuronPerShare = def3rdNeuronPerShare.sub(42277080162354100);
        def5thNeuronPerShare = def4thNeuronPerShare.sub(42277080162354100);
    }

    function setRouterAddress(address _routerAddress) onlyOwner() public {
        routerAddress = _routerAddress;
    } 

    function tokensClaimable() external view returns (uint256 claimableAmount) {
        claimableAmount = _tokensClaimable();
    }

    function _tokensClaimable() private view returns (uint256 claimableAmount) {
        if(_getunClaimToken() > 0 ) {
            uint256 blocks = block.number.sub(launchingBlock);
            uint256 de1amountBlock = deflation2nd.sub(deflation1st);
            uint256 de2amountBlock = deflation3rd.sub(deflation2nd);
            uint256 de3amountBlock = deflation4th.sub(deflation3rd);
            uint256 de4amountBlock = deflation5th.sub(deflation4th);
            if(block.number >= launchingBlock && block.number < deflation1st) {
                claimableAmount = blocks.mul(accNeuronPerShare).sub(claimed);
            }else if(block.number >= deflation1st && block.number < deflation2nd) {
                uint256 de1amountBlock = block.number.sub(deflation1st);
                claimableAmount = (deflation1st.sub(launchingBlock)).mul(accNeuronPerShare).add(de1amountBlock.mul(def1stNeuronPerShare));
            }else if(block.number >= deflation2nd && block.number < deflation3rd) {
                uint256 de2amountBlock = block.number.sub(deflation2nd);
                claimableAmount = (deflation1st.sub(launchingBlock)).mul(accNeuronPerShare).add(de1amountBlock.mul(def1stNeuronPerShare)).add(de2amountBlock.mul(def2ndNeuronPerShare));
            }else if(block.number >= deflation3rd && block.number < deflation4th) {
                uint256 de3amountBlock = block.number.sub(deflation3rd);
                claimableAmount = (deflation1st.sub(launchingBlock)).mul(accNeuronPerShare).add(de1amountBlock.mul(def1stNeuronPerShare)).add(de2amountBlock.mul(def2ndNeuronPerShare)).add(de3amountBlock.mul(def3rdNeuronPerShare));
            }else if(block.number >= deflation4th && block.number < deflation5th) {
                uint256 de4amountBlock = block.number.sub(deflation4th);
                claimableAmount = (deflation1st.sub(launchingBlock)).mul(accNeuronPerShare).add(de1amountBlock.mul(def1stNeuronPerShare)).add(de2amountBlock.mul(def2ndNeuronPerShare)).add(de3amountBlock.mul(def3rdNeuronPerShare)).add(de4amountBlock.mul(def4thNeuronPerShare));
            } else if(block.number > deflation5th && block.number < deflation5th.add(10512000)) {
                uint256 de5amountBlock = block.number.sub(deflation5th);
                claimableAmount = (deflation1st.sub(launchingBlock)).mul(accNeuronPerShare).add(de1amountBlock.mul(def1stNeuronPerShare)).add(de2amountBlock.mul(def2ndNeuronPerShare)).add(de3amountBlock.mul(def3rdNeuronPerShare)).add(de4amountBlock.mul(def4thNeuronPerShare)).add(de5amountBlock.mul(def5thNeuronPerShare));
            } else {
                claimableAmount = _getunClaimToken();
            }    
        } else {
            claimableAmount = 0;
        }
    }

    function getunClaimToken() external view returns (uint256 unClaimToken) {
        unClaimToken = _getunClaimToken();
    }

    function _getunClaimToken() private view returns (uint256 unClaimToken) {
        unClaimToken =  IERC20(neuronToken).balanceOf(address(this));
    }

    function setLaunchingBlock(uint256 _launchingBlock) claimableAddress() external {
        launchingBlock = _launchingBlock;
        deflation1st = _launchingBlock.add(10512000);
        deflation2nd = deflation1st.add(10512000);
        deflation3rd = deflation2nd.add(10512000);
        deflation4th = deflation3rd.add(10512000);
        deflation5th = deflation4th.add(10512000);
    }

    function claim(address payable _user, uint _amount) claimableAddress() external {
        require(block.number >= launchingBlock);
        uint256 claimAmount = _tokensClaimable();
        require(claimAmount > _amount);
        TransferHelper.safeTransfer(neuronToken , _user , _amount);
        claimed = claimed.add(_amount);

        emit Claim(_amount);
    } 


}

