// SPDX-License-Identifier: MIT
pragma solidity 0.8.4;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract Envoy is ReentrancyGuard,IERC721Receiver,Initializable ,OwnableUpgradeable {

    using SafeMath for uint256;

    mapping(uint256 => bool) rewarded;

    address public faeClubAddress;

    address public royaltyAddress;

    uint256 public envoyTokenStartNumber = 1;

    uint256 public reward = 0.0000001 ether;

    modifier eoaOnly() {
        require(tx.origin == msg.sender, "EOA Only");
        _;
    }


    function initialize()public initializer{
        __Context_init_unchained();
        __Ownable_init_unchained();
        envoyTokenStartNumber = 1;
    }

    
    function setFaeAddressReward(address _faeClubAddress,uint256 _reward) public onlyOwner {

        faeClubAddress = _faeClubAddress;
        reward = _reward;
    }

    function generateEnvoy(uint256[] memory tokens)
        external
        payable
        nonReentrant
        eoaOnly
    {
        require(tokens.length == 4 , "Fae num error.");
        require(envoyTokenStartNumber <= 500,"No envoy.");

        FaeClub faeClub = FaeClub(faeClubAddress);
        bool isFusion;
        bool isEnvironment;
        bool isAnimal;
        bool isGarden;
        for(uint256 i = 0 ; i < tokens.length ; i++){
            require(faeClub.ownerOf(tokens[i]) == msg.sender, "Not fae owner.");
            if(faeClub.faeRole(tokens[i]) == FaeClub.FaeRole.Fusion){
                isFusion = true;
            }else if(faeClub.faeRole(tokens[i]) == FaeClub.FaeRole.Environment){
                isEnvironment = true;
            }else if(faeClub.faeRole(tokens[i]) == FaeClub.FaeRole.Animal){
                isAnimal = true;
            }else if(faeClub.faeRole(tokens[i]) == FaeClub.FaeRole.Garden){
                isGarden = true;
            }
        }
        require(isFusion&&isEnvironment&&isAnimal&&isGarden,"Need four fae role.");
        require(faeClub.ownerOf(envoyTokenStartNumber) == address(this), "Envoy owner error.");

        for(uint256 i = 0 ; i < tokens.length ; i++){
            faeClub.transferFrom(msg.sender,address(this),tokens[i]);
        }
        faeClub.transferFrom(address(this),msg.sender,envoyTokenStartNumber);
        envoyTokenStartNumber = envoyTokenStartNumber.add(1);
    }


    function transfer(uint256[] memory tokens,address _to) external onlyOwner{

        FaeClub faeClub = FaeClub(faeClubAddress);
        for(uint256 i = 0 ; i < tokens.length ; i++){
            require(faeClub.ownerOf(tokens[i]) == msg.sender, "Not fae owner.");
            faeClub.transferFrom(address(this) , _to , tokens[i]);
        }
    }


    function setEnvoyTokenStartNumber(uint256 _envoyTokenStartNumber) external onlyOwner {

        envoyTokenStartNumber = _envoyTokenStartNumber;
    }

    function setReward(uint256 _reward) external onlyOwner {
        
        reward = _reward;
    }


    function claimReward(uint256 tokenId) public {

        FaeClub faeClub = FaeClub(faeClubAddress);
        require(faeClub.ownerOf(tokenId) == msg.sender, "Not envoy owner.");
        require(!rewarded[tokenId],"Already claimed.");
        uint256 balance = address(this).balance;
        require(balance >= reward , "Not enough ETH.");
        rewarded[tokenId] = true;
        payable(msg.sender).transfer(reward);
    }


    function alreadyClaimReward(uint256 tokenId) public view returns (bool){

        return rewarded[tokenId];
    }
    

    function setRoyaltyAddress(address _royaltyAddress) external onlyOwner {

        royaltyAddress = _royaltyAddress;
    }


    function claimAllRoyaltyByOwner(uint256 startRound) external onlyOwner {
        
        Royalty(royaltyAddress).claimAllRoyaltyByOwner(startRound);
    }


    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }


    function onERC721Received(address operator,address from,uint256 tokenId,bytes calldata data) external override pure returns (bytes4){

        return IERC721Receiver.onERC721Received.selector;
    }
}



interface Royalty {

    function claimAllRoyaltyByOwner(uint256 startRound) external;
}


interface FaeClub {

    enum FaeRole{
        Envoy,
        Fusion,
        Environment,
        Animal,
        Garden,
        Unknow
    }

    function transferFrom(address from,address to,uint256 tokenId) external;

    function ownerOf(uint256 tokenId) external view returns (address);

    function tokenOfOwnerByIndex(address owner, uint256 index) external view returns (uint256);

    function balanceOf(address owner) external view returns (uint256);

    function faeRole(uint256 tokenId) external view returns (FaeRole);
}

