pragma solidity ^0.8.13;

import "./SecurityContract.sol";

import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";

interface SecContract {
    function getCountry() external view returns (string memory);
}

contract Factory {
    using Counters for Counters.Counter;

    Counters.Counter public tokenIdCounter;

    mapping(uint256 => address) public tokenToContract;

    event SecurityContractCreated(address securityAddress);

    function createSecurityContract(
        string calldata _name,
        string calldata _symbol,
        SecurityInfo memory _info
    ) external returns (address securityContractCreated) {
        tokenIdCounter.increment();
        uint256 currentTokenId = tokenIdCounter.current();

        SecurityContract securityContract = new SecurityContract(
            _name,
            _symbol,
            _info
        );
        
        tokenToContract[currentTokenId] = address(securityContract);

        emit SecurityContractCreated(address(securityContract));
        return address(securityContract);
    }

    function getAllSecurityContracts() external view returns (address[] memory) {
        uint256 i;
        uint256 currentTokenId = tokenIdCounter.current();
        address[] memory contracts = new address[](currentTokenId + 1);
        for (i = 1; i <= currentTokenId; i++) {
            contracts[i] = tokenToContract[i];
        }
        return contracts;
    }

    function applyFilter(string memory _countryFilter) external view returns (address[] memory) {
        uint256 i;
        uint256 currentTokenId = tokenIdCounter.current();
        address[] memory contracts = new address[](currentTokenId + 1);
        uint256 j = 1;
        for (i = 1; i <= currentTokenId; i++) {
            if (
                keccak256(abi.encode(SecContract(tokenToContract[i]).getCountry())) ==
                keccak256(abi.encode(_countryFilter))
            ) {
                contracts[j] = tokenToContract[i];
                j++;
            }
        }

        address[] memory contractsFinal = new address[](j);
        uint256 k;
        for (k = 1; k < j; k++) {
            contractsFinal[k] = contracts[k];
        }
        return contractsFinal;
    }
}

