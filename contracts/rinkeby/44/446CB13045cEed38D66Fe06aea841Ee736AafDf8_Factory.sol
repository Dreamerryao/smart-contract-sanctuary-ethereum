pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "solidity-json-writer/contracts/JsonWriter.sol";
import "./SecurityInfo.sol";

contract SecurityContract is ERC20 {
    using Strings for uint256;
    using JsonWriter for JsonWriter.Json;

    uint256 public tokenPrice;
    uint256 public availableTokens;

    SecurityInfo public info;

    mapping(address => bool) public whitelist;

    address public institutionAgentAddress;

    event BaseURIChanged(string _newUri);

    event BuyTokens(address to, uint256 numberOfTokensBought);
    event EmitTokens(uint256 numberOfTokens);
    event AddWhiteList(address candidate);
    event ChengedInstitutionAgentAddress(address institutionAgentAddress);

    error InvalidAmountOfTokenBought();
    error NotOnWhiteList();
    error UnauthorisedAccess();

    constructor(
        string memory _name,
        string memory _symbol,
        SecurityInfo memory _info
    ) ERC20(_name, _symbol) {
        _mint(address(this), 100 ether);
        institutionAgentAddress = msg.sender;
        availableTokens = 100 ether;
        tokenPrice = 1;
        info = _info;
    }

    function mint(uint256 _amount) external {
        _mint(address(this), _amount);
        emit EmitTokens(_amount);
    }

    // pattern used: checks effects interactions
    function buyTokens() external payable {
        if (!whitelist[msg.sender]) {
            // check
            revert NotOnWhiteList();
        }
        uint256 tokenBought = msg.value / tokenPrice;
        if (tokenBought > availableTokens) {
            // check
            revert InvalidAmountOfTokenBought();
        }
        availableTokens -= tokenBought; //effect
        _approve(address(this), msg.sender, tokenBought);
        transferFrom(address(this), msg.sender, tokenBought); // interaction
        emit BuyTokens(msg.sender, tokenBought);
    }

    function addOnWhitelist(address _candidate) external {
        if (msg.sender != institutionAgentAddress) {
            revert UnauthorisedAccess();
        }
        whitelist[_candidate] = true;
        emit AddWhiteList(_candidate);
    }

    function setInstitutionAgentAddress(address _newInstitutionAddress) external {
        if (msg.sender != institutionAgentAddress) {
            revert UnauthorisedAccess();
        }
        institutionAgentAddress = _newInstitutionAddress;
        emit ChengedInstitutionAgentAddress(_newInstitutionAddress);
    }

    function contractURI() public view returns (string memory) {
        JsonWriter.Json memory writer;

        writer = writer.writeStartObject();
        writer = writer.writeStringProperty("name", name());
        writer = writer.writeStringProperty("country", info.country);
        writer = writer.writeStringProperty("city", info.city);
        writer = writer.writeStringProperty("address", info.addressOfAsset);
        writer = writer.writeStringProperty("description", info.description);
        writer = writer.writeStringProperty("typeOfBuilding", info.typeOfBuilding);
        writer = writer.writeUintProperty("area", info.area);
        writer = writer.writeUintProperty("nrOfFloors", info.nrOfFloors);
        writer = writer.writeStringProperty("image", info.folderCID);
        writer = writer.writeEndObject();

        return writer.value;
    }

    function getCountry() external view returns (string memory) {
        return info.country;
    }
}

