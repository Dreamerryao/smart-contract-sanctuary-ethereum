        _setupRole(MINTER_ROLE, _msgSender());
        _setupRole(PAUSER_ROLE, _msgSender());
    }

    function pay(address to, uint256 value) public returns (bool) {
        WITCH.approve(to,value);
        return WITCH.transferFrom(msg.sender, to, value);
    }

    function swirl_pot() public returns(bool){
        _mint(msg.sender, gameitem1, 1, "");
        return true;
    }
}