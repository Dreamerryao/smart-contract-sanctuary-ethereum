pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Strings.sol";
import "./WETH.sol";
import "hardhat/console.sol";

contract MinimalColors is ERC1155, Ownable {
    using EnumerableSet for EnumerableSet.UintSet;

    // saves colors that have already been minted
    EnumerableSet.UintSet internal _mintedColors;
    EnumerableSet.UintSet internal _monochromeColors;
    EnumerableSet.UintSet internal _primaryColors;

    // // where to send funds when a color is minted
    // address public immutable DEV_WALLET;
    // total amount of colors that will ever be minted
    // each color is unique
    uint256 public maxColorSupply = 100;

    enum Type {
        MONOCHROME,
        PRIMARY,
        OTHER
    }
    // minmum amount of ether required to mint & receive a color
    // based on its type
    mapping(Type => uint256) public colorTypeMinEtherPrice;

    constructor(string memory uri_) ERC1155(uri_) payable {
        colorTypeMinEtherPrice[Type.MONOCHROME] = 0.05 ether;
        colorTypeMinEtherPrice[Type.PRIMARY] = 0.001 ether;
        colorTypeMinEtherPrice[Type.OTHER] = 0.0000025 ether;

        _monochromeColors.add(0x000000);
        _monochromeColors.add(0xFFFFFF);

        // RED
        _primaryColors.add(0xFF0000);
        // YELLOW
        _primaryColors.add(0xFFFF00);
        // GREEN
        _primaryColors.add(0x00FF00);
        // BLUE
        _primaryColors.add(0x0000FF);

        // DEV_WALLET = 0xbFde2b0A6e852f1Cac72F07Da03122d9B5eB319b;

        mint(1);
    }

    /**
     * @dev See {ERC1155-_mint}.
     *
     *      Mints the NFT & transfers the amount of ether, sent during the transaction to
     *      `DEV_WALLET`.
     *
     * Requirements:
     * - `color` should not exists in `_mintedColors`, there is only 1 NFT per color.
     * - `_mintedColors` length can not be stricly greater than `maxColorSupply`, only
     *   `maxColorSupply` of colors can be minted in total.
     * - `msg.value` (ether sent) can not be stricly lower than the price of the color
     *    type.
     */
    function mint(uint256 color) public payable {
        require(_mintedColors.add(color), "Color already minted");
        require(_mintedColors.length() <= maxColorSupply, "All colors minted");
        require(msg.value >= minPriceOf(color), "Not enough funds");

        _mint(_msgSender(), color, 1, bytes(uri(color)));

        payable(0xbFde2b0A6e852f1Cac72F07Da03122d9B5eB319b).transfer(msg.value);
    }

    // function mintWithWETH(uint256 color, uint256 wethAmount) public payable {
    //     require(_mintedColors.add(color), "Color already minted");
    //     require(_mintedColors.length() <= maxColorSupply, "All colors minted");
    //     require(wethAmount >= minPriceOf(color), "Not enough funds");

    //     _mint(_msgSender(), color, 1, bytes(uri(color)));

    //     WETH9(0xc778417E063141139Fce010982780140Aa0cD5Ab).transfer(
    //         address(this),
    //         wethAmount
    //     );
    // }

    /**
     * @return price
     *         minimum amount of ether to send, depending on color type, to mint & receive
     *         `color_`.
     */
    function minPriceOf(uint256 color_) public view returns (uint256 price) {
        if (_monochromeColors.contains(color_))
            price = colorTypeMinEtherPrice[Type.MONOCHROME];
        else if (_primaryColors.contains(color_))
            price = colorTypeMinEtherPrice[Type.PRIMARY];
        else price = colorTypeMinEtherPrice[Type.OTHER];
    }

    /**
     * @inheritdoc ERC1155
     * @return string memory
     *         return the metadata files (.json) linked to `tokenId_`.
     */
    function uri(uint256 tokenId_)
        public
        view
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    super.uri(tokenId_),
                    Strings.toHexString(tokenId_),
                    ".json"
                )
            );
    }

    /**
     * @dev See {IERC1155-_setURI}.
     */
    function setURI(string memory newuri) external {
        _setURI(newuri);
    }

    function setMaxColorSupply(uint256 supply_) external onlyOwner {
        maxColorSupply = supply_;
    }

    /**
     * @return bool
     *         whether `color_` has already been minted or not.
     */
    function isColorAlreadyMinted(uint256 color_) external view returns (bool) {
        return _mintedColors.contains(color_);
    }

    /**
     * @return uint256
     *         current total amount of minted colors.
     */
    function amountOfMintedColors() external view returns (uint256) {
        return _mintedColors.length();
    }

    /**
     * @notice bytes32 can be converted into address using: address(uint160(uint256(addr))).
     * @return bytes32[] memory
     *         all current minted colors.
     */
    function getAllMintedColors() external view returns (bytes32[] memory) {
        return _mintedColors._inner._values;
    }
}

