    BasicOrderParameters order;
    address nftApprover; // conduit
    LiquidateCallParams liquidateParams;
    FlashLoanParams flashLoanParams;
  }

  struct X2Y2LiquidateERC721Params {
    Market.RunInput input;
    address nftApprover; // executionDelegate
    LiquidateCallParams liquidateParams;
    FlashLoanParams flashLoanParams;
  }

  constructor(
    LiquidatorConfig config,
    address seaport,
    address x2y2Market
  ) LiquidatorBase(config) {
    super.setSeaport(seaport);
    super.setX2Y2Market(x2y2Market);
  }

  /**
   * @dev use opensea market to flashloan liquidate ERC721
   */
  function openseaLiquidateERC721(OpenseaLiquidateERC721Params calldata params)
    public
