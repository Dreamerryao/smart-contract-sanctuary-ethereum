contract CryptoPunks is Context, Ownable {
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;

    address private _owner;

    string public standard = 'CryptoPunks';
    uint8 public decimals;
    uint256 private punkIndex;

    address constant NULL_ADDRESS = address(0x0);

    uint256 constant public SALE_START_TIMESTAMP = 1611846000;

    // Mapping from holder address to their (enumerable) set of owned tokens
    mapping (address => EnumerableSet.UintSet) private _holderTokens;

    // Enumerable mapping from token ids to their owners
    EnumerableMap.UintToAddressMap private _tokenOwners;

    // Mapping from token ID to approved address
    mapping (uint256 => address) private _tokenApprovals;

    // Mapping from token ID to name
    mapping (uint256 => string) private _tokenName;

    // Mapping if certain name string has already been reserved
    mapping (string => bool) private _nameReserved;

    // Mapping from token ID to whether the Punk was minted before reveal
    mapping (uint256 => bool) private _mintedBeforeReveal;

    // Mapping from owner to operator approvals
    mapping (address => mapping (address => bool)) private _operatorApprovals;

    // Mapping from tokenID to token struct details
    mapping (uint256 => Punk) private _tokenIDPunks;


    struct Punk {
        uint256 _punkID;
        uint256 _offerPrice;
        uint256 _maxBidPrice;
        string _uri;
        address _punkOwner;
        address _bidder;
    }

    constructor () {
        address msgSender = _msgSender();
        _owner = msgSender;
        punkIndex = 0;

        // emit OwnershipTransferred(address(0), msgSender);
    }

    modifier onlyInAuction() {
        require(getNextIndex() < totalSupply(), "Auction is over!");
        _;
    }

    function totalSupply() public pure returns(uint256) {
        return 20;
    }

    function buy() public payable onlyInAuction {
        require(msg.value == getPunkPrice(), "The value is incorrect!");
        uint _punkIndex = getNextIndex();
        mint(_msgSender(), _punkIndex);
    }

    function getPunkPrice() public view returns(uint256) {
        return _getPunkPrice(getNextIndex());
    }

    function _getPunkPrice(uint256 _punkIndex) internal pure returns(uint256) {
        uint punkPrice;
        if (_punkIndex < 2) {
            punkPrice = 500000000000000000;
        } else
        if (_punkIndex < 4) {
            punkPrice = 1000000000000000000;
        } else
        if (_punkIndex < 6) {
            punkPrice = 2000000000000000000;
        } else
        if (_punkIndex < 8) {
            punkPrice = 4000000000000000000;
        } else
        if (_punkIndex < 20) {
            punkPrice = 8000000000000000000;
        }
        return punkPrice;
    }

    function mint(address _to, uint256 _tokenID) private onlyInAuction {
        require(_to != NULL_ADDRESS, "Adress is invalid!");
        require(!_exists(_tokenID));

        _mint(_to, _tokenID);
    }

    function _mint(address _to, uint256 _tokenID) private {
        _holderTokens[_to].add(_tokenID);
        
        _tokenOwners.set(_tokenID, _to);

        Punk memory thisPunk = Punk(_tokenID, 0, 0, '', _to, NULL_ADDRESS);

        _tokenIDPunks[_tokenID] = thisPunk;

        incrementIndex();
    }

    // function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    //     require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

    //     string memory baseURI = _baseURI();
    //     return bytes(baseURI).length > 0
    //         ? string(abi.encodePacked(baseURI, tokenId.toString()))
    //         : '';
    // }

    function _exists(uint256 tokenId) internal view returns (bool) {
        return _tokenOwners.contains(tokenId);
    }


    /**
     * @dev Base URI for computing {tokenURI}. Empty by default, can be overriden
     * in child contracts.
     */
    function _baseURI() internal view virtual returns (string memory) {
        return "";
    }

    // This should be inherited.
    function transfer(address _to, uint256 _tokenID) public {
        require(_to != NULL_ADDRESS, "Invalid address!");
        require(_to != _msgSender(), "Cannot transfer to the owner!");
        require(_tokenID < totalSupply(), "TokenID is invalid!");
        (, address thisOwner) = _tokenOwners.at(_tokenID);
        require(thisOwner == _msgSender(), "Not the owner!");

        _holderTokens[_msgSender()].remove(_tokenID);
        _holderTokens[_to].add(_tokenID);
        _tokenOwners.set(_tokenID, _to);
        _tokenIDPunks[_tokenID]._punkOwner = _msgSender();
    }

    // ERC721
    function balanceOf(address _tokenOwner) public view returns(uint256) {
        return _holderTokens[_tokenOwner].length();
    }

    // ERC721
    function ownerOf(uint256 _tokenID) public view returns(address) {
        return _tokenOwners.get(_tokenID);
    }

    // // setOfferPrice
    // function setOfferPrice() {
        
    // }

    // // Set a bid price for a punk
    // function bid() public payable {

    // }

    function getNextIndex() public view returns(uint256) {
        return punkIndex;
    }

    function incrementIndex() internal {
        punkIndex++;
    }
}



