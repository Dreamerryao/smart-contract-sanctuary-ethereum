pragma solidity ^0.8.0;
import "../interfaces/IInbox.sol";
import "../libraries/AddressAliasHelper.sol";

import "hardhat/console.sol";

contract EthDepositor {

    IInbox public arb_bridge;


    constructor(address _arb_bridge) {
        arb_bridge = IInbox(_arb_bridge);
    }

    function createTicket(uint256 maxSubmissionCost,
        uint256 maxGas,
        uint256 gasPriceBid
        ) public payable{
        bytes memory data;
        arb_bridge.createRetryableTicket{value:msg.value}(msg.sender,msg.value,maxSubmissionCost,msg.sender,msg.sender,maxGas,gasPriceBid,data);
    }
    
}

