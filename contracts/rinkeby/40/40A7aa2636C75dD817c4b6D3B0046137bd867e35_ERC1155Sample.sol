        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721EnumerableUpgradeable, ERC721Upgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function getInfo(uint256 tokenId)
        external
        view
        returns (
            address owner,
            uint256 mintedAt,
            address artistAddress,
            uint256 paidValue,
            address offererAddress,
            uint256 offeredValue,
            uint256 shareToPF,
            uint256 shareToArtistForRequest,
            LibW80.OrderStatus orderStatus,
            uint256 nonce,
            string memory uri
        )
    {
        require(_exists(tokenId), "nonexistent token");
        owner = ownerOf(tokenId);
        mintedAt = token[tokenId].mintedAt;
        artistAddress = token[tokenId].artistAddress;
        paidValue = token[tokenId].paidValue;
        offererAddress = token[tokenId].offererAddress;
        offeredValue = token[tokenId].offeredValue;
        shareToPF = token[tokenId].shareToPF;
        shareToArtistForRequest = token[tokenId].shareToArtistForRequest;
        orderStatus = token[tokenId].orderStatus;
        nonce = token[tokenId].nonce;
        uri = tokenURI(tokenId);
    }

