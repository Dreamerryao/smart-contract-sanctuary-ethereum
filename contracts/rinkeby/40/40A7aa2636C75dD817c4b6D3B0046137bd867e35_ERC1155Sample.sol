    {
        return
            artistAddressToRequesters[_artistAddress].contains(
                _requesterAddress
            );
    }

    function tokenOfArtistAddressByIndex(address _artistAddress, uint256 _index)
        external
        view
        returns (uint256 _tokenId)
    {
        _tokenId = artistAddressToTokenIds[_artistAddress].at(_index);
    }

    function setOrderStatus(uint256 tokenId, LibW80.OrderStatus _status)
        external
        onlyOwner
    {
        token[tokenId].orderStatus = _status;
    }

    function approve(address to, uint256 tokenId)
        public
        override(ERC721Upgradeable)
    {
        require(
            token[tokenId].orderStatus == LibW80.OrderStatus.RECEIVED,
            "W80Token: should be received first"
        );
        super.approve(to, tokenId);
    }

    function _safeTransfer(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) internal override(ERC721Upgradeable) {
        require(
            token[tokenId].orderStatus == LibW80.OrderStatus.RECEIVED ||
                owner() == msg.sender,
            "W80Token: should be received first"
        );
        super._safeTransfer(from, to, tokenId, _data);
    }

    // /**
    //  * @dev See {IERC721Metadata-tokenURI}.
