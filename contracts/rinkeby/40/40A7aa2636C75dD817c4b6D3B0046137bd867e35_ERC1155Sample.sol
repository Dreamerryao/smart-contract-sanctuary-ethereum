    //         "ERC721URIStorage: URI set of nonexistent token"
    //     );
    //     token[tokenId].uri = _tokenURI;
    // }

    // function setToken(uint256 tokenId, LibW80.Token calldata _token) external {
    //     token[tokenId] = _token;
    // }

    function mintWithToken(address _minter, LibW80.Token calldata _token)
        external
        payable
    {
        uint256 tokenId = totalSupply();

        _safeMint(_minter, tokenId);
        _setTokenURI(tokenId, _token.uri);
        token[tokenId] = _token;

        artistAddressToTokenIds[_token.artistAddress].add(tokenId);
        artistAddressToRequesters[_token.artistAddress].add(_minter);
    }
}

