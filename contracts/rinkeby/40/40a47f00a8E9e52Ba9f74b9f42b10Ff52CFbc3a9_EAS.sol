pragma solidity 0.8.4;

import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";
import "@chainlink/contracts/src/v0.8/ConfirmedOwner.sol";

/**
 * Find information on LINK Token Contracts and get the latest ETH and LINK faucets here: https://docs.chain.link/docs/link-token-contracts/
 */

import "hardhat/console.sol";

import "./EASHelpers.sol";

contract EASCollectionManager is ChainlinkClient, ConfirmedOwner {
    mapping(address => Collection) internal collections;

    struct Collection {
        bool baseUriMethod;
        mapping(address => bool) isAdmin;
        address linkedOwner;
    }

    function updateAdmins(address[] memory _admins, address collection) external onlyOwner {
        for (uint256 j = 0; j < _admins.length; j++) {
            collections[collection].isAdmin[_admins[j]] = true;
        }
    }

    function updateBaseUriMethod(bool _baseUriMethod, address collection) external onlyOwner {
        collections[collection].baseUriMethod = _baseUriMethod;
    }

    function updateCollectionOwnership(address collectionAddress, address owner) internal {
        collections[collectionAddress].linkedOwner = owner;
    }

    // Chainlink
    using Chainlink for Chainlink.Request;

    mapping(address => address) ownerOfCollection;

    uint256 public volume;

    bytes32 private jobId;
    uint256 private fee;

    mapping(bytes32 => address) public ownershipRequestId;

    mapping(string => Network) private networks;

    struct Network {
        string id; // "ETHEREUM:", "SOLANA:", etc
        string apiUrl; // "https://api.rarible.org"
        string path; // "/v0.1/collections/"
        string jsonPath; // chainLink json path query search
        string addressPrefix; // ie: "0x", "bc1", etc
    }

    event OwnershipUpdated(bytes32 indexed requestId, string owner, address contractAddress);

    /**
     * @notice Initialize the link token and target oracle
     *
     * Kovan Testnet details:
     * Link Token: 0xa36085F69e2889c224210F603D836748e7dC0088
     * Oracle: 0x74EcC8Bdeb76F2C6760eD2dc8A46ca5e581fA656 (Chainlink DevRel)
     * jobId: 7d80a6386ef543a3abb52817f6707e3b
     *
     */
    constructor() ConfirmedOwner(msg.sender) {
        setChainlinkToken(0x01BE23585060835E02B77ef475b0Cc51aA1e0709);
        setChainlinkOracle(0xf3FBB7f3391F62C8fe53f89B41dFC8159EE9653f);
        jobId = "7d80a6386ef543a3abb52817f6707e3b";
        fee = 10**16; // 0.01 link
    }

    function _setChainlinkOracle(address oracle) external onlyOwner {
        setChainlinkOracle(0xf3FBB7f3391F62C8fe53f89B41dFC8159EE9653f);
    }

    function _setChainlinkToken(address token) external onlyOwner {
        setChainlinkToken(0x01BE23585060835E02B77ef475b0Cc51aA1e0709);
    }

    function setJobId(bytes32 _jobId) external onlyOwner {
        jobId = _jobId;
    }

    /**
     * Register a new network
     */
    function registerNetwork(string memory name, Network memory _network) public onlyOwner {
        networks[name] = Network(
            _network.id,
            _network.apiUrl,
            _network.path,
            _network.jsonPath,
            _network.addressPrefix
        );
    }

    /**
     * Create a Chainlink request to retrieve API response, find the target
     * data, update ownership of a collection based in trusted endpoint
     */
    function linkOwnership(string memory _network, address _contractAddress) public returns (bytes32 requestId) {
        //todo: check if network is supported
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);

        string memory uri = string(
            abi.encodePacked(
                networks[_network].apiUrl,
                networks[_network].path,
                networks[_network].id,
                networks[_network].addressPrefix,
                EASHelpers.toAsciiString(_contractAddress)
            )
        );

        // Set the URL to perform the GET request on
        req.add("get", uri);

        // Set the path to find the desired data in the API response, where the response format is:
        // {"owner":
        //  }

        req.add("path", networks[_network].jsonPath);

        ownershipRequestId[req.id] = _contractAddress;

        // Sends the request
        return sendChainlinkRequest(req, fee);
    }

    /**
     * Chainlink callback. Receive the response
     */
    function fulfill(bytes32 _requestId, string memory ownerOf) public recordChainlinkFulfillment(_requestId) {
        emit OwnershipUpdated(_requestId, ownerOf, ownershipRequestId[_requestId]);

        updateCollectionOwnership(
            ownershipRequestId[_requestId],
            EASHelpers.parseAddr(EASHelpers.substring(ownerOf, 9, 51))
        );
    }

    /**
     * Allow withdraw of Link tokens from the contract
     */
    function withdrawLink() public onlyOwner {
        LinkTokenInterface link = LinkTokenInterface(chainlinkTokenAddress());
        require(link.transfer(msg.sender, link.balanceOf(address(this))), "Unable to transfer");
    }

    // REMOVE IN PROD

    function test(string memory toParse) public view returns (address) {
        string memory substring = EASHelpers.substring(toParse, 9, 51);

        console.log(substring);

        address parsed = EASHelpers.parseAddr(substring);
        console.log(parsed);

        return parsed;
    }
}

