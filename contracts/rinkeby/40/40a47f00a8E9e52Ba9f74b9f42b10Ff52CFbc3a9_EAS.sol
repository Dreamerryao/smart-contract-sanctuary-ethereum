pragma solidity 0.8.4;

import "./interfaces/IOwnable.sol";
import "./EASCollectionManager.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";

import "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

// option 1
contract EAS is ERC165, EASCollectionManager {
    uint256 public avatarId = 1;

    mapping(uint256 => Token) private avatarToToken;
    mapping(uint256 => string) private avatarURI;
    mapping(uint256 => string) private secondaryAvatarURI;

    mapping(address => mapping(uint256 => uint256)) private tokenToAvatar;

    string public name = "Ethereum Avatar System";
    string public symbol = "EAS";

    struct Token {
        uint256 tokenId;
        address contractAddress;
    }

    mapping(address => address) public adminOfAvatar;

    function isOwnerOrAdminOfCollection(address contractAddress) internal view returns (bool) {
        if (collections[contractAddress].isAdmin[msg.sender]) return true;
        if (collections[contractAddress].linkedOwner == msg.sender) return true;

        if (isOwnableCompatible(contractAddress)) {
            IOwnable ownable = IOwnable(contractAddress);
            if (ownable.owner() == msg.sender) return true;
        }

        return false;
    }

    function isOwnableCompatible(address contractAddress) internal view returns (bool) {
        if (
            ERC165Checker.supportsInterface(contractAddress, type(IERC721Metadata).interfaceId) ||
            ERC165Checker.supportsInterface(contractAddress, type(IERC721).interfaceId)
        ) return true;
        else return false;
    }

    function isOwnerOrAdminOfNFT(address contractAddress, uint256 tokenId) internal view returns (bool) {
        if (
            ERC165Checker.supportsInterface(contractAddress, type(IERC721Metadata).interfaceId) ||
            ERC165Checker.supportsInterface(contractAddress, type(IERC721).interfaceId)
        ) {
            IERC721Upgradeable nftContract = IERC721Upgradeable(contractAddress);
            if (nftContract.ownerOf(tokenId) == msg.sender || adminOfAvatar[contractAddress] == msg.sender) return true;
        } else {
            if (adminOfAvatar[contractAddress] == msg.sender) return true;
        }

        return false;
    }

    function registerAvatar(
        address contractAddress,
        uint256 tokenId,
        string memory _avatarURI
    ) external {
        require(isOwnerOrAdminOfCollection(contractAddress), "You are not allowed to edit this mapping");
        require(tokenToAvatar[contractAddress][tokenId] == 0, "An Avatar was already registered for this NFT");

        IERC721Upgradeable nftContract = IERC721Upgradeable(contractAddress);

        avatarURI[avatarId] = _avatarURI;

        avatarToToken[avatarId].tokenId = tokenId;
        avatarToToken[avatarId].contractAddress = contractAddress;

        tokenToAvatar[contractAddress][tokenId] = avatarId;

        // hack to get listed on erc721 indexers
        emit Transfer(address(this), nftContract.ownerOf(tokenId), avatarId);

        avatarId++;
    }

    function emitTransfer(uint256 _avatarId) external {
        require(_avatarId < avatarId, "Non existent token");
        Token memory token = avatarToToken[_avatarId];

        uint256 tokenId = token.tokenId;

        IERC721Upgradeable nftContract = IERC721Upgradeable(token.contractAddress);

        // hack to update transfer on indexer services
        emit Transfer(address(this), nftContract.ownerOf(tokenId), _avatarId);
    }

    function updateAvatar(uint256 _avatarId, string memory _avatarURI) external {
        require(_avatarId < avatarId, "Non existent token");

        Token memory token = avatarToToken[_avatarId];

        address contractAddress = token.contractAddress;

        require(isOwnerOrAdminOfCollection(contractAddress), "You are not allowed to edit this mapping");

        avatarURI[_avatarId] = _avatarURI;
    }

    function updateAvatarSecondaryURI(uint256 _avatarId, string memory _avatarURI) external {
        require(_avatarId < avatarId, "Non existent token");

        Token memory token = avatarToToken[_avatarId];

        uint256 tokenId = token.tokenId;
        address contractAddress = token.contractAddress;

        require(
            isOwnerOrAdminOfNFT(contractAddress, tokenId) || isOwnerOrAdminOfCollection(contractAddress),
            "You are not allowed to edit this mapping"
        );

        avatarURI[_avatarId] = _avatarURI;
    }

    function tokenURI(uint256 _avatarId) public view returns (string memory) {
        require(_avatarId < avatarId, "Non existent token");

        return avatarURI[_avatarId];
    }

    function _secondaryAvatarURI(uint256 _avatarId) public view returns (string memory) {
        require(_avatarId < avatarId, "Non existent token");

        return secondaryAvatarURI[_avatarId];
    }

    function avatarsToken(uint256 _avatarId) public view returns (Token memory) {
        return avatarToToken[_avatarId];
    }

    function ownerOf(uint256 _avatarId) public view returns (address) {
        require(_avatarId < avatarId, "Non existent token");

        IERC721Upgradeable nftContract = IERC721Upgradeable(avatarToToken[_avatarId].contractAddress);

        return nftContract.ownerOf(avatarToToken[_avatarId].tokenId);
        //return avatarToToken[avatarId];
    }

    // hack to get listed on erc721 indexers
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC165) returns (bool) {
        return
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721Metadata).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    event Transfer(address indexed from, address indexed to, uint256 indexed tokenId);
}

