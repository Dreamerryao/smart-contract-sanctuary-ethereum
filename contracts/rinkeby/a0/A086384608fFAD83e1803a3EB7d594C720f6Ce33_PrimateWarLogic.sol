pragma solidity 0.8.7;

library Calculations {
    function assetsCalculation(
        uint256 a,
        uint256 b,
        uint256 c,
        uint256 d
    ) internal pure returns (uint256) {
        uint256 e = (a * (b * (c + 1))) / 100;
        return e - (((e / 2) * (100 - d)) / 100);
    }

    function warPower(
        uint256 a,
        uint256 b,
        uint256 c,
        uint256 d,
        uint256 e
    ) internal pure returns (uint256) {
        uint256 f = (a * b * c * d) / 100;
        return f + ((f * e) / 100);
    }

    function rate(uint256 a, uint256 b) internal pure returns (uint256) {
        return (a * b) / 100;
    }

    function primateWarLost(
        uint256 a,
        uint256 b,
        bool sum
    ) internal pure returns (uint256) {
        if (sum) {
            return ((5 + (((200 - a) * 5) / 100)) * b) / 100;
        } else {
            return ((30 - (((a - 100) * 15) / 100)) * b) / 100;
        }
    }
}

