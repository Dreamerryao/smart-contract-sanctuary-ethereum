import "./libraries/Calculations.sol";
import "hardhat/console.sol";

contract PrimateWarLogic {
    using Calculations for uint256;
    using Calculations for uint8;

    mapping(uint256 => mapping(address => uint256)) private _balances;
    mapping(address => mapping(address => bool)) private _operatorApprovals;
    string private _uri;
    mapping(uint256 => uint256) public nextWorkTime;
    mapping(uint256 => uint256) public lastFeedTime;
    mapping(address => clan) public clans;
    mapping(uint256 => uint256) public shield;
    uint256 public clanId;
    address logicContract;
    uint8[6][5] primatesDefaultValues = [
        [5, 10, 7, 6, 6, 5],
        [10, 6, 5, 9, 8, 6],
        [7, 7, 7, 8, 7, 6],
        [6, 5, 6, 7, 4, 10],
        [5, 5, 10, 6, 5, 10]
    ];

    uint8[8][5] primatesSeasonWarPower = [
        [5, 20, 0, 15, 10, 15, 15, 10],
        [20, 0, 15, 0, 20, 10, 20, 10],
        [15, 5, 20, 0, 20, 5, 20, 10],
        [10, 10, 0, 10, 5, 10, 10, 15],
        [0, 10, 5, 10, 10, 10, 15, 10]
    ];

    struct clan {
        primateLevels primateLevel;
        uint256 id;
        uint256 population;
        uint8 defance;
        uint8 morale;
        uint8 techLevel;
        uint8 farmingLevel;
        uint8 workmanshipLevel;
    }

    struct primateLevels {
        primates primateType;
        uint8 power;
        uint8 intelligence;
        uint8 reproduction;
        uint8 durability;
        uint8 nutrition;
        uint8 sociality;
    }

    enum primates {
        HOMO_SAPIENS,
        GORILLA_GORILLA_BEREINGEI,
        PAN_TROGLODYTES_SCHWEINFURTHII,
        PONGO_PYGMAEUS,
        PAN_PANISCUS
    }

    enum developTypes {
        TECH_LEVEL,
        FARMING_LEVEL,
        WORKMANSHIP_LEVEL
    }

    enum weaponTypes {
        NO_WEAPON,
        SMALL_STICK,
        BIG_STICK,
        DOUBLE_BIG_STICK,
        SMALL_AX,
        AX,
        DOUBLE_AX,
        SIMPLE_SPEAR,
        SPEAR,
        STRONG_SPEAR,
        SIMPLE_SWORD,
        SWORD,
        DOUBLE_SWORD,
        SIMPLE_BOW,
        BOW,
        STRONG_BOW
    }

    enum primateProperties {
        POWER,
        INTELLIGENCE,
        REPRODUCTION,
        DURABILITY,
        SOCIALITY
    }

    function season() public view returns (uint8) {
        return uint8((block.timestamp % 2419200) / 604800);
    }

    function createPrimateClan(primates primateType) external {
        require(clans[msg.sender].id == 0, "you already have clan");
        clans[msg.sender].id = clanId;
        clans[msg.sender].population = 1000;
        clans[msg.sender].morale = 100;

        uint8 primateNumber = uint8(primateType);

        clans[msg.sender].primateLevel = primateLevels({
            primateType: primateType,
            power: primatesDefaultValues[primateNumber][0],
            intelligence: primatesDefaultValues[primateNumber][1],
            reproduction: primatesDefaultValues[primateNumber][2],
            durability: primatesDefaultValues[primateNumber][3],
            nutrition: primatesDefaultValues[primateNumber][4],
            sociality: primatesDefaultValues[primateNumber][5]
        });
    }

    function sendPrimateWorkers(uint256 _length)
        external
        returns (uint256[] memory, uint256[] memory)
    {
        clan storage _clan = clans[msg.sender];

        require(nextWorkTime[_clan.id] < block.timestamp, "primates are tired");

        uint256[] memory ids = new uint256[](_length);
        uint256[] memory amounts = new uint256[](_length);

        uint256 _food = (_clan.population * 10).rate(
            120 + (_clan.farmingLevel * 5)
        );
        amounts[0] = _food - (_food.rate(season() * 2));

        ids[0] = 15;

        if (_clan.techLevel >= 1) {
            amounts[1] = _clan.population.assetsCalculation(
                30,
                _clan.workmanshipLevel,
                _clan.morale
            );
            ids[1] = 16;
        }
        if (_clan.techLevel >= 2) {
            amounts[2] = _clan.population.assetsCalculation(
                15,
                _clan.workmanshipLevel,
                _clan.morale
            );
            ids[2] = 17;
        }
        if (_clan.techLevel >= 4) {
            amounts[3] = _clan.population.assetsCalculation(
                5,
                _clan.workmanshipLevel,
                _clan.morale
            );
            ids[3] = 18;
        }

        nextWorkTime[_clan.id] = block.timestamp + 86400;

        return (ids, amounts);
    }

    function feedClan() external returns (uint256) {
        clan storage _clan = clans[msg.sender];

        uint256 unfedTime;

        if (lastFeedTime[_clan.id] != 0) {
            unfedTime = (block.timestamp - lastFeedTime[_clan.id]) / 86400;
        }

        if (unfedTime > 1) {
            if (
                (unfedTime - 1) * (20 - _clan.primateLevel.sociality) >
                _clan.morale
            ) {
                _clan.morale = 0;
            } else {
                _clan.morale -= uint8(
                    (unfedTime - 1) * (20 - _clan.primateLevel.sociality)
                );
            }
        }

        if (100 >= _clan.morale + _clan.primateLevel.sociality) {
            _clan.morale += _clan.primateLevel.sociality;
        } else {
            _clan.morale = 100;
        }

        _clan.population += _clan.population.rate(
            _clan.primateLevel.reproduction
        );

        lastFeedTime[_clan.id] = block.timestamp;

        return uint256(_clan.population * _clan.primateLevel.nutrition);
    }

    function develop(developTypes developType) external returns (uint256) {
        clan storage _clan = clans[msg.sender];

        require(_clan.morale == 100, "primates morale must be 100");

        uint256 amountInFood;

        if (developType == developTypes.FARMING_LEVEL) {
            require(
                _clan.farmingLevel < _clan.techLevel + 1 &&
                    _clan.farmingLevel < _clan.workmanshipLevel + 1,
                "difference between technologies cannot be more than 1"
            );
            require(
                _clan.primateLevel.durability >= 11 + _clan.farmingLevel,
                "not enogh durability level"
            );

            amountInFood =
                _clan.population *
                (10**uint256(_clan.farmingLevel + 1));

            _clan.farmingLevel++;
        } else if (developType == developTypes.TECH_LEVEL) {
            require(
                _clan.techLevel < _clan.farmingLevel + 1 &&
                    _clan.techLevel < _clan.workmanshipLevel + 1,
                "difference between technologies cannot be more than 1"
            );

            require(
                _clan.primateLevel.intelligence >= 11 + _clan.techLevel,
                "not enough intelligence"
            );

            amountInFood =
                _clan.population *
                (10**uint256(_clan.techLevel + 1));

            _clan.techLevel++;
        } else {
            require(
                _clan.workmanshipLevel < _clan.farmingLevel + 1 &&
                    _clan.workmanshipLevel < _clan.techLevel + 1,
                "difference between technologies cannot be more than 1"
            );

            require(
                _clan.primateLevel.power >= 11 + _clan.workmanshipLevel,
                "not enough power"
            );

            amountInFood =
                _clan.population *
                (10**uint256(_clan.workmanshipLevel + 1));

            _clan.workmanshipLevel++;
        }

        return amountInFood;
    }

    function primateUpgrate(primateProperties primateProperty)
        external
        returns (uint256, uint256)
    {
        clan storage _clan = clans[msg.sender];

        require(_clan.morale == 100, "morale must be 100");

        uint256 newLevel;

        if (primateProperty == primateProperties.POWER) {
            _clan.primateLevel.power++;
            newLevel = _clan.primateLevel.power;
        } else if (primateProperty == primateProperties.INTELLIGENCE) {
            _clan.primateLevel.intelligence++;
            newLevel = _clan.primateLevel.intelligence;
        } else if (primateProperty == primateProperties.REPRODUCTION) {
            _clan.primateLevel.reproduction++;
            newLevel = _clan.primateLevel.reproduction;
        } else if (primateProperty == primateProperties.DURABILITY) {
            _clan.primateLevel.durability++;
            newLevel = _clan.primateLevel.durability;
        } else {
            _clan.primateLevel.sociality++;
            newLevel = _clan.primateLevel.sociality;
        }

        require(newLevel < 16, "maximum level exceeds");

        return (newLevel, newLevel * _clan.population);
    }

    function createWeapon(
        weaponTypes weaponType,
        uint256 amountWeapon,
        uint256 _length
    ) external view returns (uint256[] memory, uint256[] memory) {
        require(weaponType != weaponTypes.NO_WEAPON, "invalid weapon type");

        uint256 _weaponTechLevel = weaponTechLevel(
            clans[msg.sender].techLevel,
            weaponType
        );

        uint256[] memory ids = new uint256[](_length);
        uint256[] memory amounts = new uint256[](_length);

        ids[0] = 16;

        if (_weaponTechLevel == 1) {
            if (uint256(weaponType) - 1 == 0) {
                amounts[0] = 2 * amountWeapon;
            } else if (uint256(weaponType) - 1 == 1) {
                amounts[0] = 3 * amountWeapon;
            } else {
                amounts[0] = 4 * amountWeapon;
            }
        } else if (_weaponTechLevel == 2) {
            ids[1] = 17;

            if (uint256(weaponType) - 4 == 0) {
                amounts[0] = 4 * amountWeapon;
                amounts[1] = 1 * amountWeapon;
            } else if (uint256(weaponType) - 4 == 1) {
                amounts[0] = 5 * amountWeapon;
                amounts[1] = 2 * amountWeapon;
            } else {
                amounts[0] = 6 * amountWeapon;
                amounts[1] = 4 * amountWeapon;
            }
        } else if (_weaponTechLevel == 3) {
            ids[1] = 17;
            if (uint256(weaponType) - 7 == 0) {
                amounts[0] = 6 * amountWeapon;
                amounts[1] = 2 * amountWeapon;
            } else if (uint256(weaponType) - 7 == 1) {
                amounts[0] = 7 * amountWeapon;
                amounts[1] = 3 * amountWeapon;
            } else {
                amounts[0] = 8 * amountWeapon;
                amounts[1] = 5 * amountWeapon;
            }
        } else if (_weaponTechLevel == 4) {
            ids[1] = 18;

            if (uint256(weaponType) - 10 == 0) {
                amounts[0] = 8 * amountWeapon;
                amounts[1] = 2 * amountWeapon;
            } else if (uint256(weaponType) - 10 == 1) {
                amounts[0] = 9 * amountWeapon;
                amounts[1] = 3 * amountWeapon;
            } else {
                amounts[0] = 10 * amountWeapon;
                amounts[1] = 5 * amountWeapon;
            }
        } else {
            ids[1] = 18;

            if (uint256(weaponType) - 13 == 0) {
                amounts[0] = 11 * amountWeapon;
                amounts[1] = 5 * amountWeapon;
            } else if (uint256(weaponType) - 13 == 1) {
                amounts[0] = 12 * amountWeapon;
                amounts[1] = 5 * amountWeapon;
            } else {
                amounts[0] = 14 * amountWeapon;
                amounts[1] = 6 * amountWeapon;
            }
        }

        return (ids, amounts);
    }

    function upgradeDefance() external returns (uint256, uint8) {
        require(uint256(clans[msg.sender].defance) < 5, "maximum defance type");

        uint256 amountInFood = clans[msg.sender].population *
            (10**uint256(clans[msg.sender].defance + 1));

        clans[msg.sender].defance++;

        return (amountInFood, clans[msg.sender].defance);
    }

    function attactToClan(
        address defenderAccount,
        uint256 _clanId,
        uint256 amountPrimate,
        weaponTypes[] memory weapons,
        uint256[] memory weaponsAmount
    ) external returns (bool) {
        require(shield[_clanId] < block.timestamp, "defender has shield");
        require(
            weapons.length == weaponsAmount.length,
            "weapon typles and weapon amounts must be same length"
        );

        clan storage attackerPrimates = clans[msg.sender];
        clan storage defenderPrimates = clans[defenderAccount];

        require(
            amountPrimate <= attackerPrimates.population,
            "amountPrimate exceeds clan population"
        );

        require(attackerPrimates.morale > 10, "morale must be above 10");

        uint256 totalWeaponPower;
        uint256 weaponCount;

        for (uint256 i = 0; i < weapons.length; i++) {
            totalWeaponPower +=
                (uint256(weapons[i]) * 2 * weaponsAmount[i]) -
                weaponsAmount[i];

            weaponCount += weaponsAmount[i];

            weaponTechLevel(attackerPrimates.techLevel, weapons[i]);
        }

        require(
            weaponCount <= amountPrimate,
            "weapon can not be greater than primates"
        );

        uint256 attackerSeasonEffect = (
            primatesSeasonWarPower[
                uint256(attackerPrimates.primateLevel.primateType)
            ][season() * 2]
        );

        uint256 defenderSeasonEffect = (
            primatesSeasonWarPower[
                uint256(defenderPrimates.primateLevel.primateType)
            ][season() * 2 + 1]
        );

        uint256 attackPower = attackerPrimates
            .primateLevel
            .intelligence
            .warPower(
                attackerPrimates.primateLevel.durability,
                (amountPrimate + totalWeaponPower),
                attackerPrimates.morale,
                20 - attackerSeasonEffect
            );
        uint256 defancerPower = defenderPrimates.population.warPower(
            defenderPrimates.primateLevel.intelligence,
            defenderPrimates.primateLevel.durability +
                uint256(defenderPrimates.defance) *
                2,
            defenderPrimates.morale,
            20 - defenderSeasonEffect
        );

        if (attackPower > defancerPower) {
            uint256 _rate = (attackPower * 100) / defancerPower;

            if (_rate >= 200) {
                attackerPrimates.population -= amountPrimate.rate(5);
                defenderPrimates.population -= defenderPrimates.population.rate(
                    30
                );
            } else {
                attackerPrimates.population -= _rate.primateWarLost(
                    amountPrimate,
                    true
                );
                defenderPrimates.population -= _rate.primateWarLost(
                    defenderPrimates.population,
                    false
                );
            }
            shield[defenderPrimates.id] = block.timestamp + 1209600;
            return true;
        } else if (defancerPower > attackPower) {
            uint256 _rate = (defancerPower * 100) / attackPower;
            if (_rate >= 200) {
                defenderPrimates.population -= defenderPrimates.population.rate(
                    5
                );
                attackerPrimates.population -= amountPrimate.rate(30);
            } else {
                defenderPrimates.population -= _rate.primateWarLost(
                    defenderPrimates.population,
                    true
                );
                attackerPrimates.population -= _rate.primateWarLost(
                    amountPrimate,
                    false
                );
            }
            return false;
        } else {
            attackerPrimates.population -= amountPrimate.rate(20);
            defenderPrimates.population -= defenderPrimates.population.rate(20);

            return false;
        }
    }

    function weaponTechLevel(uint256 techLevel, weaponTypes weapon)
        private
        pure
        returns (uint256)
    {
        uint256 _weaponTechLevel = uint8(weapon) / 3;

        if (uint8(weapon) % 3 > 0) {
            _weaponTechLevel++;
        }
        require(techLevel >= _weaponTechLevel, "not enough tech level");
        return _weaponTechLevel;
    }
}

