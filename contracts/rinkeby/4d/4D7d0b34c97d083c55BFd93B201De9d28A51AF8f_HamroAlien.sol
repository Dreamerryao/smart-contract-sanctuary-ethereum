  function tokenURI(uint256 tokenId) override public view returns (string memory) {
    require(_exists(tokenId),"Token doesnot exist");
    Alien memory alien = randomOne(tokenId);
    return string(abi.encodePacked(rl4,Base64.encode(bytes(string(abi.encodePacked
    (rl1,tokenId.toString(),
    getTraits(alien),
    Base64.encode(bytes(genSVG(alien))),
    rl3))))));
  }

    //Contract state , 0=>paused, 1=>presale , 2=> public sale

  function claim(uint state,bool isCommunity, uint numberOfToken,bytes32[] calldata _merkleProof) public nonReentrant {

      require(state!=0);

      if(state==1){
          //If it is a presale .... 2 possibilities either community or devs 
          require(contractState==1);
              bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
            if(isCommunity){
                require(uint(balanceOf(msg.sender)+numberOfToken)<=communityMintAmount,"Invalid claim");
                require(MerkleProof.verify(_merkleProof, communityMerkleRoot, leaf),"Unverified");
            }
            else{
                require(uint(balanceOf(msg.sender)+numberOfToken)<=whiteListMintAmount,"Invalid claim");
                require(MerkleProof.verify(_merkleProof, whitelistMerkleRoot, leaf),"Unverified");
            }
        
        
        _claim(numberOfToken);
     
      }
      else if(state==2){
        require(contractState==2);
        require((balanceOf(msg.sender)+numberOfToken)<=publicMintAmount,"Invalid claim");
      _claim(numberOfToken);
      }
     // require(contractState==2,"No public");
    //   require((AliensPerWallet[msg.sender]+numberOfToken)<=publicMintAmount);
    //   _claim(numberOfToken);
    
  }
