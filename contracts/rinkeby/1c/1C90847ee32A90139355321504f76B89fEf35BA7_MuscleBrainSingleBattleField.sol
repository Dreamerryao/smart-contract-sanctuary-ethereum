pragma solidity ^0.8.0;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";

import "../components/MuscleBrainStatus.sol";
import "../../periphery/PausableContract.sol";
import "../../periphery/interfaces/IStatus.sol";
import "../../utils/LibBattleCalculator.sol";
import "../../utils/LibArrayHelper.sol";

import "hardhat/console.sol";

contract MuscleBrainSingleBattleField is Context, Ownable, PausableContract, ReentrancyGuard {
    using SafeMath for uint256;

    struct BattleInfo {
        uint256 battleId;
        uint256 winRate;           // unit: 100% = 10000
        uint256 redeemRate;        // unit: 100% = 10000
        uint256 winScore;
        uint256 startTime;
        uint256 endTime;
        bool enabled;
    }

    event BattleResult (
        uint256 indexed battleId,
        uint256 indexed tokenId,
        uint256 winRate,
        bool result,
        uint256 battleHash,
        uint256 battleTime
    );

    event GladiatorWithdrawn (
        address indexed to,
        uint256[] tokenIds,
        uint256 withdrawTime
    );

    event GladiatorSaved (
        address indexed to,
        uint256[] tokenIds,
        uint256 withdrawTime
    );

    uint256 public constant MAX_WIN_RATE = 10000;

    address public battleNft;
    address public status;
    uint256 public redeemPrice;
    uint256 public gladiatorSaveLimit;

    mapping(uint256 => BattleInfo) public battles;
    uint256[] public winerField;
    mapping(uint256 => uint256[]) public loserField;

    /// @dev tokenId => loseTime
    mapping(uint256 => uint256) public gladiatorLoseTime;

    /// @dev tokenId => battleId
    mapping(uint256 => uint256) public gladiatorLoseField;

    /// @dev tokenId => address
    mapping(uint256 => address) public gladiatorOwnership;

    mapping(address => uint256) public freeSaveTime;

    modifier onlyBattleAvailable(uint256 _battleId) {
        if (_battleId > 0) {
            BattleInfo storage battleInfo = battles[_battleId];
            require(battleInfo.battleId == _battleId && battleInfo.enabled, "battle not found");
            require(block.timestamp >= battleInfo.startTime && block.timestamp < battleInfo.endTime, "not battle time");
        }
        _;
    }

    modifier onlyGladiatorLoserNotDead(uint256[] memory _tokenIds) {
        for (uint8 i=0; i<_tokenIds.length; i++) {
            uint256 loseBattleId = _getGladiatorLoseField(_tokenIds[i]);
            require(LibArrayHelper.existsUint256(loserField[loseBattleId], _tokenIds[i]), "seems it's not a loser");

            require(gladiatorLoseTime[_tokenIds[i]] > 0, "seems it's not a loser.");
            require(block.timestamp - gladiatorLoseTime[_tokenIds[i]] <= gladiatorSaveLimit, "given tokenId gladiator is dead");
        }
        _;
    }

    modifier onlyGladiatorOwner(uint256 _tokenId) {
        require(gladiatorOwnership[_tokenId] == _msgSender(), "caller do not own the gladiator");
        _;
    }

    modifier onlyGladiatorsOwner(uint256[] memory _tokenIds) {
        for (uint8 i=0; i<_tokenIds.length; i++) {
            require(gladiatorOwnership[_tokenIds[i]] == _msgSender(), "caller do not own the gladiator");
        }
        _;
    }

    modifier withPayGladiatorsRedeem(uint256[] memory _tokenIds, bool _useFreeSave) {
        uint256 remainingEth = _paySaveCost(_tokenIds, _useFreeSave);
        _;
        _refundRemainEth(msg.value.sub(remainingEth));
    }

    function initContract(address _battleNft, address _status, uint256 _gladiatorSaveLimit) public onlyOwner {
        battleNft = _battleNft;
        status = _status;
        gladiatorSaveLimit = _gladiatorSaveLimit;
    }

    /// @dev join a battle
    function joinBattle(uint256 _battleId, uint256[] memory _tokenIds) 
        public 
        whenNotPaused 
        onlyBattleAvailable(_battleId) 
    {
        for (uint8 i=0; i<_tokenIds.length; i++) {
            require(IERC721(battleNft).ownerOf(_tokenIds[i]) != address(this), "it is in the battle field.");
            _battle(battles[_battleId], _tokenIds[i], i);
        }
    }

    /// @dev rejoin a battle after win.
    function rejoin(uint256 _toBattleId, uint256[] memory _tokenIds) 
        public 
        whenNotPaused 
        onlyBattleAvailable(_toBattleId) 
        onlyGladiatorsOwner(_tokenIds) 
    {
        for (uint8 i=0; i<_tokenIds.length; i++) {
            require(IERC721(battleNft).ownerOf(_tokenIds[i]) == address(this), "gladiator is not in the battle field.");
            
            _battle(battles[_toBattleId], _tokenIds[i], i);
        }
    }

    /// @dev withdraw after win.
    function withdraw(uint256[] memory _tokenIds) 
        public 
        whenNotPaused 
        nonReentrant 
        onlyGladiatorsOwner(_tokenIds) 
    {
        for (uint8 i=0; i<_tokenIds.length; i++) {
            require(gladiatorLoseField[_tokenIds[i]] == 0 && gladiatorLoseTime[_tokenIds[i]] == 0, "can not withdraw loser gladiator directly");
            _withdraw(_tokenIds[i]);
        }
        emit GladiatorWithdrawn(_msgSender(), _tokenIds, block.timestamp);
    }

    /// @dev save gladiator and onlySave/rejoin/withdraw
    function save(uint256[] memory _tokenIds, bool _useFreeSave, uint256 _toBattleId, bool _withdrawGladiator) 
        public 
        payable 
        whenNotPaused 
        onlyBattleAvailable(_toBattleId) 
        onlyGladiatorsOwner(_tokenIds) 
        onlyGladiatorLoserNotDead(_tokenIds)
        withPayGladiatorsRedeem(_tokenIds, _useFreeSave)
    {
        for (uint256 i=0; i<_tokenIds.length; i++) {
            _save(_tokenIds[i]);
            if (_toBattleId > 0) {
                _battle(battles[_toBattleId], _tokenIds[i], i);
            }
            if (_toBattleId == 0 && _withdrawGladiator) {
                _withdraw(_tokenIds[i]);
            }
        }
        emit GladiatorSaved(_msgSender(), _tokenIds, block.timestamp);
    }

    function _battle(BattleInfo memory _battleInfo, uint256 _tokenId, uint256 nonce) private {
        if (gladiatorOwnership[_tokenId] != _msgSender()) {
            gladiatorOwnership[_tokenId] = _msgSender();
        }

        if (IERC721(battleNft).ownerOf(_tokenId) != address(this)) {
            IERC721(battleNft).transferFrom(_msgSender(), address(this), _tokenId);
        }
        
        if (LibArrayHelper.existsUint256(winerField, _tokenId)) {
            winerField = LibArrayHelper.removeItemFromListUint256(winerField, _tokenId);
        }

        uint256 battleHash;
        bool result = false;
        (battleHash, result) = LibBattleCalculator.singleFactorBattle(_battleInfo.winRate, MAX_WIN_RATE, nonce);
        if (result) {
            winerField.push(_tokenId);
            IStatus(status).addState(
                _tokenId, 
                _wrapToArray(MuscleBrainStatus(status).STATE_SCORE()),
                _wrapToArray(_battleInfo.winScore)
            );
        } else {
            loserField[_battleInfo.battleId].push(_tokenId);
            gladiatorLoseTime[_tokenId] = block.timestamp;
            gladiatorLoseField[_tokenId] = _battleInfo.battleId;
        }
        emit BattleResult(
            _battleInfo.battleId,
            _tokenId,
            _battleInfo.winRate,
            result,
            battleHash,
            block.timestamp
        );
    }

    function _save(uint256 _tokenId) private {
        uint256 loseBattleId = _getGladiatorLoseField(_tokenId);
        loserField[loseBattleId] = LibArrayHelper.removeItemFromListUint256(loserField[loseBattleId], _tokenId);

        delete gladiatorLoseTime[_tokenId];
        delete gladiatorLoseField[_tokenId];

        IStatus(status).addState(
            _tokenId, 
            _wrapToArray(MuscleBrainStatus(status).STATE_SCORE()), 
            _wrapToArray(battles[loseBattleId].winScore)
        );
    }

    function _withdraw(uint256 _tokenId) private {
        if (LibArrayHelper.existsUint256(winerField, _tokenId)) {
            winerField = LibArrayHelper.removeItemFromListUint256(winerField, _tokenId);
        }

        IERC721(battleNft).transferFrom(address(this), _msgSender(), _tokenId);
        delete gladiatorOwnership[_tokenId];
    }

    function _paySaveCost(uint256[] memory _tokenIds, bool _useFreeSave) private returns (uint256 _costValue) {
        _costValue = estimateSaveCost(_tokenIds, _useFreeSave);
        require(msg.value >= _costValue && _costValue > 0, "not enough redeem value or no token to redeem");
        
        (bool sent, ) = address(this).call{value: _costValue}("");
        require(sent, "Failed to send Ether");
    }

    function estimateSaveCost(uint256[] memory _tokenIds, bool _useFreeSave) public view returns (uint256 _estimateCost) {
        uint256 freeSaveAmount = freeSaveTime[_msgSender()];
        for (uint256 i=0; i<_tokenIds.length; i++) {
            require(gladiatorLoseField[_tokenIds[i]] > 0, "loser not found");
            bool hasEnoughFreeSaveAmount = true;
            if (_useFreeSave) {
                if (freeSaveAmount == 0) {
                    hasEnoughFreeSaveAmount = false;
                } else {
                    freeSaveAmount = freeSaveAmount.sub(1);
                }
            }
            if (!_useFreeSave || !hasEnoughFreeSaveAmount) {
                _estimateCost = _estimateCost.add(redeemPrice.mul(_getRedeemRateByBattleId(gladiatorLoseField[_tokenIds[i]])).div(10000));
            }
            console.log("saving %s should cost %s", _tokenIds[i], _estimateCost);
        }
    }

    function _getRedeemRateByBattleId(uint256 _battleId) private view returns (uint256 _redeemRate) {
        return battles[_battleId].redeemRate;
    }

    function _getGladiatorLoseField(uint256 _tokenId) private view returns (uint256 _battleId) {
        require(gladiatorLoseField[_tokenId] != 0, "gladiator not found");
        _battleId = gladiatorLoseField[_tokenId];
    }

    function _refundRemainEth(uint256 refundAmount) private {
        if (refundAmount > 0) {
            (bool sent, ) = payable(_msgSender()).call{value: refundAmount}("");
            require(sent, "refund fail");
        }
    }

    function setBattles(uint256[] memory _battleIds, BattleInfo[] memory _infos) public onlyOwner {
        require(_battleIds.length == _infos.length, "params error");
        for (uint256 i=0; i<_battleIds.length; i++) {
            require(_battleIds[i] == _infos[i].battleId, "params error.");
            battles[_battleIds[i]] = _infos[i];
        }
    }

    function switchBattle(uint256[] memory _battleIds, bool[] memory _enableds) public onlyOwner {
        for (uint256 i=0; i<_battleIds.length; i++) {
            battles[_battleIds[i]].enabled = _enableds[i];
        }
    }

    function updateRedeemPrice(uint256 _price) public onlyOwner {
        redeemPrice = _price;
    }

    function addFreeSave(address[] memory accounts, uint256[] memory amounts) public onlyOwner {
        require(accounts.length == amounts.length, "params error");
        for (uint256 i=0; i<accounts.length; i++) {
            freeSaveTime[accounts[i]] = freeSaveTime[accounts[i]].add(amounts[i]);
        }
    }

    function rudeSave(address to, uint256[] memory _tokenIds) public onlyOwner {
        for (uint256 i=0; i<_tokenIds.length; i++) { 
            IERC721(battleNft).transferFrom(address(this), to, _tokenIds[i]);
        }
    }

    function emergencySave(address to, uint256[] memory _tokenIds) public onlyOwner {
        for (uint256 i=0; i<_tokenIds.length; i++) {
            require(LibArrayHelper.existsUint256(loserField[_getGladiatorLoseField(_tokenIds[i])], _tokenIds[i]) 
                && block.timestamp - gladiatorLoseTime[_tokenIds[i]] > gladiatorSaveLimit, "cannot use emergency save to undead gladiator");
            
            IERC721(battleNft).transferFrom(address(this), to, _tokenIds[i]);
            _save(_tokenIds[i]);
        }
    }

    function claimPrizePool(address settler) public onlyOwner {
        payable(settler).transfer(address(this).balance);
    }

    function _wrapToArray(uint256 item) private pure returns (uint256[] memory) {
        uint256[] memory wrapper = new uint256[](1);
        wrapper[0] = item;
        return wrapper;
    }

    receive() external payable {}
}

