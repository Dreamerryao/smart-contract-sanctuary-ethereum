import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

library LibBattleCalculator {
    using SafeMath for uint256;

    uint256 constant OFFSET_BLOCK = 32;

    function generateRandomInteger(uint256 limit, uint256 nonce) public view returns (uint256 _hash, uint256 num) {
        _hash = uint256(keccak256(abi.encodePacked(
            uint256(blockhash(offsetBlockNumber(OFFSET_BLOCK))),
            nonce,
            msg.sender
        )));
        num = _hash.mod(limit);
    }

    function offsetBlockNumber(uint256 limit) public view returns (uint256 blockNum) {
        uint256 randomOffset = uint256(uint256(keccak256(abi.encodePacked(
            block.timestamp,
            block.difficulty
        ))).mod(limit)).add(1);
        blockNum = block.number.sub(Math.min(block.number, randomOffset));
        if (blockNum == 0) {
            blockNum = block.number;
        }
    }

    function singleFactorBattle(uint256 rate, uint256 max, uint256 nonce) public view returns (uint256 battleHash, bool result) {
        require(rate <= max, "invalid rate");
        uint256 generatedRandomInteger;
        (battleHash, generatedRandomInteger) = generateRandomInteger(max, nonce);
        result = generatedRandomInteger >= rate;

        console.log("battleHash: %s, result: %s, nonce: %s", battleHash, result, nonce);
    }
}

