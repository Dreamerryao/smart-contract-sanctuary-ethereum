import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Status is Context, Ownable, ReentrancyGuard, AccessControlEnumerable {
    using SafeMath for uint256;

    bytes32 public constant ACTIVE_FIELD = keccak256("ACTIVE_FIELD");

    mapping(uint256 => mapping(uint256 => uint256)) public state;

    event StatusGrowth(
        uint256 indexed target,
        uint256[] attributes,
        uint256[] amounts
    );

    event StatusDeleted(
        uint256 indexed target,
        uint256[] attributes,
        uint256[] values
    );

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function getState(uint256 attribute, uint256 target) public view returns (uint256) {
        return state[attribute][target];
    }

    function addState(uint256 target, uint256[] memory attributes, uint256[] memory amounts) 
        public 
        nonReentrant 
        onlyRole(ACTIVE_FIELD) 
    {
        require(attributes.length == amounts.length, "state error");
        for (uint256 i=0; i<attributes.length; i++) {
            state[attributes[i]][target] = state[attributes[i]][target].add(amounts[i]);
        }
        emit StatusGrowth(target, attributes, amounts);
    }

    function deleteState(uint256 target, uint256[] memory attributes) 
        public 
        nonReentrant 
        onlyRole(ACTIVE_FIELD) 
    {
        uint256[] memory values = new uint256[](attributes.length);
        for (uint8 i=0; i<attributes.length; i++) {
            values[i] = state[attributes[i]][target];
            delete state[attributes[i]][target];
        }
        emit StatusDeleted(target, attributes, values);
    }

    function emergencySetState(uint256 target, uint256[] memory attributes, uint256[] memory _states)
        public 
        onlyOwner
    {
        require(attributes.length == _states.length, "params error");
        for (uint8 i=0; i<attributes.length; i++) {
            state[attributes[i]][target] = _states[i];
        }
    }
}

