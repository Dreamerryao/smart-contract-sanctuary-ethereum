pragma solidity =0.8.9;

import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";

/**
 * Eternal storage of medical data.
 * You can access medical data through medical data manager contract.
 */
contract MedicalDataStorage {
    struct MedicalData {
        address payable patient;
        string name;
        string sex;
        string bloodType;
    }

    Counters.Counter private idCounter;
    using Counters for Counters.Counter;
    address private owner;
    address private manager;
    mapping(uint256 => MedicalData) private medicalData;
    mapping(uint256 => address payable[]) private approvedMedicalProviders;

    /**
     * Only constructor can set owner
     */
    constructor(address _owner) {
        owner = _owner;
    }

    /**
     * Accessible only by owner
     */
    modifier onlyOwner() {
        require(msg.sender == owner, "Only owner can call this function");
        _;
    }

    /**
     * Accessible only by manager
     */
    modifier onlyManager() {
        require(msg.sender == manager, "Only manager can call this function");
        _;
    }

    /**
     * Set medical data manager address.
     * Only owner can call it.
     */
    function setManagerAddress(address _manager) external onlyOwner {
        manager = _manager;
        emit SetManagerAddress(manager);
    }

    /**
     * Create medical data.
     * Only manager can call it.
     * All parameters except id can be registered, id is setted by contract automatically.
     * The results can be seen in the emit event of CreateMedicalData.
     */
    function createMedicalData(
        address payable _patient,
        string memory _name,
        string memory _sex,
        string memory _bloodType
    ) external onlyManager {
        idCounter.increment();
        medicalData[idCounter.current()].patient = _patient;
        medicalData[idCounter.current()].name = _name;
        medicalData[idCounter.current()].sex = _sex;
        medicalData[idCounter.current()].bloodType = _bloodType;

        emit CreateMedicalData(idCounter.current(), msg.sender);
    }

    /**
     * Select medical data.
     * Only manager can call it.
     */
    function selectMedicalDataById(uint256 _id)
        external
        view
        onlyManager
        returns (MedicalData memory)
    {
        return medicalData[_id];
    }

    /**
     * Select id of medical data by patient.
     * Only manager can call it.
     */
    function selectIdByPatient(address _patient)
        external
        view
        onlyManager
        returns (uint256)
    {
        for (uint256 _id = 1; _id <= idCounter.current(); _id++) {
            if (medicalData[_id].patient == _patient) {
                return _id;
            }
        }
        return 0;
    }

    /**
     * Update medical data.
     * Only manager can call it.
     * All parameters except id can be updated.
     * The results can be seen in the emit event of UpdateMedicalData.
     */
    function updateMedicalData(
        uint256 _id,
        address payable _patient,
        string memory _name,
        string memory _sex,
        string memory _bloodType
    ) external onlyManager {
        medicalData[_id].patient = _patient;
        medicalData[_id].name = _name;
        medicalData[_id].sex = _sex;
        medicalData[_id].bloodType = _bloodType;
        emit UpdateMedicalData(_id, msg.sender);
    }

    /**
     * Delete medical data.
     * Only manager can call it.
     * The results can be seen in the emit event of DeleteMedicalData.
     */
    function deleteMedicalData(uint256 _id) external onlyManager {
        delete medicalData[_id];
        emit DeleteMedicalData(_id, msg.sender);
    }

    /**
     * Update medical data.
     * Only manager can call it.
     * Add specified medical provider to approved list.
     * The results can be seen in the emit event of PushApprovedMedicalProvider.
     */
    function pushApprovedMedicalProvider(
        uint256 _id,
        address payable _medicalProvider
    ) external onlyManager {
        approvedMedicalProviders[_id].push(_medicalProvider);
        emit PushApprovedMedicalProvider(_id, msg.sender);
    }

    /**
     * Update medical data.
     * Only manager can call it.
     * Remove specified medical provider from approved list.
     * The results can be seen in the emit event of RemoveApprovedMedicalProvider.
     */
    function removeApprovedMedicalProvider(
        uint256 _id,
        address payable _medicalProvider
    ) external onlyManager {
        address payable[] memory old = approvedMedicalProviders[_id];
        delete approvedMedicalProviders[_id];
        for (uint256 i = 0; i < old.length; i++) {
            if (old[i] != _medicalProvider) {
                approvedMedicalProviders[_id].push(old[i]);
            }
        }
        emit RemoveApprovedMedicalProvider(_id, msg.sender);
    }

    /**
     * Select approved medical provider addresses.
     * Only manager can call it.
     */
    function getApprovedMedicalProvidersById(uint256 _id)
        external
        view
        onlyManager
        returns (address payable[] memory)
    {
        return approvedMedicalProviders[_id];
    }

    event CreateMedicalData(uint256 id, address manager);
    event UpdateMedicalData(uint256 id, address manager);
    event DeleteMedicalData(uint256 id, address manager);
    event PushApprovedMedicalProvider(uint256 id, address manager);
    event RemoveApprovedMedicalProvider(uint256 id, address manager);
    event SetManagerAddress(address manager);
}

