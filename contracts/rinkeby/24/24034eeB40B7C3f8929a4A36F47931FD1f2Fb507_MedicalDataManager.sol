pragma solidity =0.8.9;

import "hardhat/console.sol";
import "../storage/MedicalDataStorage.sol";

/**
 * Manager contract of medical data storage.
 * You can access medical data through this contract.
 */
contract MedicalDataManager {
    address private owner;
    address private eternalStorage;

    /**
     * Only constructor can set owner
     */
    constructor(address _owner) {
        owner = _owner;
    }

    /**
     * Accessible only by owner
     */
    modifier onlyOwner() {
        require(msg.sender == owner, "Only owner can call this function");
        _;
    }

    /**
     * Accessible only by not registered patient
     */
    modifier onlyNotRegisteredPatient(address _patient) {
        require(
            MedicalDataStorage(eternalStorage)
                .selectMedicalDataById(
                    MedicalDataStorage(eternalStorage).selectIdByPatient(
                        _patient
                    )
                )
                .patient == address(0),
            "Patient is already registered"
        );
        _;
    }

    /**
     * Accessible only by registered patient
     */
    modifier onlyRegisteredPatient(address _patient) {
        require(
            MedicalDataStorage(eternalStorage)
                .selectMedicalDataById(
                    MedicalDataStorage(eternalStorage).selectIdByPatient(
                        _patient
                    )
                )
                .patient == _patient,
            "Patient is not registered"
        );
        _;
    }

    /**
     * Accessible only by not approved medical provider
     */
    modifier onlyNotApprovedMedicalProvider(
        address _patient,
        address _medicalProvider
    ) {
        address payable[] memory _approvedMedicalProviders = MedicalDataStorage(
            eternalStorage
        ).getApprovedMedicalProvidersById(
                MedicalDataStorage(eternalStorage).selectIdByPatient(_patient)
            );
        for (uint256 i = 0; i < _approvedMedicalProviders.length; i++) {
            require(
                _approvedMedicalProviders[i] != _medicalProvider,
                "Medical provider is already approved"
            );
        }
        _;
    }

    /**
     * Accessible only by approved medical provider
     */
    modifier onlyApprovedMedicalProvider(
        address _patient,
        address _medicalProvider
    ) {
        address payable[] memory _approvedMedicalProviders = MedicalDataStorage(
            eternalStorage
        ).getApprovedMedicalProvidersById(
                MedicalDataStorage(eternalStorage).selectIdByPatient(_patient)
            );
        bool _isApproved = false;
        for (uint256 i = 0; i < _approvedMedicalProviders.length; i++) {
            if (_approvedMedicalProviders[i] == _medicalProvider) {
                _isApproved = true;
                break;
            }
        }
        require(_isApproved, "Medical provider is not approved");
        _;
    }

    /**
     * Accessible only by registered patient or approved medical provider
     */
    modifier onlyRegisteredPatientOrApprovedMedicalProvider(
        uint256 _id,
        address sender
    ) {
        bool _isPatient = false;
        bool _isMedicalProvider = false;
        if (
            MedicalDataStorage(eternalStorage)
                .selectMedicalDataById(_id)
                .patient == sender
        ) {
            _isPatient = true;
        }
        address payable[] memory _medicalProviders = MedicalDataStorage(
            eternalStorage
        ).getApprovedMedicalProvidersById(_id);
        for (uint256 i = 0; i < _medicalProviders.length; i++) {
            if (_medicalProviders[i] == sender) {
                _isMedicalProvider = true;
                break;
            }
        }
        require(
            _isPatient || _isMedicalProvider,
            "Sender is not a patient or an approved medical provider"
        );
        _;
    }

    /**
     * Set medical data storage address.
     * Only owner can call it.
     */
    function setStorageAddress(address _eternalStorage) external onlyOwner {
        eternalStorage = _eternalStorage;
        emit SetStorageAddress(eternalStorage);
    }

    /**
     * Create medical data through medical data storage.
     * Only patient can call it.
     * Create medical data as init status, it means only name is setted.
     * The results can be seen in the emit event of RegisterMedicalData.
     */
    function registerMedicalData(string memory _name)
        external
        onlyNotRegisteredPatient(msg.sender)
    {
        MedicalDataStorage(eternalStorage).createMedicalData(
            payable(msg.sender),
            _name,
            "",
            ""
        );
        emit RegisterMedicalData(msg.sender);
    }

    /**
     * Update medical data through medical data storage.
     * Only patient can call it.
     * Add specified medical provider to approved list, approved medical provider can edit patient's medical data.
     * The results can be seen in the emit event of ApproveToEditMedicalData.
     */
    function approveToEditMedicalData(address payable _medicalProvider)
        external
        onlyRegisteredPatient(msg.sender)
        onlyNotApprovedMedicalProvider(msg.sender, _medicalProvider)
    {
        MedicalDataStorage(eternalStorage).pushApprovedMedicalProvider(
            MedicalDataStorage(eternalStorage).selectIdByPatient(msg.sender),
            _medicalProvider
        );
        emit ApproveToEditMedicalData(msg.sender, _medicalProvider);
    }

    /**
     * Update medical data through medical data storage.
     * Only patient can call it.
     * Remove specified medical provider from approved list, removed medical provider can't edit patient's medical data.
     * The results can be seen in the emit event of DisapproveToEditMedicalData.
     */
    function disapproveToEditMedicalData(address payable _medicalProvider)
        external
        onlyRegisteredPatient(msg.sender)
        onlyApprovedMedicalProvider(msg.sender, _medicalProvider)
    {
        MedicalDataStorage(eternalStorage).removeApprovedMedicalProvider(
            MedicalDataStorage(eternalStorage).selectIdByPatient(msg.sender),
            _medicalProvider
        );
        emit DisapproveToEditMedicalData(msg.sender, _medicalProvider);
    }

    /**
     * Update medical data through medical data storage.
     * Only medical provider can call it.
     * Update some parameter of medical data as a result of medical check.
     * The results can be seen in the emit event of EditMedicalData.
     */
    function editMedicalData(
        address payable _patient,
        string memory _sex,
        string memory _bloodType
    )
        external
        onlyRegisteredPatient(_patient)
        onlyApprovedMedicalProvider(_patient, msg.sender)
    {
        uint256 _id = MedicalDataStorage(eternalStorage).selectIdByPatient(
            _patient
        );
        MedicalDataStorage.MedicalData memory _medicalData = MedicalDataStorage(
            eternalStorage
        ).selectMedicalDataById(_id);
        MedicalDataStorage(eternalStorage).updateMedicalData(
            _id,
            _medicalData.patient,
            _medicalData.name,
            _sex,
            _bloodType
        );
        emit EditMedicalData(_patient, msg.sender);
    }

    /**
     * Select id of your medical data.
     * Only patient can call it.
     */
    function getIdForPatient() external view returns (uint256) {
        return MedicalDataStorage(eternalStorage).selectIdByPatient(msg.sender);
    }

    /**
     * Select your patient's id of medical data.
     * Only medical provider can call it.
     */
    function getIdForMedicalProvider(address _patient)
        external
        view
        returns (uint256)
    {
        return MedicalDataStorage(eternalStorage).selectIdByPatient(_patient);
    }

    /**
     * Select medical data.
     * Only approved medical provider and patient can call it.
     */
    function getMedicalData(uint256 _id)
        external
        view
        onlyRegisteredPatientOrApprovedMedicalProvider(_id, msg.sender)
        returns (MedicalDataStorage.MedicalData memory)
    {
        return MedicalDataStorage(eternalStorage).selectMedicalDataById(_id);
    }

    event SetStorageAddress(address storageAddress);
    event RegisterMedicalData(address patient);
    event ApproveToEditMedicalData(
        address patient,
        address approvedMedicalProvider
    );
    event DisapproveToEditMedicalData(
        address patient,
        address disapprovedMedicalProvider
    );
    event EditMedicalData(address patient, address edittedMedicalProvider);
}

