pragma solidity ^0.8.9;
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

contract LotteryGang  {

    struct Lottery{
        uint id;
        address payable Owner;
        uint ticketMax ; 
        address payable[] players; 
        uint totalCagnotte; 
        bool isCLosed;
        uint ticketPrice;
        address winner;
        address smartcontractNFT;
        uint idNFT;
    }

    bool LotteryClosed; 

    mapping(uint => Lottery) public Lotterys;
    uint idLottery; 


    address payable owner; 

    constructor (){
        owner = payable(msg.sender);
    }
     modifier onlyOwner(){
            require(msg.sender == owner, "you are not the owner");
            _;
        }

    function pickWinner(uint _idLottery) internal {
        require(Lotterys[_idLottery].players.length == Lotterys[_idLottery].ticketMax,"Lottery is not closed");
        require( Lotterys[_idLottery].isCLosed == false);
        uint winner = 1 ;// retour oracle 
        Lotterys[_idLottery].Owner.transfer(Lotterys[_idLottery].totalCagnotte / 100*90);
        IERC721(Lotterys[_idLottery].smartcontractNFT).approve(Lotterys[_idLottery].players[winner], Lotterys[_idLottery].idNFT);
        IERC721( Lotterys[_idLottery].smartcontractNFT).safeTransferFrom(address(this), Lotterys[_idLottery].players[winner], Lotterys[_idLottery].idNFT);
        owner.transfer(Lotterys[_idLottery].totalCagnotte/100*10);
        Lotterys[_idLottery].winner= Lotterys[_idLottery].players[winner];
        Lotterys[_idLottery].isCLosed == true;
        Lotterys[_idLottery].totalCagnotte = 0;
        }

    function enter(uint _idLottery, uint nbreTicket) external payable {
        require(Lotterys[_idLottery].isCLosed == false,"the lottery is closed");
        require(Lotterys[_idLottery].players.length < Lotterys[_idLottery].ticketMax, "every ticket has been sold");
        require(nbreTicket <= Lotterys[_idLottery].ticketMax -Lotterys[_idLottery].players.length,"Order less tickets " );
        require(msg.value == nbreTicket * Lotterys[_idLottery].ticketPrice , "not enought funds provided");
        for (uint i = 0;i < nbreTicket;i++){
        Lotterys[_idLottery].players.push(payable(msg.sender));
        }
        Lotterys[_idLottery].totalCagnotte = Lotterys[_idLottery].totalCagnotte+ msg.value;
        if (Lotterys[_idLottery].players.length == Lotterys[_idLottery].ticketMax){
            pickWinner(_idLottery);
        }
    }

    function getWinner(uint _idLottery) external view returns(address){
        return Lotterys[_idLottery].winner;
    }

    function getbalance(uint _idLottery)external onlyOwner view returns (uint){
        return  Lotterys[_idLottery].totalCagnotte ;
    }


    function LotteryIsItClose(uint _idLottery) external view returns(bool){
        return  Lotterys[_idLottery].isCLosed ;
    }


    function getPlayers(uint _idLottery) external view returns( address payable[] memory){
        return  Lotterys[_idLottery].players;
    }

    function getTicketPrice(uint _idLottery)external view returns(uint){
        return Lotterys[_idLottery].ticketPrice;
    }

    function getTotalbalance() external view onlyOwner returns(uint){
        return address(this).balance;
    }


    function refund(uint _idLottery) public {
        require(msg.sender == owner || msg.sender == Lotterys[_idLottery].Owner, "you are not a owner");
        //function refund tous les participants -gas fee 
        for (uint i; i<Lotterys[_idLottery].players.length; i++){
            // en suposant qu'il n' y a pas de gas fee 
          
            Lotterys[_idLottery].players[i].transfer(Lotterys[_idLottery].ticketPrice );
            Lotterys[_idLottery].totalCagnotte = Lotterys[_idLottery].totalCagnotte - Lotterys[_idLottery].ticketPrice;
        }
        Lotterys[_idLottery].isCLosed = true; 
    }

// regarder si ne pas mettre en onlyOwner si tout le monde peut créer une lottery risque de balagan avec le back et passé l'addresse du vendeur en argument 

    function CreateLottery( 
        uint ticketMax ,  
        uint ticketPrice, 
        address smartcontractNFT,
        uint idNFT
        ) public {
        address  payable[] memory players ;
        address payable winner;
        uint totalCagnotte = 0;
        bool isCLosed =false;
        IERC721(smartcontractNFT).safeTransferFrom(msg.sender,address(this),idNFT);
        Lottery memory lottery = Lottery(idLottery,payable(0x5B38Da6a701c568545dCfcB03FcB875f56beddC4),ticketMax,players,totalCagnotte,isCLosed,ticketPrice, winner,smartcontractNFT,idNFT);
        Lotterys[idLottery] = lottery;
        idLottery++;
    }
    function onERC721Received(address, address, uint256, bytes calldata) external pure returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

}

