// Contract that saves your lock and you can add or remove lp
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./utils/IBabyDogeV2Router02.sol";
import "./utils/IBabyDogeV2Factory.sol";
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

contract TreatLiquidity is Ownable {
    address public treatToken;
    address public router;
    using SafeMath for uint256;

    constructor(address _token, address _router) {
        treatToken = _token;
        router = _router;
    }

    mapping(address => uint256) public liquidityDeposits;

    // Receive ETH
    receive() external payable {}

    function addLiquidity(
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        uint256 deadline
    )
        external
        returns (
            uint256 amountA,
            uint256 amountB,
            uint256 liquidity
        )
    {
        (amountA, amountB, liquidity) = IBabyDogeV2Router02(router)
            .addLiquidity(
                treatToken,
                tokenB,
                amountADesired,
                amountBDesired,
                amountAMin,
                amountBMin,
                msg.sender,
                deadline
            );

        liquidityDeposits[msg.sender] = liquidityDeposits[msg.sender].add(
            liquidity
        );
    }

    function addLiquidityETH(
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountETHMin,
        uint256 deadline
    )
        external
        payable
        returns (
            uint256 amountToken,
            uint256 amountETH,
            uint256 liquidity
        )
    {
        IERC20(treatToken).transferFrom(
            msg.sender,
            address(this),
            amountTokenDesired
        );
        IERC20(treatToken).approve(router, amountTokenMin);
        (amountToken, amountETH, liquidity) = IBabyDogeV2Router02(router)
            .addLiquidityETH{value: msg.value}(
            treatToken,
            amountTokenDesired,
            amountTokenMin,
            amountETHMin,
            msg.sender,
            deadline
        );

        liquidityDeposits[msg.sender] = liquidityDeposits[msg.sender].add(
            liquidity
        );
    }

    function removeLiquidity(
        address tokenB,
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin,
        uint256 deadline
    ) external returns (uint256 amountA, uint256 amountB) {
        require(
            liquidityDeposits[msg.sender] >= liquidity,
            "Can only withdraw your liquidity"
        );
        liquidityDeposits[msg.sender] = liquidityDeposits[msg.sender].sub(
            liquidity
        );
        address pair = IBabyDogeV2Factory(IBabyDogeV2Router02(router).factory())
            .getPair(treatToken, tokenB);
        IERC20(pair).transferFrom(msg.sender, address(this), liquidity);
        IERC20(pair).approve(router, liquidity);
        (amountA, amountB) = IBabyDogeV2Router02(router).removeLiquidity(
            treatToken,
            tokenB,
            liquidity,
            amountAMin,
            amountBMin,
            address(this),
            deadline
        );
        IERC20(treatToken).transfer(msg.sender, amountA);
        IERC20(tokenB).transfer(msg.sender, amountB);
    }

    function removeLiquidityETH(
        uint256 liquidity,
        uint256 amountTokenMin,
        uint256 amountETHMin,
        uint256 deadline
    ) external returns (uint256 amountToken, uint256 amountETH) {
        require(
            liquidityDeposits[msg.sender] >= liquidity,
            "Can only withdraw your liquidity"
        );
        liquidityDeposits[msg.sender] = liquidityDeposits[msg.sender].sub(
            liquidity
        );

        address pair = IBabyDogeV2Factory(IBabyDogeV2Router02(router).factory())
            .getPair(treatToken, IBabyDogeV2Router02(router).WETH());
        IERC20(pair).transferFrom(msg.sender, address(this), liquidity);
        IERC20(pair).approve(router, liquidity);
        (amountToken, amountETH) = IBabyDogeV2Router02(router)
            .removeLiquidityETH(
                treatToken,
                liquidity,
                amountTokenMin,
                amountETHMin,
                address(this),
                deadline
            );
        payable(msg.sender).transfer(amountETH);
        IERC20(treatToken).transfer(msg.sender, amountToken);
    }
}

