interface UniswapV3PoolState {

    function slot0()
        external
        view
        returns (
            uint160 sqrtPriceX96,
            int24 tick,
            uint16 observationIndex,
            uint16 observationCardinality,
            uint16 observationCardinalityNext,
            uint8 feeProtocol,
            bool unlocked
        ); 

}

interface NonfungiblePositionManager {
    function ownerOf(uint256 tokenId) external view returns (address owner);
    function positions(uint256 tokenId)
        external
        view
        returns (
            uint96 nonce,
            address operator,
            address token0,
            address token1,
            uint24 fee,
            int24 tickLower,
            int24 tickUpper,
            uint128 liquidity,
            uint256 feeGrowthInside0LastX128,
            uint256 feeGrowthInside1LastX128,
            uint128 tokensOwed0,
            uint128 tokensOwed1
        );
}

contract PFanStaking is Ownable {
    using SafeMath for uint256;

    NonfungiblePositionManager nPositionsManager;

    struct TxData {
        uint256 tokenId;
        uint256 timestamp;
        string txHash;
    }

    struct LastClaimed {
        uint256 timestamp;
        uint256 amount;
        uint256 daysStaked;
        bool saved;
    }

    struct RewardBundle {
        uint256 thirtyDay;
        uint256 sixtyDay;
        uint256 ninetyDay;
    }

    struct StartReward {
        uint256 timestamp;
        uint256 ethAmount;
        uint256 pfanTokens;
        RewardBundle rewards;
        bool started;
    }

    // tokenId => LastClaimed
    mapping(uint256 => LastClaimed) public lastClaimedArray;

    // tokenId => startReward
    mapping(uint256 => StartReward) public startRewardsArray;

    event StartRewards(address indexed stakerAddress, uint256 indexed tokenID);
    event UpdateTokenPrice(uint256 indexed tokenPrice);
    event AddTokens(uint256 indexed tokenAmount);
    event TakeOutTokens(uint256 indexed tokenAmount);
    event UpdatePoolAddresss(address indexed poolAddress);
    event UpdatePositionManagerAddresss(address indexed positionManagerAddress);
    event WithdrawReward(address operator, uint256 amount, uint256 daysStaked);
    event UpdateERCTokenAddress(address tokenAddress);

    /* Initial values to be updated by owner*/
    IERC20 public token;
    address public ercTokenAddress;
    uint256 public PFAN_PRICE;
    uint256 public COOL_DOWN_DAYS;
    address public uniswapPositionManagerAddress;
    address public wrappedEthAddress;

    constructor(address _Pfantoken) public {
        ercTokenAddress = _Pfantoken;
        token = IERC20(_Pfantoken);

        uniswapPositionManagerAddress = 0xC36442b4a4522E871399CD717aBDD847Ab11FE88;
        wrappedEthAddress = 0xc778417E063141139Fce010982780140Aa0cD5Ab;
        nPositionsManager = NonfungiblePositionManager(
            uniswapPositionManagerAddress
        );

        PFAN_PRICE = 14000000000000; // https://eth-converter.com/
        COOL_DOWN_DAYS = 0; // 7
    }

    //-------------------------------------------------------------------------
    // PUBLIC FUNCTIONS
    //-------------------------------------------------------------------------

    function getPositionData(uint256 tokenId)
        public
        view
        returns (
            uint96 nonce,
            address operator,
            address token0,
            address token1,
            uint24 fee,
            int24 tickLower,
            int24 tickUpper,
            uint128 liquidity,
            uint256 feeGrowthInside0LastX128,
            uint256 feeGrowthInside1LastX128,
            uint128 tokensOwed0,
            uint128 tokensOwed1
        )
    {
        return nPositionsManager.positions(tokenId);
    }

//get principle amount from LiquidityAmounts library
    function principal(
        NonfungiblePositionManager positionManager,
        uint256 tokenId,
        uint160 sqrtRatioX96
    ) public view returns (uint256 amount0, uint256 amount1) {
        (, , , , , int24 tickLower, int24 tickUpper, uint128 liquidity, , , , ) = nPositionsManager.positions(tokenId);

        return
            LiquidityAmounts.getAmountsForLiquidity(
                sqrtRatioX96,
                TickMath.getSqrtRatioAtTick(tickLower),
                TickMath.getSqrtRatioAtTick(tickUpper),
                liquidity
            );
    }

    /* @notice Get tokens amount from Uniswap libraries
     * @param tokenId The id of the position
        @param poolAddress is different for every single NFT! 
     */
    function getTokensAmount(uint256 tokenId, address poolAddress)
        public
        view
        returns (uint256, uint256)
    {
        (uint160 sqrtPriceX96, , , , , , ) = UniswapV3PoolState(poolAddress)
            .slot0(); // Pool address is different for every single NFT!

        return principal(nPositionsManager, tokenId, sqrtPriceX96);
    }

    // Total amount of all tokens
    function getContractTokensAmount() public view returns (uint256) {
        return token.balanceOf(address(this));
    }

    // Check if started NFT
    function checkIfStartedRewardsForNFT(uint256 tokenId)
        public
        view
        returns (bool)
    {
        return (startRewardsArray[tokenId].started == true);
    }

    function calculateInterestByDaysStaked(
        uint256 tokenWETH,
        address poolAddress,
        uint256 daysStaked
    ) public view returns (uint256) {

        uint256 interest = InterestMath.getCompoundInterest(
            tokenWETH,
            daysStaked,
            PFAN_PRICE
        );

        return interest;
    }

    function startEarningInterest(uint256 tokenId, address poolAddress) public {
        // Check token owner
        require(
            _msgSender() == nPositionsManager.ownerOf(tokenId) ||
                _msgSender() == owner(),
            "Not Position Owner"
        );

//(uint96 nonce, address operator, address token0, address token1, uint24 fee, int24 tickLower, int24 tickUpper, uint128 liquidity, uint256 feeGrowthInside0LastX128, uint256 feeGrowthInside1LastX128, uint128 tokensOwed0, uint128 tokensOwed1)
        (,,address token0,address token1,,,,,,,,)=nPositionsManager.positions(tokenId);

        //check if wrapped eth pair
        require(wrappedEthAddress == token0 || wrappedEthAddress == token1);

        // Get user token pool amount via NFT id
        (uint256 tokenWeth, uint256 pfanTokens) = getTokensAmount(
            tokenId,
            poolAddress
        );

        uint256 tokenEthAmount;
        uint256 pfans; 

        if(tokenWeth < pfanTokens) {
             tokenEthAmount = tokenWeth;
             pfans = pfanTokens;
         } else {
             tokenEthAmount = pfanTokens;
             pfans = tokenWeth;
         }

        uint256 thirty = InterestMath.getCompoundInterest(
            tokenEthAmount,
            30,
            PFAN_PRICE
        );

        uint256 sixty = InterestMath.getCompoundInterest(
            tokenEthAmount,
            60,
            PFAN_PRICE
        );

        uint256 ninety = InterestMath.getCompoundInterest(
            tokenEthAmount,
            90,
            PFAN_PRICE
        );

        // Later when claim reward make sure the eth amount is still there and not over before after lowerside divided by 2.
        startRewardsArray[tokenId] = StartReward(
            block.timestamp,
            tokenEthAmount,
            pfans,
            RewardBundle(thirty, sixty, ninety),
            true
        );

        emit StartRewards(_msgSender(), tokenId);
    }

    // Get days staked from an actual reward timestamp
    function getDaysStaked(uint256 tokenId) public view returns (uint256) {
        uint256 _timestamp;

        if (lastClaimedArray[tokenId].saved) {
            _timestamp = lastClaimedArray[tokenId].timestamp;
        } else {
            _timestamp = startRewardsArray[tokenId].timestamp;
        }

        uint256 daysStaked = calculateDays(_timestamp);

        return daysStaked;
    }

    /* @notice Withdraw reward tokens
     * @param data contains token id, timestamp, transaction hash
     * @param signature of data signed by user
     */
    function withdrawReward(uint256 tokenId, address poolAddress) public {
        // Check owner
        require(
            _msgSender() == nPositionsManager.ownerOf(tokenId) ||
                _msgSender() == owner(),
            "Not Position Owner"
        );

        // Check started rewards
        require(startRewardsArray[tokenId].started == true, "NOT_YET_STARTED");

        //(uint256 tokenWETH, uint256 pfanAmount) = getTokensAmount(tokenId, poolAddress);
        uint256 ethAmount = startRewardsArray[tokenId].ethAmount;
         
        // Check there are enough ETH staked
        require(ethAmount > 0, "NOT_ENOUGH_ETH_STAKED");

        // Get interest per days staked
        uint256 daysStaked = getDaysStaked(tokenId);

        uint256 interest = 0;

        if(daysStaked < 30) {
                interest = InterestMath.getCompoundInterest(
                            ethAmount,
                            daysStaked,
                            PFAN_PRICE
                        );
        } else if(daysStaked >= 30 && daysStaked < 60) {
                interest = startRewardsArray[tokenId].rewards.thirtyDay;
        } else if(daysStaked >= 60 && daysStaked < 90) {
            interest = startRewardsArray[tokenId].rewards.sixtyDay;
        } else if(daysStaked >= 90) {
            interest = startRewardsArray[tokenId].rewards.ninetyDay;
        }

        // Send rewards to the staker
        token.transfer(_msgSender(), interest);

        // Update lastClaimed mapping
        lastClaimedArray[tokenId] = LastClaimed(
            block.timestamp,
            interest,
            daysStaked,
            true
        );

        emit WithdrawReward(_msgSender(), interest, daysStaked);
    }

    //-------------------------------------------------------------------------
    // INTERNAL FUNCTIONS
    //-------------------------------------------------------------------------

    function calculateDays(uint256 depositTime)
        public
        view
        returns (uint256)
    {
        return block.timestamp.sub(depositTime).div(1 minutes); //todo days
    }

    function getChainId() internal view returns (uint256 chainId) {
        /* @dev - silence state mutability warning without generating bytecode
         * See https://github.com/ethereum/solidity/issues/2691
         */
        this;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            chainId := chainid()
        }
    }

    //-------------------------------------------------------------------------
    // ONLY OWNER FUNCTIONS
    //-------------------------------------------------------------------------

    // update wrappedEthAddress
    function updateWrappedEthAddress(address wEthAddress)
        public
        onlyOwner
    {
        wrappedEthAddress = wEthAddress;
    }

    // Owner can update position Manager address
    function updateUniswapPositionManagerAddress(address positionManagerAddress)
        public
        onlyOwner
    {
        uniswapPositionManagerAddress = positionManagerAddress;

        nPositionsManager = NonfungiblePositionManager(positionManagerAddress);
        emit UpdatePositionManagerAddresss(positionManagerAddress);
    }

    function updateErcTokenAddress(address _ercAddress) public onlyOwner {
        ercTokenAddress = _ercAddress;
        token = IERC20(_ercAddress);
        emit UpdateERCTokenAddress(_ercAddress);
    }

    // Owner can update updateTokenPriceForRewards
    function updateTokenPriceForRewards(uint256 tokenPrice) public onlyOwner {
        PFAN_PRICE = tokenPrice;

        emit UpdateTokenPrice(tokenPrice);
    }

    /* @notice Admin can add PowerFan tokens to smart contract
     * @dev Only admin can add reward PowerFan tokens
     * @param tokenAmount The amount admin want to add in smart contract
     */
    function addRewardTokens(uint256 tokenAmount) public onlyOwner {
        require(tokenAmount > 0, "SHOULD_NOT_ZERO");

        token.transferFrom(msg.sender, address(this), tokenAmount);

        emit AddTokens(tokenAmount);
    }

    // Owner take out tokens
    function ownerTakeOutRewardTokens(uint256 tokenAmount) public onlyOwner {
        require(tokenAmount > 0, "SHOULD_NOT_ZERO");

        token.transfer(msg.sender, tokenAmount);

        emit TakeOutTokens(tokenAmount);
    }

    // Owner take out all tokens
    function ownerTakeOutAllTokens() public onlyOwner {
        token.transfer(msg.sender, token.balanceOf(address(this))); //todo

        emit TakeOutTokens(token.balanceOf(address(this)));
    }

    // update days until next reward claim
    function updateCoolDown(uint256 _coolDown) public onlyOwner {
        COOL_DOWN_DAYS = _coolDown;
    }
}

