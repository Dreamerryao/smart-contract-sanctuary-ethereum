pragma solidity >=0.7.6;

import "@openzeppelin/contracts/math/SafeMath.sol";

library InterestMath {
    using SafeMath for uint256;
    using SafeMath for uint8;

    uint256 constant THIRTY_DAYS = 30;
    uint256 constant SIXTY_DAYS = 60;
    uint256 constant NINETY_DAYS = 90;

    function getCompoundInterest(
        uint256 ethAmount,
        uint256 daysStaked,
        uint256 pFanPrice
    ) internal pure returns (uint256) {
        //  Get number of PFANS
        uint256 quotient = ethAmount.mul(1e18).div(pFanPrice);
        uint256 rem = ethAmount.mod(pFanPrice);
        uint256 principal = quotient.add(rem);

        // Calculate rate
        uint8 r_numerator = 0;
        uint8 r_denominator = 0;

        if (daysStaked < THIRTY_DAYS) {
            r_numerator = 1;
            r_denominator = 4;
        } else if (daysStaked >= THIRTY_DAYS && daysStaked < SIXTY_DAYS) {
            r_numerator = 1;
            r_denominator = 2;
        } else if (daysStaked >= SIXTY_DAYS && daysStaked < NINETY_DAYS) {
            r_numerator = 3;
            r_denominator = 4;
        } else if (daysStaked >= NINETY_DAYS) {
            r_numerator = 1;
            r_denominator = 1;
        } else {
            revert("INVALID_DAYS_STAKED");
        }

        require(r_numerator != 0, "INVALID_R_NUMERATOR");
        require(r_denominator != 0, "INVALID_R_DENOMINATOR");

        uint256 eq_1 = r_numerator.mul(1e18).div(r_denominator);
        uint256 totalBalance = principal.mul(eq_1).div(1e18).div(365).mul(daysStaked);

        return totalBalance;
    }

}

