// Root file: contracts/MGPNftOrder.sol


pragma solidity >=0.6.0 <0.8.0;


// import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
// import "@openzeppelin/contracts/access/Ownable.sol";
// import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
// import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
// import "@openzeppelin/contracts/math/SafeMath.sol";
// import "@openzeppelin/contracts/utils/Address.sol";
// import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";
// import "contracts/MGPNft.sol";
// import "contracts/MgpExchangeMastOracle.sol";
// import "contracts/XMastStake.sol";
// import "@openzeppelin/contracts/token/ERC20/ERC20Burnable.sol";

contract MGPNftOrder is Ownable, ReentrancyGuard {

    event Buy(address indexed user, address token, uint256 amount, uint256 mgp, uint256 [] tokenIds);
    event Join(address indexed user, address token, uint256 amount, uint256 mgp, uint256 tokenId);
    event Upgrade(address indexed user, address token, uint256 amount, uint256 mgp, uint256 tokenId, uint256 level);
    event UpdateOrder(address indexed user, Order order, uint256 eventType);
    event UpdateLevelConfig(uint256 level, LevelConfig levelConfig);
    event SetLockTime(uint256 lockTime);
    event SetBurnProp(uint256 burnProp);
    event SetRewardProp(uint256 rewardProp);
    event SetBuyNftPayMgpAmount(uint256 buyNftPayMgpAmount);
    event SetOracle(address oracle);
    event SetXMastStake(address stake);
    event SetEmergency(bool emergency);
    event  Claimed(uint256 tokenId, uint256 nftId, address user, uint256 amount);

    using SafeMath for uint256;
    using Address for address;
    using SafeERC20 for IERC20;

    struct Order {
        uint256 id;
        uint256 orderType; // 1 nft owner 2 user
        uint256 startTime;
        uint256 endTime;
        uint256 stakeAmount;
        address stakeToken;
        uint256 lockReward;
        uint256 withdrawAmount;
        uint256 mgpAmount;
    }

    struct LevelConfig {
        uint256 payMgpAmount;
        uint256 checkNftStakeAmount;
        uint256 userRatio;
        uint256 ownerRatio;
    }

    struct NftInfo {
        uint256 id;
        uint256 level;
        uint256 totalStake;
    }

    struct NftStake {
        uint256 debt;
        uint256 reward;
        uint256 income;
        uint256 amount;
    }

    uint256 public constant  NFT_MOD = 100000;
    uint256 public constant  MAX_NFT_COUNT = 7000;

    bool public emergency;

    MGPNft public nft;

    MgpExchangeMastOracle public oracle;

    uint256 public tokenCount = 0;

    uint256 public buyNftPayMgpAmount;

    // 0.9 * 1e8
    uint256 public burnProp = 90000000;

    // 0.01 * 1e8
    uint256 public rewardProp = 1000000;




    // order lock time
    uint256 public lockTime;

    IERC20 public mgp;

    IERC20 public mast;

    IERC20 public xmgp;

    IERC20 public xmast;

    XMastStake public xmastStake;

    mapping(uint256 => LevelConfig) public levelConfig;
    mapping(uint256 => NftInfo) public nftInfos;
    mapping(address => Order[]) public orders;
    mapping(address => uint256) public userJoins;

    uint256 public rewardPerShare;
    uint256 public totalStake;
    uint256 public totalReward;
    mapping(uint256 => NftStake) public nftStakes;

    modifier noContractAllowed() {
        require(
            !address(msg.sender).isContract() && msg.sender == tx.origin,
            "Sorry we do not accept contract!"
        );
        _;
    }

    modifier noEmergency() {
        require(!emergency, "emergency.");
        _;
    }


    constructor(address nft_, address mgp_, address mast_, address xmgp_, address xmast_) public {
        nft = MGPNft(nft_);
        mgp = IERC20(mgp_);
        mast = IERC20(mast_);
        xmgp = IERC20(xmgp_);
        xmast = IERC20(xmast_);
    }

    function setEmergency(bool emergency_) external onlyOwner {
        emergency = emergency_;
        emit SetEmergency(emergency_);
    }

    function emergencyWithdraw(address token_, uint256 amount) external onlyOwner
    {
        require(emergency, "no emergency.");
        IERC20(token_).safeTransfer(msg.sender, amount);
    }


    function setLockTime(uint256 lockTime_) external onlyOwner {
        lockTime = lockTime_;
        emit SetLockTime(lockTime);
    }

    function setBurnProp(uint256 burnProp_) external onlyOwner {
        require(burnProp_ <= 1e8, "!limit");
        burnProp = burnProp_;
        emit SetBurnProp(burnProp);
    }

    function setRewardProp(uint256 rewardProp_) external onlyOwner {
        require(rewardProp_ <= 1e8, "!limit");
        rewardProp = rewardProp_;
        emit SetRewardProp(rewardProp);
    }

    function setBuyNftPayMgpAmount(uint256 buyNftPayMgpAmount_) external onlyOwner {
        buyNftPayMgpAmount = buyNftPayMgpAmount_;
        emit SetBuyNftPayMgpAmount(buyNftPayMgpAmount);
    }

    function setOracle(address oracle_) external onlyOwner {
        oracle = MgpExchangeMastOracle(oracle_);
        emit SetOracle(oracle_);
    }

    function setXMastStake(address xmastStake_) external onlyOwner {
        xmastStake = XMastStake(xmastStake_);
        emit SetXMastStake(xmastStake_);
    }

    function setLevelConfig(uint256 level,
        uint256 payMgpAmount,
        uint256 checkNftStakeAmount,
        uint256 userRatio,
        uint256 ownerRatio) external onlyOwner {

        levelConfig[level] = LevelConfig(
            payMgpAmount,
            checkNftStakeAmount,
            userRatio,
            ownerRatio
        );
        emit UpdateLevelConfig(level, levelConfig[level]);
    }


    function buy(address token, uint256 count, uint256 mastAmountLimit)
    external
    noEmergency
    nonReentrant
    noContractAllowed
    {
        require(count > 0, "!count");
        require(tokenCount.add(count) <= MAX_NFT_COUNT, "beyond");

        uint256[] memory tokenIds = new uint256[](count);
        for (uint256 i = 1; i <= count; i = i.add(1)) {
            uint256 tokenId = tokenCount.add(1);
            nft.mint(msg.sender, tokenId);
            tokenCount = tokenCount.add(1);
            tokenIds[i - 1] = tokenId;
            uint256 nftId = tokenId;
            nftInfos[tokenId] = NftInfo(nftId, 0, 0);
        }

        uint256 payMgp = count.mul(buyNftPayMgpAmount);
        if (token == address(mgp)) {
            mgp.safeTransferFrom(msg.sender, address(this), payMgp);
            emit Buy(msg.sender, token, payMgp, payMgp, tokenIds);
            _burnAmount(token, payMgp);
        } else {
            require(address(mast) == token, "!token");
            uint256 mastAmount = oracle.consult(address(mgp), payMgp);
            require(mastAmountLimit >= mastAmount && mastAmount > 0, "NFTOrder.BUY:Insufficient balance or excess slippage");
            mast.safeTransferFrom(msg.sender, address(this), mastAmount);
            emit Buy(msg.sender, token, mastAmount, payMgp, tokenIds);
            _burnAmount(token, mastAmount);
        }
    }


    function _burnAmount(address token, uint256 payAmount) internal {
        uint256 burnAmount = payAmount.mul(burnProp).div(1e8);
        uint256 injectionAmount = payAmount.sub(burnAmount);
        if (burnAmount > 0) {
            ERC20Burnable(token).burn(burnAmount);
        }
        if (injectionAmount > 0) {
            IERC20(token).safeApprove(address(xmastStake), injectionAmount);
            xmastStake.injection(token, injectionAmount);
        }
    }


    // upgrade nft
    function upgrade(uint256 tokenId, address token, uint256 mastAmountLimit)
    external
    noEmergency
    nonReentrant
    noContractAllowed
    {
        require(nft.ownerOf(tokenId) == msg.sender, "!Owner");

        uint256 level = tokenId.div(NFT_MOD);
        require(level <= 4, "NFTOrder.Upgrade:Can't upgrade");

        uint256 newLevel = level.add(1);

        LevelConfig memory config = levelConfig[newLevel];
        NftInfo storage nftInfo = nftInfos[getTokenId(tokenId)];

        require(nftInfo.id > 0, "!NFT ID");

        nft.burn(tokenId);

        nftInfo.level = newLevel;


        require(nftInfo.totalStake >= config.checkNftStakeAmount, "NFTOrder.Upgrade:Upgrade condition not met");
        uint256 newTokenId = newLevel.mul(NFT_MOD).add(nftInfo.id);
        nft.mint(msg.sender, newTokenId);

        if (config.payMgpAmount > 0) {
            uint256 payMgp = config.payMgpAmount;

            if (token == address(mgp)) {
                mgp.safeTransferFrom(msg.sender, address(this), payMgp);
                emit Upgrade(msg.sender, token, payMgp, payMgp, nftInfo.id, newLevel);
                _burnAmount(token, payMgp);
            } else {
                require(address(mast) == token, "!token");
                uint256 mastAmount = oracle.consult(address(mgp), payMgp);
                require(mastAmountLimit >= mastAmount && mastAmount > 0, "NFTOrder.Upgrade:Insufficient balance or excess slippage");
                mast.safeTransferFrom(msg.sender, address(this), mastAmount);
                emit Upgrade(msg.sender, token, mastAmount, payMgp, nftInfo.id, newLevel);
                _burnAmount(token, mastAmount);
            }
        }
        _updateNft(nftInfo.id);
    }

    function getTokenId(uint256 tokenId) public view returns (uint256){
        address owner = nft.ownerOf(tokenId);
        require(owner != address(0), "!NFT Owner");
        uint256 id = tokenId.mod(NFT_MOD);
        NftInfo memory nftInfo = nftInfos[id];
        require(nftInfo.id > 0, "!NFT ID");
        return id;
    }

    function getNftInfo(uint256 tokenId) public view returns (NftInfo memory){
        NftInfo memory nftInfo = nftInfos[getTokenId(tokenId)];
        return nftInfo;
    }

    function getOrderLength(address user) public view returns (uint256){
        return orders[user].length;
    }


    function getOrders(
        address user,
        uint256 offset,
        uint256 size
    ) external view returns (Order[] memory) {
        Order[] memory orderList = orders[user];
        if (offset >= orderList.length) {
            return new Order[](0);
        }
        // length = 2
        // offset = 0 size = 3
        // max size = 2 - 0 = 2
        if (size >= orderList.length - offset) {
            size = orderList.length - offset;
        }
        Order[] memory result = new Order[](size);
        for (uint256 i = 0; i < size; i++) {
            result[i] = orderList[offset + i];
        }
        return result;
    }



    // join nft team
    function join(uint256 tokenId, address token, uint256 amount, uint256 mgpAmountLimitMin)
    external
    noEmergency
    nonReentrant
    noContractAllowed
    {
        address owner = nft.ownerOf(tokenId);

        require(owner != address(0), "!nft");
        require(amount > 0, "NFTOrder.JOIN:Balance is too small");

        uint256 joinNft = userJoins[msg.sender];

        NftInfo storage nftInfo = nftInfos[getTokenId(tokenId)];
        require(nftInfo.level > 0, "NFTOrder.JOIN:Grade not eligible");


        if (joinNft == 0) {
            userJoins[msg.sender] = nftInfo.id;
        } else {
            require(joinNft == nftInfo.id, "NFTOrder.JOIN:Joined other ambassadors");
        }

        LevelConfig memory config = levelConfig[nftInfo.level];

        uint256 mgpAmount = amount;
        if (token == address(xmgp)) {
            xmgp.safeTransferFrom(msg.sender, address(this), amount);
        } else {
            require(address(xmast) == token, "!token");
            mgpAmount = oracle.consult(address(mast), amount);
            require(mgpAmountLimitMin <= mgpAmount, "NFTOrder.JOIN:Insufficient balance or excess slippage");
            xmast.safeTransferFrom(msg.sender, address(this), amount);
        }
        nftInfo.totalStake = nftInfo.totalStake.add(mgpAmount);


        totalReward = totalReward.add(mgpAmount.mul(rewardProp).div(1e8));
        _updateNft(nftInfo.id);


        {
            // user
            Order memory order = Order(
                orders[msg.sender].length,
                2,
                block.timestamp,
                block.timestamp.add(lockTime),
                amount,
                token,
                mgpAmount.mul(config.userRatio).div(1e18),
                0,
                mgpAmount
            );
            orders[msg.sender].push(order);
            emit UpdateOrder(owner, order, 1);
        }

        // nft owner
        if (mgpAmount.mul(config.ownerRatio).div(1e18) > 0) {
            Order memory order = Order(
                orders[owner].length,
                1,
                block.timestamp,
                block.timestamp.add(lockTime),
                0,
                address(0),
                mgpAmount.mul(config.ownerRatio).div(1e18),
                0,
                0
            );
            orders[owner].push(order);
            emit UpdateOrder(owner, order, 2);
        }
        emit Join(msg.sender, token, amount, mgpAmount, tokenId);


    }

    // order claim
    function claim(uint256[] calldata orderId)
    external
    noEmergency
    nonReentrant
    noContractAllowed
    {
        uint256 endTime = block.timestamp;
        for (uint256 i = 0; i < orderId.length; i = i.add(1)) {
            Order storage order = orders[msg.sender][orderId[i]];
            if (order.startTime > 0 && order.lockReward > order.withdrawAmount) {
                uint256 amount = 0;
                if (order.endTime <= endTime) {
                    amount = order.lockReward.sub(order.withdrawAmount);
                } else {
                    amount = endTime.sub(order.startTime).div(
                        order.endTime .sub(order.startTime)
                    ).mul(order.lockReward).sub(order.withdrawAmount);
                }

                if (amount > 0) {
                    order.withdrawAmount = order.withdrawAmount.add(amount);
                    mgp.safeTransfer(msg.sender, amount);
                    emit UpdateOrder(msg.sender, order, 3);
                }
            }
        }
    }

    // order withdraw
    function withdraw(uint256[] calldata orderId)
    external
    noEmergency
    nonReentrant
    noContractAllowed
    {
        uint256 endTime = block.timestamp;
        NftInfo storage nftInfo = nftInfos[userJoins[msg.sender]];
        for (uint256 i = 0; i < orderId.length; i = i.add(1)) {
            Order storage order = orders[msg.sender][orderId[i]];
            if (order.startTime > 0 && order.endTime <= endTime && order.stakeAmount > 0) {
                IERC20(order.stakeToken).safeTransfer(msg.sender, order.stakeAmount);
                nftInfo.totalStake = nftInfo.totalStake.sub(order.mgpAmount);
                _updateNft(nftInfo.id);
                order.stakeAmount = 0;
                emit UpdateOrder(msg.sender, order, 4);
            }
        }
    }

    function _updateNftRewardPerShare() internal {
        (uint256 _reward, uint256 _perShare) = currentRewardShare();
        rewardPerShare = _perShare;
        totalReward = totalReward.sub(_reward);
    }


    function _updateNftReward(uint256 nftId) internal {
        NftStake storage nftStake = nftStakes[nftId];
        if (nftStake.amount > 0) {
            uint256 debt = nftStake.amount.mul(rewardPerShare).div(1e18);
            uint256 nftReward = debt.sub(nftStake.debt);
            nftStake.reward = nftStake.reward.add(nftReward);
            nftStake.debt = debt;
        }
    }

    function _updateNft(uint256 nftId) internal {
        NftInfo storage nftInfo = nftInfos[nftId];
        if (nftInfo.level == 5) {
            _updateNftRewardPerShare();
            _updateNftReward(nftInfo.id);
            NftStake storage nftStake = nftStakes[nftInfo.id];
            totalStake = totalStake.sub(nftStake.amount);
            nftStake.amount = nftInfo.totalStake;
            nftStake.debt = nftStake.amount.mul(rewardPerShare).div(1e18);
            totalStake = totalStake.add(nftStake.amount);
        }
    }


    function currentRewardShare()
    public
    view
    virtual
    returns (uint256 _reward, uint256 _perShare)
    {
        if (totalStake > 0) {
            _reward = totalReward;
            _perShare = rewardPerShare.add(_reward.mul(1e18).div(totalStake));
        } else {
            _reward = 0;
            _perShare = rewardPerShare;
        }
        return (_reward, _perShare);
    }


    function claimNftReward(uint256 tokenId)
    external
    nonReentrant

    {
        require(nft.ownerOf(tokenId) == msg.sender, "!Owner");
        uint256 nftId = getTokenId(tokenId);

        _updateNftRewardPerShare();
        _updateNftReward(nftId);

        NftStake storage nftStake = nftStakes[nftId];
        if (nftStake.reward > 0) {
            uint256 amount = nftStake.reward;
            nftStake.reward = 0;
            nftStake.income = nftStake.income.add(amount);
            mgp.safeTransfer(msg.sender, amount);
            emit Claimed(tokenId, nftId, msg.sender, amount);
        }
    }


    function calculateNftIncome(uint256 tokenId)
    external
    view
    virtual
    returns (uint256)
    {
        uint256 nftId = getTokenId(tokenId);
        NftStake storage nftStake = nftStakes[nftId];
        uint256 _rewardPerShare = rewardPerShare;
        if (totalStake > 0) {
            (, _rewardPerShare) = currentRewardShare();
        }
        uint256 userReward = nftStake.amount.mul(_rewardPerShare).div(1e18).sub(nftStake.debt);
        return nftStake.reward.add(userReward);
    }

}