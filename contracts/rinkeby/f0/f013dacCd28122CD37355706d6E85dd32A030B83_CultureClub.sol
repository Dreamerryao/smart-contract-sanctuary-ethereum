// File contracts/CultureClub.sol

// This contract is used for the Culture Club NFT

pragma solidity ^0.8.9;

contract CultureClub is ERC721, AccessControl {
    using Counters for Counters.Counter;
    using BitMaps for BitMaps.BitMap;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    Counters.Counter private _tokenIdCounter;
    Counters.Counter private _insiderMintCounter;
    Counters.Counter private _privateMintCounter;
    Counters.Counter private _publicMintCounter;

    BitMaps.BitMap private _insiderMintList;
    BitMaps.BitMap private _privateMintList;

    mapping(uint => uint) public tokenIdToType;
    // Token Types => 0: Insider, 1: Private, 2: Public
    mapping(uint => string) public tokenTypeToUri;
    // Token Type URIs => 0: insiderTokenUri, 1: privateTokenUri, 2: publicTokenUri

    uint256 public maxPublicSupply;

    uint256 public constant PRICE_PER_TOKEN = 0.005 ether; // Use small value for testing (real value will be 1 ether)

    mapping(address => uint256) private _publicMintList;
    bytes32 public immutable insiderRoot;
    bytes32 public immutable privateRoot;

    bool public privateSaleIsActive = false;
    bool public insiderSaleIsActive = false;
    bool public publicSaleIsActive = false;

    constructor(
        string memory name,
        string memory symbol,
        string memory insiderTokenUri_,
        string memory privateTokenUri_,
        string memory publicTokenUri_,
        uint256 maxPublicSupply_,
        bytes32 insiderMerkleroot,
        bytes32 privateMerkleroot
    ) ERC721(name, symbol) {
        insiderRoot = insiderMerkleroot;
        privateRoot = privateMerkleroot;
        _setupRole(ADMIN_ROLE, _msgSender());
        tokenTypeToUri[0] = insiderTokenUri_;
        tokenTypeToUri[1] = privateTokenUri_;
        tokenTypeToUri[2] = publicTokenUri_;
        maxPublicSupply = maxPublicSupply_;
        // Start counters at 1
        _tokenIdCounter.increment();
        _insiderMintCounter.increment();
        _privateMintCounter.increment();
        _publicMintCounter.increment();
    }

    function setTokenTypeURI(uint tokenType, string calldata newTokenTypeURI)
        external
        onlyRole(ADMIN_ROLE)
    {
        tokenTypeToUri[tokenType] = newTokenTypeURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        return tokenTypeToUri[tokenIdToType[tokenId]];
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setInsiderSaleState(bool newState) public onlyRole(ADMIN_ROLE) {
        insiderSaleIsActive = newState;
    }

    function setPrivateSaleState(bool newState) public onlyRole(ADMIN_ROLE) {
        privateSaleIsActive = newState;
    }

    function setPublicSaleState(bool newState) public onlyRole(ADMIN_ROLE) {
        publicSaleIsActive = newState;
    }

    function hasInsiderMinted(uint index) public view returns (bool) {
        return _insiderMintList.get(index);
    }

    function insiderMint(
        address account,
        uint256 index,
        bytes32[] calldata proof
    ) external payable returns (uint256) {
        require(insiderSaleIsActive, "Insider sale is not active");
        require(
            PRICE_PER_TOKEN == msg.value,
            "Ether value sent is not correct"
        );
        require(
            _verify(_leaf(account, index), proof, insiderRoot),
            "Invalid merkle proof"
        );
        require(
            !_insiderMintList.get(index),
            "User has already minted for insider sale"
        );
        uint256 tokenId = _tokenIdCounter.current();
        _insiderMintList.set(index);
        _tokenIdCounter.increment();
        _insiderMintCounter.increment();
        tokenIdToType[tokenId] = 0;
        _safeMint(account, tokenId);
        return tokenId;
    }

    function privateMint(
        address account,
        uint256 index,
        bytes32[] calldata proof
    ) external payable {
        require(privateSaleIsActive, "Insider sale is not active");
        require(
            PRICE_PER_TOKEN == msg.value,
            "Ether value sent is not correct"
        );
        require(
            _verify(_leaf(account, index), proof, privateRoot),
            "Invalid merkle proof"
        );
        require(
            !_privateMintList.get(index),
            "User has already minted for private sale"
        );
        uint256 tokenId = _tokenIdCounter.current();
        _privateMintList.set(index);
        _tokenIdCounter.increment();
        _privateMintCounter.increment();
        tokenIdToType[tokenId] = 1;
        _safeMint(account, tokenId);
    }

    function publicMint(address account, bytes calldata signature)
        external
        payable
    {
        require(publicSaleIsActive, "Public sale is not active");
        require(
            PRICE_PER_TOKEN == msg.value,
            "Ether value sent is not correct"
        );
        require(
            _verifySignature(_hash(account), signature),
            "Invalid signature"
        );
        require(
            _publicMintList[account] == 0,
            "User already minted for public sale"
        );
        require(
            _publicMintCounter.current() <= maxPublicSupply,
            "Max public supply reached"
        );
        uint256 tokenId = _tokenIdCounter.current();
        _publicMintList[account] = 1;
        _tokenIdCounter.increment();
        _publicMintCounter.increment();
        tokenIdToType[tokenId] = 2;
        _safeMint(account, tokenId);
    }

    function _leaf(address account, uint256 index)
        internal
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(index, account));
    }

    function _verify(
        bytes32 leaf,
        bytes32[] memory proof,
        bytes32 root
    ) internal pure returns (bool) {
        return MerkleProof.verify(proof, root, leaf);
    }

    function _hash(address account) internal pure returns (bytes32) {
        return
            ECDSA.toEthSignedMessageHash(keccak256(abi.encodePacked(account)));
    }

    function _verifySignature(bytes32 digest, bytes memory signature)
        internal
        view
        returns (bool)
    {
        return hasRole(ADMIN_ROLE, ECDSA.recover(digest, signature));
    }
}