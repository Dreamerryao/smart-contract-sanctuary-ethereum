pragma solidity ^0.8.11;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./ERC884Upgradeable.sol";
import "./IEnkainosManagedFund.sol";

// @name MachineOperations
// @notice ERC721 representing ownership in an off-chain Enkainos managed machine operation
contract MachineOperations is ERC721, ERC721Enumerable, Ownable {

    IEnkainosManagedFund internal enkainos;
    IERC20 internal usdc;

    mapping(address => uint256) public claimableEnk;

    constructor(address _enkainos, address _usdc, string memory _operation) ERC721("Machine Operations", _operation) {
        enkainos = IEnkainosManagedFund(_enkainos);
        usdc = IERC20(_usdc);
    }

    // @name Receive Payment
    // @notice Used to receive profit payments fo operations holders
    function receivePayment(uint256 _amount) public { //TODO access control - only approved payment methods
        bool success = usdc.transferFrom(msg.sender, address(this), _amount);
        if(!success){revert("No USDC received");}

        enkainos.operationsPayment(_amount); //We now have ENK to act with
        uint256 enkPerToken = enkainos.balanceOf(address(this)) / totalSupply(); // ENK received / total OP tokens

        // TODO: improved gas efficiency - one holder could own multiple tokens, claim all at once?
        // Iterate over token holders and make received proportions of ENK claimable
        for(uint256 i; i < totalSupply(); i++){
            claimableEnk[ownerOf(tokenByIndex(i))] += enkPerToken;
        }
    }

    // @name Check Reserves
    // @notice Returns amount of available ENK owned by the MachineOperations contract
    function checkReserves() external returns(uint256) {
        return claimableEnk[address(this)];
    }

    // @name Claim
    // @notice Claims all available ENK for caller
    function claim() public {
        require(claimableEnk[msg.sender] > 0, "No ENK to claim");
        enkainos.transfer(msg.sender, claimableEnk[msg.sender]);
        claimableEnk[msg.sender] = 0;
    }

    // TODO: replace with real minting method
    function safeMint(address to, uint256 tokenId) public onlyOwner {
        _safeMint(to, tokenId);
    }

    // Required for Enumerable
    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // Required for Enumerable
    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

