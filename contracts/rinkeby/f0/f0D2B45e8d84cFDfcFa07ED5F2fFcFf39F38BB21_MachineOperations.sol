// @name IEnkainosManagedFund
// @notice Holds USDC, mints ENK when receiving payments from Operations contracts
interface IEnkainosManagedFund is IERC20 {

    function machines(uint256) external returns(address);
    function machineDesignations(address) external returns(uint256);
    function operationsStatus(address) external returns(bool);
    function numMachines() external returns(uint256);

    // @name Operations Payment
    // @notice Receives USDC payment from operation and mints ENK
    function operationsPayment(uint256 _amount) external;

    // @name Add New Operation
    // @notice Adds a new managed operation to the partnership
    function addNewOperation(address _machine, uint256 _designation) external;

    // @name Withdraw USDC
    // @notice Exchanges and burns ENK for the underlying USDC
    function withdraw(uint256 _amount) external;
}

