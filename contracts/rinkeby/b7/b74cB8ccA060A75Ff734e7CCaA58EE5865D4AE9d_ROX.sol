pragma solidity 0.8.4;

import "openzeppelin-solidity/contracts/token/ERC20/ERC20.sol";
import "openzeppelin-solidity/contracts/utils/math/SafeMath.sol";
import "openzeppelin-solidity/contracts/access/Ownable.sol";
import "openzeppelin-solidity/contracts/security/Pausable.sol";
import "./interface/IUniswapV2Router02.sol";
import "./interface/IUniswapV2Factory.sol";

/// @title ROX
/// @notice ERC-20 implementation of ROX token
contract ROX is ERC20, Ownable, Pausable {
    using SafeMath for uint256;
    enum FeeToken {
        ROX,
        BNB,
        BUSD,
        CAKE
    }

    uint8 public tokenDecimals;
    address public project;

    // BSC Mainnet BUSD and CAKE Token address
    IERC20 public BUSD = IERC20(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56); //solhint-disable-line var-name-mixedcase
    IERC20 public CAKE = IERC20(0x0E09FaBB73Bd3Ade0a17ECC321fD13a19e81cE82); //solhint-disable-line var-name-mixedcase

    // PERCENT ARE DEFINED IN TERMS OF 10000 i.e. 1% = 10000
    uint256 public constant MAX_TX_LIQUIDITY_FEE = 100_000; // 10%
    uint256 public constant MAX_TX_HOLDERS_FEE = 100_000; // 10%
    uint256 public constant MAX_TX_ROX_FEE = 300_000; // 30%
    uint256 public constant MAX_TX_BURN_FEE = 50_000; // 5%

    // Fee variables
    uint256 public liquidityFee;
    uint256 public holdersFee;
    uint256 public roxFee;
    uint256 public burnFee;

    FeeToken public feeToken;

    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;

    bool private inSwapAndLiquify = false;
    bool public swapAndLiquifyEnabled = true;

    // Below values can be changed prior to actual deployment
    uint256 public _maxTxAmount = 500_000 * 10**7;
    uint256 private numTokensSellToAddToLiquidity = 50_000 * 10**7;

    event LogProjectChanged(address oldProject, address newProject);
    event LogDecimalsChanged(uint8 oldDecimal, uint8 newDecimal);
    event LogLiquidityFeeChanged(uint256 oldFee, uint256 newFee);
    event LogHoldersFeeChanged(uint256 oldFee, uint256 newFee);
    event LogRoxFeeChanged(uint256 oldFee, uint256 newFee);
    event LogBurnFeeChanged(uint256 oldFee, uint256 newFee);
    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    modifier onlyProjectOrOwner() {
        //solhint-disable-next-line reason-string
        require(
            msg.sender == project || msg.sender == owner(),
            "Caller is neither rox nor owner"
        );
        _;
    }

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    /**
     * @dev Sets the values for {name = FaveRoxCoin}, and {symbol = ROX}.
     *
     * All of these values are immutable: they can only be set once during
     * construction.
     */
    constructor(address _project) ERC20("RoxInfinity", "ROX") {
        require(_project != address(0), "Project cannot be address zero");

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );
        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        project = _project;
        tokenDecimals = 7;
        liquidityFee = 50_000; // 5%
        holdersFee = 10_000; // 1%
        burnFee = 50_000; // 5%
        roxFee = 100_000; // 10%
        feeToken = FeeToken.ROX;
        // Since Total supply 10 Millions, used ether as token decimals are 18
        super._mint(address(this), 10_000_000 * (10**tokenDecimals));
    }

    /**
     * @dev Contract might receive/hold BNB as part of the maintenance process.
     * The receive function is executed on a call to the contract with empty calldata.
     */
    // solhint-disable-next-line no-empty-blocks
    receive() external payable {}

    fallback() external payable {}

    /**
     * @dev To update liquidity fee
     *
     * Requirements:
     *
     * can only be invoked by the owner of the contract
     */
    function updateLiquidityFee(uint256 newFee) external onlyOwner {
        uint256 oldLiquidityFee = liquidityFee;
        //solhint-disable-next-line reason-string
        require(
            newFee <= MAX_TX_LIQUIDITY_FEE,
            "liquidityFee: cannot set more than max fee"
        );
        liquidityFee = newFee;
        emit LogLiquidityFeeChanged(oldLiquidityFee, newFee);
    }

    /**
     * @dev To update holders fee
     *
     * Requirements:
     *
     * can only be invoked by the owner of the contract
     */
    function updateHoldersFee(uint256 newFee) external onlyOwner {
        uint256 oldHoldersFee = holdersFee;
        //solhint-disable-next-line reason-string
        require(
            newFee <= MAX_TX_HOLDERS_FEE,
            "holdersFee: cannot set more than max fee"
        );
        holdersFee = newFee;
        emit LogHoldersFeeChanged(oldHoldersFee, newFee);
    }

    /**
     * @dev To update rox fee
     *
     * Requirements:
     *
     * can only be invoked by the owner of the contract
     */
    function updateRoxFee(uint256 newFee) external onlyOwner {
        uint256 oldRoxFee = roxFee;
        //solhint-disable-next-line reason-string
        require(
            newFee <= MAX_TX_ROX_FEE,
            "roxFee: cannot set more than max fee"
        );
        roxFee = newFee;
        emit LogRoxFeeChanged(oldRoxFee, newFee);
    }

    /**
     * @dev To update burn fee
     *
     * Requirements:
     *
     * can only be invoked by the owner of the contract
     */
    function updateBurnFee(uint256 newFee) external onlyOwner {
        uint256 oldBurnFee = burnFee;
        //solhint-disable-next-line reason-string
        require(
            newFee <= MAX_TX_BURN_FEE,
            "burnFee: cannot set more than max fee"
        );
        burnFee = newFee;
        emit LogBurnFeeChanged(oldBurnFee, newFee);
    }

    /**
     * @dev To set to accept fee in either ROX, BNB, BUSD or CAKE
     *
     * Requirements
     *
     * - can only be invoked by the owner of the contract
     */
    function acceptFeeIn(FeeToken _feeToken) external onlyOwner {
        if (_feeToken == FeeToken.ROX) {
            feeToken = FeeToken.ROX;
        } else if (_feeToken == FeeToken.BNB) {
            feeToken = FeeToken.BNB;
        } else if (_feeToken == FeeToken.BUSD) {
            feeToken = FeeToken.BUSD;
        } else if (_feeToken == FeeToken.CAKE) {
            feeToken = FeeToken.CAKE;
        }
    }

    /**
     * @dev To buy ROX tokens
     */
    function buy(uint256 amount) external payable whenNotPaused {
        uint256 liquidityAmt = calculatePercentage(amount, liquidityFee);
        uint256 holdersAmt = calculatePercentage(amount, holdersFee);
        uint256 roxAmt = calculatePercentage(amount, roxFee);
        uint256 burnAmt = calculatePercentage(amount, burnFee);
        transfer(msg.sender, amount);
        //add liquidity
        swapAndLiquify(liquidityAmt);
        //Call to receive token holders
        transfer(address(this), holdersAmt);
        transferFeeToProject(roxAmt);
        super._burn(msg.sender, burnAmt);
    }

    /**
     * @dev To sell ROX tokens
     */
    function sell(address to, uint256 amount) external payable whenNotPaused {
        uint256 liquidityAmt = calculatePercentage(amount, liquidityFee);
        uint256 roxAmt = calculatePercentage(amount, roxFee);
        uint256 burnAmt = calculatePercentage(amount, burnFee);
        //add liquidity
        swapAndLiquify(liquidityAmt);
        transferFeeToProject(roxAmt);
        super._burn(msg.sender, burnAmt);
        transfer(to, (amount - (liquidityAmt + roxAmt + burnAmt)));
    }

    /**
     * @dev To update project wallet
     *
     * Requirements:
     * - can only be invoked by the contract owner
     */
    function updateProject(address _project) external onlyOwner {
        //solhint-disable-next-line reason-string
        require(_project != address(0), "New project can't be address zero");
        require(project != _project, "New project can't be old project");
        address oldProject = project;
        project = _project;
        emit LogProjectChanged(oldProject, _project);
    }

    /**
     * @dev To update number of decimals for a token
     *
     * Requirements:
     * - invocation can be done, only by the contract owner & when contract is not paused
     */
    function updateDecimals(uint8 noOfDecimals)
        external
        onlyOwner
        whenNotPaused
    {
        uint8 oldDecimal = tokenDecimals;
        tokenDecimals = noOfDecimals;
        emit LogDecimalsChanged(oldDecimal, noOfDecimals);
    }

    /**
     * @dev To transfer all BNBs stored in the contract to the caller
     *
     * Requirements:
     * - invocation can be done, only by the contract owner & when the contract is not paused
     */
    function withdrawAll() external onlyOwner whenNotPaused {
        //solhint-disable-next-line avoid-low-level-calls
        (bool success, ) = payable(msg.sender).call{
            gas: 2300,
            value: address(this).balance
        }("");
        require(success, "Withdraw failed");
    }

    /**
     * @dev Triggers stopped state.
     *
     * Requirements:
     *
     * - The contract must not be paused.
     * - invocation can be done, only by the contract owner & when the contract is not paused
     */
    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     *
     * Requirements:
     *
     * - The contract must be paused.
     * - invocation can be done, only by the contract owner & when the contract is paused
     */
    function unpause() public onlyOwner whenPaused {
        _unpause();
    }

    /**
     * @dev Returns the number of decimals used to get its user representation.
     * For example, if `decimals` equals `2`, a balance of `505` tokens should
     * be displayed to a user as `5,05` (`505 / 10 ** 2`).
     *
     * Tokens usually opt for a value of 18, imitating the relationship between
     * Ether and Wei. This is the value {ERC20} uses, unless this function is
     * overridden;
     *
     * NOTE: This information is only used for _display_ purposes: it in
     * no way affects any of the arithmetic of the contract, including
     * {IERC20-balanceOf} and {IERC20-transfer}.
     */
    function decimals() public view override returns (uint8) {
        return tokenDecimals;
    }

    /**
     * @dev To transfer fee to the project wallet depending on feeToken
     */
    function transferFeeToProject(uint256 _fee) internal {
        if (feeToken == FeeToken.ROX) {
            transfer(project, _fee);
        } else if (feeToken == FeeToken.BNB) {
            //solhint-disable-next-line avoid-low-level-calls
            (bool success, ) = payable(project).call{gas: 2300, value: _fee}(
                ""
            );
            require(success, "BNB transfer failed");
        } else if (feeToken == FeeToken.BUSD) {
            BUSD.transfer(project, _fee);
        } else if (feeToken == FeeToken.CAKE) {
            CAKE.transfer(project, _fee);
        }
    }

    /**
     * @dev Calculate and returns the percentage of the provided amount
     */
    function calculatePercentage(uint256 amount, uint256 percentage)
        internal
        view
        returns (uint256)
    {
        return ((percentage * amount) / 1_000_000);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp //solhint-disable-line not-rely-on-time
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(0x0000000000000000000000000000000000000000),
            block.timestamp //solhint-disable-line not-rely-on-time
        );
    }

    function swapAndLiquify(uint256 contractTokenBalance) private lockTheSwap {
        // split the contract balance into halves
        uint256 half = contractTokenBalance.div(2);
        uint256 otherHalf = contractTokenBalance.sub(half);

        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = address(this).balance;

        // swap tokens for ETH
        swapTokensForEth(half); // <- this breaks the ETH -> HATE swap when swap+liquify is triggered

        // how much ETH did we just swap into?
        uint256 newBalance = address(this).balance.sub(initialBalance);

        // add liquidity to uniswap
        addLiquidity(otherHalf, newBalance);

        emit SwapAndLiquify(half, newBalance, otherHalf);
    }
}

