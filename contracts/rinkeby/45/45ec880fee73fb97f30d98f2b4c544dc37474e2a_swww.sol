contract swww  {
    mapping(address => mapping(uint256 => Listing)) public listings; 
    mapping(address => uint256) public balances;

    struct Listing {
        address willSwapFor;
        address seller;


    }

    function addListing(address contractAddr, uint256 tokenId, address willSwapFor) public {
        IERC721 token = IERC721(contractAddr);
        require(token.ownerOf(tokenId) == msg.sender, "No Balance");
        require(token.isApprovedForAll(msg.sender, address(this)), "Not Approved");

        listings[contractAddr][tokenId] = Listing(willSwapFor, msg.sender);
    }

    // function removeListing(address contractAddr, uint tokenId) public {
    //     IERC721 token = IERC721(contractAddr);
    //     require(token.ownerOf(tokenId) == msg.sender, "You are not the owner of this token");
    //     require(listings[contractAddr][tokenId].price >= 0, "This tokenId is not listed");

    //     delete listings[contractAddr][tokenId];
    // }

    function withdraw(uint256 amount, address payable destAddr) public  {
        require(amount <= balances[msg.sender], "No balance to withdraw");
        balances[msg.sender] -= amount;
        destAddr.transfer(amount);

    }


    function purchase(address contractAddr, uint256 tokenId, address willSwapFor2, uint tokenId2) public payable {
        
        Listing memory item = listings[contractAddr][tokenId];
        require(willSwapFor2 == item.willSwapFor, "Seller wont accept this swap");

        IERC721 token2 = IERC721(willSwapFor2);
        require(token2.ownerOf(tokenId2) == msg.sender, "No Balance");
        require(token2.isApprovedForAll(msg.sender, address(this)), "Not Approved");


        // balances[item.seller] += msg.value; 
        


        IERC721 token = IERC721(contractAddr);
        token.safeTransferFrom(item.seller, msg.sender, tokenId);
        token2.safeTransferFrom(msg.sender, item.seller, tokenId2);

    }


}