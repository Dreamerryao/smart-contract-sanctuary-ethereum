pragma solidity ^0.8.9;

import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/math/SignedSafeMath.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import 'base64-sol/base64.sol';

library GachaLibrary {
    using SafeMath for uint256;
    using SignedSafeMath for int256;
    using Counters for Counters.Counter;
    using Address for address;
    using Address for address payable;
    using Strings for string;

    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;
    uint256 public constant _FALSE = 1;
    uint256 public constant _TRUE = 2;

    /*****************************************************************************************************************************************************
    Structures
    ****************************************************************************************************************************************************** */
    struct RoleData {
        mapping(address => bool) members;
        bytes32 adminRole;
    }

    /*****************************************************************************************************************************************************
    Events
    ****************************************************************************************************************************************************** */
    /**
     * @dev Emitted when the ownership of the contract changes.
     */
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
    
    /**
     * @dev Emitted when `newAdminRole` is set as ``role``'s admin role, replacing `previousAdminRole`
     *
     * `DEFAULT_ADMIN_ROLE` is the starting admin for all roles, despite
     * {RoleAdminChanged} not being emitted signaling this.
     *
     * _Available since v3.1._
     */
    event RoleAdminChanged(bytes32 indexed role, bytes32 indexed previousAdminRole, bytes32 indexed newAdminRole);

    /**
     * @dev Emitted when `account` is granted `role`.
     *
     * `sender` is the account that originated the contract call, an admin role
     * bearer except when using {AccessControl-_setupRole}.
     */
    event RoleGranted(bytes32 indexed role, address indexed account, address indexed sender);

    /**
     * @dev Emitted when `account` is revoked `role`.
     *
     * `sender` is the account that originated the contract call:
     *   - if using `revokeRole`, it is the admin role bearer
     *   - if using `renounceRole`, it is the role bearer (i.e. `account`)
     */
    event RoleRevoked(bytes32 indexed role, address indexed account, address indexed sender);
    
    /**
     * @dev Emitted when the pause is triggered by `account`.
     */
    event Paused(address account);

    /**
     * @dev Emitted when the pause is lifted by `account`.
     */
    event Unpaused(address account);

    /*****************************************************************************************************************************************************
    Modifiers
    ****************************************************************************************************************************************************** */
    modifier notEmptyString(string memory value) {
        require(bytes(value).length > 0, 'String value must not be empty');
        _;
    }

    modifier notZeroAddress(address _address)
    {
        require(!isZeroAddress(_address), "Address: Address must not be zero address");
        _;
    }

    modifier gtzBytes(bytes memory _bytes)
    {
        require(_bytes.length > 0, 'Bytes: Empty Bytes');
        _;
    }

    modifier gtzUint(uint256 _value)
    {
        require(_value > 0, 'UINT256: Not greater than zero');
        _;
    }

    modifier fromSender(address _address)
    {
        require(isSender(_address), "Address: Address must be from sender");
        _;
    }

    /*****************************************************************************************************************************************************
    Functions
    ****************************************************************************************************************************************************** */
    function _msgSender() internal view returns (address) {
        return msg.sender;
    }

    function _msgData() internal pure returns (bytes calldata) {
        return msg.data;
    }

    function isSender(address _address) internal view returns (bool) {
        return _address == _msgSender();
    }

    function isZeroAddress(address _address) internal pure returns (bool) {
        return _address == address(0);
    }

    function propertyString(string memory key, string memory value, bool includeComma)
        internal pure
        notEmptyString(key)
        notEmptyString(value)
        returns (string memory property)
    {
        property = string(abi.encodePacked('"', key, '":"', value, '"'));

        if(includeComma)
        {
            property = string(abi.encodePacked(property, ','));
        }
    }

    function GachaDescriptor(string memory gachaType, uint256 id, string memory value)
        internal pure
        notEmptyString(gachaType)
        gtzUint(id)
        notEmptyString(value)
        returns (string memory descriptor)
    {
        descriptor = string(abi.encodePacked('GachaNFT ', gachaType, ' #', id, ': ', value));
    }

    function EncodeToBase64(string memory dataType, bytes memory data)
        internal pure
            notEmptyString(dataType)
            gtzBytes(data)
        returns (string memory base64String)
    {
        base64String = string(
            abi.encodePacked(
                'data:',
                dataType,
                ';base64,',
                Base64.encode(data)
            )
        );
    }

    function GenerateGenericImage(string memory gachaType, uint256 id, string memory value, string memory fill, string memory stroke)
        internal pure
            notEmptyString(gachaType)
            gtzUint(id)
            notEmptyString(value)
            notEmptyString(fill)
            notEmptyString(stroke)
        returns (string memory image)
    {
        image = EncodeToBase64('image/svg', abi.encodePacked(
            '<svg xmlns="http://www.w3.org/2000/svg" height="30" width="200"><text x="0" y="15" fill="',
            fill,
            '" stroke="',
            stroke,
            '">',
            GachaDescriptor(gachaType, id, value),
            '</text></svg>'
        ));
    }

    function GenerateObjectBytes(string memory name, string[] memory keys, string[] memory values, bool includeComma)
        internal pure
        returns (bytes memory attributesString)
    {
        require(keys.length > 0 && values.length > 0, 'Arrays must not be empty');
        require(keys.length == values.length, 'Length of supplied keys and values must match');

        if(bytes(name).length > 0)
        {
            attributesString = abi.encodePacked('"', name, '":{');
        }
        else
        {
            attributesString = '{';
        }

        for(uint i = 0; i < keys.length; i++)
        {
            attributesString = abi.encodePacked(attributesString, propertyString(keys[i], values[i], i != keys.length-1));
        }

        attributesString = abi.encodePacked(attributesString, '}');

        if(includeComma)
        {
            attributesString = abi.encodePacked(attributesString, ',');
        }
    }

    function GenerateObjectString(string memory name, string[] memory keys, string[] memory values, bool includeComma)
        internal pure
        returns (string memory attributesString)
    {
        attributesString = string(GenerateObjectBytes(name, keys, values, includeComma));
    }

    function GenerateGenericAttributeObject(address minter, string memory gachaType, uint256 id, string memory value)
        internal pure
        returns (string[] memory keys, string[] memory values)
    {
        keys[0] = 'Id';
        values[0] = UintToString(id);
        keys[1] = 'Type';
        values[1] = gachaType;
        keys[2] = 'Minter';
        values[2] = string(abi.encodePacked(minter));
        keys[3] = 'Value';
        values[3] = value;
    }

    function GenerateGenericTokenUriObject(address minter, string memory gachaType, uint256 id, string memory value, string memory fill, string memory stroke)
        internal pure
        returns (string[] memory keys, string[] memory values)
    {
        string memory descriptor = GachaDescriptor(gachaType, id, value);

        keys[0] = 'id';
        keys[1] = 'name';
        keys[2] = 'description';
        keys[3] = 'image';
        keys[4] = 'attributes';

        values[0] = UintToString(id);
        values[1] = descriptor;
        values[2] = descriptor;
        values[3] = GenerateGenericImage(gachaType, id, value, fill, stroke);
        values[4] = GenerateGenericAttributeObjectString(minter, gachaType, id, value, false);
    }

    function GenerateGenericAttributeObjectString(address minter, string memory gachaType, uint256 id, string memory value, bool includeComma)
        internal pure
        returns (string memory attributeObjectString)
    {
        (string[] memory keys, string[] memory values) = GenerateGenericAttributeObject(minter, gachaType, id, value);
        attributeObjectString = GenerateObjectString('', keys, values, includeComma);
    }

    function GenerateGenericTokenUri(address minter, string memory gachaType, uint256 id, string memory value, string memory fill, string memory stroke)
        internal pure
        returns (string memory tokenUri)
    {
        (string[] memory keys, string[] memory values) = GenerateGenericTokenUriObject(minter, gachaType, id, value, fill, stroke);
        bytes memory objectString = GenerateObjectBytes('', keys, values, false);

         tokenUri = EncodeToBase64('application/json', objectString);
    }
    
    function GenerateRandomNumbers(
        uint256 seed,
        uint256 quantity,
        uint256 nonce
    )
        internal pure
        returns (uint256[] memory expandedValues)
    {
        expandedValues = new uint256[](quantity);
        
        for (uint256 i = 0; i < quantity; i++) {
            expandedValues[i] = uint256(keccak256(abi.encode(seed, ++nonce)));
        }
        
        return expandedValues;
    }

    function UintToString(uint value)
        internal pure
        returns (string memory result)
    {
        if (value == 0) {
            return '0';
        }
        uint j = value;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (value != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(value - value / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            value /= 10;
        }
        result = string(bstr);
    }
}

