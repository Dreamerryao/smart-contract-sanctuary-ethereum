    function checkForPayout(string memory unitId/*, address _bconContract*/) override public {
        Metadata storage md = metadata();
        address _bconContract = md.bconContract;

        BillingData storage bd = billingData();

        // ReportConfig laden
        IConfigurationHandler.ReportConfig memory config = IBconContract(_bconContract).getReportConfig();
        require(config.bconContract == _bconContract);
        
        // lade unit nach letzer aktualisierung
        IBillingUnits.BillingUnit memory unit = bd.billingUnits[unitId];

        console.log("[PaymentHandler] completion rates (total | claimed)",unit.completionRateTotal,unit.completionRateClaimed);
        // die total-rate auf unit-ebene ist (menge an items * 100%);
        // beispiel
        // unit hat 2 items und damit eine total-rate von 200%
        // unit kann ab 50% fertigstellungsgrad ausgezahlt werden
        // 50% von item-1 (=25% der unit) und 50% von item-2 (=25% der unit) reichen für auszahlung
        if(unit.completionRateTotal == unit.completionRateClaimed) {
            // Fertigstellungsgrad = 100%
            bd.billingUnits[unit.id].state = SharedStates.BillingUnitState.Completed;

            uint _securityDeposit = mulScale(unit.payoutClaimed,config.securityDeposit);
            // throw event in BconContract
            IBconContract(_bconContract).throwBillingUnitPayoutTriggered(unit.id,md.client,SharedStates.MessageType.PaymentConfirmation,block.timestamp,(block.timestamp+config.paymentInterval),(unit.payoutClaimed-_securityDeposit),_securityDeposit);

            console.log("[PaymentHandler] new state >> Completed",unit.id);

        } else {
            // Fertigstellungsgrad < 100%
            if(config.partialPayment == true) {
                uint256 currentStageOfCompletion = (unit.completionRateClaimed * 100) / unit.completionRateTotal;
                uint256 minStageOfCompletion = config.minStageOfCompletion;
                
                console.log("[PaymentHandler] stage of completion",minStageOfCompletion,currentStageOfCompletion);
                if(currentStageOfCompletion >= minStageOfCompletion) {
                    // Fertigstellungsgrad >= Mindestfertigstellungsgrad?

                    // Teilzahlung erlauben
                    bd.billingUnits[unit.id].state = SharedStates.BillingUnitState.PartiallyCompleted;
                    console.log("[PaymentHandler] new state >> PartiallyCompleted", unit.id);

                    // Fertigstellungsgrad setzen (bereits geschehen => ist berechenbar nach obigem Schema)
                    
                    console.log("[PaymentHandler] pre deposit", unit.payoutClaimed,config.securityDeposit);
                    uint _securityDeposit = 0;
                    if(config.securityDeposit > 0) {
                        _securityDeposit = mulScale(unit.payoutClaimed,config.securityDeposit);
                    }
                     
                    console.log("[PaymentHandler] security deposit", _securityDeposit);
                    // throw event in BconContract
                    // message.id?
                    IBconContract(_bconContract).throwBillingUnitPayoutTriggered(unit.id,msg.sender,SharedStates.MessageType.PaymentConfirmation,block.timestamp,(block.timestamp+config.paymentInterval),(unit.payoutClaimed-_securityDeposit),_securityDeposit);

                    console.log("[PaymentHandler] new state >> Partially Paid",unit.id);
                } else {
                    console.log("[PaymentHandler] not enough reported to get a payout");
                }
            } else {
                console.log("[PaymentHandler] partial payment is not allowed; not enough work completed");
            }
        }
    }

    // util to calc percent values
    // x = number the percent value should be calculated for
    // y = percentage (e.g. 90 = 90%)
    function mulScale (uint x, uint y) internal pure returns (uint) {
        // Assumption: 100% = 100 scale
        uint128 scale = 100;

        uint a = x / scale;
        uint b = x % scale;
        uint c = y / scale;
        uint d = y % scale;

        return a * c * scale + a * d + b * c + b * d / scale;
    }
}

