pragma solidity ^0.8.0;
//SPDX-License-Identifier: MIT

import "./ERC20.sol";
import "./utils/Ownable.sol";
import "./IUniswapV2Router.sol";
import "./IUniswapV2Factory.sol";
import "./utils/Safemath.sol";
import "hardhat/console.sol";

contract Bev is Ownable, ERC20 {
    using SafeMath for uint256;
    uint8 private _decimal = 9;
    uint256 private _maxTxAmount;
    uint256 private _buyBackThreshold = 10**14;
    uint256 private _bbReservedShare = 0;

    bool private _swapping = false;
    bool private _coolDown;
    bool private tradingOpened = false;
    bool private isLiq = true;
    bool private _bbEnabled = true;

    address payable private _devWallet;
    address payable private _marketingWallet;
    IUniswapV2Router private _uniRouter;
    address private _uniPair;

    mapping(address => bool) internal bots;
    mapping(address => uint256) internal buycooldown;
    mapping(address => bool) internal _isExcludedFromFee;
    mapping(address => uint256) internal sellcooldown;
    mapping(address => bool) internal fromTeam;

    //Disable swap when contract is doing buybacks.
    modifier inSwap() {
        _swapping = true;
        _;
        _swapping = false;
    }
    modifier onlyTeam(address sender) {
        require(fromTeam[sender]);
        _;
    }

    constructor(
        string memory _name_,
        string memory _symbol_,
        address _owner_,
        address _marketing
    ) ERC20(_name_, _symbol_) Ownable(_owner_) {
        _mint(_owner_, 1000000000000 * (10**_decimal));
        _devWallet = payable(_owner_);
        _marketingWallet = payable(_marketing);
        _uniRouter = IUniswapV2Router(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );
        _isExcludedFromFee[_owner_] = true;
        _isExcludedFromFee[_marketing] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[address(0)] = true;
        fromTeam[_owner_] = true;
        fromTeam[_marketing] = true;
    }

    function addLiquidity() external onlyOwner {
        _approve(address(this), address(_uniRouter), type(uint256).max);
        _uniPair = IUniswapV2Factory(_uniRouter.factory()).createPair(
            address(this),
            _uniRouter.WETH()
        );
        _uniRouter.addLiquidityETH{value: address(this).balance}(
            address(this),
            balanceOf(address(this)),
            0,
            0,
            owner(),
            block.timestamp
        );
        isLiq = false;
        _maxTxAmount = 3000000000 * 10**9;
        IERC20(_uniPair).approve(address(_uniRouter), type(uint256).max);
    }

    receive() external payable {}

    function decimals() public view override returns (uint8) {
        return _decimal;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        console.log("transferring: ", amount);
        require(from != address(0), "ERC20: transfer from the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        if (from != owner() && to != owner()) {
            if (_coolDown) {
                if (
                    from != address(this) &&
                    to != address(this) &&
                    from != address(_uniRouter) &&
                    to != address(_uniRouter)
                ) {
                    require(
                        _msgSender() == address(_uniRouter) ||
                            _msgSender() == _uniPair,
                        "ERR: Uniswap only"
                    );
                }
            }
            require(!bots[from] && !bots[to]);
            if (
                from == _uniPair &&
                to != address(_uniRouter) &&
                !_isExcludedFromFee[to]
            ) {
                require(tradingOpened);
                require(amount <= _maxTxAmount);
                require(buycooldown[to] < block.timestamp);
                buycooldown[to] = block.timestamp + (30 seconds);
            }
            if (!_swapping && from != _uniPair && !isLiq) {
                require(sellcooldown[from] < block.timestamp);
                uint256 contractTokenBalance = balanceOf(address(this));
                sellcooldown[from] = block.timestamp + (30 seconds);
                if (contractTokenBalance > 0) {
                    swapTokensForEth(contractTokenBalance);
                }
                uint256 contractETHBalance = address(this).balance;
                if (contractETHBalance.sub(_bbReservedShare) > 0) {
                    console.log(
                        "Contract balance in ETH is: ",
                        contractETHBalance
                    );
                    console.log("ReservedShare: ", _bbReservedShare);
                    console.log(
                        "Amount to distribute: ",
                        contractETHBalance.sub(_bbReservedShare)
                    );
                    distributeFees(contractETHBalance.sub(_bbReservedShare));
                    if (_bbEnabled) {
                        attemptBuyBack();
                    }
                } else {
                    console.log("somethings wrong in stage 1");
                }
            }
        }
        collectFeesAndSwap(to, from, amount);
        emit Transfer(from, to, amount);
    }

    function swapTokensForEth(uint256 tokenAmount) private inSwap {
        console.log("Swapping: ", tokenAmount, " for eth");
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = _uniRouter.WETH();
        _approve(address(this), address(_uniRouter), tokenAmount);
        _uniRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function swapEthForTokensAndBurn(uint256 _amount) private inSwap {
        console.log("Swapping: ", _amount, " Wei for tokens and burning");
        address[] memory path = new address[](2);
        path[0] = _uniRouter.WETH();
        path[1] = address(this);
        //approve before buy
        _uniRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: _amount
        }(0, path, address(0), (block.timestamp));
    }

    function distributeFees(uint256 _amount) private {
        console.log("Distributing: ", _amount);
        uint256 devShare = _amount.mul(25).div(100);
        uint256 MarketingShare = _amount.div(2);
        uint256 BBamount = _amount.sub((devShare.add(MarketingShare)));
        _devWallet.transfer(devShare);
        _marketingWallet.transfer(MarketingShare);
        _bbReservedShare = _bbReservedShare.add(BBamount);
        console.log("bbShare: ", BBamount);
        console.log(
            "Reserved share for buyback after dist: ",
            _bbReservedShare
        );
        console.log("Contract Balance in ETH: ", address(this).balance);
    }

    function attemptBuyBack() private {
        if (_bbReservedShare >= _buyBackThreshold) {
            if (_bbReservedShare <= address(this).balance) {
                swapEthForTokensAndBurn(_bbReservedShare);
                _bbReservedShare = 0;
            } else {
                console.log("something is wrong");
            }
        }
    }

    function collectFeesAndSwap(
        address _to,
        address _from,
        uint256 _amount
    ) private {
        uint256 _amountCollected = 0;
        uint256 _amountToSend = _amount;
        if (!_isExcludedFromFee[_to] && !_isExcludedFromFee[_from]) {
            _amountCollected = (_amount.mul(8)).div(100);
            _amountToSend = _amount.sub(_amountCollected);
        }
        _balances[_to] = _balances[_to].add(_amountToSend);
        _balances[_from] = _balances[_from].sub(_amount);
        _balances[address(this)] = _balances[address(this)].add(
            _amountCollected
        );
    }

    function unlockVision(bool onoff) external onlyOwner {
        tradingOpened = onoff;
    }

    function manualBuyBack() external onlyTeam(msg.sender) {
        require(_bbReservedShare > 0);
        swapEthForTokensAndBurn(_bbReservedShare);
    }

    function manualWithdraw() external onlyTeam(msg.sender) {
        require((address(this).balance).sub(_bbReservedShare) > 0);
        distributeFees((address(this).balance).sub(_bbReservedShare));
    }

    function manualETH() external onlyTeam(msg.sender) {
        require(balanceOf((address(this))) > 0);
        swapTokensForEth(balanceOf(address(this)));
    }

    function clearETHToBuyBack() external onlyTeam(msg.sender) {
        require(address(this).balance > 0);
        swapEthForTokensAndBurn(address(this).balance);
        _bbReservedShare = 0;
    }

    function setBuyBackThreshold(uint256 _amount)
        external
        onlyTeam(msg.sender)
    {
        _buyBackThreshold = _amount;
    }

    function setMaxTxAmount(uint256 _amount) external onlyOwner {
        _maxTxAmount = _amount;
    }

    function getReservedBBShare() public view returns (uint256) {
        return _bbReservedShare;
    }
}

