// File: contracts/EtermonProxySupported/EtermonMarketplace.sol

pragma solidity 0.8.0;

contract EtermonMarketplace is Ownable, ERC1155Holder, BlackList {
    using SafeMath for uint256;
    using SafeMath for uint8;
    using Roles for Roles.Role;

    Roles.Role private etermon;
    IERC20Wrapper private wrapper;

    address private mintFactory;
    address private etermonBox;

    uint256 public boxPrice;
    uint256 public boxAvailable;

    bool private lock;
    
    struct EtermonMarketItem {
        address nftAddress;
        address seller;
        uint256 tokenId;
        uint256 buyPrice;
        ItemState currentState;
    }

    event Revoke(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed revoker
    );

    event List(
        address indexed nftAddress,
        uint256 indexed tokenId,
        address indexed seller,
        ItemState itemState
    );

    event EtermonCollectionAdded(address indexed etermonAddress);
    event EtermonCollectionRemoved(address indexed etermonAddress);

    event EtermonBoxPrice(uint256 indexed etermonAddress);
    event EtermonBoxAvailable(uint256 indexed etermonAddress);

    enum ItemState {
        Selling,
        Revoked,
        Sold
    }

    mapping(address => mapping(uint256 => EtermonMarketItem)) internal getEtermonMarketItem;
    mapping(address => uint256) public credits;

    function marketplaceInitialize(address wrapper_, address mintFactory_, address etermonBox_) external{
        require(!lock);
        wrapper = IERC20Wrapper(payable(wrapper_));
        mintFactory = mintFactory_;
        etermonBox = etermonBox_;
        lock = true;
    }

    function canSell(address etermonAddress) public view returns (bool) {
        return etermon.has(etermonAddress);
    }

    function addEtermonAddress(address etermonAddress) external onlyOwner {
        etermon.add(etermonAddress);
        emit EtermonCollectionAdded(etermonAddress);
    }

    function removeEtermonAddress(address etermonAddress) external onlyOwner{
        etermon.remove(etermonAddress);
        emit EtermonCollectionRemoved(etermonAddress);
    }

    function setBoxPrice(uint256 newPrice) external onlyOwner {
        boxPrice = newPrice;
        emit EtermonBoxPrice(boxPrice);
    }

    function addBoxAvailable(uint256 newBox) external onlyOwner {
        boxAvailable += newBox;
        emit EtermonBoxAvailable(boxAvailable);
    }

    function createListDifferentItems(
        address[] memory nftAddress,
        uint256[] memory tokenId,
        uint256[] memory buyPrice
    ) external {
        assert(
            nftAddress.length == tokenId.length &&
            nftAddress.length == buyPrice.length
        );

        for (uint8 i = 0; i < tokenId.length; i++) {
            createItem(
                nftAddress[i],
                tokenId[i],
                buyPrice[i]
            );
        }
    }

    function createListSameItems(
        address nftAddress,
        uint256[] memory tokenId,
        uint256 buyPrice
    ) external {
        for (uint8 i = 0; i < tokenId.length; i++) {
            createItem(
                nftAddress,
                tokenId[i],
                buyPrice
            );
        }
    }

    function checkOwnership(
        address nftAddress,
        uint256 tokenId,
        address checker
    ) private view returns (bool) {
        if (IERC721(nftAddress).ownerOf(tokenId) == checker && 
        (IERC721(nftAddress).getApproved(tokenId) == address(this) || IERC721(nftAddress).isApprovedForAll(checker,address(this)) == true))
        {
            return true;
        }
        return false;
    }

    function createItem(
        address nftAddress,
        uint256 tokenId,
        uint256 buyPrice
    ) public {
        assert(
            (!isBlackListed[msg.sender]) &&
            canSell(nftAddress) &&
            checkOwnership(nftAddress, tokenId, msg.sender) == true
        );

        getEtermonMarketItem[nftAddress][tokenId] = EtermonMarketItem(
            nftAddress,
            msg.sender,
            tokenId,
            buyPrice,
            ItemState.Selling
        );

        emit List(nftAddress, tokenId, msg.sender, ItemState.Selling);
    }

    function buyBox(
       uint256[] memory tokenId
    ) external {
        uint256 currentBoxAvailable = boxAvailable;

        assert(
            (!isBlackListed[msg.sender]) &&
            currentBoxAvailable - tokenId.length >= 0
        );

        for(uint256 i; i < tokenId.length; i++){
            MintFactory(mintFactory).mintTo(msg.sender, etermonBox, tokenId[i]);
        }

        boxAvailable -= tokenId.length;
        credits[owner()] += boxPrice.mul(tokenId.length);

        wrapper.safeTransferFrom(
            msg.sender,
            address(this),
            1,
            boxPrice.mul(tokenId.length),
            abi.encodePacked(address(this))
        );
    }

    function buyItems(
        address[] memory nftAddress,
        uint256[] memory tokenId
    ) external {
        assert(nftAddress.length == tokenId.length);
        for (uint8 i = 0; i < tokenId.length; i++) {
            buyItem(nftAddress[i], tokenId[i]);
        }
    }

    function buyItem(
        address nftAddress,
        uint256 tokenId
    ) public {
        address seller = getEtermonMarketItem[nftAddress][tokenId].seller;

        assert(
            (!isBlackListed[msg.sender]) &&
            checkOwnership(nftAddress, tokenId, seller) == true &&
            getEtermonMarketItem[nftAddress][tokenId].currentState == ItemState.Selling &&
            msg.sender != seller
        );

        getEtermonMarketItem[nftAddress][tokenId].currentState = ItemState.Sold;
        credits[seller] += getEtermonMarketItem[nftAddress][tokenId].buyPrice;

        wrapper.safeTransferFrom(
            msg.sender,
            address(this),
            1,
            getEtermonMarketItem[nftAddress][tokenId].buyPrice,
            abi.encodePacked(address(this))
        );
        
        IERC721(nftAddress).safeTransferFrom(seller, msg.sender, tokenId, abi.encodePacked(address(this)));
    }

    function revokeItem(address nftAddress, uint256 tokenId) external {
        address seller = getEtermonMarketItem[nftAddress][tokenId].seller;
        ItemState itemState = getEtermonMarketItem[nftAddress][tokenId].currentState;

        assert(
            checkOwnership(nftAddress, tokenId, seller) == true &&
            itemState == ItemState.Selling &&
            msg.sender == seller
        );

        getEtermonMarketItem[nftAddress][tokenId].currentState = ItemState.Revoked;

        emit Revoke(nftAddress, tokenId, msg.sender);
    }

    function allowCreditsPull(
        address receiver,
        uint256 amount
    ) private {
        credits[receiver] += amount;
    }

    function withdrawCredits() external {
        uint256 amount = credits[msg.sender];
        assert(
            amount != 0 &&
            wrapper.balanceOf(address(this), 1) >= amount
        );
        credits[msg.sender] = 0;

        wrapper.safeTransferFrom(address(this), msg.sender, 1, amount, abi.encodePacked(address(this)));
    }
}