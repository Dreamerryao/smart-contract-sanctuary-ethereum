    function etermonInitailize(string memory name_, string memory symbol_) internal {
        ERC721initialize(name_,symbol_);
    }

    function mint(
        address to, 
        uint256 tokenId
    ) public onlyMinter {
        _safeMint(to, tokenId);
        require(_exists(tokenId));
    }

    function mintBatchSameToken(
        address to,
        uint256[] memory tokenId
    ) external onlyMinter {
        for (uint256 i; i < tokenId.length; i++) {
            mint(to, tokenId[i]);
            require(_exists(tokenId[i]));
        }
    }

    function mintBatchDifferentToken(
        address[] memory to,
        uint256[] memory tokenId
    ) external onlyMinter {
        require(
            to.length == tokenId.length,
            'ERC721: Arrays length missmatch'
        );

        for (uint256 i; i < tokenId.length; i++) {
            mint(to[i], tokenId[i]);
        }
    }

