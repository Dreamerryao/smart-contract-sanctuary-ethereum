pragma solidity ^0.8.0;

//-----------------------------------------------------------------------
// IOceanaAuction
//-----------------------------------------------------------------------
interface IOceanaAuction {
    event AuctionCreated(
        uint256 indexed tokenId,
        address indexed owner,
        uint256 reservePrice,
        uint256 amount,
        uint256 startTimestamp,
        uint256 endTimestamp
    );

    event BidPlaced(
        uint256 indexed tokenId,
        address indexed owner,
        address bidder,
        uint256 bid
    );

    event BidRefunded(address indexed bidder, uint256 bid);

    event BidWithdrawn(
        uint256 indexed tokenId,
        address indexed owner,
        address bidder,
        uint256 bid
    );
    event AuctionResulted(
        uint256 indexed tokenId,
        address indexed owner,
        address winner,
        uint256 winningBidAmount
    );

    event AuctionCancelled(uint256 indexed tokenId, address indexed owner);

    event UpdateBidWithdrawalLockTime(uint256 bidWithdrawalLockTime);

    event UpdateAuctionReservePrice(
        uint256 indexed tokenId,
        address indexed owner,
        uint256 reservePrice
    );

    event UpdateAuctionStartTime(
        uint256 indexed tokenId,
        address indexed owner,
        uint256 startTime
    );

    event UpdateAuctionEndTime(
        uint256 indexed tokenId,
        address indexed owner,
        uint256 endTime
    );
}

