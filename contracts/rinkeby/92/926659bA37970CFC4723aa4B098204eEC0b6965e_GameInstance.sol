pragma solidity 0.8.4;

import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "hardhat/console.sol";

contract GameInstance is ERC721Holder, ERC1155Holder {
    using ECDSA for bytes32;
    using SafeERC20 for IERC20;

    struct Deposit {
        address user;
        address token;
        uint256 id;
        uint256 price;
        bool tokenType;
    }

    bool public isInitialized;
    address public admin;
    bool public isPaused;
    address public adminSigner;
    address public gameMaster;
    uint256 public closeTime;
    uint256 public playerCounter;
    uint256 public totalValue;
    uint256 public nftIndex;
    address public winner;

    Deposit[] public players;

    ///@notice emits event when a token is deposited
    event Deposited(
        address indexed user,
        address token,
        uint256 id,
        uint256 price,
        uint256 playerNumber,
        uint256 nftIndex
    );

    event Winner(address indexed user, address[] token, uint256[] id);
    event SelectWinnet(address indexed user);


    event Reimbursed(
        address indexed user,
        address token,
        uint256 id,
        bool tokenType
    );
    

    event EventNFTFee(
        address indexed user,
        address token,
        uint256 id,
        bool tokenType
    );


    ///@notice initialize instance
    function initialize(
        address _admin,
        address _adminSigner,
        address _gameMaster,
        uint256 _closeTime
    ) external {
        require(!isInitialized, "Instance is intinitalized");
        admin = _admin;
        adminSigner = _adminSigner;
        gameMaster = _gameMaster;
        closeTime = _closeTime;
        isInitialized = true;
    }

    modifier onlyAdmin() {
        require(admin == msg.sender, "Require admin access");
        _;
    }

    modifier whenNotPaused() {
        require(!isPaused, "Game is paused");
        _;
    }

    ///@notice pauses game
    function pause() external onlyAdmin {
        isPaused = true;
    }

    ///@notice unpauses game
    function unpause() external onlyAdmin {
        isPaused = false;
    }

    ///@notice allows users to deposit tokens
    ///@param token is the address of the contract the creates the nft
    ///@param id is the id of the nft in the token contract
    ///@param price price is the value assigned to the nft
    ///@param validity expiration date of the price
    ///@param signature signature of teh price value
    ///@param tokenType true for ERC721 false for ERC1155
    function depositToken(
        address[] memory token,
        uint256[] memory id,
        uint256[] memory price,
        uint256[] memory validity,
        bytes[] calldata signature,
        bool[] memory tokenType
    ) external whenNotPaused {
        require(token.length <= 5, "Max tokens exceeded");
        require(isInitialized, "Instance was not intinitalized");
        require(closeTime > block.timestamp, "Game has closed");
        uint256 totalPrice;
        
        for (uint256 i = 0; i < token.length; i++) {
            _depositToken(
                token[i],
                id[i],
                price[i],
                validity[i],
                signature[i],
                tokenType[i]
            );
            totalPrice += price[i];
        }
    }

    function _depositToken(
        address token,
        uint256 id,
        uint256 price,
        uint256 validity,
        bytes calldata signature,
        bool tokenType
    ) internal {
        require(price != 0, "Price should not be Zero");
        require(validity > block.timestamp, "Invalid price");
        bytes memory data = abi.encode(token, id, price, validity);
        bytes32 hash = keccak256(data).toEthSignedMessageHash();
        address recSigner = hash.recover(signature);
        require(recSigner == adminSigner, "Invalid price signature");
        Deposit memory _deposit = Deposit(
            msg.sender,
            token,
            id,
            price,
            tokenType
        );
        players.push(_deposit);
        nftIndex = players.length;
        playerCounter += 1;
        totalValue += price;
        if (tokenType) {
            IERC721 nft = IERC721(token);
            nft.safeTransferFrom(msg.sender, address(this), id);
        } else {
            IERC1155 nft = IERC1155(token);
            nft.safeTransferFrom(msg.sender, address(this), id, 1, "");
        }

        emit Deposited(msg.sender, token, id, price, playerCounter, nftIndex-1);
    }

    ///@notice given a radom number, select a winner from participants
    ///@param randomNumber random number, passed from GameMaster
    function selectWinner(uint256 randomNumber) external whenNotPaused {
        require(isInitialized, "Instance was not intinitalized");
        require(msg.sender == gameMaster, "Game Master access required");
        require(closeTime < block.timestamp, "Game is active");
        uint256 nPlayers = players.length;
        uint256 price;
        uint256 winnerValue = randomNumber % totalValue;
        for (uint256 i = 0; i < nPlayers; i++) {
            price += players[i].price;
            if (winnerValue < price) {
                winner = players[i].user;
                break;
            }
        }

        emit SelectWinnet(winner);
    }

    ///@notice send price to winner
    function claimPrize() external {
        require(winner == msg.sender, "You are not the winner");
        require(closeTime < block.timestamp, "Game is active");
        address[] memory tokens = new address[](players.length);
        uint256[] memory ids = new uint256[](players.length);
        for (uint256 i; i < players.length; i++) {
            address token = players[i].token;
            uint256 id = players[i].id;
            bool tokenType = players[i].tokenType;

            if (token == address(0)) continue;

            delete players[i];
            if (tokenType) {
                IERC721 nft = IERC721(token);
                nft.safeTransferFrom(address(this), winner, id);
            } else {
                IERC1155 nft = IERC1155(token);
                nft.safeTransferFrom(address(this), winner, id, 1, "");
            }
            tokens[i] = token;
            ids[i] = id;
        }
        emit Winner(winner, tokens, ids);
    }

    ///admin functions

    ///@notice admin can reimburse  user/users
    function reimburse(uint256[] calldata index) external onlyAdmin {
        for (uint256 i = 0; i < index.length; i++) {
            Deposit memory deposit = players[index[i]];
            players[index[i]] = Deposit(address(0), address(0), 0, 0, false);
            playerCounter -= 1;
            totalValue -= deposit.price;

            if (deposit.token == address(0)) continue;

            if (deposit.tokenType) {
                IERC721 nft = IERC721(deposit.token);
                nft.safeTransferFrom(address(this), deposit.user, deposit.id);
            } else {
                IERC1155 nft = IERC1155(deposit.token);
                nft.safeTransferFrom(
                    address(this),
                    deposit.user,
                    deposit.id,
                    1,
                    ""
                );
            }

            emit Reimbursed(
                deposit.user,
                deposit.token,
                deposit.id,
                deposit.tokenType
            );
        }
    }


    ///@notice send NFT fee to admin
    function NFTFee(uint256[] calldata index) external onlyAdmin {
        for (uint256 i = 0; i < index.length; i++) {
            Deposit memory deposit = players[index[i]];
            players[index[i]] = Deposit(address(0), address(0), 0, 0, false);
            playerCounter -= 1;
            totalValue -= deposit.price;

            if (deposit.token == address(0)) continue;

            if (deposit.tokenType) {
                IERC721 nft = IERC721(deposit.token);
                nft.safeTransferFrom(address(this), admin, deposit.id);
            } else {
                IERC1155 nft = IERC1155(deposit.token);
                nft.safeTransferFrom(
                    address(this),
                    admin,
                    deposit.id,
                    1,
                    ""
                );
            }

            emit EventNFTFee(
                deposit.user,
                admin,
                deposit.id,
                deposit.tokenType
            );
        }
    }
}

