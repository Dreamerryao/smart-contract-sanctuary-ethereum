import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import {Errors} from "../../libraries/Errors.sol";
import {MockStaking} from "../../test/staking/MockStaking.sol";
import {INuclifiCertificate} from "../../interfaces/INuclifiCertificate.sol";
import {Controller} from "../../Controller.sol";

/**
 * @title MockDualStakingStrategy.
 * @author Nuclifi.
 * @notice Serves as a mock strategy where we stake user's fund in 2 stakign programs.
 */
contract MockDualStakingStrategy is Ownable, ReentrancyGuard {
    using Address for address;

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    uint256 public certificateId;

    Controller public controller;
    MockStaking public stakingProgramA;
    INuclifiCertificate public nuclifiCertificate;

    constructor(
        address controller_,
        address stakingProgramA_,
        address nuclifiCertificate_
    ) {
        require(controller_.isContract(), Errors.ADDRESS_NOT_CONTRACT);
        require(stakingProgramA_.isContract(), Errors.ADDRESS_NOT_CONTRACT);
        require(nuclifiCertificate_.isContract(), Errors.ADDRESS_NOT_CONTRACT);

        controller = Controller(controller_);
        stakingProgramA = MockStaking(stakingProgramA_);
        nuclifiCertificate = INuclifiCertificate(nuclifiCertificate_);

        IERC20 _tokenA = IERC20(stakingProgramA.stakingToken());

        _tokenA.approve(stakingProgramA_, type(uint256).max);
    }

    function setCertificateId(uint256 id) external onlyOwner {
        require(id > 0, Errors.VALUE_IS_ZERO);

        certificateId = id;
    }

    function earned()
        public
        view
        returns (
            uint256
        )
    {
        return stakingProgramA.earned(address(this));
    }

    function stake(uint256 amountA) external nonReentrant {
        _requireCallerIsController();

        require(amountA > 0, Errors.VALUE_IS_ZERO);

        stakingProgramA.stake(amountA);
    }

    function withdraw(uint256 amountA) public nonReentrant {
        _requireCallerIsController();

        require(amountA > 0, Errors.VALUE_IS_ZERO);

        stakingProgramA.withdraw(amountA);

        address _certificateOwner = ownerOfCertificate();

        IERC20 _tokenA = IERC20(stakingProgramA.stakingToken());

        _tokenA.safeTransfer(_certificateOwner, amountA);
    }

    function getReward() public nonReentrant {
        _requireCallerIsController();
        address _certificateOwner = ownerOfCertificate();

        IERC20 _rewardToken = IERC20(
            stakingProgramA.rewardsToken()
        );

        uint256 balanceBefore = _rewardToken.balanceOf(address(this));
        stakingProgramA.getReward();
        uint256 balanceAfter = _rewardToken.balanceOf(address(this));

        _rewardToken.safeTransfer(_certificateOwner, balanceAfter.sub(balanceBefore));
    }

    function ownerOfCertificate() public view returns (address) {
        return nuclifiCertificate.ownerOf(certificateId);
    }

    function _requireCallerIsController() internal view {
        require(msg.sender == address(controller), Errors.CALLER_NOT_OWNER);
    }
}

