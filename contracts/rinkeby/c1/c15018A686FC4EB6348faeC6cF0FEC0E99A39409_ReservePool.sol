// SPDX-License-Identifier: MIT
pragma solidity 0.8.4;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interface/IGFNStore.sol";
import "./GFNStoreToken.sol";

import "hardhat/console.sol";

contract GFNStore is Context, IGFNStore, GFNStoreToken {

    address public reservePool;
    address public holderContract;

    IERC20 public usd;
    IERC20 public token;

    modifier onlyReservePoolContract() {
        require(_msgSender() == reservePool, "Caller is not the Reserve Pool Contract");
        _;
    }

    constructor() {
        reservePool = _msgSender();
    }

    // called once by the reserve pool at time of deployment
    function initialize(IERC20 _token0, IERC20 _token1) external override onlyReservePoolContract {
        usd = _token0;
        token = _token1;
        emit Initialize(address(_token0), address(_token1));
    }

    function storeMint(address to, uint256 amount) external override onlyReservePoolContract {
        _mint(to, amount);
        emit StoreMint(to, amount);
    }

    function storeBurn(address from, address receiver, uint256 amountRp, uint256 amountUsd, uint256 amountToken) external override onlyReservePoolContract {
        if(amountRp > 0) _burn(from, amountRp);
        if(amountUsd > 0) usd.transfer(receiver, amountUsd);
        if(amountToken > 0) token.transfer(from, amountToken);
        emit StoreBurn(from, receiver, amountRp, amountUsd, amountToken);
    }
    
}

// SPDX-License-Identifier: MIT
pragma solidity 0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interface/IReservePool.sol";
import "./interface/IGNFT.sol";
import "./interface/IBurn.sol";
import "./interface/IGFNConfig.sol";
import "./interface/IUSDToken.sol";
import "./interface/IGFNHolder.sol";
import "./interface/IDataUtilization.sol";
import "./interface/IEvents.sol";

import "hardhat/console.sol";

contract GFNHolder is Ownable, IGFNHolder, IEvents {

    using SafeERC20 for IERC20;
    address public usdToken;
    IBurn public burnContract;
    IReservePool public reservePool;
    IDataUtilization public dataUtilization;

    IGNFT public gnft;
    IGFNConfig public gfnConfig;
    uint8 private maxLengthPoolId = 128;

    // user address -> amount USD
    mapping(address => uint256) private debt; // -
    mapping(address => uint256) private profit; // +
    mapping(address => string) public poolOf;     // user address -> pool id

    modifier onlyReservePool() {
        require(_msgSender() == address(reservePool), "Caller is not the Reserve Pool Contract");
        _;
    }

    modifier onlyReservePoolOrDU() {
        require(_msgSender() == address(reservePool) || _msgSender() == address(dataUtilization), "Caller is not the Reserve Pool or DU");
        _;
    }

    constructor(address owner, IReservePool _reservePool, address _usdToken, IGNFT _gnft, IGFNConfig _gfnConfig, IBurn _burnContract) {
        reservePool = _reservePool;
        gnft = _gnft;
        gfnConfig = _gfnConfig;
        usdToken = _usdToken;
        burnContract = _burnContract;
        transferOwnership(owner);
    }

    function setUSDToken(address _usdToken) external override onlyOwner {
        usdToken = _usdToken;
        emit SetUSDToken(_usdToken);
    }

    function setGNFT(IGNFT _gnft) external override onlyOwner {
        gnft = _gnft;
        emit SetGNFTToken(address(_gnft));
    }

    function setGFNConfig(IGFNConfig _gfnConfig) external override onlyOwner {
        gfnConfig = _gfnConfig;
        emit SetGFNConfig(address(_gfnConfig));
    }

    function setReservePool(IReservePool _reservePool) external override onlyOwner {
        reservePool = _reservePool;
        emit SetReservePool(address(_reservePool));
    }

    function setDataUtilization(IDataUtilization _dataUtilization) external override onlyOwner {
        dataUtilization = _dataUtilization;
        emit SetDataUtilization(address(_dataUtilization));
    }

    function setPoolOfUser(string memory poolId, address user) external override onlyReservePool {
        poolOf[user] = poolId;
        emit SetPoolOfUser(poolId, user);
    }

    function setBurnContract(IBurn _burnContract) external override onlyOwner {
        burnContract = _burnContract;
        emit SetBurn(address(_burnContract));
    }

    function decreaseDebtOfUser(address user, uint256 amount) external override onlyReservePoolOrDU {
        debt[user] -= amount;
        emit DecreaseDebtOfUser(user, amount);
    }

    function increaseDebtOfUser(address user, uint256 amount) external override onlyReservePoolOrDU {
        debt[user] += amount;
        emit IncreaseDebtOfUser(user, amount);
    }

    function decreaseProfitOfUser(address user, uint256 amount) external override onlyReservePoolOrDU {
        profit[user] -= amount;
        emit DecreaseProfitOfUser(user, amount);
    }

    function increaseProfitOfUser(address user, uint256 amount) external override onlyReservePoolOrDU {
        profit[user] += amount;
        emit IncreaseProfitOfUser(user, amount);
    }

    function getPoolOfUser(address user) external override view returns(string memory poolId) {
        return poolOf[user];
    }

    function getDebt(address user) external view override returns(uint256 debtValue){
        return debt[user];
    }
    function getProfit(address user) external view override returns(uint256 profitValue){
        return profit[user];
    }

    function startWithdrawing(address user, uint256 amount) external override onlyOwner {
        require(amount > 0, "Amount to withdraw must be greater than 0");
        require(amount <= profit[user], "Not enough balance");
        profit[user] -= amount;

        burnContract.setUserStove(user, amount);
        SafeERC20.safeTransfer(IERC20(usdToken), address(burnContract), amount);
        emit StartWithdrawing(user, amount);
    }

    function finishWithdrawing(address user) external override onlyOwner {
        uint256 amount = burnContract.getUserStove(user);
        require(amount > 0, "Nothing to finish withdrawing");
        burnContract.burnUserStove(user);
        emit FinishWithdrawing(user, amount);
    }
}

