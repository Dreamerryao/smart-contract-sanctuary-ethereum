interface IEvents {
    event SetUSDToken(address usdToken);
    event SetGNFTToken(address gnftToken);
    event SetGFNToken(address gfnToken);
    event SetGFNConfig(address gfnConfig);
    event SetHolder(address holder);
    event SetReservePool(address reservePool);
    event SetBurn(address burn);
    event SetDataUtilization(address dataUtilization);
