import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface IGFNStore is IERC20 {

    event Initialize(address token0, address token1);
    event StoreMint(address to, uint256 indexed amount);
    event StoreBurn(address from, address receiver, uint256 indexed amountRp, uint256 indexed amountUsd, uint256 indexed amountToken);

    function initialize(IERC20 _token0, IERC20 _token1) external;
    function storeMint(address to, uint256 amount) external;
    function storeBurn(address from, address receiver, uint256 amountRp, uint256 amountUsd, uint256 amountToken) external;
}

