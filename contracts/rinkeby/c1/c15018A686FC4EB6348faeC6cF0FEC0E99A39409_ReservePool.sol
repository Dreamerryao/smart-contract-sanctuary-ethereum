pragma solidity 0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interface/IReservePool.sol";
import "./interface/IUSDToken.sol";
import "./interface/IGFNHolder.sol";
import "./interface/IBurn.sol";
import "./interface/IGFNStore.sol";
import "./interface/IEvents.sol";
import "./interface/IExecuteTimelock.sol";

import "./GFNStore.sol";
import "./GFNHolder.sol";

import "hardhat/console.sol";

contract ReservePool is Ownable, IReservePool, IEvents {

    using SafeERC20 for IERC20;
    IERC20 public usdToken;
    IERC20 public gfnToken;
    IExecuteTimelock public executor;

    struct QueueData {
        address investor;
        uint256 usdAmount;
    }

    string[] public allPools;
    address[] public allStores;
    IGFNHolder public holderContract;
    IBurn public burnContract;
    uint8 private maxLengthPoolId = 128;

    mapping(string => mapping(uint256 => QueueData)) public queue; // pool id -> index -> queue data
    mapping(string => uint256) public last; // pool id -> last index
    mapping(string => uint256) public first; // pool id -> first index
    mapping(string => uint256) public balanceOfPool; // pool id -> balance

    mapping(string => mapping(IERC20 => mapping(IERC20 => address))) public getStore; // pool id -> usd -> life
    mapping(address => mapping(string => uint256)) public totalMoneyInvested; // investor address -> pool id -> usd
    mapping(address => mapping(string => uint256)) public totalMoneyInQueue; // investor address -> pool id -> usd

    modifier validPool(string memory poolId) {
        require(bytes(poolId).length > 0 && bytes(poolId).length <= maxLengthPoolId, "Invalid Pool id");
        _;
    }

    modifier existPool(string memory poolId) {
        address store = getStoreInPoolId(poolId);
        require(store != address(0), 'Pool is not existed');
        _;
    }

    modifier onlyOwnerOrExecutor() {
        require(_msgSender() == owner() || _msgSender() == address(executor), "Only Owner or executor");
        _;
    }

    constructor(address owner, IERC20 _usdToken, IERC20 _gfnToken, IBurn _burnContract) {
        usdToken = _usdToken;
        gfnToken = _gfnToken;
        burnContract = _burnContract;
        transferOwnership(owner);
    }

    function setExecutor(IExecuteTimelock _executor) external override onlyOwner {
        executor = _executor;
        emit SetExecutor(address(_executor));
    }

    function setUSDToken(IERC20 _usdToken) external override onlyOwner {
        usdToken = _usdToken;
        emit SetUSDToken(address(_usdToken));
    }

    function setGFNToken(IERC20 _gfnToken) external override onlyOwner {
        gfnToken = _gfnToken;
        emit SetGFNToken(address(_gfnToken));
    }

    function setHolderContract(IGFNHolder _holderContract) external override onlyOwner {
        holderContract = _holderContract;
        emit SetHolder(address(_holderContract));
    }

    function setBurnContract(IBurn _burnContract) external override onlyOwner {
        burnContract = _burnContract;
        emit SetBurn(address(_burnContract));
    }

    function _createStore(IERC20 tokenA, IERC20 tokenB, string memory poolId) private returns (address store)  {
        bytes memory bytecode = type(GFNStore).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(tokenA, tokenB, poolId));
        assembly {
            store := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        IGFNStore(store).initialize(tokenA, tokenB);
        getStore[poolId][tokenA][tokenB] = store;
        allStores.push(store);
    }

    function createPool(string memory poolId) external override onlyOwnerOrExecutor validPool(poolId) {
        require(getStore[poolId][usdToken][gfnToken] == address(0), 'Pool exists'); // single check is sufficient

        // create store
        address newStore = _createStore(usdToken, gfnToken, poolId);
        allPools.push(poolId);
        // console.log("store addres : ",newStore);
        emit CreatePool(newStore);
    }

    function invest(address investor, string memory poolId, uint256 amount) external override onlyOwner existPool(poolId) {
        require(amount > 0, "Invalid amount");
        IUSDToken(address(usdToken)).mintUSD(address(this), amount);
        totalMoneyInvested[investor][poolId] += amount;
        totalMoneyInQueue[investor][poolId] += amount;

        balanceOfPool[poolId] += amount;
        //create queue
        queue[poolId][last[poolId]] = QueueData(investor, amount);
        last[poolId]++;
        emit Invest(investor, poolId, amount);
    }

    function startBorrowing(address customer, string memory poolId, uint256 amount) external override onlyOwner existPool(poolId) {
        require(amount > 0, "Invalid amount");
        require(amount <= balanceOfPool[poolId], "Pool does not have enough balance");

        uint256 amountTmp = amount;
        QueueData storage tmpQueueData;
        address tmpStore = getStoreInPoolId(poolId);
        do {
            tmpQueueData = queue[poolId][first[poolId]];
            if (amountTmp < tmpQueueData.usdAmount) {
                tmpQueueData.usdAmount -= amountTmp;

                totalMoneyInQueue[tmpQueueData.investor][poolId] -= amountTmp;
                IGFNStore(tmpStore).storeMint(tmpQueueData.investor, amountTmp);   // mint RP token to investor
                break;

            } else {
                if(tmpQueueData.usdAmount > 0){
                    IGFNStore(tmpStore).storeMint(tmpQueueData.investor, tmpQueueData.usdAmount);
                }
                amountTmp -= tmpQueueData.usdAmount;
                totalMoneyInQueue[tmpQueueData.investor][poolId] -= tmpQueueData.usdAmount;
                delete queue[poolId][first[poolId]];
                first[poolId]++;
            }

        } while (amountTmp > 0);

        burnContract.setBorrowStove(customer, amount);
        SafeERC20.safeTransfer(IERC20(usdToken), address(burnContract), amount);
        holderContract.setPoolOfUser(poolId, customer);
        holderContract.increaseDebtOfUser(customer, amount);
        balanceOfPool[poolId] -= amount;

        emit StartBorrowing(customer, poolId, amount);
    }

    function finishBorrowing(address customer) external override onlyOwner {
        burnContract.burnBorrowStove(customer);
        emit FinishBorrowing(customer);
    }

    function _updateQueueBalance(address investor, string memory poolId, uint256 withdrawableAmount) private {
        if (withdrawableAmount == 0) return;

        uint256 tempWithdrawAmount = withdrawableAmount;
        QueueData storage tmpQueueData;
        for (uint256 index = first[poolId]; index < last[poolId]; index++) {
            tmpQueueData = queue[poolId][index];
            if (tmpQueueData.investor == investor) {
                if (tempWithdrawAmount < tmpQueueData.usdAmount) {
                    tmpQueueData.usdAmount -= tempWithdrawAmount;
                } else {
                    tempWithdrawAmount -= tmpQueueData.usdAmount;
                    tmpQueueData.usdAmount = 0;
                }

                if (tempWithdrawAmount == 0) break;
            }
        }

        balanceOfPool[poolId] -= withdrawableAmount;
        totalMoneyInQueue[investor][poolId] -= withdrawableAmount;
    }

    function _calWithdrawAmountInStore(address store, uint256 RPAmount) private view returns (uint256 balanceUsd, uint256 balanceToken)  {
        uint256 totalRP = IGFNStore(store).totalSupply();
        if (totalRP == 0) return (0, 0);

        uint256 balanceUSDOfStore = usdToken.balanceOf(store);
        uint256 balanceTokenOfStore = gfnToken.balanceOf(store);

        balanceUsd = RPAmount * balanceUSDOfStore / totalRP;
        balanceToken = RPAmount * balanceTokenOfStore / totalRP;
    }

    function startExiting(address investor, string memory poolId, uint256 amount) external override onlyOwner existPool(poolId) {
        uint256 queueBalance = totalMoneyInQueue[investor][poolId];
        require(amount <= queueBalance, "Amount exceeds queue balance");

        address tmpStore = getStoreInPoolId(poolId);
        uint256 storeRPBalance = IGFNStore(tmpStore).balanceOf(investor);
        require((queueBalance + storeRPBalance) > 0, "Not enough balance");

        uint256 withdrawableAmountInQueue = amount;
        if (withdrawableAmountInQueue > 0) {
            _updateQueueBalance(investor, poolId, withdrawableAmountInQueue);
            // transfer USDToken from queue to burn contract
            SafeERC20.safeTransfer(IERC20(usdToken), address(burnContract), withdrawableAmountInQueue);
        }

        (uint256 balanceUsdWithdraw, uint256 balanceTokenWithdraw) = _calWithdrawAmountInStore(tmpStore, storeRPBalance);

        //burn token
        IGFNStore(tmpStore).storeBurn(investor, address(burnContract), storeRPBalance, balanceUsdWithdraw, balanceTokenWithdraw);

        burnContract.setInvestorStove(investor, poolId, withdrawableAmountInQueue + balanceUsdWithdraw);
        emit StartExiting(investor, poolId, withdrawableAmountInQueue + balanceUsdWithdraw, balanceTokenWithdraw);
    }

    function finishExiting(address investor, string memory poolId) external override onlyOwner existPool(poolId) {
        uint256 burnAmount = burnContract.getInvestorStove(investor, poolId);
        burnContract.burnInvestorStove(investor, poolId);
        emit FinishExiting(investor, poolId, burnAmount);
    }

    function allStoresLength() external view returns (uint) {
        return allStores.length;
    }

    function getStoreInPoolId(string memory poolId) public override view returns (address store) {
        return getStore[poolId][usdToken][gfnToken];
    }

    function getBalanceStoreOfPool(string memory poolId) public override view returns (uint256 usd, uint256 life) {
        address tmpStore = getStoreInPoolId(poolId);
        usd = usdToken.balanceOf(tmpStore);
        life = gfnToken.balanceOf(tmpStore);
    }

    function canInvestorExit(address investor, string memory poolId, uint256 amount) external override view existPool(poolId) returns (bool canExit) {
        address tmpStore = getStoreInPoolId(poolId);
        uint256 queueBalance = totalMoneyInQueue[investor][poolId];
        uint256 storeRPBalance = IGFNStore(tmpStore).balanceOf(investor);
        canExit = queueBalance + storeRPBalance > 0 && queueBalance >= amount;
    }

    function getInvestedPools(address investor) external override view returns (InvestedPool[] memory investedPools) {
        uint256 investedPoolNumber = _getInvestedPoolNumber(investor);
        investedPools = new InvestedPool[](investedPoolNumber);

        uint256 counter;
        for (uint256 i = 0; i < allPools.length; i++) {
            string memory poolId = allPools[i];

            if (totalMoneyInvested[investor][poolId] > 0) {
                address tmpStore = getStoreInPoolId(poolId);
                uint256 investedTotal = totalMoneyInvested[investor][poolId];
                uint256 notBorrowedTotal = totalMoneyInQueue[investor][poolId];
                uint256 borrowedTotal = IGFNStore(tmpStore).balanceOf(investor);

                InvestedPool memory investedPool = InvestedPool(
                    poolId,
                    investedTotal,
                    notBorrowedTotal,
                    borrowedTotal
                );
                investedPools[counter++] = investedPool;
            }
        }
    }

    function getDetailInvestedPool(address investor, string memory poolId) external override view existPool(poolId) returns (InvestedPool memory investedPool) {
        address tmpStore = getStoreInPoolId(poolId);
        investedPool = InvestedPool(
            poolId,
            totalMoneyInvested[investor][poolId],
            totalMoneyInQueue[investor][poolId],
            IGFNStore(tmpStore).balanceOf(investor)
        );
    }

    function getProfit(address investor) external override view returns (Profit[] memory profits) {
        uint256 investedPoolNumber = _getInvestedPoolNumber(investor);
        profits = new Profit[](investedPoolNumber);

        uint256 counter;
        for (uint256 i = 0; i < allPools.length; i++) {
            string memory poolId = allPools[i];

            if (totalMoneyInvested[investor][poolId] > 0) {
                address tmpStore = getStoreInPoolId(poolId);
                uint256 storeRPBalance = IGFNStore(tmpStore).balanceOf(investor);
                (uint256 usdProfit, uint256 lifeProfit) = _calWithdrawAmountInStore(tmpStore, storeRPBalance);

                Profit memory profit = Profit(
                    poolId,
                    usdProfit,
                    lifeProfit
                );
                profits[counter++] = profit;
            }
        }
    }

    function getBorrowedPool(address customer) external override view returns (string memory poolId, uint256 amount) {
        poolId = holderContract.getPoolOfUser(customer);
        amount = holderContract.getDebt(customer);
    }

    function getBalanceOfPool(string memory poolId) external override view existPool(poolId) returns (uint256) {
        return balanceOfPool[poolId];
    }

    function _getInvestedPoolNumber(address investor) private view returns (uint256 investedPoolNumber) {
        for (uint256 i = 0; i < allPools.length; i++) {
            if (totalMoneyInvested[investor][allPools[i]] > 0) {
                investedPoolNumber++;
            }
        }
    }
}

