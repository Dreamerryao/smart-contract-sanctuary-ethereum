pragma solidity 0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./IGFNHolder.sol";
import "./IBurn.sol";
import "./IExecuteTimelock.sol";

interface IReservePool {

    struct InvestedPool {
        string poolId;
        uint256 investedTotal;
        uint256 notBorrowedTotal;
        uint256 borrowedTotal;
    }

    struct Profit {
        string poolId;
        uint256 usd;
        uint256 life;
    }

    event CreatePool(address newStore);
    event Invest(address investor, string poolId, uint256 indexed amount);
    event StartExiting(address investor, string poolId, uint256 indexed usdAmount, uint256 indexed lifeAmount);
    event FinishExiting(address investor, string poolId, uint256 indexed amount);
    event StartBorrowing(address customer, string poolId, uint256 indexed amount);
    event FinishBorrowing(address customer);

    function setExecutor(IExecuteTimelock executor) external;
    function setUSDToken(IERC20 usdToken) external;
    function setGFNToken(IERC20 gfnToken) external;
    function setHolderContract(IGFNHolder holderContract) external;
    function setBurnContract(IBurn burnContract) external;

    function invest(address investor, string memory poolId, uint256 amount) external;
    function startExiting(address investor, string memory poolId, uint256 amount) external;
    function finishExiting(address investor, string memory poolId) external;

    function startBorrowing(address customer, string memory poolId, uint256 amount) external;
    function finishBorrowing(address customer) external;

    function createPool(string memory poolId) external;
    function getStoreInPoolId(string memory poolId) external returns (address store);
    function getBalanceStoreOfPool(string memory poolId) external view returns (uint256 usd, uint256 life);
    function canInvestorExit(address investor, string memory poolId, uint256 amount) external view returns (bool canExit);

    function getInvestedPools(address investor) external view returns (InvestedPool[] memory investedPools);
    function getDetailInvestedPool(address investor, string memory poolId) external view returns (InvestedPool memory investedPool);
    function getProfit(address investor) external view returns (Profit[] memory profits);
    function getBorrowedPool(address customer) external view returns (string memory poolId, uint256 amount);
    function getBalanceOfPool(string memory poolId) external view returns (uint256);
}

