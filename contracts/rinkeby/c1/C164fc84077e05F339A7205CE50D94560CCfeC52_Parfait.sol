//compounds interace
interface CErc20 {
    function mint(uint256) external returns (uint256);

    function exchangeRateStored() external view returns (uint256);

    function supplyRatePerBlock() external returns (uint256);

    function redeem(uint) external returns (uint);

    function redeemUnderlying(uint) external returns (uint);

    function balanceOf(address) external view returns (uint);
}

interface CEther {
    function mint() external payable;

    function exchangeRateStored() external view returns (uint256);

    function redeem(uint) external returns (uint);

    function redeemUnderlying(uint) external returns (uint);

    function balanceOf(address) external view returns (uint);
}

interface IWETH is IERC20 {
    function deposit() external payable;
    function withdraw(uint) external;
}

contract Parfait {
    // all addresses on Rinkeby
    IWETH constant WETH = IWETH(0xc778417E063141139Fce010982780140Aa0cD5Ab);
    IERC20 constant WBTC = IERC20(0x577D296678535e4903D59A4C929B718e1D575e0A);
    IERC20 constant DAI = IERC20(0x5592EC0cfb4dbc12D3aB100b257153436a1f0FEa);

    CEther constant CETH = CEther(0xd6801a1DfFCd0a410336Ef88DeF4320D6DF1883e);
    CErc20 constant CWBTC = CErc20(0x0014F450B8Ae7708593F4A46F8fa6E5D50620F96);
    CErc20 constant CDAI = CErc20(0x6D7F0754FFeb405d23C51CE938289d4835bE3b14);

    //DAI2 = Aave's version of DAI on Rinkeby
    IERC20 constant DAI2 = IERC20(0x4aAded56bd7c69861E8654719195fCA9C670EB45);
    IERC20 constant ADAI2 = IERC20(0x49866611AA7Dc30130Ac6A0DF29217D16FD87bc0);
    //Pool-Proxy-Aave address on Rinkeby
    IPool constant AAVEPool = IPool(0xE039BdF1d874d27338e09B55CB09879Dedca52D8);

    AggregatorV3Interface internal constant ETHPriceFeed =
        AggregatorV3Interface(0x8A753747A1Fa494EC906cE90E9f37563A8AF630e);
    AggregatorV3Interface internal constant BTCPriceFeed =
        AggregatorV3Interface(0xECe365B379E1dD183B20fc5f022230C044d51404);
    AggregatorV3Interface internal constant DAIPriceFeed =
        AggregatorV3Interface(0x2bA49Aaa16E6afD2a993473cfB70Fa8559B523cF);

    ISwapRouter internal constant swapRouter = ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);

    //this function receives ether, updates allocations and then rebalances 
    function rebalance(
        int _CETHAllocation,
        int _CWBTCAllocation,
        int _CDAIAllocation,
        int _ADAI2Allocation
    ) external payable {
        require(
            _CETHAllocation + _CWBTCAllocation + _CDAIAllocation + _ADAI2Allocation == 100,
            "invalid allocations sum"
        );

        sell();

        //deposit any ETH (from new deposits or redeeming CETH for ETH above) into WETH
        uint balance = address(this).balance;
        if(balance > 0) {
            WETH.deposit{ value: balance }();
        }

        buy(_CETHAllocation, _CWBTCAllocation, _CDAIAllocation, _ADAI2Allocation);
    }

    // sell all to ETH and send out
    function withdraw() external {
        sell();
        //withdraw balance of WETH for ETH
        uint WETHBalance = WETH.balanceOf(address(this));
        if(WETHBalance > 0) {
            WETH.withdraw(WETH.balanceOf(address(this)));
        }        
        //transfer out balance of ETH
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "transfer to owner unsuccessful");
    }

    // sells all strategies to ETH or WETH
    function sell() internal {
        uint CETHBalance = CETH.balanceOf(address(this));
        uint CWBTCBalance = CWBTC.balanceOf(address(this));
        uint CDAIBalance = CDAI.balanceOf(address(this));
        uint ADAI2Balance = ADAI2.balanceOf(address(this));

        if (CETHBalance > 0) {
            //redeem  CETH for ETH
            CETH.redeem(CETHBalance);
        }
        if (CWBTCBalance > 0) {
            //redeem CWBTC for WBTC
            CWBTC.redeem(CWBTCBalance);
            //then swap WBTC for WETH
            swap(address(WBTC), address(WETH), WBTC.balanceOf(address(this)));    
        }
        if (CDAIBalance > 0) {
            //redeem CDAI for DAI
            CDAI.redeem(CDAIBalance);
            //then swap DAI for WETH
            swap(address(DAI), address(WETH), DAI.balanceOf(address(this)));
        }
        if (ADAI2Balance > 0) {
            //redeem ADAI2 for DAI2
            AAVEPool.withdraw(address(DAI), ADAI2Balance, address(this));
            //then swap DAI2 for WETH
            swap(address(DAI2), address(WETH), DAI2.balanceOf(address(this)));
        }
    }

    //buys from WETH as per allocations
    function buy(int _CETHAllocation, int _CWBTCAllocation, int _CDAIAllocation, int _ADAI2Allocation) internal {
        uint WETHBalance = WETH.balanceOf(address(this));

        if(_CETHAllocation > 0) {
            uint CETHBuyAmount = WETHBalance * uint(_CETHAllocation) / 100;
            //withdraw WETH for ETH
            WETH.withdraw(CETHBuyAmount);
            //deposit ETH for CETH
            CETH.mint{ value: address(this).balance }();
        }
        if(_CWBTCAllocation > 0) {
            uint CWBTCBuyAmount = WETHBalance * uint(_CWBTCAllocation) / 100;
            //swap WETH for WBTC
            swap(address(WETH), address(WBTC), CWBTCBuyAmount);
            //deposit WBTC for CWBTC
            WBTC.approve(address(CWBTC), WBTC.balanceOf(address(this)));
            CWBTC.mint(WBTC.balanceOf(address(this)));
        }
        if(_CDAIAllocation > 0) {
            uint CDAIBuyAmount = WETHBalance * uint(_CDAIAllocation) / 100;
            //swap WETH for DAI
            swap(address(WETH), address(DAI), CDAIBuyAmount);
            //Deposit DAI for CDAI
            DAI.approve(address(CDAI), DAI.balanceOf(address(this)));
            CDAI.mint(DAI.balanceOf(address(this)));
        }
        if(_ADAI2Allocation > 0) {
            uint ADAI2BuyAmount = WETHBalance * uint(_ADAI2Allocation) / 100;
            //swap WETH for DAI2
            swap(address(WETH), address(DAI2), ADAI2BuyAmount);
            //Deposit DAI2 for ADAI2
            DAI2.approve(address(AAVEPool), DAI2.balanceOf(address(this)));
            AAVEPool.supply(address(DAI2), DAI2.balanceOf(address(this)), address(this), 0);
        }
    }

    //performs internal ERC20 swaps through uniswap
    function swap(address _tokenIn, address _tokenOut, uint _amount) internal {
        //approval
        TransferHelper.safeApprove(
            _tokenIn,
            address(swapRouter),
            _amount
        );

        //swap
        ISwapRouter.ExactInputSingleParams memory params =
            ISwapRouter.ExactInputSingleParams({
                tokenIn: _tokenIn,
                tokenOut: _tokenOut,
                fee: 3000,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: _amount,
                amountOutMinimum: 0, // this leaves txs vulnerable bad rates & MEV
                sqrtPriceLimitX96: 0
            });
        swapRouter.exactInputSingle(params);
    }

    receive() external payable {
    }
}

