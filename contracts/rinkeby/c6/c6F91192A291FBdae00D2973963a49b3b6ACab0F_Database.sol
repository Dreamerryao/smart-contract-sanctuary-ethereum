import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract Database is Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;
    Counters.Counter private _domainIds;
    Counters.Counter private _entryIds;

    using EnumerableSet for EnumerableSet.AddressSet;

    address private _owner;

    constructor() payable {
        _owner = payable(msg.sender);
        ClearlistedAddresses.add(_owner);
    }

    ///////////////////////////
    // CLEARLISTED ADDRESSES //
    ///////////////////////////

    EnumerableSet.AddressSet private ClearlistedAddresses;

    function addClearlistedAddress(address addr)
        public
        onlyOwner
        returns (bool)
    {
        return ClearlistedAddresses.add(addr);
    }

    function removeClearlistedAddress(address addr)
        public
        onlyOwner
        returns (bool)
    {
        return ClearlistedAddresses.remove(addr);
    }

    function isClearlistedAddress(address addr) public view returns (bool) {
        return ClearlistedAddresses.contains(addr);
    }

    ////////////
    // STATUS //
    ////////////

    enum Status {
        UNKNOWN,
        KNOWN_TRUSTED,
        KNOWN_UNTRUSTED
    }

    /////////////
    // DOMAINS //
    /////////////

    struct DomainStatus {
        // i.e. zip3.com is composed of:
        //  - tld, top-level domain: .com
        //  - root domain: zip3.com
        //  - a domain could have a wildcard, or a subdomain:
        //    - *.zip3.com
        //    - web.zip3.com
        string domain;
        Status status;
    }

    mapping(uint256 => string) private DomainNames;
    mapping(string => uint256) private DomainIds;
    mapping(string => Status) private DomainStatuses;

    function allDomainStatuses() public view returns (DomainStatus[] memory) {
        // TODO
    }

    ///////////////
    // CONTRACTS //
    ///////////////

    struct ContractStatus {
        // right now, let's require all contracts to be registered on their own
        // chain in our database on that chain
        address contractAddress;
        Status status;
    }

    mapping(address => Status) private ContractStatuses;

    function allContractStatuses()
        public
        view
        returns (ContractStatus[] memory)
    {
        // TODO
    }

    /////////////
    // ENTRIES //
    /////////////

    mapping(uint256 => EntryBase) private EntryIdToEntry;
    mapping(address => mapping(uint256 => bool))
        private EntryOwnerToEntryOwnership;
    mapping(uint256 => mapping(string => bool)) private EntryIdToDomainOwnerhip;
    mapping(uint256 => mapping(address => bool))
        private EntryIdToContractOwnerhip;

    uint256 EntryIdCursor = 0;
    struct EntryBase {
        uint256 id;
        // owner can be:
        //  - a multisig, for trusted entries
        //  - an admin, for untrusted entries
        address owner;
        // A short, human readable name describing the canonical and/or
        // colloquial name of the entity which is or would be included in the
        // owner multi-sig
        string legalName;
        Status status;
        uint256[] domainIds;
        address[] contractAddresses;

        // bool isDeactivated;
    }

    struct EntryExtended {
        uint256 id;
        // owner can be:
        //  - a multisig, for trusted entries
        //  - an admin, for untrusted entries
        address owner;
        // A short, human readable name describing the canonical and/or
        // colloquial name of the entity which is or would be included in the
        // owner multi-sig
        string legalName;
        Status status;
        // i.e. zip3.com is composed of:
        //  - tld, top-level domain: .com
        //  - root domain: zip3.com
        // Domain[] domains;
        DomainStatus[] domains;
        // right now, let's require all contracts to be registered on their own
        // chain in our database on that chain
        // Contract[] contracts;
        ContractStatus[] contractAddresses;

        // bool isDeactivated;
    }

    // struct EntryMeta {
    //     string coinMarketCapId;
    // }

    event EntryCreated(
        uint256 indexed id,
        address owner,
        string legalName,
        string domain,
        Status status
    );

    event EntryUpdated(
        uint256 indexed id,
        address owner,
        string legalName,
        Status status
    );

    event EntryDomainUpdated(
        uint256 indexed entryId,
        uint256 domainId,
        string domainName,
        Status status
    );

    event EntryContractUpdated(
        uint256 indexed entryId,
        address contractAddress,
        Status status
    );

    function createEntry(
        string memory legalName,
        string memory domain,
        Status status
    ) public payable nonReentrant returns (uint256) {
        require(
            isClearlistedAddress(msg.sender),
            "Permission denied to non-clearlisted message sender."
        );
        require(
            DomainStatuses[domain] == Status.UNKNOWN,
            "Domain already has known status."
        );

        // Insert Domain

        _domainIds.increment();
        uint256 domainId = _domainIds.current();

        DomainIds[domain] = domainId;
        DomainNames[domainId] = domain;
        DomainStatuses[domain] = status;

        // Insert Entry

        _entryIds.increment();
        uint256 entryId = _entryIds.current();

        uint[] memory domain_ids = new uint[](1);
        domain_ids[0] = domainId;

        EntryIdToEntry[entryId] = EntryBase(
            entryId,
            msg.sender,
            legalName,
            status,
            domain_ids,
            new address[](0)
        );

        EntryOwnerToEntryOwnership[msg.sender][entryId] = true;

        emit EntryCreated(entryId, msg.sender, legalName, domain, status);

        return entryId;
    }

    function getTotalEntryCount() public view returns (uint256) {
        return _entryIds.current();
    }

    function getDomainStatus(string memory domain)
        public
        view
        returns (Status)
    {
        return DomainStatuses[domain];
    }

    function getContractStatus(address contractAddress)
        public
        view
        returns (Status)
    {
        return ContractStatuses[contractAddress];
    }

    function isEntryOwner(uint256 id, address addr) public view returns (bool) {
        return EntryOwnerToEntryOwnership[addr][id];
    }

    function isEntryDomain(uint256 id, string memory domain)
        public
        view
        returns (bool)
    {
        return EntryIdToDomainOwnerhip[id][domain];
    }

    function isEntryContract(uint256 id, address contractAddress)
        public
        view
        returns (bool)
    {
        return EntryIdToContractOwnerhip[id][contractAddress];
    }

    // ONLYROLE: Entry Owner Multi-sig
    function updateEntry(
        uint256 id,
        string memory legalName,
        Status status
    ) public {
        require(
            isEntryOwner(id, msg.sender),
            "Permission denied. Message sender does not own specified entry."
        );

        EntryBase storage entry = EntryIdToEntry[id];

        entry.legalName = legalName;
        entry.status = status;

        emit EntryUpdated(id, entry.owner, legalName, status);
    }

    // ONLYROLE: Entry Owner Multi-sig
    function updateEntryDomain(
        uint256 entryId,
        string memory domain,
        Status status
    ) public {
        require(
            isEntryOwner(entryId, msg.sender),
            "Permission denied. Message sender does not own specified entry."
        );

        EntryBase storage entry = EntryIdToEntry[entryId];

        bool isDomainKnown = DomainStatuses[domain] != Status.UNKNOWN;

        uint256 domainId;

        if (!isDomainKnown) {
            // Domain is unknown

            // Insert Domain

            _domainIds.increment();
            domainId = _domainIds.current();

            DomainIds[domain] = domainId;
            DomainNames[domainId] = domain;
            DomainStatuses[domain] = status;

            // Update Entry Domains

            entry.domainIds.push(domainId);
        } else {
            // Domain is known

            // Require the domain to be previously registered with this entry
            require(
                isEntryDomain(entryId, domain),
                "Permission denied. Known domain does not belong to specified entry."
            );

            domainId = DomainIds[domain];

            // Update Entry Domains
            DomainStatuses[domain] = status;
        }

        emit EntryDomainUpdated(entryId, domainId, domain, status);
    }

    // ONLYROLE: Entry Owner Multi-sig
    function updateEntryContract(
        uint256 entryId,
        address contractAddress,
        Status status
    ) public {
        require(
            isEntryOwner(entryId, msg.sender),
            "Permission denied. Message sender does not own specified entry."
        );

        EntryBase storage entry = EntryIdToEntry[entryId];

        bool isContractKnown = ContractStatuses[contractAddress] !=
            Status.UNKNOWN;

        if (!isContractKnown) {
            // Contract is unknown

            // Insert Contract

            ContractStatuses[contractAddress] = status;

            // Update Entry Contracts

            entry.contractAddresses.push(contractAddress);
        } else {
            // Contract is known

            // Require the contract to be previously registered with this entry

            require(
                isEntryContract(entryId, contractAddress),
                "Permission denied. Known contract does not belong to specified entry."
            );

            // Update Entry Contracts

            ContractStatuses[contractAddress] = status;
        }

        emit EntryContractUpdated(entryId, contractAddress, status);
    }

    function allBaseEntries() public view returns (EntryBase[] memory) {
        uint256 entryCount = _entryIds.current();
        uint256 currentIndex = 0;

        EntryBase[] memory entries = new EntryBase[](entryCount);

        for (uint256 i = 0; i < entryCount; i++) {
            EntryBase storage entry = EntryIdToEntry[i + 1];
            entries[currentIndex] = entry;
            currentIndex += 1;
        }

        return entries;
    }

    function getExtendedEntryById(uint256 id)
        public
        view
        returns (EntryExtended memory)
    {
        EntryBase memory entry = EntryIdToEntry[id];
        EntryExtended memory eentry;

        eentry.id = entry.id;
        eentry.owner = entry.owner;
        eentry.legalName = entry.legalName;
        eentry.status = entry.status;

        uint256 domainCount = entry.domainIds.length;
        DomainStatus[] memory domainStatuses = new DomainStatus[](domainCount);
        for (uint256 i = 0; i < domainCount; i++) {
            DomainStatus memory domainStatus;
            domainStatus.domain = DomainNames[entry.domainIds[i]];
            domainStatus.status = DomainStatuses[domainStatus.domain];
            domainStatuses[i] = domainStatus;
        }

        uint256 contractCount = entry.contractAddresses.length;
        ContractStatus[] memory contractStatuses = new ContractStatus[](
            contractCount
        );
        for (uint256 i = 0; i < contractCount; i++) {
            ContractStatus memory contractStatus;
            contractStatus.contractAddress = entry.contractAddresses[i];
            contractStatus.status = ContractStatuses[
                contractStatus.contractAddress
            ];
            contractStatuses[i] = contractStatus;
        }

        return eentry;
    }
}

