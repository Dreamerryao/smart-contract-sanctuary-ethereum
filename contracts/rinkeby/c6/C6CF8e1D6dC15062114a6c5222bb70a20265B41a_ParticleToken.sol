pragma solidity ^0.8.1;

import "./libraries/SafeMath.sol";
import "./libraries/ECDSA.sol";
import "./tokens/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract ParticleToken is ERC1155, Ownable {
    using SafeMath for uint;

    uint public constant tokenId = 1;

    string public symbol;

    uint public maxSupply;

    uint internal _totalSupply;

    address private citizenTokenContract;

    constructor(string memory _uri, string memory _symbol, uint _maxSupply, address _citizenContract) ERC1155(_uri) {
        symbol = _symbol;
        maxSupply = _maxSupply;
        _totalSupply = 1; //initializing with 1 to save gas from 1st mint transaction as storing null to non-zero value takes substantial amount of gas
        citizenTokenContract = _citizenContract;
    }

    function totalSupply() external view returns(uint) {
        return _totalSupply - 1;
    }

    function bidParticle(uint _amount, uint _startDate, uint _endDate, bytes memory _signature) external payable {
        _totalSupply = _totalSupply.add(_amount);
        require(_totalSupply <= maxSupply, 'PARTICLE: All offerings exhausted');
        require(block.timestamp >= _startDate && block.timestamp <= _endDate, 'PARTICLE: Mint window closed');
        
        uint price = msg.value.div(_amount);
        bytes32 payloadHash = keccak256(abi.encode(msg.sender, price, _startDate, _endDate));
        bytes32 messageHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", payloadHash));
        require(ECDSA.isValidSignatureNow(owner(), messageHash, _signature), 'PARTICLE: Invalid transaction');

        _mint(msg.sender, tokenId, _amount, '');
    }

    function increaseMaxSupply(uint _additionalSupply) external onlyOwner {
        maxSupply = maxSupply.add(_additionalSupply);
    }

    function setCitizenTokenAddress(address tokenAddress) external onlyOwner {
        citizenTokenContract = tokenAddress;
    }

    function burn(address _from, uint _amount) public {
        require(msg.sender == citizenTokenContract, 'PARTICLE: Address not allowed to burn');
        _burn(_from, tokenId, _amount);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override onlyOwner {
        console.log('inside safeTransferFrom');
        super.safeTransferFrom(from, to, id, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override onlyOwner {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }
}

