pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "../Random.sol";
import "../LabStaking.sol";
import "./VirusAttributes.sol";
import "./VirusNFT.sol";
import "../erc20/DarwinCounter.sol";

contract VirusEvolutionManager is
  Initializable,
  AccessControlEnumerableUpgradeable,
  PausableUpgradeable
{
  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

  Random private _random;
  LabStaking private _stakingContract;

  DarwinCounter private _darwin;

  VirusAttributes private _attributes;
  ERC20Burnable private _aminoErc;
  VirusNFT private _virusNft;

  EvolutionRequirements[] private _evolutionRequirements;

  mapping(uint256 => uint256) private _evolutions;
  uint8 private _attributeChange;
  bool private _onlyErc20Payment;

  struct EvolutionRequirements {
    uint256 amino;
    uint256 darwin;
  }

  function initialize(
    Random random_,
    LabStaking stakingContract_,
    DarwinCounter darwin_,
    VirusNFT virusNFT_,
    VirusAttributes attributes_,
    EvolutionRequirements[] calldata evolutionRequirements_,
    uint8 attributeChange_
  ) external initializer {
    AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);

    _random = random_;
    _stakingContract = stakingContract_;
    _darwin = darwin_;
    _virusNft = virusNFT_;
    _attributes = attributes_;
    _attributeChange = attributeChange_;
    setEvolutionRequirements(evolutionRequirements_);
  }

  function evolveWithAmino(uint256 tokenId) external whenNotPaused {
    require(tx.origin == msg.sender, "no contracts");
    require(_virusNft.ownerOf(tokenId) == msg.sender, "not token owner");
    require(!_onlyErc20Payment, "amino payment disabled");
    require(
      _evolutions[tokenId] < _evolutionRequirements.length,
      "max evolutions"
    );
    _performPayment(false, tokenId);
    _evolve(tokenId);
  }

  function evolveWithAminoERC20(uint256 tokenId) external whenNotPaused {
    require(tx.origin == msg.sender, "no contracts");
    require(_virusNft.ownerOf(tokenId) == msg.sender, "not token owner");
    require(address(_aminoErc) != address(0), "erc20 not set");
    require(
      _evolutions[tokenId] < _evolutionRequirements.length,
      "max evolutions"
    );
    _performPayment(true, tokenId);
    _evolve(tokenId);
  }

  function canEvolveWithAminoERC20(uint256 tokenId, address sender)
    public
    view
    returns (bool)
  {
    if (address(_aminoErc) == address(0)) {
      return false;
    }
    return _canEvolve(true, tokenId, sender);
  }

  function canEvolveWithAmino(uint256 tokenId, address sender)
    public
    view
    returns (bool)
  {
    if (_onlyErc20Payment) {
      return false;
    }
    return _canEvolve(false, tokenId, sender);
  }

  function _canEvolve(
    bool isAminoErc20,
    uint256 tokenId,
    address sender
  ) internal view returns (bool) {
    if (_evolutions[tokenId] >= _evolutionRequirements.length) {
      return false;
    }
    EvolutionRequirements storage requirements_ = _evolutionRequirements[
      _evolutions[tokenId]
    ];

    if (_darwin.balanceOf(sender) < requirements_.darwin) {
      return false;
    }

    if (isAminoErc20) {
      if (
        _aminoErc.balanceOf(sender) < requirements_.amino ||
        _aminoErc.allowance(sender, address(this)) < requirements_.amino
      ) {
        return false;
      }
    } else {
      if (_stakingContract.balanceOf(sender) < requirements_.amino) {
        return false;
      }
    }
    return true;
  }

  function _performPayment(bool isAminoErc20, uint256 tokenId) internal {
    EvolutionRequirements storage requirements = _evolutionRequirements[
      _evolutions[tokenId]
    ];

    _darwin.transfer(requirements.darwin, msg.sender, address(0));

    if (isAminoErc20) {
      _aminoErc.burnFrom(msg.sender, requirements.amino);
    } else {
      _stakingContract.transferTreasury(
        requirements.amino,
        msg.sender,
        address(0)
      );
    }
  }

  function _evolve(uint256 tokenId) internal {
    uint8[] memory op = _random.randoms8Bits(6, 0, 1);
    uint8[] memory val = _random.randoms8Bits(6, 0, _attributeChange);
    VirusAttributes.Attributes memory attr = _attributes.attributes(tokenId);
    VirusAttributes.Attributes memory newAttr = VirusAttributes.Attributes(
      op[0] == 0
        ? addMax(attr.virulence, val[0])
        : subMin(attr.virulence, val[0]),
      op[1] == 0
        ? addMax(attr.severity, val[1])
        : subMin(attr.severity, val[1]),
      op[2] == 0 ? addMax(attr.purity, val[2]) : subMin(attr.purity, val[2]),
      op[3] == 0
        ? addMax(attr.mortality, val[3])
        : subMin(attr.mortality, val[3]),
      op[4] == 0
        ? addMax(attr.resistance, val[4])
        : subMin(attr.resistance, val[4]),
      op[5] == 0
        ? addMax(attr.incubation, val[5])
        : subMin(attr.incubation, val[5]),
      attr.initialRarity,
      attr.generation
    );
    _evolutions[tokenId]++;
    _attributes.setAttributes(tokenId, newAttr);
  }

  function addMax(uint8 a, uint8 b) private pure returns (uint8) {
    if (a + b > 100) {
      return 100;
    }
    return a + b;
  }

  function subMin(uint8 a, uint8 b) private pure returns (uint8) {
    if (b >= a) {
      return 0;
    }
    return a - b;
  }

  function evolutionRequirements(uint256 tokenId)
    public
    view
    returns (EvolutionRequirements memory)
  {
    return _evolutionRequirements[_evolutions[tokenId]];
  }

  function evolutionRequirements()
    public
    view
    returns (EvolutionRequirements[] memory)
  {
    return _evolutionRequirements;
  }

  function evolutions(uint256 tokenId) public view returns(uint256) {
    return _evolutions[tokenId];
  }

  function setAminoErc20(Amino aminoErc_) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _aminoErc = aminoErc_;
  }

  function setEvolutionRequirements(
    EvolutionRequirements[] calldata evolutionRequirements_
  ) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    delete _evolutionRequirements; // reset length to 0
    for (uint256 i = 0; i < evolutionRequirements_.length; i++) {
      _evolutionRequirements.push(evolutionRequirements_[i]);
    }
  }

  function pause() public {
    require(hasRole(PAUSER_ROLE, msg.sender), "must have pauser role");
    _pause();
  }

  function unpause() public {
    require(hasRole(PAUSER_ROLE, msg.sender), "must have pauser role");
    _unpause();
  }

  function setDarwin(DarwinCounter darwin_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _darwin = darwin_;
  }

  function setOnlyErc20Payment(bool onlyErc20Payment_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _onlyErc20Payment = onlyErc20Payment_;
  }

  function setAttributeChange(uint8 attributeChange_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _attributeChange = attributeChange_;
  }
}

