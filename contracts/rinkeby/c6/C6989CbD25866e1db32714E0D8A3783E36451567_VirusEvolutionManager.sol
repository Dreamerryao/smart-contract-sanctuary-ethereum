pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "./StakingBonusManager.sol";

contract LabStaking is
  Initializable,
  IERC721ReceiverUpgradeable,
  AccessControlEnumerableUpgradeable,
  PausableUpgradeable
{
  using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
  bytes32 public constant TRANSFERRER_ROLE = keccak256("TRANSFERRER_ROLE");

  struct Stake {
    address owner;
    uint256 lastClaimedPeriod;
    uint256 stakedAtPeriod;
  }

  struct StakePeriod {
    uint256 start;
    uint256 totalStaked;
  }

  // tokenId => Stake
  mapping(uint256 => Stake) private _stakes;
  // user address => tokenIds staked
  mapping(address => EnumerableSetUpgradeable.UintSet) private _userStakes;

  // period number => StakePeriod
  mapping(uint256 => StakePeriod) private _stakePeriods;

  uint256 private _lastPopulatedPeriod;

  IERC721 private _nft;
  uint256 private _maxPeriodRewards;
  uint256 private _periodLength;
  uint256 private _stakingLockPeriods;

  mapping(address => uint256) private _balances;

  // for the halving mechanism
  uint256 private _halvingInPeriods;
  uint256 private _halvingReduction;
  uint256 private _nextHalving;

  StakingBonusManager private _stakingBonusManager;

  event Staked(uint256 tokenId, address owner, uint256 period);
  event UnStaked(uint256 tokenId, address owner, uint256 period);
  event RewardsCollected(
    uint256 tokenId,
    address owner,
    uint256 periods,
    uint256 amount
  );

  function initialize(
    IERC721 nft_,
    uint256 maxPeriodRewards_,
    uint256 periodLength_,
    uint256 stakingLockPeriods_,
    uint256 halvingInPeriods_,
    uint256 halvingReduction_,
    StakingBonusManager stakingBonusManager_
  ) external initializer {
    AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();
    PausableUpgradeable.__Pausable_init();
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);
    _setupRole(TRANSFERRER_ROLE, msg.sender);
    _nft = nft_;
    _maxPeriodRewards = maxPeriodRewards_;
    _periodLength = periodLength_;
    _stakingLockPeriods = stakingLockPeriods_;
    _stakePeriods[0] = StakePeriod(block.timestamp, 0);
    _lastPopulatedPeriod = 0;
    _halvingInPeriods = halvingInPeriods_;
    _halvingReduction = halvingReduction_;
    _nextHalving = halvingInPeriods_;
    _stakingBonusManager = stakingBonusManager_;
  }

  function onERC721Received(
    address operator,
    address from,
    uint256 tokenId,
    bytes calldata data
  ) external whenNotPaused returns (bytes4) {
    operator;
    data;
    require(msg.sender == address(_nft), "nft address not supported");
    require(from == tx.origin, "no contracts allowed");
    uint256 currPeriod = currentPeriod();
    _stakes[tokenId] = Stake(from, currPeriod, currPeriod);
    _userStakes[from].add(tokenId);
    if (_stakePeriods[currPeriod].start == 0) {
      // the first time an NFT is staked on this period
      _stakePeriods[currPeriod] = StakePeriod(
        _stakePeriods[_lastPopulatedPeriod].start +
          ((currPeriod - _lastPopulatedPeriod) * _periodLength),
        _stakePeriods[_lastPopulatedPeriod].totalStaked + 1
      );
      _lastPopulatedPeriod = currPeriod;
    } else {
      _stakePeriods[currPeriod].totalStaked++;
    }
    emit Staked(tokenId, from, currPeriod);
    return IERC721ReceiverUpgradeable.onERC721Received.selector;
  }

  function collectRewardsBatch(
    uint256[] calldata tokenIds,
    uint256[] calldata periods
  ) external whenNotPaused returns (uint256) {
    uint256 rewards = 0;
    for (uint256 i = 0; i < tokenIds.length; i++) {
      rewards += _collectRewards(tokenIds[i], periods[i]);
    }
    return rewards;
  }

  function _collectRewards(uint256 tokenId, uint256 periods)
    internal
    returns (uint256)
  {
    require(_stakes[tokenId].owner == msg.sender, "tokenId non owner");

    uint256 currPeriod = currentPeriod();
    uint256 tokenLastClaimedPeriod = _stakes[tokenId].lastClaimedPeriod;
    uint256 nextClaimedPeriod = tokenLastClaimedPeriod + periods;
    require(nextClaimedPeriod < currPeriod, "trying to claim too many periods");
    _calculateHalving(nextClaimedPeriod);
    StakePeriod storage lastClaimed = _stakePeriods[tokenLastClaimedPeriod];
    uint256 lastTotalStaked = lastClaimed.totalStaked;
    uint256 rewards = 0;

    for (uint256 i = tokenLastClaimedPeriod + 1; i <= nextClaimedPeriod; i++) {
      StakePeriod storage period = _stakePeriods[i];
      if (period.start == 0) {
        if (i == nextClaimedPeriod) {
          // last one that will be processed as part of this tx
          // It is empty and this will be the lastClaimedPeriod, so we need this
          // period to be actually populated.
          _stakePeriods[i] = StakePeriod(
            lastClaimed.start +
              ((nextClaimedPeriod - tokenLastClaimedPeriod) * _periodLength),
            lastTotalStaked
          );
          _lastPopulatedPeriod = i;
        }
        rewards += (_maxPeriodRewards / lastTotalStaked);
      } else {
        lastTotalStaked = period.totalStaked;
        rewards += (_maxPeriodRewards / lastTotalStaked);
      }
    }
    rewards = _stakingBonusManager.applyAllBonuses(
      rewards,
      currPeriod,
      tokenLastClaimedPeriod,
      msg.sender
    );
    _balances[msg.sender] += rewards;
    _stakes[tokenId].lastClaimedPeriod = nextClaimedPeriod;
    emit RewardsCollected(tokenId, msg.sender, periods, rewards);
    return rewards;
  }

  function _calculateHalving(uint256 period) internal {
    if (period >= _nextHalving) {
      uint256 reduction = (_maxPeriodRewards * _halvingReduction) / 100;
      _maxPeriodRewards = _maxPeriodRewards - reduction;
      _nextHalving = _nextHalving + _halvingInPeriods;
    }
  }

  function collectAllRewardsBatch(uint256[] calldata tokenIds)
    external
    whenNotPaused
  {
    uint256 currPeriod = currentPeriod();
    for (uint256 i = 0; i < tokenIds.length; i++) {
      _collectRewards(
        tokenIds[i],
        currPeriod - _stakes[tokenIds[i]].lastClaimedPeriod - 1
      );
    }
  }

  // currPeriod is a param to avoid calling currentPeriod() in loop
  function _collectAllRewards(uint256 tokenId, uint256 currPeriod) internal {
    _collectRewards(
      tokenId,
      currPeriod - _stakes[tokenId].lastClaimedPeriod - 1
    );
  }

  function pendingRewards(uint256 tokenId, address addr)
    public
    view
    returns (uint256)
  {
    uint256 currPeriod = currentPeriod();
    StakePeriod storage lastClaimed = _stakePeriods[
      _stakes[tokenId].lastClaimedPeriod
    ];
    uint256 lastTotalStaked = lastClaimed.totalStaked;
    if (lastTotalStaked == 0) {
      return 0;
    }
    if (currPeriod == _stakes[tokenId].lastClaimedPeriod) {
      return 0;
    }
    if (_stakes[tokenId].owner == address(0)) {
      return 0;
    }
    uint256 periods = currPeriod - _stakes[tokenId].lastClaimedPeriod - 1;
    uint256 nextClaimedPeriod = _stakes[tokenId].lastClaimedPeriod + periods;

    uint256 rewards = 0;

    for (
      uint256 i = _stakes[tokenId].lastClaimedPeriod + 1;
      i <= nextClaimedPeriod;
      i++
    ) {
      StakePeriod storage period = _stakePeriods[i];
      if (period.start == 0) {
        rewards += (_maxPeriodRewards / lastTotalStaked);
      } else {
        lastTotalStaked = period.totalStaked;
        rewards += (_maxPeriodRewards / lastTotalStaked);
      }
    }
    rewards = _stakingBonusManager.applyAllBonuses(
      rewards,
      currPeriod,
      _stakes[tokenId].lastClaimedPeriod,
      addr
    );
    return rewards;
  }

  function unstakeBatch(uint256[] calldata tokenIds) external whenNotPaused {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      _unstake(tokenIds[i]);
    }
  }

  function _unstake(uint256 tokenId) internal {
    uint256 currPeriod = currentPeriod();
    require(_stakes[tokenId].owner == msg.sender, "tokenId non owner");
    require(
      _stakes[tokenId].stakedAtPeriod + _stakingLockPeriods <= currPeriod,
      "locking period not met"
    );
    require(
      _stakes[tokenId].lastClaimedPeriod == currPeriod - 1,
      "pending rewards"
    );

    // giving back what is yours
    _nft.transferFrom(address(this), _stakes[tokenId].owner, tokenId);

    // clearing state vars
    _stakes[tokenId].owner = address(0);
    // changing total staked. checking if current period exists
    if (_stakePeriods[currPeriod].start == 0) {
      // period does not exist. Creating it
      _stakePeriods[currPeriod] = StakePeriod(
        _stakePeriods[_lastPopulatedPeriod].start +
          ((currPeriod - _lastPopulatedPeriod) * _periodLength),
        _stakePeriods[_lastPopulatedPeriod].totalStaked - 1
      );
      _lastPopulatedPeriod = currPeriod;
    } else {
      // period exists. Modifying it
      _stakePeriods[currPeriod].totalStaked--;
    }
    _userStakes[msg.sender].remove(tokenId);
    emit UnStaked(tokenId, msg.sender, currPeriod);
  }

  function exitBatch(uint256[] calldata tokenIds) external whenNotPaused {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      _exit(tokenIds[i]);
    }
  }

  function _exit(uint256 tokenId) public {
    uint256 currPeriod = currentPeriod();
    _collectAllRewards(tokenId, currPeriod);
    _unstake(tokenId);
  }

  function transferTreasury(
    uint256 amount,
    address from_,
    address to
  ) public {
    require(
      hasRole(TRANSFERRER_ROLE, msg.sender),
      "must have transferrer role"
    );
    uint256 senderBalance = _balances[from_];
    require(senderBalance >= amount, "transfer amount exceeds balance");
    _balances[from_] = senderBalance - amount;
    _balances[to] += amount;
  }

  function setMaxPeriodRewards(uint256 maxPeriodRewards_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _maxPeriodRewards = maxPeriodRewards_;
  }

  function setStakingLockPeriods(uint256 stakingLockPeriods_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _stakingLockPeriods = stakingLockPeriods_;
  }

  function setHalvingInPeriods(uint256 halvingInPeriods_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _halvingInPeriods = halvingInPeriods_;
  }

  function setHalvingReduction(uint256 halvingReduction_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _halvingReduction = halvingReduction_;
  }

  function setNextHalving(uint256 nextHalving_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _nextHalving = nextHalving_;
  }

  function pause() public virtual {
    require(hasRole(PAUSER_ROLE, msg.sender), "must have pauser role");
    _pause();
  }

  function unpause() public virtual {
    require(hasRole(PAUSER_ROLE, msg.sender), "must have pauser role");
    _unpause();
  }

  function currentPeriod() public view returns (uint256) {
    return (block.timestamp - _stakePeriods[0].start) / _periodLength;
  }

  function secondsLeftToClaim(uint256 tokenId) public view returns (uint256) {
    uint256 nextPeriodToClaim = _stakes[tokenId].lastClaimedPeriod + 1;
    (
      uint256 nextPeriodStartTimestamp,
      uint256 nextPeriodEndTimestamp
    ) = periodTimestamp(nextPeriodToClaim);
    nextPeriodStartTimestamp;
    if (nextPeriodEndTimestamp <= block.timestamp) {
      return 0;
    }
    return nextPeriodEndTimestamp - block.timestamp;
  }

  // returns (periodStartTimestamp, periodEndTimestamp)
  function periodTimestamp(uint256 period)
    public
    view
    returns (uint256, uint256)
  {
    uint256 periodStart = _stakePeriods[0].start + (period * _periodLength);
    return (periodStart, periodStart + _periodLength);
  }

  function totalStaked() public view returns (uint256) {
    return _stakePeriods[_lastPopulatedPeriod].totalStaked;
  }

  function stakes(uint256 tokenId) public view returns (Stake memory) {
    return _stakes[tokenId];
  }

  function userStakesLength(address user) public view returns (uint256) {
    return _userStakes[user].length();
  }

  function userStake(address user, uint256 index)
    public
    view
    returns (uint256)
  {
    return _userStakes[user].at(index);
  }

  function stakePeriods(uint256 periodNumber)
    public
    view
    returns (StakePeriod memory)
  {
    return _stakePeriods[periodNumber];
  }

  function lastClaimedPeriod(uint256 tokenId) public view returns (uint256) {
    return _stakes[tokenId].lastClaimedPeriod;
  }

  function stakedAtPeriod(uint256 tokenId) public view returns (uint256) {
    return _stakes[tokenId].stakedAtPeriod;
  }

  function balanceOf(address user) public view returns (uint256) {
    return _balances[user];
  }

  function lastPopulatedPeriod() public view returns (uint256) {
    return _lastPopulatedPeriod;
  }

  function stakingLockPeriods() public view returns (uint256) {
    return _stakingLockPeriods;
  }

  function maxPeriodRewards() public view returns (uint256) {
    return _maxPeriodRewards;
  }

  function halvingInPeriods() public view returns (uint256) {
    return _halvingInPeriods;
  }

  function halvingReduction() public view returns (uint256) {
    return _halvingReduction;
  }

  function nextHalving() public view returns (uint256) {
    return _nextHalving;
  }
}

// SPDX-License-Identifier: GPL-3.0-only
pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "../Random.sol";

contract VirusAttributes is Initializable, AccessControlEnumerableUpgradeable {
  bytes32 public constant ATTR_ROLE = keccak256("ATTR_ROLE");

  struct Attributes {
    uint8 virulence;
    uint8 severity;
    uint8 purity;
    uint8 mortality;
    uint8 resistance;
    uint8 incubation;
    // rarity initially goes from 1 to 5
    uint8 initialRarity;
    uint8 generation;
  }

  struct BodyPartsColors {
    uint24 mouth;
    uint24 eyes;
    uint24 membrane;
    uint24 texture;
    uint24 body;
    uint24 tail;
  }

  struct AttributesConfiguration {
    uint8 min;
    uint8 max;
  }

  struct RarityConfiguration {
    uint256 rarity;
    uint256 chance;
  }

  Random private _random;

  uint8 _currentGeneration;

  // tokenId -> Attributes
  mapping(uint256 => Attributes) private _attributes;
  // tokenId -> BodyPartsColors
  mapping(uint256 => BodyPartsColors) private _bodyPartsColors;
  // initialRarity => config
  mapping(uint256 => AttributesConfiguration) private _attributesConfiguration;

  uint8[] private _rarityChances;
  uint24[] private _bodyPartsInitialColorsConfiguration;

  event AttributesChanged(uint256 tokenId, Attributes attributes);

  event BodyPartsColorsChanged(
    uint256 tokenId,
    BodyPartsColors bodyPartsColors
  );

  function initialize(
    Random random_,
    AttributesConfiguration[] memory config_,
    uint8[] memory rarityChances_,
    uint24[] memory bodyPartsInitialColorsConfiguration_
  ) external initializer {
    AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();

    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

    _random = random_;
    _rarityChances = rarityChances_;
    _bodyPartsInitialColorsConfiguration = bodyPartsInitialColorsConfiguration_;
    _setConfiguration(config_);
  }

  function randomizeAttributes(uint256 tokenId, uint8 rarity_) public {
    require(hasRole(ATTR_ROLE, msg.sender), "attr role");
    _randomizeInitialAttributes(tokenId, rarity_);
  }

  function setAttributes(uint256 tokenId, Attributes memory attr_) public {
    require(hasRole(ATTR_ROLE, msg.sender), "attr role");

    _attributes[tokenId] = attr_;

    emit AttributesChanged(tokenId, attr_);
  }

  function setBodyPartsColors(
    uint256 tokenId,
    BodyPartsColors memory bodyPartsColors_
  ) public {
    require(hasRole(ATTR_ROLE, msg.sender), "attr role");

    _bodyPartsColors[tokenId] = bodyPartsColors_;

    emit BodyPartsColorsChanged(tokenId, bodyPartsColors_);
  }

  function randomizeBodyColors(uint256 tokenId) public {
    require(hasRole(ATTR_ROLE, msg.sender), "attr role");
    _randomizeInitialBodyColors(tokenId);
  }

  function _randomizeInitialAttributes(uint256 tokenId, uint8 rarity_) internal {

    uint8 rarity;
    if(rarity_ == 0) {
      rarity = _calculateRarity(_random.randoms8Bits(1, 1, 100)[0]);
    } else {
      rarity = _calculateRarity(rarity_);
    }
    uint8[] memory randomAttributes = _random.randoms8Bits(
      6,
      _attributesConfiguration[rarity].min,
      _attributesConfiguration[rarity].max
    );
    setAttributes(
      tokenId,
      Attributes(
        randomAttributes[0],
        randomAttributes[1],
        randomAttributes[2],
        randomAttributes[3],
        randomAttributes[4],
        randomAttributes[5],
        rarity,
        _currentGeneration
      )
    );
  }

  function _randomizeInitialBodyColors(uint256 tokenId) internal {
    uint8[] memory randomColorsIndexes = _random.randoms8Bits(
      6,
      0,
      uint8(_bodyPartsInitialColorsConfiguration.length) - 1
    );

    setBodyPartsColors(
      tokenId,
      BodyPartsColors(
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[0]],
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[1]],
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[2]],
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[3]],
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[4]],
        _bodyPartsInitialColorsConfiguration[randomColorsIndexes[5]]
      )
    );
  }

  function _calculateRarity(uint8 rarity) internal view returns (uint8) {
    if (rarity <= _rarityChances[0]) {
      return 5;
    }
    if (rarity <= _rarityChances[1]) {
      return 4;
    }
    if (rarity <= _rarityChances[2]) {
      return 3;
    }
    if (rarity <= _rarityChances[3]) {
      return 2;
    }
    return 1;
  }

  function _setConfiguration(AttributesConfiguration[] memory config_)
    internal
  {
    for (uint256 i = 0; i < config_.length; i++) {
      _attributesConfiguration[i + 1] = config_[i];
    }
  }

  function attributes(uint256 tokenId) public view returns (Attributes memory) {
    return _attributes[tokenId];
  }

  function bodyColors(uint256 tokenId)
    public
    view
    returns (BodyPartsColors memory)
  {
    return _bodyPartsColors[tokenId];
  }

  function setRarityChances(uint8[] memory rarityChances_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _rarityChances = rarityChances_;
  }

  function setCurrentGeneration(uint8 newGen_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _currentGeneration = newGen_;
  }

  function setConfiguration(AttributesConfiguration[] memory config_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _setConfiguration(config_);
  }

  function setBodyPartsInitialColorsConfiguration(uint24[] memory config_)
    public
  {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    delete _bodyPartsInitialColorsConfiguration;
    _bodyPartsInitialColorsConfiguration = config_;
  }
}

// SPDX-License-Identifier: GPL-3.0-only
pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./VirusAttributes.sol";
import "../LabStaking.sol";
import "../erc20/Amino.sol";
import "../VRFConsumerBaseV2Upgradeable.sol";

contract VirusNFT is
  Initializable,
  AccessControlEnumerableUpgradeable,
  ERC721EnumerableUpgradeable,
  ERC721BurnableUpgradeable,
  PausableUpgradeable,
  VRFConsumerBaseV2Upgradeable
{
  using CountersUpgradeable for CountersUpgradeable.Counter;
  using StringsUpgradeable for uint256;
  using StringsUpgradeable for uint8;
  using StringsUpgradeable for uint24;

  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

  CountersUpgradeable.Counter private _tokenIdTracker;

  string private _baseTokenURI;
  LabStaking private _stakingContract;
  VirusAttributes private _attributes;
  Amino private _aminoErc;

  uint256 private _virusAminoPriceInWei;
  bool private _onlyErc20Payment;

  // vrf stuff
  bool private _vrfEnabled;
  VRFCoordinatorV2Interface _vrfCoordinator;
  bytes32 private _vrfKeyHash;
  uint64 private _vrfSubscriptionId;
  uint8 private _maxVirusesPerTx;
  // store chainlink VRF requestIds => information needed to resolve
  // the async flow
  mapping(uint256 => PendingVRFMint) private _vrfRequestIds;
  mapping(address => uint256) private _userRequestId;

  struct PendingVRFMint {
    address minter;
    uint8 amount;
    uint256 aminoPending;
    bool paidWithErc20;
  }

  event VirusCreated(uint256 tokenId, address to);

  function initialize(
    string memory name_,
    string memory symbol_,
    string memory baseTokenURI_,
    LabStaking stakingContract_,
    VirusAttributes attributes_,
    uint256 virusAminoPriceInWei_,
    address vrfCoordinator_,
    bytes32 vrfKeyHash_,
    uint64 vrfSubscriptionId_
  ) external initializer {
    AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();
    ERC721EnumerableUpgradeable.__ERC721Enumerable_init();
    ERC721Upgradeable.__ERC721_init(name_, symbol_);
    VRFConsumerBaseV2Upgradeable.__VRFConsumerBaseV2_init(vrfCoordinator_);
    _vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinator_);
    _vrfSubscriptionId = vrfSubscriptionId_;
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);

    _vrfEnabled = true;
    _vrfKeyHash = vrfKeyHash_;
    _maxVirusesPerTx = 8;
    _baseTokenURI = baseTokenURI_;
    _stakingContract = stakingContract_;
    _attributes = attributes_;
    _virusAminoPriceInWei = virusAminoPriceInWei_;
  }

  function mintWithAmino(uint8 amount, address to) external whenNotPaused {
    require(tx.origin == msg.sender, "no contracts");
    require(!_onlyErc20Payment, "amino payment disabled");
    require(amount > 0 && amount <= _maxVirusesPerTx, "wrong amount");
    if (!_vrfEnabled) {
      // pseudo random rarity
      for (uint256 i = 0; i < amount; i++) {
        _stakingContract.transferTreasury(
          _virusAminoPriceInWei,
          msg.sender,
          address(0)
        );
        _internalMint(to, 0);
      }
    } else {
      require(_userRequestId[msg.sender] == 0, "there is a pending request");
      // chainlink VRF rarity
      _stakingContract.transferTreasury(
        _virusAminoPriceInWei * amount,
        msg.sender,
        address(this)
      );
      uint256 requestId = _vrfCoordinator.requestRandomWords(
        _vrfKeyHash,
        _vrfSubscriptionId,
        3,
        2500000,
        1
      );

      _vrfRequestIds[requestId] = PendingVRFMint(
        msg.sender,
        amount,
        _virusAminoPriceInWei * amount,
        false
      );
      _userRequestId[msg.sender] = requestId;
    }
  }

  function mintWithAminoERC20(uint8 amount, address to)
    external
    whenNotPaused
  {
    require(tx.origin == msg.sender, "no contracts");
    require(address(_aminoErc) != address(0), "erc20 not set");
    require(amount > 0 && amount <= _maxVirusesPerTx, "wrong amount");
    if (!_vrfEnabled) {
      for (uint256 i = 0; i < amount; i++) {
        _aminoErc.burnFrom(msg.sender, _virusAminoPriceInWei);
        _internalMint(to, 0);
      }
    } else {
      require(_userRequestId[msg.sender] == 0, "there is a pending request");
      _aminoErc.transferFrom(msg.sender,  address(this), _virusAminoPriceInWei * amount);
      uint256 requestId = _vrfCoordinator.requestRandomWords(
        _vrfKeyHash,
        _vrfSubscriptionId,
        3,
        2000000,
        1
      );

      _vrfRequestIds[requestId] = PendingVRFMint(
        msg.sender,
        amount,
        _virusAminoPriceInWei * amount,
        true      );
      _userRequestId[msg.sender] = requestId;
    }
  }

  function mint(address to) external {
    require(hasRole(MINTER_ROLE, msg.sender), "must have minter role");
    _internalMint(to, 0);
  }

  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
    internal
    override
  {
    require(requestId == _userRequestId[_vrfRequestIds[requestId].minter], "bad request id");
    if(_vrfRequestIds[requestId].paidWithErc20) {
      _aminoErc.burn(_vrfRequestIds[requestId].aminoPending);
    } else {
      _stakingContract.transferTreasury(_vrfRequestIds[requestId].aminoPending, address(this), address(0));
    }
    uint8[] memory rarities = extractNumbersFromRandom(randomWords[0], _vrfRequestIds[requestId].amount, 1, 100);
    for(uint i = 0; i < rarities.length; i++) {
      _internalMint(_vrfRequestIds[requestId].minter, rarities[i]);
    }
    _userRequestId[_vrfRequestIds[requestId].minter] = 0;
    delete _vrfRequestIds[requestId];
  }

  function _internalMint(address to, uint8 rarity_) internal {
    _mint(to, _tokenIdTracker.current());
    _attributes.randomizeAttributes(_tokenIdTracker.current(), rarity_);
    _attributes.randomizeBodyColors(_tokenIdTracker.current());
    emit VirusCreated(_tokenIdTracker.current(), to);
    _tokenIdTracker.increment();
  }

  function extractNumbersFromRandom(uint256 random, uint8 noOfRandoms, uint8 min, uint8 max) private pure returns(uint8[] memory) {
    uint8[] memory response = new uint8[](noOfRandoms);
    bytes32 r = bytes32(random);
    for (uint8 i = 0; i < noOfRandoms; i++) {
      response[i] = (uint8(r[i]) % (max - min + 1)) + min;
    }
    return response;
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal virtual override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
    super._beforeTokenTransfer(from, to, tokenId);
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(
      AccessControlEnumerableUpgradeable,
      ERC721Upgradeable,
      ERC721EnumerableUpgradeable
    )
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function transferBatch(address to, uint256[] calldata tokenIds) external {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      transferFrom(msg.sender, to, tokenIds[i]);
    }
  }

  function transferBatchMultipleRecipients(
    address[] calldata to,
    uint256[] calldata tokenIds
  ) external {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      transferFrom(msg.sender, to[i], tokenIds[i]);
    }
  }

  function safeTransferBatch(address to, uint256[] calldata tokenIds) external {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      safeTransferFrom(msg.sender, to, tokenIds[i]);
    }
  }

  function safeTransferBatchMultipleRecipients(
    address[] calldata to,
    uint256[] calldata tokenIds
  ) external {
    for (uint256 i = 0; i < tokenIds.length; i++) {
      safeTransferFrom(msg.sender, to[i], tokenIds[i]);
    }
  }

  function setAminoErc20(Amino aminoErc_) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _aminoErc = aminoErc_;
  }

  function setVirusPrice(uint256 price) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _virusAminoPriceInWei = price;
  }

  function setStakingContract(LabStaking stakingContract_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _stakingContract = stakingContract_;
  }

  function setVrfEnabled(bool vrfEnabled_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _vrfEnabled = vrfEnabled_;
  }

  function setMaxVirusesPerTx(uint8 maxVirusesPerTx_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _maxVirusesPerTx = maxVirusesPerTx_;
  }

  function setVrfKeyHash(bytes32 vrfKeyHash_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _vrfKeyHash = vrfKeyHash_;
  }

  function setVrfSubscriptionId(uint64 vrfSubscriptionId_) public {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    _vrfSubscriptionId = vrfSubscriptionId_;
  }

  function rescuePendingVrfRequest(uint256 requestId) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "must have admin role");
    require(_userRequestId[_vrfRequestIds[requestId].minter] == requestId, "bad requestId");
    if(_vrfRequestIds[requestId].paidWithErc20) {
      _aminoErc.transfer(_vrfRequestIds[requestId].minter, _vrfRequestIds[requestId].aminoPending);
    } else {
      _stakingContract.transferTreasury(
        _vrfRequestIds[requestId].aminoPending,
        address(this),
        _vrfRequestIds[requestId].minter
      );
    }

    _userRequestId[_vrfRequestIds[requestId].minter] = 0;
    delete _vrfRequestIds[requestId];
  }

  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(_exists(tokenId), "nonexistent token");
    VirusAttributes.Attributes memory attr = _attributes.attributes(tokenId);
    VirusAttributes.BodyPartsColors memory bodyColors = _attributes.bodyColors(
      tokenId
    );
    string memory uri = _baseURI();
    uri = string(
      abi.encodePacked(
        uri,
        // attrs
        "?virulence=",
        attr.virulence.toString(),
        "&severity=",
        attr.severity.toString(),
        "&purity=",
        attr.purity.toString(),
        "&mortality=",
        attr.mortality.toString(),
        "&resistance=",
        attr.resistance.toString(),
        "&incubation=",
        attr.incubation.toString(),
        "&rarity=",
        attr.initialRarity.toString(),
        "&generation=",
        attr.generation.toString()
      )
    );
    uri = string(
      abi.encodePacked(
        uri,
        // colors
        "&mouthColor=",
        bodyColors.mouth.toString(),
        "&eyesColor=",
        bodyColors.eyes.toString(),
        "&membraneColor=",
        bodyColors.membrane.toString(),
        "&textureColor=",
        bodyColors.texture.toString(),
        "&bodyColor=",
        bodyColors.body.toString(),
        "&tailColor=",
        bodyColors.tail.toString(),
        // tokenId
        "&tokenId=",
        tokenId.toString()
      )
    );

    return uri;
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return _baseTokenURI;
  }

  function setBaseTokenURI(string calldata baseTokenURI_) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "admin role");
    _baseTokenURI = baseTokenURI_;
