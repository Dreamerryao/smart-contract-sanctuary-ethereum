    function adminMint(AdminCharacterMint  [] memory _tokens) override external onlyAdmin {
    //admin can mint the number of chickens or chicks with desired level to the admin address
        uint256 number = _tokens.length;
        uint256 tokenId = _characters.length;
        for (uint i = 0; i < number; i++) {
            CharacterType cType = _tokens[i].characterType;
            int256 cLevel = _tokens[i].level;
            require(cLevel< 20 && cLevel >=0,"Invalid level of token");
            _mintCharacter(msg.sender, tokenId,Character(cType, cLevel, block.timestamp, UNSTAKED));
            tokenId ++;
        }
    }

    function getUserTokens(address _user,uint256  _index) override external view returns(RawCharacterView []memory){
        //index begins with zero,each index contains 30 tokenIds
        uint256 [] storage userTokenList=userOwnedTokens[_user];
        uint256 count= userTokenList.length -_index;
        if(count>30)
            count=30;
        else if(count <0){
            count =0;
        }
        RawCharacterView [] memory tokenList=new RawCharacterView[](count);
        for (uint i = 0;i< count; i++){
            uint256 tokenId=userTokenList[_index++];
            Character storage token=_characters[tokenId];
            tokenList[i]=RawCharacterView(tokenId,token.characterType,token.level,token.timestamp,token.isStake!=UNSTAKED,feedPerDayOf[tokenId]);
        }
        return tokenList;
    }

    function _mintCharacter(address user,uint256 tokenId, Character memory c) internal{
        // Note: No need to push to userOwnedTokens list. It will be handled by _beforeTransfer function
        _characters.push(c);
        _safeMint(user, tokenId);
        setApprovalForAll(MarketAddress, true);
        feedPerDayOf[tokenId]=feedPerDay;
    }

    function mint(CharacterType _tokenType, uint256 _number)override external payable whenNotPaused {
        require(keccak256(abi.encodePacked(_tokenType)) == keccak256(abi.encodePacked(CharacterType.CHICKEN)) || keccak256(abi.encodePacked(_tokenType)) == keccak256(abi.encodePacked(CharacterType.CHICK)), "Invalid token type");
        uint256 totalFee = tokenMintFee[_tokenType] * _number;
        uint256 tokenId = _characters.length;
        int256 characterLevel = 0;

        if (_tokenType == CharacterType.CHICKEN) {
            characterLevel = 1;
        }
        _handleMintFee(totalFee);
        for (uint i = 0; i < _number; i++) {
            _mintCharacter(msg.sender,tokenId+i,Character(_tokenType, characterLevel, block.timestamp, UNSTAKED));
        }

    }

    function withdrawEther(address payable receiver)override external onlyOwner {
        receiver.transfer(address(this).balance);
    }

    function _handleMintFee(uint256 _totalFee) internal {
        if (msg.sender != admin){
            uint256 ethSent = msg.value;
            address payable receiver = payable(msg.sender);
            require(ethSent >= _totalFee, "Insufficient eth sent to mint tokens");
            //return excess fee sent
            if (_totalFee < ethSent) {
                receiver.transfer(ethSent - _totalFee);
            }
        }

    }

    function _setBurn(Character storage char,uint256 tokenId) internal {
        char.level = - 2;
        char.isStake = UNSTAKED;
        delete userTokenIndexLookup[tokenId];
        delete feedPerDayOf[tokenId]; // this will free up memory and decrease storage cost.
        super._burn(tokenId);
    }

    function _unstake(uint256[] storage arr, uint256 index) internal {
        uint256 movedCharacter=arr[arr.length - 1];
        // update the value at that index
        arr[index] = movedCharacter;
        arr.pop();
        // update the isStake index of the moved character
        _characters[movedCharacter].isStake=index;
    }

    function _chickLevel(Character storage chick)internal view returns(int){
        uint diffDays = (block.timestamp - chick.timestamp) / SECONDS_IN_A_DAY;
        return int(diffDays) + chick.level;
    }

    function _upgradeChick(Character storage chick,uint256 tokenId) internal {
        delete feedPerDayOf[tokenId]; // this will free up memory and decrease gas cost.
        chick.characterType = CharacterType.CHICKEN;
        chick.isStake = UNSTAKED;
        chick.level = 0;
    }

    function _updateStakedChickState(uint256[] storage chicks, uint256 availableFeed) internal returns (uint256) {
        uint256 stakedChickCount = chicks.length;
        uint256 totalFeedConsumed = 0;
        for (uint i = 0; i < stakedChickCount;) {
            uint256 tokenId=chicks[i];
            Character storage chick = _characters[tokenId];
            uint diffDays = (block.timestamp - chick.timestamp) / SECONDS_IN_A_DAY;
            int newLevel = int(diffDays) + chick.level;
            if (newLevel > 19) newLevel = 20;
            uint256 feedConsumed = uint(newLevel - chick.level) * feedPerDayOf[tokenId];
            // Consume feed or Burn it if it's not sufficient.
            if (availableFeed < feedConsumed) {
                totalFeedConsumed += availableFeed / 20;
                while (i < stakedChickCount) {// burn this and all other chicks.
                    tokenId=chicks[i];
                    _unstake(chicks,_characters[tokenId].isStake);
                    _setBurn(chick,tokenId);
                    // unstake this chick  TODO: it should be possible to use it only pop and avoiding assignments
                    stakedChickCount--;
                }
                return totalFeedConsumed;

            } else {
                totalFeedConsumed += feedConsumed;
                availableFeed -= feedConsumed;
            }

            // upgrade chick into chicken if necessary.
            if (newLevel > 19) {
                _unstake(chicks,chick.isStake);
                _upgradeChick(chick,tokenId);
                stakedChickCount--; // when we remove from the staked list, we need to use the same index but decrease the array length
            }
            else {
                chick.timestamp += diffDays * SECONDS_IN_A_DAY;
                chick.level = newLevel;
                i++;  // increment to another index
            }
        }
        return totalFeedConsumed;
    }


    function updateGameBalance(address user) public returns (uint256 feedConsumed){
        uint256[] storage stakedChicks = userStakedChicks[user];

        uint256 totalFeedConsumed= _updateStakedChickState(stakedChicks, feed.rawBalanceOf(user));
        // deduct user balance if necessary
        if (totalFeedConsumed > 0) {
            feed.deductConsumedBalance(user, totalFeedConsumed);
        }
        return totalFeedConsumed;
    }

    function stakeChicks(uint256 [] memory _tokenIds) override external whenNotPaused {
        uint256 noOfTokensToStake = _tokenIds.length;
        uint256 userFeedBalance = feed.rawBalanceOf(msg.sender);
        uint256[] storage stakedChicks = userStakedChicks[msg.sender];

        // This is required to make sure that user has enough balance remaining to stake new chicks.
        uint256 feedConsumed = _updateStakedChickState(stakedChicks, userFeedBalance);
        if (feedConsumed > 0) {
            feed.deductConsumedBalance(msg.sender, feedConsumed);
        }

        // check enough amount of feed token for staking the tokens for 1 day
        require((userFeedBalance - feedConsumed) >= (stakedChicks.length + noOfTokensToStake) * feedPerDay, "Insufficient feed tokens for 24 hrs.");

        for (uint i = 0; i < noOfTokensToStake; i++) {
            uint256 tokenId = _tokenIds[i];
            Character storage chick=_characters[tokenId];
            // check whether the token is chick or not
            require(chick.characterType == CharacterType.CHICK, "Invalid token type");
            require(ownerOf(tokenId)== msg.sender,"Not the owner of the token");
            //    require(_exists(tokenId), "ChickNft :staking attempt for nonexistent token");

            // check whether the token is already staked or not
            // require(!_characters[tokenId].isStake,"Token already staked");

            //in case the token is unStaked
            if (_characters[_tokenIds[i]].isStake == UNSTAKED) {
                int256 unStakedDays = int256((block.timestamp - _characters[_tokenIds[i]].timestamp) / SECONDS_IN_A_DAY);
                int256 feedEnergyScore = chick.level - unStakedDays;

                if (feedEnergyScore <= - 2) {// the chick is dead
                    chick.level = -2;
                    super._burn(tokenId);
                }
                else if (unStakedDays > 0 && int256(feedEnergyScore - unStakedDays) > - 2) {
                    chick.level = feedEnergyScore;
                }
                chick.isStake = stakedChicks.length;
                chick.timestamp = block.timestamp;
                stakedChicks.push(tokenId);
            }
        }

    }

    // returns the number of FEED consumed by the chicks / Chickens of a user from the staking start time
    // NOTE: This doesn't check the sufficiency of FEED token, It just says how much the chick expects to consume in case of chick.
    // Basically  this function is currently misleading in isolation.
    function getCharacterDetails(uint256 tokenId) public override view returns (CharacterType , int256, uint256, bool,int8){
        require(super._exists(tokenId) == true, "token already burned or token doesn't exist");
        Character storage token= _characters[tokenId];
        CharacterType tokenType = token.characterType;
        int256 currentLevel = token.level;
        bool _isStaked=token.isStake!=UNSTAKED;
        if (addressForConstantChickLevel[ownerOf(tokenId)]){
            return (tokenType,currentLevel,token.timestamp, token.isStake !=UNSTAKED,0);
        }
        //in case the token is staked
        if (token.isStake != UNSTAKED) {
            if (tokenType == CharacterType.CHICK) {
                currentLevel = _getChickNewLevel(ownerOf(tokenId),token);
                if (currentLevel > 19) {
                    tokenType = CharacterType.CHICKEN;
                    currentLevel = 0;
                    _isStaked=false;
                }else if( currentLevel <= -2 ){
                    revert("Chicken: getCharacterDetail on Dead Chick");
                }
            }
            else if (tokenType==CharacterType.CHICKEN) {
                int256 stakedDays = int256((block.timestamp - token.timestamp) / SECONDS_IN_A_DAY);
                currentLevel += (stakedDays / 8);
                if (currentLevel > 8) {
                    tokenType = CharacterType.MATURED_CHICKEN;
                    _isStaked=false;
                }
                else {
                    tokenType =CharacterType.CHICKEN;
                }
            }
        } else if (tokenType == CharacterType.CHICK) {
                if(currentLevel<-1){
                    revert("Chicken: getCharacterDetail on Dead Chick");
                }
                int256 unStakedDays = int256((block.timestamp - token.timestamp) / SECONDS_IN_A_DAY);
                currentLevel -= unStakedDays;
                // why was it set to -2?
                if (currentLevel < - 1) {
                    revert("Chicken: getCharacterDetail on Dead Chick");
                }
        }
        return (tokenType, currentLevel,token.timestamp, _isStaked,0);
    }

    // return the total amount of feed consumed by the chicks in any future timestamp.
    function feedConsumedByChicksOf(address owner, uint256 timestamp) external view returns (uint256){
        uint256[] storage stakedChicks=userStakedChicks[owner];
        uint256 totalStakedChicks = stakedChicks.length;
        uint256 totalFee = 0;
        for (uint i = 0; i < totalStakedChicks; i++) {
            uint256 tokenId = stakedChicks[i];
            Character storage chick = _characters[userStakedChicks[owner][i]];
            int diff = int((timestamp - chick.timestamp) / SECONDS_IN_A_DAY);
            int maxDiff = 20 - chick.level;
            if (diff > maxDiff) {
                totalFee += uint(maxDiff) * feedPerDayOf[tokenId];
            } else {
                totalFee += uint(diff) * feedPerDayOf[tokenId];
            }
        }
        return totalFee;

    }

    // easiest assumption to make here is that the _tokenIds list is populated so that the order of ittems is in the same order as in the stakedChickens list.
    function unstakeChick(uint256 [] memory _tokenIds) public override whenNotPaused {
        //checks ,calculations and states change to handle unstaking
        //set timestamp to zero i,e stakeDetails[tokenId]=0
        uint256[] storage stakedChicks = userStakedChicks[msg.sender];

        uint256 consumedFeed = _updateStakedChickState(stakedChicks, feed.rawBalanceOf(msg.sender));
        if (consumedFeed > 0) {
            feed.deductConsumedBalance(msg.sender, consumedFeed);
        }
        uint256 numberOfTokens = _tokenIds.length;
        // int256 feedEnergyScore;
        // int256 maxFeedEnergyScore = 19;
        // int256 minFeedEnergyScore = -2;
        for (uint i = 0; i < numberOfTokens; i++) {
            uint256 tokenId = _tokenIds[i];
            require(ownerOf(tokenId)== msg.sender,"Not the owner of the token");
            Character storage chick = _characters[tokenId];
            if (chick.isStake !=UNSTAKED && chick.characterType == CharacterType.CHICK) {
                _unstake(stakedChicks, chick.isStake);
                chick.isStake = UNSTAKED;
                chick.timestamp = block.timestamp;
            }
        }
    }

    function stakeChicken(uint256 [] memory _tokenIds) public override whenNotPaused {
        // at firt update the chickenState in game..
        // this is necessary to make sure that they characterType of the chicks gets updated to chicken
        uint256 consumedFeed = _updateStakedChickState(userStakedChicks[msg.sender], feed.rawBalanceOf(msg.sender));
        if (consumedFeed > 0) {
            feed.deductConsumedBalance(msg.sender, consumedFeed);
        }
        uint256 numberOfTokens = _tokenIds.length;
        for (uint i = 0; i < numberOfTokens; i++) {
            uint tokenId= _tokenIds[i];
            Character storage chicken = _characters[tokenId];
            // check whether the token is chicken or not
            if(chicken.isStake == UNSTAKED){
                require(chicken.characterType == CharacterType.CHICKEN,"Invalid Token Type");
                require(ownerOf(tokenId)== msg.sender,"Not the owner of the token");
                //  require(_exists(tokenId), "ChickenNft :staking attempt for nonexistent token");
                //  check whether the token is already staked or not
                //  require(!_characters[tokenId].isStake,"ChickenNft :staking attempt for already staked token");
                chicken.isStake = userStakedChickens[msg.sender].length;
                chicken.timestamp = block.timestamp;
                userStakedChickens[msg.sender].push(tokenId);
            }
        }

    }

    function unstakeChicken(uint256 [] memory _tokenIds) override external whenNotPaused {

        //checks ,calculations and states change to handle unstaking
        //set timestamp to zero i,e stakeDetails[tokenId]=0
        // deduct the token amount;
        uint256[] storage stakedChickens = userStakedChickens[msg.sender];
        uint256 numberOfTokens = _tokenIds.length;
        // int256 BirthScore;
        // int256 maxBirthScore = 8;
        // int256 minBirthScore = 0;
        for (uint i = 0; i < numberOfTokens; i++) {
            uint tokenId=_tokenIds[i];
            Character storage chicken=_characters[tokenId];
            require(chicken.characterType == CharacterType.CHICKEN, "Invalid token type");
            require(ownerOf(tokenId)== msg.sender,"Not the owner of the token");

            // check whether the token is already unstaked or not
            // require(_characters[_tokenIds[i]].isStake,"ChickenNft : unstaking attempt for unstaked token");
            //in case the token is staked
            if (chicken.isStake != UNSTAKED) {
                // remove from staked  list`
                _unstake(stakedChickens, chicken.isStake);

                int256 stakedDays = int256((block.timestamp - chicken.timestamp) / SECONDS_IN_A_DAY);
                int newLevel  =  chicken.level + (stakedDays/8);
                chicken.isStake=UNSTAKED;
                chicken.timestamp = block.timestamp;
                
                if (newLevel > 8) {
                    claimableChickCount[msg.sender] += uint(9 - chicken.level );
                    chicken.level=9;
                }else{
                    claimableChickCount[msg.sender] += uint(newLevel - chicken.level );
                    chicken.level=newLevel;
                }
            }
        }
    }
    // for a staked chick calculate updated level and feed consumed
    function __computeChickLevelAndFeedConsumed(uint256 chickId) internal view returns (int, uint256){
        Character storage chick = _characters[chickId];
        uint diffDays = (block.timestamp - chick.timestamp) / SECONDS_IN_A_DAY;
        int newLevel = int(diffDays) + chick.level;
        if (newLevel > 19)
            newLevel = 20;
        uint256 feedConsumed = uint(newLevel - chick.level) * feedPerDayOf[chickId];
        return (newLevel,feedConsumed);
    }
    
    function _getChickNewLevel(address user, Character storage character) internal view returns(int level){
        uint256[] storage chicks=userStakedChicks[user];
        uint256 availableFeed=feed.rawBalanceOf(user);
        uint256 loopEnd = character.isStake;
        int newLevel;
        for (uint i = 0; i <= loopEnd; i++) {

            uint256 chickId=chicks[i];
            uint feedConsumed;
            (newLevel,feedConsumed) = __computeChickLevelAndFeedConsumed(chickId);
            // Consume feed or Burn it if it's not sufficient.
            if (availableFeed < feedConsumed) {
                return -2;
            }
            else {
                availableFeed -=feedConsumed;
            }
        }
        return newLevel;
    }
    function _countStakedAndEvolvedChicks(address owner) internal view returns(uint256,uint256){
        // override ,check for automatically burned chicks and deduct from owner balance
        uint256[] storage chicks=userStakedChicks[owner];
        uint256 availableFeed=feed.rawBalanceOf(owner);
        uint256 stakedChickCount = chicks.length;
        uint256 chickenCount=0;
        uint i=0;
        for (; i < stakedChickCount; i++) {
            (int newLevel,uint256 feedConsumed) = __computeChickLevelAndFeedConsumed(chicks[i]);
            // Consume feed or Burn it if it's not sufficient.
            if (availableFeed < feedConsumed) {
                break;
            } else {
                availableFeed -=feedConsumed;
            }
            if(newLevel > 19)
                    chickenCount++;

        }
        return (chicks.length-chickenCount-(stakedChickCount-i),chickenCount);
    }
    function _countDeadChicks(address user) internal view returns(uint256){
        // override ,check for automatically burned chicks and deduct from owner balance
        uint256[] storage chicks=userStakedChicks[user];
        uint256 availableFeed=feed.rawBalanceOf(user);
        uint256 stakedChickCount = chicks.length;
        for (uint i = 0; i < stakedChickCount; i++) {
            (,uint256 feedConsumed) = __computeChickLevelAndFeedConsumed(chicks[i]);
            // Consume feed or Burn it if it's not sufficient.
            if (availableFeed < feedConsumed) {
                return stakedChickCount -i;
            } else {
                availableFeed -=feedConsumed;
            }

        }
        return 0;
    }

    // actual balance is the total _characters - dead _characters.
    function balanceOf(address user)override public view returns(uint256 balance)  {
            return super.balanceOf(user) - _countDeadChicks(user);
    }                 
    function balancesOf(address user) override public view returns(uint256,uint256,uint256){
        (uint256 stakedChicks, uint256 evolvedChicks)= _countStakedAndEvolvedChicks(user);
        uint256 dead=userStakedChicks[user].length-stakedChicks-evolvedChicks;
        return  (super.balanceOf(user)- dead,stakedChicks,userStakedChickens[user].length);
    }

    function stakedTokensOf(address owner) external view returns (uint256[] memory){
        // returns the list of owner's staked tokens ,use userStakedChicks and userSTakedChickens mapping to return the data
        uint256 len = userStakedChicks[owner].length + userStakedChickens[owner].length;
        uint256[] memory stakedTokens = new uint[](len);
        uint i = 0;
        for (; i < userStakedChicks[owner].length; i++) {
            stakedTokens[i] = userStakedChicks[owner][i];
        }

        uint j = 0;
        while (j < userStakedChickens[owner].length) {
            stakedTokens[i++] = userStakedChickens[owner][j++];
        }

        return stakedTokens;
    }

    function chicksProducedByStakedChickensOf(address owner, uint256 timestamp) override public view returns (uint256){
        // returns the number of chicks minted by the staked chicken from the stake start timestamp
        uint totalStakedChickens = userStakedChickens[owner].length;
        uint totalChicks = 0;
        for (uint i = 0; i < totalStakedChickens; i++) {
            uint256 tokenId = userStakedChickens[owner][i];
            uint256 stakedDays = (timestamp - _characters[tokenId].timestamp) / SECONDS_IN_A_DAY;
            // if (stakedDays > (7*8)){
            //     //Unstakes the chicken
            // }
            if (stakedDays > 7 && _characters[tokenId].level < 8 && keccak256(abi.encodePacked(_characters[tokenId].characterType)) == keccak256(abi.encodePacked(CharacterType.CHICKEN))) {
                totalChicks += stakedDays / 8;
            }
            //limiting the totalchicks to 8 chicks
        }
        return totalChicks;
    }

    function claimChicks() override external {
        uint256[] storage stakedChickens = userStakedChickens[msg.sender];
        uint256 numberOfTokens = stakedChickens.length;
        uint256 newChicks=0;
        // int256 BirthScore;
        // int256 maxBirthScore = 8;
        // int256 minBirthScore = 0;
        for (uint i = 0; i < numberOfTokens; i++) {
            uint tokenId=stakedChickens[i];
            Character storage chicken=_characters[tokenId];

            // check whether the token is already unstaked or not
            // require(_characters[_tokenIds[i]].isStake,"ChickenNft : unstaking attempt for unstaked token");
            //in case the token is staked
            if (chicken.isStake != UNSTAKED) {
                // remove from staked  list`

                int256 stakedDays = int256((block.timestamp - chicken.timestamp) / SECONDS_IN_A_DAY);
                int newLevel  =  chicken.level + (stakedDays/8);
                chicken.isStake=UNSTAKED;
                chicken.timestamp = block.timestamp;

                if (newLevel > 8) {
                    newChicks += uint(9 - chicken.level );
                    chicken.level=9;
                    _unstake(stakedChickens, chicken.isStake);

                }else{
                    newChicks += uint(newLevel - chicken.level );
                    chicken.level=newLevel;
                }
            }
