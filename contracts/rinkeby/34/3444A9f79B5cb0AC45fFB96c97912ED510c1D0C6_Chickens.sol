pragma solidity ^0.8.0;

enum CharacterType{CHICK, CHICKEN, MATURED_CHICKEN, DEAD_CHICK}

struct RawCharacterView { // basically the details are raw, the calculations has not been made based on last timestamp
    uint256 tokenId;
    CharacterType characterType;
    // for chicks Feed energy score,for chicken birth score  level
    int256 level;
    uint256 timestamp;
    bool isStaked;
    uint256 feedConsumedPerDay;
}

struct AdminCharacterMint{
    CharacterType characterType;
    int256 level;
}

interface IChickens {
        
    // get current platform configuration
    function  configuration() external view  returns(address ownerAddress,address adminAddress,uint256 chickMintfee,uint256 chickenMintFee,uint256 feedDeductionPerDay,uint256 userBalance);


    /**
        Admin dashboard functions to set platform parameters.
     */
    function setChickMintFee(uint256 fee) external;
    function setChickenMintFee(uint256 fee) external;
    function setFeedPerDay(uint256 _feedPerDay) external;
    function setAdmin(address _admin)external ;
    function adminMint(AdminCharacterMint  [] memory _tokens) external ;
    //  owher should be able to withdraw ethers.
    function withdrawEther(address payable receiver) external ;


    // Get list of tokens of a user. This function returns paginated response, each paga containing
    // 30 characters in the page.
    // the returned character's data is existing data, and doesn't include game calculation. 
    // game calculation must be done in the frontend
    function getUserTokens(address _user,uint256  _index) external view returns(RawCharacterView []memory);
    // return characeter detail
    function getCharacterDetails(uint256 tokenId) external view returns (CharacterType characterType, int256 level, uint256 lastActivityTime,bool isStaked, int8 yield);


    // Mint n chicken or chicks
    function mint(CharacterType _tokenType, uint256 _count) external  payable;

    // stake and unstake chick
    function stakeChicks(uint256 [] memory _tokenIds) external;
    function unstakeChick(uint256 [] memory _tokenIds) external;

    // stake and unstake chicken
    function stakeChicken(uint256 [] memory _tokenIds) external;
    function unstakeChicken(uint256 [] memory _tokenIds) external;
    
    // returns balances totalBalanced is balance of all staked and unstaked chicks+chickens
    function balancesOf(address user) external view returns(uint256 totalBalance,uint256 stakedChickCount,uint256 stakedChickenCount);

    // returns total chicks produced by the chickens of a user at given timestamp
    function chicksProducedByStakedChickensOf(address owner, uint256 timestamp) external view returns (uint256);

    // claim chicks produced by staked chickens.
    // call this when user wants to claim the chicks.
    function claimChicks() external;

    // mint cock
    function mintCock(uint256[]memory _tokenIds) external;
}

