// SPDX-License-Identifier: MIT
// Creator: R1

pragma solidity ^0.8.7;


import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./deps/ERC721A.sol";
import "./RandomValue.sol";
import "./Splitter.sol";

//// add payment splitter
contract Boku is  
  ERC721A, 
  Ownable, 
  Pausable, 
  ReentrancyGuard,
  PaymentSplitter
  {

  using Strings for uint256;  

  uint256 public MINTGEN0_PRICE = 0.005 ether;
  uint256 public WLGEN0_PRICE = 0.002 ether;
  uint256 public MINTGEN1_PRICE = 0.01 ether;
  uint256 public WLGEN1_PRICE = 0.004 ether;

  uint16 public MAXGEN0_MINT = 1000;
  uint16 public MAXGEN1_MINT = 2000;

  uint16 public MINTED;

  uint public whitelistStartTime;
  uint public whitelistEndTime;

  bytes32 private merkleRoot;

  string public baseURI;
  string public notRevealedURI;
  string public baseExtension = ".json";

  bool public revealed = false;

  mapping(address => bool) randomController;


  /// Differents steps of selling
  enum Steps {
    Close,
    Whitelist,
    Gen0,
    Between,
    Gen1,
    Soldout
  }
  

  Steps public mintStep;



  /// mapp token w/ score 
  mapping(uint16 => uint8) private tokenScore;
  /// how many token have been minted by address (MAX : 2)
  mapping(address => uint8) public whiteListMintCounts;

  address[] private _funds = [
    0x505B2661a3BC739f6651F4743b2793018d32E357,   //// VAULT
    0x7F4b48B16F4fF322930FeBBCAEC38CC8411408f2,   //// BUILD
    0xcC58400ea44239dD947d8BF4563d5496Bc0a434C,   //// MARKETING
    0x29beed3FaE6033e4F06177A48c7FEA596827316d    //// TEAM
  ];

    uint[] private _fundsShares = [
        50,  //// VAULT
        20,  //// BUILD
        10,  //// MARKETING
        20   //// TEAM
    ];
   

  /// Reference to Splitter contract
  Splitter public splitter;
  /// Reference to RandomValue contract
  RandomValue public randomValue;

  constructor(
   uint _startWhitelist, 
   uint _endWhitelist,
   string memory _baseURI,
   string memory _notRevealedURI
   
   ) 
   ERC721A("Boku no Babes", "BABES") 
   PaymentSplitter(_funds, _fundsShares)
   {
      whitelistStartTime = _startWhitelist;
      whitelistEndTime = _endWhitelist;
      mintStep = Steps.Close;
      baseURI = _baseURI;
      notRevealedURI = _notRevealedURI;
      

  }


  event Minted(address from, uint8 amount);
    

  function mint(uint8 _amount) public payable whenNotPaused nonReentrant {
    require(tx.origin == _msgSender(), "Only EOA");
    require(_amount > 0 && _amount <= 30, "Invalid amount");
    require(mintStep == Steps.Gen0 || mintStep == Steps.Gen1, "Mint close");
    if (MINTED < MAXGEN0_MINT) {
      require(msg.value == MINTGEN0_PRICE * _amount, "Invalid price GEN0");
      require(MINTED + _amount <= MAXGEN0_MINT, "GEN0 SOLD OUT");
    }
    else {
      require(msg.value == MINTGEN1_PRICE * _amount, "Invalid price GEN1");
      require(MINTED + _amount <= MAXGEN1_MINT, "SOLD OUT");
    }
    for (uint i=0;i<_amount;i++){
      MINTED++;  
      randomValue.requestRandomScore();
    }
    checkMinted(MINTED);
    _safeMint(_msgSender(), _amount);   
    emit Minted(_msgSender(), _amount);
  }


  function whitelistMint(address _from, bytes32[] calldata _proof, uint8 _amount) public payable whenNotPaused nonReentrant {
    require(tx.origin == _msgSender(), "Only EOA");
    require(isWhiteListed(_from, _proof), "You'r not whitelisted");
    require(mintStep == Steps.Whitelist, "Whitelist mint close");
    require(_amount > 0 && whiteListMintCounts[_msgSender()] + _amount <= 2, "Whitelist can only mint 2");
    
    if (MINTED < MAXGEN0_MINT){
      require(msg.value == WLGEN0_PRICE * _amount, "Invalid whitelist price GEN0");
      require(MINTED + _amount <= MAXGEN0_MINT, "GEN0 SOLD OUT");
    }
    else {
      require(msg.value == WLGEN1_PRICE * _amount, "Invalid whitelist price GEN1");
      require(MINTED + _amount <= MAXGEN1_MINT, "SOLD OUT");
    }

    for (uint i=0;i<_amount;i++){
      MINTED++;  
      randomValue.requestRandomScore();
      whiteListMintCounts[_msgSender()]++;
    }
    checkMinted(MINTED);
    _safeMint(_msgSender(), _amount);      
    emit Minted(_msgSender(), _amount);
  }

    /*
    * UTILS 
    */

   function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public override nonReentrant {
    if (_msgSender() != address(splitter)) {
      require(
        _isApprovedOrOwner(_msgSender(), tokenId),
        "ERC721A: transfer caller is not owner nor approved"
      );
    }
    _transfer(from, to, tokenId);
  }


  function setScoreOf(uint8 _score, uint16 _tokenId) external onlyRandomValue{
    tokenScore[_tokenId] = _score;
  }

  modifier onlyRandomValue(){
    require(randomController[msg.sender], "Not controller");
    _;
  }

  function _leaf(address account) internal pure returns(bytes32) {
    return keccak256(abi.encodePacked(account));
  }
  
  function _verify(bytes32 leaf, bytes32[] memory proof) internal view returns(bool) {
    return MerkleProof.verify(proof, merkleRoot, leaf);
  }

  function checkMinted(uint16 _amountMint) internal {
    if (_amountMint == MAXGEN0_MINT) {
      mintStep = Steps.Between;
    }
    if (_amountMint == MAXGEN1_MINT) {
      mintStep = Steps.Soldout;
    }
  }


    /*
    * VIEWS 
    */

  function getScoreOf(uint16 _tokenId) external view returns (uint8) {
    return revealed ? tokenScore[_tokenId] : 0;
  }

  function isWhiteListed(address account, bytes32[] calldata proof) public view returns(bool) {
    return _verify(_leaf(account), proof);
  }

  function tokenURI(uint256 _nftId) public view override(ERC721A) returns (string memory) {
    require(_exists(_nftId), "This NFT doesn't exist.");
    if(revealed == false) {
      return notRevealedURI;
    }
        
    string memory currentBaseURI = _baseURI();
    return 
      bytes(currentBaseURI).length > 0 
      ? string(abi.encodePacked(currentBaseURI, _nftId.toString(), baseExtension))
      : "";
    }

    function getWhitelistMintCounts(address _from) public view returns(uint8) {
      return whiteListMintCounts[_from];
    }
    

    /*
    * ONLY OWNER
    */


  function launchGEN0Mint() public onlyOwner {
    mintStep = Steps.Gen0;
  }

  function launchGEN1Mint() public onlyOwner {
    mintStep = Steps.Gen1;
  }

  function launchWhitelistMint() public onlyOwner {
    mintStep = Steps.Whitelist;
  }

  function launchReveal() public onlyOwner {
    revealed = true;
  }

  function closeMint() public onlyOwner {
    mintStep = Steps.Close;
  }


  function setMintGEN0Price(uint256 _newPrice) external onlyOwner{
    MINTGEN0_PRICE = _newPrice;
  }

  function setWLGEN0Price(uint256 _newPrice) external onlyOwner {
    WLGEN0_PRICE = _newPrice;
  }

  function setWLStartTime(uint _timestamp) public onlyOwner {
    whitelistStartTime = _timestamp;
  }

  function setWLEndTime(uint _timestamp) public onlyOwner {
    whitelistEndTime = _timestamp;
  }

  function setMaxGEN0Mint(uint16 _newAmount) external onlyOwner {
    MAXGEN0_MINT = _newAmount;
  }
  function setMaxGEN1Mint(uint16 _newAmount) external onlyOwner {
    MAXGEN1_MINT = _newAmount;
  }

  function setSplitter(address _NewSplitter) public onlyOwner {
    splitter = Splitter(_NewSplitter);
  }

  function setRandomValue(address _NewRandomizer) public onlyOwner {
    randomValue = RandomValue(_NewRandomizer);
    randomController[_NewRandomizer] = true;
  }

  function setMerkleRoot(bytes32 _newMerkleRoot) external onlyOwner {
    merkleRoot = _newMerkleRoot;
  }

  function setBaseUri(string memory _newBaseURI) external onlyOwner {
    baseURI = _newBaseURI;
  }

  function setNotRevealURI(string memory _notRevealedURI) external onlyOwner {
     notRevealedURI = _notRevealedURI;
  }

}

