pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./Boku.sol";
import "./Token.sol";
import "./Club.sol";
import "./RandomValue.sol";


contract Splitter is Ownable, Pausable, ReentrancyGuard {

  using SafeMath for uint256;

  uint256 public tokenToPrint = 1000 ether;
  uint16 public totalStaked;
  bool public stakingOpen = false;

    
  struct TokenInfo {
      address owner;
      uint8 score;
      uint stakeTimestamp; 
  }
  
  mapping(address => uint16[]) private tokensOf; 
  mapping(address => uint256) private amountByAddress;
  mapping(uint16 => TokenInfo) private tokenInfo;


  Boku public boku;
  Token public token;
  Club public club;
  RandomValue public randomValue;


  event TokenDeposited(address _from, uint256 _amount);
  event TokenWithdraw(address _from, uint16[] _tokenIds);
  event tokenClaimed(address _from);


  constructor(
      Boku _boku,
      Token _token
  ) {
       boku = _boku;
       token = _token;
    }


    function lockManyAndOrderToken(address _account, uint16[] calldata _tokenIds) external whenNotPaused nonReentrant whenIsOpen {
        require((_account == _msgSender() && _account == tx.origin), "BRUH");
        require(_tokenIds.length > 0, "OOOPS");
        /// initialization of the order 
        uint256 amountToMint = 0;
        for(uint i=0; i<_tokenIds.length; i++){
            require(boku.ownerOf(_tokenIds[i]) == _msgSender(), "NOT YOUR TOKENS");
            require(boku.getScoreOf(_tokenIds[i]) > 0, "THE SCORE HAS NOT YET BEEN INITIALIZED");
            /// lock the NFT into the contract
            boku.transferFrom(_msgSender(), address(this), _tokenIds[i]);
            /// add owner of the token for give it back when unstake
            addTokensOf(_msgSender(), _tokenIds[i]);
            /// add information about token, timestamp will be used when unstake
            addTokenInfo(_tokenIds[i], _msgSender(), block.timestamp);
            amountToMint+=tokenToPrint * getScoreOfToken(_tokenIds[i]);
            totalStaked++;
        }
        token.mint(address(this), amountToMint);
        /// security mapping used when unstake
        addAmountByAddress(_msgSender(), amountToMint);
        if (amountToMint > 0){
            addTokenToClub(_msgSender(), amountToMint);
        }
    }


    function unlockManyAndDropToken(address _account, uint16[] calldata _tokenIds) external whenNotPaused nonReentrant{
        require((_account == _msgSender() && _account == tx.origin), "BRUH");
        require(_tokenIds.length > 0, "OOOPS");
        uint256 amountToDrop = 0;
        for(uint i=0; i<_tokenIds.length; i++){
            require(getOwnerOfToken(_tokenIds[i]) == _msgSender(), "NOT YOUR TOKENS");
            amountToDrop += tokenToPrint * getScoreOfToken(_tokenIds[i]);
            removeTokenInfo(_tokenIds[i]);
            totalStaked--;
        }
        if (amountToDrop <= getAmountByAddress(_account)){
            club.withdraw(_account, amountToDrop);
            token.burn(address(this), amountToDrop);
            reduceAmountByAddress(_account, amountToDrop);
            for (uint i=0;i<_tokenIds.length;i++){
                boku.transferFrom(address(this), _account, _tokenIds[i]);
                removeTokensOf(_account, _tokenIds[i]);
            }
            emit TokenWithdraw(_account, _tokenIds);
        }
    }

    function claimAllRewards(address _account) external whenNotPaused nonReentrant{
        require((_account == _msgSender() && _account == tx.origin), "BRUH");
        club.withdraw(_account, 0);
        emit tokenClaimed(_account);
    }

    /* EMERGENCY ONLY. unlock all NFTs without caring about rewards. */
    function emergencyUnlockAndDropToken(address _account) external whenNotPaused nonReentrant {
        require((_account == _msgSender() && _account == tx.origin), "BRUH");
        uint16[] memory walletOf = getTokensOf(_account);
        uint256 amountToDrop = 0;
        if (walletOf.length > 0){
            for (uint i=0;i<walletOf.length;i++){
                require(getOwnerOfToken(walletOf[i]) == _msgSender(), "NOT YOUR TOKENS");
                amountToDrop += tokenToPrint * boku.getScoreOf(walletOf[i]);
                removeTokenInfo(walletOf[i]);
                totalStaked--;
            }
            club.emergencyWithdraw(_account);
            reduceAmountByAddress(_account, amountToDrop);
            token.burn(address(this), amountToDrop);

             for (uint i=0;i<walletOf.length;i++){
                boku.transferFrom(address(this), _account, walletOf[i]);
                removeTokensOf(_account, walletOf[i]);
            }
            emit TokenWithdraw(_account, walletOf);
        }
    }


  /*
  * UTILS
  */

    function addTokenToClub(address _account, uint256 _amount) internal {
        require((_account == _msgSender() && _account == tx.origin), "BRUH");
        token.approve(address(club), _amount);
        club.deposit(_account, _amount);
        emit TokenDeposited(_account, _amount);
    }

    // add id of token staked in mapping
    function addTokensOf(address _account, uint16 _tokenId) internal {
        uint16[] storage tokens = tokensOf[_account];
        tokens.push(_tokenId);
    }

    // remove id of token staked in mapping
    function removeTokensOf(address _account, uint _tokenId) internal {
        uint16[] storage tokens = tokensOf[_account];
        for (uint16 i = 0; i < tokens.length; i++) {
            if(tokens[i] == _tokenId) {
                tokens[i] = tokens[tokens.length - 1];
                tokens.pop();
                break;
            }
        }
    }

    function addAmountByAddress(address _from, uint256 _amount) internal {
        uint256 amountBeforeAdd = amountByAddress[_from];
        amountByAddress[_from] = amountBeforeAdd.add(_amount);
    }

    // reduce when unstack
    function reduceAmountByAddress(address _from, uint256 _amount) internal {
        uint256 amountBeforeReduce = amountByAddress[_from];
        amountByAddress[_from] = amountBeforeReduce.sub(_amount);
    }


    function addTokenInfo(uint16 _tokenId, address _owner, uint _timestamp) internal {
        TokenInfo storage tokenData = tokenInfo[_tokenId];
        tokenData.owner = _owner;
        tokenData.stakeTimestamp = _timestamp;
        tokenData.score = boku.getScoreOf(_tokenId);
    }

    function removeTokenInfo(uint16 _tokenId) internal {
        delete tokenInfo[_tokenId];
    }

    modifier whenIsOpen() {
        require(stakingOpen, "Staking is closed");
        _;
    }

    /*
    *   VIEWS
    */

    function getTokensOf(address _account)  public view returns(uint16[] memory) {
        return tokensOf[_account];
    }

    function getAmountByAddress(address _owner) public view returns(uint256){
        return amountByAddress[_owner];
    }

    function getOwnerOfToken(uint16 _tokenId) public view returns(address){
        return tokenInfo[_tokenId].owner;
    }

    function getScoreOfToken(uint16 _tokenId)public view returns(uint8){
        return tokenInfo[_tokenId].score;
    }

    function getTimestampOfToken(uint16 _tokenId)public view returns(uint){
        return tokenInfo[_tokenId].stakeTimestamp;
    }

    /*
    *   ONLY OWNER
    */

    function setClub(address _newClub) external onlyOwner {
        club = Club(_newClub);
    }

    function setRandomValue(address _NewRandomizer) public onlyOwner {
        randomValue = RandomValue(_NewRandomizer);
    }

    function openStaking() external onlyOwner {
        stakingOpen = true;
    }
  

}

