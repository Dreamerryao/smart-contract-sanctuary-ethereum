pragma solidity >= 0.8.0;

import {IJsonGenerator} from "../interface/IJsonGenerator.sol";

library Json {    
    function openJsonObject(IJsonGenerator generator) internal pure returns (string memory) {        
        return string(abi.encodePacked("{"));
    }

    function closeJsonObject(IJsonGenerator generator) internal pure returns (string memory) {
        return string(abi.encodePacked("}"));
    }

    function openJsonArray(IJsonGenerator generator) internal pure returns (string memory) {        
        return string(abi.encodePacked("["));
    }

    function closeJsonArray(IJsonGenerator generator) internal pure returns (string memory) {        
        return string(abi.encodePacked("]"));
    }

    function pushJsonPrimitiveStringAttribute(IJsonGenerator, string memory name, string memory value, bool insertComma) internal pure returns (string memory) {
        return string(abi.encodePacked('"', name, '": "', value, '"', insertComma ? ',' : ''));
    }

    function pushJsonPrimitiveNonStringAttribute(IJsonGenerator, string memory name, string memory value, bool insertComma) internal pure returns (string memory) {
        return string(abi.encodePacked('"', name, '": ', value, insertComma ? ',' : ''));
    }

    function pushJsonComplexAttribute(IJsonGenerator, string memory name, string memory value, bool insertComma) internal pure returns (string memory) {
        return string(abi.encodePacked('"', name, '": ', value, insertComma ? ',' : ''));
    }

    function pushJsonArrayElement(IJsonGenerator, string memory value, bool insertComma) internal pure returns (string memory) {
        return string(abi.encodePacked(value, insertComma ? ',' : ''));
    }
}

