interface IERC721Receiver {
    function onERC721Received(address operator, address from, uint256 tokenId, bytes calldata data) external returns (bytes4);
}

interface IERC165 {
    function supportsInterface(bytes4 interfaceId) external view returns (bool);
}

interface ERC721 is IERC165 {
    event Transfer(address indexed from, address indexed to, uint256 indexed tokenId);
    event ConsecutiveTransfer(uint256 indexed fromTokenId, uint256 toTokenId, address indexed fromAddress, address indexed toAddress);
    event Approval(address indexed owner, address indexed approved, uint256 indexed tokenId);
    event ApprovalForAll(address indexed owner, address indexed operator, bool approved);

    function balanceOf(address owner) external view returns (uint256 balance);
    function ownerOf(uint256 tokenId) external view returns (address owner);
    function safeTransferFrom(address from, address to, uint256 tokenId) external;
    function transferFrom(address from, address to, uint256 tokenId) external;

    function approve(address to, uint256 tokenId) external;
    function getApproved(uint256 tokenId) external view returns (address operator);
    function setApprovalForAll(address operator, bool _approved) external;
    function isApprovedForAll(address owner, address operator) external view returns (bool);
    function safeTransferFrom(address from, address to, uint256 tokenId, bytes calldata data) external;
}

interface ERC721Metadata {
    function name() external view returns (string memory _name);
    function symbol() external view returns (string memory _symbol);
    function tokenURI(uint256 _tokenId) external view returns (string memory);
}

contract PakBaselDev is ERC721, ERC721Metadata, NiftyEntity, IJsonGenerator {

    // Libraries used for SVG/Metadata generation.
    using Json for IJsonGenerator;
    using Base64 for string;
    using Strings for uint256;
    using ABDKMath64x64 for int128;
    using SafeMath for uint256;
    
    // Pointer to an SVG generator contract
    ISvgGenerator public generator;

    // Mapping from token ID to owner address.
    mapping (uint256 => address) private _owners;

    // Mapping from owner address to token ID.
    mapping (address => uint256) private _tokens;

    // Mapping token ID to masse value.
    mapping (uint256 => uint256) private _values;

    // Mapping owner address to token count.
    mapping (address => uint256) private _balances;

    // Mapping from token ID to approved address.
    mapping (uint256 => address) private _tokenApprovals;

    // Mapping from owner to operator approvals.
    mapping (address => mapping (address => bool)) private _operatorApprovals;

    // Intial receiver (NG Omnibus) of each newly minted NFT.
    address immutable public _defaultOwner;

    // Track the number of NFTs minted so far. 
    uint256 public _mintCount;

    // Track running mass total.
    uint256 public _totalMass;

    // Token artifact location.
    string private _baseURI;

    string private _name;

    string private _symbol;

    /**
     * @dev Set the values carefully!
     *
     * Requirements:
     *
     * - `niftyRegistryContract_` use this for access control
     * - `defaultOwner_` the NG omnibus wallet
     *
     */
    constructor(address niftyRegistryContract_, address defaultOwner_) NiftyEntity(niftyRegistryContract_) {
       _defaultOwner = defaultOwner_;
       _name = "Pak Basel Dev";
       _symbol = "PBD";
    }

    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    /**
     * @dev Generate the NFTs of this collection. 
     *
     * Requirements:
     *
     * - `values_` provided as a list of addresses, each of
     *             which implicitly corresponds to a tokenId, 
     *             derrived by the index of the value in the 
     *             input array. The values map to a color
     *             attribute.
     *
     * Emits a {ConsecutiveTransfer} event.
     */
    function mint(uint256[] memory values_) external onlyValidSender {

        uint256 index = _mintCount;
        uint256 massAdded = 0;

        for (uint256 i = 1; i <= values_.length; i++) {

            index = _mintCount + i;

            _values[index] = values_[i - 1]; 

            // This section validates the token id, enforcing that token ids embed their RGB color and specific nifty number
            (uint r, uint g, uint b, uint mass) = decodeMassAndColor(values_[i - 1]);
            require(r <= 255 && g <= 255 && b <= 255, "Values of R,G,B must be 0 to 255");
            require(mass > 0 && mass <= 99999999, "Mass must be 1 to 99999");

            massAdded += values_[i - 1]; // Nathan was here...
        }

        emit ConsecutiveTransfer(_mintCount, index, address(0), _defaultOwner);

        _mintCount += values_.length;

        _balances[_defaultOwner] = _mintCount;

        _totalMass += massAdded;
    }

    /**
     * @dev Returns the deleted token, the point of the function is to
     *      eliminate the token with the smaller value.  
     * @param tokenId00 uint256 ID of the first token to be compared.
     * @param tokenId01 uint256 ID of the other token to be compared.
     * @return The deleted token, the one with a lower value. 
     */
    function merge(uint256 tokenId00, uint256 tokenId01) public onlyValidSender returns (uint256) {

        uint256 mass00 = decodeMass(_values[tokenId00]);
        uint256 mass01 = decodeMass(_values[tokenId01]);

        address owner00 = ownerOf(tokenId00);
        address owner01 = ownerOf(tokenId01);

        if (mass00 >= mass01) {
            _values[tokenId00] += mass01;
            delete _values[tokenId01];

            _balances[owner00] += 1;
            _balances[owner01] -= 1;

            return tokenId01; 
        }
        _values[tokenId01] += mass00;
        delete _values[tokenId00];

        _balances[owner01] += 1;
        _balances[owner00] -= 1;

        return tokenId00;
    }

    function safeTransferFrom(address from, address to, uint256 tokenId) public virtual override {
        safeTransferFrom(from, to, tokenId, "");
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory _data) public virtual override {
        require(_isApprovedOrOwner(msg.sender, tokenId), "ERC721: transfer caller is not owner nor approved");
        _transfer(from, to, tokenId);
        require(_checkOnERC721Received(from, to, tokenId, _data), "ERC721: transfer to non ERC721Receiver implementer");
    }

    function transferFrom(address from, address to, uint256 tokenId) public virtual override {
        //solhint-disable-next-line max-line-length
        require(_isApprovedOrOwner(msg.sender, tokenId), "ERC721: transfer caller is not owner nor approved");
        _transfer(from, to, tokenId);
    }

    /**
     * @dev This is the heart of the transfer mechanism.
     *
     *  in the case that a token has been merged while _inside_ of NG, 
     *  and someone tries to withdraw that token, it'll throw an error.
     *
     *  since the _defaultOwner will have many tokens, it doens't make sense
     *  to track ownership in _tokens since it would keep getting overwritten,
     *  we could also track it in the _owners mapping, but it would be redundant.
     *
     *  if we're sending a token to an address that doesn't have any tokens
     *  in it yet (_tokens[to] == 0), then it just works like a normal ERC721
     *  token transfer.
     *
     *  the final case is where we're sending a token to an address that _already_
     *  has a mass in it, in which case we delete the smaller of the mass values,
     *  and correspondingly burn the information associated with the destroyed token. 
     */
    function _transfer(address from, address to, uint256 tokenId) internal {
        require(ownerOf(tokenId) == from, "ERC721: transfer of token that is not own");
        require(to != address(0), "ERC721: transfer to the zero address");

        if(from == _defaultOwner){
            
            uint256 mass = decodeMass(_values[tokenId]);
            require(mass != 0, "ERC721: transfer attempt for nonexistent token");

        }

        _approve(address(0), tokenId);

        if(to == _defaultOwner){

            delete _tokens[from];
            delete _owners[tokenId];

            _balances[to] += 1;
            _balances[from] -= 1;

            emit Transfer(from, to, tokenId);
            return;

        }

        if(_tokens[to] == 0){
             
            _tokens[to] = tokenId;
            _owners[tokenId] = to;

            _balances[to] += 1;
            _balances[from] -= 1;

            emit Transfer(from, to, tokenId);
            return;
            
        }

        uint256 tokenId00 = _tokens[to];
        uint256 tokenId01 = tokenId;
        uint256 candidate = merge(tokenId00, tokenId01);

        if(candidate == tokenId00){

            _owners[tokenId01] = to;
            _tokens[to] = tokenId01;
            emit Transfer(from, to, tokenId01);

        } else {

            _owners[tokenId00] = to;
            _tokens[to] = tokenId00;
            emit Transfer(from, to, tokenId00);
        }
        delete _owners[candidate];
        emit Transfer(to, address(0), candidate);
    }

    /**
     * @dev Tell you who owns this token.
     *
     * token owned by the _defaultOwner, in that wallet
     * aren't tracked in the _owners mapping, they're 
     * tracked by the _values[tokenId] association, as 
     * you can see...
     */
    function ownerOf(uint256 tokenId) public view virtual override returns (address) {
        address owner = _owners[tokenId];
        uint256 mass = decodeMass(_values[tokenId]);

        if(owner == address(0) && mass != 0){
            return _defaultOwner;
        }
        require(owner != address(0), "ERC721: owner query for nonexistent token");
        return owner;
    }

    function balanceOf(address owner) public view override returns (uint256 balance) {
        uint256 balance = _balances[owner];
        return balance;
    }

    function massOf(uint256 tokenId) public view virtual returns (uint256) {
        return decodeMass(_values[tokenId]);
    }

    function tokenOf(address owner) public view virtual returns (uint256) {
        uint256 token = _tokens[owner];
        return token;
    }

    function approve(address to, uint256 tokenId) public virtual override {
        address owner = ownerOf(tokenId);
        require(to != owner, "ERC721: approval to current owner");

        require(msg.sender == owner || isApprovedForAll(owner, msg.sender),
            "ERC721: approve caller is not owner nor approved for all"
        );
        _tokenApprovals[tokenId] = to;
        emit Approval(ownerOf(tokenId), to, tokenId);
    }

    function _approve(address to, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId] = to;
        emit Approval(ownerOf(tokenId), to, tokenId);
    }

    function getApproved(uint256 tokenId) public view virtual override returns (address) {
        require(_exists(tokenId), "ERC721: approved query for nonexistent token");
        return _tokenApprovals[tokenId];
    }

    function setApprovalForAll(address operator, bool approved) public virtual override {
        require(operator != msg.sender, "ERC721: approve to caller");
        _operatorApprovals[msg.sender][operator] = approved;
        emit ApprovalForAll(msg.sender, operator, approved);
    }

    function isApprovedForAll(address owner, address operator) public view virtual override returns (bool) {
        return _operatorApprovals[owner][operator];
    }

    function _isApprovedOrOwner(address spender, uint256 tokenId) internal view virtual returns (bool) {
        require(_exists(tokenId), "ERC721: operator query for nonexistent token");
        address owner = ownerOf(tokenId);
        return (spender == owner || getApproved(tokenId) == spender || isApprovedForAll(owner, spender));
    }

    function _exists(uint256 tokenId) internal view virtual returns (bool) {
        return _values[tokenId] != 0;
    }

    /**
     * @dev This is how you initialize the contract to point to an SVG generator
     * If there are mistakes in the art, a new SVG generator can be deployed and this
     * pointer can be updated so you aren't SOL
     */
    function setSvgGenerator(address _generator) external onlyValidSender {
        // TODO: Lock This Down To Smart Contract Owner Or Supported Admin Address
        //require(msg.sender == owner, "You are not the owner");
        generator = ISvgGenerator(_generator);
    }

    /**
     * @dev All json metadata and SVG image are generated in a way that everything renders properly on secondary markets
     * Nifty Gateway marketplace should be able to make this call to get what is needed as well.
     */
    function tokenURI(uint256 tokenId) public virtual view override returns (string memory) {
        string memory svgData = getSvg(tokenId, 500);  
        string memory json = Base64.encode(bytes(string(abi.encodePacked(getJson(tokenId, svgData)))));
        return string(abi.encodePacked('data:application/json;base64,', json));
    }

    /**
     * @dev This is the hook to the external SVG generator contract - its job is to encode parameters to the SVG generator
     * in accordance with its documented interface and call generateSvg on the external contract.
     */
    function getSvg(uint tokenId, uint256 canvasSize) private view returns (string memory) {
        require(address(generator) != address(0), "Generator not set");
        (uint r, uint g, uint b) = getColor(tokenId);
        string memory color = string(abi.encodePacked("rgb(", r.toString(), ", ", g.toString(), ", ", b.toString(), ")"));
        bytes memory encodedParams = abi.encode(canvasSize, ABDKMath64x64.divu(massOf(tokenId), _totalMass), color);
        return generator.generateSvg(encodedParams);    
    }

    /**
     * @dev For Now Using Alternative 1 From ColorAlternatives.sol 
     * Replace this implementation with the agreed upon color mechanism
     * 0 - 10     => GOLD
     * 11 - 100   => SILVER
     * 101 - 1000 => BRONZE
     * 1001+      => BROWN
     */ 
    function getColor(uint256 tokenId) public view returns (uint, uint, uint) {
        (uint r, uint g, uint b, uint mass) = decodeMassAndColor(_values[tokenId]);
        return (r, g, b);        
    }

    /**
     * @dev Helper function for generating the Json ERC721 metadata
     */
    function getJson(uint tokenId, string memory svgData) private view returns (string memory) {
        bytes memory byteString;    
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).openJsonObject());
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("name", name(), true));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("description", "TODO: Describe NFT Rules, Behavior, Warnings", true));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("created_by", "Pak", true));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("image_data", svgData, true));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonComplexAttribute("attributes", getAttributes(massOf(tokenId)), false));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).closeJsonObject());
    
        return string(byteString);
    }

    /**
     * @dev Helper function for generating the traits/attributes section of the Json ERC721 metadata
     */
    function getAttributes(uint256 mass) private view returns (string memory) {
        bytes memory byteString;
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).openJsonArray());
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).pushJsonArrayElement(getAttribute(true, "number", true, "Mass", false, mass.toString()), false));
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).closeJsonArray());
    
        return string(byteString);
    }

    /**
     * @dev Helper function for generating the traits/attributes section of the Json ERC721 metadata - call once per attribute we want to incorporate
     */
    function getAttribute(bool includeDisplayType, string memory displayType, bool includeTraitType, string memory traitType, bool isValueAString, string memory value) private view returns (string memory) {
        bytes memory byteString;
        
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).openJsonObject());
    
        if(includeDisplayType) {
          byteString = abi.encodePacked(
            byteString,
            IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("display_type", displayType, true));
        }
    
        if(includeTraitType) {
          byteString = abi.encodePacked(
            byteString,
            IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("trait_type", traitType, true));
        }
    
        if(isValueAString) {
          byteString = abi.encodePacked(
            byteString,
            IJsonGenerator(address(this)).pushJsonPrimitiveStringAttribute("value", value, false));
        } else {
          byteString = abi.encodePacked(
            byteString,
            IJsonGenerator(address(this)).pushJsonPrimitiveNonStringAttribute("value", value, false));
        }
    
        byteString = abi.encodePacked(
          byteString,
          IJsonGenerator(address(this)).closeJsonObject());
    
        return string(byteString);
    }

    /**
     * @dev Helper function that gives us an encoded value that contains R,G,B and mass
     */
    function encodeMassAndColor(uint r, uint g, uint b, uint mass) public pure returns (uint) {
        require(r <= 255 && g <= 255 && b <= 255, "Values of R,G,B must be 0 to 255");
        require(mass > 0 && mass <= 99999999, "Mass must be 1 to 99999999");
        return ((r * 100000000000000) + (g * 100000000000) + (b * 100000000) + mass);
    }

    /**
     * @dev Helper function that will decompose a value into its R,G,B and mass values
     */
    function decodeMassAndColor(uint value) public pure returns (uint, uint, uint, uint) {
        uint r = value.div(100000000000000);
        uint g = value.sub(r.mul(100000000000000)).div(100000000000);
        uint b = value.sub(r.mul(100000000000000)).sub(g.mul(100000000000)).div(100000000);
        uint mass = value.sub(r.mul(100000000000000)).sub(g.mul(100000000000)).sub(b.mul(100000000));
        require(r <= 255 && g <= 255 && b <= 255, "Values of R,G,B must be 0 to 255");
        require(mass > 0 && mass <= 99999999, "Mass must be 1 to 99999999");
        return (r, g, b, mass);
    }

    /**
     * @dev Helper function that will decompose a value into its R component
     */
    function decodeColorR(uint value) public pure returns (uint) {
        return value.div(100000000000000);
    }
  
    /**
     * @dev Helper function that will decompose a value into its G component
     */
    function decodeColorG(uint value) public pure returns (uint) {
        return value.sub(decodeColorR(value).mul(100000000000000)).div(100000000000);
    }

    /**
     * @dev Helper function that will decompose a value into its B component
     */
    function decodeColorB(uint value) public pure returns (uint) {
        return value.sub(decodeColorR(value).mul(100000000000000)).sub(decodeColorG(value).mul(100000000000)).div(100000000);
    }

    /**
     * @dev Helper function that will decompose a value into its mass component
     */
    function decodeMass(uint value) public pure returns (uint) {
        return value.sub(decodeColorR(value).mul(100000000000000)).sub(decodeColorG(value).mul(100000000000)).sub(decodeColorB(value).mul(100000000));
    }
 
    /** TODO: Create a test for this function, I refactored it to be _slightly_ less ugly... 
     */
    function _checkOnERC721Received(address from, address to, uint256 tokenId, bytes memory _data) private returns (bool) {
        if (isContract(to)) {
            try IERC721Receiver(to).onERC721Received(msg.sender, from, tokenId, _data) returns (bytes4 retval) {
                return retval == IERC721Receiver(to).onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert("ERC721: transfer to non ERC721Receiver implementer");
                }
                // solhint-disable-next-line no-inline-assembly
                assembly {
                    revert(add(32, reason), mload(reason))
                }
            }
        }
        return true;
    }

    function isContract(address account) internal view returns (bool) {
        uint256 size;
        // solhint-disable-next-line no-inline-assembly
        assembly { size := extcodesize(account) }
        return size > 0;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        bytes4 ERC165 = 0x01ffc9a7;
        bytes4 ERC721 = 0x80ac58cd;
        bytes4 ERC721Metadata = 0x5b5e139f;
        return interfaceId == ERC165 
            || interfaceId == ERC721
            || interfaceId == ERC721Metadata;
    }
}

