pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";
import "./coreInterfaces/IZBond.sol";
import "./coreInterfaces/ITokenDescriptor.sol";
import "./coreInterfaces/IFeeSelector.sol";
import "./coreInterfaces/IComptroller.sol";
import "./ProvisioningPool.sol";
import "./coreInterfaces/IZNft.sol";
import "./ZumerMiner.sol";

contract ZBond is Initializable, UUPSUpgradeable, OwnableUpgradeable, ERC721EnumerableUpgradeable, ReentrancyGuard, IZBond {
    function initialize(
        string memory name_,
        string memory symbol_,
        address ZNft_,
        address feeSelector_,
        address comptroller_,
        address underlying_,
        address tokenDescriptor_
    ) public initializer {
        __Ownable_init();
        __ERC721_init(name_, symbol_);
        ZNft = ZNft_;
        feeSelector = feeSelector_;
        comptroller = comptroller_;
        underlying = underlying_;
        creditSpreadMantissa = 0.05 * 1e18;
        underwritingFeeRatioMantissa = 0.01 * 1e18;
        tokenDescriptor = tokenDescriptor_;
        fullAwardCollectionDuration = 30 days;
        maximumLoanDuration = 180 days;
        minimumPaymentDueFrequency = 30 days;
        maximumInterestGainDuration = 270 days;
        isZBond = true;
        IERC721(IZNft(ZNft).underlying()).setApprovalForAll(ZNft, true);
    }

    /******************
     * VIEW FUNCTIONS *
     ******************/

    // @notice Gets the tokenURI of a specified ZBond nft tokenId
    // @return The ZBond Nft metadata
    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        require(ownerOf(_tokenId) != address(0), "ZBond: Query for nonexistent token");
        SupplySnapshot storage supplySnapshot = tokenIdToSupplySnapshot[_tokenId];
        return ITokenDescriptor(tokenDescriptor).tokenURI(_tokenId, supplySnapshot.principalSupply, supplySnapshot.startDate, (supplySnapshot.virtualBalance * getExchangeRateMantissa()) / 1e18);
    }

    // @notice Gets the rate between the total supply of ZBondToken: totalSupply
    // @return The exchange rate of the virtual balance vs underlying
    function getExchangeRateMantissa() public view override returns (uint256) {
        if (totalVirtualSupply == 0) {
            return 1e18;
        } else {
            return (totalSupplyPrinciplePlusInterest * 1e18) / totalVirtualSupply;
        }
    }

    // @notice Gets the borrower's current borrow balance
    // @param  borrower  The borrow address of which current borrow balance is to be calculated
    // @return The borrower's current borrow balance, including accrued interest based on the current block
    function getAccountCurrentBorrowBalance(address borrower) public view override returns (uint256) {
        uint256 duration = accountBorrows[borrower].loanDuration;
        uint256 deadline = accountBorrows[borrower].deadline;
        uint256 accruedPeriod;

        if (block.timestamp >= deadline) {
            // if due/overdue, balance should be all the principle and all the interests
            accruedPeriod = duration;
        } else {
            // remaining loan duration = deadline - block.timestamp
            // accruedPeriod = loan duration - remaining loan duration
            accruedPeriod = duration - (deadline - block.timestamp);
        }
        return _getAccountBorrowBalance(borrower, accruedPeriod);
    }

    // @notice Gets the borrower's borrow balance at the time of maturity
    // @param  borrower  The borrow address of which current borrow balance is to be calculated
    // @return The borrower's borrow balance at the time of maturity, including accrued interest that will have accrued
    function getAccountBorrowBalanceAtMaturity(address borrower) public view override returns (uint256) {
        return _getAccountBorrowBalance(borrower, accountBorrows[borrower].loanDuration);
    }

    /**********************
     * EXTERNAL FUNCTIONS *
     **********************/

    // @notice A caller can pledge zNfts and borrow underlying tokens in one transaction
    // @param  tokenIds  The ZNft tokenIds that the caller would like to pledge
    // @param  amount  The amount the caller would like to borrow
    // @param  duration  The duration of the loan
    function pledgeThenBorrow(
        uint256[] calldata tokenIds,
        uint256 amount,
        uint256 duration
    ) external override nonReentrant {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721(IZNft(ZNft).underlying()).transferFrom(msg.sender, address(this), tokenIds[i]);
        }
        IZNft(ZNft).mint(tokenIds, msg.sender);
        borrowInternal(amount, duration, msg.sender, msg.sender);
    }

    // @notice A caller can repay all outstanding loan and redeem ZNfts
    // @param  tokenIds  The ZNft tokenIds that the caller would like to redeem
    function repayAllThenRedeem(uint256[] calldata tokenIds) external override nonReentrant {
        uint256 borrowBalance = getAccountCurrentBorrowBalance(msg.sender);
        repayBorrowInternal(msg.sender, msg.sender, borrowBalance);
        IZNft(ZNft).redeem(tokenIds, msg.sender);
    }

    // @notice A caller can deposit underlying tokens into ZBond and receive a ZBond Nft
    // @param  amount  The amount of underlying tokens the caller would like to deposit
    // @return  The ZBond NFT tokenId minted for this deposit
    function mint(uint256 amount) external override nonReentrant returns (uint256 tokenId) {
        return mintInternal(amount);
    }

    // @notice A caller can redeem his deposit from the ZBond by burning multiple ZBond Nfts
    // @param  tokenIds  The ZBonds tokenIds that the caller would like to redeem
    // @return The amount of underlying redeemed
    function redeem(uint256[] calldata tokenIds) external override nonReentrant returns (uint256) {
        uint256 redeemable = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(ownerOf(tokenIds[i]) == msg.sender, "ZBond: only zBond owner can redeem");

            SupplySnapshot storage supplySnapshot = tokenIdToSupplySnapshot[tokenIds[i]];
            redeemable += redeemInternal(tokenIds[i], supplySnapshot.virtualBalance);
        }

        return redeemable;
    }

    // @notice A caller can redeem his deposit from the ZBond by burning multiple ZBond Nft, until reaching a specified balance to burn
    // @param  tokenIds  The ZBonds tokenIds that the caller would like to redeem
    // @param  totalVirtualBalance  The target virtual balance to burn
    // @return The amount of underlying redeemed
    function redeem(uint256[] calldata tokenIds, uint256 totalVirtualBalance) external override nonReentrant returns (uint256) {
        uint256 redeemable = 0;

        for (uint256 i = 0; i < tokenIds.length && totalVirtualBalance != 0; i++) {
            require(ownerOf(tokenIds[i]) == msg.sender, "ZBond: only zBond owner can redeem");

            SupplySnapshot storage supplySnapshot = tokenIdToSupplySnapshot[tokenIds[i]];

            if (totalVirtualBalance < supplySnapshot.virtualBalance) {
                // partial redeem if already reaching the target
                redeemable += redeemInternal(tokenIds[i], totalVirtualBalance);
                totalVirtualBalance = 0;
            } else {
                // full redeem if not reaching the target redeem
                totalVirtualBalance -= supplySnapshot.virtualBalance;
                redeemable += redeemInternal(tokenIds[i], supplySnapshot.virtualBalance);
            }
        }
        return redeemable;
    }

    // @notice A caller can borrow underlying tokens from this ZBond
    // @param  amount  The amount the caller would like to borrow
    // @param  duration  The duration of the loan
    function borrow(uint256 amount, uint256 duration) external override nonReentrant {
        borrowInternal(amount, duration, msg.sender, msg.sender);
    }

    // @notice A caller can borrow on behalf of a borrower, who will bear the liability of the loan while the caller
    //         will receive the borrowed amount of underlying token Note that borrowOnBehalf should only be called by
    //         a ZumerBuyNowPayLater contract, which is verified and checked against by the comtroller.borrowOnBehalfAllowed function
    // @param  amount  The amount the caller would like to borrow
    // @param  duration  The duration of the loan
    // @param  borrower  The borrow that this borrowed amount should be recorded against
    function borrowOnBehalf(
        uint256 amount,
        uint256 duration,
        address borrower
    ) external override nonReentrant {
        IComptroller(comptroller).borrowOnBehalfAllowed(address(this), msg.sender);
        borrowInternal(amount, duration, borrower, msg.sender);
    }

    // @notice A caller can repay his debt
    // @param  amount  The amount of underlying token that the caller would like to repay
    //                 Note that if the amount is type(uint256).max, it means to pay off all outstanding balance at the
    //                 time the tx is being mined.
    //                 This design is for situations when the front end sends the tx to repay the total outstanding
    //                 balance at the time the user interacts with the DAPP, while the tx is only confirmed
    //                 some time later, by then more interests will have been accrued and the intended full repayment will not
    //                 be fulfilled due to the tiny amount of 'dust' interests accrued.
    function repayBorrow(uint256 amount) external override nonReentrant {
        repayBorrowInternal(msg.sender, msg.sender, amount);
    }

    // @notice A caller can repay someone else's debt
    // @param  borrowr  The borrower whose debt the caller is repaying for
    // @param  amount  The amount of underlying token that the caller would like to repay
    //                 Note: see function repayBorrow: @param amount
    function repayBorrowBehalf(address borrower, uint256 amount) external override nonReentrant {
        repayBorrowInternal(borrower, msg.sender, amount);
    }

    // @notice A caller can liquidate a borrower's ZNft tokenId when liquidation threshold is satisfied
    // @param  liquidator  The liquidator that will receive the liquidated ZNft
    // @dev Note that currently this has to be the provisioningPool associated with this ZBond
    // @param  amount  The amount of underlying token that the caller would like to repay
    //                 Note: see function repayBorrow: @param amount
    function liquidateBorrow(
        address liquidator,
        address borrower,
        uint256 tokenId
    ) external override nonReentrant returns (uint256) {
        return liquidateBorrowInternal(liquidator, borrower, tokenId);
    }

    /**********************
     * INTERNAL FUNCTIONS *
     **********************/

    // @notice Calculates the borrow address's borrow balance given accruedPeriod
    // @param  borrower  The borrower whose account borrow balance is to be calculated
    // @param  accruedPeriod  The duration of time elapsed for the borrower's loan
    // @return The borrower's total borrow balance including the accrued interest
    function _getAccountBorrowBalance(address borrower, uint256 accruedPeriod) internal view returns (uint256) {
        uint256 principle = accountBorrows[borrower].principalBorrow;

        if (principle == 0) {
            return 0;
        }

        uint256 interestRate = accountBorrows[borrower].weightedInterestRate;

        // interestRate is annualised percentage rate
        uint256 interest = (principle * interestRate * accruedPeriod) / 365 days / 1e18;
        return principle + interest;
    }

    // @notice Handles the mint operation when someone deposits underlying tokens
    // @param  amountIn  The amount of underlying token deposited
    // @return The ZBond NFT tokenId minted
    function mintInternal(uint256 amountIn) internal returns (uint256 tokenId) {
        uint256 virtualBalance = (amountIn * 1e18) / getExchangeRateMantissa();
        tokenId = nextTokenId;
        nextTokenId += 1;
        _mint(msg.sender, tokenId);

        // change state
        tokenIdToSupplySnapshot[tokenId].principalSupply = amountIn;
        tokenIdToSupplySnapshot[tokenId].virtualBalance = virtualBalance;
        tokenIdToSupplySnapshot[tokenId].startDate = block.timestamp;
        // change global state
        totalVirtualSupply += virtualBalance;
        totalSupplyPrinciplePlusInterest += amountIn;

        // interaction
        IERC20(underlying).transferFrom(msg.sender, address(this), amountIn);
        emit Mint(tokenId, amountIn, virtualBalance, totalSupplyPrinciplePlusInterest);
        return tokenId;
    }

    // @notice Handles the redeem operation when someone redeems their ZBond Nft
    // @param  tokenId  The ZBond Nft tokenId that the caller would like to redeem
    // @param  virtualAmount The virtual amount to be redeemed
    // @return The underlying asset that will be transfered out from the contract
    function redeemInternal(uint256 tokenId, uint256 virtualAmount) internal returns (uint256) {
        SupplySnapshot storage supplySnapshot = tokenIdToSupplySnapshot[tokenId];

        if (virtualAmount >= supplySnapshot.virtualBalance) {
            virtualAmount = supplySnapshot.virtualBalance;
        }

        uint256 redeemable = (virtualAmount * getExchangeRateMantissa()) / 1e18; // redeemable underlying in this transaction
        uint256 totalRedeemable = (supplySnapshot.virtualBalance * getExchangeRateMantissa()) / 1e18; // current total redeemable underlying
        uint256 withdrawRatio = (virtualAmount * 1e18) / supplySnapshot.virtualBalance; // ratio of the withdrawn and the total withdrawable

        // draw money from the provisioning pool if there's not enough in zBond
        if (redeemable > IERC20(underlying).balanceOf(address(this))) {
            uint256 amountDrawnFromPP = redeemable - IERC20(underlying).balanceOf(address(this));
            underlyingOwedToProvisioningPool += amountDrawnFromPP;
            //totalSupplyPrinciplePlusInterest += amountDrawnFromPP;
            IProvisioningPool(provisioningPool).replenishLendingPool(amountDrawnFromPP);
        }

        // reduce total supply
        // change global state
        totalVirtualSupply -= virtualAmount;
        totalSupplyPrinciplePlusInterest -= redeemable;

        // withdraw interest and principle proportionally
        uint256 currentTotalInterest = totalRedeemable - supplySnapshot.principalSupply;
        uint256 interestRedeemed = (currentTotalInterest * withdrawRatio) / 1e18;
        supplySnapshot.principalSupply -= (supplySnapshot.principalSupply * withdrawRatio) / 1e18;
        supplySnapshot.virtualBalance -= virtualAmount;

        // interaction
        address tokenOwner = ownerOf(tokenId);
        emit Redeem(tokenId, redeemable, supplySnapshot.virtualBalance, tokenOwner);

        // penalty for interest rate if staked less than 270 days
        uint256 ratio = ((block.timestamp - supplySnapshot.startDate) * 1e18) / maximumInterestGainDuration;
        if (ratio < 1e18) {
            uint256 penalty = (interestRedeemed * ratio) / 1e18;
            IERC20(underlying).transfer(address(provisioningPool), penalty);
            redeemable -= penalty;
        }

        IERC20(underlying).transfer(tokenOwner, redeemable);

        // clean up if the deposit is empty
        if (supplySnapshot.virtualBalance == 0) {
            delete tokenIdToSupplySnapshot[tokenId];
            _burn(tokenId);
        }

        return redeemable;
    }

    // @notice Handles the borrow operation when someone borrows underlying tokens from ZBond
    // @param  amount  The borrowed amount
    // @param  duration The duration of the loan
    // @param  borrower The borrower who is liable for this loan
    // @param  receiver The receiver who receives the loan amount
    function borrowInternal(
        uint256 amount,
        uint256 duration,
        address borrower,
        address receiver
    ) internal {
        IComptroller(comptroller).borrowAllowed(address(this), borrower, amount, duration);
        uint256 fundingRateMantissa = IFeeSelector(feeSelector).getFundingCostForDuration(duration, maximumLoanDuration);
        // interestRate = fundingRate + creditSpread
        uint256 interestRateMantissa = fundingRateMantissa + creditSpreadMantissa;
        // effects
        updateUserStateAfterBorrow(borrower, amount, duration, interestRateMantissa);
        totalBorrows += amount;

        // interactions
        // transfer underwriting fee to the admin
        uint256 underwritingFee = (amount * underwritingFeeRatioMantissa) / 1e18;
        IERC20(underlying).transfer(owner(), underwritingFee);
        // transfer principle borrow to the loan receiver
        IERC20(underlying).transfer(receiver, amount - underwritingFee);
        emit Borrow(msg.sender, amount, totalBorrows, duration);
    }

    // @notice Handles the liquidateBorrow operation
    // @param  liquidator  The liquidator that initiates the liquidateBorrow operation
    // @param  borrower  The borrower whose debt is underwater / in arrears
    // @param  tokenId  The ZNft to be liquidated for repaying the debt
    // @return The total repayment amount
    function liquidateBorrowInternal(
        address liquidator,
        address borrower,
        uint256 tokenId
    ) internal returns (uint256) {
        IComptroller(comptroller).liquidateBorrowAllowed(address(this), address(ZNft), liquidator, borrower, tokenId);

        uint256 repaymentAmount = IComptroller(comptroller).calculateLiquidationAmount(borrower, address(this), tokenId, address(ZNft));

        repayBorrowInternal(borrower, liquidator, repaymentAmount);

        // seize collaterals;
        IZNft(ZNft).seize(liquidator, borrower, tokenId);

        emit LiquidateBorrow(liquidator, borrower, repaymentAmount, ZNft);
        return repaymentAmount;
    }

    // @notice Handles the repayBorrow operation
    // @param  borrower  The borrower whose debt is to be repaid
    // @param  payer  The payer who actually repays the debt
    // @param  repaymentAmount  The amount of underlying token to be drawn from the payer to repay the debt
    // @return actualRepaymentAmount  The actual repayment amount to be drawn from the payer
    // @return provisioningInterest  The share of interest payment attributable to the provisioningPool as fee share
    function repayBorrowInternal(
        address borrower,
        address payer,
        uint256 repaymentAmount
    ) internal returns (uint256 actualRepaymentAmount, uint256 provisioningInterest) {
        require(address(provisioningPool) != address(0), "ZBond: provisioningPool not set");
        IComptroller(comptroller).repayBorrowAllowed(address(this), payer, borrower, repaymentAmount);

        uint256 currentBorrowBalance = getAccountCurrentBorrowBalance(borrower);

        if (repaymentAmount == type(uint256).max || repaymentAmount > currentBorrowBalance) {
            actualRepaymentAmount = currentBorrowBalance;
        } else {
            actualRepaymentAmount = repaymentAmount;
        }

        // effects
        uint256 interestPaid = updateUserStateAfterRepay(borrower, actualRepaymentAmount, currentBorrowBalance);

        provisioningInterest = (provisioningPoolMantissa * interestPaid) / 1e18;
        totalSupplyPrinciplePlusInterest += (interestPaid - provisioningInterest);
        underlyingOwedToProvisioningPool += provisioningInterest;

        emit RepayBorrow(payer, borrower, actualRepaymentAmount);
        // interactions
        // repay pp first
        if (underlyingOwedToProvisioningPool > actualRepaymentAmount) {
            underlyingOwedToProvisioningPool -= actualRepaymentAmount;
            IERC20(underlying).transferFrom(borrower, address(provisioningPool), actualRepaymentAmount);
        } else {
            uint256 repaymentAmountDueForProvisioningPool = underlyingOwedToProvisioningPool;
            uint256 repaymentAmountDueForZBond = actualRepaymentAmount - underlyingOwedToProvisioningPool;
            underlyingOwedToProvisioningPool = 0;
            IERC20(underlying).transferFrom(borrower, address(provisioningPool), repaymentAmountDueForProvisioningPool);
            IERC20(underlying).transferFrom(payer, address(this), repaymentAmountDueForZBond);
        }
        return (actualRepaymentAmount, provisioningInterest);
    }

    // @notice Handles the state changes for repayment
    // @param  borrower  The borrower whose debt is to be repaid
    // @param  repaymentAmount  The amount of underlying token to be drawn from the payer to repay the debt
    // @param  currentAccountBalance  The borrower's current debt balance including interests accrued
    // @return interestAccrued  The interest accrued
    function updateUserStateAfterRepay(
        address borrower,
        uint256 repaymentAmount,
        uint256 currentBorrowBalance
    ) internal returns (uint256 interestAccrued) {
        uint256 oldPrincipalBorrow = accountBorrows[borrower].principalBorrow;
        interestAccrued = currentBorrowBalance - oldPrincipalBorrow;
        require(repaymentAmount <= currentBorrowBalance, "ZBond: cannot overpay");
        require(repaymentAmount >= interestAccrued, "ZBond: cannot pay less than interests payable");
        // if user reduced position (or at least paid all of their interests), then reduce principal borrow and carry over the minimum payment due time, total loan due time is not affected
        if (currentBorrowBalance > repaymentAmount) {
            // partial repayment
            accountBorrows[borrower].minimumPaymentDue = block.timestamp + minimumPaymentDueFrequency;
            accountBorrows[borrower].principalBorrow = currentBorrowBalance - repaymentAmount;
        } else {
            // complete repayment
            delete accountBorrows[borrower];
        }

        // since repaymentAmounts <= currentBorrowBalance and repaymentAmounts >= interestsAccrued
        // curentBorrowBalance - repaymentAmount has to be smaller than oldPrincipalBorrow
        uint256 principalBorrowDecreased = oldPrincipalBorrow - (currentBorrowBalance - repaymentAmount);
        if (zumerMiner != address(0) && principalBorrowDecreased > 0) {
            // we change user reward balance unless the zumer miner is not set.
            IZumerMiner(zumerMiner).decreaseBalance(address(this), borrower, principalBorrowDecreased);
        }
        return interestAccrued;
    }

    // @notice Handles the state changes for borrowing
    // @param  borrower  The borrower of the loan
    // @param  borrowAmount  The amount of the loan
    // @param  duration  The duration of the loan
    // @param  interestRateMantissa  The interest rate = fundingRate + creditSpread
    function updateUserStateAfterBorrow(
        address borrower,
        uint256 borrowAmount,
        uint256 duration,
        uint256 interestRateMantissa
    ) internal {
        // initialize borrow state if dueTime is not set
        if (accountBorrows[borrower].minimumPaymentDue == 0) {
            accountBorrows[borrower].minimumPaymentDue = block.timestamp + minimumPaymentDueFrequency;
        }
        if (accountBorrows[borrower].deadline == 0) {
            accountBorrows[borrower].loanDuration = duration;
            accountBorrows[borrower].deadline = block.timestamp + accountBorrows[borrower].loanDuration;
        }
        require((accountBorrows[borrower].minimumPaymentDue >= block.timestamp) && (accountBorrows[borrower].deadline >= block.timestamp), "ZBond: cannot increase position if overdue");
        // set weighted interest
        uint256 durationLeft = accountBorrows[borrower].deadline - block.timestamp;

        accountBorrows[borrower].weightedInterestRate =
            ((accountBorrows[borrower].weightedInterestRate * accountBorrows[borrower].principalBorrow * accountBorrows[borrower].loanDuration + interestRateMantissa * borrowAmount * durationLeft)) /
            (accountBorrows[borrower].principalBorrow * accountBorrows[borrower].loanDuration + borrowAmount * durationLeft);

        accountBorrows[borrower].principalBorrow += borrowAmount;
        if (zumerMiner != address(0)) {
            // we change user reward balance unless the zumer miner is not set.
            IZumerMiner(zumerMiner).increaseBalance(address(this), msg.sender, borrowAmount);
        }
    }

    /*******************
     * ADMIN FUNCTIONS *
     *******************/

    // @notice Owner is able to set provisioningPool address and its feeShare
    // @param  newProvisioningPoolAddress  The provisioningPool address
    // @param  newProvisioingPoolMantissa  The percentage of interest payment that is attributable to the provisioningPool as fee share
    function setProvisioningPool(address newProvisioningPoolAddress, uint256 newProvisioingPoolMantissa) external override onlyOwner {
        require(newProvisioingPoolMantissa < 1 ether, "ZBond: provisioningPool feeShare should not >= 100% of the interest payment");
        provisioningPool = newProvisioningPoolAddress;
        provisioningPoolMantissa = newProvisioingPoolMantissa;
    }

    // @notice Owner is able to set the underwriting fee
    // @param  newUnderwritingFeeRatioMantissa  The new underwriting fee ratio
    function setUnderwritingFeeRatio(uint256 newUnderwritingFeeRatioMantissa) external override onlyOwner {
        require(newUnderwritingFeeRatioMantissa < 1 ether, "ZBond: newUnderwritingFeeRatioMantissa should not >= 100% of the interest payment");
        underwritingFeeRatioMantissa = newUnderwritingFeeRatioMantissa;
    }

    // @notice Owner is able to set the credit spread specific to the nft collateral
    // @param  newCreditSpreadMantissa  The new credit spread
    function setCreditSpread(uint256 newCreditSpreadMantissa) public override onlyOwner {
        creditSpreadMantissa = newCreditSpreadMantissa;
    }

    // @notice Owner is able to set ZumerMiner contract address
    // @param  newZumerMiner  The new ZumerMiner contract
    function setZumerMiner(address newZumerMiner) external override onlyOwner {
        zumerMiner = newZumerMiner;
    }

    /**********************
     * PROXY FUNCTIONS *
     **********************/
    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
}

