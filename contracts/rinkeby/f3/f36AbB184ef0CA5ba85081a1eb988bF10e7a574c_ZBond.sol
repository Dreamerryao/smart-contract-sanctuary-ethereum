import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./coreInterfaces/IZBond.sol";
import "./coreInterfaces/IZNft.sol";
import "./coreInterfaces/ITokenDescriptor.sol";
import "./coreInterfaces/IComptroller.sol";
import "./coreInterfaces/IAuctionMarket.sol";
import "./coreInterfaces/ITokenDescriptor.sol";
import "./coreInterfaces/IProvisioningPool.sol";
import "./coreInterfaces/IZumerMiner.sol";

contract ProvisioningPool is Initializable, UUPSUpgradeable, OwnableUpgradeable, ReentrancyGuard, ERC721EnumerableUpgradeable, IProvisioningPool {
    // @notice minimum required staking duration
    uint256 public constant minRequiredDuration = 30 days;
    // @notice minimum required staking duration to receive rewards
    uint256 public constant minDurationForRewards = 6 * 30 days;
    // @notice maximum required staking duration to receive rewards
    uint256 public constant maxDurationForRewards = 365 * 2 days;
    // @notice minimum reward weight given to those who stake for minDurationForRewards
    uint256 public constant minRewardWeight = 0.5 ether;
    // @notice maximum reward weight given to those who stake for longer thanmaxDurationForRewards
    uint256 public constant maxRewardWeight = 2.25 ether;

    function initialize(
        address _comptroller,
        address _ZNft,
        address _ZBond,
        string memory _name,
        string memory _symbol,
        address _auctionMarket,
        address _underlying,
        address _tokenDescriptor
    ) public initializer {
        __Ownable_init();
        __ERC721_init(_name, _symbol);

        comptroller = _comptroller;
        ZNft = _ZNft;
        ZBond = _ZBond;
        auctionMarket = _auctionMarket;
        underlying = _underlying;
        tokenDescriptor = _tokenDescriptor;
        IERC721(ZNft).setApprovalForAll(address(auctionMarket), true);
        IERC20(underlying).approve(address(ZBond), type(uint256).max);
    }

    /******************
     * VIEW FUNCTIONS *
     ******************/

    // @notice  This function returns the current exchange rate between totalVirtualSupply vs underlying token balance
    // @return  Current exchange rate, scaled by 1e18
    function getExchangeRateMantissa() public view override returns (uint256) {
        if (totalVirtualSupply == 0) {
            return 1e18;
        } else {
            return (IERC20(underlying).balanceOf(address(this)) * 1e18) / totalVirtualSupply;
        }
    }

    // @notice  This function returns the total claimable underlying token of an account
    //          if the user has multiple staking units (pp Nft), it will return the sum
    // @param   account  The account whose total claimable underlying token is to be calculated
    // @return  total claimable underlying token of an account
    function getMaxClaimableUnderlying(address account) public view override returns (uint256) {
        // calculate maximum claimable underlying tokens.
        uint256 maxClaimable = 0;
        uint256[] memory tokenIds = ownedTokenIds(account);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            StakeData storage sd = tokenIdToStakeData[tokenIds[i]];
            if (sd.expireTime < block.timestamp) {
                maxClaimable += (sd.virtualBalance * getExchangeRateMantissa()) / 1e18;
            }
        }
        return maxClaimable;
    }

    // @notice This function returns an array of all provisioningPool Nft tokenIds owned by a user
    // @param  user  The user address
    // @return An array of all provisioningPool Nft tokenIds owned by the user
    function ownedTokenIds(address user) public view override returns (uint256[] memory tokenIds) {
        uint256 balance = balanceOf(user);
        tokenIds = new uint256[](balance);
        for (uint256 i = 0; i < balance; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(user, i);
        }
        return tokenIds;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(ownerOf(tokenId) != address(0), "Query for nonexistent token");
        StakeData storage sd = tokenIdToStakeData[tokenId];
        return ITokenDescriptor(tokenDescriptor).tokenURI(tokenId, sd.stakedUnderlying, sd.expireTime, (sd.virtualBalance * getExchangeRateMantissa()) / 1e18);
    }

    /**********************
     * EXTERNAL FUNCTIONS *
     **********************/

    // @notice This function allows a user to stake 'amount' of underlying token for 'time' duration
    // @param  amount  The amount of underlying token to be staked
    // @param  time  The duration of the staking unit
    // @return The provisioningPool Nft tokenId minted
    function stake(uint256 amount, uint256 duration) external override returns (uint256) {
        return stakeInternal(amount, duration);
    }

    // @notice This function allows a user to unstake underlying tokens from the provisioningPool, given an array of provsisioningPool Nft tokenIds
    // @param  tokenIds  An array of provisioningPool tokenIds
    // @return The total amount of underlying tokens unstaked
    function unstake(uint256[] calldata tokenIds) external override nonReentrant returns (uint256) {
        require(tokenIds.length > 0, "provisioningPool: array length should not be 0");
        uint256 totalUnstakedUnderlyingAmount;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(ownerOf(tokenIds[i]) == msg.sender, "provisioningPool: msg.sender not the nft owner");
            totalUnstakedUnderlyingAmount += unstakeInternal(tokenIds[i]);
        }
        return totalUnstakedUnderlyingAmount;
    }

    // @notice This function allows a ZBond contract to borrow 'amount' of underlying token
    //         when the liquidity in the ZBond contract is insufficient to satisfy a valid redemption
    // @dev    This function should only be called by the ZBond associated with this contract
    // @param  amount  The amount of underlying token to be drawn from this contract
    function replenishLendingPool(uint256 amount) external override {
        require(msg.sender == address(ZBond));
        require(amount <= IERC20(underlying).balanceOf(address(this)), "provisioning: insufficient funds");
        IERC20(underlying).transfer(msg.sender, amount);
    }

    // @notice This function can be called when a ZNft tokenId satisfy the liquidation threshold
    //         That is, when a borrower is missing interest payments or his borrowed position is under water
    //         This contract repays the owed amount first, and then initiate the auction for the ZNft tokenId
    // @param  tokenId  The ZNft tokenId to be liquidated
    function liquidateZNft(uint256 tokenId) external override {
        // require(address(address(ZNft).comptroller()) == comptroller, "comptroller does not match");

        address borrower = IERC721(ZNft).ownerOf(tokenId);
        uint256 actualRepayAmount = IZBond(ZBond).liquidateBorrow(address(this), borrower, tokenId);

        require(IERC721(ZNft).ownerOf(tokenId) == address(this), "provisioningPool: zNft liquidation failed");
        // start auction
        IAuctionMarket(auctionMarket).startAuction(tokenId, actualRepayAmount, borrower, ZNft);
    }

    /**********************
     * INTERNAL FUNCTIONS *
     **********************/

    // @notice Handles the stake operation when someone stakes underlying tokens
    // @param  underlyingAmount  The amount of underlyingToken to be staked
    // @param  time The duration of the staking unit
    // @return tokenId  The provisioiningPool Nft tokenId minted
    function stakeInternal(uint256 underlyingAmount, uint256 duration) internal returns (uint256) {
        require(duration >= minRequiredDuration, "provisioningPool: Stake should be at least minRequiredDuration");

        uint256 virtualAmount = (underlyingAmount * 1e18) / getExchangeRateMantissa();
        uint256 tokenId = nextTokenId;
        uint256 expireTime = duration + block.timestamp;
        totalVirtualSupply += virtualAmount;
        totalStakedUnderlying += underlyingAmount;
        nextTokenId += 1;
        StakeData memory stakeData = StakeData({duration: duration, stakedUnderlying: underlyingAmount, expireTime: expireTime, virtualBalance: virtualAmount});
        tokenIdToStakeData[tokenId] = stakeData;
        _mint(msg.sender, tokenId);
        IERC20(underlying).transferFrom(msg.sender, address(this), underlyingAmount);
        emit Staked(tokenId, underlyingAmount, expireTime);
        uint256 rewardBalance = getRewardBalance(virtualAmount, duration);
        if (zumerMiner != address(0) && rewardBalance > 0) {
            IZumerMiner(zumerMiner).increaseBalance(address(this), msg.sender, rewardBalance);
        }
        return tokenId;
    }

    // @notice Handles the unstake operation when someone stakes underlying tokens
    // @param  tokenId  The provisioningPool Nft tokenId to be unstaked
    // @return unstakeUnderlyingAmount  The amount of underlying token unstaked
    function unstakeInternal(uint256 tokenId) internal returns (uint256 unstakeUnderlyingAmount) {
        StakeData storage stakeData = tokenIdToStakeData[tokenId];
        if (stakeData.expireTime <= block.timestamp) {
            unstakeUnderlyingAmount = (stakeData.virtualBalance * getExchangeRateMantissa()) / 1e18;
            require(
                totalStakedUnderlying - unstakeUnderlyingAmount >= IZBond(ZBond).totalBorrows(),
                "provisioningPool: totalStakedUnderyling has to be gt ZBond.totalBorrows after this unstake operation"
            );
            totalVirtualSupply -= stakeData.virtualBalance;
            totalStakedUnderlying -= unstakeUnderlyingAmount;

            uint256 duration = stakeData.duration;
            uint256 virtualBalance = stakeData.virtualBalance;
            uint256 expireTime = stakeData.expireTime;

            delete tokenIdToStakeData[tokenId];
            IERC20(underlying).transfer(ownerOf(tokenId), unstakeUnderlyingAmount);
            emit Unstaked(tokenId, unstakeUnderlyingAmount, expireTime);
            uint256 rewardBalance = getRewardBalance(virtualBalance, duration);
            if (zumerMiner != address(0) && rewardBalance > 0) {
                IZumerMiner(zumerMiner).decreaseBalance(address(this), msg.sender, rewardBalance);
            }

            _burn(tokenId);
        }
    }

    // @notice Calculates the amount of weighted reward balance
    // @param  amount  The amount of underlying token
    // @param  duration The duration of the staking unit
    // @return The amount of weighted reward balance
    function getRewardBalance(uint256 amount, uint256 duration) internal pure returns (uint256) {
        if (duration < minDurationForRewards) {
            return 0;
        }

        uint256 durationOnTop = duration - minDurationForRewards;
        uint256 rewardTimeWeight = duration > maxDurationForRewards
            ? maxRewardWeight
            : minRewardWeight + (durationOnTop * (maxRewardWeight - minRewardWeight)) / (maxDurationForRewards - minDurationForRewards);
        return (amount * rewardTimeWeight) / 1e18;
    }

    // @notice Overrides the ERC721 _transfer function so as to adjust the reward balance
    // @param  from  The Nft is to be transferred from 'from' address
    // @param  to  The Nft is to be transferred to 'to' address
    // @param  tokenId The Nft tokenId to be transferred
    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        super._transfer(from, to, tokenId);
        StakeData storage stakeData = tokenIdToStakeData[tokenId];
        uint256 rewardBalance = getRewardBalance(stakeData.virtualBalance, stakeData.duration);
        if (zumerMiner != address(0) && rewardBalance > 0) {
            IZumerMiner(zumerMiner).decreaseBalance(address(this), from, rewardBalance);
            IZumerMiner(zumerMiner).increaseBalance(address(this), to, rewardBalance);
        }
    }

    /*******************
     * ADMIN FUNCTIONS *
     *******************/

    // @notice Owner is able to set ZumerMiner contract address
    // @param  newZumerMiner  The new ZumerMiner contract
    function setZumerMiner(address newZumerMiner) external override onlyOwner {
        zumerMiner = newZumerMiner;
    }

    /*******************
     * PROXY FUNCTIONS *
     *******************/

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
}

