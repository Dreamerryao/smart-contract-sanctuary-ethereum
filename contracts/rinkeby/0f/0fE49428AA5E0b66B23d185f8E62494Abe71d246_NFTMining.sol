pragma solidity 0.8.6;

import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "./utils/Operator.sol";
import "./libraries/LibHelper.sol";
import "./interfaces/IOnePieceNFT.sol";
import "./interfaces/IOnePieceToken.sol";
import "hardhat/console.sol";

struct SlotItem {
    uint128 tokenId;
    uint32 roleId;
    uint32 quality;
    uint64 power;
}

contract NFTMining is Operator {
    uint256 public constant SLOT_NUM = 76;
    uint256 public constant MAX_QUALITY = 5;
    uint256[MAX_QUALITY] _quality2Power = [100, 300, 1800, 5400, 16200];

    // 0.00275 ether / 1 days
    uint256 public _rewardRatePerPower = 31828703703;
    // 2015000000 ether / 2 / 4 / 365 / 1 days;
    uint256 public _referenceRewardRate = 7986903855910705225;
    uint256 public _reserve;
    uint256 public _halveTimes = 1;

    mapping(address => uint256[SLOT_NUM]) _slots;
    uint256[3] public _referPowerRates;

    uint256 _accOpcPerShare;
    uint256 _lastRewardTime;

    mapping(address => uint256) _debts;
    mapping(address => uint256) _stakedPowers;
    mapping(address => address) _referrers;
    mapping(address => address[]) _referrals;
    mapping(address => uint256) _referrerRewards;
    mapping(address => uint256) _withdrews;
    mapping(address => uint256) _withdrewReferRewards;
    uint256 _stakedTotalPower;

    IOnePieceToken _opc;
    IOnePieceNFT _opnft;

    event PowerChanged(
        address indexed account,
        uint256 totalPower,
        uint256 power,
        uint256 time
    );

    constructor(address operator) {
        operatorSetted(operator, true);

        _referPowerRates = [5, 5, 5];
        _lastRewardTime = block.timestamp;
    }

    function setOnePieceAddresses(address opc, address opnft)
        external
        onlyOperator
    {
        if (opc != address(0)) _opc = IOnePieceToken(opc);
        if (opnft != address(0)) _opnft = IOnePieceNFT(opnft);
    }

    function setReferPowerRate(uint256 index, uint256 rate)
        external
        onlyOperator
    {
        _referPowerRates[index] = rate;
    }

    function setHalveTimes(uint256 halveTimes) external onlyOperator {
        require(halveTimes > 0, "Must be greater than zero");
        _halveTimes = halveTimes;
    }

    function emergencyWithdraw(address token) public onlyOperator {
        IERC20(token).transfer(
            _msgSender(),
            IERC20(token).balanceOf(address(this))
        );
    }

    function update() public {
        if (_stakedTotalPower == 0) {
            _lastRewardTime = block.timestamp;
            return;
        }

        (uint256 rewards, uint256 referenceReward) = getRewards(
            block.timestamp - _lastRewardTime
        );
        if (rewards > referenceReward) {
            uint256 diff = rewards - referenceReward;
            if (_reserve >= diff) {
                _reserve -= diff;
            } else if (_reserve > 0) {
                rewards = referenceReward + _reserve;
                _reserve = 0;
            } else {
                rewards = referenceReward;
            }
        } else if (rewards < referenceReward) {
            _reserve += referenceReward - rewards;
        }

        _accOpcPerShare += (rewards * 1e12) / _stakedTotalPower;
        _lastRewardTime = block.timestamp;
    }

    function stake(
        uint256 tokenId,
        address referrer,
        bytes memory signature
    ) external {
        address sender = _msgSender();
        require(
            _checkSignature(abi.encodePacked(sender, referrer), signature),
            "Invalid signature"
        );
        update();
        if (_referrers[sender] != address(0) && referrer != address(0)) {
            _referrers[sender] = referrer;
            _referrals[referrer].push(sender);
        }

        uint256 stakedPower = _stakedPowers[sender];
        if (stakedPower > 0) {
            uint256 pending = (stakedPower * _accOpcPerShare) /
                1e12 -
                _debts[sender];
            _opc.transfer(sender, pending);
            _withdrews[sender] += pending;
            if (referrer != address(0))
                _referrerRewards[referrer] += _getReferrerRewards(
                    referrer,
                    pending
                );
        }

        uint256 power;
        uint256 numericTrait = _opnft.randomNums(tokenId);
        uint256 slotId = LibHelper.getRoleTrait(numericTrait) - 1;
        uint256 prevTokenId = _slots[sender][slotId];
        if (prevTokenId > 0) {
            power = getPower(prevTokenId);
            if (unionType(prevTokenId) > 0) {
                power += getUnionPower(sender);
            }
            stakedPower -= power;
            _stakedTotalPower -= power;
            _opnft.transferFrom(address(this), sender, prevTokenId);
        }

        // need use below
        _slots[sender][slotId] = tokenId;

        power = _getPower(LibHelper.getQualityTrait(numericTrait));
        if (unionType(prevTokenId) > 0) {
            power += getUnionPower(sender);
        }

        stakedPower += power;
        _stakedTotalPower += power;

        _debts[sender] = (_accOpcPerShare * stakedPower) / 1e12;
        _stakedPowers[sender] = stakedPower;

        // set token into slot
        _opnft.transferFrom(sender, address(this), tokenId);
        emit PowerChanged(
            sender,
            _stakedTotalPower,
            stakedPower,
            block.timestamp
        );
    }

    function unstake(uint256 roleId) external {
        address sender = _msgSender();
        update();
        uint256 stakedPower = _stakedPowers[sender];
        uint256 pending = (stakedPower * _accOpcPerShare) /
            1e12 -
            _debts[sender];
        _opc.transfer(sender, pending);
        _withdrews[sender] += pending;
        if (_referrers[sender] != address(0))
            _referrerRewards[_referrers[sender]] += _getReferrerRewards(
                _referrers[sender],
                pending
            );
        if (roleId > 0) {
            roleId = roleId - 1;
            uint256 prevTokenId = _slots[sender][roleId];
            if (prevTokenId > 0) {
                uint256 power = getPower(prevTokenId);
                if (unionType(prevTokenId) > 0) {
                    power += getUnionPower(sender);
                }
                stakedPower -= power;
                _stakedTotalPower -= power;
                _opnft.transferFrom(address(this), sender, prevTokenId);
                _slots[sender][roleId] = 0;

                emit PowerChanged(
                    sender,
                    _stakedTotalPower,
                    stakedPower,
                    block.timestamp
                );
            }
        }

        _debts[sender] = (_accOpcPerShare * stakedPower) / 1e12;
        _stakedPowers[sender] = stakedPower;
    }

    function withdrawReferRewards() external {
        address sender = _msgSender();
        uint256 amount = _referrerRewards[sender];
        _referrerRewards[sender] = 0;
        _withdrewReferRewards[sender] += amount;
        _opc.transfer(sender, amount);
    }

    function pendingReferRewards(address account)
        external
        view
        returns (uint256 pending, uint256 withdrew)
    {
        pending = _referrerRewards[account];
        withdrew = _withdrewReferRewards[account];
    }

    function withdrawableReferRewards(address account)
        external
        view
        returns (uint256)
    {
        return _referrerRewards[account];
    }

    function getPowers(address account)
        public
        view
        returns (
            uint256 stakedPower,
            uint256 uinonPower,
            uint256 referPower
        )
    {
        uinonPower = getUnionPower(account);
        stakedPower = _stakedPowers[account] - uinonPower;
        referPower = getReferPower(account);
    }

    function getReferPower(address account)
        public
        view
        returns (uint256 power)
    {
        uint256 rate = _getReferPowerRate(account);
        if (rate == 0) {
            return 0;
        }
        for (uint256 index = 0; index < _referrals[account].length; index++) {
            power += (_stakedPowers[_referrals[account][index]] * rate) / 100;
        }
    }

    function getUnionPower(address account) public view returns (uint256) {
        return getUnionPower(_slots[account]);
    }

    function getUnionPower(uint256[SLOT_NUM] memory slotTokenIds)
        internal
        view
        returns (uint256 power)
    {
        /**
            1, 2, 10, 16, 20 // LR Alliance +50%
            28, 36, 37  // FE  Alliance +30%
         */
        if (
            slotTokenIds[0] > 0 &&
            slotTokenIds[1] > 0 &&
            slotTokenIds[9] > 0 &&
            slotTokenIds[15] > 0 &&
            slotTokenIds[19] > 0
        ) {
            power =
                (getPower(slotTokenIds[0]) +
                    getPower(slotTokenIds[1]) +
                    getPower(slotTokenIds[9]) +
                    getPower(slotTokenIds[15]) +
                    getPower(slotTokenIds[19])) /
                2;
        }
        if (
            slotTokenIds[27] > 0 && slotTokenIds[35] > 0 && slotTokenIds[36] > 0
        ) {
            power +=
                ((getPower(slotTokenIds[27]) +
                    getPower(slotTokenIds[35]) +
                    getPower(slotTokenIds[36])) * 3) /
                10;
        }
    }

    function unionType(uint256 tokenId) internal view returns (uint256) {
        uint256 role = LibHelper.getRoleTrait(_opnft.randomNums(tokenId));
        if (role == 1 || role == 2 || role == 10 || role == 16 || role == 20) {
            return 1;
        } else if (role == 28 || role == 36 || role == 37) {
            return 2;
        } else {
            return 0;
        }
    }

    function pendingOpc(address account)
        external
        view
        returns (uint256 pending, uint256 withdrew)
    {
        uint256 accOpcPerShare = _accOpcPerShare;
        if (block.timestamp > _lastRewardTime && _stakedTotalPower != 0) {
            (uint256 rewards, uint256 referenceReward) = getRewards(
                block.timestamp - _lastRewardTime
            );
            if (
                rewards > referenceReward &&
                _reserve < rewards - referenceReward
            ) {
                rewards = referenceReward + _reserve;
            }
            accOpcPerShare += (rewards * 1e12) / _stakedTotalPower;
        }
        pending =
            (_stakedPowers[account] * accOpcPerShare) /
            1e12 -
            _debts[account];
        withdrew = _withdrews[account];
    }

    function slotsInfo(address owner)
        external
        view
        returns (
            SlotItem[SLOT_NUM] memory slots,
            SlotItem[5] memory lrAlliance,
            SlotItem[3] memory feAlliance,
            uint256 stakedPower,
            uint256 uinonPower,
            uint256 referPower
        )
    {
        for (uint32 index = 1; index <= SLOT_NUM; index++) {
            slots[index - 1] = _getSlotItem(owner, index);
        }

        lrAlliance[0] = _getSlotItem(owner, 1);
        lrAlliance[1] = _getSlotItem(owner, 2);
        lrAlliance[2] = _getSlotItem(owner, 10);
        lrAlliance[3] = _getSlotItem(owner, 16);
        lrAlliance[4] = _getSlotItem(owner, 20);

        feAlliance[0] = _getSlotItem(owner, 28);
        feAlliance[1] = _getSlotItem(owner, 36);
        feAlliance[2] = _getSlotItem(owner, 37);

        (stakedPower, uinonPower, referPower) = getPowers(owner);
    }

    function getPower(uint256 tokenId) public view returns (uint256) {
        return _getPower(LibHelper.getQualityTrait(_opnft.randomNums(tokenId)));
    }

    function _getPower(uint256 quality) public view returns (uint256) {
        return _quality2Power[quality - 1];
    }

    // todo:
    function getRewards(uint256 time) internal view returns (uint256, uint256) {
        uint256 rewards = (time * _rewardRatePerPower * _stakedTotalPower) /
            _halveTimes;
        uint256 referenceReward = (time * _referenceRewardRate) / _halveTimes;
        return (rewards, referenceReward);
    }

    function _getSlotItem(address account, uint32 roleId)
        internal
        view
        returns (SlotItem memory)
    {
        uint128 tokenId = uint128(_slots[account][roleId - 1]);
        if (tokenId == 0) {
            return SlotItem(0, roleId, 0, 0);
        }
        uint16 quality = LibHelper.getQualityTrait(_opnft.randomNums(tokenId));
        return SlotItem(tokenId, roleId, quality, uint64(_getPower(quality)));
    }

    function _getReferrerRewards(address account, uint256 pending)
        internal
        view
        returns (uint256)
    {
        return (pending * _getReferPowerRate(account)) / 100;
    }

    function _getReferPowerRate(address account)
        internal
        view
        returns (uint256 rate)
    {
        if (_stakedPowers[account] > 200000) {
            return _referPowerRates[2];
        }
        if (_stakedPowers[account] > 20000) {
            return _referPowerRates[1];
        }
        if (_stakedPowers[account] > 2000) {
            return _referPowerRates[0];
        }
    }

    function _checkSignature(bytes memory packed, bytes memory signature)
        internal
        view
        returns (bool)
    {
        return
            operator(
                ECDSAUpgradeable.recover(
                    ECDSAUpgradeable.toEthSignedMessageHash(keccak256(packed)),
                    signature
                )
            );
    }
}

