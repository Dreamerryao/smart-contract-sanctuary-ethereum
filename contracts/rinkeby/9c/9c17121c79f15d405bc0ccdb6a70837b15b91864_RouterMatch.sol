// File: rupaydex.sol

contract RouterMatch is Ownable{
    IUniswapV2Router02[] public routers;

    function addRouter(IUniswapV2Router02 _router) external onlyOwner{
        routers.push(_router);
    }

    function removeRouter(IUniswapV2Router02 _router) external onlyOwner{
        for(uint i= 0; i< routers.length; i++){
            if(routers[i] == _router){
                delete routers[i];
            }
        }
    }

    function getMinPrice(address[] memory _path, uint _inputAmount, uint _outputAmount) public view returns(address[] memory, uint[] memory){
        address[] memory _addresses = new address[](routers.length);
        uint[] memory _amounts = new uint[](routers.length);
        for(uint i = 0; i < routers.length; i++){
            address pair = IUniswapV2Factory(routers[i].factory()).getPair(_path[_path.length - 1], _path[_path.length - 2]);
            if(pair != address(0)){
                if(_outputAmount == 0){
                    uint[] memory outAmount = routers[i].getAmountsOut(_inputAmount, _path);
                    _addresses[i] = address(routers[i]);
                    _amounts[i] = outAmount[outAmount.length-1];
                }else{
                    uint[] memory inAmount = routers[i].getAmountsIn(_outputAmount, _path);
                    _addresses[i] = address(routers[i]);
                    _amounts[i] = inAmount[inAmount.length - 1];
                }
            }
        }
        return (_addresses, _amounts);
    }

    function swap(IUniswapV2Router02 _router, address[] memory _path, uint256 _amountIn, uint _amountOut, uint256 _amountOutMin, uint256 _amountInMax) public {
        if(_amountIn > 0){
            IERC20(_path[0]).transferFrom(msg.sender, address(this), _amountIn);
            IERC20(_path[0]).approve(address(_router), _amountIn);
            _router.swapExactTokensForTokens(_amountIn, _amountOutMin, _path, msg.sender, block.timestamp+900);
        }else{
            IERC20(_path[0]).transferFrom(msg.sender, address(this), _amountInMax);
            IERC20(_path[0]).approve(address(_router), _amountInMax);
            _router.swapTokensForExactTokens(_amountOut, _amountInMax, _path, msg.sender,block.timestamp+900);
        }
    }

    function getrpice(IUniswapV2Router02 _router, uint _inputAmount, address[] memory _path) public view returns(uint){
        uint[] memory _amounts;
        _amounts = _router.getAmountsOut(_inputAmount, _path);
        return _amounts[_amounts.length-1];
        // _amounts[0] = outAmount[1];
    }
}