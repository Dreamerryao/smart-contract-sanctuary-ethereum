pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./IBot.sol";
import "./IBotMetadata.sol";
import "./Managable.sol";
import "./LibBot.sol";

import "hardhat/console.sol";

contract BotMinter is Managable {
    struct Contracts {
        address bot;
        address botMetadata;
        address oil;
        address bit;
        address oilTreasury;
        address bitTreasury;
    }
    Contracts public contracts;

    uint32[] public cooldowns = [
        uint32(1 minutes),
        uint32(2 minutes),
        uint32(5 minutes),
        uint32(10 minutes),
        uint32(30 minutes),
        uint32(1 hours),
        uint32(2 hours),
        uint32(4 hours),
        uint32(8 hours),
        uint32(16 hours),
        uint32(1 days),
        uint32(2 days),
        uint32(4 days),
        uint32(7 days)
    ];
    uint32 public bitsPrice;
    uint32 public oilPrice;
    uint32 public revealCooldown = uint32(5 days);

    // events
    event ModifiedCooldowns();
    event ModifiedContracts();
    event ModifiedBitPrice(uint32 _price);
    event ModifiedOilPrice(uint32 _price);
    event BotBreed(uint256 indexed _tokenId);

    constructor(
        uint32 _bitsPrice,
        uint32 _oilPrice,
        Contracts memory _contracts
    ) {
        bitsPrice = _bitsPrice;
        oilPrice = _oilPrice;
        contracts = _contracts;

        _addManager(msg.sender);
    }

    function setCooldowns(uint32[] calldata _cooldowns) external onlyManager {
        cooldowns = _cooldowns;
        emit ModifiedCooldowns();
    }

    function setContracts(Contracts calldata _contracts) external onlyManager {
        contracts = _contracts;
        emit ModifiedContracts();
    }

    function setBitPrice(uint32 _price) external onlyManager {
        bitsPrice = _price;
        emit ModifiedBitPrice(_price);
    }

    function setOilPrice(uint32 _price) external onlyManager {
        oilPrice = _price;
        emit ModifiedOilPrice(_price);
    }    
    
    function breed(uint256 _matronId, uint256 _sireId) external {
        address _sender = msg.sender;

        require(_bot().ownerOf(_matronId) == _sender, "you not owner of matron");
        require(_bot().ownerOf(_sireId) == _sender, "you not owner of sire");    

        LibBot.Bot memory matron = _botMetadata().getBot(_matronId);
        LibBot.Bot memory sire = _botMetadata().getBot(_sireId);

        require(_canBreed(matron), "matron can't breed");
        require(_canBreed(sire), "sire can't breed");
        require(_notCooldown(matron), "matron on cooldown");
        require(_notCooldown(sire), "sire on cooldown");
        require(_notRelatives(matron, sire), "bots are relatives");            

        // Transfering payments
        require(IERC20(contracts.bit).transferFrom(_sender, contracts.bitTreasury, bitsPrice), "bit transfer failure");
        require(IERC20(contracts.oil).transferFrom(_sender, contracts.oilTreasury, oilPrice), "oil transfer failure");

        // Creating a new bot
        uint256 tokenId = _bot().mint(_sender);
        LibBot.Bot memory bot;
        bot.id = tokenId;
        bot.generation = 1;
        bot.matronId = uint64(_matronId);
        bot.sireId = uint64(_sireId);
        bot.revealCooldown = block.timestamp + revealCooldown;
        _botMetadata().setBot(tokenId, bot);

        // Increasing breed amount for matron
        matron.breedCount += 1;
        matron.lastBreed = block.timestamp;
        _botMetadata().setBot(_matronId, matron);

        // Increasing breed amount for sire
        sire.breedCount += 1;
        sire.lastBreed = block.timestamp;
        _botMetadata().setBot(_sireId, sire);

        emit BotBreed(tokenId);
    }

    function mintGenesisTokenId(address _to, uint256 _tokenId, uint256 _dna) external onlyManager {
        LibBot.Bot memory bot;
        bot.genes = _dna;
        bot.generation = 0;
        bot.breedCount = 0;
        bot.birthTime = block.timestamp;
        bot.lastBreed = 0;
        bot.matronId = 0;
        bot.sireId = 0;
        bot.revealCooldown = 0;

        _bot().mintTokenId(_to, _tokenId);
        _botMetadata().setBot(_tokenId, bot);

        emit BotBreed(_tokenId);        
    }

    function mintGenesis(address _to, uint256 _dna) external onlyManager {
        LibBot.Bot memory bot;
        bot.genes = _dna;
        bot.generation = 0;
        bot.breedCount = 0;
        bot.birthTime = block.timestamp;
        bot.lastBreed = 0;
        bot.matronId = 0;
        bot.sireId = 0;
        bot.revealCooldown = 0;

        uint256 _tokenId = _bot().mint(_to);
        _botMetadata().setBot(_tokenId, bot);

        emit BotBreed(_tokenId);                
    }

    function canBreed(uint256 _matronId, uint256 _sireId) public view returns(bool) {
        if (_matronId == _sireId) {
            return false;
        }

        LibBot.Bot memory matron = _botMetadata().getBot(_matronId);
        LibBot.Bot memory sire = _botMetadata().getBot(_sireId);

        return _canBreed(matron) && _canBreed(sire) && _notCooldown(matron) && _notCooldown(sire) && _notRelatives(matron, sire);
    }

    function canBreedMultiple(uint256 _matronId, uint256[] calldata _siresIds) public view returns(bool[] memory) {
        bool[] memory _canBreedWith = new bool[](_siresIds.length);
        for(uint256 i = 0; i < _siresIds.length; i++) {
            _canBreedWith[i] = canBreed(_matronId, _siresIds[i]);
        }

        return _canBreedWith;
    }

    function _bot() private view returns(IBot) {
        return IBot(contracts.bot);
    }

    function _botMetadata() private view returns(IBotMetadata) {
        return IBotMetadata(contracts.botMetadata);
    }

    function _notCooldown(LibBot.Bot memory _b) private view returns(bool) {
        if (_b.breedCount == 0) {
            return true;
        }

        uint32 _cooldown = cooldowns[_b.breedCount];
        if (_cooldown == 0) {
            return true;
        }

        return _b.lastBreed + _cooldown < block.timestamp;
    }

    function _canBreed(LibBot.Bot memory _b) private pure returns(bool) {
        if (_b.generation == 0 && _b.breedCount <= 12) {
            return true;
        }

        return _b.breedCount <= 7;
    }

    function _notRelatives(LibBot.Bot memory _matron, LibBot.Bot memory _sire) private pure returns(bool) {
        // First generation don't have any limits
        if (_matron.generation == 0 || _sire.generation == 0) {
            return true;
        }
        
        // If they have same partens it's can't be done
        if (_matron.matronId == _sire.matronId && _matron.sireId == _sire.sireId ) {
            return false;
        }
        
        // You can't breed with you kids
        if (_matron.id == _sire.matronId || _matron.id == _sire.sireId) {
            return false;
        }

        if (_sire.id == _matron.matronId || _sire.id == _matron.sireId) {
            return false;
        }

        return true;
    }
}

