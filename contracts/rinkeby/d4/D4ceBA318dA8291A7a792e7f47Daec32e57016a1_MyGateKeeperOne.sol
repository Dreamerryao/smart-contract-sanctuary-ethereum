pragma solidity ^0.6.0;

import "SafeMath.sol";

contract MyGateKeeperOne {
    using SafeMath for uint256;
    address public entrant;
    address payable owner;
    event passGateOne(address);
    event passGateTwo(address);
    event finallyPassGateThree(address);
    event passGateThreeStepOne(address);
    event passGateThreeStepTwo(address);
    event remainingGas(uint256);

    constructor() public {
        owner = payable(msg.sender);
    }

    modifier gateOne() {
        require(msg.sender != tx.origin, "Gate one fails");
        emit passGateOne(address(this));
        _;
    }

    modifier gateTwo() {
        uint256 remaining_gas = gasleft();
        emit remainingGas(remaining_gas);
        require(gasleft().mod(8191) == 0, "Gate two fails");
        emit passGateTwo(address(this));
        _;
    }

    modifier gateThree(bytes8 _gateKey) {
        require(
            uint32(uint64(_gateKey)) == uint16(uint64(_gateKey)),
            "GatekeeperOne: invalid gateThree part one"
        );
        emit passGateThreeStepOne(address(this));
        require(
            uint32(uint64(_gateKey)) != uint64(_gateKey),
            "GatekeeperOne: invalid gateThree part two"
        );
        emit passGateThreeStepTwo(address(this));
        require(
            uint32(uint64(_gateKey)) == uint16(tx.origin),
            "GatekeeperOne: invalid gateThree part three"
        );
        emit finallyPassGateThree(address(this));
        _;
    }

    //
    //gateOne
    //gateTwo
    function enter(bytes8 _gateKey)
        public
        gateTwo
        gateThree(_gateKey)
        returns (bool)
    {
        entrant = tx.origin;
        return true;
    }

    function kill() public {
        selfdestruct(owner);
    }
}

