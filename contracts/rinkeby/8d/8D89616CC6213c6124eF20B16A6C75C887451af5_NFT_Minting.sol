contract NFT_Minting is ERC1155 {
     string public name;
     
     struct details{
         uint256 maxNft;
         uint256 currentMintedNft;
     }
     
     mapping(uint256 => details) public collections;
     mapping(address => bool) public minters;
   
    constructor(string memory _name) {
        name = _name;
    }
    
      function setMinters(address _minter,bool value) public onlyOwner {
        minters[_minter] = value;
    }
    
    
    function mint(
        address to,
        uint256 id,
        uint256 value,
        string memory _tokenURI,
        string memory data,
        uint256 collectionId
    ) public {
        require(collections[collectionId].maxNft != 0,"collectionId is not created");
        require(collections[collectionId].currentMintedNft < collections[collectionId].maxNft,"collection limit Exceeds,cannot mint your NFT");
        require(minters[msg.sender],"you are not the minter" );
        _mint(to, id, value, _tokenURI, data);
        collections[collectionId].currentMintedNft++;
    }
    
    function mintwithID(
        address to,
        uint256 id,
        uint256 value,
        string memory _tokenURI,
        string memory data,
        uint256 collectionId,
        uint256 maxNFTToBeMinted
    ) public {
        require(collections[collectionId].maxNft == 0, "collectionId already created");
        require(minters[msg.sender],"you are not the minter" );
        collections[collectionId].maxNft = maxNFTToBeMinted;
        collections[collectionId].currentMintedNft =  0;
        _mint(to, id, value, _tokenURI, data);
        collections[collectionId].currentMintedNft++;
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory values,
        string memory data
    ) public {
        _mintBatch(to, ids, values, data);
    }

    function burn(
        address owner,
        uint256 id,
        uint256 value
    ) public {
        _burn(owner, id, value);
    }

    function burnBatch(
        address owner,
        uint256[] memory ids,
        uint256[] memory values
    ) public {
        _burnBatch(owner, ids, values);
    }
}