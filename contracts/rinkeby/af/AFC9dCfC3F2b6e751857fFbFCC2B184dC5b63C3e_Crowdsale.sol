    /**
     * @notice Calculates the number of tokens to purchase.
     * @dev Override to extend the way in which ether is converted to tokens.
     * @param _amount Value in wei or token to be converted into tokens.
     * @return tokenAmount Number of tokens that can be purchased with the specified amount.
     */
     /**
      * @notice 구매할 토큰 수를 계산합니다.
      * @dev 이더가 토큰으로 변환되는 방식을 확장하기 위해 재정의합니다.
      * @param _amount 토큰으로 변환할 wei 또는 토큰의 값입니다.
      * @return tokenAmount 지정된 금액으로 구매할 수 있는 토큰의 수입니다.
      */
    function _getTokenAmount(address _paymentToken, uint256 _amount) internal view returns (uint256) {
    // 클레이일경우 해당 함수 사용해서 비율구하기
    // kip7의경우 추가 필요.
    require(finalized(),"It cannot be used until the crowdsale is complete");
        if(_paymentToken == paymentCurrencyList.kethAddress) {
            return _amount.mul(10**18).div(uint256(rate)).mul(KethRate);
        } else if(_paymentToken == paymentCurrencyList.korcAddress) {
            return _amount.mul(10**18).div(uint256(rate)).mul(KorcRate);
        } else if(_paymentToken == paymentCurrencyList.kspAddress) {
            return _amount.mul(10**18).div(uint256(rate)).mul(KspRate);
        }
    }

    /**
     * @notice Checks if the sale is open.
     * @return isOpen True if the crowdsale is open, false otherwise.
     */
    function isOpen() public view returns (bool) {
        return block.timestamp >= uint256(marketInfo.startTime) && block.timestamp <= uint256(marketInfo.endTime);
    }

    /**
     * @notice Checks if the sale has ended.
     * @return auctionEnded True if sold out or time has ended.
     */
    function auctionEnded() public view returns (bool) {
        return block.timestamp > uint256(marketInfo.endTime);
    }

    /**
     * @notice Checks if the sale has been finalised.
     * @return bool True if sale has been finalised.
     */
    function finalized() public view returns (bool) {
        return marketStatus.finalized;
    }

    function finalize() external {
        require(marketInfo.endTime >= block.timestamp);
        require(hasAdminRole(msg.sender));
        marketStatus.finalized = true;
        _safeTransfer(paymentCurrencyList.kethAddress, wallet, uint256(marketStatus.kethCommitmentsTotal));
        _safeTransfer(paymentCurrencyList.korcAddress, wallet, uint256(marketStatus.korcCommitmentsTotal));
        _safeTransfer(paymentCurrencyList.kspAddress, wallet, uint256(marketStatus.kspCommitmentsTotal));
        _safeTokenPayment(ETH_ADDRESS, wallet, uint256(marketStatus.klayCommitmentsTotal));

        

    }


    //--------------------------------------------------------
    // Setter Functions
    //--------------------------------------------------------

    /**
     * @notice Admin can set start and end time through this function.
     * @param _startTime Auction start time.
     * @param _endTime Auction end time.
     */
    function setAuctionTime(uint256 _startTime, uint256 _endTime) external {
        require(hasAdminRole(msg.sender));
        require(_startTime < 10000000000, "Crowdsale: enter an unix timestamp in seconds, not miliseconds");
        require(_endTime < 10000000000, "Crowdsale: enter an unix timestamp in seconds, not miliseconds");
        require(_startTime >= block.timestamp, "Crowdsale: start time is before current time");
        require(_endTime > _startTime, "Crowdsale: end time must be older than start price");

        marketInfo.startTime = BoringMath.to64(_startTime);
        marketInfo.endTime = BoringMath.to64(_endTime);
        
        emit AuctionTimeUpdated(_startTime,_endTime);
    }

    /**
     * @notice Admin can set auction price through this function.
     * @param _rate Price per token.
     */
    function setAuctionPrice(uint256 _rate, uint256 _KethKlayRate, uint256 _KorcKlayRate, uint256 _KspKlayRate) external {
        require(hasAdminRole(msg.sender));
        require(_rate > 0, "Crowdsale: rate is 0");

        rate = BoringMath.to128(_rate);
        KethRate = rate.mul(_KethKlayRate);
        KorcRate = rate.mul(_KorcKlayRate);
        KspRate = rate.mul(_KspKlayRate);

        marketStatus.commitmentsTotal = BoringMath.to128(uint256(marketStatus.commitmentsTotal
        .add(marketStatus.kethCommitmentsTotal.mul(_KethKlayRate))
        .add(marketStatus.korcCommitmentsTotal.mul(_KorcKlayRate))
        .add(marketStatus.kspCommitmentsTotal.mul(_KspKlayRate))
        .add(marketStatus.klayCommitmentsTotal)));

        emit AuctionPriceUpdated(rate, KethRate, KorcRate, KspRate);
    }

    function setNeuronPerShare(uint256 _accNeuronPerShare) external {
        require(hasAdminRole(msg.sender));
        accNeuronPerShare = _accNeuronPerShare;
