    /**
     * @notice Updates commitment of the buyer and the amount raised, emits an event.
     * @param _addr Recipient of the token purchase.
     * @param _commitment Value in wei or token involved in the purchase.
     */
    //  /**
    //   * @notice 구매자의 약정과 모금액을 업데이트하고 이벤트를 발생시킵니다.
    //   * @param _addr 토큰 구매 수신자.
    //   * @param _commitment 구매와 관련된 wei 또는 토큰의 값입니다.
    //   */
    function _addCommitment(address _paymentToken, address _addr, uint256 _commitment) internal {
        require(block.timestamp >= uint256(marketInfo.startTime) && block.timestamp <= uint256(marketInfo.endTime), "Crowdsale: outside auction hours");
        require(_addr != address(0), "Crowdsale: beneficiary is the zero address");

        // uint256 newCommitment = commitments[_addr].add(_commitment);
        // paymentCurrency[0] == keth paymentCurrency[1] == korc  paymentCurrency[2] == ksp  ETH_ADDRESS == KLAY
         if(_paymentToken == paymentCurrencyList.kethAddress) {
            kethCommitments[_addr] = _commitment;
            marketStatus.kethCommitmentsTotal = BoringMath.to128(uint256(marketStatus.kethCommitmentsTotal).add(_commitment));
        } else if(_paymentToken == paymentCurrencyList.korcAddress) {
            korcCommitments[_addr] = _commitment;
            marketStatus.korcCommitmentsTotal = BoringMath.to128(uint256(marketStatus.korcCommitmentsTotal).add(_commitment));
        } else if(_paymentToken == paymentCurrencyList.kspAddress) {
            kspCommitments[_addr] = _commitment;
            marketStatus.kspCommitmentsTotal = BoringMath.to128(uint256(marketStatus.kspCommitmentsTotal).add(_commitment));
        } else if(_paymentToken == ETH_ADDRESS) {
            klayCommitments[_addr] = _commitment;
            marketStatus.klayCommitmentsTotal = BoringMath.to128(uint256(marketStatus.klayCommitmentsTotal).add(_commitment));
        }
      
        /// @dev Update state.
        emit AddedCommitment(_addr, _paymentToken, _commitment);

    }
    function claim(address _user) external {
        require(block.timestamp >= marketInfo.endTime);
        uint256 claimAmount = tokensClaimable(_user);
        _safeTransfer(auctionToken,_user,claimAmount);
        
        if(endTimeClaimCheck) {
            endTimeClaimCheck = false;
        }        
        lastRewardBlock = block.number;
        claimed[_user] = claimed[_user].add(claimAmount);
    }
    /**
     * @notice Adjusts users commitment depending on amount already claimed and unclaimed tokens left.
     * @return claimerCommitment How many tokens the user is able to claim.
     */
    //  /**
    //   * @notice 이미 청구된 토큰과 청구되지 않은 남은 토큰의 양에 따라 사용자 약정을 조정합니다.
    //   * @return claimerCommitment 사용자가 청구할 수 있는 토큰 수입니다.
    //   */
    function tokensClaimable(address _user) public view returns (uint256 claimerCommitment) {
        // 현재 컨트랙트에 남아있는 토큰으 갯수
        uint256 unclaimedTokens = IERC20(auctionToken).balanceOf(address(this));
        uint256 kethKlayAmount = kethCommitments[_user].mul(KethRate);
        uint256 korcKlayAmount = korcCommitments[_user].mul(KorcRate);
        uint256 kspKlayAmount = kspCommitments[_user].mul(KspRate);
        // 유저가 받을 토큰의 비율.
        uint256 userClaimerCommitmentPercent = (kethKlayAmount.add(korcKlayAmount).add(kspKlayAmount)
                            .add(klayCommitments[_user])).div(marketStatus.commitmentsTotal).mul(100);

        // 유저가 받게될 총 토큰의 수량
        uint256 userClaimerCommitmentTotal = userClaimerCommitmentPercent.mul(marketInfo.totalTokens);

        // endTime 20% 보상
        uint256 endTimeClaimerCommitment =  userClaimerCommitmentTotal.mul(20).div(100);

        uint256 userClaimerCommitment;
        //블록당 보상 
        if(block.number > lastRewardBlock && unclaimedTokens != 0){
            uint256 blocks = block.number.sub(lastRewardBlock);
            userClaimerCommitment = blocks.mul(accNeuronPerShare).mul(userClaimerCommitmentPercent);
        }

        if(endTimeClaimCheck == false ) {
            claimerCommitment = userClaimerCommitment.add(endTimeClaimerCommitment);
        } else {
            claimerCommitment = userClaimerCommitment;
        }                         
        
        if(claimerCommitment > unclaimedTokens){
            claimerCommitment = unclaimedTokens;
        }
    }

