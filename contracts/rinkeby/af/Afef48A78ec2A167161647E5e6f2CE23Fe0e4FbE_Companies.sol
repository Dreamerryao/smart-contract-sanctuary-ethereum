import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./Random.sol";
import "hardhat/console.sol";

// "special" companies are for a set of Regular IDs that share a trait, like McD's workers
// "not-special" companies get assigned to regular IDs randomly.
// edge-case ... we don't mint out, create new companies, users continue to mint

contract Companies is AccessControl {
    using Random for Random.Manifest;
    using EnumerableSet for EnumerableSet.UintSet;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    struct Company {     
        uint baseSalary;  
        uint limit;
        uint startIndex;                     
        string name;
    }

    Company[] companies;                // Companies stored by company ID
    EnumerableSet.UintSet[] jobIds;     // jobIds ??

    mapping(uint => uint[]) private specials;
    mapping(uint => Random.Manifest) private specialDecks;
    Random.Manifest private mainDeck;

    event jobIDCreated (uint256 regularId, uint newJobId, uint companyId, address sender);
    
	constructor() {
	    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
	    _grantRole(MINTER_ROLE, msg.sender);
        uint startIndex = 0;
        // companies for 'special' regulars MUST come after the regular regulars
        addCompany(99, 10, startIndex, "Dollar Store");
        addCompany(1, 10, startIndex += companies[numCompanies() - 1].limit, "McDanny's");
        addCompany(2, 10, startIndex += companies[numCompanies() - 1].limit, "Whoppy Jr.");
        addCompany(3, 10, startIndex += companies[numCompanies() - 1].limit, "WGMI Inc.");
        addCompany(4, 10, startIndex += companies[numCompanies() - 1].limit, "Global International");
        addCompany(5, 10, startIndex += companies[numCompanies() - 1].limit, "N.E.X.T. Rugs");
        addCompany(6, 10, startIndex += companies[numCompanies() - 1].limit, "Alpha Limited");
        addCompany(7, 10, startIndex += companies[numCompanies() - 1].limit, "G.M.");
        addCompany(8, 10, startIndex += companies[numCompanies() - 1].limit, "Partners & Partners");
        addCompany(9, 10, startIndex += companies[numCompanies() - 1].limit, "Boss e-Systems");
        addCompany(10, 10, startIndex += companies[numCompanies() - 1].limit, "Block Busters");
        addCompany(11, 10, startIndex += companies[numCompanies() - 1].limit, "Hexagon Research Group");
        addCompany(12, 32, startIndex += companies[numCompanies() - 1].limit, "Crabby Shack"); // special
        addCompany(13, 55, startIndex += companies[numCompanies() - 1].limit, "Wenders"); // special

        // SPECIAL SETS OF REGULAR IDs

        // Devils
        specials[12] = [4466, 4684, 5342, 5437, 5932, 6838, 8043, 1175, 1274, 2005, 2497, 2592, 3063, 3285, 3300, 3316, 3454, 3983, 4541, 4856, 5171, 5219, 5265, 6643, 6719, 6982, 7147, 7303, 8012, 8944, 9644, 9822];
        // Clowns
        specials[13] = [379, 391, 874, 1004, 1245, 1258, 1398, 1584, 1869, 1940, 1952, 2269, 2525, 2772, 3055, 3455, 3472, 3541, 3544, 3607, 3617, 4103, 4117, 4149, 4195, 4230, 4425, 5065, 5101, 5188, 5494, 5845, 6016, 6042, 6073, 6109, 6436, 6649, 7092, 7574, 7863, 8077, 8110, 8326, 8359, 8480, 8629, 8825, 9303, 9319, 9339, 9770, 9800, 9858, 9870];
        // McD's Workers + Managers
        specials[14] = [1617, 1808, 2149, 2632, 2833, 2847, 3301, 3524, 4822, 5139, 5735, 5906, 5946, 6451, 6663, 6762, 6831, 7278, 7519, 8365, 9434, 64, 488, 642, 946, 1014, 1650, 1823, 1949, 2178, 2593, 2992, 3070, 3331, 3745, 3944, 3961, 4030, 4070, 4090, 4197, 4244, 4719, 5551, 5761, 5779, 5895, 6044, 6048, 6276, 6599, 6681, 6832, 6873, 6889, 7124, 7550, 7975, 8130, 8579, 8599, 8689, 8784, 8794, 8903, 9053, 9205, 9254, 9407, 9994];
        // Wendy's
        specials[15] = [317, 456, 878, 1588, 2702, 2974, 3047, 3224, 3308, 3441, 4082, 4107, 5490, 5574, 5622, 6232, 6317, 6350, 6404, 6539, 7654, 7947, 7961, 8248, 8400, 8437, 8643, 8667, 8728, 9221, 9611, 9709, 9754, 9950];
        // Makeup
        specials[16] = [4, 27, 48, 101, 136, 143, 157, 165, 172, 175, 226, 277, 388, 389, 418, 420, 444, 457, 493, 516, 518, 610, 638, 679, 681, 703, 743, 784, 867, 917, 959, 1013, 1032, 1042, 1084, 1127, 1142, 1196, 1234, 1279, 1295, 1296, 1297, 1310, 1323, 1356, 1390, 1405, 1440, 1482, 1566, 1596, 1598, 1660, 1663, 1695, 1700, 1708, 1905, 1929, 1986, 2018, 2026, 2037, 2067, 2097, 2125, 2148, 2176, 2207, 2247, 2262, 2347, 2494, 2516, 2543, 2549, 2603, 2612, 2622, 2662, 2731, 2741, 2776, 2788, 2809, 2965, 3004, 3013, 3014, 3031, 3035, 3044, 3112, 3113, 3144, 3236, 3287, 3338, 3385, 3519, 3545, 3548, 3647, 3800, 3896, 3985, 4156, 4165, 4203, 4215, 4243, 4273, 4294, 4328, 4365, 4402, 4463, 4587, 4691, 4799, 4814, 4920, 4923, 4926, 4934, 4947, 4981, 5018, 5049, 5104, 5110, 5131, 5135, 5152, 5313, 5317, 5333, 5376, 5379, 5386, 5423, 5426, 5510, 5608, 5638, 5663, 5667, 5758, 5780, 5787, 5792, 5838, 5883, 5934, 5977, 6068, 6081, 6099, 6120, 6150, 6191, 6213, 6243, 6251, 6257, 6373, 6385, 6409, 6418, 6445, 6454, 6481, 6519, 6549, 6563, 6581, 6606, 6619, 6622, 6718, 6742, 6808, 6868, 6906, 6920, 7028, 7111, 7131, 7156, 7171, 7212, 7223, 7253, 7269, 7305, 7326, 7346, 7410, 7414, 7434, 7465, 7470, 7537, 7593, 7683, 7689, 7697, 7725, 7824, 7838, 7847, 7885, 7952, 8014, 8056, 8064, 8102, 8124, 8151, 8153, 8161, 8243, 8372, 8390, 8391, 8424, 8435, 8455, 8467, 8468, 8472, 8481, 8493, 8555, 8609, 8642, 8659, 8692, 8768, 8781, 8818, 8822, 8899, 8930, 8940, 8950, 8975, 9011, 9055, 9060, 9106, 9129, 9192, 9231, 9244, 9275, 9324, 9413, 9455, 9638, 9689, 9970, 9977];

        // create new decks for each set of special regulars
        uint notSpecialCount = 0;
        for (uint i = 0; i < numCompanies(); i++) {
            if (specials[i].length > 0){
                specialDecks[i].setup(specials[i].length);
                // console.log("Special deck created for CompanyId = ", i, ", length = ", specials[i].length);
            } else {
                notSpecialCount += companies[i].limit;
            }
        }
        // setup the main deck for the remaining non-special regulars
        mainDeck.setup(notSpecialCount);
        console.log("Instantiating Companies.");
        console.log("Maximum Job Ids : ", maxJobIds());
        console.log("Maximum NOT SPECIAL Job Ids : ", notSpecialCount);
	}

// ADMIN FUNCTIONS

    // Companies are added in constructor
    function addCompany(uint _baseSalary, uint _limit, uint _startIndex, string memory _name) public onlyRole(MINTER_ROLE) {
        require(_baseSalary > 0, "baseSalary must be > 0");
        Company memory _company = Company({
            baseSalary : _baseSalary,
            limit : _limit,
            startIndex : _startIndex,
            name : _name
        });
        companies.push(_company);
        jobIds.push({});
    }

// Pull a new id for the JOB NFT based on ID from their REG NFT
    function makeJobId(uint _regularId) public returns (uint,uint) { // onlyRole(MINTER_ROLE) returns (uint,uint) {
        uint _pull;
        uint _offset;
        uint _newJobId;
        // Pull random card from appropriate deck
        if (!isSpecial(_regularId)) { // If Regular id is NOT special
            _pull = mainDeck.draw();
            _offset == 0; 
        } else { // If Regular id IS special
            _pull = specialDecks[getSpecialCompanyId(_regularId)].draw();
            _offset = companies[getSpecialCompanyId(_regularId)].startIndex;
        }
        _newJobId = _pull + _offset;

        // Add jobId to company
        uint i;
        for (i = 0; i < numCompanies() -1; i++) {
            if (_newJobId >= companies[i].startIndex && _newJobId < companies[i+1].startIndex){
                jobIds[i].add(_newJobId);
                break;
            }
        }
        console.log("Adding jobId",_newJobId, "in company", i);
        emit jobIDCreated(_regularId, _newJobId, i, msg.sender);
        return (_newJobId,i); // return jobId + companyId
    }

//

    function updateCompany(uint _companyId, string memory _name, uint _baseSalary) public onlyRole(MINTER_ROLE)  {
        companies[_companyId].name = _name;
        companies[_companyId].baseSalary = _baseSalary;
    }

    function transferJobToCompany(uint _tokenId, uint _newCompanyId ) public onlyRole(MINTER_ROLE) {
        uint _oldCompany = getCompanyByJobId(_tokenId);
        jobIds[_oldCompany].remove(_tokenId);
        jobIds[_newCompanyId].add(_tokenId);
    }

// View Functions

    function numCompanies() public view returns (uint) {
        return companies.length;
    }

    function getCompanyByJobId(uint _tokenId) public view returns (uint) {
        for (uint i = 0; i < numCompanies(); i++) {
            if (jobIds[i].contains(_tokenId))
                return i;
        }
        revert("not found");
    }

    function getjobIdsOfCompany(uint _companyId) public view returns (uint[] memory) {
        return jobIds[_companyId].values();
    }

    function companyMemberCount(uint _companyId) public view returns (uint) {
        return jobIds[_companyId].length();
    }

    function maxJobIds() public view returns (uint) {
        uint _total = 0;
        for (uint i = 0; i < numCompanies(); i++) {
            _total += companies[i].limit;
        }
        return _total;
    }

    function specialCountByCompany(uint _companyId) public view returns (uint) {
        return specials[_companyId].length;
    }

// External 

    function generateSalary(uint _companyId) external view returns (uint) {
        uint _baseSalary = companies[_companyId].baseSalary;
        return (_baseSalary * 2) + ( uint(random()) % _baseSalary );
    }

// Internal

    // check if a regular ID is special
    function isSpecial(uint _regTokenId) internal view returns (bool) {
        for (uint i = 0; i < numCompanies(); i++) {
            for (uint s = 0; s < specials[i].length;s++){
                if (specials[i][s] == _regTokenId)
                    return true;
            }
        }
        return false;
    }

    function getSpecialCompanyId(uint _tokenId) internal view returns (uint) {
        for (uint i = 0; i < numCompanies(); i++) {
            for (uint s = 0; s < specials[i].length;s++){
                if (specials[i][s] == _tokenId)
                    return i;
            }
        }
        revert("TokenId not special");
    }

    function getStartIndex(uint _companyId) internal view returns (uint) {
        return companies[_companyId].startIndex;
    }

    function random() internal view returns (bytes32) {
        // change to block.number - 1 before deploy
        return keccak256(abi.encodePacked(blockhash(block.number - 1), block.timestamp, msg.sender)) ; 
    }

}

