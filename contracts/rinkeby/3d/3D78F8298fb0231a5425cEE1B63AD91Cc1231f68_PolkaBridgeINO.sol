pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract PolkaBridgeNFT is ERC721URIStorage, Ownable {

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    constructor()
        ERC721("PolkaBridge NFT", "PBR NFT")
    {
        //
    }

    function mintNFT(address recipient_, string memory tokenURI_)
        public
        onlyOwner
        returns (uint256)
    {
        _tokenIds.increment();

        uint256 newItemId = _tokenIds.current();
        _mint(recipient_, newItemId);
        _setTokenURI(newItemId, tokenURI_);

        return newItemId;
    }

    function multiMintNFT(address recipient_, string memory tokenURI_, uint256 amount_)
        public
        onlyOwner
        returns (uint256[] memory)
    {
        uint256[] memory mintedIds;
        for (uint256 i=0; i<amount_; i++)
        {
            _tokenIds.increment();

            uint256 newItemId = _tokenIds.current();
            _mint(recipient_, newItemId);
            _setTokenURI(newItemId, tokenURI_);
            mintedIds[i] = newItemId;
        }
        return mintedIds;
    }
}

