        for (uint256 i=0; i<pools[pid].TotalPackageCount; i++)
        {
            uint256 packageId = poollist[pid][i].Id;
            whitelist[pid][packageId][user].Id = pid;
            whitelist[pid][packageId][user].UserAddress = user;
            whitelist[pid][packageId][user].IsWhitelist = true;
            whitelist[pid][packageId][user].WhitelistDate = block.timestamp;
            whitelist[pid][packageId][user].IsActived = true;
        }
    }
    
    // Add Multiple Whitelist function 
    function addMulWhitelist(address[] memory user, uint256 pid)
        public onlyOwner        
    {
        for (uint256 i = 0; i < user.length; i++) {
            for (uint256 j=0; j<pools[pid].TotalPackageCount; j++) {
                uint256 packageId = poollist[pid][j].Id;
                whitelist[pid][packageId][user[i]].Id = pid;
                whitelist[pid][packageId][user[i]].UserAddress = user[i];
                whitelist[pid][packageId][user[i]].IsWhitelist = true;
                whitelist[pid][packageId][user[i]].WhitelistDate = block.timestamp;
                whitelist[pid][packageId][user[i]].IsActived = true;
            }
        }
    }
