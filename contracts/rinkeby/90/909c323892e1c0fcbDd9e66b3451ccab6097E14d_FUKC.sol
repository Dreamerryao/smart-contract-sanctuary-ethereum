    uint256 myShare = 11;
    uint256 totalRebBalance = 100;
    uint256 totalLordBalance = 90;

    string[][][] proposal;

    constructor(uint256 _verifyNum) ERC20("FukcingToken", "FUKC") {
        
    }    

    function testMath(uint256 _number) view public returns(uint256){
