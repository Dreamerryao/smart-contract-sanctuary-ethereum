     * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, ``from``'s `tokenId` will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}
}

library SafeMath {

    function add(uint256 a, uint256 b) internal pure returns (uint256) {
        uint256 c = a + b;
        require(c >= a, "SafeMath: addition overflow");

        return c;
    }

    function sub(uint256 a, uint256 b) internal pure returns (uint256) {
        return sub(a, b, "SafeMath: subtraction overflow");
    }

    function sub(uint256 a, uint256 b, string memory errorMessage) internal pure returns (uint256) {
        require(b <= a, errorMessage);
        uint256 c = a - b;

        return c;
    }

    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a == 0) {
            return 0;
        }

        uint256 c = a * b;
        require(c / a == b, "SafeMath: multiplication overflow");

        return c;
    }


    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        return div(a, b, "SafeMath: division by zero");
    }

    function div(uint256 a, uint256 b, string memory errorMessage) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        uint256 c = a / b;
        // assert(a == b * c + a % b); // There is no case in which this doesn't hold

        return c;
    }

    function mod(uint256 a, uint256 b) internal pure returns (uint256) {
        return mod(a, b, "SafeMath: modulo by zero");
    }

    function mod(uint256 a, uint256 b, string memory errorMessage) internal pure returns (uint256) {
        require(b != 0, errorMessage);
        return a % b;
    }
}


contract Ownable is Context {
    address private _owner;
    address private _creator;
    address private _previousOwner;
    uint256 private _lockTime;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    constructor () internal {
        address msgSender = _msgSender();
        _owner = msgSender;
        _creator = msgSender;
        emit OwnershipTransferred(address(0), msgSender);
    }

    function owner() public view returns (address) {
        return _owner;
    }

    function creator() public view returns (address) {
        return _creator;
    }

    modifier onlyOwner() {
        require(_owner == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    modifier onlyCreator() {
        require(_creator == _msgSender(), "Ownable: caller is not the creator");
        _;
    }
    
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }

    function getUnlockTime() public view returns (uint256) {
        return _lockTime;
    }
    
    function getTime() public view returns (uint256) {
        return now;
    }

    function lock(uint256 time) public virtual onlyOwner {
        _previousOwner = _owner;
        _owner = address(0);
        _lockTime = now + time;
        emit OwnershipTransferred(_owner, address(0));
    }
    
    function unlock() public virtual {
        require(_previousOwner == msg.sender, "You don't have permission to unlock");
        require(now > _lockTime , "Contract is locked until 7 days");
        emit OwnershipTransferred(_owner, _previousOwner);
        _owner = _previousOwner;
    }
}

contract invertedNFT is ERC721, Ownable {
    using SafeMath for uint256;

    /// @notice Emitted when an account buy NFTs
    event BuyItem(address buyer, uint256 id);

    /// @notice Emitted when an admin updates price
    event NewPrice(uint256 oldPrice, uint256 newPrice);

    /// @notice Emitted when an admin updates first presale price
    event NewPresalePrice1(uint256 oldPrice, uint256 newPrice);

    /// @notice Emitted when an admin updates second presale price
    event NewPresalePrice2(uint256 oldPrice, uint256 newPrice);

    /// @notice Emitted when an admin updates third presale price
    event NewPresalePrice3(uint256 oldPrice, uint256 newPrice);

    /// @notice Emitted when an admin updates fourth presale price
    event NewPresalePrice4(uint256 oldPrice, uint256 newPrice);

    /// @notice Emitted when an admin updates uri
    event NewURI(string oldURI, string newURI);

    /// @notice Emitted when an admin withdraws asset
    event WithdrawBalance(uint256 balance, address to);    
    
    uint256 public maxIDs = 10000;
    uint256 public totalSold = 0;

    uint256 public presalePrice1 = 0.5 ether;
    uint256 public presalePrice2 = 1 ether;
    uint256 public presalePrice3 = 1.5 ether;
    uint256 public presalePrice4 = 2 ether;

    uint256 public price = 2.5 ether;

    string public uri;

    mapping (uint256 => bool) public nftUnavailable;
    mapping (address => uint256[]) public nftOwned;
 
    constructor(string memory _uri) public ERC721("TEST", "TEST") {
        uri = _uri;
    }

    receive() external payable {}

     /**
     * @dev Allows user to buy an NFT on the Grid.
     */
    function buy(uint256 _id) internal returns (bool) {
        // Make sure ID is valid
        require(_id > 0 && _id <= 10000, "buy:: NFT ID is invalid.");
        // Make sure NFT is still available
        require(nftUnavailable[_id] == false, "buy::NFT is already purchased.");

        address recipient = msg.sender;

        // Changed this to be safeMint from Mint
        _safeMint(recipient, _id);
      //  _setTokenURI(_id, uri);

        nftUnavailable[_id] = true;
        totalSold = totalSold.add(1);

        nftOwned[recipient].push(_id);

        emit BuyItem(msg.sender, _id);

        return true;
    }

    /** 
    * @dev Allows user to buy more than one NFT at a time 
    */

    function buyMultiple(uint256[] memory _ids) external 
        payable returns (bool) {
        require (_ids.length > 0, "buyMultiple:: There are no ids in the array");

        uint256 currentPrice = 0;

        if (totalSold < 250) {
            currentPrice = presalePrice1;
        } else if (totalSold >= 250 && totalSold < 500) {
            currentPrice = presalePrice2;
        } else if (totalSold >= 500 && totalSold < 750) {
            currentPrice = presalePrice3;
        } else if (totalSold >= 750 && totalSold < 1000) {
            currentPrice = presalePrice4;
        } else {
            currentPrice = price;
        }

        // Make sure buyer is paying right price
        require(msg.value == currentPrice.mul(_ids.length), 
            "buyMultiple:: The price paid is wrong");
       
        for (uint256 i = 0; i < _ids.length; i++) {
            buy(_ids[i]);
        }

        return true;
    }

     /**
     * @dev Allows owner to change token URI.
     */
    function setUri(string memory _uri) external onlyOwner {

        emit NewURI(uri, _uri);
        uri = _uri;
    }

    function setPrice (uint256 _price) external onlyOwner {

        emit NewPrice(price, _price);
        price = _price;
    }

    function setPresalePrice1 (uint256 _presalePrice1) external onlyOwner {
        
        emit NewPresalePrice1(presalePrice1, _presalePrice1);
        presalePrice1 = _presalePrice1;
    }

    function setPresalePrice2 (uint256 _presalePrice2) external onlyOwner {
        
        emit NewPresalePrice2(presalePrice2, _presalePrice2);
        presalePrice2 = _presalePrice2;
    }

    function setPresalePrice3 (uint256 _presalePrice3) external onlyOwner {
    
        emit NewPresalePrice3(presalePrice3, _presalePrice3);
        presalePrice3 = _presalePrice3;
    }

    function setPresalePrice4 (uint256 _presalePrice4) external onlyOwner {
        
        emit NewPresalePrice4(presalePrice4, _presalePrice4);
        presalePrice4 = _presalePrice4;
    }

    function withdrawFunds() external onlyOwner {
        require(address(this).balance > 0, 
            "withdrawFunds:: There aren't any funds to remove.");

        emit WithdrawBalance(address(this).balance, owner());
        payable(owner()).transfer(address(this).balance);
    }
}