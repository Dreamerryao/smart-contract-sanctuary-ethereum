pragma solidity ^0.8.4;

import "./JobTransferFunction.sol";
import "./Companies.sol";
import "./FinanceDept.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
// import "@openzeppelin/contracts/utils/Strings.sol";

contract Jobs is ERC721, ERC721Enumerable, AccessControl {
    using Counters for Counters.Counter;
    using Strings for uint256;

    struct Job {
        uint lastClaimed;                               // This will be used for ERC20 dispersal hh 
        uint companyID;
        uint baseSalary;
        uint regID;                                     // each job has either a Regular ID or 10,001 (none assigned)
        uint unclaimedSalary;
    }

    JobTransferFunction jobTransferFunction;
    Companies companies;
    FinanceDept financeDept;
    ERC721Enumerable regularsNFT;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    Counters.Counter private _tokenIdCounter;           // Job NFT Ids do not correspond to Regular IDs
    bool mintOpen = true;                               // Is minting?
    mapping(uint => Job) jobs;                          // Job Id > Jobs  
    mapping(uint => bool) public claimed;               // Regular IDs that have already claimed
    mapping(uint => bool) public hasJob;                // true if RegID has a job
    mapping(uint => bool) public isReset;               // Job has no reg if unassigned == true
    string private baseURI = "https://jobs.regular.world/cards/id/"; 

    event Mint(uint jobId, uint indexed companyId, uint baseSalary, uint regularId);
    event Update(uint jobId, uint indexed companyId, uint baseSalary, uint regularId, string name);
    event RegularIDChange (uint256 indexed jobId, uint regID);

    constructor() ERC721("Regular Jobs", "JOBS") {
        console.log("Instantiating Jobs2.");
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        regularsNFT = ERC721Enumerable(0x6d0de90CDc47047982238fcF69944555D27Ecb25);
        jobTransferFunction = new JobTransferFunction();
        financeDept = new FinanceDept(address(this));
        companies = new Companies();
    }

// Primary Functions

    // edge case -- what if someone mints a job to a reg that is already assigned?

    function safeMint(uint _regId) public {  // update with "to" param
        address _to = msg.sender; 
        // console.log(_regId, "RegID => safeMint()");
        // require(regularsNFT.ownerOf(_regId) == _to);
        require(!claimed[_regId], "Already claimed");
        require(mintOpen, "Not minting");
        require(!hasJob[_regId],"Reg is working another job");
        // Mark this Reg ID as claimed
        claimed[_regId] = true;
        // Create a Job ID, Company ID + base salary
        (uint256 _tokenId, uint _companyId) = companies.makeJobId(_regId);
        uint _baseSalary = companies.generateSalary(_companyId);
        console.log("Base Salary:" , _baseSalary);
        jobs[_tokenId] = Job({
            lastClaimed : block.timestamp, 
            companyID : _companyId,
            baseSalary : _baseSalary,
            regID : _regId,
            unclaimedSalary : 0
        });
        hasJob[_regId] = true;
        _safeMint(_to, _tokenId);
        emit Mint(_tokenId, _companyId, _baseSalary, _regId);
    }

    function setRegularId(uint _jobId, uint _regID) public {
        require(ownerOf(_jobId) == msg.sender, "Not the owner of this job.");
        require(regularsNFT.ownerOf(_regID) == msg.sender, "Not the owner of Regular");
        require(jobs[_jobId].regID != _regID, "This Regular already assigned to this job.");
        require(!hasJob[_regID] == false, "Reg already assigned to another job.");
        uint _oldRegID = jobs[_jobId].regID;
        hasJob[_oldRegID] = false; 
        hasJob[_regID] = true;
        jobs[_jobId].regID = _regID;                     
        isReset[_jobId] = false;                    
        emit RegularIDChange(_jobId, _regID);
    }

    function safeMintMany(uint[] memory _regIDs) public { // update with "to" param
        for (uint i; i< _regIDs.length;i++){
            safeMint(_regIDs[i]);
        }
    }
// Private

    function resetJob(uint _jobId) private {
        isReset[_jobId] = true; 
        uint _oldRegID = jobs[_jobId].regID;
        hasJob[_oldRegID] = false;
    }

// Admin Functions

    function toggleMinting() public onlyRole(MINTER_ROLE) {
        mintOpen = !mintOpen;
    }

    function setJobTransferContract(address _addr) public onlyRole(MINTER_ROLE){
        jobTransferFunction = JobTransferFunction(_addr);
    }

    function setCompaniesContract(address _addr) public onlyRole(MINTER_ROLE){
        companies = Companies(_addr);
    }

    function setFinanceDeptContract(address _addr) public onlyRole(MINTER_ROLE){
        financeDept = FinanceDept(_addr);
    }

    function setBaseURI(string memory _newPath) public onlyRole(MINTER_ROLE) {
        baseURI = _newPath;
    }

    function transferJobToCompany(uint _tokenId, uint _newCompanyId ) public onlyRole(MINTER_ROLE) {
        jobs[_tokenId].companyID = _newCompanyId;
        companies.transferJobToCompany(_tokenId,_newCompanyId); // TO-DO: REVIEW THIS
    }

// Overrides

    function tokenURI(uint256 tokenId) public view override(ERC721) returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : "";
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        resetJob(tokenId);  // when jobs are transferred, unassign their regular
        jobTransferFunction.jobTransfer(from,to,tokenId); 
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

// Proxy Methods

    function unclaimedRegularsByAddress(address _wallet) public view returns(uint[] memory){
        uint[] memory nfts = new uint[](regularsNFT.balanceOf(_wallet));
        for (uint i = 0; i < nfts.length;i++){
            uint _regId = regularsNFT.tokenOfOwnerByIndex(_wallet, i);
            if (!claimed[_regId])
                nfts[i] = _regId;
        }
        return nfts;
    }

    function allRegularsByAddress(address _wallet) public view returns(uint[] memory){
        uint[] memory nfts = new uint[](regularsNFT.balanceOf(_wallet));
        for (uint i = 0; i < nfts.length;i++){
            nfts[i] = regularsNFT.tokenOfOwnerByIndex(_wallet, i);
        }
        return nfts;
    }

}

