pragma solidity 0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./interfaces/ILandworks.sol";
import "./BaseRentAdapter.sol";
import {ITribeOne} from "../interfaces/ITribeOne.sol";
import {DataTypes} from "../libraries/DataTypes.sol";
import {LibLandworks} from "./libraries/LibLandworks.sol";

import "hardhat/console.sol";

contract LandworksAdapter is BaseRentAdapter {
    enum RentStatus {
        NONE,
        RENTAL,
        DELISTED,
        WITHDRAWN
    }

    address public immutable LAND_WORKS_ADDRESS;
    // loanId => assetId
    mapping(uint256 => uint256) public rentAssetIdMap;
    mapping(uint256 => RentStatus) public rentStatusMap;

    constructor(address _LAND_WORKS_ADDRESS, address _TRIBE_ONE_ADDRESS) BaseRentAdapter(_TRIBE_ONE_ADDRESS) {
        LAND_WORKS_ADDRESS = _LAND_WORKS_ADDRESS;
    }

    function listNFTforRenting(
        uint256 _metaverseId,
        uint256 _minPeriod,
        uint256 _maxPeriod,
        uint256 _maxFutureTime,
        address _paymentToken,
        uint256 _pricePerSecond,
        uint256 _loanId
    ) external onlyTribeOne {
        // Validate Listing
        DataTypes.Loan memory _loan = ITribeOne(TRIBE_ONE_ADDRESS).getLoans(_loanId);
        // Loan should be active
        require(_loan.status == DataTypes.Status.LOANACTIVED, "Rent: Loan is not active now");

        ITribeOne(TRIBE_ONE_ADDRESS).listNFTToRent(_loanId);
        IERC721(_loan.nftItem.nftAddress).safeTransferFrom(TRIBE_ONE_ADDRESS, address(this), _loan.nftItem.nftId);

        IERC721(_loan.nftItem.nftAddress).approve(LAND_WORKS_ADDRESS, _loan.nftItem.nftId);

        // call list function in Landworks smart contract
        uint256 assetId = ILandworks(LAND_WORKS_ADDRESS).list(
            _metaverseId,
            _loan.nftItem.nftAddress, // _metaverseRegistry
            _loan.nftItem.nftId, // _metaverseAssetId
            _minPeriod,
            _maxPeriod,
            _maxFutureTime,
            _paymentToken,
            _pricePerSecond
        );

        // set cosumer with loan borrower address
        ILandworks(LAND_WORKS_ADDRESS).changeConsumer(_loan.borrower, assetId);

        rentAssetIdMap[_loanId] = assetId;
        rentStatusMap[_loanId] = RentStatus.RENTAL;
    }

    function delist(uint256 _loanId) external onlyTribeOne {
        _delist(_loanId);
    }

    function _delist(uint256 _loanId) private {
        DataTypes.Loan memory _loan = ITribeOne(TRIBE_ONE_ADDRESS).getLoans(_loanId);
        uint256 assetId = rentAssetIdMap[_loanId];

        ILandworks(LAND_WORKS_ADDRESS).delist(assetId);

        if (IERC721(_loan.nftItem.nftAddress).ownerOf(_loan.nftItem.nftId) == address(this)) {
            // NFT was withdrawn
            IERC721(_loan.nftItem.nftAddress).safeTransferFrom(address(this), TRIBE_ONE_ADDRESS, _loan.nftItem.nftId);
            ITribeOne(TRIBE_ONE_ADDRESS).removeFromRent(_loanId);
            rentStatusMap[_loanId] = RentStatus.WITHDRAWN;
        } else {
            // remain NFT in Landworks because someone is leasing it now
            rentStatusMap[_loanId] = RentStatus.DELISTED;
        }
    }

    function withdraw(uint256 _loanId) external onlyTribeOne {
        _withdraw(_loanId);
    }

    function _withdraw(uint256 _loanId) private {
        uint256 assetId = rentAssetIdMap[_loanId];

        ILandworks(LAND_WORKS_ADDRESS).withdraw(assetId);

        require(ILandworks(LAND_WORKS_ADDRESS).ownerOf(assetId) == address(0), "LandworksAdapter: Withdraw was failed");
        {
            DataTypes.Loan memory _loan = ITribeOne(TRIBE_ONE_ADDRESS).getLoans(_loanId);
            IERC721(_loan.nftItem.nftAddress).safeTransferFrom(address(this), TRIBE_ONE_ADDRESS, _loan.nftItem.nftId);
        }

        ITribeOne(TRIBE_ONE_ADDRESS).removeFromRent(_loanId);

        rentStatusMap[_loanId] = RentStatus.WITHDRAWN;
    }

    function withdrawNFTFromRent(uint256 loanId) external override onlyTribeOne {
        if (rentStatusMap[loanId] == RentStatus.RENTAL) {
            _delist(loanId);
        } else if (rentStatusMap[loanId] == RentStatus.DELISTED) {
            _withdraw(loanId);
        }
    }
}

