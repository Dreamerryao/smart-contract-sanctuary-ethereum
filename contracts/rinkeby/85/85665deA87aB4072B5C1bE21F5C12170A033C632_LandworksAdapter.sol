        uint256 _loanId,
        address _agent,
        bool _accepted
    ) external payable;

    function getLoans(uint256 _loanId) external view returns (DataTypes.Loan memory);

    function getLoanRent(uint256 _loanId) external view returns (address);

    function listNFTToRent(uint256 _loanId) external;

    function removeFromRent(uint256 _loanId) external;
}

