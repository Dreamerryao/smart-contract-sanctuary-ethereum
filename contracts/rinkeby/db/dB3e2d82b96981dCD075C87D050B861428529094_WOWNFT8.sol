pragma solidity ^0.8.2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./interfaces/IUniswapV2Pair.sol";

contract WOWNFT8 is ERC721, ERC721Burnable, Ownable {
    using SafeMath for uint256;
    event MintOfKind(
        address to,
        uint256 token,
        uint256 tokenGlobal,
        uint256 kind
    );
    event MintGlobal(address to, uint256 token, uint256 kind);

    struct Kind {
        bool    isBox;
        uint256 next;
        uint256 limit;
        uint256 minted;
        uint256 price;
    }

    address public wow; // wow ,token address
    address payable public recipient; // 接受USDTaddress
    address public usdtAddress; // usdt-contract address
    uint256 public wizardOfWOWTokenAmount; //  zhuoshushi

    uint256 public paySwitch;
    address public bnb2usdt;
    address public wow2usdt;

    mapping(uint256 => Kind) public kinds;
    mapping(uint256 => uint256) public tokenToKind;
    mapping(address => bool) public miners;

    uint256 public blindBoxesNext;
    mapping(address => mapping(uint256 => bool)) public userBlindBoxes;

    constructor(
        address wow_,
        address usdtAddress_, // usdt token address
        address payable recipient_,
        uint256 blindBoxesNext_,
        uint256 wizardOfWOWTokenAmount_,
        uint256[] memory ids,
        uint256[] memory from,
        uint256[] memory limits,
        uint256[] memory prices
    ) ERC721("WOW-NFT", "WOW-NFT") {
        require(wow_ != address(0), "wow_ is 0");
        require(usdtAddress_ != address(0), "usdtAddress_ is 0");
        require(recipient_ != address(0), "recipient_ is 0");
        wow = wow_;
        usdtAddress = usdtAddress_;
        recipient = recipient_;
        wizardOfWOWTokenAmount = wizardOfWOWTokenAmount_;
        blindBoxesNext = blindBoxesNext_;
        for (uint256 i = 0; i < ids.length; i++) {
            require(prices[i] > 0, "price is 0");
            Kind storage kind = kinds[ids[i]];
            kind.next = from[i];
            kind.limit = limits[i];
            kind.price = prices[i];
        }
    }

    function setKind(
        bool isBox,
        uint256 id,
        uint256 from,
        uint256 limit,
        uint256 price
    ) public onlyOwner {
        require(price > 0 && id > 0, "id or price is 0");
        Kind storage kind = kinds[id];
        require(limit >= kind.minted, "limit < minted");
        if (kind.price == 0) {
            kind.next = from;
            kind.isBox = isBox;
        }
        kind.limit = limit;
        kind.price = price;
    }

    /**
     * call this method only if you know what you do!
     */

    function setKindNext(uint256 id, uint256 next) public onlyOwner {
        Kind storage kind = kinds[id];
        kind.next = next;
    }

    function setPaySwitch(uint256 _paySwitch) public onlyOwner {
        if (paySwitch == 1) {
            require(bnb2usdt != address(0), "bnb2usdt is 0");
        }
        if (paySwitch == 2) {
            require(wow2usdt != address(0), "wow2usdt is 0");
        }
        paySwitch = _paySwitch;
    }

    function setBNB2USDTPair(address _bnb2usdt, address _wow2usdt)
        public
        onlyOwner
    {
        bnb2usdt = _bnb2usdt;
        wow2usdt = _wow2usdt;
    }

    function setRecipient(address payable recipient_) public onlyOwner {
        recipient = recipient_;
    }

    function setMiner(address account, bool allow) public onlyOwner {
        miners[account] = allow;
    }

    function setUsdtAddress(address usdtAddress_) public onlyOwner {
        usdtAddress = usdtAddress_;
    }

    function setWizardOfWowTokenAmount(uint256 wizard_of_wow_token_amount_)
        public
        onlyOwner
    {
        wizardOfWOWTokenAmount = wizard_of_wow_token_amount_;
    }

    function _seekId(uint256 next) internal view returns (uint256) {
        for (uint256 i = 0; i < 100; i++) {
            if (tokenToKind[next] == 0) {
                break;
            }
            next += 1;
        }
        return next;
    }

    function mintmany(
        address to,
        uint256 kindID,
        uint256 amount
    ) public payable {
        require(amount > 0, "amount is 0");
        // 非 miner 挖 100类型的NFT，则需要支付USDT
        Kind memory kind = kinds[kindID];
        if (!_isOwnerOrMiners(msg.sender)) {
            if (paySwitch == 1) {
                require(
                    msg.value >=
                        _getAmount(
                            IUniswapV2Pair(bnb2usdt),
                            kind.price.mul(amount)
                        ),
                    "insufficient bnb"
                );
                recipient.transfer(msg.value);
            } else if (paySwitch == 2) {
                require(
                    IERC20(wow).transferFrom(
                        msg.sender,
                        recipient,
                        _getAmount(
                            IUniswapV2Pair(wow2usdt),
                            kind.price.mul(amount)
                        )
                    )
                );
            } else {
                require(
                    IERC20(usdtAddress).transferFrom(
                        msg.sender,
                        recipient,
                        kind.price.mul(amount)
                    )
                );
            }
        }
        if (kind.isBox) {
            _mintBlindBox(to, kindID, amount);
        } else {
            _mint(to, kindID, amount, 0);
        }
    }

    // 挖靓号
    function mintBeauty(
        address to,
        uint256 kindID,
        uint256 id
    ) public onlyOwner {
        Kind storage kind = kinds[kindID];
        require(kind.minted < kind.limit, "limit reached");
        if (kind.next > 0) {
            require(
                id >= kind.next && id < kind.next - kind.minted + kind.limit,
                "id out of range"
            );
        }
        require(tokenToKind[id] == 0, "id exists");
        kind.minted = kind.minted.add(1);

        _safeMint(to, id);
        emit MintOfKind(to, id, kindID, 0);

        tokenToKind[id] = kindID;
    }

    // 开盲盒
    function openBlindBox(
        address to,
        uint256 id,
        uint256 toKind,
        uint256 deadline,
        bytes memory signature
    ) public {
        require(deadline >= block.timestamp, "expired");
        _checkSignature(abi.encodePacked(to, id, toKind, deadline), signature);

        require(userBlindBoxes[msg.sender][id], "caller is not owner");
        userBlindBoxes[msg.sender][id] = false;
        _mint(to, toKind, 1, id);
    }

    function _mint(
        address to,
        uint256 kindID,
        uint256 amount,
        uint256 globalID
    ) private {
        Kind storage kind = kinds[kindID];
        if (kindID == 101) {
            // 100NFT与101NFT按照1:200限量铸造
            require(kind.minted < kinds[100].minted.mul(200), "limit reached");
        } else {
            require(kind.minted < kind.limit, "limit reached");
        }

        uint256 next = kind.next;
        for (uint256 i = 0; i < amount; i++) {
            next = _seekId(next);
            _safeMint(to, next);
            emit MintOfKind(to, next, kindID, globalID); // id 为0，买的NFT，id 不为0 ，开盲盒出来的NFT

            tokenToKind[next] = kindID;
            next = next.add(1);
        }
        kind.next = next;
        kind.minted = kind.minted.add(amount);

        if (kindID == 101) {
            // 祭司长NFT的用户，可获附赠1000 WOW/祭司长NFT
            if (!_isOwnerOrMiners(to)) {
                require(
                    IERC20(wow).transfer(to, wizardOfWOWTokenAmount.mul(amount))
                );
            }
        }
    }

    function _mintBlindBox(
        address to,
        uint256 kindID,
        uint256 amount
    ) private {
        Kind storage kind = kinds[kindID];
        uint256 minted = kind.minted.add(amount);
        require(minted <= kind.limit, "limit reached");
        kind.minted = minted;
        for (uint256 i = 0; i < amount; i++) {
            userBlindBoxes[to][blindBoxesNext] = true;
            emit MintGlobal(to, blindBoxesNext, kindID);
            blindBoxesNext = blindBoxesNext.add(1);
        }
    }

    function _checkSignature(bytes memory packed, bytes memory signature)
        internal
        view
    {
        // this recreates the message that was signed on the client
        require(
            ECDSA.recover(
                ECDSA.toEthSignedMessageHash(keccak256(packed)),
                signature
            ) == owner(),
            "signature invalid"
        );
    }

    function _isOwnerOrMiners(address user) private view returns (bool) {
        return user == owner() || miners[user];
    }

    function _getAmount(IUniswapV2Pair pair, uint256 _amount)
        internal
        view
        returns (uint256 amount_)
    {
        (uint256 _reserve0, uint256 _reserve1, ) = pair.getReserves();
        if (pair.token0() == usdtAddress) {
            amount_ = _reserve1.mul(_amount).div(_reserve0);
        } else {
            amount_ = _reserve0.mul(_amount).div(_reserve1);
        }
    }
}

