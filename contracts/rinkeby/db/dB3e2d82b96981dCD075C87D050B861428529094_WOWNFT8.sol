    function token0() external view returns (address);

    function token1() external view returns (address);

    function getReserves()
        external
        view
        returns (
            uint112 reserve0,
            uint112 reserve1,
            uint32 blockTimestampLast
        );
}

interface IWOWNFT {
    function getPrice(uint256) external view returns (uint256);

    function mintmany(
        address to,
        uint256 kindID,
        uint256 amount
    ) external;
}

contract WOWMiner is Ownable {
    using SafeMath for uint256;

    address public usdt; // usdt-contract address
    address public wow;
    address public eth2usdt;
    address public wow2usdt;
    IWOWNFT public wowNFT;

    address payable private _recipient; // 接受USDTaddress

    constructor(
        address wow_,
        address usdtAddress_, // usdt token address
        address payable recipient_,
        IWOWNFT wowNFT_
    ) {
        require(wow_ != address(0), "wow_ cannot be zero address");
        require(
            usdtAddress_ != address(0),
            "usdtAddress_ cannot be zero address"
        );
        require(recipient_ != address(0), "recipient_ cannot be zero address");
        wow = wow_;
        usdt = usdtAddress_;
        _recipient = recipient_;
        wowNFT = wowNFT_;
    }

    function setETH2USDT(address eth2usdt_) public onlyOwner {
        require(eth2usdt_ != address(0), "zero address");
        eth2usdt = eth2usdt_;
    }

    function setWOW2USDT(address wow2usdt_) public onlyOwner {
        require(wow2usdt_ != address(0), "zero address");
        wow2usdt = wow2usdt_;
    }

    function setRecipient(address payable recipient_) public onlyOwner {
        require(recipient_ != address(0), "cannot be zero address");
        _recipient = recipient_;
    }

    function getRecipient() public view returns (address) {
        return _recipient;
    }

    function setUsdtAddress(address usdtAddress_) public onlyOwner {
        usdt = usdtAddress_;
    }

    function getUsdtAddress() public view returns (address) {
        return usdt;
    }

    function mintByBNB(
        address to,
        uint256 kindID,
        uint256 amount
    ) public payable {
        require(amount > 0, "amount must larger than 0");
        uint256 price = wowNFT.getPrice(kindID);
        uint256 amountBNB = _getAmount(
            IUniswapV2Pair(eth2usdt),
            price.mul(amount)
        );
        require(msg.value >= amountBNB, "insufficient eth");
        _recipient.transfer(msg.value);
        wowNFT.mintmany(to, kindID, amount);
    }

    function mintByWOW(
        address to,
        uint256 kindID,
        uint256 amount
    ) public {
        require(amount > 0, "amount must larger than 0");
        uint256 price = wowNFT.getPrice(kindID);
        uint256 amountWOW = _getAmount(
            IUniswapV2Pair(wow2usdt),
            price.mul(amount)
        );
        require(IERC20(wow).transferFrom(msg.sender, _recipient, amountWOW));
        wowNFT.mintmany(to, kindID, amount);
    }

    function getAmountBNB(uint256 id, uint256 amount)
        public
        view
        returns (uint256 amountBNB)
    {
        uint256 price = wowNFT.getPrice(id);
        amountBNB = _getAmount(IUniswapV2Pair(eth2usdt), price.mul(amount));
    }

    function getAmountWOW(uint256 id, uint256 amount)
        public
        view
        returns (uint256 amountWOW)
    {
        uint256 price = wowNFT.getPrice(id);
        amountWOW = _getAmount(IUniswapV2Pair(wow2usdt), price.mul(amount));
    }

    function _getAmount(IUniswapV2Pair pair, uint256 _amount)
        private
        view
        returns (uint256 amount_)
    {
        (uint256 _reserve0, uint256 _reserve1, ) = pair.getReserves();
        if (pair.token0() == usdt) {
            amount_ = _reserve1.mul(_amount).div(_reserve0);
        } else {
            amount_ = _reserve0.mul(_amount).div(_reserve1);
        }
    }
}

