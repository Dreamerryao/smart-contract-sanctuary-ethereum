import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract WOWTOKEN is ERC20, ERC20Burnable, Ownable {
    using SafeMath for uint256;
    mapping(address => bool) private _miners;

    constructor() ERC20("WOW", "WOW") {
        //zhengshiu
        _mint(0xd811e77Be6607aa12B5DC889f54C09b8bf940eCB, 1000_0000 * 1e18);
        //测试服
        _mint(0xE169F325d8B21Dac48218C7580ACDA6F34171Ff4, 1000_0000 * 1e18);
    }

    function decimals() public pure override returns (uint8) {
        return 18;
    }

    function mint(address to, uint256 amount) public {
        require(
            msg.sender == owner() || _miners[msg.sender],
            "caller must be a miner"
        );
        require(
            totalSupply().add(amount) <= 1_0000_0000 * 1e18,
            "exceeding maximum supply"
        );
        _mint(to, amount);
    }

    function setMiner(address account, bool allow) public onlyOwner {
        _miners[account] = allow;
    }

    function getMiner(address account) public view returns (bool) {
        return _miners[account];
    }
}

