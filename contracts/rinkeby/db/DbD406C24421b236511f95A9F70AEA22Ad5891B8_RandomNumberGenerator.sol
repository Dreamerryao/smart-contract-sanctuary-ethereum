import "./RandomNumberGeneratorInterface.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/SafeCast.sol";
import "@chainlink/contracts/src/v0.6/VRFConsumerBase.sol";
import "hardhat/console.sol";

/**
 * @title Random Number Generator
 * @author Keyko
 *
 * @dev Contract in charge of storing and managing the random number requests provided by the Chainlink Defender
 *
 */
contract RandomNumberGenerator is
    RandomNumberGeneratorInterface,
    VRFConsumerBase,
    AccessControl,
    Ownable
{
    bytes32 public constant BACKEND_ADMIN_ROLE =
        keccak256("BACKEND_ADMIN_ROLE");

    // using SafeMath for uint256;
    using SafeCast for uint256;

    event KeyHashSet(bytes32 keyHash);
    event FeeSet(uint256 fee);
    event VrfCoordinatorSet(address indexed vrfCoordinator);
    event VRFRequested(bytes32 indexed requestId);

    /**
     * @dev The keyhash used by the Chainlink VRF
     */
    bytes32 public keyHash;

    /**
     * @dev The request fee of the Chainlink VRF
     */
    uint256 public fee;

    /**
     * @dev address of Chainlink Defender
     */
    address private backendAddress;

    /**
     * @dev A list of blocks to be locked at based on past requests mapped by party id
     */
    mapping(uint256 => uint32) internal requestLockBlock;

    /**
     * @dev A mapping from Chainlink request id to id of the party for which the request has been made
     */
    mapping(bytes32 => uint256) internal chainlinkRequestPartyId;

    /**
     * @dev A mapping from the party id to the latest random number obtained for this party
     */
    mapping(uint256 => uint256) internal randomNumbers;

    /**
     * @dev A mapping from party id to a boolean representing if a reuqest is ongoing for that party
     */
    mapping(uint256 => bool) internal requestOngoing;

    /**
     * @dev Public constructor
     * @param _vrfCoordinator address of the Chainlink VRF Coordinator
     * @param _link LINK token address on the corresponding network (Ethereum, Polygon, BSC, etc)
     * @param _backendAddress address of Chainlink Defende
     */
    constructor(
        address _vrfCoordinator,
        address _link,
        address _backendAddress
    ) public VRFConsumerBase(_vrfCoordinator, _link) {
        backendAddress = _backendAddress;
        _setupRole(BACKEND_ADMIN_ROLE, _backendAddress);
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        emit VrfCoordinatorSet(_vrfCoordinator);
    }

    /**
     * @notice Returns the address of LINK Token used by Random Number Generator
     */
    function getLink() external view returns (address) {
        return address(LINK);
    }

    /**
     * @notice Allows owner to set the Chainlink VRF keyhash
     * @param _keyhash The keyhash to be used by the Chainlink VRF
     */
    function setKeyHash(bytes32 _keyhash) external onlyOwner {
        keyHash = _keyhash;

        emit KeyHashSet(keyHash);
    }

    /**
     * @notice Allows owner to set the fee per request required by the Chainlink VRF
     * @param _fee The fee to be charged for a request
     */
    function setFee(uint256 _fee) external onlyOwner {
        fee = _fee;

        emit FeeSet(fee);
    }

    /**
     * @notice Gets the Fee for making a Request against an RNG service
     * @return feeToken The address of the token that is used to pay fees
     * @return requestFee The fee required to be paid to make a request
     */
    function getRequestFee()
        external
        view
        override
        returns (address feeToken, uint256 requestFee)
    {
        return (address(LINK), fee);
    }

    /**
     * @notice Sends a request for a random number to the 3rd-party service
     * @dev Some services will complete the request immediately, others may have a time-delay
     * @dev Some services require payment in the form of a token, such as $LINK for Chainlink VRF
     * @param _partyId The ID of the party for which random number request has been made
     * @return lockBlock The block number at which the RNG service will start generating time-delayed 
     * randomness. The calling contract
     * should "lock" all activity until the result is available via the `requestId`
     */
    function requestRandomNumber(uint256 _partyId)
        external
        override
        returns (uint32 lockBlock)
    {
        require(
            hasRole(BACKEND_ADMIN_ROLE, _msgSender()),
            "This function can be called only by backend!"
        );
        // check if a request is already ongoing
        require(
            !requestOngoing[_partyId],
            "A request for this party is already ongoing!"
        );

        // collect fee for payment
        require(
            LINK.transferFrom(_msgSender(), address(this), fee),
            "Fee transfer failed"
        );

        lockBlock = uint32(block.number);
        requestLockBlock[_partyId] = lockBlock;

        // mark the request
        requestOngoing[_partyId] = true;

        // send request (costs fee)
        bytes32 requestId = _requestRandomness();

        chainlinkRequestPartyId[requestId] = _partyId;

        emit RandomNumberRequested(_partyId, requestId);
    }

    /**
     * @notice Checks if the request for randomness from the 3rd-party service has completed
     * @dev For time-delayed requests, this function is used to check/confirm completion
     * @param _partyId The ID of the party for which random number request has been made
     * @return isCompleted True if the request has completed and a random number is available, false otherwise
     */
    function isRequestComplete(uint256 _partyId)
        external
        view
        override
        returns (bool isCompleted)
    {
        return !requestOngoing[_partyId];
    }

    /**
     * @dev Returns the block number at which the RNG service will start generating time-delayed randomness
     * @param _partyId The ID of the party for which the 'lockBlock' is returned
     * @return lockBlock The block number at which the RNG service will start generating time-delayed randomness. 
     * The calling contract should "lock" all activity until the result is available via the `requestId`
     */
    function getLockBlock(uint256 _partyId)
        external
        view
        override
        returns (uint256 lockBlock)
    {
        return requestLockBlock[_partyId];
    }

    /**
     * @notice Gets the random number produced by the 3rd-party service
     * @param _partyId The ID of the party for which random number request has been made
     * @return randomNumber The random number
     */
    function getRandomNumber(uint256 _partyId)
        external
        view
        override
        returns (uint256 randomNumber)
    {
        return randomNumbers[_partyId];
    }

    /**
     * @notice used to change the address of the Vault contract
     * @param _backendAddress new address of the Chainlink Defender
     */
    function setBackendAddress(address _backendAddress) external onlyOwner {
        require(
            _backendAddress != address(0),
            "RNG: Backend contract address cannot be address 0!"
        );
        backendAddress = _backendAddress;
        grantRole(BACKEND_ADMIN_ROLE, backendAddress);
    }

    /**
     * @dev Requests a new random number from the Chainlink VRF
     * @dev The result of the request is returned in the function `fulfillRandomness`
     */
    function _requestRandomness() internal returns (bytes32 requestId) {
        // Complete request
        requestId = requestRandomness(keyHash, fee);
        emit VRFRequested(requestId);
    }

    /**
     * @notice Callback function used by VRF Coordinator
     * @dev The VRF Coordinator will only send this function verified responses.
     * @dev The VRF Coordinator will not pass randomness that could not be verified.
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        uint256 partyId = chainlinkRequestPartyId[requestId];

        // Store random value
        randomNumbers[partyId] = randomness;

        emit RandomNumberCompleted(partyId, requestId, randomness);
    }
}

