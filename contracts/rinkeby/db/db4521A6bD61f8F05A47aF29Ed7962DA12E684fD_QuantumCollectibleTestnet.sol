pragma solidity 0.8.9;









contract QuantumCollectibleTestnet is ERC1155, Ownable {
    using Strings for uint256;
    using Counters for Counters.Counter;
    AggregatorV3Interface internal priceFeed;

    uint16 constant ROYALTIES_BASIS = 10000;

    bool public metadataUpdatable;

    string public name;
    string public symbol;
    string public baseURI;

    address public royaltiesAddress;
    uint256 public royaltiesBasisPoints;

    mapping (uint256 => bool) public freezeTokenUris;
    mapping (uint256 => uint256) public tokenSupply;
    mapping(uint256 => string) private _tokenURIs;

    mapping(uint256 => uint16) private _tokenTiers;
    mapping(uint16 => uint256) private _tierPrices;
    mapping(uint256 => uint256) private _saleOrders;
    mapping(uint256 => address) private _saleAddresses;

    // Represents the number of decimals the prices contain.
    uint64 private decimals = 8;

    mapping(uint256 => uint256[]) private _tierToTokenIds;

    // NFT IDs tracket
    Counters.Counter private _tokenIds;

    event PermanentURI(string _value, uint256 indexed _id); // https://docs.opensea.io/docs/metadata-standards
    event PermanentURIGlobal();

    string constant DEFAULT_URI = "ipfs://";

    constructor(
        Config.Deployment memory deploymentConfig,
        Config.Runtime memory runtimeConfig
    ) ERC1155(DEFAULT_URI) {
        _validateDeploymentConfig(deploymentConfig);
        royaltiesAddress = runtimeConfig.royaltiesAddress;
        royaltiesBasisPoints = runtimeConfig.royaltiesBps;

        metadataUpdatable = runtimeConfig.metadataUpdatable;
        baseURI = runtimeConfig.baseURI;

        name = deploymentConfig.name;
        symbol = deploymentConfig.symbol;

        transferOwnership(deploymentConfig.owner);
        setApprovalForAll(deploymentConfig.eggAddress, true);

        // set price aggregator
        if(runtimeConfig.aggregatorAddress != address(0)) {
            priceFeed = AggregatorV3Interface(runtimeConfig.aggregatorAddress);
        }
    }

    function setURI(string memory _newURI) public onlyOwner {
        _setURI(_newURI);
    }

    function updateTokenUri(uint256 _tokenId, string memory _newUri, bool _isFreezeTokenUri)
    public
    onlyOwner {
        require(_exists(_tokenId), "Quantum NFT: update URI query for nonexistent token");
        require(metadataUpdatable, "Quantum NFT: Token uris are frozen gloonlyOwnertransfbally");
        require(freezeTokenUris[_tokenId] != true, "Quantum NFT: Token is frozen");
        require(_isFreezeTokenUri || (bytes(_newUri).length != 0), "Quantum NFT: Either _newUri or _isFreezeTokenUri=true required");

        if (bytes(_newUri).length != 0) {
            require(keccak256(bytes(_tokenURIs[_tokenId])) != keccak256(bytes(string(abi.encodePacked(_newUri)))), "Quantum NFT: New token URI is same as updated");
            _tokenURIs[_tokenId] = _newUri;
            emit URI(_newUri, _tokenId);
        }
        if (_isFreezeTokenUri) {
            freezeTokenUris[_tokenId] = true;
            emit PermanentURI(_tokenURIs[_tokenId], _tokenId);
        }
    }

    function burn(
        uint256 id,
        uint256 value
    ) public onlyOwner {
        _burn(owner(), id, value);
        tokenSupply[id] -= value;
    }

    function burnBatch(
        uint256[] memory ids,
        uint256[] memory values
    ) public onlyOwner {
        _burnBatch(owner(), ids, values);
        for (uint256 i = 0; i < ids.length; i++) {
            tokenSupply[ids[i]] -= values[i];
        }
    }

    function transferByOwner(
        address to,
        uint256 id,
        uint256 amount
    ) public onlyOwner {
        _safeTransferFrom(owner(), to, id, amount, "");
    }

    function transferByOwnerBatch(
        address[] memory to,
        uint256[] memory ids,
        uint256[] memory amounts
    ) public onlyOwner {
        for (uint256 i = 0; i < ids.length; i++) {
            _safeTransferFrom(owner(), to[i], ids[i], amounts[i], "");
        }
    }

    function update(
        Config.Runtime calldata newConfig
    ) public
    onlyOwner {
        // If metadata is frozen, baseURI cannot be updated
        require(
            metadataUpdatable ||
            (keccak256(abi.encodePacked(newConfig.baseURI)) ==
                keccak256(abi.encodePacked(baseURI))),
            "Metadata is frozen"
        );

        baseURI = newConfig.baseURI;
        royaltiesAddress = newConfig.royaltiesAddress;
        royaltiesBasisPoints = newConfig.royaltiesBps;
        // set price aggregator
        if(newConfig.aggregatorAddress != address(0)) {
            priceFeed = AggregatorV3Interface(newConfig.aggregatorAddress);
        }

        if (!newConfig.metadataUpdatable && metadataUpdatable) {
            metadataUpdatable = false;
            emit PermanentURIGlobal();
        }
    }

    function setTierPrices(
        uint256 tierOnePrice
    ) public onlyOwner {
        require(tierOnePrice > 0, "Price should be above zero");
        uint32 denominator = 1;
        for(uint16 ind = 1; ind <= 5; ++ind) {
            _tierPrices[ind] = tierOnePrice / denominator;
            denominator *= 2;
        }
    }

    function totalSupply(uint256 _id) public view returns (uint256) {
        return tokenSupply[_id];
    }

    function uri(uint256 _id) public override view returns (string memory) {
        if (bytes(_tokenURIs[_id]).length > 0) {
            if (bytes(baseURI).length > 0) {
                return string(abi.encodePacked(baseURI, _tokenURIs[_id]));
            } else {
                return _tokenURIs[_id];
            }
        } else {
            return super.uri(_id);
        }
    }

    function tierById(uint256 _id) public view returns (uint16) {
        return _tokenTiers[_id];
    }

    function tierPrice(uint16 tier) public view returns (uint256) {
        return _tierPrices[tier];
    }

    function tokensByTier(uint16 tier) public view returns (uint256) {
        return _tierToTokenIds[tier].length;
    }

    function tokenIdByTier(uint16 tier, uint256 id) public view returns (uint256) {
        return _tierToTokenIds[tier][id];
    }

    function sellNft(uint256 id, uint256 amount) public {
        require(isApprovedForAll(msg.sender, address(this)), "Token's owner is not approved");
        require(_exists(id), "Token doesn't exist");
        require(amount > 0, "Amount of tokens should be above zero");
        require(balanceOf(msg.sender, id) != 0, "You should be token owner");
        _saleOrders[id] = amount;
        _saleAddresses[id] = msg.sender;
    }

    function cancelSale(uint256 id) public {
        require(_exists(id), "Token doesn't exist");
        require(balanceOf(msg.sender, id) != 0, "You should be token owner");
        _saleOrders[id] = 0;
        _saleAddresses[id] = address(0);
    }

    function buyNft(uint256 id, uint256 amount) 
    external
    payable {
        require(_exists(id), "Token doesn't exist");
        require(_saleAddresses[id] != address(0), "Sale is not exists");
        require(amount > 0, "Amount of tokens should be above zero");

        uint256 price = _tierPrices[_tokenTiers[id]] * amount;
        (,int usdPrice,,,) = priceFeed.latestRoundData();
        uint256 sentValue = msg.value * uint256(usdPrice) / (10 ** decimals);
        require(sentValue > price, "Price of NFT is higher than sent amount");
        
        (bool success, ) = _saleAddresses[id].call{value: sentValue}("");
        require(success, "Quantum NFT: Can't pay for token");

        _safeTransferFrom(_saleAddresses[id], msg.sender, id, amount, "");
        _saleOrders[id] -= amount;
        if(_saleOrders[id] == 0) {
            _saleAddresses[id] = address(0);
        }

        // Give back change
        uint256 neededValue = price / uint256(usdPrice) / (10 ** decimals);
        if(msg.value > neededValue) {
            (bool returned, ) = msg.sender.call{value: msg.value - neededValue}("");
            require(returned, "Quantum NFT: Change can't be transfered back");
        }
    }

    function mintByOwner(address account, uint16 tier, uint256 amount, string memory uri)
    public
    onlyOwner {
        require(amount > 0, "Quantum NFT: amount must be > 0");
        require(tier >= 1 && tier <= 5, "Quantum NFT: tier is not present");

        _tokenIds.increment();
        if (bytes(uri).length > 0) {
            _tokenURIs[_tokenIds.current()] = uri;
            emit URI(uri, _tokenIds.current());
        }
        _mint(account, _tokenIds.current(), amount, "");
        tokenSupply[_tokenIds.current()] += amount;

        _tierToTokenIds[tier].push(_tokenIds.current());
        _tokenTiers[_tokenIds.current()] = tier;
    }

    function mintByOwnerBatch(
        address[] memory to,
        uint16[] memory tiers,
        uint256[] memory amounts,
        string[] memory uris
    ) public onlyOwner {
        require(to.length > 0, "You should mint at least one token");
        for (uint256 ind = 0; ind < to.length; ++ind) {
            require(tiers[ind] >= 1 && tiers[ind] <= 5 , "Quantum NFT: tier is not present");
            require(to[ind] == address(to[ind]), "Quantum NFT: one of addresses is invalid");
            require(amounts[ind] > 0, "Quantum NFT: all amounts must be > 0");

            _tokenIds.increment();
            tokenSupply[_tokenIds.current()] += amounts[ind];
            if (bytes(uris[ind]).length > 0) {
                _tokenURIs[_tokenIds.current()] = uris[ind];
                emit URI(uris[ind], _tokenIds.current());
            }
            _mint(to[ind], _tokenIds.current(), amounts[ind], "");
            _tierToTokenIds[tiers[ind]].push(_tokenIds.current());
            _tokenTiers[_tokenIds.current()] = tiers[ind];
        }
    }

    function onERC1155Received(address, address, uint256, uint256, bytes memory) public virtual returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(address, address, uint256[] memory, uint256[] memory, bytes memory) public virtual returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function royaltyInfo(
        uint256 tokenId,
        uint256 salePrice
    ) external view returns (address, uint256) {
        return (royaltiesAddress, royaltiesBasisPoints * salePrice / ROYALTIES_BASIS);
    }

    function contractURI() external view returns (string memory) {
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        // solium-disable-next-line quotes
                        '{"seller_fee_basis_points": ', // solhint-disable-line
                        royaltiesBasisPoints.toString(),
                        // solium-disable-next-line quotes
                        ', "fee_recipient": "', // solhint-disable-line
                        uint256(uint160(royaltiesAddress)).toHexString(20),
                        // solium-disable-next-line quotes
                        '"}' // solhint-disable-line
                    )
                )
            )
        );

        string memory output = string(
            abi.encodePacked("data:application/json;base64,", json)
        );

        return output;
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC1155) returns (bool)
    {
        return ERC1155.supportsInterface(interfaceId) || interfaceId == type(IERC2981).interfaceId;
    }

    function _exists(uint256 _tokenId) internal view virtual returns (bool) {
        return tokenSupply[_tokenId] > 0;
    }

    /// Validate deployment config
    function _validateDeploymentConfig(Config.Deployment memory config)
        internal
        pure
    {
        require(config.owner != address(0), "Contract must have an owner");
        require(config.eggAddress != address(0), "Cosmic Egg address cannot be null");
    }
}