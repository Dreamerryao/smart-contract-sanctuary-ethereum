    return ownerToIds[_owner][_index];
  }

  function tokenOfOwnerByIndexInternal(
    address _owner,
    uint256 _index
  )
    internal
    view
    returns (uint256)
  {
    require(_index < ownerToIds[_owner].length, INVALID_INDEX);
    return ownerToIds[_owner][_index];
  }

  //Does the address eligible to claim for free ?
  function isEligibleToFutureMints(address who, uint256 _modulo) external view returns (bool)
  {
    
    for (uint256 i=0; i<ownerToNFTokenCount[who]; i++)
    {
        uint256 token_id = tokenOfOwnerByIndexInternal(who, i);
        if(token_id % 21 == _modulo)
        {
          return true;
        }
    }

    return false;
  }

  function isWhitelisted(address isWhitelistedAddr) public view returns (bool) {
      bool result = false;
      for (uint256 i=0; i<whitelisted.length; i++)
      {
          if( whitelisted[i] == isWhitelistedAddr)
          {
              return true;
          }
      }
      return result;
  } 

  function claim() external payable{
    bool whitelisted_res = isWhitelisted(msg.sender);
    if ( (whitelisted_res == true) && (ownerToNFTokenCount[msg.sender] < 1) ){
        _mint(msg.sender,actualMintNr);
    }
    else{
        require(msg.value == 0.0666 ether, "Claiming (another) memebrship costs 0.0666 ETH for this address");
        require(ownerToNFTokenCount[msg.sender] <= 3, "Claiming too many assets per address");
        
        _mint(msg.sender,actualMintNr);
        // Transfer mint price to contract owner
        payable(owner()).transfer(0.0666 ether);
    }
  }

  /**
   * @notice This is an internal function which should be called from user-implemented external
   * mint function. Its purpose is to show and properly initialize data structures when using this
   * implementation.
   * @dev Mints a new NFT.
