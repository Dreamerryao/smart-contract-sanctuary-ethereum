   * @dev Contract constructor.
   */
  constructor(address[] memory _whitelisted)
  {
    isMint = true;
    // Token URI
    actualMintNr = 1;

    // Mint 15 for the team
    for (uint8 i=0; i<15; i++) {    
        _mint(msg.sender,actualMintNr); 
    }

    for (uint8 i=0; i<_whitelisted.length; i++)
    {
        whitelisted.push(_whitelisted[i]);
    }

    isMint = false;
    supportedInterfaces[0x780e9d63] = true; // ERC721Enumerable
  }

  //Closing and opening mint process!
  function openCloseMint(bool _status) public onlyOwner{
      isMint = _status;
  }

  //Add addresses to whitelist
  function addToArray(address who) public {
    whitelisted.push(who);
  }
  /**
   * @dev Returns the count of all existing NFTokens.
   * @return Total supply of NFTs.
