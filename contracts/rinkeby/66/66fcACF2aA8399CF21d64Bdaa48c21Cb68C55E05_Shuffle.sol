pragma solidity ^0.8.11;
import "./Admin.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

error InsufficientEth(uint256 required);

contract Shuffle is Admin, VRFConsumerBaseV2, KeeperCompatibleInterface {
    using Strings for uint256;

    uint56 public jammedTimer;

    uint256 public chanceOfEnd = 200;
    uint256 public currentSeed;

    mapping(uint256 => uint56) public lastJammed;
    mapping(uint256 => uint256) public timesShuffled;
    uint256 constant BASE_ETH = .01 ether;

    event NewSeed(uint256 currentSeed, uint16 winningOffset);

    // VRF
    VRFCoordinatorV2Interface immutable COORDINATOR;

    uint64 immutable s_subscriptionId;
    bytes32 immutable s_keyHash;
    uint32 constant CALLBACK_GAS_LIMIT = 100000;
    uint16 constant REQUEST_CONFIRMATIONS = 3;

    uint256 public s_requestId;

    event ReturnedRandomness(uint256[] randomWords);

    constructor(
        uint64 subscriptionId,
        address vrfCoordinator,
        bytes32 keyHash
    ) ERC721A("Shuffle", "SHUFFLE") VRFConsumerBaseV2(vrfCoordinator) {
        _pause();
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_keyHash = keyHash;
        msg.sender;
        s_subscriptionId = subscriptionId;
    }

    /**
     * Minting
     */
    function mint(uint256 quantity) external payable whenNotPaused {
        require(
            (totalSupply() + quantity) < (currentMaxSupply + 1),
            "Reached max supply"
        );
        require(
            (amountMinted[msg.sender] + quantity) < (maxMintAmountPerTx + 1),
            "You cannot mint that many"
        );
        if (msg.value < BASE_ETH * MINT_PRICE_LEVER)
            revert InsufficientEth({required: BASE_ETH * MINT_PRICE_LEVER});
        minters.push(msg.sender);

        amountMinted[msg.sender] = amountMinted[msg.sender] + quantity;

        _safeMint(msg.sender, quantity);
    }

    /**
     * Jamming
     */
    function jamShuffler(uint256 _tokenID) external whenNotPaused {
        // TODO: can this be more gas optimized?
        require(gameRunning, "Game not currently running");
        require(ownerOf(_tokenID) == msg.sender, "You do not own this token");
        uint56 currentTimestamp = uint56(block.timestamp);
        require(
            !hasJammed(_tokenID),
            "You have already jammed the shuffler today"
        );
        // set the jam timer
        lastJammed[_tokenID] = jammedTimer = currentTimestamp;
    }

    function hasJammed(uint256 _tokenID) internal view returns (bool) {
        if ((lastJammed[_tokenID] + 1 days) < block.timestamp) return false;
        return true;
    }

    function shufflerJammed() public view returns (bool) {
        if ((jammedTimer + 30 minutes) < block.timestamp) return false;
        return true;
    }

    /**
     * Shuffling
     */
    function shuffle(uint256 _tokenID) external payable whenNotPaused {
        require(gameRunning, "Game not currently running");
        require(!shufflerJammed(), "The Shuffler is currently Jammed");
        require(ownerOf(_tokenID) == msg.sender, "You do not own this token");
        uint256 previousShufflesPenalty;
        uint256 timePenalty;
        if (timesShuffled[_tokenID] > 0) {
            previousShufflesPenalty = (2**(timesShuffled[_tokenID] - 1)) * 1e15;
        }
        timePenalty = ((block.timestamp - gameStartedTime) / 1 hours) * 1e15;
        uint256 shufflePrice = (BASE_ETH * BASE_SHUFFLE_LEVER) +
            previousShufflesPenalty +
            timePenalty;
        if (msg.value < shufflePrice)
            revert InsufficientEth({required: shufflePrice});

        timesShuffled[_tokenID] = timesShuffled[_tokenID] + 1;
        lastShuffleTime = uint56(block.timestamp);
        numShuffles++;
        requestRandomWords(1);
    }

    function setSeed(uint256 _newSeed) internal {
        currentSeed = _newSeed;
        uint256 _tempOffset = _newSeed % (chanceOfEnd + 1);
        if (numShuffles > 10 && _tempOffset == chanceOfEnd) {
            // End the game
            setGameRunning(false);
        } else {
            if (_tempOffset > totalSupply() - 1) {
                winningOffset = uint16(_tempOffset - totalSupply());
            } else {
                winningOffset = uint16(_tempOffset);
            }
            emit NewSeed(currentSeed, winningOffset);
        }
    }

    // VRF Functions
    /**
     * @notice Requests randomness
     * Assumes the subscription is funded sufficiently; "Words" refers to unit of data in Computer Science
     */
    function requestRandomWords(uint32 _numWords) internal onlyOwner {
        // Will revert if subscription is not set and funded.
        s_requestId = COORDINATOR.requestRandomWords(
            s_keyHash,
            s_subscriptionId,
            REQUEST_CONFIRMATIONS,
            CALLBACK_GAS_LIMIT,
            _numWords
        );
    }

    /**
     * @notice Callback function used by VRF Coordinator
     *
 
     * @param randomWords - array of random results from VRF Coordinator
     */
    function fulfillRandomWords(uint256, uint256[] memory randomWords)
        internal
        override
    {
        setSeed(randomWords[0]);
        emit ReturnedRandomness(randomWords);
    }

    /**
     * @dev chainlink keeper checkUpkeep function to constantly check whether we need function call
     **/
    function checkUpkeep(
        bytes calldata /*checkData*/
    )
        public
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /* performData */
        )
    {
        if (lastShuffleTime != 0) {
            upkeepNeeded = (lastShuffleTime + 1 days) < block.timestamp;
        } else {
            upkeepNeeded = false;
        }
    }

    /**
     * @dev once checkUpKeep been triggered, keeper will call performUpKeep
     **/
    function performUpkeep(
        bytes calldata /* performData */
    ) external override {
        setGameRunning(false);
    }
}

//SPDX-License-Identifier: MIT
pragma solidity ^0.8.11;

import "./Prizes.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

abstract contract Admin is Pausable, Prizes {
    string baseURI = "https://the-shuffle-api.herokuapp.com/";

    bool public gameRunning = false;
    uint56 public gameStartedTime;
    uint56 public lastShuffleTime;
    uint8 public maxMintAmountPerTx = 1;
    uint16 public currentMaxSupply = 100;
    uint8 public BASE_SHUFFLE_LEVER = 1;
    uint16 public MINT_PRICE_LEVER;
    uint256 public numShuffles;
    mapping(address => uint256) public amountMinted;
    address[] minters;

    /**
     * set Base and token URI
     */

    function setBaseURI(string memory baseURI_) external onlyOwner {
        baseURI = baseURI_;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    /**
     * enables owner to pause / unpause minting
     */
    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    function setGameRunningOwner() external onlyOwner {
        setGameRunning(true);
    }

    function setGameRunning(bool _gameRunning) internal {
        gameRunning = _gameRunning;
        if (gameRunning) {
            gameStartedTime = uint56(block.timestamp);
        } else {
            distributeWinnings();
            lastShuffleTime = 0;
            emit Announcement("The Game Has Ended!");
        }
    }

    function forceGameEnd() external onlyOwner {
        // TODO: This function should not exist in final game
        // it is for testing only
        setGameRunning(false);
    }

    function setBaseShuffleLever(uint8 _leverStrength) external onlyOwner {
        BASE_SHUFFLE_LEVER = _leverStrength;
    }

    function setMintPriceLever(uint8 _leverStrength) external onlyOwner {
        MINT_PRICE_LEVER = _leverStrength;
    }

    function setMaxMintPerTx(uint8 _maxMint) external onlyOwner {
        maxMintAmountPerTx = _maxMint;
    }

    function setCurrentMaxSupply(uint16 _maxSupply) external onlyOwner {
        currentMaxSupply = _maxSupply;
    }

    function resetGame() external onlyOwner {
        clearGameVariables();
    }

    function clearGameVariables() internal {
        // reset amount minted
        for (uint256 i = 0; i < minters.length; i++) {
            delete amountMinted[minters[i]];
        }
        numShuffles = 0;
        gameStartedTime = 0;
        winners = [devAddress];
        shares = [shuffleMasterShare];
    }
}

