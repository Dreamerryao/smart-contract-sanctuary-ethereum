pragma solidity ^0.8.11;

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";

abstract contract Prizes is ERC721A, Ownable {
    address constant devAddress = 0xCa5f91BC11b93991F800EB86eb5dA7D78a9e59A0;
    uint256 public totalWinners = 10;
    uint256 constant shuffleMasterShare = 14;
    uint256 constant bigWinnerShare = 50;
    uint16 public winningOffset;

    mapping(address => uint256[]) internal depositedNFTAddresses;
    address[] depositKeys;
    address[] winners = [devAddress];
    uint256[] shares = [shuffleMasterShare];

    event Announcement(string announcement);

    // Set an NFT as part of the Prize Pool
    function setNFTPrize(uint256 _tokenId, address _tokenContract)
        public
        onlyOwner
    {
        depositKeys.push(_tokenContract);
        depositedNFTAddresses[_tokenContract].push(_tokenId);
    }

    // Distribute winnings
    function distributeWinnings() public {
        uint256 currentWinner;
        for (uint256 i = 0; i < totalWinners; i++) {
            // payOutWinners
            if (winningOffset + i > totalSupply() - 1) {
                currentWinner = winningOffset + i - totalSupply();
            } else {
                currentWinner = winningOffset + i;
            }
            winners.push(ownerOf(currentWinner));
            if (i == 0) {
                shares.push(bigWinnerShare);
            } else {
                shares.push(
                    (100 - bigWinnerShare - shuffleMasterShare) /
                        (totalWinners - 1)
                );
            }
        }
        require(winners.length == shares.length, "Invalid Payout");

        // TODO: make sure this is protected
        uint256 balance = address(this).balance;
        require(balance > 0);
        for (uint256 i = 0; i < winners.length; i++) {
            (bool sent, ) = winners[i].call{value: (balance * shares[i]) / 100}(
                ""
            );
            require(sent);
        }
        bigWinner();
        emit Announcement("Prizes have been distributed!");
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function bigWinner() internal {
        // BIG Winner gets the NFT Prize(s)
        for (uint256 i = 0; i < depositKeys.length; i++) {
            for (
                uint256 j = 0;
                j < depositedNFTAddresses[depositKeys[i]].length;
                j++
            ) {
                transferNFT(
                    depositKeys[i],
                    ownerOf(winningOffset),
                    depositedNFTAddresses[depositKeys[i]][j]
                );
            }
            delete depositedNFTAddresses[depositKeys[i]];
        }
    }

    function transferNFT(
        address _nftToTransfer,
        address _winnerAddress,
        uint256 _tokenId
    ) internal {
        IERC721 contr = IERC721(_nftToTransfer);
        contr.transferFrom(address(this), _winnerAddress, _tokenId);
    }
}

