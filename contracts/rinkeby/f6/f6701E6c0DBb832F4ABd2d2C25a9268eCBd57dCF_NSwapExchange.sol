pragma solidity ^0.8.0;

import "./interface/ICurrencyManager.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {EnumerableSetUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

abstract contract CurrencyManager is
    Initializable,
    OwnableUpgradeable,
    ICurrencyManager
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;

    EnumerableSetUpgradeable.AddressSet private _approvedCurrencies;

    event EventCurrencyApproved(address indexed currency);
    event EventCurrencyRevoked(address indexed currency);

    function __CurrencyManager_init_unchained(address[] memory currencies)
        internal
        initializer
    {
        for (uint256 i = 0; i < currencies.length; i++) {
            address currency = currencies[i];
            _approvedCurrencies.add(currency);

            emit EventCurrencyApproved(currency);
        }
    }

    /**
     * @notice approve a currency in the system
     * @param currency address of the currency not approved
     */
    function approveCurrency(address currency) external override onlyOwner {
        require(
            !_approvedCurrencies.contains(currency),
            "Currency: Already approved"
        );
        _approvedCurrencies.add(currency);

        emit EventCurrencyApproved(currency);
    }

    /**
     * @notice revoke a currency in the system
     * @param currency address of the currency approved
     */
    function revokeCurrency(address currency) external override onlyOwner {
        require(
            _approvedCurrencies.contains(currency),
            "Currency: Not approved"
        );
        _approvedCurrencies.remove(currency);

        emit EventCurrencyRevoked(currency);
    }

    /**
     * @notice Returns if a currency is in the system
     * @param currency address of the currency
     * @return isApproved currency is approved or not
     */
    function isCurrencyApproved(address currency)
        public
        view
        override
        returns (bool)
    {
        return _approvedCurrencies.contains(currency);
    }

    /**
     * @notice Returns if a currency is in the system
     * @param index address of the currency
     * @return currency currency is approved or not
     */
    function viewApprovedCurrency(uint256 index)
        external
        view
        override
        returns (address)
    {
        return _approvedCurrencies.at(index);
    }

    /**
     * @notice View number of approved currencies
     * @return count the number of approved currencies
     */
    function viewCountApprovedCurrencies()
        external
        view
        override
        returns (uint256)
    {
        return _approvedCurrencies.length();
    }
}

