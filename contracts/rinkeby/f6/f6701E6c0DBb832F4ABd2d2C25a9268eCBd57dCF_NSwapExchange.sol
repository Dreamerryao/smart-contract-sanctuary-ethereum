pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./ProtocolFeeManager.sol";
import "./interface/ITransferManager.sol";
import "./lib/LibAsset.sol";
import "./TransferExecutor.sol";
import "./RoyaltiesManager.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

abstract contract NswapTransferManager is
    Initializable,
    ITransferManager,
    TransferExecutor,
    ProtocolFeeManager,
    RoyaltiesManager
{
    using SafeMathUpgradeable for uint256;

    function __NswapTransferManager_init_unchained(
        address[] memory currencies,
        uint256 defaultProtocolFee,
        address defaultFeeRecipient,
        uint256 newRoyaltiesLimit
    ) internal initializer {
        __TransferExecutor_init_unchained(currencies);
        __ProtocolFeeManager_init_unchained(
            defaultProtocolFee,
            defaultFeeRecipient
        );
        __RoyaltiesManager_init_unchained(newRoyaltiesLimit);
    }

    /**
     * @notice prepare to tansfer assets including NFT, ProtocolFee and royaltyFee...
     * @param sellMakeAmount  The amount of nft paid by the seller.
     * @param buyMakeAmount  The amount of token paid by the buyer.
     * @param sell The order of NFT seller.
     * @param buy The order of NFT buyer.
     * @param sellExtraData  Extra data of seller's order.
     * @param buyExtraData  Extra data of buyer's order.
     */
    function executeTransfer(
        uint256 sellMakeAmount,
        uint256 buyMakeAmount,
        LibOrder.Order memory sell,
        LibOrder.Order memory buy,
        LibOrderData.Data memory sellExtraData,
        LibOrderData.Data memory buyExtraData
    ) internal override {
        // Transfer protocol fee and royalty fee.
        // Transfer rest asset to seller.
        executeTransferFees(
            buyMakeAmount,
            buy.maker,
            sellExtraData,
            sell.nft.assetType,
            sell.price.assetType
        );

        // Transfer NFT to buyer.
        transferPayouts(
            sellMakeAmount,
            sell.maker,
            sell.nft.assetType,
            buyExtraData.payouts
        );
    }

    /**
     * @notice prepare to tansfer fees including ProtocolFee and royaltyFee...
     * @param amount the amount of buyer would transfer
     * @param from the buyer
     * @param sellExtraData Extra data of nft seller order.
     * @param sellMakeAsset The asset type of seller make asset.
     * @param buyMakeAsset The asset type of seller take nft.
     */
    function executeTransferFees(
        uint256 amount,
        address from,
        LibOrderData.Data memory sellExtraData,
        LibAsset.AssetType memory sellMakeAsset,
        LibAsset.AssetType memory buyMakeAsset
    ) internal {
        uint256 pre = gasleft();
        uint256 rest = transferProtocolFee(amount, from, buyMakeAsset);
        uint256 post = gasleft();
        console.log("transferProtocolFee",pre-post);
        rest = transferRoyaltyFee(
            amount,
            from,
            rest,
            sellMakeAsset,
            buyMakeAsset
        );
        transferPayouts(rest, from, buyMakeAsset, sellExtraData.payouts);
    }

    /**
     * @notice prepare to tansfer fees protocol fee
     * @param amount the amount of buyer would transfer
     * @param from the buyer
     * @param assetType The asset type of buyer's asset.
     * @return the rest amount of buyer's asset after transfer protocol fee
     */
    function transferProtocolFee(
        uint256 amount,
        address from,
        LibAsset.AssetType memory assetType
    ) internal returns (uint256) {
        (uint256 rest, uint256 fee) = _calcFeeAndRest(
            amount,
            amount,
            _getProtocolFee(from)
        );

        if (fee > 0) {
            transfer(LibAsset.Asset(assetType, fee), from, getFeeReceiver());
        }

        return rest;
    }

    /**
     * @notice prepare to tansfer royalty fee
     * @param amount the amount of buyer would transfer
     * @param from the buyer
     * @param rest the remain amount after transfer other fees before transfer royalty fee
     * @param sellMakeAsset The asset type of seller make asset.
     * @param buyMakeAsset The asset type of seller take asset.
     * @return The rest amount of seller take asset after transfer royalty fee.
     */
    function transferRoyaltyFee(
        uint256 amount,
        address from,
        uint256 rest,
        LibAsset.AssetType memory sellMakeAsset,
        LibAsset.AssetType memory buyMakeAsset
    ) internal returns (uint256) {
        LibPayInfo.PayInfo[] memory fees = getRoyaltiesByAssetType(
            sellMakeAsset
        );

        (uint256 result, uint256 totalRoyalties) = transferFees(
            amount,
            from,
            rest,
            buyMakeAsset,
            fees
        );
        require(totalRoyalties <= 3000, "Royalties are too high (>30%)");
        return result;
    }

    /**
     * @notice get nft royalty type and royalty fee by royalty registry
     * @param matchNft the asset type of seller's nft
     * @return  royalties recipient and royaltyType
     */
    function getRoyaltiesByAssetType(LibAsset.AssetType memory matchNft)
        internal
        returns (LibPayInfo.PayInfo[] memory royalties)
    {
        if (
            matchNft.assetClass == LibAsset.ERC1155_ASSET_CLASS ||
            matchNft.assetClass == LibAsset.ERC721_ASSET_CLASS
        ) {
            (address token, uint256 tokenId) = (
                matchNft.tokenAddress,
                matchNft.tokenID
            );
            return getRoyalties(token, tokenId);
        }
        LibPayInfo.PayInfo[] memory empty;
        return empty;
    }

    /**
     * @notice transfer the fee to each recipient
     * @param amount the amount of buyer would transfer
     * @param from the buyer
     * @param rest the remain amount after transfer other fees before transfer this fee
     * @param matchCalculate the asset type of buyer's asset
     * @param fees fee Recipient
     * @return restValue after this transfer and totalFees in this tranfer
     */
    function transferFees(
        uint256 amount,
        address from,
        uint256 rest,
        LibAsset.AssetType memory matchCalculate,
        LibPayInfo.PayInfo[] memory fees
    ) internal returns (uint256 restValue, uint256 totalFees) {
        totalFees = 0;
        restValue = rest;
        for (uint256 i = 0; i < fees.length; i++) {
            totalFees = totalFees.add(fees[i].share);
            (uint256 newRestValue, uint256 feeValue) = _calcFeeAndRest(
                restValue,
                amount,
                fees[i].share
            );
            restValue = newRestValue;
            if (feeValue > 0) {
                transfer(
                    LibAsset.Asset(matchCalculate, feeValue),
                    from,
                    fees[i].receiver
                );
            }
        }
    }

    /**
     * @notice transfer fee to seller
     * @param amount the total fee to seller
     * @param from the buyer
     * @param buyMakeAsset the asset type of buyer's asset type.
     * @param payouts sellers
     */
    function transferPayouts(
        uint256 amount,
        address from,
        LibAsset.AssetType memory buyMakeAsset,
        LibPayInfo.PayInfo[] memory payouts
    ) internal {
        uint256 sumBps = 0;
        uint256 restValue = amount;
        for (uint256 i = 0; i < payouts.length - 1; i++) {
            uint256 currentAmount = _shareToAmount(amount, payouts[i].share);
            sumBps = sumBps.add(payouts[i].share);
            if (currentAmount > 0) {
                restValue = restValue.sub(currentAmount);
                transfer(
                    LibAsset.Asset(buyMakeAsset, currentAmount),
                    from,
                    payouts[i].receiver
                );
            }
        }
        LibPayInfo.PayInfo memory lastPayout = payouts[payouts.length - 1];
        sumBps = sumBps.add(lastPayout.share);
        require(sumBps == 10000, "TM: SumBps <> 100%");
        if (restValue > 0) {
            transfer(
                LibAsset.Asset(buyMakeAsset, restValue),
                from,
                lastPayout.receiver
            );
        }
    }

    /**
     * @notice caculate fee and rest value.
     * @param value the rest value before sub fee.
     * @param total the total value before sub any fee
     * @param share the fee share in base point.
     * @return rest means the value after sub fee and realFee means the real fee caculate by feeInBp
     */
    function _calcFeeAndRest(
        uint256 value,
        uint256 total,
        uint256 share
    ) internal pure returns (uint256 rest, uint256 realFee) {
        uint256 fee = _shareToAmount(total, share);
        if (value > fee) {
            rest = value.sub(fee);
            realFee = fee;
        } else {
            rest = 0;
            realFee = value;
        }
    }

    /**
     * @notice caculate amount based on share.
     * @param total the total amount.
     * @param share the share in base point.
     */
    function _shareToAmount(uint256 total, uint256 share)
        internal
        pure
        returns (uint256)
    {
        return total.mul(share).div(10000);
    }

    uint256[50] private __gap;
}

