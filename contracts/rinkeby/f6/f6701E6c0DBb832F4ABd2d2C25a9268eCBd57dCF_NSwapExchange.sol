pragma solidity ^0.8.0;

import "./interface/IProtocolFeeManager.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

abstract contract ProtocolFeeManager is
    Initializable,
    OwnableUpgradeable,
    IProtocolFeeManager
{
    uint256 public defaultFee;
    mapping(address => uint256) public fee;

    address public defaultFeeReceiver;

    event EventUpdatedDefaultProtocolFee(uint256 indexed defaultFee);
    event EventUpdatedDefaultProtocolFeeReceiver(address indexed defaultFeeReceiver);

    function __ProtocolFeeManager_init_unchained(
        uint256 defaultProtocolFee,
        address defaultFeeRecipient
    ) internal initializer {
        __Ownable_init_unchained();
        _setDefaultProtocolFee(defaultProtocolFee);
        _setDefaultFeeReceiver(payable(defaultFeeRecipient));
    }

    function setDefaultProtocolFee(uint256 newProtocolFee)
        external
        override
        onlyOwner
    {
        _setDefaultProtocolFee(newProtocolFee);
    }

    function _setDefaultProtocolFee(uint256 newProtocolFee) internal {
        defaultFee = newProtocolFee;
        emit EventUpdatedDefaultProtocolFee(newProtocolFee);
    }

    function setDefaultFeeReceiver(address payable newDefaultFeeReceiver)
        external
        override
        onlyOwner
    {
        _setDefaultFeeReceiver(newDefaultFeeReceiver);
    }

    function _setDefaultFeeReceiver(address payable newDefaultFeeReceiver)
        internal
    {
        defaultFeeReceiver = newDefaultFeeReceiver;
        emit EventUpdatedDefaultProtocolFeeReceiver(newDefaultFeeReceiver);
    }

    function setProtocolFee(address wallet, uint256 newProtocolFee)
        external
        override
        onlyOwner
    {
        fee[wallet] = newProtocolFee;
    }

    function _getProtocolFee(address wallet) internal view returns (uint256) {
        uint256 newFee = fee[wallet];
        if (newFee != 0) {
            return newFee;
        }
        return defaultFee;
    }

    function getProtocolFee(address wallet) public view returns (uint256) {
        return _getProtocolFee(wallet);
    }

    function getFeeReceiver()
        public
        view
        override
        returns (address)
    {
        return defaultFeeReceiver;
    }
}

