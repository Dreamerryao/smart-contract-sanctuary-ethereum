pragma solidity ^0.8.0;

import "./LibAsset.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

library LibOrder {
    using SafeMathUpgradeable for uint256;

    // Side: buy or sell.
    enum Side {
        Sell,
        Buy
    }

    // Currently supported kinds of sale: fixed price, Dutch auction.
    enum SaleKind {
        FixedPrice,
        DutchAuction
    }

    bytes32 constant ORDER_TYPEHASH =
        keccak256(
            "Order(uint8 side,uint8 saleKind,address maker,address taker,Asset nft,Asset price,uint256 salt,uint256 start,uint256 end,bytes4 version,bytes extra)Asset(AssetType assetType,uint256 value)AssetType(bytes4 assetClass,address tokenAddress,uint256 tokenID)"
        );

    struct Order {
        // Side (buy/sell).
        Side side;
        // Kind of sale.
        SaleKind saleKind;
        // Order maker address.
        address maker;
        // Order taker address, if specified.
        address taker;
        // NFT to buy/sell, support bulk nft buy/sell.
        LibAsset.Asset nft;
        // NFT price.
        LibAsset.Asset price;
        // Order salt, used to prevent duplicate hashes.
        uint256 salt;
        // Start Listing timestamp.
        uint256 start;
        // Expiration timestamp - 0 for no expiry.
        uint256 end;
        // The version of order.
        bytes4 version;
        // Order extra parameter.
        bytes extra;
    }

    function hash(Order memory order) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    ORDER_TYPEHASH,
                    order.side,
                    order.saleKind,
                    order.maker,
                    order.taker,
                    LibAsset.hash(order.nft),
                    LibAsset.hash(order.price),
                    order.salt,
                    order.start,
                    order.end,
                    order.version,
                    keccak256(order.extra)
                )
            );
    }
}

