pragma solidity ^0.8.0;

import "./LibPayInfo.sol";
import "./LibOrder.sol";

library LibOrderData {
    bytes4 public constant V1 = bytes4(keccak256("V1"));

    struct Data {
        // Pay to multiple receivers,
        // and each one has a certain share.
        LibPayInfo.PayInfo[] payouts;
    }

    /**
     * @dev Parse order extra data.
     * @param order Order to parse.
     * @return extraData
     */
    function parse(LibOrder.Order memory order)
        internal
        pure
        returns (Data memory extraData)
    {
        if (order.version == V1) {
            extraData = abi.decode(order.extra, (Data));
        } else if (order.version == 0xffffffff) {} else {
            revert("Unknown Order data type");
        }

        if (extraData.payouts.length == 0) {
            extraData.payouts = _setReceiver(order.maker);
        }
    }

    /**
     * @dev Set asset receiver.
     * @param receiver Asset receiver.
     */
    function _setReceiver(address receiver)
        internal
        pure
        returns (LibPayInfo.PayInfo[] memory)
    {
        LibPayInfo.PayInfo[] memory payout = new LibPayInfo.PayInfo[](1);
        payout[0].receiver = payable(receiver);
        payout[0].share = 10000;
        return payout;
    }
}

