pragma solidity ^0.8.0;

import "./lib/LibOrder.sol";
import "./lib/LibSignature.sol";
import "./lib/LibOrderData.sol";
import "./interface/IERC1271.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";

/**
 * @title Verify the validity of the order parameters.
 */
abstract contract OrderValidator is
    Initializable,
    ContextUpgradeable,
    EIP712Upgradeable
{
    using LibSignature for bytes32;
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;
    // Value UINT256_MAX means the order has been canceled.
    uint256 private constant UINT256_MAX = type(uint256).max;

    bytes4 private constant EIP_1271_MAGIC_VALUE = 0x1626ba7e;
    // fillsStat record orders filled status, key is the order hash,
    // and value is filled amount.
    // Value UINT256_MAX means the order has been canceled.
    mapping(bytes32 => uint256) public fillsStat;

    function __OrderValidator_init_unchained() internal initializer {
        __EIP712_init_unchained("Exchange", "1");
    }

    /**
     * @notice Cancel order.
     * @dev Cancelled orders cannot be reopened.
     * @param orderHash  The hash of the order.
     */
    function _cancel(bytes32 orderHash) internal virtual {
        fillsStat[orderHash] = UINT256_MAX;
    }

    /**
     * @dev Validate order parameters, and signature.
     * @param hash Order hash (already calculated, passed to avoid recalculation).
     * @param sig ECDSA signature.
     * @param order Order to validate.
     */
    function _validateOrder(
        LibOrder.Order memory order,
        bytes memory sig,
        bytes32 hash
    ) internal view {
        _validateParameters(order, hash);
        _validateSignature(order, sig, hash);
    }

    /**
     * @notice Validate order parameters.
     * @param order  Order to validate.
     * @param hash  The hash of the order.
     */
    function _validateParameters(LibOrder.Order memory order, bytes32 hash)
        internal
        view
    {
        // Order must have a maker.
        require(order.maker != address(0), "Order must have a maker");
        // Order salt cannot be 0.
        require(order.salt != 0, "0 salt can't be used");
        // Order must be started and not be expired.
        require(
            order.start == 0 || order.start < block.timestamp,
            "Order start validation failed"
        );
        require(
            order.end == 0 || order.end > block.timestamp,
            "Order end validation failed"
        );
        // Cancelled order cannot be matched.
        require(
            fillsStat[hash] < UINT256_MAX,
            "Order fill stat validation failed"
        );

        if (order.side == LibOrder.Side.Sell) {
            require(
                order.nft.assetType.assetClass == LibAsset.ERC721_ASSET_CLASS ||
                    order.nft.assetType.assetClass ==
                    LibAsset.ERC1155_ASSET_CLASS,
                "asset sell must erc721/erc1155"
            );
        } else if (order.side == LibOrder.Side.Buy) {
            require(
                order.price.assetType.assetClass == LibAsset.ETH_ASSET_CLASS ||
                    order.price.assetType.assetClass ==
                    LibAsset.ERC20_ASSET_CLASS,
                "asset pay must eth/erc20"
            );
        }
    }

    /**
     * @notice Validate order signature.
     * @dev Support verify externally owned accounts signature
     * and EIP-1271 contract accounts signature.
     * @param order  Order to validate.
     * @param sig ECDSA signature.
     * @param hash  The hash of the order.
     */
    function _validateSignature(
        LibOrder.Order memory order,
        bytes memory sig,
        bytes32 hash
    ) internal view {
        // message sender need not verify self signed order.
        if (_msgSender() != order.maker) {
            address signer;
            // Verify externally owned accounts signature.
            if (sig.length == 65) {
                signer = _hashTypedDataV4(hash).recover(sig);
            }
            // Verify EIP-1271 contract accounts signature.
            if (signer != order.maker) {
                if (order.maker.isContract()) {
                    require(
                        IERC1271(order.maker).isValidSignature(
                            _hashTypedDataV4(hash),
                            sig
                        ) == EIP_1271_MAGIC_VALUE,
                        "contract order signature verification error"
                    );
                } else {
                    revert("order signature verification error");
                }
            }
        }
    }

    /**
     * @notice Get filled amount of orders.
     * @param sellHash  The hash of the seller's order.
     * @param buyHash  The hash of the buyer's order.
     * @return sellFilledAmount Has completed fill amount of sell order (0 if order is unfilled).
     * @return buyFilledAmount Has completed fill amount of buy order (0 if order is unfilled).
     */
    function _getFilledAmount(bytes32 sellHash, bytes32 buyHash)
        internal
        view
        returns (uint256 sellFilledAmount, uint256 buyFilledAmount)
    {
        // Get has completed fill amount.
        sellFilledAmount = fillsStat[sellHash];
        buyFilledAmount = fillsStat[buyHash];
    }

    /**
     * @notice Update filled amount of orders.
     * @param sellMakeAmount  The amount of nft paid by the seller.
     * @param buyMakeAmount  The amount of token paid by the buyer.
     * @param sellHash  The hash of the seller's order.
     * @param buyHash  The hash of the buyer's order.
     */
    function _updateFilledAmount(
        uint256 sellMakeAmount,
        uint256 buyMakeAmount,
        bytes32 sellHash,
        bytes32 buyHash
    ) internal {
        // Get amount already filled .
        (uint256 sellFilled, uint256 buyFilled) = _getFilledAmount(
            sellHash,
            buyHash
        );

        // Update fill status.
        fillsStat[sellHash] = sellFilled.add(sellMakeAmount);
        fillsStat[buyHash] = buyFilled.add(sellMakeAmount);
    }
}

