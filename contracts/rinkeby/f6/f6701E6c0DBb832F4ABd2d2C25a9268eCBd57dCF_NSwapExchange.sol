pragma solidity ^0.8.0;

library LibAsset {
    bytes4 public constant ETH_ASSET_CLASS = bytes4(keccak256("ETH"));
    bytes4 public constant ERC20_ASSET_CLASS = bytes4(keccak256("ERC20"));
    bytes4 public constant ERC721_ASSET_CLASS = bytes4(keccak256("ERC721"));
    bytes4 public constant ERC1155_ASSET_CLASS = bytes4(keccak256("ERC1155"));

    bytes32 constant ASSET_TYPE_TYPEHASH =
        keccak256(
            "AssetType(bytes4 assetClass,address tokenAddress,uint256 tokenID)"
        );

    bytes32 constant ASSET_TYPEHASH =
        keccak256(
            "Asset(AssetType assetType,uint256 value)AssetType(bytes4 assetClass,address tokenAddress,uint256 tokenID)"
        );

    struct AssetType {
        // Supported asset type:eth,erc20,erc721,erc1155.
        bytes4 assetClass;
        // Token address of erc20,erc721,erc1155, eth token address is zero address.
        address tokenAddress;
        // Token id of erc721,erc1155, eth and erc20 token id is 0.
        uint256 tokenID;
    }

    struct Asset {
        // Asset type. Support {ETH,ERC20,ERC721,ERC1155}_ASSET_CLASS.
        AssetType assetType;
        // Asset value.
        uint256 value;
    }

    function hash(AssetType memory assetType) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    ASSET_TYPE_TYPEHASH,
                    assetType.assetClass,
                    assetType.tokenAddress,
                    assetType.tokenID
                )
            );
    }

    function hash(Asset memory asset) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(ASSET_TYPEHASH, hash(asset.assetType), asset.value)
            );
    }
}

