pragma solidity ^0.8.0;

import "./interface/IRoyaltiesProvider.sol";
import "./interface/IRoyaltiesNswap.sol";
import "./interface/IERC2981.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

error RoyaltiesRegistry_Wrong_RoyaltiesType();
error RoyaltiesRegistry_Invalid_Address();
error RoyaltiesRegistry_Invalid_Royalty_Value();
error RoyaltiesRegistry_Sum_Royalties_Exceed();
error RoyaltiesRegistry_ERC2981_Royalties_Exceed();
error RoyaltiesRegistry_Not_Owner();
error RoyaltiesRegistry_Get_Royalties_Error();
error RoyaltiesRegistry_Royalties_Limit_Exceed();

abstract contract RoyaltiesManager is IRoyaltiesProvider, OwnableUpgradeable {
    //bytes4(keccak256("getRoyaltiesNswap(uint256)")) == 0x3db963bb
    bytes4 constant _INTERFACE_ID_NSWAP = 0x3db963bb;
    //bytes4(keccak256("royaltyInfo(uint256,uint256)")) == 0x2a55205a
    bytes4 constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
    //total point in percentage, 10,000 = 100%
    uint256 constant _TOTAL_POINT = 10000;
    uint256 constant _TOTAL_ROYALTY_TYPE = 4;
    struct RoyaltiesInfo {
        bool isByToken;
        LibPayInfo.PayInfo[] royalties;
    }

    mapping(address => RoyaltiesInfo) public royaltiesByToken;
    mapping(address => RoyaltyType) public royaltiesType;

    uint256 public royaltiesLimit;

    /// @dev supported royalties types
    // 0 - royalties type is unset
    // 1 - royaltiesByToken, 2 - Nswap, 3 - EIP-2981,
    // 4 - unsupported royalties type
    enum RoyaltyType {
        UNSET,
        BYTOKEN,
        NSWAP,
        EIP2981,
        UNSUPPORTED
    }

    event EventUpdatedRoyaltiesLimit(uint256 newRoyaltiesLimit);
    event EventUpdatedRoyaltiesByToken(
        address indexed token,
        LibPayInfo.PayInfo[] royalties
    );

    function __RoyaltiesManager_init_unchained(uint256 newRoyaltiesLimit)
        internal
        initializer
    {
        _updateRoyaltiesLimit(newRoyaltiesLimit);
    }

    /**
     * @notice update royalties fee limit
     * @param newRoyaltiesLimit new royalties fee limit
     */
    function updateRoyaltiesLimit(uint256 newRoyaltiesLimit)
        external
        onlyOwner
    {
        _updateRoyaltiesLimit(newRoyaltiesLimit);
    }

    /**
     * @notice clears and sets new royalties type for token contract
     * @param token token contract
     * @param newRoyaltiesType new royalties type
     */
    function setRoyaltiesType(address token, uint256 newRoyaltiesType)
        external
    {
        checkOwner(token);
        if (newRoyaltiesType <= 0 || newRoyaltiesType > _TOTAL_ROYALTY_TYPE) {
            revert RoyaltiesRegistry_Wrong_RoyaltiesType();
        }

        _setRoyaltiesType(token, RoyaltyType(newRoyaltiesType));
    }

    /**
     * @notice clears royalties type for token contract
     * @param token token contract
     */
    function clearRoyaltiesType(address token) external {
        checkOwner(token);
        delete royaltiesType[token];
    }

    /**
     * @notice sets royalties for token contract in royaltiesByToken mapping and royalties type = 1
     * @param token token contract
     * @param royalties new royalties for token
     */
    function setRoyaltiesByToken(address token, LibPayInfo.PayInfo[] memory royalties)
        external
    {
        checkOwner(token);
        delete royaltiesType[token];
        _setRoyaltiesType(token, RoyaltyType.BYTOKEN);
        uint256 sumRoyalties = 0;
        delete royaltiesByToken[token];
        for (uint256 i = 0; i < royalties.length; i++) {
            if (royalties[i].receiver == address(0x0)) {
                revert RoyaltiesRegistry_Invalid_Address();
            }
            if (royalties[i].share == 0) {
                revert RoyaltiesRegistry_Invalid_Royalty_Value();
            }
            royaltiesByToken[token].royalties.push(royalties[i]);
            sumRoyalties += royalties[i].share;
        }
        if (sumRoyalties > royaltiesLimit) {
            revert RoyaltiesRegistry_Sum_Royalties_Exceed();
        }
        royaltiesByToken[token].isByToken = true;
        emit EventUpdatedRoyaltiesByToken(token, royalties);
    }

    /**
     * @notice returns royalties type for token contract
     * @param token token contract
     */
    function getRoyaltiesType(address token)
        external
        view
        override
        returns (uint256)
    {
        return uint256(_getRoyaltiesType(token));
    }

    /**
     * @notice returns royalties for token contract and token id
     * @param token token contract
     * @param tokenId token id
     */
    function getRoyalties(address token, uint256 tokenId)
        public
        override
        returns (LibPayInfo.PayInfo[] memory)
    {
        RoyaltyType royaltyType = _getRoyaltiesType(token);
        if (royaltyType == RoyaltyType.UNSET) {
            royaltyType = _calculateRoyaltiesType(token);
            _setRoyaltiesType(token, royaltyType);
        }

        if (royaltyType == RoyaltyType.BYTOKEN) {
            return royaltiesByToken[token].royalties;
        }

        if (royaltyType == RoyaltyType.NSWAP) {
            try IRoyaltiesNswap(token).getRoyaltiesNswap(tokenId) returns (
                LibPayInfo.PayInfo[] memory result
            ) {
                return result;
            } catch {
                return new LibPayInfo.PayInfo[](0);
            }
        }

        if (royaltyType == RoyaltyType.EIP2981) {
            try IERC2981(token).royaltyInfo(tokenId, _TOTAL_POINT) returns (
                address receiver,
                uint256 royaltyPoint
            ) {
                LibPayInfo.PayInfo[] memory result;
                if (royaltyPoint == 0) {
                    return result;
                }
                if (royaltyPoint >= _TOTAL_POINT) {
                    revert RoyaltiesRegistry_ERC2981_Royalties_Exceed();
                }
                result = new LibPayInfo.PayInfo[](1);
                result[0].receiver = payable(receiver);
                result[0].share = uint96(royaltyPoint);
                return result;
            } catch {
                return new LibPayInfo.PayInfo[](0);
            }
        }

        if (royaltyType == RoyaltyType.UNSUPPORTED) {
            return new LibPayInfo.PayInfo[](0);
        }

        revert RoyaltiesRegistry_Get_Royalties_Error();
    }

    /**
     * @notice update royalties fee limit
     * @param newRoyaltiesLimit new royalties fee limit
     */
    function _updateRoyaltiesLimit(uint256 newRoyaltiesLimit) internal {
        if (newRoyaltiesLimit > 5000) {
            revert RoyaltiesRegistry_Royalties_Limit_Exceed();
        }
        royaltiesLimit = newRoyaltiesLimit;
        emit EventUpdatedRoyaltiesLimit(newRoyaltiesLimit);
    }

    /**
     * @notice sets royalties type for token contract
     * @param token token contract
     * @param newRoyaltiesType new royalties type
     */
    function _setRoyaltiesType(address token, RoyaltyType newRoyaltiesType)
        internal
    {
        royaltiesType[token] = RoyaltyType(newRoyaltiesType);
    }

    /**
     * @notice returns royalties type for token contract
     * @param token token contract
     */
    function _getRoyaltiesType(address token)
        internal
        view
        returns (RoyaltyType)
    {
        return royaltiesType[token];
    }

    /**
     * @notice checks if msg.sender is owner of this contract or owner of the token contract
     * @param token token contract
     */
    function checkOwner(address token) internal view {
        if (
            (owner() != _msgSender()) &&
            (OwnableUpgradeable(token).owner() != _msgSender())
        ) {
            revert RoyaltiesRegistry_Not_Owner();
        }
    }

    /**
     * @notice calculates royalties type for token contract
     * @param token token contract
     */
    function _calculateRoyaltiesType(address token)
        internal
        view
        returns (RoyaltyType)
    {
        if (royaltiesByToken[token].isByToken) {
            return RoyaltyType.BYTOKEN;
        }

        try
            IERC165Upgradeable(token).supportsInterface(_INTERFACE_ID_NSWAP)
        returns (bool result) {
            if (result) {
                return RoyaltyType.NSWAP;
            }
        } catch {}

        try
            IERC165Upgradeable(token).supportsInterface(_INTERFACE_ID_ERC2981)
