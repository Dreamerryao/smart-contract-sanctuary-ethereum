// error NSWAP_TransferExecutor_Transfer_ERC20_Failed();

abstract contract TransferExecutor is
    Initializable,
    OwnableUpgradeable,
    ITransferExecutor,
    CurrencyManager
{
    using LibTransfer for address;

    function __TransferExecutor_init_unchained(address[] memory currencies)
        internal
        initializer
    {
        __CurrencyManager_init_unchained(currencies);
    }

    /**
     * @notice Transfers `value` amount of `token` from the `from` address to the `to` address specified.
     * @param token The NFT address
     * @param from Source address
     * @param to Target address
     * @param value Transfer amount
     */
    function erc20safeTransferFrom(
        IERC20Upgradeable token,
        address from,
        address to,
        uint256 value
    ) internal {
        require(
            token.transferFrom(from, to, value),
            "failure while transferring"
        );
    }

    /**
     * @notice Transfers `token`'s `tokenId` from the `from` address to the `to` address specified (with safety call).
     * @param token The NFT address
     * @param from The current owner of the NFT
     * @param to The new owner
     * @param tokenId The NFT to transfer
     */
    function erc721safeTransferFrom(
        IERC721Upgradeable token,
        address from,
        address to,
        uint256 tokenId
    ) internal {
        token.safeTransferFrom(from, to, tokenId);
    }

    /**
     * @notice Transfers `value` amount of an `token`'s `id` from the `from` address to the `to` address specified (with safety call).
     * @param token The NFT address
     * @param from Source address
     * @param to Target address
     * @param id ID of the token type
     * @param value Transfer amount
     */
    function erc1155safeTransferFrom(
        IERC1155Upgradeable token,
        address from,
        address to,
        uint256 id,
        uint256 value
    ) internal {
        token.safeTransferFrom(from, to, id, value, "");
    }

    /**
     * @notice the actual transfer any asset
     * @param from Source address
     * @param to Target address
     */
    function transfer(
        LibAsset.Asset memory asset,
        address from,
        address to
    ) internal override {
        if (asset.assetType.assetClass == LibAsset.ETH_ASSET_CLASS) {
            to.transferEth(asset.value);
        } else if (asset.assetType.assetClass == LibAsset.ERC20_ASSET_CLASS) {
            address token = asset.assetType.tokenAddress;
            require(
                isCurrencyApproved(token),
                "erc20 token address is not approve"
            );
            uint pre = gasleft();
            erc20safeTransferFrom(
                IERC20Upgradeable(token),
                from,
                to,
                asset.value
            );
            uint post = gasleft();
            console.log("transfer erc20",pre-post);
        } else if (asset.assetType.assetClass == LibAsset.ERC721_ASSET_CLASS) {
            // (address token, uint256 tokenId) = (
            //     asset.assetType.tokenAddress,
            //     asset.assetType.tokenID
            // );
            require(asset.value == 1, "erc721 value error");
            IERC721 erc721Addr = IERC721(asset.assetType.tokenAddress);
            uint tokenID = asset.assetType.tokenID;
            uint pre = gasleft();
            // erc721safeTransferFrom(
            //     IERC721Upgradeable(asset.assetType.tokenAddress),
            //     from,
            //     to,
            //     asset.assetType.tokenID
            // );
            erc721Addr.transferFrom(from, to, tokenID);
            uint post = gasleft();
            console.log("transfer nft", pre-post);
        } else if (asset.assetType.assetClass == LibAsset.ERC1155_ASSET_CLASS) {
            (address token, uint256 tokenId) = (
                asset.assetType.tokenAddress,
                asset.assetType.tokenID
            );
            erc1155safeTransferFrom(
                IERC1155Upgradeable(token),
                from,
                to,
                tokenId,
                asset.value
            );
        } else {
            revert("not support Token type");
        }
    }

    uint256[50] private __gap;
}

