pragma solidity ^0.8.0;

import "./lib/LibOrder.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

abstract contract AssetMatcher is Initializable {
    function __AssetMatcher_init_unchained() internal initializer {}

    function matchAssets(
        LibAsset.AssetType memory sell,
        LibAsset.AssetType memory buy
    ) internal pure  {
        require(buy.assetClass == sell.assetClass, "asset class match fail");
        if (sell.assetClass == LibAsset.ETH_ASSET_CLASS) {} else if (
            sell.assetClass == LibAsset.ERC20_ASSET_CLASS
        ) {
            require(
                sell.tokenAddress == buy.tokenAddress,
                "token addr match fail"
            );
        } else if (
            sell.assetClass == LibAsset.ERC721_ASSET_CLASS ||
            sell.assetClass == LibAsset.ERC1155_ASSET_CLASS
        ) {
            require(
                sell.tokenAddress == buy.tokenAddress,
                "token addr match fail"
            );
            require(sell.tokenID == buy.tokenID, "token id match fail");
        } else {
            require(LibAsset.hash(sell) == LibAsset.hash(buy), "asset hash match fail");
        }
    }

    uint256[50] private __gap;
}

