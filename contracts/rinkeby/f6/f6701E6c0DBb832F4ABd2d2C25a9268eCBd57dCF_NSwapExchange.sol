pragma solidity ^0.8.0;

import "./NswapTransferManager.sol";
import "./AssetMatcher.sol";
import "./OrderValidator.sol";
import "./TransferExecutor.sol";
import "./lib/LibAsset.sol";
import "./lib/LibOrder.sol";
import "./lib/LibOrderData.sol";
import "./interface/ITransferManager.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
/**
 * @title NSwap NFTs marketplace exchange contract.
 * @notice Supported NFTs transactions types:
 * fix price, offer, private sale, collection offer.
 */
contract NSwapExchange is
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable,
    AssetMatcher,
    OrderValidator,
    NswapTransferManager
{
    using SafeMathUpgradeable for uint256;
    using LibTransfer for address;

    /**
     * @notice Emitted when an NFT order is canceled.
     * @param hash The hash of the order.
     * @param maker The maker of the order.
     * @param priceAssetType The price asset type of the order.
     * @param nftAssetType The nft asset type of the order.
     */
    event EventCancel(
        bytes32 hash,
        address maker,
        LibAsset.AssetType priceAssetType,
        LibAsset.AssetType nftAssetType
    );

    /**
     * @notice Emitted when orders are matched.
     * @dev Prefix buy is NFT buyer, prefix sell is NFT seller.
     * @param buyHash The hash of buy NFT order.
     * @param sellHash The hash of sell NFT order.
     * @param buyMaker The address of buy NFT order maker.
     * @param sellMaker The address of sell NFT order maker.
     * @param priceFilled The amount matched of buy order.
     * @param nftFilled The amount matched of sell order.
     * @param priceAssetType The make asset type of the buyer's order.
     * @param nftAssetType The make asset type of the seller's order.
     */
    event EventMatch(
        bytes32 buyHash,
        bytes32 sellHash,
        address buyMaker,
        address sellMaker,
        uint256 priceFilled,
        uint256 nftFilled,
        LibAsset.AssetType priceAssetType,
        LibAsset.AssetType nftAssetType
    );

    /**
     * @notice Initialize contracts.
     * @param currencies Supported erc20 asset list.
     * @param defaultProtocolFee Default protocol fee.
     * @param defaultFeeRecipient Default protocol fee recipient address.
     * @param newRoyaltiesLimit Default royalties limit
     */
    function initialize(
        address[] memory currencies,
        uint256 defaultProtocolFee,
        address defaultFeeRecipient,
        uint256 newRoyaltiesLimit
    ) public initializer {
        __Context_init_unchained();
        __ReentrancyGuard_init_unchained();
        __Pausable_init_unchained();
        __Ownable_init_unchained();

        __AssetMatcher_init_unchained();
        __OrderValidator_init_unchained();
        __NswapTransferManager_init_unchained(
            currencies,
            defaultProtocolFee,
            defaultFeeRecipient,
            newRoyaltiesLimit
        );
    }

    /**
     * @notice Cancel on chain order.
     * @dev Orders that have not been started or partial fill can be canceled,
     * and there will be no error when canceling orders that have already been completed.
     * @param order  Order to cancel.
     */
    function cancel(LibOrder.Order calldata order) external nonReentrant whenNotPaused{
        require(_msgSender() == order.maker, "not the maker");
        require(order.salt != 0, "0 salt can't be used");
        bytes32 orderHash = LibOrder.hash(order);
        _cancel(orderHash);
        emit EventCancel(
            orderHash,
            order.maker,
            order.price.assetType,
            order.nft.assetType
        );
    }

    /**
     * @notice Match orders from NFT seller and buyer.
     * @dev The seller is an NFT seller and the buyer is an NFT buyer.
     * Pay attention to the order of parameters.
     * @param sell  NFT seller's order.
     * @param sellSignature  Signature of NFT seller's order.
     * @param buy  NFT buyer's order.
     * @param buySignature  Signature of NFT buyer's order.
     */
    function matchOrders(
        LibOrder.Order memory sell,
        bytes memory sellSignature,
        LibOrder.Order memory buy,
        bytes memory buySignature
    ) external payable nonReentrant whenNotPaused {
        // STAGE CHECKS
        bytes32 sellHash = LibOrder.hash(sell);
        bytes32 buyHash = LibOrder.hash(buy);

        // Prevent self-matching.
        require(buyHash != sellHash, "Self-matching orders is prohibited");
        require(sell.saleKind == buy.saleKind, "sale kind must same");
        require(sell.side == LibOrder.Side.Sell, "first order must sell");
        require(buy.side == LibOrder.Side.Buy, "second order must buy");

        _validateOrder(sell, sellSignature, sellHash);
        _validateOrder(buy, buySignature, buyHash);

        // STAGE EFFECTS
        // Orders can complete fill or partial fill,
        // calculate amout that can be fill.
        (uint256 sellMakeAmount, uint256 buyMakeAmount) = _calculateMatchAmount(
            sell,
            buy,
            sellHash,
            buyHash
        );

        require(sellMakeAmount > 0 && buyMakeAmount > 0, "nothing to fill");

        _updateFilledAmount(sellMakeAmount, buyMakeAmount, sellHash, buyHash);

        // STAGE INTERACTIONS
        _executeAssetTransfer(sellMakeAmount, buyMakeAmount, sell, buy);
        emit EventMatch(
            buyHash,
            sellHash,
            buy.maker,
            sell.maker,
            buyMakeAmount,
            sellMakeAmount,
            sell.price.assetType,
            sell.nft.assetType
        );
    }

    /**
     * @notice Calculate fill amount based on buyer and seller's orders.
     * @param sell  NFT seller's order.
     * @param buy  NFT buyer's order.
     * @param sellHash  Hash of seller's order.
     * @param buyHash  Hash of buyer's order.
     * @return sellMakeAmount  The amount of nft paid by the seller.
     * @return buyMakeAmount  The amount of token paid by the buyer.
     */
    function _calculateMatchAmount(
        LibOrder.Order memory sell,
        LibOrder.Order memory buy,
        bytes32 sellHash,
        bytes32 buyHash
    ) internal view returns (uint256 sellMakeAmount, uint256 buyMakeAmount) {
        // Order taker needs to be empty or match counterparty address.
        if (sell.taker != address(0) && sell.taker != buy.maker) {
            revert("orders maker taker mismatch");
        }

        if (buy.taker != address(0) && buy.taker != sell.maker) {
            revert("orders maker taker mismatch");
        }

        // Order taker/maker asset type needs to be match counterparty maker/taker asset type.
        matchAssets(sell.nft.assetType, buy.nft.assetType);
        matchAssets(sell.price.assetType, buy.price.assetType);
        // Get amount already filled .
        (uint256 sellFilledAmount, uint256 buyFilledAmount) = _getFilledAmount(
            sellHash,
            buyHash
        );

        // Calculate the current orders fill amount.
        (sellMakeAmount, buyMakeAmount) = _calculatefillAmount(
            sell,
            buy,
            sellFilledAmount,
            buyFilledAmount
        );
    }

    /**
     * @notice Execute asset transfer and pay fees.
     * @param sellMakeAmount  The amount of nft paid by the seller.
     * @param buyMakeAmount  The amount of token paid by the buyer.
     * @param sell  NFT seller's order.
     * @param buy  NFT buyer's order.
     */
    function _executeAssetTransfer(
        uint256 sellMakeAmount,
        uint256 buyMakeAmount,
        LibOrder.Order memory sell,
        LibOrder.Order memory buy
    ) internal {
        LibOrderData.Data memory sellData = LibOrderData.parse(sell);
        LibOrderData.Data memory buyData = LibOrderData.parse(buy);

        executeTransfer(
            sellMakeAmount,
            buyMakeAmount,
            sell,
            buy,
            sellData,
            buyData
        );
        // Return remaining eth to buyer.
        if (sell.price.assetType.assetClass == LibAsset.ETH_ASSET_CLASS) {
            if (msg.value > buyMakeAmount) {
                address(msg.sender).transferEth(msg.value.sub(buyMakeAmount));
            }
        }
    }

    /**
     * @dev  calculate current orders fill amount.
     * @param sell The order of NFT seller.
     * @param buy The order of NFT buyer.
     * @param sellFilledAmount Has completed fill amount of sell order (0 if order is unfilled).
     * @param buyFilledAmount Has completed fill amount of buy order (0 if order is unfilled).
     */
    function _calculatefillAmount(
        LibOrder.Order memory sell,
        LibOrder.Order memory buy,
        uint256 sellFilledAmount,
        uint256 buyFilledAmount
    ) internal pure returns (uint256 sellMakeAmount, uint256 buyMakeAmount) {
        if (sell.saleKind == LibOrder.SaleKind.FixedPrice) {
            require(buy.price.value >= sell.price.value, "price mismatch");

            uint256 sellRemaining = sell.nft.value - sellFilledAmount;
            uint256 buyRemaining = buy.nft.value - buyFilledAmount;

            sellMakeAmount = (sellRemaining > buyRemaining)
                ? buyRemaining
                : sellRemaining;

            buyMakeAmount = sell.price.value * sellMakeAmount;
        }
    }
    
    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
}

