pragma solidity ^0.8.2;

// import "hardhat/console.sol";

// Mock CryptoPunksMarket to use in rinkeby
contract CryptoPunksMarketV1 {

    function punkIndextoAddress(uint16 punkId) external view returns (address) {
        if (punkId % 2 == 0) {
            return msg.sender;
        }
        return address(this);
    }

    constructor() {
    }

}