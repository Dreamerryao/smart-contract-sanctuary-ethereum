// import "./Doodles.sol";

import "./Dooplicator.sol";

contract FlashBorrower is IERC3156FlashBorrowerUpgradeable, IERC721Receiver {
    enum Action {NORMAL, STEAL, REENTER}

    uint256 public flashBalance;
    address public flashUser;
    address public flashToken;
    uint256 public flashValue;
    uint256 public flashFee;

    /// @dev ERC-3156 Flash loan callback
    function onFlashLoan(address user, address token, uint256 value, uint256 fee, bytes calldata data) external override returns (bytes32) {
      (Action action, uint16 redeemAmount, address nft, address nftxVault, address claimToken) = abi.decode(data, (Action, uint16, address, address, address));
        flashUser = user;
        flashToken = token;
        flashValue = value;
        flashFee = fee;
        NFTXVaultUpgradeable nftVault = NFTXVaultUpgradeable(nftxVault);
        if (action == Action.NORMAL) {
            uint256[] memory arr = new uint256[](0);
            flashBalance = IERC20Upgradeable(token).balanceOf(address(this));
            IERC20Upgradeable(token).approve(msg.sender, value + fee); // Resolve the flash loan
            ERC721(nft).setApprovalForAll(nftxVault, true);
            uint256[] memory redeemNFTs = nftVault.redeem(redeemAmount, arr);
            // Dooplicator(claimToken).claim();
            uint256[] memory amounts = new uint256[](redeemAmount);
            for (uint i = 0; i < redeemAmount; i++) {
                amounts[i] = 1;
            }
            nftVault.mint(redeemNFTs, amounts);
        } else if (action == Action.STEAL) {
            // Do nothing
        } else if (action == Action.REENTER) {
            flashBorrow(msg.sender, token, value * 2, redeemAmount, nft, nftxVault, claimToken);
            IERC20Upgradeable(token).approve(msg.sender, value + fee);
        }
        return keccak256("ERC3156FlashBorrower.onFlashLoan");
    }

    function flashBorrow(
      address lender,
      address token,
      uint256 value,
      uint16 redeemAmount,
      address nft,
      address nftxVault,
      address claimToken
    ) public {
        // Use this to pack arbitrary data to `onFlashLoan`
        bytes memory data = abi.encode(Action.NORMAL, redeemAmount, nft, nftxVault, claimToken);
        IERC3156FlashLenderUpgradeable(lender).flashLoan(IERC3156FlashBorrowerUpgradeable(address(this)), token, value, data);
    }

    function flashBorrowAndSteal(address lender, address token, uint256 value) public {
        // Use this to pack arbitrary data to `onFlashLoan`
        bytes memory data = abi.encode(Action.STEAL);
        IERC3156FlashLenderUpgradeable(lender).flashLoan(IERC3156FlashBorrowerUpgradeable(address(this)), token, value, data);
    }

    function flashBorrowAndReenter(address lender, address token, uint256 value) public {
        // Use this to pack arbitrary data to `onFlashLoan`
        bytes memory data = abi.encode(Action.REENTER);
        IERC3156FlashLenderUpgradeable(lender).flashLoan(IERC3156FlashBorrowerUpgradeable(address(this)), token, value, data);
    }

    function mint(
      address nftxVault,
      address nft,
      uint256[] calldata tokenIds,
      uint256[] calldata amounts
    ) public {
      ERC721(nft).setApprovalForAll(nftxVault, true);
      NFTXVaultUpgradeable(nftxVault).mint(tokenIds, amounts);
    }

    /**
     * @dev See {IERC721Receiver-onERC721Received}.
     *
     * Always returns `IERC721Receiver.onERC721Received.selector`.
     */
    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

