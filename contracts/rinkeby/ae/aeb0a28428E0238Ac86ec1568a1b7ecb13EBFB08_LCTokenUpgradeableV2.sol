// https://github.com/ProjectOpenSea/opensea-erc1155/blob/master/contracts/ERC1155Tradable.sol
// ID 0 is never used, as incremented to 1 the first time - should add an ERC20 like implementation for it?
// id0 reserved cash ? what about reserving a part of the uint256 for NFT, the other part for FT?
// CHANGE MODIFIER IN MINT if you want to control who is entitled to mint
// MORE SERVER-SIDE CHECKS ON URI + NEED FUNCTION TO MODIFY URI
// ADD FUNCTION TO DELETE (burn?)
// ADD FUNCTION TO make transferable or not - inherited?

// Solidity 0.8.0 has check against overflow, Safemath becomes useless

contract LCTokenUpgradeableV2 is ERC1155PausableUpgradeable {
    using Strings for string;
    uint256 private _currentTokenID;

    string public name; // name of the contract
    string public symbol; // symbol of the contract
    string internal _generic_uri_path;
    string internal _generic_uri;

    struct Metadata {
        string _uri;
        string _offchain_hash; // this is calculated off-chain, only stored on-chain
        bool _isDeleted; // need to add function to change this flag?
        bool _transferable; // need to add function to change this flag?
    }

    address private _owner;
    // !!!! only one issuer per token id
    // !!!! should be initialised in the create function
    mapping(uint256 => address) public issuers; // issuers[tokenId] < > address of issuer
    mapping(uint256 => uint256) public tokenSupply; // tokenSupply[tokenId] < > quantity
    mapping(uint256 => Metadata) public metadataById; // metadataById[tokenId] < > Metadata

    // !!!! is it inherited in ERC1155 transfer functions?
    mapping(address => mapping(uint256 => uint256)) internal balances; // balances[address] < > list(tokenId < > quantity)

    uint16 private _version;

    modifier onlyOwner() {
        require(msg.sender == _owner, "ONLY_OWNER_ALLOWED_V2");
        _;
    }

    modifier onlyIssuer(uint256 _id) {
        require(
            issuers[_id] == msg.sender,
            "ERC1155Tradable#onlyIssuer: ONLY_ISSUER_ALLOWED"
        );
        _;
    }

    modifier onlyHolders(uint256 _id) {
        // MUST BE REWORKED ?
        require(
            balances[msg.sender][_id] > 0,
            "ERC1155Tradable#onlyHolders: ONLY_HOLDER_ALLOWED"
        );
        _;
    }

    function initialize(string memory _name, string memory _symbol)
        public
        initializer
    {
        _owner = msg.sender;
        _currentTokenID = 0;
        name = _name;
        symbol = _symbol;
        _generic_uri_path = "http://example.com/foo/path/";
        _generic_uri = string(abi.encodePacked(_generic_uri_path, "{id}.json"));
        __ERC1155_init(_generic_uri);
    }

    //constructor() initializer {}

    /*
    constructor(string memory _name, string memory _symbol)
        ERC1155(generic_uri)
    {
        owner = msg.sender;
        name = _name;
        symbol = _symbol;
    }
    */

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function uri(uint256 _id) public view override returns (string memory) {
        require(_exists(_id), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");
        return metadataById[_id]._uri;
    }

    function uriAndHash(uint256 _id)
        public
        view
        returns (string memory, string memory)
    {
        require(_exists(_id), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");
        return (metadataById[_id]._uri, metadataById[_id]._offchain_hash);
    }

    function setTokenUri(
        uint256 _id,
        string memory _uri,
        string memory _offchain_hash
    ) public onlyOwner {
        require(_exists(_id), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");
        metadataById[_id]._uri = _uri;
        metadataById[_id]._offchain_hash = _offchain_hash;
    }

    function changeMetadataBooleans(
        uint256 _id,
        bool _isDeleted,
        bool _transferable
    ) public onlyOwner {
        require(_exists(_id), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");
        metadataById[_id]._isDeleted = _isDeleted;
        metadataById[_id]._transferable = _transferable;
    }

    function totalSupply(uint256 _id) public view returns (uint256) {
        return tokenSupply[_id];
    }

    function getOwner() public view returns (address) {
        return _owner;
    }

    function getVersion() public view returns (uint16) {
        return _version;
    }

    function setVersion(uint16 version) public onlyOwner {
        _version = version;
    }

    function create(
        // find the next free id
        address _initialOwner,
        uint256 _initialSupply,
        string memory _uri, // memory or calldata
        string memory _offchain_hash, // memory or calldata
        bool _transferable,
        bytes memory _data // memory or calldata
    ) external onlyOwner returns (uint256) {
        uint256 _id = _getNextTokenID();
        _incrementTokenTypeId();
        issuers[_id] = msg.sender; // MUST BE REWORKED

        /* start MORE SERVER-SIDE CHECKS ON URI */
        if (bytes(_uri).length == 0) {
            _uri = _generic_uri;
        }
        metadataById[_id]._uri = _uri;
        metadataById[_id]._offchain_hash = _offchain_hash;
        metadataById[_id]._transferable = _transferable;
        emit URI(_uri, _id);
        /* end MORE SERVER-SIDE CHECKS ON URI */

        _mint(_initialOwner, _id, _initialSupply, _data);
        tokenSupply[_id] = _initialSupply;
        return _id;
    }

    // change modifier to: "onlyIssuer(_id)" ?
    function mint(
        address _to,
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) public onlyOwner {
        _mint(_to, _id, _quantity, _data);
        tokenSupply[_id] = SafeMath.add(tokenSupply[_id], _quantity);
    }

    // change modifier?
    // Mismatch between modifier and require commented
    function batchMint(
        address _to,
        uint256[] memory _ids,
        uint256[] memory _quantities,
        bytes memory _data
    ) public onlyOwner {
        for (uint256 i = 0; i < _ids.length; i++) {
            uint256 _id = _ids[i];
            /*
            require(
                issuers[_id] == msg.sender,
                "ERC1155Tradable#batchMint: ONLY_ISSUER_ALLOWED"
            );
            */
            uint256 quantity = _quantities[i];
            tokenSupply[_id] = SafeMath.add(tokenSupply[_id], quantity);
        }
        //_batchMint(_to, _ids, _quantities, _data);
        _mintBatch(_to, _ids, _quantities, _data);
    }

    // change modifier?
    function setIssuer(address _to, uint256[] memory _ids) public onlyOwner {
        require(
            _to != address(0),
            "ERC1155Tradable#setIssuer: INVALID_ADDRESS."
        );
        for (uint256 i = 0; i < _ids.length; i++) {
            uint256 id = _ids[i];
            _setIssuer(_to, id);
        }
    }

    // change modifier?
    // function _setCreator(address _to, uint256 _id) internal onlyIssuer(_id) {
    function _setIssuer(address _to, uint256 _id) private {
        issuers[_id] = _to;
    }

    // change modifier?
    function _exists(uint256 _id) internal view returns (bool) {
        return issuers[_id] != address(0); // assume every tokenId has a creator, and non existing tokenId have by default address(0)
    }

    // change modifier?
    function _getNextTokenID() private view returns (uint256) {
        return SafeMath.add(_currentTokenID, 1);
    }

    // change modifier?
    function _incrementTokenTypeId() private {
        _currentTokenID++;
    }
}

