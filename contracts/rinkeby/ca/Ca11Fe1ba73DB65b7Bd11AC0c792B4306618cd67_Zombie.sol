pragma solidity ^0.8.0;

import "sombieownership.sol";
import "ierc721metadata.sol";
import "ierc721enumerable.sol";
import "ierc165.sol";
import "erc165.sol";
import "strings.sol";

contract Zombie is ZombieOwnership, IERC721Metadata, IERC721Enumerable {
	string private _zombieTokenName;
	string private _zombieTokenSymbol;
	string private _zombieTokenBaseLink;

	constructor (string memory _name, string memory _symbol, string memory _baseLink) public {
        _zombieTokenName = _name;
        _zombieTokenSymbol = _symbol;
        _zombieTokenBaseLink = _baseLink;
	}

	function name() external view returns (string memory _name) {
        return _zombieTokenName;
	}

    function symbol() external view returns (string memory _symbol) {
        return _zombieTokenSymbol;
    }

    function tokenURI(uint256 _tokenId) external view returns (string memory) {
    	require(_exists(_tokenId), "ERC721Metadata: URI query for nonexistent token");

        return string(abi.encodePacked(_zombieTokenBaseLink, "0000", Strings.toString(zombies[_tokenId].dna), "-zombie.json"));
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ZombieOwnership, IERC165) returns (bool) {
        return
            interfaceId == type(IERC721Enumerable).interfaceId ||
            interfaceId == type(IERC721Metadata).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function tokenOfOwnerByIndex(address owner, uint256 index) public view virtual override returns (uint256) {
        require(index < this.balanceOf(owner), "ERC721Enumerable: owner index out of bounds");
        return index;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return zombies.length;
    }

    function tokenByIndex(uint256 index) public view virtual override returns (uint256) {
        require(index < this.totalSupply(), "ERC721Enumerable: global index out of bounds");
        return index;
    }
}

