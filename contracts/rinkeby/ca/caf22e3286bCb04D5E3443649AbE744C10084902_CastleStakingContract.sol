pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";

import "./CastleStakingUtils.sol";
import "hardhat/console.sol";

contract CastleStakingContract is CastleStakingUtils, ERC1155HolderUpgradeable {
    using ArrayUtils for uint256[];

    constructor(
        IERC1155 _pieceContr,
        IERC1155 _puzzleContr,
        CrownsContractInterface _crownsContr,
        WorkerStakingContractInterface _workerStakingContr
    ) {}

    function initialize(
        IERC1155 _pieceContr,
        IERC1155 _puzzleContr,
        CrownsContractInterface _crownsContr,
        WorkerStakingContractInterface _workerStakingContr
    ) public initializer {
        __Ownable__init();
        __ERC1155Holder_init();

        pieceContr = _pieceContr;
        puzzleContr = _puzzleContr;
        workerStakingContr = _workerStakingContr;
        crownsContr = _crownsContr;
    }

    // -----------------------------------------STAKING - CASTLES-------------------------------------------------------
    function stakeManyPiecePuzzles(
        uint256[] calldata tokenIds,
        uint256[] calldata amounts,
        bool arePuzzles
    ) external {
        require(tokenIds.length == amounts.length, "INVALID");

        if (arePuzzles) {
            require(
                puzzleContr.isApprovedForAll(msg.sender, address(this)),
                "PUZZLESNOTAPPROVED"
            );

            for (uint256 i = 0; i < tokenIds.length; i++) {
                require(
                    puzzleContr.balanceOf(msg.sender, tokenIds[i]) >=
                        amounts[i],
                    "NOTENOUGHPUZZLES"
                );

                uint256 key = generateKey(msg.sender, tokenIds[i]);
                uint256[] storage stakedtokenIds = indexedPuzzlesByOwner[
                    msg.sender
                ];

                if (!stakedtokenIds.isValueInArray(tokenIds[i])) {
                    stakedtokenIds.push(tokenIds[i]);
                }

                stakedPuzzles[key].amount += amounts[i];
            }

            puzzleContr.safeBatchTransferFrom(
                msg.sender,
                address(this),
                tokenIds,
                amounts,
                "0x0"
            );
        } else {
            require(
                pieceContr.isApprovedForAll(msg.sender, address(this)),
                "PIECESNOTAPPROVED"
            );

            for (uint256 i = 0; i < tokenIds.length; i++) {
                require(
                    pieceContr.balanceOf(msg.sender, tokenIds[i]) >= amounts[i],
                    "NOTENOUGHPIECES"
                );

                uint256 key = generateKey(msg.sender, tokenIds[i]);
                uint256[] storage stakedTokenIds = indexedPiecesByOwner[
                    msg.sender
                ];

                if (!stakedTokenIds.isValueInArray(tokenIds[i])) {
                    stakedTokenIds.push(tokenIds[i]);
                }

                stakedPieces[key].amount += amounts[i];
            }

            pieceContr.safeBatchTransferFrom(
                msg.sender,
                address(this),
                tokenIds,
                amounts,
                "0x0"
            );
        }
    }

    function unstakeManyPieces(
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                indexedPiecesByOwner[msg.sender].isValueInArray(tokenIds[i]),
                "NOTYOURPIECE"
            );

            StakedPiecePuzzle storage existingStake = stakedPieces[
                generateKey(msg.sender, tokenIds[i])
            ];

            if (existingStake.workers.length > 0) {
                workerStakingContr.unstakeManyWorkers(existingStake.workers);
            }

            require(existingStake.amount >= amounts[i], "NOTENOUGHPIECES");

            existingStake.amount -= amounts[i];
            indexedPiecesByOwner[msg.sender].deleteValueInArray(tokenIds[i]);
        }

        pieceContr.safeBatchTransferFrom(
            address(this),
            msg.sender,
            tokenIds,
            amounts,
            "0x0"
        );
    }

    function unstakeManyPuzzles(
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                indexedPiecesByOwner[msg.sender].isValueInArray(tokenIds[i]),
                "NOTYOURPUZZLE"
            );

            StakedPiecePuzzle storage existingStake = stakedPuzzles[
                generateKey(msg.sender, tokenIds[i])
            ];

            require(existingStake.amount >= amounts[i], "NOTENOUGHPUZZLES");

            if (existingStake.workers.length > 0) {
                workerStakingContr.unstakeManyWorkers(existingStake.workers);
            }

            existingStake.amount -= amounts[i];
            indexedPiecesByOwner[msg.sender].deleteValueInArray(tokenIds[i]);
        }

        puzzleContr.safeBatchTransferFrom(
            address(this),
            msg.sender,
            tokenIds,
            amounts,
            "0x0"
        );
    }

    function claimManyLandlordTaxes(
        uint256[] calldata piecePuzzleTokenIds,
        bool[] calldata arePuzzles
    ) external {
        require(piecePuzzleTokenIds.length == arePuzzles.length, "INVALID");

        uint256 taxesClaimed;
        StakedPiecePuzzle memory existingStake;

        for (uint256 i = 0; i < piecePuzzleTokenIds.length; i++) {
            uint256 key = generateKey(msg.sender, piecePuzzleTokenIds[i]);

            if (arePuzzles[i]) {
                existingStake = stakedPuzzles[key];
            } else {
                existingStake = stakedPieces[key];
            }

            for (uint256 x = 0; x < existingStake.workers.length; x++) {
                address landlord = workerStakingContr.getStakedWorkersLandlord(
                    existingStake.workers[x]
                );

                require(landlord == msg.sender, "NOTYOURLAND");

                uint256[] memory arr = new uint256[](1);
                arr[0] = existingStake.workers[x];

                (, uint256[] memory ownersTaxes, ) = workerStakingContr
                    .calculateRewardsForManyWorkers(arr);

                workerStakingContr.setTaxesCollectedByLandLordForWorker(
                    existingStake.workers[x],
                    ownersTaxes[0]
                );

                taxesClaimed += ownersTaxes[0];
            }

            crownsContr.mint(msg.sender, taxesClaimed);
        }
    }

    // -----------------------------------------UTILS - PURE------------------------------------------------------------

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] memory id,
        uint256[] memory value,
        bytes memory data
    ) public pure override returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes memory data
    ) public pure override returns (bytes4) {
        return this.onERC1155Received.selector;
    }
}

