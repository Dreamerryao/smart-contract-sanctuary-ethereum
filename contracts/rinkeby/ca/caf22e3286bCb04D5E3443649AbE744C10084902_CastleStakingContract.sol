import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

import "./CommonStakingUtils.sol";
import "./ArrayUtils.sol";
import "./interfaces/WorkerStakingContractInterface.sol";

abstract contract CastleStakingUtils is CommonStakingUtilities {
    using ArrayUtils for uint256[];
    // -----------------------------------------UTILS - CONTRACTS-------------------------------------------------------

    IERC1155 internal pieceContr;
    IERC1155 internal puzzleContr;
    WorkerStakingContractInterface internal workerStakingContr;

    // -----------------------------------------UTILS - MODIFIERS-------------------------------------------------------

    modifier onlyWorkerStakingContract() {
        require(msg.sender == address(workerStakingContr), "INVALIDOPERATION");
        _;
    }

    // -----------------------------------------UTILS - STAKING---------------------------------------------------------

    mapping(uint256 => StakedPiecePuzzle) public stakedPieces;
    mapping(uint256 => StakedPiecePuzzle) public stakedPuzzles;

    mapping(address => uint256[]) public indexedPiecesByOwner;
    mapping(address => uint256[]) public indexedPuzzlesByOwner;

    function getStaked(uint256 key, bool isPrivileged)
        external
        view
        returns (StakedPiecePuzzle memory stake)
    {
        if (!isPrivileged) {
            stake = stakedPieces[key];
        } else {
            stake = stakedPuzzles[key];
        }
    }

    function getTaxRate(uint256 key, bool isPrivileged)
        external
        view
        returns (uint256)
    {
        if (!isPrivileged) {
            return stakedPieces[key].taxRate;
        } else {
            return stakedPuzzles[key].taxRate;
        }
    }

    function getWorkersCount(uint256 key, bool isPrivileged)
        external
        view
        returns (uint256 workers, uint256 amount)
    {
        StakedPiecePuzzle storage stake;
        if (!isPrivileged) {
            stake = stakedPieces[key];
        } else {
            stake = stakedPuzzles[key];
        }

        return (stake.workers.length, stake.amount);
    }

    function addNewWorker(
        uint256 key,
        uint256 tokenId,
        bool isPrivileged
    ) external onlyWorkerStakingContract {
        if (!isPrivileged) {
            stakedPieces[key].workers.push(tokenId);
        } else {
            stakedPuzzles[key].workers.push(tokenId);
        }
    }

    function removeWorker(
        uint256 key,
        uint256 tokenId,
        bool isPrivileged
    ) external onlyWorkerStakingContract {
        if (!isPrivileged) {
            stakedPieces[key].workers.deleteValueInArray(tokenId);
        } else {
            stakedPuzzles[key].workers.deleteValueInArray(tokenId);
        }
    }
}

