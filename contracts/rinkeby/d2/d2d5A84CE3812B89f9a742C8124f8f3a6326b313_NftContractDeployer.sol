pragma solidity ^0.8.4;

import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC1155/ERC1155.sol';
import '@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol';
import '@openzeppelin/contracts/token/common/ERC2981.sol';
import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';



contract NFTWithErc1155UnmintedPersonal is
    ERC1155,
    ERC1155Supply,
    ERC2981,
    Ownable
{
    using Address for address;

    using ECDSA for bytes32;

    mapping ( uint256 => uint256 ) private _maxSupply;

    address public nftTradeAddress;

    string private _name;

    string public version;

    string private _symbol;

    struct MintData {
        uint256 id;
        uint256 maxSupply;
    }

    bytes32 private constant MINTDATA_TYPE_HASH = keccak256("MintData(uint256 id,uint256 maxSupply)");
    bytes32 private constant EIP712_DOMAIN_TYPE_HASH = keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)");


    constructor(
        string memory name_,
        string memory symbol_,
        string memory uri_,
        string memory version_,
        address newOwner_,
        address royalty_,
        uint96 royaltyFee_,
        address nftTradeAddress_
    ) ERC1155(uri_) {
        _name = name_;
        _symbol = symbol_;
        version = version_;
        _transferOwnership(newOwner_);
        _setDefaultRoyalty(royalty_, royaltyFee_);
        nftTradeAddress = nftTradeAddress_;
    }
    function getChainID() private view returns (uint256) {  
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function mint(address account, uint256 id, uint256 amount, uint256 maxSupply_) external onlyOwner {
        if(_maxSupply[id] == 0) {
            _maxSupply[id] = maxSupply_;
        }
        require(totalSupply(id) + amount <= _maxSupply[id], "Exceeded max supply");
        _mint(account, id, amount, "");
    }
    function mintBatch(address account,uint256[] memory ids,uint256[] memory amounts, uint256[] memory maxSupply_) external onlyOwner {
        require(ids.length == maxSupply_.length, "ids and maxSupply_ length mismatch");
        _mintBatch(account, ids, amounts, "");
        for (uint256 i = 0; i < ids.length; i++) {
            if(_maxSupply[ids[i]] == 0) {
                _maxSupply[ids[i]] = maxSupply_[i];
            }
            require(totalSupply(ids[i]) <= _maxSupply[ids[i]], "Exceeded max supply");
        }
    }

    function setURI(string memory newURI) external onlyOwner {
        _setURI(newURI);
    }



    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
        if (operator == nftTradeAddress && ids.length == 1 && amounts.length == 1 && from == owner() && data.length == 97) {
            bytes32 r;
            bytes32 s;
            uint8 v;
            uint256 maxSupply_;
            assembly {
                r := mload(add(data, 0x20))
                s := mload(add(data, 0x40))
                v := byte(0, mload(add(data, 0x60)))
                maxSupply_ := mload(add(data, 0x61))
            }
            MintData memory mintdata;
            mintdata.id = ids[0];
            mintdata.maxSupply = maxSupply_;
            if(_validSignature(mintdata,v,r,s)){
                if(_maxSupply[ids[0]] == 0) {
                    _maxSupply[ids[0]] = maxSupply_;
                }
                require(totalSupply(ids[0]) + amounts[0] <= _maxSupply[ids[0]], "Exceeded max supply");
                _mint(from, ids[0], amounts[0], "");
            }
        }
    }
    function maxSupply(uint256 id) public view returns (uint256) {
        return _maxSupply[id];
    }
    function hashData(MintData memory data) private pure returns (bytes32) {
        return keccak256(
        abi.encode(
            MINTDATA_TYPE_HASH,
            data
        ));
    }
    function hashPacked(bytes32 data) private view returns (bytes32) {
        return keccak256(
        abi.encodePacked(
            "\x19\x01",
            _deriveDomainSeparator(),
            data
      ));
    }
    function _deriveDomainSeparator() private view returns (bytes32) {
        uint256 chainId;
        chainId = getChainID();
        return keccak256(
            abi.encode(
              EIP712_DOMAIN_TYPE_HASH,
              keccak256(bytes(_name)),
              keccak256(bytes(version)),
              chainId,
              address(this)
            )
        );
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
    function setDefaultRoyalty(address receiver, uint96 feeNumerator) external onlyOwner{
        _setDefaultRoyalty(receiver, feeNumerator);
    }
    function setTokenRoyalty(uint256 tokenId, address receiver, uint96 feeNumerator) external onlyOwner {
        _setTokenRoyalty(tokenId, receiver, feeNumerator);
    }
    function deleteDefaultRoyalty() external onlyOwner {
        _deleteDefaultRoyalty();
    }
    function resetTokenRoyalty(uint256 tokenId) external onlyOwner {
        _resetTokenRoyalty(tokenId);
    }
    function _validSignature(MintData memory data, uint8 v, bytes32 r, bytes32 s) private view returns (bool) {
        return hashPacked(hashData(data)).recover(v,r,s) == owner();
    }
    function setNftTradeAddress(address _nftTradeAddress) external onlyOwner{
        nftTradeAddress = _nftTradeAddress;
    }

}

