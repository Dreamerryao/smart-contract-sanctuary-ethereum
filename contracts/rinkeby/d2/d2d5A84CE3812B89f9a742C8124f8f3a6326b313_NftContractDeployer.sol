pragma solidity >0.8.4;

import "./nfts/NFTWithErc721a.sol";
import "./nfts/NFTWithErc1155UnmintedPersonal.sol";


contract NftContractDeployer {

    event ContractInfo (
        string index,
        string spec,
        address contractAddress,
        address deployer,
        uint256 time
    );

    function deployErc721(
        string memory index,
        uint256 maxSupply_,
        string memory name_,
        string memory symble_,
        address newOwner_,
        address royalty_,
        uint96 royaltyFee_
    ) public {
        NFTWithErc721a erc721contract;
        erc721contract = new NFTWithErc721a(maxSupply_, name_, symble_, newOwner_, royalty_, royaltyFee_);
        emit ContractInfo(
            index,
            "ERC-721",
            address(erc721contract),
            msg.sender,
            block.timestamp
        );

    }
    function deployErc1155(
        string memory index,
        string memory name_,
        string memory symbol_,
        string memory uri_,
        string memory version_,
        address newOwner_,
        address royalty_,
        uint96 royaltyFee_,
        address nftTradeAddress_
    ) public {
        NFTWithErc1155UnmintedPersonal erc1155contract; 
        erc1155contract = new NFTWithErc1155UnmintedPersonal(
            name_, symbol_, uri_, version_, newOwner_, royalty_, royaltyFee_, nftTradeAddress_
        );
        emit ContractInfo(
            index,
            "ERC-1155",
            address(erc1155contract),
            msg.sender,
            block.timestamp
        );
    }

}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import '../libs/ERC721A.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/common/ERC2981.sol';
import '@openzeppelin/contracts/utils/Address.sol';

error MaxSupplyReached();


contract NFTWithErc721a is ERC721A, ERC2981, Ownable {
  using Address for address;

  uint256 public immutable maxSupply;

  string private _baseTokenURI;

  constructor(
    uint256 maxSupply_,
    string memory name_,
    string memory symble_,
    address newOwner_,
    address royalty_,
    uint96 royaltyFee_
  ) ERC721A(name_, symble_) {
    _name = name_;
    _symbol = symble_;
    maxSupply = maxSupply_;
    _transferOwnership(newOwner_);
    _setDefaultRoyalty(royalty_, royaltyFee_);
  }

  function airdrop(
    address[] calldata receivers,
    uint256[] calldata numTokens
  ) external onlyOwner {
    for (uint256 i; i < receivers.length; ++i) {
      _mintWrapper(receivers[i], numTokens[i]);
    }
  }
  function devClaim(uint256 numTokens) external onlyOwner {
    _mintWrapper(msg.sender, numTokens);
  }

  function _mintWrapper(address to, uint256 numTokens) internal {
    if (totalSupply() + numTokens > maxSupply) {
      revert MaxSupplyReached();
    }
    _mint(to, numTokens, '', true);
  }


  function _baseURI() internal view virtual override returns (string memory) {
    return _baseTokenURI;
  }

  function setBaseURI(string calldata baseURI) external onlyOwner {
    _baseTokenURI = baseURI;
  }

  string private _name;
  string private _symbol;

  function name() public view override returns (string memory) {
    return _name;
  }

  function symbol() public view override returns (string memory) {
    return _symbol;
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721A, ERC2981) returns (bool) {
    return super.supportsInterface(interfaceId);
  }
  function setDefaultRoyalty(address receiver, uint96 feeNumerator) external onlyOwner{
    _setDefaultRoyalty(receiver, feeNumerator);
  }
  function setTokenRoyalty(uint256 tokenId, address receiver, uint96 feeNumerator) external onlyOwner {
    _setTokenRoyalty(tokenId, receiver, feeNumerator);
  }
  function deleteDefaultRoyalty() external onlyOwner {
    _deleteDefaultRoyalty();
  }
  function resetTokenRoyalty(uint256 tokenId) external onlyOwner {
     _resetTokenRoyalty(tokenId);
  }
}

