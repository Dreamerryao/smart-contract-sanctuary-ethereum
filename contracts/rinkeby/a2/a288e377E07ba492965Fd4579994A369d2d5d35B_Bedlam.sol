pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./BaseERC721.sol";

contract PhaseOne is Ownable {
    using Strings for uint256;

    struct PhaseOneToken {
        uint tokenId;
        uint rarity;
        bool isMinted;        
    }

    mapping(string => PhaseOneToken) internal phaseOne;

    function isMinted(string calldata id) public view returns (bool) {
        return phaseOne[id].isMinted;
    }

    /** @dev view to get all phaseOne holders and update their
        metadata with the associated rarity
     */
    function getPhaseOne(string calldata id) public view returns (uint256, uint256) {
        PhaseOneToken memory value = phaseOne[id];
        require(value.rarity != 0, "Not a real submission");
        return (value.tokenId, value.rarity);
    }

    function updatePhaseOne(string calldata id, uint256 tokenId, uint256 rarity) internal {
        phaseOne[id].isMinted = true;        
        phaseOne[id].tokenId = tokenId;
        phaseOne[id].rarity = rarity;
    }
}

