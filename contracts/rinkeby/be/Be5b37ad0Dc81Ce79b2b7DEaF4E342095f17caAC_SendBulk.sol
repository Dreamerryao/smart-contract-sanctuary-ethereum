// File contracts/SendBulk.sol


pragma solidity =0.8.9;





contract SendBulk is Ownable {
    using SafeMath for uint;

    address public receiverAddress;
    uint public txFee = 0.01 ether;
    uint public VIPFee = 1 ether;
    uint public maxInTx = 255;

    /* VIP List */
    mapping(address => uint) public vipList;

    receive() external payable {}

    function withdrawERC20(address _tokenAddress) public onlyOwner {
        address _receiverAddress = getReceiverAddress();
        IERC20 token = IERC20(_tokenAddress);
        uint256 balance = token.balanceOf(address(this));
        token.transfer(_receiverAddress, balance);
    }

    function registerVIP() public payable  {
        require(msg.value >= VIPFee, "Value must be VIP fee");
        address _receiverAddress = getReceiverAddress();
        payable(_receiverAddress).transfer(msg.value);
        vipList[msg.sender] = block.timestamp + 1 days;
    }

    function addToVIPList(address[] memory _vipList, uint256[] memory _times) public onlyOwner {
        for (uint i = 0; i < _vipList.length; i++) {
            vipList[_vipList[i]] = _times[i];
        }
    }

    function removeFromVIPList(address[] memory _vipList) public onlyOwner {
        for (uint i = 0; i < _vipList.length; i++) {
            vipList[_vipList[i]] = 0;
        }
    }

    function isVIP(address _addr) public view returns(bool) {
        return _addr == owner() || vipList[_addr] > block.timestamp;
    }

    function setReceiverAddress(address _addr) public onlyOwner  {
        require(_addr != address(0), "Address can't be zero");
        receiverAddress = _addr;
    }

    function getReceiverAddress() public view returns(address) {
        return receiverAddress;
    }

    function setVIPFee(uint _fee) external onlyOwner  {
        VIPFee = _fee;
    }

    /*
     * set tx fee
     */
    function setTxFee(uint _fee) external onlyOwner {
        txFee = _fee;
    }

    function ethSendDifferentValue(address[] memory _to,uint[] memory  _value) external payable  {

        uint sendAmount = _value[0];
        uint remainingValue = msg.value;

        bool vip = isVIP(msg.sender);
        if (vip) {
            require(remainingValue >= sendAmount);
        } else {
            require(remainingValue >= sendAmount.add(txFee));
        }

        require(_to.length == _value.length);
        require(_to.length <= 255);

        for (uint8 i = 0; i < _to.length; i++) {
            remainingValue = remainingValue.sub(_value[i]);
            require(payable(_to[i]).send(_value[i]));
        }
    }

    function erc20SendDifferentValue(address _tokenAddress, address[] memory  _to, uint[] memory  _value) external payable {
        uint sendValue = msg.value;
        bool vip = isVIP(msg.sender);
        if (!vip) {
            require(sendValue >= txFee, "You must pay fees");
        }

        require(_to.length == _value.length);
        require(_to.length <= maxInTx);

        IERC20 token = IERC20(_tokenAddress);

        for (uint8 i = 0; i < _to.length; i++) {
            token.transferFrom(msg.sender, _to[i], _value[i]);
        }

    }

    function erc721SendDifferentValue(address _tokenAddress, address[] memory  _to, uint[] memory  _ids) external payable {
        uint sendValue = msg.value;
        bool vip = isVIP(msg.sender);
        if (!vip) {
            require(sendValue >= txFee, "You must pay fees");
        }

        require(_to.length == _ids.length);
        require(_to.length <= maxInTx);

        IERC721 token = IERC721(_tokenAddress);

        for (uint8 i = 0; i < _to.length; i++) {
            token.safeTransferFrom(msg.sender, _to[i], _ids[i]);
        }
    }

    function erc1155SendDifferentValue(address _tokenAddress, address[] memory  _to, uint[] memory  _ids, uint[] memory _amounts) external payable {
        uint sendValue = msg.value;
        bool vip = isVIP(msg.sender);
        if (!vip) {
            require(sendValue >= txFee, "You must pay fees");
        }

        require(_to.length == _ids.length);
        require(_to.length == _amounts.length);
        require(_to.length <= maxInTx);

        IERC1155 token = IERC1155(_tokenAddress);

        for (uint8 i = 0; i < _to.length; i++) {
            token.safeTransferFrom(msg.sender, _to[i], _ids[i], _amounts[i], "");
        }
    }


}