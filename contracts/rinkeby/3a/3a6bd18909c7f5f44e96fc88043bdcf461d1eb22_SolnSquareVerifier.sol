/* Contrat Ownable
    1) définition d'une variable privée '_owner' de type adresse avec une fonction publique getter
    2) définition d'un constructeur interne qui définit la var _owner sur le créateur du contrat
    3) définition d'un modificateur 'onlyOwner' qui se lance s'il est appelé par un compte autre que le propriétaire.
    4) définit la fonction transferOwnership (son utilité parle d'elle-même)
    5) définit un événement qui émet à chaque fois que ownerShip est transféré (y compris dans le constructeur)
*/
contract Ownable {
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
    address private _owner;

    /**
     * @dev Initialise le contrat définissant celui qui déploie comme propriétaire initial
     */
    constructor () internal {
        _owner = msg.sender;
        emit OwnershipTransferred(address(0), _owner);
    }

    /**
     * @dev Exécuté si appelé par un compte autre que le propriétaire
     */
    modifier onlyOwner() {
        require(isOwner(), "Le propriétaire de ce contrat est requis");
        _;
    }

    /**
     * @dev Renvoie l'adresse du propriétaire actuel
     */
    function owner() public view returns (address) {
        return _owner;
    }

    /**
     * @dev Renvoie true si l'appelant est le propriétaire actuel
     */
    function isOwner() public view returns (bool) {
        return msg.sender == _owner;
    }

    /**
     * @dev Définit le contrat sans propriétaire. Il ne sera plus possible de l'appeler
     * `onlyOwner` ne fonctionne plus. Ne peut être appelé que par le propriétaire actuel
     *
     * > Remarque: le renoncement à la propriété laissera le contrat sans propriétaire,
     * supprimant ainsi toute fonctionnalité qui n'est disponible que pour le propriétaire. 
     */
    function renounceOwnership() public onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    function transferOwnership(address newOwner) public onlyOwner {
      /* 1) ajoute une fonctionnalité pour transférer le contrôle du contrat à un newOwner.
             s'assure que le nouveau propriétaire est une vraie adresse */
        _transferOwnership(newOwner);
    }

    /**
     * @dev Transfère la propriété du contrat vers un nouveau compte (`newOwner`).
     */
    function _transferOwnership(address newOwner) internal {
        require(newOwner != address(0), "Adresse invalide");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

/* Contrat Pausable qui hérite du contrat 'Ownable'
   1) définit une variable privée '_paused' de type bool
   2) définit un setter public en utilisant le modificateur onlyOwner hérité
   3) définit un constructeur interne qui définit la variable _paused sur false
   4) définit un modificateur 'whenNotPaused' & 'paused' qui dirige vers la situation appropriée
   5) définit un événement Paused & Unpaused qui émet l'adresse qui a déclenché l'événement
*/
contract Pausable is Ownable {
    /**
     * @dev Émis lorsque la pause est déclenchée par un 'Pauser'
     */
    event Paused(address account);

    /**
     * @dev Émis lorsque la pause est levée par un 'Pauser'
     */
    event Unpaused(address account);

    bool private _paused;

    /**
     * @dev Initialise le contrat à l'état non suspendu. Attribue le rôle 'Pauser' au déployeur
     */

    constructor () internal {
        _paused = false;
    }

    /**
     * @dev Renvoie vrai si le contrat est suspendu et faux dans le cas contraire
     */
    function paused() public view returns (bool) {
        return _paused;
    }

        /**
     * @dev Modificateur pour rendre une fonction appelable uniquement lorsque le contrat n'est pas suspendu
     */
    modifier whenNotPaused() {
        require(!_paused, "Le contrat est actif");
        _;
    }

    /**
     * @dev Modificateur pour rendre une fonction appelable uniquement lorsque le contrat est suspendu
     */
    modifier whenPaused() {
        require(_paused, "Le contrat est en pause");
        _;
    }

    /**
     * @dev Appelé par un 'Pauser'pour mettre en pause, déclenche l'état d'arrêt
     */
    function pause() public onlyOwner whenNotPaused {
        _paused = true;
        emit Paused(msg.sender);
    }

    /**
     * @dev Appelé par un 'Pauser' pour réactiver, retourne à l'état normal
     */
    function unpause() public onlyOwner whenPaused {
        _paused = false;
        emit Unpaused(msg.sender);
    }
}

