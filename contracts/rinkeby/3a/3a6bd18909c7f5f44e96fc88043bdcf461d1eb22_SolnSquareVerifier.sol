     * @return uint256 représentant le montant appartenant à l'adresse transmise
    */
    function balanceOf(address owner) public view returns (uint256) {
        require(owner != address(0), "Adresse invalide");
        return _ownedTokensCount[owner].current();
    }

    /**
     * @dev Renvoie le propriétaire de l'ID du token spécifié
     * @param tokenId uint256 ID du token pour interroger le propriétaire de
     * @return adresse actuellement marquée comme propriétaire de l'ID du token donné
    */
    function ownerOf(uint256 tokenId) public view returns (address) {
        address owner = _tokenOwner[tokenId];
        require(owner != address(0), "Token non-existant");

        return owner;
    }

    /**
     * @dev Approuve une autre adresse pour transférer l'ID du token donné
     * L'adresse zéro indique qu'il n'y a pas d'adresse approuvée
     * Il ne peut y avoir qu'une seule adresse approuvée par token à la fois
     * Ne peut être appelé que par le propriétaire du token ou un opérateur agréé
     * @param to adresse à approuver pour l'ID du token donné
     * @param tokenId uint256 ID du token à approuver
    */
    function approve(address to, uint256 tokenId) public {
        // exige que l'adresse donnée ne soit pas le propriétaire du tokenId
        address owner = ownerOf(tokenId);
        require(to != owner, "Cette adresse est celle du propriétaire du token");

        // exige que l'expéditeur du msg soit le propriétaire du contrat ou que isApprovedForAll() retourne vrai
        require(msg.sender == owner || isApprovedForAll(owner, msg.sender),
            "Requête non autorisée"
        );

        // ajoute une adresse 'to' aux approbations de tokens
        _tokenApprovals[tokenId] = to;

        // emet l'évènement d'approbation
        emit Approval(owner, to, tokenId);
    }

    /**
     * @dev Obtient l'adresse approuvée pour un ID de token, ou zéro si aucune adresse n'est définie
     * Annule si l'ID de token n'existe pas
     * @param tokenId uint256 ID du token pour demander l'approbation de
     * @return adresse actuellement approuvée pour l'ID de token donné
    */
    function getApproved(uint256 tokenId) public view returns (address) {
        require(_exists(tokenId), "Tokne non-existant");

        return _tokenApprovals[tokenId];
    }

    /**
     * @dev Définit ou annule l'approbation d'un opérateur donné
     * Un opérateur est autorisé à transférer tous les tokens de l'expéditeur en son nom
     * @param to adresse de l'opérateur pour définir l'approbation
     * @param approved représente le statut de l'approbation à définir
     */
    function setApprovalForAll(address to, bool approved) public {
        require(to != msg.sender, "Pas besoin de définir un propriétaire");
        _operatorApprovals[msg.sender][to] = approved;
        emit ApprovalForAll(msg.sender, to, approved);
    }

    /**
     * @dev Indique si un opérateur est approuvé par un propriétaire donné
     * @param owner adresse du propriétaire dont on souhaite demander l'approbation
     * @param operator adresse de l'opérateur dont on souhaite demander l'approbation
     * @return bool si l'opérateur donné est approuvé par le propriétaire donné
     */
    function isApprovedForAll(address owner, address operator) public view returns (bool) {
        return _operatorApprovals[owner][operator];
    }

    /**
     * @dev Transfère la propriété d'un identifiant de token donné à une autre adresse
     * L'utilisation de cette méthode est déconseillée, utilisez {safeTransferFrom} autant que possible
     * Nécessite que msg.sender soit le propriétaire, l'approbation ou l'opérateur
     * @param from propriétaire actuel du token
     * @param to adresse pour recevoir la propriété de l'ID de token donné
     * @param tokenId uint256 ID du token à transférer
    */
    function transferFrom(address from, address to, uint256 tokenId) public {
        require(_isApprovedOrOwner(msg.sender, tokenId), "Seul le propriétaire peut effectuer un transfert");

        _transferFrom(from, to, tokenId);
    }

    /**
     * @dev Transfère en toute sécurité la propriété d'un identifiant de token donné à une autre adresse
     * Si l'adresse cible est un contrat, elle doit implémenter {IERC721Receiver-onERC721Received},
     * qui est appelé sur un transfert sécurisé, et renvoie la valeur magique
     * `bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"))`; autrement, le transfert est annulé
     * Nécessite que msg.sender soit le propriétaire, l'approbation ou l'opérateur
     * @param from propriétaire actuel du token
     * @param to adresse pour recevoir la propriété de l'ID de token donné
     * @param tokenId uint256 ID du token à transférer
    */
    function safeTransferFrom(address from, address to, uint256 tokenId) public {
        safeTransferFrom(from, to, tokenId, "");
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory _data) public {
        transferFrom(from, to, tokenId);
        require(_checkOnERC721Received(from, to, tokenId, _data), "Vérification de l'interface onERC721Received");
    }

    /**
     * @dev Renvoie si le token spécifié existe
     * @param tokenId uint256 ID du token pour interroger l'existence de
     * @return bool si le token existe
     */
    function _exists(uint256 tokenId) internal view returns (bool) {
        address owner = _tokenOwner[tokenId];
        return owner != address(0);
    }

    /**
     * @dev Renvoie si l'acheteur donné peut transférer un identifiant de token donné
     * @param spender adresse de l'acheteur à interroger
     * @param tokenId uint256 ID du token à transférer
     * @return bool si le msg.sender est approuvé pour l'ID du token donné,
     * est un opérateur du propriétaire ou est le propriétaire du token
     */
    function _isApprovedOrOwner(address spender, uint256 tokenId) internal view returns (bool) {
        address owner = ownerOf(tokenId);
        return (spender == owner || getApproved(tokenId) == spender || isApprovedForAll(owner, spender));
    }

    /**
     * @dev Fonction interne pour 'minter' un nouveau token.
     * Annule si l'ID de token donné existe déjà
     * @param to L'adresse qui possédera le jeton 'minté'
     * @param tokenId uint256 ID du token à 'minter'
    */
    function _mint(address to, uint256 tokenId) internal {
        // annuler si le tokenId donné existe déjà ou si l'adresse donnée n'est pas valide
        require(to != address(0), "'Mint' vers l'adresse zero");
        require(!_exists(tokenId), "Ce token existe déjà");

        // 'mint' tokenId à l'adresse donnée et incrémente le nombre de tokens du propriétaire
        _tokenOwner[tokenId] = to;
        _ownedTokensCount[to].increment();

        // émet l'événement de transfert
        emit Transfer(address(0), to, tokenId);
    }

    // @dev Fonction interne pour transférer la propriété d'un identifiant de token donné vers une autre adresse.
    // Tip: se souvenir des fonctions à utiliser pour les compteurs. vous pouvez consulter le mapping au-dessus
    function _transferFrom(address from, address to, uint256 tokenId) internal {
        // vérifie que l'adresse est bien le propriétaire du token donné
        require(ownerOf(tokenId) == from, "Propriétaire incorrect");

        // Exige que le token soit transféré à une adresse valide
        require(to != address(0), "Non autorisé à efectuer un transfert");
        _clearApproval(tokenId);

        // mettre à jour le nombre de tokens et transférer la propriété de l'ID du token
        _ownedTokensCount[from].decrement();
        _ownedTokensCount[to].increment();
        _tokenOwner[tokenId] = to;

        // émet le bon évènement
        emit Transfer(from, to, tokenId);
    }

    // Fonction permettant le "burn' d'un token spécifique
    function _burn(address owner, uint256 tokenId) internal {
        require(ownerOf(tokenId) == owner, "'Burn' le token sans appartenance");
        _clearApproval(tokenId);
        _ownedTokensCount[owner].decrement();
        _tokenOwner[tokenId] = address(0);
        emit Transfer(owner, address(0), tokenId);
    }

    function _burn(uint256 tokenId) internal {
        _burn(ownerOf(tokenId), tokenId);
    }

    /**
     * @dev Fonction interne pour appeler `onERC721Received` sur une adresse cible
     * L'appel n'est pas exécuté si l'adresse cible n'est pas un contrat
     * @param from adresse représentant l'ancien propriétaire de l'ID de token donné
     * @param to adresse cible qui recevra le token
     * @param tokenId uint256 ID du token à transférer
     * @param _data bytes données optionnelles à envoyer avec l'appel
     * @return bool si l'appel a correctement renvoyé la valeur magique attendue
     */
    function _checkOnERC721Received(address from, address to, uint256 tokenId, bytes memory _data)
        internal returns (bool)
    {
        if (!to.isContract()) {
            return true;
        }

        bytes4 retval = IERC721Receiver(to).onERC721Received(msg.sender, from, tokenId, _data);
        return (retval == _ERC721_RECEIVED);
    }

    // @dev Fonction privée pour effacer l'approbation actuelle d'un identifiant de token donné
    function _clearApproval(uint256 tokenId) private {
        if (_tokenApprovals[tokenId] != address(0)) {
            _tokenApprovals[tokenId] = address(0);
        }
    }
}

contract ERC721Enumerable is ERC165, ERC721 { // Provient de OpenZeppelin : https://github.com/OpenZeppelin/openzeppelin-contracts/tree/master/contracts/token/ERC721/extensions
    // Mapping from owner to list of owned token IDs
    mapping(address => uint256[]) private _ownedTokens;

    // Mapping from token ID to index of the owner tokens list
    mapping(uint256 => uint256) private _ownedTokensIndex;

    // Array with all token ids, used for enumeration
    uint256[] private _allTokens;

    // Mapping from token id to position in the allTokens array
    mapping(uint256 => uint256) private _allTokensIndex;

    bytes4 private constant _INTERFACE_ID_ERC721_ENUMERABLE = 0x780e9d63;
    /*
     * 0x780e9d63 ===
     *     bytes4(keccak256('totalSupply()')) ^
     *     bytes4(keccak256('tokenOfOwnerByIndex(address,uint256)')) ^
     *     bytes4(keccak256('tokenByIndex(uint256)'))
     */

    /**
     * @dev Constructor function
     */
    constructor () public {
        // register the supported interface to conform to ERC721Enumerable via ERC165
        _registerInterface(_INTERFACE_ID_ERC721_ENUMERABLE);
    }

    /**
     * @dev Gets the token ID at a given index of the tokens list of the requested owner
     * @param owner address owning the tokens list to be accessed
     * @param index uint256 representing the index to be accessed of the requested tokens list
     * @return uint256 token ID at the given index of the tokens list owned by the requested address
     */
    function tokenOfOwnerByIndex(address owner, uint256 index) public view returns (uint256) {
        require(index < balanceOf(owner), "Index plus grand que le solde");
        return _ownedTokens[owner][index];
    }

    /**
     * @dev Gets the total amount of tokens stored by the contract
     * @return uint256 representing the total amount of tokens
     */
    function totalSupply() public view returns (uint256) {
        return _allTokens.length;
    }

    /**
     * @dev Gets the token ID at a given index of all the tokens in this contract
     * Reverts if the index is greater or equal to the total number of tokens
     * @param index uint256 representing the index to be accessed of the tokens list
     * @return uint256 token ID at the given index of the tokens list
     */
    function tokenByIndex(uint256 index) public view returns (uint256) {
        require(index < totalSupply(), "Index est plus grand que la quantité totale");
        return _allTokens[index];
    }

    /**
     * @dev Internal function to transfer ownership of a given token ID to another address.
     * As opposed to transferFrom, this imposes no restrictions on msg.sender.
     * @param from current owner of the token
     * @param to address to receive the ownership of the given token ID
     * @param tokenId uint256 ID of the token to be transferred
     */
    function _transferFrom(address from, address to, uint256 tokenId) internal {
        super._transferFrom(from, to, tokenId);

        _removeTokenFromOwnerEnumeration(from, tokenId);
        _addTokenToOwnerEnumeration(to, tokenId);
    }

    /**
     * @dev Internal function to mint a new token
     * Reverts if the given token ID already exists
     * @param to address the beneficiary that will own the minted token
     * @param tokenId uint256 ID of the token to be minted
     */
    function _mint(address to, uint256 tokenId) internal {
        super._mint(to, tokenId);

        _addTokenToOwnerEnumeration(to, tokenId);
        _addTokenToAllTokensEnumeration(tokenId);
    }

    // Fonction pour 'burn' un token spécifique
    function _burn(address owner, uint256 tokenId) internal {
        super._burn(owner, tokenId);
        _removeTokenFromOwnerEnumeration(owner, tokenId);
        _ownedTokensIndex[tokenId] = 0;
        _removeTokenFromAllTokensEnumeration(tokenId);
    }

    /**
     * @dev Gets the list of token IDs of the requested owner
     * @param owner address owning the tokens
     * @return uint256[] List of token IDs owned by the requested address
     */
    function _tokensOfOwner(address owner) internal view returns (uint256[] storage) {
        return _ownedTokens[owner];
    }

    /**
     * @dev Private function to add a token to this extension's ownership-tracking data structures.
     * @param to address representing the new owner of the given token ID
     * @param tokenId uint256 ID of the token to be added to the tokens list of the given address
     */
    function _addTokenToOwnerEnumeration(address to, uint256 tokenId) private {
        _ownedTokensIndex[tokenId] = _ownedTokens[to].length;
        _ownedTokens[to].push(tokenId);
    }

    /**
     * @dev Private function to add a token to this extension's token tracking data structures.
     * @param tokenId uint256 ID of the token to be added to the tokens list
     */
    function _addTokenToAllTokensEnumeration(uint256 tokenId) private {
        _allTokensIndex[tokenId] = _allTokens.length;
        _allTokens.push(tokenId);
    }

    /**
     * @dev Private function to remove a token from this extension's ownership-tracking data structures. Note that
     * while the token is not assigned a new owner, the _ownedTokensIndex mapping is _not_ updated: this allows for
     * gas optimizations e.g. when performing a transfer operation (avoiding double writes).
     * This has O(1) time complexity, but alters the order of the _ownedTokens array.
     * @param from address representing the previous owner of the given token ID
     * @param tokenId uint256 ID of the token to be removed from the tokens list of the given address
     */
    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId) private {
        // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = _ownedTokens[from].length.sub(1);
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        _ownedTokens[from].length--;

        // Note that _ownedTokensIndex[tokenId] hasn't been cleared: it still points to the old slot (now occupied by
        // lastTokenId, or just over the end of the array if the token was the last one).
    }

    /**
     * @dev Private function to remove a token from this extension's token tracking data structures.
     * This has O(1) time complexity, but alters the order of the _allTokens array.
     * @param tokenId uint256 ID of the token to be removed from the tokens list
     */
    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {
        // To prevent a gap in the tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = _allTokens.length.sub(1);
        uint256 tokenIndex = _allTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary. However, since this occurs so
        // rarely (when the last minted token is burnt) that we still do the swap here to avoid the gas cost of adding
        // an 'if' statement (like in _removeTokenFromOwnerEnumeration)
        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        // This also deletes the contents at the last position of the array
        _allTokens.length--;
        _allTokensIndex[tokenId] = 0;
    }
}

contract ERC721Metadata is ERC721Enumerable, usingOraclize {

    // crée des variables privées pour le token _name, _symbol et _baseTokenURI (string)
    // crée un mappage privé de tokenId vers des uri de token appelés '_tokenURIs'

    // Nom du token
    string private _name;

    // Symbole du token
    string private _symbol;

    // Base URI du token
    string private _baseTokenURI;

    // Mappage facultatif pour les URIs de token
    mapping(uint256 => string) private _tokenURIs;

    bytes4 private constant _INTERFACE_ID_ERC721_METADATA = 0x5b5e139f;
    /*
     * 0x5b5e139f ===
     *     bytes4(keccak256('name()')) ^
     *     bytes4(keccak256('symbol()')) ^
     *     bytes4(keccak256('tokenURI(uint256)'))
     */


    constructor (string memory name, string memory symbol, string memory baseTokenURI) public {
        // définie les valeurs des variables d'instance
        _name = name;
        _symbol = symbol;
        _baseTokenURI = baseTokenURI;

        // enregistre les interfaces prises en charge pour se conformer à ERC721 via ERC165
        _registerInterface(_INTERFACE_ID_ERC721_METADATA);
    }

    /**
     * @dev Récupère le nom du token
     * @return string représentant le nom du token
    */
    function name() external view returns (string memory) {
        return _name;
    }

    /**
     * @dev Récupère le symbole du token.
     * @return string représentant le symbole du token
    */
    function symbol() external view returns (string memory) {
        return _symbol;
    }

    function baseTokenURI() external view returns(string memory) {
        return _baseTokenURI;
    }

    /**
     * @dev Renvoie un URI pour un ID de token donné
     * Lève une erreur si l'ID de token n'existe pas. Peut renvoyer un string vide
     * @param tokenId uint256 ID du token à interroger
    */
    function tokenURI(uint256 tokenId) external view returns (string memory) {
        require(_exists(tokenId), "Ce token n'existe pas");
        return _tokenURIs[tokenId];
    }

    // Crée une fonction interne pour définir le tokenURI d'un tokenId spécifié
    // Cela doit être le _baseTokenURI + le tokenId sous forme de string
    // ASTUCE # 1: utiliser strConcat() à partir de la bibliothèque oraclizeAPI importée pour définir l'URI du token complet
    // ASTUCE # 2: on peut également utiliser uint2str() pour convertir un uint en string
        // voir https://github.com/oraclize/ethereum-api/blob/master/oraclizeAPI_0.5.sol pour strConcat()
    // exige que le token existe avant de définir quoi que ce soit
    function _setTokenURI(uint256 tokenId) internal {
        require(_exists(tokenId), "Ce token n'existe pas");
        _tokenURIs[tokenId] = strConcat(_baseTokenURI,uint2str(tokenId));
    }

    // Fonction pour 'burn' un token spécifique
    function _burn(address owner, uint256 tokenId) internal {
        super._burn(owner, tokenId);
        if (bytes(_tokenURIs[tokenId]).length != 0) {
            delete _tokenURIs[tokenId];
        }
    }
}

//  Contrat CustomERC721Token qui hérite du contrat ERC721Metadata.
