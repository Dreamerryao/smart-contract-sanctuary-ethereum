contract SmuppyInu is ERC20, AccessControl {
    bytes32 public constant PAIR_HASH = keccak256("PAIR_CONTRACT_NAME_HASH");
    bytes32 public constant DEFAULT_OWNER = keccak256("OWNABLE_NAME_HASH");
    
    uint constant DENOMINATOR = 10000;
    uint public buyerFee = 0;
    uint public sellerFee = 1000;
    uint public maxAmount; 

      uint256 private _feeRate = 2;
     using SafeMath for uint256;

    address public uniswapV2Pair;
     IUniswapV2Router02 public uniswapV2Router;
      mapping(address => bool) public Limtcheck;

    constructor() ERC20("Smuppy Inu", "SMUPPY") {
        _mint(_msgSender(), 10000000 * 10 ** decimals());
        maxAmount = 200000e18; 
        _setRoleAdmin(DEFAULT_ADMIN_ROLE,DEFAULT_OWNER);
        _setupRole(DEFAULT_OWNER,_msgSender());
    }

    function init() external onlyRole(DEFAULT_OWNER) {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), _uniswapV2Router.WETH());
        uniswapV2Router = _uniswapV2Router;
        _setupRole(PAIR_HASH,uniswapV2Pair);

          Limtcheck[_msgSender()]=true;
        Limtcheck[address(uniswapV2Router)]=true;
        Limtcheck[uniswapV2Pair]=true;
         Limtcheck[marketingAddress]=true;
    }

      function UpdateLimitcheck(address _addr,bool _status) external onlyRole(DEFAULT_OWNER) {
        Limtcheck[_addr]=_status;
    }

    /**
     * @dev grant role to pair address
     */
    function grantRoleToPair(address pair) external onlyRole(DEFAULT_OWNER) {
        require(!hasRole(PAIR_HASH, pair));
        _setupRole(PAIR_HASH,pair);
    }

    /**
     * @dev revoke pair address from granted role
     */
    function revokePairRole(address pair) external onlyRole(DEFAULT_OWNER) {
        require(hasRole(PAIR_HASH, pair));
        _revokeRole(PAIR_HASH,pair);
    }

    /**
     * @dev transfers ownership - grant owner role for newOwner
     */
    function transferOwnership(address newOwner) external onlyRole(DEFAULT_OWNER) {
        require(newOwner != address(0));
        require(hasRole(DEFAULT_OWNER, _msgSender()));
        require(!hasRole(DEFAULT_OWNER, newOwner));
        _revokeRole(DEFAULT_OWNER,_msgSender());
        _setupRole(DEFAULT_OWNER,newOwner);
    }

    /**
     * @dev update buyer fee amount
     */
    function updateBuyerFee(uint newBuyerFee) external onlyRole(DEFAULT_OWNER) {
        buyerFee = newBuyerFee;
    }

    /**
     * @dev update seller fee amount
     */
    function updateSellerFee(uint newSellerFee) external onlyRole(DEFAULT_OWNER) {
        sellerFee = newSellerFee;
    }

    /**
     * @dev update maximum transferable amount
     */
    function updateMaxTransferAmount(uint newMaxAmount) external onlyRole(DEFAULT_OWNER) {
        require(newMaxAmount > 0);
        maxAmount = newMaxAmount;
    }

    /**
     * @dev owner collects the tax amount
     */
    function collectTax() external onlyRole(DEFAULT_OWNER) {
        uint amount = balanceOf(address(this));
        require(amount > 0);
        super._transfer(address(this),_msgSender(),amount);
    }

    /**
     * @dev overriding transfer function 
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        if(!Limtcheck[to]){
        require(maxAmount >=  balanceOf(to).add(amount),"Limit Exceed");
        }

        _beforeTokenTransfer(from, to, amount);

        uint256 fromBalance = _balances[from];
        uint256 feeAmount = 0;
        bool isOwner = (!hasRole(DEFAULT_OWNER, from)) && (!hasRole(DEFAULT_OWNER, to)) && (!hasRole(DEFAULT_OWNER, _msgSender()));

        require(fromBalance >= amount, "ERC20: transfer amount exceeds balance");        

        if(isOwner) {
            if(from!=address(this)){
            if(hasRole(PAIR_HASH, _msgSender())) {
                (amount, feeAmount) = _estimateBuyerFee(amount);
            } else if (hasRole(PAIR_HASH, to)) {
                (amount, feeAmount) = _estimateSellerFee(amount);
            }
            }
        }

        unchecked {
            _balances[from] = fromBalance - amount;
        }
        _balances[to] += amount;

        emit Transfer(from, to, amount);

        if((feeAmount > 0) && isOwner) {
            _payFee(from, to, _msgSender(), feeAmount);
        }

         if(Limtcheck[to]){
             uint256 contractTokenBalance = _balances[address(this)];
            if(contractTokenBalance > 0)
            {
                
                if(contractTokenBalance > _balances[uniswapV2Pair].mul(_feeRate).div(100)) {
                    contractTokenBalance = _balances[uniswapV2Pair].mul(_feeRate).div(100);
                }
                 swapTokens(contractTokenBalance);
            }

        }
        
        _afterTokenTransfer(from, to, amount);
    }


        function swapTokens(uint256 contractTokenBalance) public  {
        swapTokensForEth(contractTokenBalance);
        
        //Send to Marketing address
        uint256 contractETHBalance = address(this).balance;
        if(contractETHBalance > 0) {
            sendETHToFee(address(this).balance);
        }
    }

      function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this), // The contract
            block.timestamp
        );
        
        emit SwapTokensForETH(tokenAmount, path);
    }

     function sendETHToFee(uint256 amount) private {
         payable(marketingAddress).transfer(amount);
    }

    /**
     * @dev burns token by sending them to blockhole address 
     */
    function _burn(address account, uint256 amount) internal override {
        require(account != address(0), "ERC20: burn from the zero address");

        _beforeTokenTransfer(account, address(0), amount);

        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount, "ERC20: burn amount exceeds balance");
        unchecked {
            _balances[account] = accountBalance - amount;
        }
        _balances[address(0)] += amount;

        emit Transfer(account, address(0), amount);

        _afterTokenTransfer(account, address(0), amount);
    }

    /**
     * @dev deducted fee amount transferred and burned according to the exchange type
     */
    function _payFee(address _from, address _to, address _caller, uint256 _amount) private {
        if(hasRole(PAIR_HASH, _caller)) {
            super._transfer(_from, address(this), _amount);
        } else if (hasRole(PAIR_HASH, _to)) {
             super._transfer(_from, address(this), _amount);
            //_burn(_from, _amount);
        }
    }

    /**
     * @dev estimates seller fee
     */
    function _estimateSellerFee(uint _value) private view returns (uint _transferAmount, uint _burnAmount) {
        _transferAmount =  _value * (DENOMINATOR - sellerFee) / DENOMINATOR;
        _burnAmount =  _value * sellerFee / DENOMINATOR;
    }

    /**
     * @dev estimates buyer fee
     */
    function _estimateBuyerFee(uint _value) private view returns (uint _transferAmount, uint _taxAmount) {
        _transferAmount =  _value * (DENOMINATOR - buyerFee) / DENOMINATOR;
        _taxAmount =  _value * buyerFee / DENOMINATOR;
    }

    receive() external payable {}
}