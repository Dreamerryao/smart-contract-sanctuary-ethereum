pragma solidity ^0.8.8;

import "./PriceConverter.sol";
import "hardhat/console.sol";

// to replace require, can do custom serrors.

error FundMe__NotOwner();

/** @title A contract for crowd funding
 * @author CK
 * @notice This contract is a demo of a sample funding
 * @dev This implements price feeds as our library
 */
contract FundMe {
    // Typer declarations
    using PriceConverter for uint256;

    // State variables

    uint256 public constant MINIMUM_USD = 1 * 1e18; // constant doesn't take as much cost // not storage. Part of the contract bytecode

    // for private variables that we want to return and be accessible we create getter functions at the bottom.

    AggregatorV3Interface private s_priceFeed;

    address[] private s_funders;
    mapping(address => uint256) private s_addressToAmountFunded; // append s_ to the variables names that use storage

    address private immutable i_owner; // apprend i_ to show that it's immutable. Not storage.

    modifier onlyOwner() {
        // instead of require below can use the NotOwner()
        // require(msg.sender == i_owner, "Sender is not owner");
        if (msg.sender != i_owner) {
            revert FundMe__NotOwner();
        } // saves gas b/c you don't need to emit a long string // custom error
        _;
    }

    constructor(address s_priceFeedAddress) {
        i_owner = msg.sender;
        s_priceFeed = AggregatorV3Interface(s_priceFeedAddress);
        console.log("OLD BOOMER - just ran the constructor of the FundMe");
    }

    // gets called if you're just sending value. Tx with no call data.
    receive() external payable {
        fund();
    }

    // if receive doesn't exist and there is no call data
    fallback() external payable {
        fund();
    }

    /**
     * @notice This funciton funds this contract.
     * @dev This implements price feeds as our library
     */
    function fund() public payable {
        // Want to send a min to send

        // this inow takes a parameter
        // msg.value.getConverstionRate(); // same as getConverstionRate(msg.value)

        // send at least 1 eth, otherwise revert the transaction (all prior instructions prior to the require). Undo actions and send back whatever gas left.
        // msg.value has 18 decimal places. 1 ETH = 1 plus 18 zeros.
        // original:
        // require (getConverstionRate(msg.value) >= minimumUsd, "Didn't send enough Eth"); //1e18 == 1 * 10 * 18
        // new: (note: we're not passing the variable, even though the func requires it, b/c msg.value is considered the first value for all these lib functions, can add other params, the msg.sender is assumed)

        //  with the require, we store the massive string of the error message. // massive array of text.
        require(
            msg.value.getConverstionRate(s_priceFeed) >= MINIMUM_USD,
            "Didn't send enough Eth"
        ); //1e18 == 1 * 10 * 18
        s_funders.push(msg.sender); // keep track of donors. Those who donated.
        s_addressToAmountFunded[msg.sender] = msg.value;
    }

    // using a modifier forces it to do all the actions in the modifier first.
    function withdraw() public payable onlyOwner {
        for (
            uint256 funderIndex = 0;
            funderIndex < s_funders.length;
            funderIndex++
        ) {
            address funder = s_funders[funderIndex];
            s_addressToAmountFunded[funder] = 0; //reset the address to zero
        }

        // reset the array
        s_funders = new address[](0); // brand new address array with zero address

        // transfer method of sending ETH. Issues: capped at 2300 gas. With send won't error if fails, will send a bool if it was successful or failed. Send automatically reverts.
        // msg.sender is of type address
        // payable(msg.sender) is of type payable address

        // payable(msg.sender).transfer(address(this).balance); // typecasting

        // send method of sending ETH. // need both lines

        // bool sendSuccess = payable(msg.sender).send(address(this).balance);
        // require(sendSuccess, "Send ETH failed");

        // call method of sending ETH (or native blockhain currency)
        // call returns two variables. bytes objects are arrays. We don't care about the data.
        // (bool callSuccess, bytes memory dataReturned) = payable(msg.sender).call{value: address(this).balance}("");
        (bool callSuccess, ) = payable(msg.sender).call{
            value: address(this).balance
        }("");
        require(callSuccess, "Failed to send. Call failed");
    }

    function cheaperWithdraw() public payable onlyOwner {
        // read less from storage
        // read from memory instead of reading from storage | cheaper to read.
        address[] memory funders = s_funders;
        // mappings can't be in memory.
        for (
            uint256 funderIndex = 0;
            funderIndex < funders.length;
            funderIndex++
        ) {
            address funder = funders[funderIndex];
            s_addressToAmountFunded[funder] = 0;
        }
        s_funders = new address[](0);
        (bool success, ) = i_owner.call{value: address(this).balance}("");
        require(success);
    }

    function getOwner() public view returns (address) {
        return i_owner;
    }

    function getFunder(uint256 index) public view returns (address) {
        return s_funders[index];
    }

    function getAddressToAmountFunded(address funder)
        public
        view
        returns (uint256)
    {
        return s_addressToAmountFunded[funder];
    }

    function getPriceFeed() public view returns (AggregatorV3Interface) {
        return s_priceFeed;
    }
}

