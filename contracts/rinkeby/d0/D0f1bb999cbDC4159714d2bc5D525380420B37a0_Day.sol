		require(_tokenMinted[timeZone] == false, "TimeZone already minted");
    _tokenIds.increment();
    uint256 newTokenId = _tokenIds.current();
    _safeMint(msg.sender, newTokenId);
		_tokenIdTimeZone[newTokenId] = timeZone;
		_tokenMinted[timeZone] = true;
    return newTokenId;
  }

