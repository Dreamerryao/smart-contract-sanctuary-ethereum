pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interfaces/IArmouryDockerMake.sol";
import "../interfaces/IGetRandomTrait.sol";
import "hardhat/console.sol";

contract starBaseGenerator  is Ownable{
    IArmouryDockerMake public ArmouryDockerMake;
    IGetRandomTrait public randomTraits;
    string[5] public Make = ["Lucira", "Nahla", "Yve", "Kaya", "Erixa"];
    string[4] public Docker = ["Harmonic", "Carver", "Gamov", "Ohmic"];
    string[10] public Armoury = [
        "Phase Twister",
        "Kinsey Detector",
        "Fusion Bolter",
        "Cuvier Device",
        "Margulis Tracer",
        "Ptolemy Molder",
        "Avogadro Pivoter",
        "Teller Sifter",
        "Burbank Fetcher",
        "Feynman Connector"
    ];

    string public img_start =
    '<image x="1" y="1" width="100" height="100" image-rendering="pixelated" xlink:href="data:image/png;base64,';
    string public img_end = '"/>';
    string public svg_start =
    '<svg viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">';
    string public svg_end = "</svg>";

    constructor(address armouryDockerMakeContract, address _getRandomTrait) {
        require(
            armouryDockerMakeContract != address(0),
            "Enter Vaild Address for armouryDockerMakeContract"
        );

        ArmouryDockerMake = IArmouryDockerMake(armouryDockerMakeContract);
        randomTraits = IGetRandomTrait(_getRandomTrait);
    }

    mapping (uint => uint[]) public getTraitsOfStarBase;
    mapping (address => bool) public controllers;
    mapping (address => bool) public onlyMarketplaceMapping;

    modifier onlyMarketplace {
        require (onlyMarketplaceMapping[msg.sender], 'Error: Caller Is Not Allowed To Interact');
        _;
    }

    function setOnlyMarketplace(address _marketplace) external onlyOwner {
        onlyMarketplaceMapping[_marketplace] = true;
    }

    modifier onlyControllers {
        require (controllers[msg.sender],'Error: Not Controller');
        _;
    }

    function addControllers (address _controller) external onlyOwner {
        controllers[_controller] = true;
    }

    function getStarBaseTraits (uint _tokenId) external view returns (uint[] memory) {
        return getTraitsOfStarBase[_tokenId];
    }

   function createTrait(uint _tokenId, uint randomNumber) external onlyControllers {
       uint[] memory traits = new uint[](12);
       traits[0] = randomTraits.getRandomMake(randomNumber);
       traits[1] = randomTraits.getRandomDocker(randomNumber);
       // armoury trait starting
       traits[2] = 0;
       traits[3] = 0;
       traits[4] = 0;
       traits[5] = 0;
       traits[6] = 0;
       traits[7] = 0;
       traits[8] = 0;
       traits[9] = 0;
       traits[10] = 0;
       traits[11] = 0;
       getTraitsOfStarBase[_tokenId] = traits;
   }

   function addArmouryTrait ( uint tokenId, uint position) external onlyMarketplace {
        getTraitsOfStarBase[tokenId][position] = 1;
   }
    // 5, 5 - 1, 4 => üö´, 4 - 1 => „ä£; 5 - 2 => „ä£
   function addMakeAndDockerTrait ( uint tokenId, uint position, uint attribute) external onlyMarketplace {
       require (position < 2, "Error: Invalid Position");
       if (position == 0) {
            uint[] memory starBaseTrait = getTraitsOfStarBase[tokenId];
            require (Make.length  > starBaseTrait[position],"Error: Make Max Level Already Reached");
            getTraitsOfStarBase[tokenId][position] = starBaseTrait[position]+1;
        }
       else {
           uint[] memory starBaseTrait = getTraitsOfStarBase[tokenId];
           require (Docker.length  > starBaseTrait[position],"Error: Docker Max Level Already Reached");
           getTraitsOfStarBase[tokenId][position] = starBaseTrait[position]+1;
       }
   }

    function updateTraitFromMarketplace(uint tokenId, uint traitId, uint enhancerId) external onlyMarketplace {
        getTraitsOfStarBase[tokenId][traitId] = enhancerId;
    }

   function generateStarBaseMetadata(uint tokenId) public view returns (string memory) {
      return generateMetadata(getTraitsOfStarBase[tokenId], tokenId);
   }

    function generateMetadata(uint256[] memory traits, uint256 tokenId)
        public
        view
        returns (string memory)
    {
        string memory metadata = string(
            abi.encodePacked(
                '{"name":"ChainScouts StarBase#',
                Strings.toString(tokenId),
                '","description":"Chain Scouts StarBases","attributes":[{"trait_type":"Make","value":"',
                Make[traits[0] - 1], //logic
                '"},{"trait_type":"Docker","value":"',
                Docker[traits[1] - 1], //logic
                '"}',
                metadataHelper(traits),
                '],"image":"',
                generateStarBase(traits),
                '","tokenId":',
                Strings.toString(tokenId),
                "}"
            )
        );
        console.log(metadata);
         return string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(bytes (abi.encodePacked(metadata)))
                )
            );
    }

    function metadataHelper(uint256[] memory traits)
        public
        view
        returns (string memory)
    {
        string memory armouryTratis;
        for (uint256 i = 2; i <= 11; i++) {
            if (traits[i] == 1) {
                armouryTratis = string(
                    abi.encodePacked(
                        armouryTratis,
                        ',{"trait_type":"Armoury ',
                        Strings.toString(i - 1),
                        '","value":"',
                        Armoury[i - 2],
                        '"}'
                    )
                );
            }
        }
        return armouryTratis;
    }

    function generateStarBase(uint256[] memory traits)
        public
        view
        returns (string memory)
    {
        string memory armoury;
        string memory newMakeSVG = string(
            abi.encodePacked(
                img_start,
                ArmouryDockerMake.getMake()[traits[0]-1],
                img_end
            )
        );

        string memory newDockerSVG = string(
            abi.encodePacked(
                img_start,
                ArmouryDockerMake.getDocker()[traits[1]-1],
                img_end
            )
        );

        for (uint256 i = 2; i <= 11; i++) {
            if (traits[i] == 1) {
                armoury = string(
                    abi.encodePacked(
                        armoury,
                        img_start,
                        ArmouryDockerMake.getArmoury()[i - 2],
                        img_end
                    )
                );
            }
        }

        bytes memory svg = abi.encodePacked(
            svg_start,
            newMakeSVG,
            newDockerSVG,
            armoury,
            svg_end
        );
        console.log("svg", string(svg));
        return
            string(
                abi.encodePacked(
                    "data:image/svg+xml;base64,",
                    Base64.encode(svg)
                )
            );
    }

    function setArmouryDockerMakeInterface(address ArmouryDockerMakeContract) external onlyOwner
    {
        require(
            ArmouryDockerMakeContract != address(0),
            "Enter Vaild Address for TraitNames"
        );
        ArmouryDockerMake = IArmouryDockerMake(ArmouryDockerMakeContract);
    }
}

