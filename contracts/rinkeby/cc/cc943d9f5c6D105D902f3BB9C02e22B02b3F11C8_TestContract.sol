    using SafeMathUpgradeable for uint256;

    uint256 private test1;
    uint256 private test2;

    function initialize() public initializer {
        test1 = 100;                                                                 
        test2 = 10000;
    }

    function settest1() public {
        test1 = 200;
    }

    function settest2(uint256 _add) public {
        test2 = test2.add(_add);
    }

