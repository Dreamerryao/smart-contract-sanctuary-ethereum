pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./ERC721Bound.sol";
import "./IPOAPSheet.sol";
import "./POAPLibrary.sol";
import "hardhat/console.sol";

contract POAPSheet is Ownable, ERC721Bound {
    mapping(uint256 => mapping(address => bool)) private _poapOwners;
    mapping(address => mapping(uint256 => uint256)) private _poaps;

    mapping(uint256 => POAPLibrary.Sheet) public sheets;
    mapping(address => uint256) public currentSheet;
    mapping(address => uint256) public poapsBalanceOf;
    mapping(uint256 => bytes32) public merkleRootsByPOAPId;
    mapping(uint256 => bytes32) public merkleRootsBySheetId;
    mapping(address => mapping(uint256 => bool)) public availableSheets;
    mapping(address => mapping(uint256 => uint256)) public poapPositions;

    constructor(string memory name_, string memory symbol_) ERC721Bound(name_, symbol_) {}

    function mint() external {
        _mint(msg.sender);
        availableSheets[msg.sender][1] = true;
        currentSheet[msg.sender] = 1;
    }

    function unlockSheet(uint256 sheetId, bytes32[] calldata proof) external {
        require(_minted(msg.sender), "mint required");
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(proof, merkleRootsBySheetId[sheetId], leaf), "not in whitelist");
        availableSheets[msg.sender][sheetId] = true;
    }

    function setCurrentSheet(uint256 sheetId) external {
        require(availableSheets[msg.sender][sheetId], "locked sheet");
        currentSheet[msg.sender] = sheetId;
        POAPLibrary.Sheet memory selectedSheet = sheets[sheetId];
        for (uint256 index = 0; index < selectedSheet.slots.length; index++) {
            poapPositions[msg.sender][index] = 0;
        }
    }

    function claimPOAP(uint256 id, bytes32[] calldata proof) external {
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(proof, merkleRootsByPOAPId[id], leaf), "not in whitelist");
        _claimPOAP(id);
    }

    function rearrangePOAPs(uint256[] memory slots) external {
        POAPLibrary.Sheet memory selectedSheet = sheets[currentSheet[msg.sender]];
        console.log(selectedSheet.slots.length);
        require(slots.length == selectedSheet.slots.length, "wrong size");
        for (uint256 index = 0; index < slots.length; index++) {
            uint256 poapId = slots[index];
            poapPositions[msg.sender][index] = _poapOwners[poapId][msg.sender] ? slots[index] : 0;
        }
    }

    function getAllPOAPs(address wallet) public view returns (uint256[] memory) {
        uint256 poapsCount = poapsBalanceOf[wallet];
        uint256[] memory poaps = new uint256[](poapsCount);
        for (uint256 index = 0; index < poapsCount; index++) {
            poaps[index] = _poaps[wallet][index];
        }
        return poaps;
    }

    function getSheetPOAPs(address wallet) public view returns (uint256[] memory) {
        POAPLibrary.Sheet memory selectedSheet = sheets[currentSheet[wallet]];
        uint256[] memory poaps = new uint256[](selectedSheet.slots.length);
        for (uint256 index = 0; index < poaps.length; index++) {
            poaps[index] = poapPositions[wallet][index];
        }
        return poaps;
    }

    function getCurrentSheet(address wallet) external view returns (POAPLibrary.Sheet memory) {
        return sheets[currentSheet[wallet]];
    }

    function _claimPOAP(uint256 id) internal {
        require(_minted(msg.sender), "mint required");
        require(!_poapOwners[id][msg.sender], "already owner");
        _poapOwners[id][msg.sender] = true;
        _poaps[msg.sender][poapsBalanceOf[msg.sender]] = id;
        poapsBalanceOf[msg.sender]++;
    }

    // OWNER FUNCTIONS

    function registerSheet(
        uint128 id,
        uint64 width,
        uint64 height,
        POAPLibrary.Slot[] memory slots
    ) external onlyOwner {
        require(sheets[id].id == 0, "existing sheet");
        POAPLibrary.Sheet storage newSheet = sheets[id];
        newSheet.id = id;
        newSheet.width = width;
        newSheet.height = height;
        for (uint256 index = 0; index < slots.length; index++) {
            newSheet.slots.push(slots[index]);
        }
    }

    function setMerkleRootsByPOAPId(uint256 id, bytes32 merkleRoot) external onlyOwner {
        merkleRootsByPOAPId[id] = merkleRoot;
    }

    function setMerkleRootsBySheetId(uint256 id, bytes32 merkleRoot) external onlyOwner {
        merkleRootsBySheetId[id] = merkleRoot;
    }
}
