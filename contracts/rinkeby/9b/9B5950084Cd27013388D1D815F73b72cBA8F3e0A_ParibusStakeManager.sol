pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";

import "./ParibusStakeContractV3.sol";

contract ParibusStakeManager is Ownable {
    address[] public contracts;

    function getContractsLength() public view returns (uint256) {
        return contracts.length;
    }

    function getContracts() public view returns (address[] memory) {
        return contracts;
    }

    function addContract(ParibusStakeContractV3 _contract) onlyOwner external {
        contracts.push(address(_contract));
    }

    function removeContract(ParibusStakeContractV3 _contract) onlyOwner external {
        for (uint256 it = 0; it < contracts.length; ++it) {
            if (address(_contract) == contracts[it]) {
                // if this is the only one contract, we simply remove this item
                // or if this is last element of array, we also simply remove this item
                if ((it + 1) == contracts.length) {
                    contracts.pop();
                    return;
                }

                // in any other case, we need to shuffle all elements of array in place because we can't remove item
                // from middle of array. easiest solution is to take last element and insert it into current place
                contracts[it] = contracts[contracts.length - 1];
                contracts.pop();
                return;
            }
        }

        // sorry, no such contract in array
        assert(false);
    }
}

