pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./EIP900.sol";

import "hardhat/console.sol";

contract ParibusStakeContractV3 is EIP900, Ownable, Pausable {
    struct UserShare {
        uint256 shareAmount;
        uint256 createdAt;
        uint256 paidOutAlready;
    }

    struct UserTotalCache {
        uint256 totalShareAmount;
        uint256 totalStakedAmount;
    }

    IERC20 immutable public stakingToken;
    IERC20 immutable public rewardToken;

    uint256 public fullyUnlockedAt;
    uint256 _unlockTokenAmountPerSecond;
    uint256 _lastTimeWhenUnlockedTokens;

    uint256 public amountOfShares = 0;

    uint256 public amountOfStakedTokens = 0;
    uint256 public amountOfTokensToBeUnlocked = 0;
    uint256 public amountOfTokensAlreadyUnlocked = 0;
    uint256 public amountOfPhantomTokensAlreadyUnlocked = 0;

    uint256 public counterAmountOfAlreadyClaimedTokens = 0;
    uint256 public counterAmountOfAlreadyUnlockedTokens = 0;
    uint256 public counterAmountOfTokensTransferedIntoRewardPool = 0;

    mapping (address => UserShare[]) mapOfUserToUserShare;
    mapping (address => UserTotalCache) mapOfUserToUserTotal;

    uint256 immutable public penaltyPeriod;
    uint256 immutable public penaltyNumber;
    uint256 immutable public penaltyScale;

    uint256 immutable public maximumAmountOfStakedTokenAllowed;

    string public poolName;

    constructor(
        IERC20 _stakingToken,
        IERC20 _rewardToken,
        uint256 _penaltyPeriod,
        uint256 _penaltyNumber,
        uint256 _penaltyScale,
        uint256 _maximumAmountOfStakedTokenAllowed,
        string memory _poolName
    ) {
        stakingToken = _stakingToken;
        rewardToken = _rewardToken;
        poolName = _poolName;

        penaltyPeriod = _penaltyPeriod;
        penaltyNumber = _penaltyNumber;
        penaltyScale = _penaltyScale;

        maximumAmountOfStakedTokenAllowed = _maximumAmountOfStakedTokenAllowed;

        require(_penaltyScale > 0, "ParibusStakeContractV3: Penalty scale can not be equal to 0");
    }

    function _stakeFor(address _from, address _to, uint256 _amount) whenNotPaused internal {
        unlockTokens();

        require(amountOfTokensToBeUnlocked > 0, 'ParibusStakeContractV3: no more tokens to unlock');

        uint256 newShareAmount = _amount;

        if (amountOfShares > 0 && (amountOfTokensAlreadyUnlocked + amountOfPhantomTokensAlreadyUnlocked) > 0) {
            newShareAmount = ((_amount * amountOfShares) / (amountOfTokensAlreadyUnlocked + amountOfPhantomTokensAlreadyUnlocked));
        }

        UserShare memory newShare = UserShare(newShareAmount, block.timestamp, 0);
        mapOfUserToUserShare[_to].push(newShare);

        UserTotalCache storage userTotalStorage = mapOfUserToUserTotal[_to];
        userTotalStorage.totalShareAmount += newShareAmount;
        userTotalStorage.totalStakedAmount += _amount;

        amountOfStakedTokens += _amount;
        amountOfShares += newShareAmount;

        require(amountOfStakedTokens <= maximumAmountOfStakedTokenAllowed, "ParibusStakeContractV3: maximum amount of tokens reached");

        require(
            stakingToken.transferFrom(_from, address(this), _amount),
            "ParibusStakeContractV3: transfer into staking staking pool failed"
        );

        emit Staked(_to, _amount, totalStakedFor(_to), "");
    }

    function _unstake(address _from, uint256 _amount) whenNotPaused internal {
        unlockTokens();

        require(_amount > 0, "ParibusStakeContractV3: amount is zero");
        require(totalStakedFor(_from) >= _amount, "ParibusStakeContractV3: unstake is larger then total user stakes");

        UserTotalCache storage userTotalStorage = mapOfUserToUserTotal[_from];
        uint256 amountOfSharesToUnstake = (_amount * userTotalStorage.totalShareAmount) / userTotalStorage.totalStakedAmount;

        (
            uint256 sumOfRewardShares,
            uint256 sumOfAlreadyClaimed,
            uint256 sumOfAllShares
        ) = _unstakeCleanUserShares(_from, amountOfSharesToUnstake);

        uint256 shareSumOfRewardsInTokens = _calculateRewardShareFromShare(sumOfRewardShares) - sumOfAlreadyClaimed;

        require(stakingToken.transfer(_from, _amount), "ParibusStakeContractV3: transfer out of staking pool failed");
        require(rewardToken.transfer(_from, shareSumOfRewardsInTokens), "ParibusStakeContractV3: transfer out of unlocked pool failed");

        amountOfStakedTokens -= _amount;
        amountOfTokensAlreadyUnlocked -= shareSumOfRewardsInTokens;
        counterAmountOfAlreadyClaimedTokens += shareSumOfRewardsInTokens;
        amountOfShares -= amountOfSharesToUnstake;
        amountOfPhantomTokensAlreadyUnlocked -= sumOfAlreadyClaimed;

        userTotalStorage.totalShareAmount -= sumOfAllShares;
        userTotalStorage.totalStakedAmount -= _amount;

        emit Unstaked(_from, _amount, totalStakedFor(_from), "");
        claimUnlocked();
    }

    function _unstakeCleanUserShares(address _from, uint256 _shareAmount)
        private
        returns (uint256 sumOfRewardShares, uint256 sumOfAlreadyClaimed, uint256 sumOfAllShares)
    {
        UserShare[] storage userShares = mapOfUserToUserShare[_from];

        uint256 lastShareId;
        uint256 lastShareLeftShareAmount;

        {
            uint256 sumOfFullRewardShares;
            uint256 sumOfPenaltyRewardShares;

            (
                sumOfFullRewardShares,
                sumOfPenaltyRewardShares,
                /*sumOfPenaltyLostShares*/,
                /*sumOfAllPenaltyShares*/,
                sumOfAllShares,
                sumOfAlreadyClaimed,
                lastShareId,
                lastShareLeftShareAmount
            ) = _calculateCurrentUserSharesAndRewards(_from, block.timestamp, _shareAmount);

            sumOfRewardShares = sumOfFullRewardShares + sumOfPenaltyRewardShares;
        }

        if (lastShareLeftShareAmount > 0) {
            // we need to remove lastShareId partially
            uint256 shareShareAmount = userShares[lastShareId].shareAmount;
            userShares[lastShareId].paidOutAlready = lastShareLeftShareAmount * (userShares[lastShareId].paidOutAlready / userShares[lastShareId].shareAmount);
            userShares[lastShareId].shareAmount = lastShareLeftShareAmount;

            emit UpdateShare(_from, lastShareId, shareShareAmount - lastShareLeftShareAmount, userShares[lastShareId].shareAmount, userShares[lastShareId].paidOutAlready);
        }

        uint256 sharesToPop = 0;
        if (lastShareLeftShareAmount > 0 || (lastShareId + 1) < userShares.length) {
            // if there is any share left in lastShareId or we have more shares after current one, then we need to
            // move shares around

            // in case we are in first share, and there is something left there, we can skip this step
            if (!(lastShareId == 0 && lastShareLeftShareAmount > 0)) {
                if (lastShareLeftShareAmount == 0) {
                    // if we don't have anything left in share, we can skip this index
                    lastShareId++;
                    sharesToPop++;
                }

                for (uint256 it = lastShareId; it < userShares.length; ++it) {
                    // we move elements around
                    userShares[it - lastShareId].createdAt = userShares[it].createdAt;
                    userShares[it - lastShareId].paidOutAlready = userShares[it].paidOutAlready;
                    userShares[it - lastShareId].shareAmount = userShares[it].shareAmount;

                    emit ShareDeleted(_from, it - lastShareId);
                    emit ShareMoved(_from, lastShareId, it - lastShareId);

                    sharesToPop++;
                }
            }
        } else {
            // in this case we remove all shares
            sharesToPop = userShares.length;

            emit AllSharesDeleted(_from);
        }

        for (; sharesToPop > 0; sharesToPop--) {
            userShares.pop();
        }
    }

    function unlockTokens() whenNotPaused public {
        uint256 time = block.timestamp;

        (
            /*uint256 currentTimestamp*/,
            uint256 totalTokensUnlocked,
            uint256 totalTokensLocked,
            uint256 tokensUnlockedPerSec
        ) = _calculatePotentialAdvanceAt(time);

        if (totalTokensUnlocked != amountOfTokensAlreadyUnlocked) {
            uint256 unlockedTokens = totalTokensUnlocked - amountOfTokensAlreadyUnlocked;
            counterAmountOfAlreadyUnlockedTokens += unlockedTokens;

            amountOfTokensAlreadyUnlocked = totalTokensUnlocked;
        }

        if (totalTokensLocked != amountOfTokensToBeUnlocked) {
            amountOfTokensToBeUnlocked = totalTokensLocked;
        }

        if (tokensUnlockedPerSec != _unlockTokenAmountPerSecond) {
            _unlockTokenAmountPerSecond = tokensUnlockedPerSec;

            emit UnlockedTokensChangedRewardPerSecond(_unlockTokenAmountPerSecond);
        }

        _lastTimeWhenUnlockedTokens = time;
    }

    function stake(uint256 amount, bytes calldata /* data */) override external {
        _stakeFor(msg.sender, msg.sender, amount);
    }

    function stakeFor(address user, uint256 amount, bytes calldata /* data */) override external {
        _stakeFor(msg.sender, user, amount);
    }

    function unstake(uint256 amount, bytes calldata /* data */) override external {
        _unstake(msg.sender, amount);
    }

    function totalStakedFor(address _user) override public view returns (uint256) {
        return mapOfUserToUserTotal[_user].totalStakedAmount;
    }

    function totalStaked() override external view returns (uint256) {
        return amountOfStakedTokens;
    }

    function token() override external view returns (address) {
        return address(stakingToken);
    }

    function supportsHistory() override external pure returns (bool) {
        // if you want history, fetch data at older block
        return false;
    }

    function rewardTransferIntoPool(uint256 _amount) whenNotPaused onlyOwner external {
        if (amountOfTokensToBeUnlocked > 0) {
            unlockTokens();
        } else {
            _lastTimeWhenUnlockedTokens = block.timestamp;
        }

        require(_amount > 0, 'ParibusStakeContractV3: you can not transfer 0 tokens');
        require(_unlockTokenAmountPerSecond > 0, 'ParibusStakeContractV3: pool was never opened');
        require(fullyUnlockedAt > block.timestamp, 'ParibusStakeContractV3: pool is already completely unlocked');

        require(
            rewardToken.transferFrom(msg.sender, address(this), _amount),
            "ParibusStakeContractV3: transfer into reward pool failed"
        );

        amountOfTokensToBeUnlocked += _amount;
        counterAmountOfTokensTransferedIntoRewardPool += _amount;
        _unlockTokenAmountPerSecond = amountOfTokensToBeUnlocked / (fullyUnlockedAt - block.timestamp);

        // This should not happen, since we are increasing amountOfTokensToBeUnlocked so in worst case scenario
        // _unlockTokenAmountPerSecond would be the same before calling this method. But just to be sure
        assert(_unlockTokenAmountPerSecond > 0);

        emit RewardPoolTokensTransfered(msg.sender, _amount, amountOfTokensToBeUnlocked, _unlockTokenAmountPerSecond);
    }

    function rewardChangeEndTime(uint256 _endTimestamp) whenNotPaused onlyOwner external {
        if (amountOfTokensToBeUnlocked > 0) {
            unlockTokens();
        } else {
            _lastTimeWhenUnlockedTokens = block.timestamp;
        }

        require(_unlockTokenAmountPerSecond > 0, 'ParibusStakeContractV3: pool was never opened');
        require(amountOfTokensToBeUnlocked > 0, 'ParibusStakeContractV3: pool is empty');
        require(_endTimestamp > block.timestamp, 'ParibusStakeContractV3: pool time is set in the past');

        uint256 ogFullyUnlocked = fullyUnlockedAt;
        fullyUnlockedAt = _endTimestamp;
        _unlockTokenAmountPerSecond = amountOfTokensToBeUnlocked / (fullyUnlockedAt - block.timestamp);
        require(_unlockTokenAmountPerSecond > 0, 'ParibusStakeContractV3: can not unlock zero tokens per second');

        emit RewardPoolTimeSet(msg.sender, (_endTimestamp - ogFullyUnlocked), fullyUnlockedAt, _unlockTokenAmountPerSecond);
    }

    function rewardTransferAndChange(uint256 _amount, uint256 _endTimestamp) whenNotPaused onlyOwner external {
        if (amountOfTokensToBeUnlocked > 0) {
            unlockTokens();
        } else {
            _lastTimeWhenUnlockedTokens = block.timestamp;
        }

        uint256 ogFullyUnlocked = fullyUnlockedAt;
        fullyUnlockedAt = _endTimestamp;

        require(fullyUnlockedAt > block.timestamp, 'ParibusStakeContractV3: fully open time is set in the past');
        require(_amount > 0, 'ParibusStakeContractV3: can not transfer zero tokens to reward pool');

        require(
            rewardToken.transferFrom(msg.sender, address(this), _amount),
            "ParibusStakeContractV3: transfer into reward pool failed"
        );

        counterAmountOfTokensTransferedIntoRewardPool += _amount;
        amountOfTokensToBeUnlocked += _amount;
        _unlockTokenAmountPerSecond = amountOfTokensToBeUnlocked / (fullyUnlockedAt - block.timestamp);
        assert(_unlockTokenAmountPerSecond * (fullyUnlockedAt - block.timestamp) <= amountOfTokensToBeUnlocked);

        emit RewardPoolTokensTransfered (msg.sender, _amount , amountOfTokensToBeUnlocked, _unlockTokenAmountPerSecond);
        emit RewardPoolTimeSet          (msg.sender,  (_endTimestamp - ogFullyUnlocked), fullyUnlockedAt, _unlockTokenAmountPerSecond);
    }

    function getPoolInfo()
        external view
        returns (
            string memory name,
            uint256 penaltyPercent,
            uint256 penaltyLockupPeriod,
            uint256 maximumStakedTokenAllowed,
            uint256 unlockedAt
        )
    {
        return (
            poolName,
            100 - uint32(penaltyNumber / 10 ** (penaltyScale - 1)),
            penaltyPeriod,
            maximumAmountOfStakedTokenAllowed,
            fullyUnlockedAt
        );
    }

    function getPoolState()
        external view
        returns (
            uint256 stakedTokenCurrentAmount,
            uint256 updateLastTime,
            uint256 updateUnlocksPerSecond,
            uint256 rewardTokenFromAllTimeToDistribute,
            uint256 rewardTokenTotalClaimedRewards,
            uint256 rewardTokenTotalUnlockedTokens,
            uint256 rewardTokensCurrentlyUnlocked,
            uint256 rewardTokensCurrentlyToBeUnlocked
        )
    {
        updateLastTime = _lastTimeWhenUnlockedTokens;
        updateUnlocksPerSecond = _unlockTokenAmountPerSecond;

        rewardTokenFromAllTimeToDistribute = counterAmountOfTokensTransferedIntoRewardPool;
        rewardTokenTotalClaimedRewards = counterAmountOfAlreadyClaimedTokens;
        rewardTokenTotalUnlockedTokens = counterAmountOfAlreadyUnlockedTokens;

        rewardTokensCurrentlyUnlocked = amountOfTokensAlreadyUnlocked;
        rewardTokensCurrentlyToBeUnlocked = amountOfTokensToBeUnlocked;

        stakedTokenCurrentAmount = amountOfStakedTokens;
    }

    function emergencyWithdrawn() onlyOwner whenPaused external {
        uint256 stakingAmount = 0;
        uint256 rewardAmount = 0;

        if (rewardToken == stakingToken) {
            stakingAmount = rewardToken.balanceOf(address(this));
            require(rewardToken.transfer(msg.sender, stakingAmount), 'ParibusStakeContractV3: failed to withdrawn tokens');
        } else {
            stakingAmount = stakingToken.balanceOf(address(this));
            rewardAmount = rewardToken.balanceOf(address(this));

            require(stakingToken.transfer(msg.sender, stakingAmount), 'ParibusStakeContractV3: failed to withdrawn staking tokens');
            require(rewardToken.transfer(msg.sender, rewardAmount), 'ParibusStakeContractV3: failed to withdrawn reward tokens');
        }

        emit EmergencyWithdrawn(msg.sender, stakingAmount, rewardAmount);
    }

    function pause() onlyOwner whenNotPaused external {
        _pause();
    }

    function unpause() onlyOwner whenPaused external {
        _unpause();
    }

    function getUserRewardInformationFor(address _user)
        view public
        returns (
            uint256 currentTimestamp,
            uint256 stakedTokenPenaltyFree,
            uint256 rewardAmountPenaltyFree,
            uint256 stakedTokenPenalty,
            uint256 rewardAmountAfterPenalty,
            uint256 rewardAmountLost,
            uint256 rewardPenaltyTimestamp
    ) {
        return _getUserRewardInformationFor(_user, block.timestamp);
    }

    function _getUserRewardInformationFor(address _user, uint256 _time)
        internal view
        returns (
            uint256 currentTimestamp,
            uint256 stakedTokenPenaltyFree,
            uint256 rewardAmountPenaltyFree,
            uint256 stakedTokenPenalty,
            uint256 rewardAmountAfterPenalty,
            uint256 rewardAmountLost,
            uint256 rewardPenaltyTimestamp
        )
    {
        require(_time >= _lastTimeWhenUnlockedTokens, 'ParibusStakeContractV3: this contract does not support calculation before snapshot');

        // we need to prevent 16 variable limit, so we must do some shanenigans with variable declaration
        uint256 sumOfFullRewardShares;
        uint256 sumOfPenaltyRewardShares;
        uint256 sumOfPenaltyLostShares;
        uint256 sumOfAllPenaltyShares;

        {
            // to fetch this information we need to go through all user shares to determine what can be received penalty free
            // and what need to be paid
            uint256 userTotalShareAmount = mapOfUserToUserTotal[_user].totalShareAmount;

            if (userTotalShareAmount == 0) {
                return (_time, 0, 0, 0, 0, 0, 0);
            }

            (
                sumOfFullRewardShares,
                sumOfPenaltyRewardShares,
                sumOfPenaltyLostShares,
                sumOfAllPenaltyShares,
                /* sumOfAlreadyClaimed */rewardAmountAfterPenalty,
                /* sumOfAllShares */,
                /* lastShareId */rewardPenaltyTimestamp
            ) = _calculateCurrentUserSharesAndRewards(_user, _time);
        }

        {
            uint256 userTotalStakedAmount = mapOfUserToUserTotal[_user].totalStakedAmount;

            (
                currentTimestamp,
                /* total tokens unlocked */rewardAmountLost,
                /* tokens locked */,
                /* tokens per second */
            ) = _calculatePotentialAdvanceAt(_time);


            stakedTokenPenaltyFree = _calculateStakedTokensFromShares(sumOfFullRewardShares, sumOfFullRewardShares + sumOfAllPenaltyShares, userTotalStakedAmount);
            rewardAmountPenaltyFree = _calculateRewardShareFromShare(sumOfFullRewardShares, rewardAmountLost, amountOfShares) - rewardAmountAfterPenalty;
            stakedTokenPenalty = _calculateStakedTokensFromShares(sumOfAllPenaltyShares, sumOfFullRewardShares + sumOfAllPenaltyShares, userTotalStakedAmount);
            rewardAmountAfterPenalty = _calculateRewardShareFromShare(sumOfPenaltyRewardShares, rewardAmountLost, amountOfShares);
            rewardAmountLost = _calculateRewardShareFromShare(sumOfPenaltyLostShares, rewardAmountLost, amountOfShares);
        }

        rewardPenaltyTimestamp = mapOfUserToUserShare[_user][rewardPenaltyTimestamp].createdAt + penaltyPeriod;
    }

    function _calculateShareRewardFromShare(UserShare storage currentShare, uint256 oldestPenaltyTimestamp)
        private view
        returns (
            uint256 rewardShare,
            uint256 penaltyShare
        )
    {
        return _calculateShareRewardFromShare(currentShare, oldestPenaltyTimestamp, currentShare.shareAmount);
    }

    function _calculateShareRewardFromShare(UserShare storage currentShare, uint256 oldestPenaltyTimestamp, uint256 _shareAmount)
        private view
        returns (
            uint256 rewardShare,
            uint256 penaltyShare
        )
    {
        _shareAmount = _shareAmount > currentShare.shareAmount ? currentShare.shareAmount: _shareAmount;

        // before we unstake our tokens, we need to pay our taxes, so lets check if IRS is watching
        if (currentShare.createdAt >= oldestPenaltyTimestamp) {
            // sadly for you, they are.
            // we increment scale by one, because in this case we need to start from 2 and not 1
            rewardShare = (_shareAmount * penaltyNumber) / 10 ** (penaltyScale + 1);
            penaltyShare = (_shareAmount - rewardShare);
        } else {
            rewardShare = _shareAmount;
            penaltyShare = 0;
        }
    }

    function _calculateStakedTokensFromShares(uint256 _share, uint256 _userTotalShare, uint256 _userStakedAmount)
        private pure returns (uint256)
    {
        return _share * _userStakedAmount / _userTotalShare;
    }

    function _calculateRewardShareFromShare(uint256 _share)
        private view returns (uint256)
    {
        return _calculateRewardShareFromShare(_share, amountOfTokensAlreadyUnlocked, amountOfShares, amountOfPhantomTokensAlreadyUnlocked);
    }

    function _calculateRewardShareFromShare(uint256 _share, uint256 _alreadyUnlockedTokens, uint256 _amountOfShares)
        private view returns (uint256)
    {
        return _calculateRewardShareFromShare(_share, _alreadyUnlockedTokens, _amountOfShares, amountOfPhantomTokensAlreadyUnlocked);
    }

    function _calculateRewardShareFromShare(uint256 _share, uint256 _alreadyUnlockedTokens, uint256 _amountOfShares, uint256 _alreadyUnlockedPhantomTokens)
        private pure returns (uint256)
    {
        return _share * (_alreadyUnlockedTokens + _alreadyUnlockedPhantomTokens) / _amountOfShares;
    }

    function _getOldestPenaltyTimestamp() private view returns (uint256) {
        return _getOldestPenaltyTimestamp(block.timestamp);
    }

    function _getOldestPenaltyTimestamp(uint256 _currentTime) private view returns (uint256) {
        return amountOfTokensToBeUnlocked > 0 ?
            _currentTime - penaltyPeriod :
            _currentTime;

    }

    function _calculatePotentialAdvanceAt(uint256 _time)
        internal view
        returns (uint256 currentTimestamp, uint256 totalTokensUnlocked, uint256 totalTokensLocked, uint256 tokensUnlockedPerSec)
    {
        return _calculatePotentialAdvanceAt(
            _time,
            amountOfStakedTokens,
            amountOfTokensAlreadyUnlocked,
            amountOfTokensToBeUnlocked,
            _unlockTokenAmountPerSecond,
            _lastTimeWhenUnlockedTokens
        );
    }

    function _calculatePotentialAdvanceAt(
        uint256 _time,
        uint256 _amountOfTokensStaked,
        uint256 _amountOfTokensAlreadyUnlocked,
        uint256 _amountOfTokensToBeUnlocked,
        uint256 _unlockTokensPerSecond,
        uint256 _lastTime
    )
        internal view
        returns (
            uint256 currentTimestamp,
            uint256 totalTokensUnlocked,
            uint256 totalTokensLocked,
            uint256 tokensUnlockedPerSec
        )
    {
        if (_time == _lastTime) {
            // we already updated this information in this block, so let's bail out quick
            return (
                _time,
                _amountOfTokensAlreadyUnlocked,
                _amountOfTokensToBeUnlocked,
                _unlockTokensPerSecond
            );
        }

        if (_amountOfTokensStaked > 0) {
            if (_time >= fullyUnlockedAt) {
                return (
                    _time,
                    _amountOfTokensAlreadyUnlocked + _amountOfTokensToBeUnlocked,
                    0,
                    _unlockTokensPerSecond
                );
            } else {
                uint256 amountToBeUnlocked = _unlockTokensPerSecond * (_time - _lastTime);

                return (
                    _time,
                    _amountOfTokensAlreadyUnlocked + amountToBeUnlocked,
                    _amountOfTokensToBeUnlocked - amountToBeUnlocked,
                    _unlockTokensPerSecond
                );
            }
        } else {
            // in case that we are already at fullyUnlockedAt time, we won't do anything as first user will unlock
            // everything. This is edge case and we really can't do anything about it
            if (_time < fullyUnlockedAt) {
                return (
                    _time,
                    _amountOfTokensAlreadyUnlocked,
                    _amountOfTokensToBeUnlocked,
                    _amountOfTokensToBeUnlocked / (fullyUnlockedAt - _time)
                );
            }
        }

        return (_time, _amountOfTokensAlreadyUnlocked, _amountOfTokensToBeUnlocked, _unlockTokensPerSecond);
    }

    function _calculateCurrentUserSharesAndRewards(address _user, uint256 _time)
        internal view
        returns (
            uint256 sumOfFullRewardShares,
            uint256 sumOfPenaltyRewardShares,
            uint256 sumOfPenaltyLostShares,
            uint256 sumOfAllPenaltyShares,
            uint256 sumOfAlreadyClaimed,
            uint256 sumOfAllShares,
            uint256 lastShareId
        )
    {
        (
            sumOfFullRewardShares,
            sumOfPenaltyRewardShares,
            sumOfPenaltyLostShares,
            sumOfAllPenaltyShares,
            sumOfAllShares,
            sumOfAlreadyClaimed,
            lastShareId,
            /* lastShareLeftShareAmount */
        ) = _calculateCurrentUserSharesAndRewards(_user, _time, mapOfUserToUserTotal[_user].totalShareAmount);
    }

    function _calculateCurrentUserSharesAndRewards(address _user, uint256 _time, uint256 _shareAmount)
        internal view
        returns (
            uint256 sumOfFullRewardShares,
            uint256 sumOfPenaltyRewardShares,
            uint256 sumOfPenaltyLostShares,
            uint256 sumOfAllPenaltyShares,
            uint256 sumOfAllShares,
            uint256 sumOfAlreadyClaimed,
            uint256 lastShareId,
            uint256 lastShareLeftShareAmount
        )
    {
        uint256 oldestPenaltyTimestamp = _getOldestPenaltyTimestamp(_time);

        for (lastShareId = 0; lastShareId < mapOfUserToUserShare[_user].length; ++lastShareId) {
            UserShare storage currentShare = mapOfUserToUserShare[_user][lastShareId];

            (uint256 thisShareReward, uint256 thisSharePenalty) = _calculateShareRewardFromShare(currentShare, oldestPenaltyTimestamp, _shareAmount);

            if (_shareAmount <= currentShare.shareAmount) {
                lastShareLeftShareAmount = currentShare.shareAmount - _shareAmount;
            }

            if (thisSharePenalty == 0) {
                sumOfFullRewardShares += thisShareReward;
            } else {
                sumOfPenaltyRewardShares += thisShareReward;
                sumOfPenaltyLostShares += thisSharePenalty;
                sumOfAllPenaltyShares += thisShareReward + thisSharePenalty;
            }

            sumOfAllShares += thisShareReward + thisSharePenalty;
            _shareAmount -= thisShareReward + thisSharePenalty;
            sumOfAlreadyClaimed += currentShare.paidOutAlready;

            if (_shareAmount == 0) {
                break;
            }
        }
    }

    function simulateStake(uint256 _amount, uint256 _time)
        external view
        returns (
            uint256 currentTimestamp,
            uint256 rewardFull
        )
    {
        uint256 totalTokensUnlocked;
        uint256 totalTokensLocked;
        uint256 tokensUnlockedPerSec;

        (
            currentTimestamp,
            totalTokensUnlocked,
            totalTokensLocked,
            tokensUnlockedPerSec
        ) = _calculatePotentialAdvanceAt(block.timestamp);

        require(amountOfTokensToBeUnlocked > 0, 'ParibusStakeContractV3: no more tokens to unlock');

        uint256 newShareAmount = totalTokensUnlocked > 0 ?
            ((_amount * amountOfShares) / totalTokensUnlocked) :
            _amount;

        uint256 amountOfStakedTokensSim = amountOfStakedTokens + _amount;
        uint256 amountOfSharesSim = amountOfShares + _amount;

        require(amountOfStakedTokensSim <= maximumAmountOfStakedTokenAllowed, "ParibusStakeContractV3: maximum amount of tokens reached");

        (
            currentTimestamp,
            totalTokensUnlocked,
            totalTokensLocked,
            tokensUnlockedPerSec
        ) = _calculatePotentialAdvanceAt(
            block.timestamp + _time,
            amountOfStakedTokensSim,
            totalTokensUnlocked,
            totalTokensLocked,
            tokensUnlockedPerSec,
            block.timestamp
        );

        return (
            currentTimestamp,
            _calculateRewardShareFromShare(newShareAmount, totalTokensUnlocked, amountOfSharesSim)
        );
    }

    function simulateUnstake(address _user, uint256 _amount)
        external view
        returns (
            uint256 currentTimestamp,
            uint256 reward,
            uint256 penalty,
            uint256 unlockTimestamp
        )
    {
        uint256 totalTokensUnlocked;
        uint256 amountOfSharesToUnstake;

        (
            currentTimestamp,
            totalTokensUnlocked,
            /*totalTokensLocked*/,
            /*tokensUnlockedPerSec*/
        ) = _calculatePotentialAdvanceAt(block.timestamp);

        require(_amount > 0, "ParibusStakeContractV3: amount is zero");
        require(totalStakedFor(_user) >= _amount, "ParibusStakeContractV3: unstake is larger then total user stakes");

        {
            UserTotalCache storage userTotalStorage = mapOfUserToUserTotal[_user];

            if (userTotalStorage.totalStakedAmount == 0) {
                return (currentTimestamp, 0, 0, 0);
            }

            amountOfSharesToUnstake = (_amount * userTotalStorage.totalShareAmount) / userTotalStorage.totalStakedAmount;
        }

        (
            uint256 sumOfFullRewardShares,
            uint256 sumOfPenaltyRewardShares,
            uint256 sumOfPenaltyLostShares,
            /*uint256 sumOfAllPenaltyShares*/,
            /*uint256 sumOfAllShares*/,
            uint256 sumOfAlreadyClaimed,
            uint256 lastShareId,
            /*uint256 lastShareLeftShareAmount*/
        ) = _calculateCurrentUserSharesAndRewards(_user, block.timestamp, amountOfSharesToUnstake);

        reward = sumOfFullRewardShares + sumOfPenaltyRewardShares;
        penalty = sumOfPenaltyLostShares;

        UserShare storage lastUserShare = mapOfUserToUserShare[_user][lastShareId];
        unlockTimestamp = lastUserShare.createdAt + penaltyPeriod;

        reward = _calculateRewardShareFromShare(reward, totalTokensUnlocked, amountOfShares) - sumOfAlreadyClaimed;
        penalty = _calculateRewardShareFromShare(penalty, totalTokensUnlocked, amountOfShares);
    }

    function claimUnlocked() public {
        unlockTokens();

        UserShare[] storage allUserShares = mapOfUserToUserShare[msg.sender];
        uint256 oldestTimestamp = _getOldestPenaltyTimestamp();
        uint256 fullRewardShare = 0;
        uint256 amountAlreadyClaimed = 0;

        if (allUserShares.length == 0) {
            return;
        }

        for (uint256 it = 0; it < allUserShares.length; ++it) {
            UserShare storage currentShare = allUserShares[it];

            if (currentShare.createdAt >= oldestTimestamp) {
                // if we have share that is newer than our oldest possible penalty-free share, we can finish early
                break;
            }

            fullRewardShare += currentShare.shareAmount;
            amountAlreadyClaimed += currentShare.paidOutAlready;

            uint256 amountToClaim = _calculateRewardShareFromShare(currentShare.shareAmount);

            emit ClaimedTokensFromShare(msg.sender, it, amountToClaim - currentShare.paidOutAlready, amountToClaim);

            currentShare.paidOutAlready = amountToClaim;
        }

        uint256 amountToPay = _calculateRewardShareFromShare(fullRewardShare);
        amountToPay -= amountAlreadyClaimed;

        if (amountToPay == 0) {
            return;
        }

        require(
            rewardToken.transfer(msg.sender, amountToPay),
            'ParibusStakeContractV3: transfer from reward pool failed!'
        );
        amountOfTokensAlreadyUnlocked -= amountToPay;
        amountOfPhantomTokensAlreadyUnlocked += amountToPay;
    }

    event RewardPoolTokensTransfered (address indexed sender, uint256 amountAdded, uint256 value, uint256 rewardPerSecond);
    event RewardPoolTimeSet          (address indexed sender, uint256 amountAdded, uint256 value, uint256 rewardPerSecond);
    event RewardPoolTimeExtended     (address indexed sender, uint256 amountAdded, uint256 value, uint256 rewardPerSecond);
    event EmergencyWithdrawn(address indexed sender, uint256 stakingToken, uint256 rewardToken);
    event UnlockedTokensChangedRewardPerSecond(uint256 rewardPerSecond);

    event ClaimedTokensFromShare(address indexed sender, uint256 indexed shareId, uint256 claimed, uint256 sumOfClaimed);
    event UpdateShare(address indexed shareHolder, uint256 indexed shareId, uint256 shareChange, uint256 shareAbsolute, uint256 sumOfClaimed);
    event ShareCreated(address indexed shareHolder, uint256 amount, uint256 share);
    event ShareMoved(address indexed shareHolder, uint256 indexed newShareId, uint256 indexed oldShareId);
    event ShareDeleted(address indexed shareHolder, uint256 indexed shareId);
    event AllSharesDeleted(address indexed shareHolder);
}

