                rewards = _onFightWon(initH, finalH, 100);
                if (_experience >= (_level * 100)){
                    sendUpdate(self, _health, _damage, 0, _level + 1, _skillPoints, _charges - 1);
                    sendTime(self, _lastCheck);
                }
                else{
                    sendUpdate(self, _health, _damage, _experience + 10, _level, _skillPoints, _charges - 1);
                    sendTime(self, _lastCheck);
                }
            } 
            else {
                uint newHealth = _health -10;
                if (_experience >= 100){
                    sendUpdate(self, _health, _damage, 0, _level + 1, _skillPoints, _charges - 1);
                    sendTime(self, _lastCheck);
                }
                else{
                    sendUpdate(self, _health, _damage, _experience + 5, _level, _skillPoints, _charges - 1);
                    sendTime(self, _lastCheck);
                }

                //Burn NFT if hp below zero
                if (newHealth <= 0){
                    //Burn NFT
                    nftContract.transferFrom(msg.sender, address(0x000000000000000000000000000000000000dEaD), self);
                    emit Death(msg.sender, self);
                }
            }
            emit NewFight(msg.sender, self, target, won, rewards, finalH);
        }  

    function _onFightWon(uint256 initH, uint256 finalH, uint256 rewardPercentage) private returns(uint256) {
        if (cdrFee[msg.sender] == 0) {
            cdrFee[msg.sender] = block.timestamp;
        }
        
        uint difficulty = 100; // 10% winnings base
        if (finalH <= initH.div(10)) { // less than 10% health
        difficulty = 2000; // 2x payout
        } else if (finalH <= initH.div(4)) { // less than 25% health
        difficulty = 1000; // 1x payout
        } else if (finalH <= initH.div(2)) { // less than 50% health
        difficulty = 500; // 0,5x payout
        } else if (finalH <= initH.mul(3).div(4)) { // less than 75% health 
        difficulty = 250; // 25% payout
        }

        uint256 reward = priceOracle.convertReward(mineRate).mul(difficulty).div(1000).mul(rewardPercentage * 10).div(1000);
        tokenRewards[msg.sender] = tokenRewards[msg.sender].add(reward);
        battleWagerToken.transfer(msg.sender, reward);
        return reward;
    }


    function _performFight(uint256 self, uint256 target) private returns(bool won, uint256 ih, uint256 fh) {
        getStats(self);
        _lastCheck = getTime(self);
        checkCharges();
        require(_charges > 0, "Not enough chareges to battle");
        uint256 attDmg = _damage;
        int256 attHth = int256(_health);
        uint256 strtHth = uint256(_health);
        getStats(target);
        uint256 trgDmg = _damage;
        int256 trgHth = int256(_health);
        require(attHth > 0 && trgHth > 0, "health overflow");

        seed = nonce.rand(1, nonce);
        while (attHth > 0 && trgHth > 0) { // fight.
          uint attRoll = seed.rand(attDmg, attDmg.mul(1500).div(1000));
          seed = seed.combine(attRoll);
          uint trgRoll = seed.rand(trgDmg, trgDmg.mul(1500).div(1000)); 
          seed = seed.combine(trgRoll);
          attHth -= int256(trgDmg);

          if (attHth <= 0) {
              continue;
          }
          trgHth -= int256(attDmg);
        }


        uint256 finalH = 0;
        if (attHth > 0) { // consider underflows as zero health
        finalH = uint256(attHth);
        }
        return(
        attHth >= 0,
        strtHth,
        finalH
        );
    }



    function setBattleToken(address _address) public onlyOwner{
        battleWagerToken = IERC20(_address);

    }

    function flipPause() public onlyOwner{
        paused = !paused;

    }

    // saftey functions to prevent contracts and nonowner calls
    modifier onlyNonContract() {
        require(tx.origin == msg.sender, "Contracts not allowed");
        _;
    }

    modifier characterOf(uint256 target) {
        require(nftContract.ownerOf(target) == msg.sender, "sender not owner");
        _;
    }






