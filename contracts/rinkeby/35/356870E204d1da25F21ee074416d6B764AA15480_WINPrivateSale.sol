import {IDistributeERC20} from "../../../interfaces/IDistributeERC20.sol";

contract WINPrivateSale is Ownable {
    using ECDSA for bytes32;
    using SafeMath for uint256;

    struct Round {
        uint256 maximum;
        uint256 priceStart;
        uint256 vestingPeriod;
        uint256 percentageTGE;
        uint256 extraLockupDays;
        bool active;
    }

    struct Pool {
        uint256 amount;
        bool unrestricted;
    }
    
    uint256[] public poolSteps;
    uint256 public poolStepsAmount;

    uint256 public constant NORMALIZING_DECIMALS = 36;

    mapping(string => address) public acceptedTokens;
    mapping(string => uint8) public acceptedTokenDecimals;
    
    address public immutable signer;

    address public immutable wallet;

    bool public poolsInitialized = false;

    Round[] public rounds;

    mapping(uint8 => mapping(address => uint256)) public investments;

    mapping(uint8 => mapping(address => Pool)) public pools;

    mapping(uint8 => uint256) public roundsTotalInvestments;

    mapping(bytes32 => address) public codeToInvitee;
    mapping(address => bytes32) public inviteeToCode;
    mapping(address => address) public walletToPool;

    uint256 public maximumPoolAllowance;
    uint256 public roundsAmount;

    uint256[] private maximumRebalance;

    address[] public investors;
    uint256 public numberOfInvestors;

    constructor(
        address _signer,
        address _wallet,
        string[] memory tokenNames,
        address[] memory tokenAddreses,
        uint256[] memory roundLimits,
        uint256[] memory chainCountPoolSize
    ) {
        require(
            tokenNames.length == tokenAddreses.length,
            "PrivateSale: Invalid accepted tokens length"
        );
        signer = _signer;
        wallet = _wallet;
        roundsAmount = roundLimits.length;

        maximumPoolAllowance = chainCountPoolSize[1] * 10**NORMALIZING_DECIMALS;

        for (uint256 i = 0; i < tokenNames.length; i++) {
            acceptedTokens[tokenNames[i]] = address(tokenAddreses[i]);
            acceptedTokenDecimals[tokenNames[i]] = IERC20Metadata(address(tokenAddreses[i])).decimals();
        }

        for (uint256 i = 0; i < roundLimits.length; i++) {
            rounds.push(
                Round(roundLimits[i] * 10**NORMALIZING_DECIMALS, 0, 0, 0, 0, false)
            );
            maximumRebalance.push(rounds[i].maximum * chainCountPoolSize[0]);
        }
    }

    function initPools (
        uint256[] memory roundPriceStart, 
        uint256[] memory _poolSteps, 
        uint256[] memory vestingPeriod,
        uint256[] memory percentageTGE,
        uint256[] memory extraLockupDays
        ) public {
        require(!poolsInitialized, "PrivateSale: Pools already initialized");
        require(_poolSteps[_poolSteps.length-1] == (maximumPoolAllowance / 10**NORMALIZING_DECIMALS), 
            "The last poolStep is not equal to the max pool amount.");
        require(rounds.length == roundPriceStart.length, 
            "Amount of rounds is not equal to amount of round startprices");
        require(rounds.length == vestingPeriod.length,
            "Amount of rounds is not equal to amount of round vestingPeriods");
        require(rounds.length == percentageTGE.length,
            "Amount of rounds is not equal to amount of round percentageTGEs");
        require(rounds.length == extraLockupDays.length,
        "Amount of rounds is not equal to amount of round extraLockupDays");

        for(uint i = 0; i < roundPriceStart.length; i++){
            rounds[i].priceStart = (roundPriceStart[i] * 10**15);
            rounds[i].vestingPeriod = vestingPeriod[i] * 1 days;
            rounds[i].percentageTGE = percentageTGE[i];
            rounds[i].extraLockupDays = extraLockupDays[i];
        }

        poolStepsAmount = _poolSteps.length;
        poolSteps = new uint256[](poolStepsAmount);
        for(uint i = 0; i < _poolSteps.length; i++) {
            poolSteps[i] = (_poolSteps[i] * 10**18);
        }
        poolsInitialized = true;
    }

    function getInvestors() public view returns(address[] memory) {
        return investors;
    }

    function getAnyRoundActive() public view returns(bool) {
        for(uint i = 0; i < roundsAmount; i++){
            if(rounds[i].active == true){
                return true;
            }
        }
        return false;
    }

    function enableRound(uint8 round, bool enable) public onlyOwner {
        require(_verifyRound(round, false), "PrivateSale: Invalid round");
        require(
            rounds[round].active == !enable,
            "PrivateSale: Round already has this state"
        );
        rounds[round].active = enable;
    }

    // Used by people invited by root wallet and NOT by pool investors
    function investWithCode(
        bytes32 code,
        bytes memory signature,
        string memory tokenName,
        uint256 amount,
        uint8 round
    ) public {
        require(_verifyCode(code, signature), "PrivateSale: Invalid signature");
        require(
            _verifyInviteeCode(code),
            "PrivateSale: This code was already used by a different address"
        );

        _investInRound(_msgSender(), tokenName, amount, round);
    }

    // Used by people invited by root wallet and NOT by pool investors, they need to invest first using the code
    // to create a link between their wallets and the code
    function investAgain(
        string memory tokenName,
        uint256 amount,
        uint8 round
    ) public {
        require(
            walletToPool[_msgSender()] != address(0),
            "PrivateSale: You need to invest with an invitation first"
        );

        _investInRound(walletToPool[_msgSender()], tokenName, amount, round);
    }

    function investPublicPool(
        address poolOwner,
        string memory tokenName,
        uint256 amount,
        uint8 round
    ) public {
        require(
            walletToPool[poolOwner] != address(0),
            "PrivateSale: This pool does not exist"
        );
        require(
            pools[round][poolOwner].unrestricted == true,
            "PrivateSale: The pool is not publicly open"
        );

        _investInRound(poolOwner, tokenName, amount, round);
    }
    // Investing in privatePool does not really seem necessary, since there is one pool, which we do not delete. 
    function investPrivatePool(
        address poolOwner,
        bytes32 code,
        bytes memory signature,
        string memory tokenName,
        uint256 amount,
        uint8 round
    ) public {
        require(
            code.toEthSignedMessageHash().recover(signature) == poolOwner,
            "PrivateSale: Invalid invitation codes"
        );
        require(
            inviteeToCode[poolOwner] != bytes32(0),
            "PrivateSale: This pool does not exist"
        );
        require(
            _verifyInviteeCode(code),
            "PrivateSale: This code was already used by a different address"
        );

        _investInRound(poolOwner, tokenName, amount, round);
    }

    function openPool(uint8 round, bool unrestricted) public {
        _openPool(_msgSender(), round, unrestricted);
    }

    function openPoolForAddress(
        address poolOwner,
        uint8 round,
        bool unrestricted
    ) public onlyOwner {
        _openPool(poolOwner, round, unrestricted);
    }

    function getInvestorTotalAllocation(address investor)
        public
        view
        returns (uint256[] memory, uint256[] memory)
    {
        require(
            investor != address(0),
            "PrivateSale: You must specify a buyer"
        );

        uint256[] memory investorAllocations = new uint256[](rounds.length);
        uint256[] memory poolAllocations = new uint256[](rounds.length);

        for (uint8 r = 0; r < rounds.length; r++) {
            investorAllocations[r] = investments[r][investor];

            if (walletToPool[investor] != address(0)) {
                poolAllocations[r] = pools[r][walletToPool[investor]].amount;
            } else {
                poolAllocations[r] = 0;
            }
        }

        return (investorAllocations, poolAllocations);
    }

    function rebalanceRoundLimit(uint8 round, uint256 newMaximum)
        public
        onlyOwner
    {
        require(_verifyRound(round, false), "PrivateSale: Invalid round");
        require(
            roundsTotalInvestments[round] <= newMaximum,
            "PrivateSale: The new maximum must be higher or equal than the total invested"
        );
        require(
            maximumRebalance[round] >= newMaximum,
            "PrivateSale: Cannot rebalance over the maximum rebalance limit"
        );

        rounds[round].maximum = newMaximum;
    }

    function _openPool(
        address poolOwner,
        uint8 round,
        bool unrestricted
    ) private {
        require(
            inviteeToCode[poolOwner] != bytes32(0),
            "PrivateSale: Address does not own a pool"
        );
        require(
            pools[round][poolOwner].unrestricted != unrestricted,
            "PrivateSale: The pool already has this state"
        );

        pools[round][poolOwner].unrestricted = unrestricted;
    }

    function _investInRound(
        address poolOwner,
        string memory tokenName,
        uint256 amount,
        uint8 round
    ) private {
        require(
            acceptedTokens[tokenName] != (address(0)),
            "PrivateSale: Invalid token/token not accepted"
        );
        require(amount > 0, "PrivateSale: You can't invest 0 tokens");
        
        require(
            amount % (10**acceptedTokenDecimals[tokenName]) == 0,
            "PrivateSale: Only round numbers are accepted"
        );
        require(_verifyRound(round, true), "PrivateSale: Round is not active");
        uint256 normalizedAmount = _normalizeDecimals(
            amount,
            acceptedTokenDecimals[tokenName]
        );
        require(
            roundsTotalInvestments[round].add(normalizedAmount) <=
                rounds[round].maximum,
            "PrivateSale: Round maximum allowance reached"
        );

        require(
            pools[round][poolOwner].amount.add(normalizedAmount) <=
                maximumPoolAllowance,
            "PrivateSale: Pool maximum allowance reached"
        );

        if (walletToPool[_msgSender()] == address(0)) {
            investors.push(_msgSender());
            numberOfInvestors += 1;
            walletToPool[_msgSender()] = poolOwner;
        }

        TransferHelper.safeTransferFrom(
            address(acceptedTokens[tokenName]),
            _msgSender(),
            wallet,
            amount
        );

        roundsTotalInvestments[round] = roundsTotalInvestments[round].add(
            normalizedAmount
        );

        // If this is a new pool, set it default to unrestricted
        if(pools[round][poolOwner].amount == 0) {
            pools[round][poolOwner].unrestricted = true;
        }
        pools[round][poolOwner].amount = pools[round][poolOwner].amount.add(
            normalizedAmount
        );

        investments[round][_msgSender()] = investments[round][_msgSender()].add(
            normalizedAmount
        );
    }

    function _verifyInviteeCode(bytes32 code) private returns (bool) {
        if (
            inviteeToCode[_msgSender()] != bytes32(0) ||
            codeToInvitee[code] != address(0)
        ) {
            return code == inviteeToCode[_msgSender()] &&
                codeToInvitee[code] == _msgSender();
        }

        codeToInvitee[code] = _msgSender();
        inviteeToCode[_msgSender()] = code;
        return true;
    }

    function _normalizeDecimals(uint256 amount, uint8 tokenDecimals)
        private
        view
        returns (uint256)
    {
        return
            amount.mul(
                10**uint256(NORMALIZING_DECIMALS).sub(uint256(tokenDecimals))
            );
    }

    function _verifyRound(uint8 round, bool checkActive)
        private
        view
        returns (bool)
    {
        if (round > 2) {
            return false;
        }

        if (checkActive) {
            return rounds[round].active == true;
        }

        return true;
    }

    function _verifyCode(bytes32 code, bytes memory signature)
        private
        view
        returns (bool)
    {
        return code.toEthSignedMessageHash().recover(signature) == signer;
    }
}

