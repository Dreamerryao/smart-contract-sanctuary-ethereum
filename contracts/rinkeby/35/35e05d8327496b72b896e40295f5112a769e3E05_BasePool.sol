import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "./interfaces/IPool.sol";
import "./interfaces/IRoller.sol";
import "./interfaces/IBean.sol";
import "./interfaces/IExchange.sol";
import "./interfaces/IVRF.sol";
import "./interfaces/IStakingPool.sol";

import "hardhat/console.sol";

contract BasePool is
    IPool,
    IRoller,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // a user play
    struct Play {
        address user;
        uint256 id;
        uint256 tickets; // total tickets purchased in this play
        uint256 currSumTicket; // last index of this play
        uint256 timestamp;
        bool won;
    }

    // a play session
    struct PlaySession {
        uint256[] playIds;
        uint256 totalTickets;
    }

    // config params
    string public name; // name of the pool, e.g., 0.5-WBTC-USDT
    address public beanToken; // SBT contract address (always assume one slotToken token rewards one beanToken)
    address public slotToken; // slot token contract address, e.g., USDT (always assume one slotToken takes one slot)
    address public prizeToken; // prize token contract address, e.g., WBTC
    address public exchange; // exchange contract address, e.g., Uniswap
    address public vrf; // verifiable random function address, e.g., Chainlink VRF
    address public megaRewardPool; // mega bean reward pool, to distribute among mega bean stakers
    address public gigaRewardPool; // giga bean reward pool, to distribute among  gigabean stakers
    address public feePool; // fee pool, to maintain platform operation
    uint8 public slotTokenDecimal; // slotToken decimal is usually 1e6 for stable coins
    uint256 public prizeTokenAmount; // prize token amount in Wei, e.g., 10**18
    uint256 public beanTokenPerTicket; // beanToken rewarded per slot token, e.g., 1e12 for USDC because decimal of USDC is 6 and deimal of bean is 18
    uint32 public feeRateE4; // fee rate x10000, e.g., 0.025 => 250
    uint32 public megaRewardRateE4; // reward rate x10000, e.g., 0.1 => 1000
    uint32 public gigaRewardRateE4; // reward rate x10000, e.g., 0.1 => 1000
    uint256 public sessionReward; // reward when a pay triggers pick winner in terms beanToken, e.g., 1e19
    uint256 public liquidatePeriod; // liquidate is allowed after time frame in seconds
    uint32 public liquidatorRewardRateE4; // liquidator reward rate x10000, e.g., 0.025 => 250

    // session params
    uint256[] public currPlayIds; // ids of current plays
    uint256 public liquidateTime; // count down timer to liquidate, in seconds
    uint256 public currTickets; // current tickets
    mapping(uint256 => PlaySession) public requestIdToPlaySession; // record play ids for async pick winner

    // history params
    uint256 public cusumFee; // cumulative sum of fees collected
    uint256 public cusumReward; // cumulative sum of rewards collected
    Play[] public allPlays; // all Play in history
    mapping(address => uint256[]) public userPlayIdsMap; // user address => play id array
    uint256[] public wonPlayIds; // ids of all winning plays

    /* events */
    event FailedRedeemPrize(
        uint256 _inAmount,
        uint256 _outAmount,
        address _winner
    );

    event PrizeRedeemed(uint256 _outAmount, address _winner);

    event TicketBought(uint256 _inAmount, address _buyer);

    event Liquidated(
        address _liquidator,
        uint256 _liquidatorReward,
        address _winner,
        uint256 _winnerReward
    );

    function initialize(
        string memory _name,
        address _owner,
        uint8 _slotTokenDecimal,
        uint256 _prizeTokenAmount,
        uint32 _feeRateE4,
        uint32 _megaRewardRateE4,
        uint32 _gigaRewardRateE4,
        uint256 _sessionReward,
        uint256 _liquidatePeriod,
        uint32 _liquidatorRewardRateE4
    ) public initializer {
        __Ownable_init();
        __Pausable_init();
        __Context_init();
        __ReentrancyGuard_init();
        name = _name;
        slotTokenDecimal = _slotTokenDecimal;
        prizeTokenAmount = _prizeTokenAmount;
        feeRateE4 = _feeRateE4; // 500;
        megaRewardRateE4 = _megaRewardRateE4; //750;
        gigaRewardRateE4 = _gigaRewardRateE4; //750;
        sessionReward = _sessionReward; // 10 ether;
        liquidatePeriod = _liquidatePeriod; // 60 * 60 * 12;
        liquidatorRewardRateE4 = _liquidatorRewardRateE4; //500;

        beanTokenPerTicket = 1e18 / (10**slotTokenDecimal);
        liquidateTime = type(uint256).max;

        // pause here. receive more parameters to start
        _pause();

        // owner goes to deployer
        transferOwnership(_owner);
    }

    function startWithDependencies(
        address _beanToken,
        address _slotToken,
        address _prizeToken,
        address _exchange,
        address _vrf,
        address _megaRewardPool,
        address _gigaRewardPool,
        address _feePool
    ) external onlyOwner nonReentrant {
        beanToken = _beanToken;
        slotToken = _slotToken;
        prizeToken = _prizeToken;
        exchange = _exchange;
        vrf = _vrf;
        megaRewardPool = _megaRewardPool;
        gigaRewardPool = _gigaRewardPool;
        feePool = _feePool;

        _unpause();
    }

    function getPoolInfo()
        external
        view
        returns (
            string memory _name,
            uint256 _liquidateTime,
            uint256 _currTickets,
            uint256 _prizeTokenAmount,
            uint256 _cusumFee,
            uint256 _cusumReward
        )
    {
        return (
            name,
            liquidateTime,
            currTickets,
            prizeTokenAmount,
            cusumFee,
            cusumReward
        );
    }

    function getPlay(uint256 index)
        external
        view
        returns (
            address user,
            uint256 id,
            uint256 tickets,
            uint256 currSumTicket,
            uint256 timestamp,
            bool won
        )
    {
        return (
            allPlays[index].user,
            allPlays[index].id,
            allPlays[index].tickets,
            allPlays[index].currSumTicket,
            allPlays[index].timestamp,
            allPlays[index].won
        );
    }

    function getWonPlays() external view returns (uint256[] memory) {
        return wonPlayIds;
    }

    function getWonPlayCount() external view returns (uint256) {
        return wonPlayIds.length;
    }

    function getUserPlays(address _user)
        external
        view
        returns (uint256[] memory)
    {
        return userPlayIdsMap[_user];
    }

    function getUserPlayCount(address _user) external view returns (uint256) {
        return userPlayIdsMap[_user].length;
    }

    /// @dev Buy tickets to win the prize
    function buy(uint256 _tickets)
        external
        virtual
        override
        whenNotPaused
        nonReentrant
    {
        // check _tickets lower limit
        require(_tickets >= 10**slotTokenDecimal, "bp: buy ticket limit");

        // check whether currPlays should be liquidated, if yes, liquidate first then start a new game
        if (block.timestamp > liquidateTime && currPlayIds.length > 0) {
            liquidate();
        }

        uint256 targetTickets = poolTargetTickets();

        // check _tickets upper limit
        require(_tickets <= targetTickets * 100, "bp: max 100 full pool");

        // collect _tickets
        IERC20Upgradeable(slotToken).safeTransferFrom(
            msg.sender,
            address(this),
            _tickets
        );

        // check if pool is overloaded, if yes, redeem the overloaded part
        uint256 sessionOverloadCount = _tickets / targetTickets;
        uint256 remainingTickets = _tickets % targetTickets;
        uint256 totalSessionCount = sessionOverloadCount;

        // check if currTickes is overloaded, if yes, close current session and clear all existing plays
        if (currTickets >= targetTickets) {
            uint256 requestId = requestRandomNumber();
            requestIdToPlaySession[requestId] = PlaySession({
                playIds: currPlayIds,
                totalTickets: currTickets
            });
            totalSessionCount += 1;
            currTickets = 0;
            delete currPlayIds;
        }

        // check whether prize draw should happen, if yes, get the winner
        if (currTickets + remainingTickets >= targetTickets) {
            uint256 filledTickets = targetTickets - currTickets;
            addPlay(
                Play({
                    user: msg.sender,
                    id: allPlays.length,
                    tickets: filledTickets,
                    currSumTicket: targetTickets,
                    timestamp: block.timestamp,
                    won: false
                })
            );
            remainingTickets -= filledTickets;

            uint256 requestId = requestRandomNumber();
            requestIdToPlaySession[requestId] = PlaySession({
                playIds: currPlayIds,
                totalTickets: targetTickets
            });
            totalSessionCount += 1;
            currTickets = 0;
            delete currPlayIds;
        }

        // add plays if there are overloads
        for (uint256 i = 0; i < sessionOverloadCount; i++) {
            uint256 nextId = allPlays.length;
            addPlay(
                Play({
                    user: msg.sender,
                    id: allPlays.length,
                    tickets: targetTickets,
                    currSumTicket: targetTickets,
                    timestamp: block.timestamp,
                    won: true
                })
            );
            wonPlayIds.push(nextId);
        }

        // if there are remaining tokens, put into next pool
        if (remainingTickets > 0) {
            currTickets += remainingTickets;
            addPlay(
                Play({
                    user: msg.sender,
                    id: allPlays.length,
                    tickets: remainingTickets,
                    currSumTicket: currTickets,
                    timestamp: block.timestamp,
                    won: false
                })
            );
        }

        // mint super beans. for each session completed, reward user extra 10 super beans.
        uint256 totalBeans = _tickets *
            beanTokenPerTicket +
            sessionReward *
            totalSessionCount;
        IBean(beanToken).mint(msg.sender, totalBeans);

        // transfer overload prize and add plays
        // because vrf, pick winner is now a async process, redeemPrize should only be called in a callback.
        if (sessionOverloadCount > 0) {
            (uint256 surplus, bool success) = redeemPrize(
                targetTickets * sessionOverloadCount,
                prizeTokenAmount * sessionOverloadCount,
                msg.sender
            );
            require(success, "bp: fail redeem");

            if (surplus > 0) {
                distributeRewardFee(surplus);
            }
        }

        // update liquidateTime
        liquidateTime = block.timestamp + liquidatePeriod;

        emit TicketBought(_tickets, msg.sender);
    }

    /// @dev Anyone can liquidate a pool if the last transaction.
    /// @dev session last player get 95% of the pool while liquidator get 5% reward.
    function liquidate() public whenNotPaused {
        // check liquidation conditions
        require(
            block.timestamp > liquidateTime && currPlayIds.length > 0,
            "bp: liquidate"
        );

        /// 5% to liquidator, rest to last player
        address winner = allPlays[currPlayIds[currPlayIds.length - 1]].user;

        uint256 liquidatorReward = (currTickets * 5) / 100;
        uint256 winnerReward = currTickets - liquidatorReward;
        IERC20Upgradeable(slotToken).safeTransfer(msg.sender, liquidatorReward);
        IERC20Upgradeable(slotToken).safeTransfer(winner, winnerReward);

        emit Liquidated(msg.sender, liquidatorReward, winner, winnerReward);

        /// reset
        delete currPlayIds;
        currTickets = 0;
        liquidateTime = type(uint256).max;
    }

    function handleRollResults(
        uint256 _requestId,
        uint256[] calldata _randomWords
    ) external override {
        require(msg.sender == vrf, "bp: wrong vrf");

        PlaySession memory session = requestIdToPlaySession[_requestId];
        require(
            session.totalTickets != 0 && session.playIds.length != 0,
            "bp: empty session"
        );

        address winner = pickWinner(
            _randomWords[0],
            session.totalTickets,
            session.playIds
        );

        (uint256 surplus, bool success) = redeemPrize(
            session.totalTickets,
            prizeTokenAmount,
            winner
        );
        require(success, "bp: fail redeem");

        if (surplus > 0) {
            distributeRewardFee(surplus);
        }

        delete requestIdToPlaySession[_requestId];
    }

    /// @dev swap uses fromToken to buy toToken from exchange.
    function redeemPrize(
        uint256 _inAmount,
        uint256 _outAmount,
        address _winner
    ) internal returns (uint256 surplus, bool success) {
        TransferHelper.safeApprove(slotToken, exchange, 0);
        TransferHelper.safeApprove(slotToken, exchange, _inAmount);

        try
            IExchange(exchange).swapExactOutput(
                slotToken,
                prizeToken,
                _outAmount,
                _inAmount,
                _winner
            )
        returns (uint256 _amount) {
            emit PrizeRedeemed(_outAmount, _winner);
            return (_inAmount - _amount, true);
        } catch (bytes memory) {
            emit FailedRedeemPrize(_inAmount, _outAmount, _winner);
            return (0, false);
        }
    }

    /// @dev distribute reward and fee each play with prizes redeemed.
    function distributeRewardFee(uint256 _surplus) internal {
        require(_surplus >= 0, "bp: distribute reward fee");

        uint32 totalShare = megaRewardRateE4 + gigaRewardRateE4 + feeRateE4;
        uint256 megaReward = (_surplus * megaRewardRateE4) / totalShare;
        uint256 gigaReward = (_surplus * gigaRewardRateE4) / totalShare;
        uint256 fee = _surplus - megaReward - gigaReward;

        IERC20Upgradeable(slotToken).approve(megaRewardPool, 0);
        IERC20Upgradeable(slotToken).approve(megaRewardPool, megaReward);
        IStakingPool(megaRewardPool).addReward(address(this), megaReward);

        IERC20Upgradeable(slotToken).approve(gigaRewardPool, 0);
        IERC20Upgradeable(slotToken).approve(gigaRewardPool, gigaReward);
        IStakingPool(gigaRewardPool).addReward(address(this), gigaReward);

        if (fee > 0) {
            IERC20Upgradeable(slotToken).approve(feePool, 0);
            IERC20Upgradeable(slotToken).approve(feePool, fee);
            IERC20Upgradeable(slotToken).safeTransfer(feePool, fee);
        }

        cusumReward += megaReward + gigaReward;
        cusumFee += fee;
    }

    function poolTargetTickets() public returns (uint256) {
        return
            (IExchange(exchange).getApproxAmountIn(
                slotToken,
                prizeToken,
                prizeTokenAmount
            ) * (1e4 + megaRewardRateE4 + gigaRewardRateE4 + feeRateE4)) / 1e4;
    }

    /// @dev request for random number form VRF
    function requestRandomNumber() internal returns (uint256 _requestId) {
        return IVRF(vrf).requestRandomWords(1);
    }

    /// @dev pick winner when random callback arrives.
    function pickWinner(
        uint256 _randomWord,
        uint256 _targetAmount,
        uint256[] memory _playIds
    ) internal returns (address) {
        /// decide winner
        uint256 luckyNum = (_randomWord % _targetAmount) + 1;

        // binary search for ticket range
        uint256 low = 0;
        uint256 high = _playIds.length - 1;
        uint8 rounds = 0;
        while (low < high && rounds < 20) {
            // Note that mid will always be strictly less than high (i.e. it will be a valid array index)
            // because Math.average rounds down (it does integer division with truncation).
            uint256 mid = Math.average(low, high);

            if (allPlays[_playIds[mid]].currSumTicket >= luckyNum) {
                high = mid;
            } else {
                low = mid + 1;
            }
            rounds++;
        }
        require(rounds < 20, "bp: pick winner err");

        // console.log("pickWinner low %d", low);
        // console.log("pickWinner _randomWord %d", _randomWord);
        // console.log("pickWinner _targetAmount %d", _targetAmount);
        // console.log("pickWinner _playIds[0] %d", _playIds[0]);
        // console.log("pickWinner _playIds[1] %d", _playIds[1]);

        allPlays[_playIds[low]].won = true;
        wonPlayIds.push(_playIds[low]);
        return allPlays[_playIds[low]].user;
    }

    /// @dev add a play into both current play and historical plays.
    function addPlay(Play memory _play) internal {
        currPlayIds.push(_play.id);
        allPlays.push(_play);
        userPlayIdsMap[_play.user].push(_play.id);
    }

    // function debug_printCurrPlays() internal view {
    //     for (uint256 i = 0; i < currPlay.length; i++) {
    //         Play memory p = allPlay[currPlay[i]];
    //         console.log(
    //             "Play{user:%s, id:%d, tickets:%d, ",
    //             p.user,
    //             p.id,
    //             p.tickets
    //         );
    //         console.log(
    //             "currSumTicket:%d, timestamp:%d, won:%s}",
    //             p.currSumTicket,
    //             p.timestamp,
    //             p.won
    //         );
    //     }
    // }
}

