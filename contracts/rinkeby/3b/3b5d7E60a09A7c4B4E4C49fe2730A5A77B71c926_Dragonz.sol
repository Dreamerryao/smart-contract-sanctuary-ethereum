interface DragonzEvents {
    event childToAnotherParent(
        uint256 _fromTokenId,
        uint256 _toTokenId,
        address _childContract,
        uint256 _childTokenId
    );

    event linkChildToTokenId(
        uint256 _tokenId,
        address _childContract,
        uint256 _childTokenId
    );

    event delinkChildToTokenId(
        uint256 _tokenId,
        address _childContract,
        uint256 _childTokenId
    );
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

interface IDragonz {
    enum Categories {
        common,
        uncommon,
        rare,
        superRare,
        ultraRare
    }
    struct NFTvoucher {
        uint256 tokenId;
        uint160 wallet;
        Categories choice;
        bytes signature;
    }

    function mint(NFTvoucher memory voucher) external payable;

    function getChild(
        address from,
        uint256 tokenId,
        address childContract,
        uint256 childTokenId
    ) external;

    function transferChildToAddress(
        uint256 fromTokenId,
        address to,
        address childContract,
        uint256 childTokenId
    ) external;

    function transferChildToAnotherParent(
        uint256 fromTokenId,
        uint256 toTokenId,
        address childContract,
        uint256 childTokenId
    ) external;

    function rootOwner(address childContract, uint256 childTokenId)
        external
        view
        returns (address);

    function childExists(address childContract, uint256 childTokenId)
        external
        view
        returns (bool);

    function totalChildContractOfParent(uint256 tokenId)
        external
        view
        returns (uint256);

    function totalChildOfParent(uint256 tokenId)
        external
        view
        returns (uint256);

    function getPrice(Categories choice) external view returns (uint256 price);

    function isMintingAllowed() external view returns (bool);
}

