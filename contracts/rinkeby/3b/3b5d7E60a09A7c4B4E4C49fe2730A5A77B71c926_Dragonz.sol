pragma solidity ^0.8.7;

import "./EIP712.sol";
import "./interface/DragonzEvents.sol";
import "./interface/IDragonz.sol";
import "./library/TransferHelper.sol";
import "./library/ReentrancyGuard.sol";
import "./VRFv2Consumer.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";

contract Dragonz is
    ERC721,
    Ownable,
    DragonzEvents,
    IDragonz,
    ReentrancyGuard,
    EIP712,
    VRFv2Consumer
{
    using Strings for uint256;

    uint256 private counter;
    uint256 private lotteryPrice;

    address receiverAddress;
    address private VRFv2ConsumerAddress;
    bool private isMintAllowed = true;

    // category => category price+
    mapping(Categories => uint256) private categoryPrice;

    /* Categories:
    1 -> common NFTs
    2 -> uncommon NFTs
    3 -> rare NFTs
    4 -> superrare NFTs
    5 -> ultrarare NFTs  */

    // child contract => child Id => token Id
    mapping(address => mapping(uint256 => uint256)) public childTokenToParentId;

    //tokenId => child contracts
    mapping(uint256 => address[]) public childContractsArrayMapping;

    // tokenId => childContract => child Ids
    mapping(uint256 => mapping(address => uint256[]))
        public childTokensArrayMapping;

    //tokenId => child contract => Index
    mapping(uint256 => mapping(address => uint256))
        public childContractIndexMapping;

    // tokenId => childContract => childId => Index
    mapping(uint256 => mapping(address => mapping(uint256 => uint256)))
        public childTokenIndexMapping;

    constructor(address _receiverAddress, uint64 subscriptionId)
        ERC721("Dragon Of Elements NFT", "DOE")
        EIP712("LazyNFT-voucher", "1")
        VRFv2Consumer(subscriptionId)
    {
        receiverAddress = _receiverAddress;
    }

    receive() external payable {
        TransferHelper.safeTransferETH(receiverAddress, msg.value);
    }

    function mint(NFTvoucher calldata voucher)
        public
        payable
        override
        nonReentrant
    {
        require(isMintingAllowed(), "Minting not allowed");
        require(owner() == _verify(voucher), "DOE: Signature Invalid");
        require(msg.sender == address(voucher.wallet), "DOE: Invalid Address");
        uint256 _price = categoryPrice[voucher.choice];
        require(msg.value >= _price, "DOE: Price insufficient");

        _safeMint(msg.sender, voucher.tokenId);

        TransferHelper.safeTransferETH(receiverAddress, msg.value);
    }

    // function random() public  {
    //     requestRandomWords();
    //     uint256 _randomNumber = s_randomWords[0];

    //     return _randomNumber;
    // }

    function randomMint(NFTvoucher[] calldata voucher) external payable {
        require(isMintingAllowed(), "DOE: Minting not allowed");
        require(msg.value >= lotteryPrice, "DOE: Price insufficient");

        requestRandomWords();
        uint256 _randomNumber = s_randomWords[0];
        // uint8 _randomNumber = uint8(_random(voucher) % (voucherLength - 1));

        require(
            owner() == _verify(voucher[_randomNumber]),
            "DOE: Signature Invalid"
        );
        require(
            msg.sender == address(voucher[_randomNumber].wallet),
            "DOE: Invalid Address"
        );

        if (voucher[_randomNumber].tokenId > 0) {
            _safeMint(msg.sender, voucher[_randomNumber].tokenId);
        } else {
            revert("DOE: Required category NFT exhausted");
        }

        TransferHelper.safeTransferETH(receiverAddress, msg.value);
    }

    function getChild(
        address _from,
        uint256 _tokenId,
        address _childContract,
        uint256 _childTokenId
    ) external override {
        require(
            _from == msg.sender ||
                IERC721(_childContract).isApprovedForAll(_from, msg.sender) ||
                IERC721(_childContract).getApproved(_childTokenId) == msg.sender
        );
        _receiveChild(_tokenId, _childContract, _childTokenId);
        IERC721(_childContract).transferFrom(
            _from,
            address(this),
            _childTokenId
        );
    }

    function transferChildToAddress(
        uint256 _fromTokenId,
        address _to,
        address _childContract,
        uint256 _childTokenId
    ) external override {
        uint256 tokenId = childTokenToParentId[_childContract][_childTokenId];
        require(tokenId > 0, "Child Id not mapped");
        require(tokenId == _fromTokenId, "Authentication error");
        require(_to != address(0), "Zero address passed");
        address owner = rootOwner(_childContract, _childTokenId);
        require(owner == msg.sender, "Only owner can send");
        _removeChild(tokenId, _childContract, _childTokenId);
        IERC721(_childContract).safeTransferFrom(
            address(this),
            _to,
            _childTokenId
        );
    }

    function transferChildToAnotherParent(
        uint256 _fromTokenId,
        uint256 _toTokenId,
        address _childContract,
        uint256 _childTokenId
    ) external override {
        uint256 tokenId = childTokenToParentId[_childContract][_childTokenId];
        require(
            tokenId > 0 || childExists(_childContract, _childTokenId),
            "Child not mapped!"
        );
        require(tokenId == _fromTokenId, "Wrong TokenID entered");
        address owner = rootOwner(_childContract, _childTokenId);
        require(owner == msg.sender, "Only Token ID owner can send");
        _removeChild(_fromTokenId, _childContract, _childTokenId);
        _receiveChild(_toTokenId, _childContract, _childTokenId);

        emit childToAnotherParent(
            _fromTokenId,
            _toTokenId,
            _childContract,
            _childTokenId
        );
    }

    function rootOwner(address _childContract, uint256 _childTokenId)
        public
        view
        override
        returns (address _rootOwner)
    {
        uint256 _tokenId = childTokenToParentId[_childContract][_childTokenId];
        _rootOwner = ownerOf(_tokenId);
    }

    function _receiveChild(
        uint256 _tokenId,
        address _childContract,
        uint256 _childTokenId
    ) internal {
        require(ownerOf(_tokenId) != address(0), "Token ID does not exist");
        require(
            childTokenIndexMapping[_tokenId][_childContract][_childTokenId] ==
                0,
            "Already Received"
        );
        uint256 childTokensLength = childTokensArrayMapping[_tokenId][
            _childContract
        ].length;
        if (childTokensLength == 0) {
            childContractIndexMapping[_tokenId][_childContract] =
                childContractsArrayMapping[_tokenId].length +
                1;
            childContractsArrayMapping[_tokenId].push(_childContract);
        }
        childTokensArrayMapping[_tokenId][_childContract].push(_childTokenId);
        childTokenIndexMapping[_tokenId][_childContract][_childTokenId] =
            childTokensLength +
            1;
        childTokenToParentId[_childContract][_childTokenId] = _tokenId;

        emit linkChildToTokenId(_tokenId, _childContract, _childTokenId);
    }

    function _removeChild(
        uint256 _tokenId,
        address _childContract,
        uint256 _childTokenId
    ) internal {
        uint256 tokenIndex = childTokenIndexMapping[_tokenId][_childContract][
            _childTokenId
        ];
        require(
            childExists(_childContract, _childTokenId),
            "Child token not owned by token"
        );

        uint256 lastTokenIndex = childTokensArrayMapping[_tokenId][
            _childContract
        ].length - 1;
        uint256 lastToken = childTokensArrayMapping[_tokenId][_childContract][
            lastTokenIndex
        ];
        if (_childTokenId != lastToken) {
            childTokensArrayMapping[_tokenId][_childContract][
                tokenIndex - 1
            ] = lastToken;
            childTokenIndexMapping[_tokenId][_childContract][
                lastToken
            ] = tokenIndex;
        }
        childTokensArrayMapping[_tokenId][_childContract].pop();
        delete childTokenIndexMapping[_tokenId][_childContract][_childTokenId];
        delete childTokenToParentId[_childContract][_childTokenId];

        if (lastTokenIndex == 0) {
            uint256 lastContractIndex = childContractsArrayMapping[_tokenId]
                .length - 1;
            address lastContract = childContractsArrayMapping[_tokenId][
                lastContractIndex
            ];
            if (_childContract != lastContract) {
                uint256 contractIndex = childContractIndexMapping[_tokenId][
                    _childContract
                ];
                childContractsArrayMapping[_tokenId][
                    contractIndex
                ] = lastContract;
                childContractIndexMapping[_tokenId][
                    lastContract
                ] = contractIndex;
            }
            childContractsArrayMapping[_tokenId].pop();
            delete childContractIndexMapping[_tokenId][_childContract];
        }
        emit delinkChildToTokenId(_tokenId, _childContract, _childTokenId);
    }

    function childExists(address _childContract, uint256 _childTokenId)
        public
        view
        override
        returns (bool)
    {
        uint256 tokenId = childTokenToParentId[_childContract][_childTokenId];
        return
            childTokenIndexMapping[tokenId][_childContract][_childTokenId] > 0;
    }

    function totalChildContractOfParent(uint256 _tokenId)
        external
        view
        override
        returns (uint256)
    {
        return childContractsArrayMapping[_tokenId].length;
    }

    function totalChildOfParent(uint256 _tokenId)
        external
        view
        override
        returns (uint256)
    {
        uint256 numberOfContracts = childContractsArrayMapping[_tokenId].length;
        if (numberOfContracts == 0) {
            return 0;
        } else {
            uint256 sum;
            for (uint256 i = 0; i < numberOfContracts; i++) {
                address childContract = childContractsArrayMapping[_tokenId][i];
                sum += childTokensArrayMapping[_tokenId][childContract].length;
            }
            return sum;
        }
    }

    function updateReceiverAddress(address _newAddress) external onlyOwner {
        receiverAddress = _newAddress;
    }

    function setIsMintingAllowed(bool _trueOrFalse) public onlyOwner {
        isMintAllowed = _trueOrFalse;
    }

    function setPriceForCommonNFTs(uint256 _price) external onlyOwner {
        categoryPrice[Categories.common] = _price;
    }

    function setPriceForUnCommonNFTs(uint256 _price) external onlyOwner {
        categoryPrice[Categories.uncommon] = _price;
    }

    function setPriceForRareNFTs(uint256 _price) external onlyOwner {
        categoryPrice[Categories.rare] = _price;
    }

    function setPriceForSuperRareNFTs(uint256 _price) external onlyOwner {
        categoryPrice[Categories.superRare] = _price;
    }

    function setPriceForUltraRareNFTs(uint256 _price) external onlyOwner {
        categoryPrice[Categories.ultraRare] = _price;
    }

    function setLotteryPriceForNFTs(uint256 _price) external onlyOwner {
        lotteryPrice = _price;
    }

    function getPrice(Categories choice)
        external
        view
        override
        returns (uint256 _price)
    {
        _price = categoryPrice[choice];
    }

    function isMintingAllowed() public view override returns (bool) {
        return isMintAllowed;
    }

    function contractURI() external pure returns (string memory) {
        return
            "https://gateway.pinata.cloud/ipfs/QmX584MNcqvvXYDTxyjYfoHuwhUZ8Yc2fxEvH2QPoTMeKm";
    }

    function _baseURI() internal pure override returns (string memory) {
        return
            "https://gateway.pinata.cloud/ipfs/QmYHec1ZyrAiVo11mCmwSs7FtQidHM5x6qvdvaXZXc1Dtw/";
    }

    function baseURI() public pure returns (string memory) {
        return _baseURI();
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        _requireMinted(tokenId);

        string memory baseURI_ = _baseURI();
        return
            bytes(baseURI_).length != 0
                ? string(
                    abi.encodePacked(baseURI_, tokenId.toString(), ".json")
                )
                : "";
    }

    function _hash(NFTvoucher calldata voucher) public view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "NFTVoucher(uint256 tokenId, uint160 wallet, categories choice)"
                        ),
                        voucher.tokenId,
                        voucher.wallet,
                        voucher.choice
                    )
                )
            );
    }

    function _verify(NFTvoucher calldata voucher)
        internal
        view
        returns (address)
    {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }

    function conversion(address to) public pure returns (uint160) {
        return (uint160(to));
    }
}

