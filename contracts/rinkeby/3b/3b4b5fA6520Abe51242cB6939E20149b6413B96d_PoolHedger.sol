pragma solidity 0.8.0;
pragma experimental ABIEncoderV2;

// Libraries
import "./synthetix/SafeDecimalMath.sol";
import "./synthetix/SignedSafeDecimalMath.sol";
// Inherited
// Interfaces
import "@openzeppelin/contracts/access/Ownable.sol";
import "./openzeppelin-l2/SafeERC20.sol";
import "./interfaces/ILiquidityPool.sol";
import "./interfaces/ICollateralShort.sol";
import "./interfaces/IOzillaGlobals.sol";
import "./interfaces/IOptionGreekCache.sol";
import "./interfaces/IPoolHedger.sol";
import "./openzeppelin-l2/SafeERC20.sol";
import "./interfaces/IShortCollateral.sol";
import "hardhat/console.sol";

/**
 * @title PoolHedger
 * @author Ozilla
 * @dev Uses the delta hedging funds from the LiquidityPool to hedge option deltas,
 * so LPs are minimally exposed to movements in the underlying asset price.
 * Now we may deprecate this contract because we don't want to use Synthetix to hedge
 */
contract PoolHedger is IPoolHedger, Ownable {
  using SafeMath for uint;
  using SafeDecimalMath for uint;
  using SignedSafeMath for int;
  using SafeERC20 for IERC20;

  IOzillaGlobals internal globals;
  IOptionMarket internal optionMarket;
  IOptionGreekCache internal optionGreekCache;
  ILiquidityPool internal liquidityPool;
  IShortCollateral internal shortCollateral;
  IERC20 internal quoteAsset;
  IERC20 internal baseAsset;

  bool public initialized = false;
  bool public override shortingInitialized = false;
  /// @dev The ID of our short that is opened when we init the contract.
  uint public override shortId;
  /// @dev The last time a short or long position was updated
  uint public override lastInteraction;
  /// @dev How long before balance can be updated
  uint public override interactionDelay;
  /// @dev Counter for reentrancy guard
  uint internal counter = 1;
  /// @dev The poolFee
  uint24 public constant poolFee = 3000;

  /// @dev Initialize the debts
  Debts public debts = Debts(0,0,0,0);

  constructor() Ownable() {
    interactionDelay = 3 hours;
    emit InteractionDelaySet(interactionDelay);
  }

  /**
   * @dev Initialize the contract.
   *
   * @param _globals OzillaGlobals address
   * @param _optionMarket OptionMarket address
   * @param _liquidityPool LiquidityPool address
   * @param _quoteAsset Quote asset address
   * @param _baseAsset Base asset address
   */
  function init(
    IOzillaGlobals _globals,
    IOptionMarket _optionMarket,
    IOptionGreekCache _optionGreekCache,
    ILiquidityPool _liquidityPool,
    IShortCollateral _shortCollateral,
    IERC20 _quoteAsset,
    IERC20 _baseAsset
  ) external {
    require(!initialized, "contract already initialized");
    globals = _globals;
    optionMarket = _optionMarket;
    optionGreekCache = _optionGreekCache;
    liquidityPool = _liquidityPool;
    shortCollateral = _shortCollateral;
    quoteAsset = _quoteAsset;
    baseAsset = _baseAsset;
    initialized = true;
  }

  function getDebts() external pure override returns (Debts memory debts) {
    return debts;
  }

  /**
   * @dev Set the contract interaction delay.
   *
   * @param newInteractionDelay The new interaction delay.
   */
  function setInteractionDelay(uint newInteractionDelay) external override onlyOwner {
    interactionDelay = newInteractionDelay;
    emit InteractionDelaySet(interactionDelay);
  }

  function getExpectedHedge() public view returns (int) {
    // Update any stale boards to get an accurate netDelta value
    int netOptionDelta = optionGreekCache.getGlobalNetDelta();

    // Subtract the baseAsset balance from netDelta, to account for the variance from collateral held by LP.
    // expectedHedge = netDelta - base in LiquidityPool - baseInDeltaHedge + debtBaseToShortCollateral.
    // 计算ShortCollateral借出的Base，但是不计算LiquidityPool借出的Base
    int expectedHedge = netOptionDelta.sub(int(baseAsset.balanceOf(address(liquidityPool)))).sub(int(baseAsset.balanceOf(address(this)))).add(int(debts.debtBaseToShortCollateral));

    return expectedHedge;
  }

  // preHedge to check if we lack of assets
  function estimateHedge() external override view returns (bool) {
    int expectedHedge = getExpectedHedge();
    if (expectedHedge < 0) {
      uint expectedShort = uint(-expectedHedge);
      if (expectedShort > baseAsset.balanceOf(address(liquidityPool)).add(baseAsset.balanceOf(address(this))).add(baseAsset.balanceOf(address(shortCollateral)))) {
        return false;
      }
    }
    return true;
  }

  function getDebtValueQuote(uint basePrice) external override view returns (uint) {
    return debts.debtBaseToShortCollateral + debts.debtQuoteToShortCollateral;
  }

  /**
   * @dev Retrieves the netDelta from the OptionGreekCache and updates the hedge position.
  */
  function hedgeDelta() external override reentrancyGuard {
    // require(shortingInitialized, "shorting must be initialized");

    // Update any stale boards to get an accurate netDelta value
    int netOptionDelta = optionGreekCache.getGlobalNetDelta();

    // Subtract the baseAsset balance from netDelta, to account for the variance from collateral held by LP.
    int expectedHedge = netOptionDelta.sub(int(baseAsset.balanceOf(address(liquidityPool))));

    // Bypass interactionDelay if we want to set netDelta to 0
    if (expectedHedge != 0 && interactionDelay != 0) {
      require(lastInteraction.add(interactionDelay) <= block.timestamp, "Interaction delay");
    }

    console.logInt(expectedHedge);

    _hedgeDelta(expectedHedge);
  }

  /**
   * @dev Updates the hedge position. This may need to be called several times as it will only do one step at a time
   * I.e. to go from a long position to asho
   *
   * @param expectedHedge The expected final hedge value.
   */
  function _hedgeDelta(int expectedHedge) internal {
    IOzillaGlobals.ExchangeGlobals memory exchangeGlobals = globals.getExchangeGlobals(address(optionMarket));
    // sell baseAsset to quoteAsset if expectedHedge < 0
    if (expectedHedge < 0) {
      // first check if we have enough baseAsset to sell
      uint expectedShort = uint(-expectedHedge);
      if (expectedShort > baseAsset.balanceOf(address(liquidityPool))) {
        // we donot have enough baseAsset to sell, so we need borrow from ShortCollateral
        uint bal = baseAsset.balanceOf(address(liquidityPool));
        // borrow from ShortCollateral
        uint receivedBase = shortCollateral.lendBaseToHedge(exchangeGlobals, expectedShort.sub(bal));
        debts.debtBaseToShortCollateral += receivedBase;
        liquidityPool.transferBaseToHedge(bal);
        debts.debtBaseToLiquidityPool += bal;
      } else {
        // we have enough baseAsset to sell
        liquidityPool.transferBaseToHedge(expectedShort);
        debts.debtBaseToLiquidityPool += expectedShort;
      }

      ISwapRouter.ExactInputParams memory params =
      ISwapRouter.ExactInputParams({
      path: abi.encodePacked(exchangeGlobals.baseAddress, poolFee, exchangeGlobals.quoteAddress),
      recipient: address(this),
      deadline: block.timestamp + 3600,
      amountIn: expectedShort,
      amountOutMinimum: 0
      });
      exchangeGlobals.swapRouter.exactInput(params);

    } else {
      uint expectedLong = uint(expectedHedge);
      // buy baseAsset with quoteAsset if expectedHedge > 0
      if (expectedLong.multiplyDecimal(exchangeGlobals.spotPrice) > quoteAsset.balanceOf(address(liquidityPool))) {
        // we donot have enough quoteAsset to hedge
        uint bal = quoteAsset.balanceOf(address(liquidityPool));
        // borrow from ShortCollateral
        uint receivedQuote = shortCollateral.lendQuoteToHedge(exchangeGlobals, expectedLong.multiplyDecimal(exchangeGlobals.spotPrice).sub(bal));
        debts.debtQuoteToShortCollateral += receivedQuote;
        liquidityPool.transferQuoteToHedge(exchangeGlobals,bal);
        debts.debtQuoteToLiquidityPool += bal;
      } else {
        // we have enough quoteAsset to hedge
        liquidityPool.transferQuoteToHedge(exchangeGlobals,expectedLong.multiplyDecimal(exchangeGlobals.spotPrice));
        debts.debtQuoteToLiquidityPool += expectedLong.multiplyDecimal(exchangeGlobals.spotPrice);
      }

      ISwapRouter.ExactInputParams memory params =
      ISwapRouter.ExactInputParams({
      path: abi.encodePacked(exchangeGlobals.quoteAddress, poolFee, exchangeGlobals.baseAddress),
      recipient: address(this),
      deadline: block.timestamp + 3600,
      amountIn: expectedLong.multiplyDecimal(exchangeGlobals.spotPrice),
      amountOutMinimum: 0
      });
      exchangeGlobals.swapRouter.exactInput(params);
    }

    lastInteraction = block.timestamp;

    // All proceeds should be sent back to the LP
    repayBaseToShortCollateral();
    repayQuoteToShortCollateral();
    repayBaseToLiquidityPool();
    repayQuoteToLiquidityPool();
    // transfer all quotes and assets to the LP
    transferAllAssetsToLP();
  }

  /**
   * @dev Repays the shortCollateral to the LP
   */
  function transferAllAssetsToLP() internal {
    require(baseAsset.transfer(address(liquidityPool), baseAsset.balanceOf(address(this))), "all base transfer failed");
    require(quoteAsset.transfer(address(liquidityPool), quoteAsset.balanceOf(address(this))), "all quote transfer failed");
  }

  /**
 * @dev Repay all base asset to the `ShortCollateral`.
   */
  function repayBaseToShortCollateral() internal {
    uint baseBal = baseAsset.balanceOf(address(this));
    if (debts.debtBaseToShortCollateral > baseBal ) {
      require(baseAsset.transfer(address(shortCollateral), baseBal), "quote transfer failed");
      debts.debtBaseToShortCollateral -= baseBal;
      emit QuoteReturnedToShortCollateral(baseBal);
    } else {
      require(baseAsset.transfer(address(shortCollateral), debts.debtBaseToShortCollateral), "quote transfer failed");
      debts.debtBaseToShortCollateral = 0;
      emit QuoteReturnedToShortCollateral(debts.debtBaseToShortCollateral);
    }
  }

  function repayQuoteToShortCollateral() internal {
    uint quoteBal = quoteAsset.balanceOf(address(this));
    if (debts.debtQuoteToShortCollateral > quoteBal ) {
      require(quoteAsset.transfer(address(shortCollateral), quoteBal), "quote transfer failed");
      debts.debtQuoteToShortCollateral -= quoteBal;
      emit BaseReturnedToShortCollateral(quoteBal);
    } else {
      require(quoteAsset.transfer(address(shortCollateral), debts.debtQuoteToShortCollateral), "quote transfer failed");
      debts.debtQuoteToShortCollateral = 0;
      emit BaseReturnedToShortCollateral(debts.debtQuoteToShortCollateral);
    }
  }

  function repayBaseToLiquidityPool() internal {
    uint baseBal = baseAsset.balanceOf(address(this));
    if (debts.debtBaseToLiquidityPool > baseBal ) {
      require(baseAsset.transfer(address(liquidityPool), baseBal), "quote transfer failed");
      debts.debtBaseToLiquidityPool -= baseBal;
      emit QuoteReturnedToLiquidityPool(baseBal);
    } else {
      require(baseAsset.transfer(address(liquidityPool), debts.debtBaseToLiquidityPool), "quote transfer failed");
      debts.debtBaseToLiquidityPool = 0;
      emit QuoteReturnedToLiquidityPool(debts.debtBaseToLiquidityPool);
    }
  }

  function repayQuoteToLiquidityPool() internal {
    uint quoteBal = quoteAsset.balanceOf(address(this));
    if (debts.debtQuoteToLiquidityPool > quoteBal ) {
      require(quoteAsset.transfer(address(liquidityPool), quoteBal), "quote transfer failed");
      debts.debtQuoteToLiquidityPool -= quoteBal;
      emit BaseReturnedToLiquidityPool(quoteBal);
    } else {
      require(quoteAsset.transfer(address(liquidityPool), debts.debtQuoteToLiquidityPool), "quote transfer failed");
      debts.debtQuoteToLiquidityPool = 0;
      emit BaseReturnedToLiquidityPool(debts.debtQuoteToLiquidityPool);
    }
  }

  modifier reentrancyGuard virtual {
    counter = counter.add(1); // counter adds 1 to the existing 1 so becomes 2
    uint guard = counter; // assigns 2 to the "guard" variable
    _;
    require(guard == counter, "reentrancy");
  }

  /// Events
  /**
   * @dev Emitted when the short buffer ratio is set.
   */
  event ShortBufferSet(uint newShortBuffer);
  /**
   * @dev Emitted when the interaction delay is set.
   */
  event InteractionDelaySet(uint newInteractionDelay);
  /**
   * @dev Emitted when the short is initialized.
   */
  event ShortInitialized(uint shortId);
  /**
   * @dev Emitted when the hedge position is updated.
   */
  event PositionUpdated(int oldNetDelta, int currentNetDelta, int expectedNetDelta);
  /**
   * @dev Emitted when base is sold
   */
  event BaseExchanged(uint baseAmount);
  /**
   * @dev Emitted when base is sold
   */
  event QuoteExchanged(uint quoteAmount);
  /**
   * @dev Emitted when the long exposure of the hedge contract is adjusted.
   */
  event LongSetTo(uint oldAmount, uint newAmount);
  /**
   * @dev Emitted when short or short collateral is adjusted.
   */
  event ShortSetTo(uint oldShort, uint newShort, uint oldCollateral, uint newCollateral);
  /**
   * @dev Emitted when proceeds of the short are sent back to the LP.
   */
  event QuoteReturnedToLP(uint amountQuote);

  /**
 * @dev Emitted when proceeds of the short are sent back to the LP.
   */
  event BaseReturnedToShortCollateral(uint amountBase);
  event QuoteReturnedToShortCollateral(uint amountQuote);
  event BaseReturnedToLiquidityPool(uint amountBase);
  event QuoteReturnedToLiquidityPool(uint amountQuote);

  event HedgeDelta(int expectedHedge);

}

