// RecPass generatees all of the passes for the rec platform.
// Each pass is rendered as a dynamic SVG and can change over time.
contract RecPass is ERC721, Ownable {
  using Counters for Counters.Counter;

  Counters.Counter private _tokenCounter;

  constructor() public ERC721("RecPass", "REC") {
    // seed for first token.
    _tokenCounter.increment();
  }

  // ============ PUBLIC READ-ONLY FUNCTIONS ============

  function getCurrentTokenID() external view returns (uint256) {
    return _tokenCounter.current();
  }

  // == NFT Core ==

  // Mints a basic mint pass.
  function mint() public returns (uint256) {
    // TODO: Add price checks here

    uint256 tokenId = _tokenCounter.current();
    console.log("Ceating token", tokenId);
    _safeMint(_msgSender(), tokenId);

    // get next token
    _tokenCounter.increment();
    console.log("Next Token", _tokenCounter.current());

    return tokenId;
  }

  function renderRecPass(uint256 tokenId) public view returns (string memory) {
    return
      string(
        abi.encodePacked(
          // header
          '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style>;',
          // letters
          buildLetter(tokenId, 0),
          buildLetter(tokenId, 1),
          buildLetter(tokenId, 2),
          // footer
          "</svg>"
        )
      );
  }

  function renderMetadata(uint256 tokenId) public view returns (string memory) {
    string memory recPass = renderRecPass(tokenId);

    return
      string(
        abi.encodePacked(
          '{"name": "Rec Pass',
          '", "description": "Rec is a creative channel", "image": ""}'
        )
      );

    // return
    //   string(
    //     abi.encodePacked(
    //       '{"name": "Rec Pass',
    //       '", "description": "Rec is a creative channel", "image": "data:image/svg+xml;base64,',
    //       Base64.encode(bytes(recPass)),
    //       '"}'
    //     )
    //   );
  }

  /**
   * @dev See {IERC721Metadata-tokenURI}.
   */
  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(_exists(tokenId), "Nonexistent token");

    return
      string(
        abi.encodePacked(
          "data:image/svg+xml;base64,",
          Base64.encode(bytes(renderMetadata(tokenId)))
        )
      );
  }

  function buildLetter(uint256 tokenId, uint256 index)
    internal
    view
    returns (string memory)
  {
    uint256 rand = Helpers.random(
      string(abi.encodePacked(Helpers.toString(tokenId)))
    );

    return
      string(
        abi.encodePacked(
          '<path d="',
          logoParts[index],
          '" ',
          'fill="',
          foregroundColor[rand % foregroundColor.length],
          '"',
          "/>"
        )
      );
  }

