        RollingTotal = 0;        
        while (WinningGroupWeek == 0){
            uint256 CurrentGroupBalance = _groupBalance[GroupIDList[i]];
            if (RollingTotal < WinningNumberWeek && WinningNumberWeek <= RollingTotal + CurrentGroupBalance){
                WinningGroupWeek = GroupIDList[i];
            } else {
                RollingTotal += CurrentGroupBalance;
            }
            _groupBalance[GroupIDList[i]] = (_groupBalance[GroupIDList[i]]*99)/100;
            i++;
        }

        address[] memory GroupWinnerList = GetGroupHolders(WinningGroupWeek);
        
        for (uint j=0; j<GroupWinnerList.length; j++) {
            uint256 CurrentUserBalance = balanceOf(GroupWinnerList[j]);
            if (RollingTotal < WinningNumberWeek && WinningNumberWeek <= RollingTotal + CurrentUserBalance){
                  WinningWalletWeek = GroupWinnerList[j];
            }
            RollingTotal += CurrentUserBalance; 
        }
        
        WeekCounter++;
        _mint(WinningWalletWeek, EligibleSupply()/100);
    
        LastWeekJackpotTime = block.timestamp;
        NextWeekJackpotTime = LastWeekJackpotTime + WeekJackpotFreq;
        WeekJackpotStarted = 0;
        
    }
    
    function ShowMojo(uint256 Groups) public view virtual returns(uint256){
        uint256 i = 1;
        uint256 Temp;
        while(i <= Groups){
            Temp += (_groupBalance[i]);
            i++;
        }
        return Temp;
    }
    
    function ShowMojo2() public view virtual returns(uint256){
        
        uint256 Groups = ShowGroupIDList().length;
        
        uint256 Temp; 
        uint i = 1; 
        
        while(i <= Groups){

            address[] memory TempList = GetGroupHolders(i); 
            for (uint j=0; j<TempList.length; j++) {
                Temp += balanceOf(TempList[j]);
            }
            i++;
        }
            
        return Temp;
    }
    
    function ShowMojo3() public returns(uint256) {
        randomResult = ShowMojo2();
        return randomResult;
    }
    
    
