pragma solidity ^0.8.0;

import "./interfaces/IRentalsRouter.sol";
import "./libraries/EnumerableMap.sol";
import "./ContractControlList.sol";

contract RentalsRouter is IRentalsRouter {
    using EnumerableMap for EnumerableMap.Bytes32ToAddress;
    using EnumerableSet for EnumerableSet.Bytes32Set;

    EnumerableMap.Bytes32ToAddress internal rentals;
    ContractControlList internal ccl;

    constructor(ContractControlList ccl_) {
        ccl = ccl_;
    }

    modifier ensureRentalIsRegistered(bytes32 _rental) {
        if (!isRegisteredRental(_rental)) {
            revert RentalLogicNotExist(_rental);
        }
        _;
    }

    modifier ensureRentalIsNotRegistered(bytes32 _rental) {
        if (isRegisteredRental(_rental)) {
            revert RentalLogicAlreadyExists(_rental);
        }
        _;
    }

    function ensureHasRentalRegistratonRole(address addr) internal view {
        if (!ccl.hasRentalRegistrationRole(addr)) {
            revert NotAuthorityToRegister();
        }
    }

    modifier onlyRentalRegistratonRole(address addr) {
        ensureHasRentalRegistratonRole(addr);
        _;
    }

    function registerRentalLogic(IRentalLogic _rental)
        external
        override
        ensureRentalIsNotRegistered(_rental.rentalId())
        onlyRentalRegistratonRole(msg.sender)
    {
        bytes32 rentalId = _rental.rentalId();
        address rentalAddr = address(_rental);
        rentals.set(rentalId, rentalAddr);
        emit RentalRegistered(rentalId, rentalAddr);
    }

    function supportedRentalIds()
        external
        view
        override
        returns (bytes32[] memory)
    {
        return rentals._keys.values();
    }

    function isRegisteredRental(bytes32 _rental)
        public
        view
        override
        returns (bool)
    {
        return rentals.contains(_rental);
    }

    function rental(bytes32 _rental)
        external
        view
        override
        ensureRentalIsRegistered(_rental)
        returns (IRentalLogic)
    {
        if (!isRegisteredRental(_rental)) {
            revert RentalLogicNotExist(_rental);
        }

        return IRentalLogic(rentals.get(_rental));
    }
}

