pragma solidity ^0.8.0;

import "./IRentalLogic.sol";

/// @title IRentalsRouter
/// @author Blocklabs
/// @notice This contarct is a type of registry for all rentals logics used by rentable wrapper
interface IRentalsRouter {
    error RentalLogicAlreadyExists(bytes32 _rentalId);
    error RentalLogicNotExist(bytes32 _rentalId);
    error NotAuthorityToRegister();

    event RentalRegistered(bytes32 rentalId, address rentalAddress);

    /// @notice function used to register a particular form of the rental mechanism
    /// @param _rental instance of the rental
    function registerRentalLogic(IRentalLogic _rental) external;

    /// @notice function returns all of the registered rentals logics ids.
    /// @return array of the supported rental logics ids.
    function supportedRentalIds() external view returns (bytes32[] memory);

    /// @notice function returns information about that rental is already registerd
    /// @param _rental id of rental mechanism
    /// @return true if the rental is already registered
    function isRegisteredRental(bytes32 _rental) external view returns (bool);

    /// @notice returns address of the rental logic
    /// @param _rental id of the rental machanism
    /// @return address of the rental logic
    function rental(bytes32 _rental) external view returns (IRentalLogic);
}

