pragma solidity 0.8.11;

contract UniProccesor is Ownable {
    uint256 public constant DECIMALS18 = 1e18;

    IUniswapV2Pair public pair;
    IUniswapV2Router02 public router;
    
    IERC20 public token;

    event PairCreated(IUniswapV2Pair pair);
    event AddLiquidity(uint256 tokenAmount, uint256 native, uint256 liquidity);

    constructor(address router_) {
        require(router_ != address(0), "!=0");
        router = IUniswapV2Router02(router_);
    }

    function createPair(address token_) external onlyOwner {
        require(address(pair) == address(0), "Already initialized");
        require(token_ != address(0), "Zero address");
        
        token = IERC20(token_);
        pair = IUniswapV2Pair(IUniswapV2Factory(router.factory()).createPair(token_, router.WETH()));
        emit PairCreated(pair);
    }

    function addLiquidity(uint256 tokenAmount_) external payable {
        token.approve(address(router), tokenAmount_);
        (
            uint256 tokenAmount,
            uint256 native,
            uint256 liquidity
        ) = router.addLiquidityETH{value: msg.value}(
                address(token),
                tokenAmount_,
                0,
                0,
                owner(),
                block.timestamp
            );
        emit AddLiquidity(tokenAmount, native, liquidity);
    }

    function swap(uint256 amount) external payable {
        address[] memory path = new address[](2);
        path[0] = address(token);
        path[1] = router.WETH();

        token.approve(address(router), amount);
        router.swapExactTokensForETH(amount, 0, path, _msgSender(), block.timestamp);
    }

    function getPrice() external view returns (uint256) {
        return _getPrice();
    }

    //How much one token costs in ETH
    function _getPrice() internal view returns (uint256) {
        (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();

        require(reserve0 > 0 && reserve1 > 0, "Zero amount");

        return
            pair.token0() == router.WETH()
                ? (reserve0 * DECIMALS18) / reserve1
                : (reserve1 * DECIMALS18) / reserve0;
    }
}