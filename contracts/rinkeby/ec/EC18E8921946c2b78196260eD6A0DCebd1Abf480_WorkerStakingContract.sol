library ArrayUtils {
    struct Object {
        uint256 key;
        uint256 value;
    }

    function getFilledArray(uint256 size, uint256 fill)
        internal
        pure
        returns (uint256[] memory array)
    {
        uint256[] memory amounts = new uint256[](size);
        for (uint256 i = 0; i < size; i++) {
            amounts[i] = fill;
        }
        return amounts;
    }

    function isValueInArray(uint256[] memory array, uint256 id)
        internal
        pure
        returns (bool idWasFound)
    {
        for (uint256 i = 0; i < array.length; i++) {
            if (id == array[i]) {
                idWasFound = true;
                break;
            }
        }
    }

    function getIndexByValue(uint256[] memory array, uint256 tokenId)
        internal
        pure
        returns (uint256 index)
    {
        for (uint256 i = 0; i < array.length; i++) {
            if (tokenId == array[i]) {
                index = i;
                break;
            }
        }
    }

    function deleteValueInArray(uint256[] storage array, uint256 tokenId)
        internal
        returns (uint256[] storage)
    {
        uint256 index = getIndexByValue(array, tokenId);

        array[index] = array[array.length - 1];
        array.pop();

        return array;
    }

    function sum(uint256[] memory array)
        internal
        pure
        returns (uint256 sumOfValues)
    {
        for (uint256 i = 0; i < array.length; i++) {
            sumOfValues += array[i];
        }
    }
}

