pragma solidity ^0.8.4;

import "./CommonStakingUtils.sol";

import "./interfaces/CitizenContractInterface.sol";
import "./interfaces/CastleStakingContractInterface.sol";

abstract contract WorkerStakingUtilities is CommonStakingUtilities {
    // -----------------------------------------UTILS - STRUCTS---------------------------------------------------------

    struct Raider {
        uint256 timestamp;
        uint256 dailyCount;
    }

    struct RaidRequest {
        uint256 tokenId;
        address owner;
    }

    // -----------------------------------------UTILS - INTERFACES------------------------------------------------------

    CitizenContractInterface internal citizenContr;
    CastleStakingContractInterface internal castleStakingContr;

    // -----------------------------------------UTILS - EVENTS----------------------------------------------------------

    event RaidRequested(uint256 requestId);

    // -----------------------------------------UTILS - CONSTANTS-------------------------------------------------------
    uint8 public constant MAX_WORKERS_PER_PIECE = 2;
    uint8 public constant MAX_WORKERS_PER_PUZZLE = 50;
    uint8 public constant MAX_RAIDS_PER_DAY = 5;
    uint8 public constant RAID_PERCENTAGE = 30;
    uint8 public constant RAIDER_TAX_RATE = 15;

    uint256[] public RAID_PRICES = [
        0,
        (1 ether) / 2,
        1 ether,
        3 ether,
        5 ether
    ];

    // -----------------------------------------UTILS - MODIFIERS-------------------------------------------------------

    modifier onlyCastleStakingContract() {
        require(msg.sender == address(castleStakingContr), "INVALIDOPERATION");
        _;
    }

    // -----------------------------------------UTILS - STAKING---------------------------------------------------------

    mapping(uint256 => Worker) public stakedWorkers;
    mapping(address => uint256[]) public indexedWorkersByOwner;

    function getStakedWorker(uint256 tokenId)
        public
        view
        returns (Worker memory)
    {
        return stakedWorkers[tokenId];
    }

    function getStakedWorkersLandlord(uint256 tokenId)
        public
        view
        returns (address)
    {
        return stakedWorkers[tokenId].landlord;
    }

    function setTaxesCollectedByLandLordForWorker(
        uint256 tokenId,
        uint256 taxesCollectedByLandlord
    ) public onlyCastleStakingContract {
        stakedWorkers[tokenId]
            .taxesCollectedByLandLord = taxesCollectedByLandlord;
    }

    // -----------------------------------------UTILS - RAIDERS---------------------------------------------------------

    Raider[] public lurkingRaiders;
    mapping(uint256 => RaidRequest) public requestedRaids;

    uint256 internal raidersUnclaimedTaxes;

    // -----------------------------------------UTILS - STATISTICS------------------------------------------------------

    uint256 public totalCrownsEarned;
    uint256 public totalCrownsRaided;
    uint256 public totalRaidsDone;

    // -----------------------------------------UTILS - EVENTS----------------------------------------------------------

    event RaidDone(address raidersOwner, uint256 amountStolen);

    // -----------------------------------------UTILS - VIEW------------------------------------------------------------

    function getMinutesStaked(uint256 tokenId)
        public
        view
        returns (uint256 minutesStaked)
    {
        minutesStaked =
            uint256(block.timestamp - stakedWorkers[tokenId].timestamp) /
            (1 minutes);
    }

    function getRaidCost(uint256 tokenId)
        public
        view
        returns (uint256 timeDiff, uint256)
    {
        timeDiff = block.timestamp - lurkingRaiders[tokenId].timestamp;
        uint256 raidsDone = timeDiff > 1 days
            ? 0
            : lurkingRaiders[tokenId].dailyCount;

        return (timeDiff, RAID_PRICES[raidsDone]);
    }

    // -----------------------------------------UTILS - PURE------------------------------------------------------------

    function getBaseRate(uint256 tokenId, bool isPrivileged)
        public
        pure
        returns (uint256 baseRate)
    {
        require(!isPrivileged && 0 < tokenId && tokenId <= 100, "INVALID");
        require(isPrivileged && 0 < tokenId && tokenId <= 4, "INVALID");

        if (!isPrivileged) {
            if (tokenId <= 25) {
                baseRate = 0.03125 ether;
            } else if (tokenId <= 50) {
                baseRate = 0.416666666666 ether;
            } else if (tokenId <= 75) {
                baseRate = 0.52083333333333 ether;
            } else if (tokenId <= 100) {
                baseRate = 0.10416666666666 ether;
            }
        } else {
            if (tokenId == 1) {
                baseRate = 0.416666666666 ether;
            } else if (tokenId == 2) {
                baseRate = 0.0625 ether;
            } else if (tokenId == 3) {
                baseRate = 0.208333333333 ether;
            } else if (tokenId == 4) {
                baseRate = 0.416666666666 ether;
            }
        }
    }
}

