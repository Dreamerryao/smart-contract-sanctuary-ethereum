pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";

import "./ArrayUtils.sol";

import "./interfaces/CastleStakingContractInterface.sol";

import "./WorkerStakingUtils.sol";
import "./ChainLinkUtils.sol";
import "./VRFConsumerBaseUpgradeableV2.sol";

import "hardhat/console.sol";

contract WorkerStakingContract is
    WorkerStakingUtilities,
    ChainLinkUtils,
    ERC721HolderUpgradeable,
    VRFConsumerBaseUpgradeableV2
{
    using ArrayUtils for uint256[];

    constructor(
        address _vrfCoordinator,
        address _link,
        uint64 _subscriptionId,
        CrownsContractInterface _crowns,
        CitizenContractInterface _citizenContr
    ) {}

    function initialize(
        address _vrfCoordinator,
        address _link,
        uint64 _subscriptionId,
        CrownsContractInterface _crowns,
        CitizenContractInterface _citizenContr
    ) public initializer {
        __VRFConsumerBaseUpgradeableV2__init(_vrfCoordinator);
        __Ownable__init();
        __ERC721Holder_init();
        coordinator = VRFCoordinatorV2Interface(_vrfCoordinator);

        crownsContr = _crowns;
        citizenContr = _citizenContr;

        if (_subscriptionId == 0) {
            linkToken = LinkTokenInterface(_link);

            subscriptionId = coordinator.createSubscription();
            coordinator.addConsumer(subscriptionId, address(this));
        } else {
            subscriptionId = _subscriptionId;
        }
    }

    // -----------------------------------------STAKING - CONFIG--------------------------------------------------------
    /** @dev Initializes connection to the CastleStakingContract
     * @param _castleStakingContr contract address of CastleStakingContract
     */
    function setCastleStakingContractAddress(address _castleStakingContr)
        external
        onlyOwner
    {
        castleStakingContr = CastleStakingContractInterface(
            _castleStakingContr
        );
    }

    // -----------------------------------------STAKING - WORKERS-------------------------------------------------------
    /** @dev Stakes many workers
     * @param tokenIds array of token ids to be staked
     * @param homes array of token ids (pieces/puzzles) where the workers get to work, zero if homeless
     * @param arePrivileged array of booleans that indicate if home is a piece or puzzle
     * @param landlords array of addresses that indicate the owners of said homes
     */
    function stakeManyWorkers(
        uint256[] calldata tokenIds,
        uint256[] calldata homes,
        bool[] calldata arePrivileged,
        address[] calldata landlords
    ) external {
        require(
            tokenIds.length == homes.length &&
                tokenIds.length == arePrivileged.length &&
                tokenIds.length == landlords.length,
            "INVALIDPARAMS"
        );
        for (uint8 i = 0; i < tokenIds.length; i++) {
            require(!citizenContr.isRaider(tokenIds[i]), "NOTAWORKER");
            require(
                citizenContr.ownerOf(tokenIds[i]) == msg.sender,
                "NOTYOURWORKER"
            );
            require(
                citizenContr.isApprovedForAll(msg.sender, address(this)),
                "NOTAPPROVED"
            );
            uint256[] storage stakedTokenIds = indexedWorkersByOwner[
                msg.sender
            ];
            stakedTokenIds.push(tokenIds[i]);
            Worker storage existingStake = stakedWorkers[tokenIds[i]];
            existingStake.timestamp = block.timestamp;
            if (homes[i] != 0) {
                handleWorkerWithHome(
                    existingStake,
                    homes[i],
                    landlords[i],
                    tokenIds[i],
                    arePrivileged[i]
                );
            } else {
                existingStake.landlord = msg.sender;
            }
            citizenContr.safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i]
            );
        }
    }

    /** @dev Called when a worker is staked in combination with a piece/puzzle
     * @param existingStake staked worker struct (storage)
     * @param newHome tokenId of piece/puzzle where the worker gets to work
     * @param landlord address of owner of the piece/puzzle
     * @param tokenId array of addresses that indicate the owners of said homes
     * @param isPrivileged array of addresses that indicate the owners of said homes
     */
    function handleWorkerWithHome(
        Worker storage existingStake,
        uint256 newHome,
        address landlord,
        uint256 tokenId,
        bool isPrivileged
    ) internal {
        uint256 key = generateKey(landlord, tokenId);
        uint256 newHomeWorkersCount;
        uint256 amount;

        (newHomeWorkersCount, amount) = castleStakingContr.getWorkersCount(
            key,
            isPrivileged
        );

        if (!isPrivileged) {
            require(
                newHomeWorkersCount < MAX_WORKERS_PER_PIECE * amount,
                "PIECEFULL"
            );
        } else {
            require(
                newHomeWorkersCount < MAX_WORKERS_PER_PUZZLE * amount,
                "PUZZLEFULL"
            );
        }

        castleStakingContr.addNewWorker(key, tokenId, isPrivileged);

        existingStake.home = newHome;
        existingStake.isPrivileged = isPrivileged;
        existingStake.landlord = landlord;
    }

    /** @dev Unstakes specified tokenIds and transfers earning to the owner
     * @param tokenIds Identifiers of tokens to be unstaked
     */
    function unstakeManyWorkers(uint256[] calldata tokenIds) external {
        (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        ) = calculateRewardsForManyWorkers(tokenIds);

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                citizenContr.ownerOf(tokenIds[i]) == address(this),
                "WORKERNOTSTAKED"
            );
            require(
                indexedWorkersByOwner[msg.sender].isValueInArray(tokenIds[i]),
                "NOTYOURWORKER"
            );

            Worker storage existingStake = stakedWorkers[tokenIds[i]];

            indexedWorkersByOwner[msg.sender].deleteValueInArray(tokenIds[i]);

            if (existingStake.home != 0) {
                uint256 key = generateKey(
                    existingStake.landlord,
                    existingStake.home
                );

                castleStakingContr.removeWorker(
                    key,
                    tokenIds[i],
                    existingStake.isPrivileged
                );

                if (existingStake.landlord == msg.sender) {
                    rewards[i] += landlordTaxes[i];
                } else {
                    crownsContr.mint(existingStake.landlord, landlordTaxes[i]);
                }
            }

            existingStake.owner = address(0);
            existingStake.landlord = address(0);
            existingStake.home = 0;

            existingStake.stolen = 0;
            existingStake.taxesCollectedByLandLord = 0;

            citizenContr.transferFrom(address(this), msg.sender, tokenIds[i]);
        }

        raidersUnclaimedTaxes += raidersTaxes.sum();

        crownsContr.mint(msg.sender, rewards.sum());
    }

    // -----------------------------------------STAKING - REWARDS - WORKERS---------------------------------------------

    /** @dev Collects the rewards for many workers
     * @param tokenIds Identifiers of the staked workers
     *
     */
    function collectManyWorkersRewards(uint256[] calldata tokenIds) external {
        (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        ) = calculateRewardsForManyWorkers(tokenIds);

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                citizenContr.ownerOf(tokenIds[i]) == address(this),
                "WORKERNOTSTAKED"
            );
            require(
                indexedWorkersByOwner[msg.sender].isValueInArray(tokenIds[i]),
                "NOTYOURWORKER"
            );

            Worker storage existingStake = stakedWorkers[tokenIds[i]];

            existingStake.stolen = 0;
            existingStake.taxesCollectedByLandLord = 0;

            if (existingStake.landlord == msg.sender) {
                rewards[i] += landlordTaxes[i];
            } else {
                crownsContr.mint(existingStake.landlord, landlordTaxes[i]);
            }
        }

        raidersUnclaimedTaxes += raidersTaxes.sum();
        crownsContr.mint(msg.sender, rewards.sum());
    }

    // -----------------------------------------STAKING - REWARDS - RAIDERS---------------------------------------------

    /** @dev Collects the rewards for raiders
     * @param tokenIds Identifiers of the raiders
     */
    function collectManyRaidersRewards(uint256[] calldata tokenIds) external {
        uint256 raidersRewards;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                citizenContr.ownerOf(tokenIds[i]) == msg.sender,
                "NOTYOURRAIDER"
            );

            raidersRewards += raidersUnclaimedTaxes / lurkingRaiders.length;
        }

        crownsContr.mint(msg.sender, raidersRewards);
    }

    // -----------------------------------------RAIDS - WORKERS---------------------------------------------------------
    /** @dev Initiates a Raid for a given raider and requests a random number to choose the worker
     * @param tokenId Identifier of the raider
     */
    function raidAWorker(uint256 tokenId) public payable {
        (uint256 timeDiff, uint256 raidCost) = getRaidCost(tokenId);

        require(citizenContr.ownerOf(tokenId) == msg.sender, "NOTYOURRAIDER");
        require(msg.value >= raidCost, "UNDERPRICED");

        Raider memory raider = lurkingRaiders[tokenId];

        require(raider.dailyCount < MAX_RAIDS_PER_DAY, "MAXRAIDSDONE");

        if (timeDiff > 1 days) {
            raider.timestamp = block.timestamp;
            raider.dailyCount = 1;
        } else {
            raider.dailyCount++;
        }

        uint256 requestId = coordinator.requestRandomWords(
            KEYHASH,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            1
        );

        requestedRaids[requestId] = RaidRequest(tokenId, msg.sender);
        emit RaidRequested(requestId);
    }

    /** @dev Callback function which is executed after a VRF request was successful
     * @param requestId Identifier of the previously sent request
     * @param randomWords Result of the request containing the actual random numbers
     */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        address raidersOwner = citizenContr.ownerOf(
            requestedRaids[requestId].tokenId
        );

        require(raidersOwner != requestedRaids[requestId].owner, "INVALID");

        uint256 raidedWorkerId;
        uint256 raidedWorkerIndex;

        uint256 randomNr = uint256(keccak256(abi.encode(randomWords[0], 1)));

        for (uint256 i = 0; true; i++) {
            raidedWorkerIndex =
                randomNr %
                citizenContr.balanceOf(address(this));
            raidedWorkerId = citizenContr.tokenOfOwnerByIndex(
                address(this),
                raidedWorkerIndex
            );

            if (
                stakedWorkers[raidedWorkerId].isPrivileged &&
                stakedWorkers[raidedWorkerId].home == 1
            ) {
                randomNr = uint256(keccak256(abi.encode(randomNr, 1)));
                continue;
            }

            break;
        }

        uint256[] memory arr = new uint256[](1);
        arr[0] = raidedWorkerId;

        (uint256[] memory rewards, , ) = calculateRewardsForManyWorkers(arr);

        uint256 amountStolen = (((rewards[0]) * RAID_PERCENTAGE) / 100);
        stakedWorkers[raidedWorkerId].stolen = amountStolen;

        crownsContr.mint(raidersOwner, amountStolen);
        emit RaidDone(raidersOwner, amountStolen);
    }

    // -----------------------------------------UTILS - VIEW------------------------------------------------------------

    /** @dev Calculates the collectable staking reward of a single token
     * @param tokenIds Identifier token (worker)
     * @return rewards Amount of CROWNS earned by this worker
     * @return landlordTaxes Amount of CROWNS earned by this worker belonging to the landlord
     * @return raidersTaxes Amount of CROWNS earned by this worker belonging to the raiders
     */
    function calculateRewardsForManyWorkers(uint256[] memory tokenIds)
        public
        view
        returns (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        )
    {
        uint256 baseRatePerMinute = 0.0138888888888 ether;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            Worker memory existingStake = stakedWorkers[tokenIds[i]];
            uint256 minutesStaked = getMinutesStaked(tokenIds[i]);

            if (minutesStaked < 1 days) {
                rewards[i] = 0;
                landlordTaxes[i] = 0;
                raidersTaxes[i] = 0;
            } else {
                if (existingStake.home != 0) {
                    baseRatePerMinute = getBaseRate(
                        existingStake.home,
                        existingStake.isPrivileged
                    );
                }

                uint256 landlordTaxRate = existingStake.landlord != msg.sender
                    ? castleStakingContr.getTaxRate(
                        generateKey(existingStake.landlord, existingStake.home),
                        existingStake.isPrivileged
                    )
                    : 0;

                uint256 untaxedReward = (baseRatePerMinute * minutesStaked) -
                    existingStake.stolen;

                raidersTaxes[i] = (((untaxedReward * RAIDER_TAX_RATE)) / 100);
                landlordTaxes[i] = (((untaxedReward - raidersTaxes[i]) *
                    landlordTaxRate) / 100);

                rewards[i] = untaxedReward - raidersTaxes[i] - landlordTaxes[i];
            }
        }
    }

    // -----------------------------------------UTILS - PURE------------------------------------------------------------
    /*** @dev Whenever an {IERC721} `tokenId` token is transferred to this contract via {IERC721-safeTransferFrom}
     * by `operator` from `from`, this function is called.
     *
     * It must return its Solidity selector to confirm the token transfer.
     * If any other value is returned or the interface is not implemented by the recipient,
     * the transfer will be reverted.
     *
     * The selector can be obtained in Solidity with `IERC721Receiver.onERC721Received.selector`.
     */
    /* solhint-disable no-unused-vars */
    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes memory data
    ) public pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

