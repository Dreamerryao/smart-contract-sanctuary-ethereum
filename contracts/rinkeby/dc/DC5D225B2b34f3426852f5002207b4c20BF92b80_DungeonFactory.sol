import "./Controllable.sol";
import "../proxy/ProxyControlled.sol";
import "../lib/StatLib.sol";
import "../openzeppelin/EnumerableSet.sol";

contract DungeonFactory is Controllable, IDungeonFactory {
  using EnumerableSet for EnumerableSet.AddressSet;

  // ---- CONSTANTS ----

  /// @notice Version of the contract
  /// @dev Should be incremented when contract changed
  string public constant VERSION = "1.0.0";
  uint public constant MIN_LEVEL_FOR_TREASURY = 5;

  // ---- VARIABLES ----

  EnumerableSet.AddressSet internal _freeDungeons;
  mapping(uint => EnumerableSet.AddressSet) internal _freeDungeonsByLevel;

  // ---- EVENTS ----

  event DungeonLaunched(
    address dungeon,
    address heroToken,
    uint heroTokenId,
    address treasuryToken,
    uint treasuryAmount
  );

  // ---- INITIALIZER ----

  function init(
    address controller_
  ) external initializer {
    __Controllable_init(controller_);
  }

  // ---- RESTRICTIONS ----

  function onlyDungeon() internal view {
    require(IController(_controller()).validDungeons(msg.sender), "Not dungeon");
  }

  // ---- VIEWS ----

  function _statController() internal view returns (IStatController) {
    return IStatController(IController(_controller()).statController());
  }

  function _treasury() internal view returns (ITreasury) {
    return ITreasury(IController(_controller()).treasury());
  }

  function _oracle() internal view returns (IOracle) {
    return IOracle(IController(_controller()).oracle());
  }

  function treasuryPart(address payToken, uint level) external view returns (uint) {
    return _treasuryPart(payToken, level);
  }

  function _treasuryPart(address payToken, uint heroLevel) internal view returns (uint) {
    uint treasuryAmount = _treasury().balanceOfToken(payToken);
    require(heroLevel <= StatLib.MAX_LEVEL, "Wrong level");
    if (heroLevel < MIN_LEVEL_FOR_TREASURY) {
      return 0;
    }
    heroLevel -= (MIN_LEVEL_FOR_TREASURY - 1);

    uint256 maxMultiplier = StatLib.log2((StatLib.MAX_LEVEL + 1) * 1e18);
    uint256 multiplier = (maxMultiplier - StatLib.log2((StatLib.MAX_LEVEL - heroLevel + 1) * 1e18)) / 1000;
    require(multiplier < 1e18, "Wrong multiplier");
    return treasuryAmount * multiplier / 1e18;
  }

  function dungeonLevelForHeroLevel(uint heroLevel) external pure returns (uint) {
    return _dungeonLevelForHeroLevel(heroLevel);
  }

  function _dungeonLevelForHeroLevel(uint heroLevel) internal pure returns (uint) {
    return heroLevel - (heroLevel % StatLib.DUNGEON_LEVEL_STEP) + 1;
  }

  function freeDungeonsLength() external view returns (uint) {
    return _freeDungeons.length();
  }

  function freeDungeons(uint id) external view returns (address) {
    return _freeDungeons.at(id);
  }

  function freeDungeonsByLevelLength(uint level_) external view returns (uint) {
    return _freeDungeonsByLevel[level_].length();
  }

  function freeDungeonsByLevel(uint id, uint level_) external view returns (address) {
    return _freeDungeonsByLevel[level_].at(id);
  }

  // ---- ACTIONS ----

  function launch(address heroToken, uint heroTokenId, address treasuryToken) external returns (address) {
    require(IController(_controller()).validHeroes(heroToken), "Not hero");
    require(IHero(heroToken).isOwner(msg.sender, heroTokenId), "Not hero owner");
    uint heroLevel = IHero(heroToken).stats(heroTokenId).level;

    address dungeonLogic = _getDungeonLogic(_dungeonLevelForHeroLevel(heroLevel));
    address dungeonProxy = address(new ProxyControlled(dungeonLogic));

    IController(_controller()).registerDungeon(dungeonProxy);

    IDungeon(dungeonProxy).init(
      _controller(),
      heroToken,
      heroTokenId,
      treasuryToken
    );

    require(IDungeon(dungeonProxy).dungeonLevel() * StatLib.DUNGEON_LEVEL_STEP >= heroLevel, "Wrong dungeon level");

    uint treasuryAmount = _treasuryPart(treasuryToken, heroLevel);
    if (treasuryAmount != 0) {
      _treasury().sendToDungeon(dungeonProxy, treasuryToken, treasuryAmount);
    }

    emit DungeonLaunched(dungeonProxy, heroToken, heroTokenId, treasuryToken, treasuryAmount);
    return dungeonProxy;
  }

  function addFreeDungeon(address dungeon_) external override {
    onlyDungeon();
    require(_freeDungeons.add(dungeon_), "Dungeon not free");
    require(_freeDungeonsByLevel[IDungeon(dungeon_).dungeonLevel()].add(dungeon_), "Dungeon not free by level");
  }

  /// @dev Can be called with not existing dungeon
  function removeFreeDungeon(address dungeon_) external override {
    onlyDungeon();
    _freeDungeons.remove(dungeon_);
    _freeDungeonsByLevel[IDungeon(dungeon_).dungeonLevel()].remove(dungeon_);
  }

  function _getDungeonLogic(uint dungeonLevel) internal returns (address) {
    require(dungeonLevel != 0, "Dungeon level start from 1");
    uint size = IController(_controller()).dungeonImplementationsLength(dungeonLevel);
    if (size == 0) {
      require(dungeonLevel > 1, "No dungeon for this level");
      return _getDungeonLogic(dungeonLevel - 1);
    }
    if (size == 1) {
      return IController(_controller()).dungeonImplementationsByLevel(dungeonLevel, 0);
    }
    return IController(_controller())
    .dungeonImplementationsByLevel(dungeonLevel, _oracle().getRandomNumber(size - 1));
  }

}

