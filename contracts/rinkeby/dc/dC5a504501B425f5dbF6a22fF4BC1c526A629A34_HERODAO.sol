        delete _ownedTokens[from][lastTokenIndex];
    }

    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {

        uint256 lastTokenIndex = _allTokens.length - 1;
        uint256 tokenIndex = _allTokensIndex[tokenId];

        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        // This also deletes the contents at the last position of the array
        delete _allTokensIndex[tokenId];
        _allTokens.pop();
    }
}

pragma solidity ^0.8.0;

contract HERODAO is ERC721, ERC721Enumerable, Ownable {

    using Strings for uint256;
    using SafeMath for uint256;

    uint256 public constant tokenPrice = 500000000000000; // 0.05 ETH
    uint public maxTokenPurchase = 30;
    uint public maxTokenPerWallet = 10000;
    uint256 public MAX_TOKENS = 10000;
    bool public saleIsActive = false;
    bool public revealed = false;
    string public notRevealedUri;
    string public baseExtension = ".json";
    string private _baseURIextended;

    address payable treasury = payable(0x64108034f4e255DAa4425057a0297E5F74f2822c); // Hero Treasury
    address payable bank = payable(0xB565D3A7Bcf568f231726585e0b84f9E2a3722dB); // Bank

    GOLD public GOLD_TOKEN = GOLD(0xb7C00E0A5d172746384a765c29Ae43e5A611c491);

    struct Hero {
        uint256 rarity;
        uint256 goldPerDay;
        uint256 goldCollectedLast;
        uint256 tokenId;
    }

    mapping(uint256 => Hero) public heros;

    mapping (address => uint) private _numberOfWallets;
    // mapping (address => uint[]) private _usertokenId;
    
    event SetMaxTokensPurchase(uint _maxTokens);
    event SetMaxTokensWallet(uint _maxTokens);
    
    constructor( ) ERC721("HeroDAO", "HERODAO") {
    }
    
    function updateBank(address payable _b) 
    public onlyOwner
    {
        bank = _b;
    }

    function updateGold(address _g) 
    public onlyOwner
    {
        GOLD_TOKEN = GOLD(_g);
    }

    function reveal() public onlyOwner {
        revealed = true;
    }

    function setNotRevealedURI(string memory _notRevealedURI) public onlyOwner {
        notRevealedUri = _notRevealedURI;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function setBaseURI(string memory baseURI_) external onlyOwner() {
        _baseURIextended = baseURI_;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseURIextended;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
        _exists(tokenId),
        "ERC721Metadata: URI query for nonexistent token"
        );
        
        if(revealed == false) {
            return notRevealedUri;
        }


        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0
            ? string(abi.encodePacked(currentBaseURI, ( tokenId + 1 ).toString(), baseExtension))
            : "";
    }

    function flipSaleState() public onlyOwner {
        saleIsActive = !saleIsActive;
    }
    
    function mintToken(uint numberOfTokens, uint256[] memory rarity) public payable {
        require(saleIsActive, "Sale must be active to mint Tokens");
        require(numberOfTokens <= maxTokenPurchase, "Exceeded max token purchase");
        require(_numberOfWallets[msg.sender] + numberOfTokens <= maxTokenPerWallet, "Exceeded max token purchase per wallet");
        require(totalSupply() + numberOfTokens <= MAX_TOKENS, "Purchase would exceed max supply of tokens");

        require(mintPrice() * numberOfTokens <= msg.value, "Ether value sent is not correct");

        for(uint i = 0; i < numberOfTokens; i++) {
            uint256 mintIndex = totalSupply();
            if (totalSupply() < MAX_TOKENS) {
                _safeMint(msg.sender, mintIndex);
                createHero(mintIndex, rarity[i]);
                // _usertokenId[msg.sender].push(mintIndex)
            }
        }
        _numberOfWallets[msg.sender] = _numberOfWallets[msg.sender] + numberOfTokens;
        payAccounts();
    }
    function mintPrice() private view returns (uint256) 
    {
        if(totalSupply() <= 1000 ) {
            return 500000000000000;
        }
        else if(totalSupply() <= 3000) {
            return 700000000000000;
        }
        else if(totalSupply() <= 6000) {
            return 800000000000000;
        }
        else {
            return 900000000000000;
        }
    }
    function getTokenids(address owneraddress) public view returns (uint256[] memory){
        uint256[] memory ids;
        for (uint256 i = 0; i < balanceOf(owneraddress); i++) {
            uint256 index = tokenOfOwnerByIndex(owneraddress, i);
            ids[i] = index;
        }
        return ids;
    }
    function createHero(uint256 id, uint256 rarity) 
    private
    {
        uint256 goldPerDay = getGoldPerDay(rarity);
        uint32 dayInSeconds = 86400;
        heros[id] = Hero(
            rarity,
            goldPerDay,
            block.timestamp - dayInSeconds,
            id
        );
    }

    function getGoldPerDay(uint256 rarity) 
    private
    pure
    returns (uint256)
    {
        if (rarity == 1) {
            return 50; // rare
        }
        else if (rarity == 2) {
            return 25; // uncommon
        }
        else {//3
            return 10; // common
        }
    }

    function collectGold(uint256 id)
    public 
    {
        Hero memory hero = heros[id];
        uint32 dayInSeconds = 86400;
        require(ownerOf(id) == msg.sender, "only owner can collect eggs.");
        require(block.timestamp > (hero.goldCollectedLast + dayInSeconds), "gold already collected.");
        GOLD_TOKEN.mint(msg.sender, hero.goldPerDay * ((block.timestamp - hero.goldCollectedLast) / dayInSeconds));
        heros[id].goldCollectedLast = block.timestamp;
    }

    function collectAll()
    public 
    {
        uint256 gold = 0;
        for (uint8 i = 0; i < balanceOf(msg.sender); i++) {
            uint256 index = tokenOfOwnerByIndex(msg.sender, i);
            Hero memory hero = heros[index];
            uint32 dayInSeconds = 86400;
            if (block.timestamp > (hero.goldCollectedLast + dayInSeconds)) {
                gold += hero.goldPerDay * ((block.timestamp - hero.goldCollectedLast) / dayInSeconds);
                heros[index].goldCollectedLast = block.timestamp;
            }
        }
        GOLD_TOKEN.mint(msg.sender, gold);
    }

    function payAccounts() public payable {
        uint256 balance = address(this).balance;
        if (balance > 0) {
            // uint256 devCut = balance.mul(30).div(100);
            // uint256 bankCut = balance.mul(70).div(100);
            uint256 bankCut = balance.mul(100).div(100);
            // treasury.transfer(devCut);
            bank.transfer(bankCut);
        }
    }

    // Update Max Tokens
    function setMaxTokensPurchase(uint _maxTokens) external onlyOwner {
        maxTokenPurchase = _maxTokens;
        emit SetMaxTokensPurchase(_maxTokens);
    }

    function setMaxTokensWallet(uint _maxTokens) external onlyOwner {
        maxTokenPerWallet = _maxTokens;
        emit SetMaxTokensWallet(_maxTokens);
    }
    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

}

