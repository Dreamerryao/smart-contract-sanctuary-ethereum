pragma solidity ^0.8.0;

library HFEnum {
    /// 0 - Void: New tickets have no status initially
    /// 1 - Open: Open for Investment
    /// 2 - Closed: the ticket (the deal) is closed. No longer accepting investment
    /// 3 - Canceled: ticket is canceled before the closing date
    /// 4 - Expired: no claim was made until the end of the coverage period
    /// 5 - ClaimProcessing:  ClaimStatus is neither 'NotStarted' nor 'PendingClaim'
    /// 6 - Claimed: claim event is verified and the buyer received the claim
    enum TicketStatus {
        Void,
        Open,
        Closed,
        Canceled,
        Expired,
        ClaimProcessing,
        Claimed
    }

    /// These are subStatuses under TicketStatus 'ClaimProcessing'
    /// 0 - NotStarted: Claim process has not started yet.
    /// 1 - ClaimInProcess: Getting data and making claim decision
    /// 2 - ClaimUnderReview: Decision been made and waiting for user response
    /// 3 - JuryProcessing: Jurors are in voting process on the claim
    /// 4 - Buyer took the claim
    enum ClaimStatus {
        NotStarted,
        ClaimInProcess,
        ClaimUnderReview,
        JuryProcessing,
        ClaimRewarded
    }
    
    /// These are possible voting options for Jury
    /// 0 - NoVote
    /// 1 - Accept
    /// 2 - Reject
    enum JuryVote {
        NoVote,
        Accept,
        Reject
    }

    /// These are possible NFT token types
    /// 0 - None
    /// 1 - Hedgefy NFT
    /// 2 - ERC721 (eg. Rarible)
    /// 3 - ERC1155 (eg. OpenSea)
    enum NFTType {
        None,
        HFNFT,
        ERC721,
        ERC1155
    }
}

