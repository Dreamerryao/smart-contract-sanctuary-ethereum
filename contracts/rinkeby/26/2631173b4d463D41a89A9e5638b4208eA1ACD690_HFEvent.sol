pragma solidity ^0.8.0;

import "./HFEnum.sol";

library HFEntity {
    // Ticket contains final (fixed) values (-1 in case the value is currently in bidding process.)
    struct Ticket {
        uint256 ticketId;
        string bidProcessType;
        int256 claimAmount;         // Final claim amount
        int256 premiumAmount;       // buyer needs to payoff as bidding selected as final premium amount
        int256 authorizedAmount;    // Investor needs to payoff as bidding selected as marginRatio * final claim amount / 100
        int256 donatedAmount;
        int64 marginRatio;          // marginRatio is in 0 - 100
        string ticketName;
        HFEnum.TicketStatus ticketStatus;
        HFEntity.TicketDate dates;
        HFEntity.Investment selectedBidding;
    }

    struct TicketDate {
        uint256 closingDate;
        uint256 startDate;
        uint256 endDate;
    }

    // Investment contains investor's bidding values
    struct Investment {
        uint256 ticketId;
        address investorAddress;
        int256 askingAmount;        // bidding amount as submitting the bid
        int256 earning;
        string bidProcessType;
        string ticketName;
        bool tookPremium;
        bool reimbursedInvest;
        bool removed;
        int256 expiredInvestAmount;
        uint256 askingExpireDate;
        uint256 investDate;
    }

    // Premium contains buyer's proposals
    struct Premium {
        uint256 ticketId;
        address buyerAddress;
        int256 askingClaimAmount;
        int256 askingPremiumAmount;
        string bidProcessType;
        string ticketName;
        bool reimbursedPremium;
    }

    struct Claim {
        uint256 claimId;
        uint8 round;
        HFEnum.ClaimStatus claimStatus;
        ClaimCondition condition;
        bool isApproved;
		uint256 lastDecisionDate;
        uint256 oracleData;
        uint256 roundStartDate;
    }

    struct ClaimCondition {
        string claimType;
        string[] constraints;
        string[] parameters;
    }

    struct JuryResult {
        uint8 Accept;
        uint8 Reject;
    }
    
    struct NFT {
        uint256 tokenId;
        address tokenContract;
        HFEnum.NFTType tokenType;
    }

    struct Donation {
        uint256 ticketId;
        address donorAddress;
        int256 amount;
        string option;
        bool refunded;
    }
}

