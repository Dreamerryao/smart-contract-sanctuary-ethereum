import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./library/HFEnum.sol";
import "./library/HFEntity.sol";

contract HFEvent {

    event TicketCreated(
        address indexed user,
        uint256 indexed ticketId,
        string bidProcessType,
        int256 claimAmount,
        int256 premiumAmount,
        int256 authorizedAmount,
        int64 marginRatio,
        string ticketName,
        HFEnum.TicketStatus ticketStatus,
        HFEntity.TicketDate dates,
        uint256 eventTimestamp
    );

    event TicketStatusUpdate(
        address indexed user,
        uint256 indexed ticketId,
        string bidProcessType,
        int256 claimAmount,
        int256 premiumAmount,
        int256 authorizedAmount,
        int64 marginRatio,
        string ticketName,
        HFEnum.TicketStatus previousStatus,
        HFEnum.TicketStatus ticketStatus,
        HFEntity.TicketDate dates,
        HFEntity.Investment selectedBidding,
        uint256 eventTimestamp
    );

    event PremiumCreated(
        address indexed buyer,
        uint256 indexed ticketId,
        int256 askingClaimAmount,
        int256 askingPremiumAmount,
        string bidProcessType,
        string ticketName,
        bool reimbursedPremium,
        uint256 eventTimestamp
    );

    event NFTMinted(
        address user,
        uint256 tokenId,
        string nftURI,
        uint256 eventTimestamp
    );

    event NFTBurned(
        uint256 indexed tokenId,
        uint256 eventTimestamp
    );

    event NFTTransferred(
        address indexed from,
        address indexed to,
        uint256 indexed tokenId,
        uint256 eventTimestamp
    );

    event NFTCreated(
        uint256 indexed tokenId,
        address indexed tokenContract,
        HFEnum.NFTType tokenType,
        uint256 eventTimestamp
    );

    event NFTAdded(
        uint256 indexed ticketId,
        uint256 indexed tokenId,
        uint256 eventTimestamp
    );

    event BiddingAdded(
        address indexed investor,
        uint256 indexed ticketId,
        string bidProcessType,
        int256 askingAmount,
        uint256 askingExpireDate,
        string ticketName,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event BiddingUpdated(
        address indexed investor,
        uint256 indexed ticketId,
        string bidProcessType,
        int256 askingAmount,
        uint256 askingExpireDate,
        string ticketName,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event BiddingRemoved(
        address indexed investor,
        uint256 indexed ticketId,
        string bidProcessType,
        uint256 eventTimestamp
    );

    event BiddingSelected(
        address indexed investor,
        uint256 indexed ticketId,
        string bidProcessType,
        int256 askingAmount,
        int256 claimAmount,
        int256 premiumAmount,
        string ticketName,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event DonationCreated(
        address indexed donor,
        uint256 indexed ticketId,
        int256 amount,
        string option,
        bool refunded,
        uint256 eventTimestamp
    );

    event DonationUpdated(
        address indexed donor,
        uint256 indexed ticketId,
        int256 amount,
        string option,
        bool refunded,
        uint256 eventTimestamp
    );

    event DonationsReceived(
        address indexed donee,
        uint256 indexed ticketId,
        int256 sum,
        uint256 eventTimestamp
    );

    event DonationRefunded(
        address indexed donor,
        uint256 indexed ticketId,
        int256 amount,
        string option,
        bool refunded,
        uint256 eventTimestamp
    );

    event InvestorEarned(
        address indexed investor,
        uint256 indexed ticketId,
        int256 investAmount,
        int256 claimAmount,
        int256 premiumAmount,
        int256 earning,
        string ticketName,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event InvestReimbursed(
        address indexed investor,
        uint256 indexed ticketId,
        int256 reimburseAmount,
        int256 totalInvestments,
        string ticketName,
        uint256 eventTimestamp
    );

    event PremiumReimbursed(
        address indexed buyer,
        uint256 indexed ticketId,
        int256 reimburseAmount,
        int256 totalPremium,
        string ticketName,
        uint256 eventTimestamp
    );

    event InvestsClaimed(
        address indexed buyer,
        uint256 indexed ticketId,
        int256 totalInvestments,
        string ticketName,
        uint256 eventTimestamp
    );

    event HFCoinMinted(
        address indexed user,
        uint256 indexed ticketId,
        string ticketName,
        string currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event HFCoinBurned(
        address indexed user,
        uint256 indexed ticketId,
        string ticketName,
        string currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event HFCoinTransferredFrom(
        address indexed sender,
        address indexed receiver,
        uint256 indexed ticketId,
        string ticketName,
        string currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus,
        uint256 eventTimestamp
    );

    event HFClaimCreated(
        address indexed user,
        uint256 indexed ticketId,
        uint256 indexed claimId,
        uint8 round,
        HFEnum.ClaimStatus claimStatus,
        bool isApproved,
        uint256 oracleData,
        uint8 juryAccept,
        uint8 juryReject,
        uint256 lastDecisionDate,
        uint256 roundStartDate,
		HFEntity.ClaimCondition condition,
        uint256 eventTimestamp
    );

    event HFClaimUpdated(
        address indexed user,
        uint256 indexed ticketId,
        uint256 indexed claimId,
        uint8 round,
        HFEnum.ClaimStatus claimStatus,
        bool isApproved,
        uint256 oracleData,
        uint8 juryAccept,
        uint8 juryReject,
        uint256 lastDecisionDate,
        uint256 roundStartDate,
        uint256 eventTimestamp
    );

    function EmitHFCoinMinted(
        address user,
        uint256 ticketId,
        string calldata ticketName,
        string calldata currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus
    ) external {
        emit HFCoinMinted(user, ticketId, ticketName, currency, amount, ticketStatus, block.timestamp);
    }

    function EmitHFCoinBurned(
        address user,
        uint256 ticketId,
        string calldata ticketName,
        string calldata currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus
    ) external {
        emit HFCoinBurned(user, ticketId, ticketName, currency, amount, ticketStatus, block.timestamp);
    }

    function EmitHFCoinTransferredFrom(
        address sender,
        address recipient,
        uint256 ticketId,
        string calldata ticketName,
        string calldata currency,
        uint256 amount,
        HFEnum.TicketStatus ticketStatus
    ) external {
        emit HFCoinTransferredFrom(sender, recipient, ticketId, ticketName, currency, amount, ticketStatus, block.timestamp);
    }

    function EmitTicketCreated(
        address user,
        uint256 ticketId,
        HFEntity.Ticket memory ticket) external {

        emit TicketCreated(
            user,
            ticketId,
            ticket.bidProcessType,
            ticket.claimAmount,
            ticket.premiumAmount,
            ticket.authorizedAmount,
            ticket.marginRatio,
            ticket.ticketName,
            ticket.ticketStatus,
            ticket.dates,
            block.timestamp);
    }

    function EmitTicketStatusUpdate(
        address user,
        uint256 ticketId, HFEnum.TicketStatus previousStatus,
        HFEntity.Ticket memory ticket) external {

        emit TicketStatusUpdate(
            user,
            ticketId,
            ticket.bidProcessType,
            ticket.claimAmount,
            ticket.premiumAmount,
            ticket.authorizedAmount,
            ticket.marginRatio,
            ticket.ticketName,
            previousStatus,
            ticket.ticketStatus,
            ticket.dates,
            ticket.selectedBidding,
            block.timestamp);
    }

    function EmitPremiumCreated(
        address buyer,
        uint256 ticketId,
        HFEntity.Premium memory premium) external {

        emit PremiumCreated(
            buyer,
            ticketId,
            premium.askingClaimAmount,
            premium.askingPremiumAmount,
            premium.bidProcessType,
            premium.ticketName,
            premium.reimbursedPremium,
            block.timestamp);
    }

    function EmitBiddingAdded(
        address investor, uint256 ticketId,
        string calldata bidProcessType, int256 askingAmount,
        uint256 askingExpireDate, string calldata ticketName,
        HFEnum.TicketStatus ticketStatus) external {

        emit BiddingAdded(
            investor,
            ticketId,
            bidProcessType,
            askingAmount,
            askingExpireDate,
            ticketName,
            ticketStatus,
            block.timestamp
        );
    }

    function EmitBiddingUpdated(
        address investor, uint256 ticketId,
        string calldata bidProcessType, int256 askingAmount,
        uint256 askingExpireDate, string calldata ticketName,
        HFEnum.TicketStatus ticketStatus) external {

        emit BiddingUpdated(
            investor,
            ticketId,
            bidProcessType,
            askingAmount,
            askingExpireDate,
            ticketName,
            ticketStatus,
            block.timestamp
        );
    }

    function EmitBiddingRemoved(
        address investor,
        uint256 ticketId,
        string calldata bidProcessType) external {

        emit BiddingRemoved(investor, ticketId, bidProcessType, block.timestamp);
    }

    function EmitBiddingSelected(
        address investor, uint256 ticketId,
        string calldata bidProcessType, int256 askingAmount,
        int256 claimAmount, int256 premiumAmount,
        string calldata ticketName, HFEnum.TicketStatus ticketStatus) external {

        emit BiddingSelected(
            investor,
            ticketId,
            bidProcessType,
            askingAmount,
            claimAmount,
            premiumAmount,
            ticketName,
            ticketStatus,
            block.timestamp
        );
    }

    function EmitDonationCreated(
        address donor,
        uint256 ticketId,
        HFEntity.Donation memory donate) external {

        emit DonationCreated(
            donor,
            ticketId,
            donate.amount,
            donate.option,
            donate.refunded,
            block.timestamp
        );
    }

    function EmitDonationUpdated(
        address donor,
        uint256 ticketId,
        int256 amount,
        string calldata option,
        bool refunded) external {

        emit DonationUpdated(
            donor,
            ticketId,
            amount,
            option,
            refunded,
            block.timestamp
        );
    }

    function EmitDonationsReceived(
        address donee,
        uint256 ticketId,
        int256 sum) external {

        emit DonationsReceived(
            donee,
            ticketId,
            sum,
            block.timestamp);
    }

    function EmitDonationRefunded(
        address donor,
        uint256 ticketId,
        int256 amount,
        string calldata option,
        bool refunded) external {

        emit DonationRefunded(
            donor,
            ticketId,
            amount,
            option,
            refunded,
            block.timestamp);
    }

    function EmitInvestorEarned(
        address investor,
        uint256 ticketId,
        int256 earning,
        HFEntity.Ticket memory ticket) external {

        emit InvestorEarned(
            investor,
            ticketId,
            ticket.authorizedAmount,
            ticket.claimAmount,
            ticket.premiumAmount,
            earning,
            ticket.ticketName,
            ticket.ticketStatus,
            block.timestamp
        );
    }

    function EmitInvestReimbursed(
        address investor, uint256 ticketId,
        int256 reimburseAmount, int256 totalInvestments,
        string memory ticketName) external {

        emit InvestReimbursed(
            investor,
            ticketId,
            reimburseAmount,
            totalInvestments,
            ticketName,
            block.timestamp
        );
    }

    function EmitPremiumReimbursed(
        address buyer, uint256 ticketId,
        int256 reimburseAmount, int256 totalPremium,
        string memory ticketName) external {

        emit PremiumReimbursed(
        buyer,
        ticketId,
        reimburseAmount,
        totalPremium,
        ticketName,
        block.timestamp
        );
    }

    function EmitInvestsClaimed(
        address buyer, uint256 ticketId,
        int256 totalInvestments, string memory ticketName) external {

        emit InvestsClaimed(
            buyer,
            ticketId,
            totalInvestments,
            ticketName,
            block.timestamp
        );
    }

    function EmitHFClaimCreated(address user, uint256 ticketId, HFEntity.Claim memory claim, uint256 oracleData, uint8 juryAccept, uint8 juryReject) external {
        emit HFClaimCreated(
            user,
            ticketId,
            claim.claimId,
            claim.round,
            claim.claimStatus,
            claim.isApproved,
            oracleData,
            juryAccept,
            juryReject,
            claim.lastDecisionDate,
            claim.roundStartDate,
            claim.condition,
            block.timestamp);
    }

    function EmitHFClaimUpdated(address user, uint256 ticketId, HFEntity.Claim memory claim, uint256 oracleData, uint8 juryAccept, uint8 juryReject) external {
        emit HFClaimUpdated(
            user,
            ticketId,
            claim.claimId,
            claim.round,
            claim.claimStatus,
            claim.isApproved,
            oracleData,
            juryAccept,
            juryReject,
            claim.lastDecisionDate,
            claim.roundStartDate,
            block.timestamp);
    }

    function EmitNFTMinted(address user, uint256 tokenId, string memory nftURI) external {
        emit NFTMinted(user, tokenId, nftURI, block.timestamp);
    }

    function EmitNFTBurned(uint256 tokenId) external {
        emit NFTBurned(tokenId, block.timestamp);
    }

    function EmitNFTTransferred(address from, address to, uint256 tokenId) external {
        emit NFTTransferred(from, to, tokenId, block.timestamp);
    }

    function EmitNFTCreated(HFEntity.NFT memory nft) external {
        emit NFTCreated(nft.tokenId, nft.tokenContract, nft.tokenType, block.timestamp);
    }

    function EmitNFTAdded(uint256 ticketId, uint256 tokenId) external {
        emit NFTAdded(ticketId, tokenId, block.timestamp);
    }
}

