pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "../libraries/AppStorage.sol";
import "../libraries/LibDiamond.sol";
import "../CalcDate.sol";

contract TokenAvgPrice1Facet {
    //Facet common storage
    AppStorage s;

    // Check if the date format is avaliable
    modifier isAvailableDate(uint8 month, uint8 day) {
        uint16 year = s.dateCalc.getYear(block.timestamp);
        require(month > 0 && month <= 12, "unavailable month");
        require(day > 0 && day <= s.dateCalc.getDaysInMonth(month, year), "unavailable day");
        _;
    }

    // Get previous total price
    function getPrevTotalPrice(uint8 month, uint8 day) private view returns (uint256) {
        if(month == 1 && day == 1) {
            return 0;
        }

        if(day == 1) {
            uint16 year = s.dateCalc.getYear(block.timestamp);
            uint8 prevMonDays = s.dateCalc.getDaysInMonth(month - 1, year);
            return s.priceInfos[month - 2][prevMonDays - 1].totalPrice;
        }

        return s.priceInfos[month - 1][day - 2].totalPrice;
    }

    // Get everyday price
    function getEverydayPrice(uint8 month, uint8 day) external view isAvailableDate(month, day) returns (uint256) {
        console.log("getEverydayPrice(month: %d, day: %d) = %d", month, day, s.priceInfos[month - 1][day - 1].price);
        return s.priceInfos[month - 1][day - 1].price;
    }

    // Set everyday price
    function setEverydayPrice(uint8 month, uint8 day, uint256 price) external isAvailableDate(month, day) {
        s.priceInfos[month - 1][day - 1].price = price;
        s.priceInfos[month - 1][day - 1].totalPrice = getPrevTotalPrice(month, day) + price;
    }

    // Get token average price
    function getAvgPriceFromTo(uint8 fromMonth, uint8 fromDay, uint8 toMonth, uint8 toDay) external view isAvailableDate(fromMonth, fromDay) isAvailableDate(toMonth, toDay) returns(uint256) {
        require( (toMonth == fromMonth && toDay >= fromDay) || (toMonth > fromMonth), "FromDate must be before toDate." );

        uint256 daysFromTo = getDaysFromTo(fromMonth, fromDay, toMonth, toDay);
        uint256 avgPrice = s.priceInfos[toMonth - 1][toDay - 1].totalPrice - s.priceInfos[fromMonth - 1][fromDay - 1].totalPrice + s.priceInfos[fromMonth - 1][fromDay - 1].price;
        avgPrice = avgPrice / daysFromTo;
        console.log("getAvgPriceFromTo(%d days) = %d", daysFromTo, avgPrice);
        
        return avgPrice;
    }

    // Get days between from and to
    function getDaysFromTo(uint8 fromMonth, uint8 fromDay, uint8 toMonth, uint8 toDay) private view returns(uint) {
        uint16 daysFromTo;
        uint8[12] memory _daysInMonthList = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];

        if(fromMonth == toMonth) {
            daysFromTo = toDay - fromDay + 1;
        } else {
            uint16 year = s.dateCalc.getYear(block.timestamp);
            _daysInMonthList[1] = s.dateCalc.getDaysInMonth(2, year);
            for(uint8 i = fromMonth + 1; i < toMonth; i ++)
                daysFromTo += _daysInMonthList[i - 1];
            daysFromTo += s.dateCalc.getDaysInMonth(fromMonth, year) - fromDay + 1 + toDay;
        }

        return daysFromTo;
    }
}

