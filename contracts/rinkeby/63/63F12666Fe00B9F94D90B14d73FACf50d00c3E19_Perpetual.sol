        uint256 ownerKey,
        uint256 amount
	) internal pure returns (bytes32 h) {
		h = keccak256(abi.encode(
			"FORCED_WITHDRAWAL",
			ownerKey,
			amount
		));
	}

    function forcedWithdrawalRequest(
        uint256 ownerKey,
        uint256 amount
        //bool premiumCost
    //) external notFrozen onlyKeyOwner(starkKey) {
    ) external {
		bytes32 req = getForceWithdrawHash(
