import "./Storage.sol";

// /// @author Stars Labs
abstract contract Deposits is Storage {
    event LogDeposit(
        address ethAddr,
        uint256 l2Key,
        uint256 amount
    );

    event LogDepositCancel(uint256 l2Key);

    event LogDepositCancelReclaimed(
        uint256 l2Key,
        uint256 amount
    );

    function depositERC20(
        IERC20 token,
        uint256 l2Key,
        uint256 amount
    ) internal {
        // Update the balance.
        //uint256 amount = ext_amount * (10 ** token.decimals());
        console.log("deposit amount ", amount);
        pendingDeposits[l2Key] += amount;
        require(pendingDeposits[l2Key] >= amount, "DEPOSIT_OVERFLOW");  // TODO : remove as 0.8 overflow evm assert works

        // Disable the cancellationRequest timeout when users deposit into their own account.
        if (cancellationRequests[l2Key] != 0 //TODO: && isMsgSenderKeyOwner(l2Key)
        ) {
            delete cancellationRequests[l2Key];
        }

        // Transfer the tokens to the Deposit contract.
        console.log("balance before : ", token.balanceOf(address(this)));
        require(Utils.transferFromERC20(token, msg.sender, address(this),
                SafeCast.toUint128(amount)), "depoist fail");
        console.log("balance after : ", token.balanceOf(address(this)));
        // TODO : require(deposit_amount <= MAX_DEPOSIT_AMOUNT, "C2");

        // // Log event.
        emit LogDeposit(
            msg.sender,
            l2Key,
            amount
        );
    }

    function deposit(
        uint256 l2Key,
        uint256 amount
    ) public {
        depositERC20(collateralToken, l2Key, amount);
    }

    function depositCancel(
        uint256 l2Key
    ) external onlyKeyOwner(l2Key) {
        // Start the timeout.
        cancellationRequests[l2Key] = block.timestamp;

        // Log event.
        emit LogDepositCancel(l2Key);
    }

    function depositReclaim(
        uint256 l2Key
    ) external onlyKeyOwner(l2Key) {
        require(ethKeys[l2Key] != address(0), "depositReclaim : Invalid l2Key");

        // Time Lock
        uint256 requestTime = cancellationRequests[l2Key];
        require(requestTime != 0, "DEPOSIT_NOT_CANCELED");
        uint256 freeTime = requestTime + DEPOSIT_CANCEL_TIMELOCK;
        assert(freeTime >= DEPOSIT_CANCEL_TIMELOCK);
        require(block.timestamp >= freeTime, "DEPOSIT_LOCKED"); // NOLINT: timestamp.

        // Clear deposit.
        uint256 amount = pendingDeposits[l2Key];
        delete pendingDeposits[l2Key];
        delete cancellationRequests[l2Key];

        require(Utils.sendERC20(collateralToken, ethKeys[l2Key], amount), "depositReclaim fail"); 

        emit LogDepositCancelReclaimed(
            l2Key,
            amount
        );
    }
}

