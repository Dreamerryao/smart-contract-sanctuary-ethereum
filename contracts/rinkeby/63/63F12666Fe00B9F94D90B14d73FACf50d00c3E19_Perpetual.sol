    event DEBUG_ORDER_STATE_HASH(
        bytes32 oraclePriceHash,
        bytes32 globalFundingIndexHash,
        bytes32 global_state_hash,
        bytes32 orderStateHash
    );

    function encodePackU64Array(uint64[] memory a, uint padLen)
        internal pure returns(bytes memory data
    ){
        for(uint i=0; i<padLen; i++){
            if (i < a.length) {
                data = abi.encodePacked(data, a[i]);
            } else {
                data = abi.encodePacked(data, uint64(0));
            }
        }
    }

    function getOrderStateHash(
        CommitBlockInfo calldata b,
        // bool firstBlock,             // TODO : check first/last block timestamp expected
        uint64[] memory oracle_price
    ) internal returns (bytes32 newOrderStateHash) {
        if (oracle_price.length == 0 && b.globalFundingIndex.length == 0) {
            return b.orderStateHash;
        }

        bytes32 oraclePriceHash = b.oraclePriceHash;
        if (oracle_price.length != 0) {
            if (oracle_price.length != MAX_ASSETS_COUNT) {
                console.log("oracle price fullencode :");
                bytes memory encode_data = encodePackU64Array(oracle_price, MAX_ASSETS_COUNT);
                console.logBytes(encode_data);
                oraclePriceHash = sha256(encode_data);
            } else {
                oraclePriceHash = sha256(abi.encodePacked(oracle_price));
            }
        }

        bytes32 globalFundingIndexHash = b.globalFundingIndexHash;
        if (b.globalFundingIndex.length != 0) {
            if (b.globalFundingIndex.length != MAX_ASSETS_COUNT) {
                globalFundingIndexHash = sha256(encodePackU64Array(b.globalFundingIndex, MAX_ASSETS_COUNT));
            } else {
                globalFundingIndexHash = sha256(abi.encodePacked(b.globalFundingIndex));
            }
        }

        bytes32 global_state_hash = sha256(abi.encodePacked(uint32(b.timestamp), globalFundingIndexHash, oraclePriceHash));
        // if (b.blockNumber == 2) {   // TODO
        //     global_state_hash = sha256(abi.encodePacked(uint32(0x6278a534), globalFundingIndexHash, oraclePriceHash));
        // }
        newOrderStateHash = sha256(abi.encodePacked(b.orderRoot, global_state_hash));
        console.log("sol get orderStateHash");
        console.logBytes32(oraclePriceHash);
        console.logBytes32(globalFundingIndexHash);
        console.logBytes32(global_state_hash);

        emit DEBUG_ORDER_STATE_HASH(
            oraclePriceHash,
            globalFundingIndexHash,
            global_state_hash,
            newOrderStateHash);
    }

    function verifyProofCommitment(
        CommitBlockInfo[] calldata _newBlocksData,
        ProofInput calldata _proof,
        uint64[] calldata lastestOraclePrice
    ) internal returns (bytes32 curOrderStateHash) {
        bytes32 curAccountRoot = accountRoot;
        curOrderStateHash = orderStateHash;
        for (uint256 i = 0; i < _newBlocksData.length; ++i) {

            // Create block commitment, and check with proof commitment
            // require(_proof.globalConfigHash == globalConfigHash, "proof globalConfigHash invalid");
            uint64[] memory oraclePrice;
            if (i == _newBlocksData.length - 1) {
                oraclePrice = lastestOraclePrice;
            }
            bytes32 newOrderStateHash = getOrderStateHash(_newBlocksData[i], oraclePrice);
            bytes32 commitment = createBlockCommitment(curAccountRoot, curOrderStateHash, newOrderStateHash, _newBlocksData[i]);
            emit DEBUG_PROOF(newOrderStateHash, _proof.commitments[i] & INPUT_MASK, uint256(commitment) & INPUT_MASK);
            require(_proof.commitments[i] & INPUT_MASK == uint256(commitment) & INPUT_MASK, "proof commitment invalid");

            curAccountRoot = _newBlocksData[i].accountRoot;
            curOrderStateHash = newOrderStateHash;
        }
    }

    event DEBUG_DAC_SIGNATURE(
        bytes32 concatValdiumHash,
        bytes32 msgHash,
        bytes[] validium_signature
    );

    function verifyValidiumSignature(
        CommitBlockInfo[] calldata _newBlocksData,
        bytes[] calldata validium_signature
    ) internal {
        require (validium_signature.length >= MIN_DAC_SIGNATURE);

        bytes32 concatValdiumHash = keccak256("");
        for (uint256 i = 0; i < _newBlocksData.length; ++i) {
            concatValdiumHash = keccak256(abi.encodePacked(concatValdiumHash, _newBlocksData[i].all_data_commitment));
        }

        bytes memory message = abi.encodePacked(
                "\x19Ethereum Signed Message:\n66",  // TODO
                "0x",
                Bytes.bytesToHexASCIIBytes(abi.encodePacked(concatValdiumHash))
        );
        bytes32 msgHash = keccak256(message);


        emit DEBUG_DAC_SIGNATURE(
            concatValdiumHash,
            msgHash,
            validium_signature
        );

        uint32 sig_dac_num = 0;
        bytes32[MIN_SIGNATURE_MEMBER] memory sigHashs;
        for (uint256 i = 0; i < validium_signature.length; ++i) {
            bytes32 sigHash = keccak256(validium_signature[i]);

            uint256 j;
            for (j = 0; j < sig_dac_num; ++j) {
                if (sigHashs[j] == sigHash) {
                    console.log("ignore same signature : ");
                    console.logBytes(validium_signature[i]);
                    break;
                }
            }

            if (j != sig_dac_num) { // ignore same signature 
                continue;
            }

            address signer = Utils.recoverAddressFromEthSignature(validium_signature[i], msgHash);
            console.log("signer : ", signer);
            require(dacs[signer] != false, "ValidiumHash Sinature Invalid");

            sigHashs[sig_dac_num++] = sigHash;
            if (sig_dac_num == MIN_SIGNATURE_MEMBER) { // ignore additional signature.
                break;
            }
        }
        require(sig_dac_num >= MIN_SIGNATURE_MEMBER, "sig dac memeber lesser");
    }

    function updateBlocks(
        CommitBlockInfo[] calldata _newBlocksData,
        bytes[] calldata validium_signature,
        ProofInput calldata _proof,
        uint64[] calldata lastestOraclePrice  // concat(BTC-USD,ETH-USD,...)
        ) external onlyValidator {
        //) external nonReentrant {

    //     requireActive();

        require (_newBlocksData.length >= 1);
        verifyValidiumSignature(_newBlocksData, validium_signature);
        bytes32 newOrderStateHash = verifyProofCommitment(_newBlocksData, _proof, lastestOraclePrice);

    // TODO: verify lastestOraclePrice signature

        // block prove
        console.log("sol _proof.recursiveInput ", _proof.recursiveInput[0]);
        require(verifier.verifyAggregatedBlockProof(
                            _proof.recursiveInput,
                            _proof.recursiveInput,
                            _proof.proof,
                            _proof.vkIndexes,
                            _proof.commitments,
                            _proof.subproofsLimbs), "p");
        //require(success, "p"); // Aggregated proof verification fail

        //postprocess onchain operation
        for (uint256 i = 0; i < _newBlocksData.length; ++i) {
            postProcess(_newBlocksData[i].onchainPubData);
        }

        // update block status
        accountRoot = _newBlocksData[_newBlocksData.length - 1].accountRoot;
        orderStateHash = newOrderStateHash;
        emit BlockUpdate(_newBlocksData[0].blockNumber,
                         _newBlocksData[_newBlocksData.length - 1].blockNumber);
    }

}

