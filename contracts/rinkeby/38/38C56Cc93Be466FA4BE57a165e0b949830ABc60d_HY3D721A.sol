        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}

// File: contracts/HY3D721A.sol



pragma solidity ^0.8.0;






interface IAttitudes {
    function setAttitudeType(uint256 tokenId) external;
}

contract HY3D721A is ERC721A, ReentrancyGuard, AccessControl, Pausable {

    // DutchAuction --------

    struct sales_config {
        uint256 MAX_AUCTION_MINT;
        uint256 AUCTION_PRICE_CURVE_LENGTH;
        uint256 AUCTION_DROP_INTERVAL;
        uint256 AUCTION_START_PRICE; //起始价格
        uint256 AUCTION_START_TIME; //开始时间
        uint256 AUCTION_END_TIME; //结束时间
        uint256 AUCTION_END_PRICE; //最低降到0.004
        uint256 AUCTION_DISCOUNTRATE_RATE; //每20分钟降价0.005
    }

    sales_config public salesConfig;

    //铸币期间每个地址只能铸造5个
    mapping (address => uint256) public maxPerAddressDuringMint;

    bytes32 public root;

    //fressMint 总量
    uint256 public constant MAX_FREEMINT = 100;

    //记录freeMint 已经mint了多少个
    uint256 public FreeAlreadyMint;

    //记录白名单已经mint了多少个
    mapping (address => uint256) public AddressAlreadyMint;

    //整个token限制的总量
    uint256 public constant _totalSupply = 100;

    //起始价格
    uint256 public constant WHITELIST_PRICE = 0.015 ether;

    //前缀
    string private UriPrefix3d = "https://bafkreiavb6swaelth6qtim6fatx35p5z3bogusrnuuofb7z4iubbx6nwxu.ipfs.nftstorage.link/";
    //后缀
    string private UriSuffix3d = ".json";

    //态度token 初始化
    IAttitudes public _Attitudes;

    //白名单已售卖的总数
    uint256 public allowListSales;

    //态度token的权限
    bytes32 public constant ATTITUDE_ROLE = keccak256("ATTITUDE_ROLE");

    receive() external payable {}

  constructor(
  ) ERC721A("HY3D-06", "3D", 200, 10000) {

        salesConfig.MAX_AUCTION_MINT = 5;
        salesConfig.AUCTION_PRICE_CURVE_LENGTH = 5 hours;
        salesConfig.AUCTION_DROP_INTERVAL = 20 minutes;
        salesConfig.AUCTION_START_PRICE = 0.01 ether;
        salesConfig.AUCTION_END_PRICE = 0.004 ether;
        salesConfig.AUCTION_DISCOUNTRATE_RATE = 0.0005 ether;
        
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
  }

    //设置荷兰拍开始时间
    function auctionStartTime(uint256 _startTime) external onlyRole(DEFAULT_ADMIN_ROLE) {
        salesConfig.AUCTION_START_TIME = _startTime;
        salesConfig.AUCTION_END_TIME = _startTime + salesConfig.AUCTION_PRICE_CURVE_LENGTH;
    }

    function setRoot(bytes32 _root) external onlyRole(DEFAULT_ADMIN_ROLE){
        root = _root;
    }

    function setAttitudeAddress(address _address) external onlyRole(DEFAULT_ADMIN_ROLE) {
         _Attitudes = IAttitudes(_address);
    }

    //获取荷兰拍实时价格
    function getPrice() public view returns (uint256) {
        uint timeElapsed = (block.timestamp - salesConfig.AUCTION_START_TIME) / salesConfig.AUCTION_DROP_INTERVAL;

        uint256 discount;

        if(timeElapsed >= 12){
            discount = salesConfig.AUCTION_END_PRICE;
        }else{
            discount = salesConfig.AUCTION_START_PRICE - (salesConfig.AUCTION_DISCOUNTRATE_RATE * timeElapsed);
        }

        return discount;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721A, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    modifier callerIsUser() {
        require(tx.origin == msg.sender, "The caller is another contract");
        _;
    }

    //开启暂停
    function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    //取消暂停
    function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }

    //设置前缀 后缀 UriPrefix3d
    function setPrefixAndSuffi(string calldata _uriPrefix, string calldata _uriSuffix) external onlyRole(DEFAULT_ADMIN_ROLE){
        UriSuffix3d = _uriSuffix;
        UriPrefix3d = _uriPrefix;
    }

    //荷兰拍
    function auctionMint(uint256 quantity) external payable nonReentrant whenNotPaused callerIsUser{

        //判断开始时间
        require(block.timestamp >= salesConfig.AUCTION_START_TIME,"sale has not started ye");

        // //判断结束时间
        require(block.timestamp < salesConfig.AUCTION_END_TIME, "auction expired");

        // //判断用户是否已经Mint了5个
        require((maxPerAddressDuringMint[msg.sender] + quantity) <= salesConfig.MAX_AUCTION_MINT, "the quantity has exceeded the limit");

        // //判断总量是否已mint满
        require((totalSupply() + quantity) <= _totalSupply, "not enough remaining reserved for auction to support desired mint amount");

        // //获取荷兰拍当前价格
        uint256 price = getPrice();

        // //判断传入的以太坊是否满足价格
        require(msg.value >= (price * quantity), "insufficient amount of ether entered");

        // //出来的eth原路打回给用户
        if(msg.value > (price * quantity)){
            payable(msg.sender).transfer(msg.value - (price * quantity));
        }
        
        // //把本次用户mint的数量加上
        maxPerAddressDuringMint[msg.sender] += quantity;

        _safeMint(msg.sender, quantity);

    }

    //白名单mint
    function allowlistMint(bytes32[] calldata _proofs, uint256 quantity,uint256 mintQuantity) external payable nonReentrant whenNotPaused callerIsUser{

        //数量必须小于或者等于白名单设置的数量
        require(AddressAlreadyMint[msg.sender] + mintQuantity  <= quantity,"The quantity has exceeded the limit");

        // 传入的数量必须大于0
        require(mintQuantity > 0, "Quantity must be greater than 0");

        require((totalSupply() + mintQuantity) <= _totalSupply, "reached max supply");
        //金额必须等于数量*单价

        require(msg.value >= (WHITELIST_PRICE * mintQuantity), "Amount does not match quantity");

        bytes32 _leaf = keccak256(abi.encodePacked(msg.sender,uint256(quantity)));

        (bool res) = MerkleProof.verify(_proofs, root, _leaf);

        require(res,"verify false");

        //多出来的eth原路打回给用户
        if(msg.value > (WHITELIST_PRICE * mintQuantity)){
            payable(msg.sender).transfer(msg.value - (WHITELIST_PRICE * mintQuantity));
        }

        allowListSales += mintQuantity;

        // 计算白名单已经mint的总数
        AddressAlreadyMint[msg.sender] += mintQuantity;

        _safeMint(msg.sender,mintQuantity);
    }
    //管理员自由Mint
    function freeMint(address to, uint256 quantity)
        external
        whenNotPaused
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(FreeAlreadyMint < MAX_FREEMINT, "reached max supply");

        FreeAlreadyMint += quantity;
        
        _safeMint(to,quantity);

    }

    //返回URI
    function tokenURI(uint256 tokenId) public view override(ERC721A) returns(string memory){
        
        if(tokenId > 0){
            return UriPrefix3d;
        }else{
            return UriPrefix3d;
        }
            
    }

    function _afterTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal override {

        _Attitudes.setAttitudeType(startTokenId);

         super._afterTokenTransfers(from,to,startTokenId,quantity);
    }

    function getList(address _addr, uint256 pageNo, uint256 pageSize) external view returns (uint256[]memory ids){
        uint256 nftBalance = balanceOf(_addr);
        if (nftBalance == 0 || pageSize == 0) {
            return new uint256[](0);
        }
        uint start = 0;
        uint end = 0;
        if (nftBalance <= pageSize) {
            end = nftBalance;
        } else {
            start = pageNo * pageSize;
            end = start + pageSize;
            if (end >= nftBalance) {
                end = nftBalance;
            }
        }
        ids = new uint256[](end - start);
        uint index;
        for (; start < end; start++) {
            ids[index] = tokenOfOwnerByIndex(_addr, start);
            index++;
        }

    }
  
}