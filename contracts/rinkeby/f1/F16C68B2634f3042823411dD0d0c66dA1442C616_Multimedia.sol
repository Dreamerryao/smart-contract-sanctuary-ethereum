pragma solidity ^0.8.11;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./MultimediaSchema.sol";

import "./HelperFunctions.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

library MultimediaMeta {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.universe.multimedia.storage");

  modifier onlyDAO() {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msg.sender == ds.daoAddress, "Wrong address");
    _;
  }

  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly {
      ds.slot := position
    }
  }

  function setMetaRegistry(MultimediaSchema.TokenRef memory ref, string memory title, string memory description, string[] memory tags) external returns (MultimediaSchema.MetaContext memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(tags.length <= 7 && tags.length >= 1, 'Tags length');
    require(keccak256(bytes(title)) != keccak256(bytes('')), 'Title empty');
    require(keccak256(bytes(description)) != keccak256(bytes('')), 'Description empty');

    MultimediaSchema.MetaContext storage context = ds.metaRegistry[ref.tokenAddress].tokenId[ref.tokenId];

    ERC721Upgradeable nftContract = ERC721Upgradeable(ref.tokenAddress);
    // if (nftContract.getOwner() == msg.sender) {
    //   context.authorTitle = title;
    //   context.authorDescription = description;
    //   context.authorTags = tags;
    //   if (!context.published) context.published = true;
    // }
    // else 
    if (nftContract.ownerOf(ref.tokenId) == msg.sender) {
      context.title = title;
      context.description = description;
      context.tags = tags;
      if (!context.published) context.published = true;
    }

    return context;
  }


  function createTokenRef(string memory name, string memory urlParam) public onlyDAO {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    ds.tokenRefCounter = ds.tokenRefCounter.add(1);
    ds.tokenRefMapping[ds.tokenRefCounter] = MultimediaSchema.TokenRefMapping({
      name: name,
      urlParam: urlParam
    });
  }


  function setTokenRefs(uint256 tokenId, MultimediaSchema.TokenRef[][] memory refs, string[] memory contentTypes) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(refs.length == contentTypes.length, 'Invalid data');

    uint256 nftId = tokenId;
    uint256 dropId = 0;
    if (tokenId > 1000000000) {
      nftId = HelperFunctions.safeParseInt(HelperFunctions.substring(HelperFunctions.uint2str(tokenId), bytes(HelperFunctions.uint2str(tokenId)).length.sub(9), bytes(HelperFunctions.uint2str(tokenId)).length));
      dropId = tokenId.sub(nftId).div(1000000000);
    }

    for (uint256 i = 0; i < refs.length; i++) {
      if (refs[i].length > 0) delete ds.tokenRefTypes[contentTypes[i]].refs[tokenId];
      for (uint256 j = 0; j < refs[i].length; j++) {
        MultimediaSchema.TokenRef memory ref = refs[i][j];
        bool published = ds.metaRegistry[ref.tokenAddress].tokenId[ref.tokenId].published;
        address owner = ERC721Upgradeable(ref.tokenAddress).ownerOf(ref.tokenId);
        if (ds.drops[dropId].requireOwnership ? (owner == msg.sender && published) : published) {
          for (uint256 k = 1; k <= ds.tokenRefCounter; k++) {
            if (keccak256(bytes(contentTypes[i])) == keccak256(bytes(ds.tokenRefMapping[k].name))) {
              ds.tokenRefTypes[contentTypes[i]].refs[tokenId].push(ref);
            }
          }
        }
      }
    }
  }

  function getTokenRefs(uint256 tokenId) external view returns (MultimediaSchema.TokenRef[][] memory tokenRefs) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.TokenRef[][] memory refs = new MultimediaSchema.TokenRef[][](ds.tokenRefCounter);
    for (uint256 i = 1; i <= ds.tokenRefCounter; i++) {
      refs[i.sub(1)] = ds.tokenRefTypes[ds.tokenRefMapping[i].name].refs[tokenId];
    }

    return refs;
  }


  function renameToken(uint256 tokenId, string memory name) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    uint256 nftId = tokenId;
    uint256 dropId = 0;
    if (tokenId > 1000000000) {
      nftId = HelperFunctions.safeParseInt(HelperFunctions.substring(HelperFunctions.uint2str(tokenId), bytes(HelperFunctions.uint2str(tokenId)).length.sub(9), bytes(HelperFunctions.uint2str(tokenId)).length));
      dropId = tokenId.sub(nftId).div(1000000000);
    }
    if (ds.drops[dropId].renamable) ds.nftNames[tokenId] = name;
  }


  function tokenURI(uint256 tokenId, address owner) external view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    // Parse token and drop IDs from tokenId
    uint256 nftId = tokenId;
    uint256 dropId = 0;
    if (tokenId > 1000000000) {
      nftId = HelperFunctions.safeParseInt(HelperFunctions.substring(HelperFunctions.uint2str(tokenId), bytes(HelperFunctions.uint2str(tokenId)).length.sub(9), bytes(HelperFunctions.uint2str(tokenId)).length));
      dropId = tokenId.sub(nftId).div(1000000000);
    }

    string memory refUri = '';
    for (uint256 i = 1; i <= ds.tokenRefCounter; i++) {
      refUri = string(abi.encodePacked(refUri, i == 1 ? '?' : '&', ds.tokenRefMapping[i].urlParam, '=['));
      MultimediaSchema.TokenRef[] memory ref = ds.tokenRefTypes[ds.tokenRefMapping[i].name].refs[tokenId];
      bool removeComma = false;
      for (uint256 j = 0; j < ref.length; j++) {
        address refTokenOwner = ERC721Upgradeable(ref[j].tokenAddress).ownerOf(ref[j].tokenId);
        // No ownership required to point to ref
        if (!ds.drops[dropId].requireOwnership) {
          refUri = string(abi.encodePacked(
            refUri,
            "{'uri':'", ERC721Upgradeable(ref[j].tokenAddress).tokenURI(ref[j].tokenId), "',"
            "'owner':'0x", HelperFunctions.toAsciiString(refTokenOwner), "'},"));
            removeComma = true;
        } else if (ref[j].tokenAddress != address(0) && refTokenOwner == owner) {
          refUri = string(abi.encodePacked(refUri, "'", ERC721Upgradeable(ref[j].tokenAddress).tokenURI(ref[j].tokenId), "',"));
          removeComma = true;
        }
      }
      refUri = string(abi.encodePacked(removeComma ? HelperFunctions.substring(refUri, 0, bytes(refUri).length.sub(1)) : refUri, ']'));
    }

    // Rarity traits
    string memory encodedTraits = '';
    string memory specialParams = '&m=';
    for (uint256 i = 0; i < ds.randomizedTraits[tokenId].length; i++) {
      encodedTraits = string(abi.encodePacked(
        encodedTraits,
        '{"trait_type":"',
        'Type',
        '", "value":"',
        ds.drops[dropId].traits[ds.randomizedTraits[tokenId][i]],
        '"}',
        i == ds.randomizedTraits[tokenId].length.sub(1) ? '' : ',')
      );

      specialParams = string(abi.encodePacked(
          specialParams,
          HelperFunctions.uint2str(ds.randomizedTraits[tokenId][i])
      ));
    }

    // NFT Drop Season Trait
    if (keccak256(bytes(ds.drops[dropId].season)) != keccak256(bytes(''))) {
      encodedTraits = string(abi.encodePacked(
        encodedTraits,
        ds.randomizedTraits[tokenId].length > 0 ? ',' : '',
        '{"trait_type":"',
        'Season',
        '", "value":"',
        ds.drops[dropId].season,
        '"}')
      );
    }

    string memory tokenName = string(abi.encodePacked(
      ds.drops[dropId].name,
      ds.drops[dropId].editioned ? ' #' : '',
      ds.drops[dropId].editioned ? HelperFunctions.uint2str(nftId) : ''));

    if (ds.drops[dropId].renamable && keccak256(bytes(ds.nftNames[tokenId])) != keccak256(bytes(''))) {
      tokenName = ds.nftNames[tokenId];
    }

    return string(abi.encodePacked(
      "data:application/json;base64,",
      Base64.encode(bytes(abi.encodePacked(
        '{"name":"', tokenName,
        '", "description":"', ds.drops[dropId].description,
        '","image":"',
          ds.drops[dropId].uri,
          refUri,
          specialParams,
        '","animation_uri":"',
          ds.drops[dropId].uri,
          refUri,
          specialParams,
        '","attributes": [', encodedTraits,
        '] }'
      ))))
    );
  }
}

