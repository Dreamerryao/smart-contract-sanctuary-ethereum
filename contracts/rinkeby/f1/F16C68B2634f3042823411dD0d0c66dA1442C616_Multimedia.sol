pragma solidity ^0.8.11;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./MultimediaSchema.sol";

import "./HelperFunctions.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

library MultimediaDrop {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.universe.multimedia.storage");
  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly {
      ds.slot := position
    }
  }


  modifier onlyDAO() {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msg.sender == ds.daoAddress, "Wrong address");
    _;
  }


  function createDrop(MultimediaSchema.Drop memory drop) external onlyDAO returns (uint256 dropCounter, MultimediaSchema.DropData memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(drop.traits.length == drop.traitChances.length, 'Trait mismatch');
    ds.drops[ds.dropCounter] = drop;
    ds.dropData[ds.dropCounter] = MultimediaSchema.DropData({
      tokenIdCounter: drop.reserveQuantity,
      reserveIdCounter: 0,
      randomizedTraitsCounter: 0,
      useVRF: drop.traits.length > 0,
      initialized: true
    });
    ds.dropCounter = ds.dropCounter.add(1);
    return (ds.dropCounter.sub(1), ds.dropData[ds.dropCounter.sub(1)]);
  }


  // Check if minting with incorrect value drains user's funds
  function mint(uint256 dropId, uint256 quantity, uint256 ethValue, uint256 tokenValue) external returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(ds.dropData[dropId].initialized, 'Invalid drop');

    ds.dropData[dropId].tokenIdCounter = ds.dropData[dropId].tokenIdCounter.add(1);
    if (ds.drops[dropId].totalQuantity > 0)
      require(ds.dropData[dropId].tokenIdCounter.add(quantity).sub(1) <= ds.drops[dropId].totalQuantity, 'Sold out');
    if (ds.drops[dropId].saleToken == address(0))
      require(tokenValue == ds.drops[dropId].salePrice.mul(quantity), "Incorrect value");
    else {
      IERC20 token = IERC20(ds.drops[dropId].saleToken);
      require(ethValue <= token.balanceOf(msg.sender), "Not enough balance");
      require(ethValue == ds.drops[dropId].salePrice.mul(quantity), "Incorrect value");
      token.transfer(ds.daoAddress, ethValue);
    }

    uint256 tokenId = uint256(dropId.mul(1000000000)) + uint256(ds.dropData[dropId].tokenIdCounter);
    ds.dropData[dropId].tokenIdCounter = ds.dropData[dropId].tokenIdCounter.add(quantity).sub(1);
    return tokenId;
  }


  // Check if minting with incorrect value drains user's funds
  function mintReserve(uint256 dropId, uint256 quantity) external onlyDAO returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(ds.dropData[dropId].initialized, 'Invalid drop');

    ds.dropData[dropId].reserveIdCounter = ds.dropData[dropId].reserveIdCounter.add(1);
    require(ds.drops[dropId].reserveQuantity > 0, 'None reserved');
    require(ds.dropData[dropId].reserveIdCounter.add(quantity).sub(1) <= ds.drops[dropId].reserveQuantity, 'All minted');
    uint256 tokenId = uint256(dropId.mul(1000000000)) + uint256(ds.dropData[dropId].reserveIdCounter);
    ds.dropData[dropId].reserveIdCounter = ds.dropData[dropId].reserveIdCounter.add(quantity).sub(1);
    return tokenId;
  }


  function randomMetadata(uint vrfNumber, uint tokenId) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    for (uint i = 0; i < ds.drops[0].traitChances.length; i++) {
      if (uint(keccak256(abi.encodePacked(
        block.difficulty, block.timestamp, vrfNumber, tokenId, i
        ))) % 10000 < ds.drops[0].traitChances[i]
      ) {
        ds.randomizedTraits[tokenId].push(i);
      }
    }
  }

  // Used for testing purposes mocking random function
  function fulfillVRFMock() external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    uint remainder = ds.dropData[0].tokenIdCounter.sub(ds.dropData[0].randomizedTraitsCounter);
    if (remainder > 100) remainder = 100;

    uint randomWord = uint(keccak256(
      abi.encodePacked(block.difficulty, block.timestamp, uint(234), uint(1982)
    )));

    for (uint i = 0; i < remainder; i++) {
      randomMetadata(randomWord, ds.dropData[0].tokenIdCounter + i);
    }

    ds.dropData[0].randomizedTraitsCounter.add(remainder);
  }
}

