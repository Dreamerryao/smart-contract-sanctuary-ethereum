pragma solidity ^0.8.11;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import "./MultimediaSchema.sol";
import "./MultimediaMeta.sol";
import "./MultimediaDrop.sol";

contract Multimedia is ERC721Upgradeable {
  function initialize(string memory name, string memory symbol) public initializer {
    MultimediaSchema.Storage storage ds = MultimediaMeta.multimediaStorage();
    ds.daoAddress = payable(msg.sender);
    __ERC721_init(name, symbol);
  }

  event DropEvent(uint256 dropCounter);
  event TokenRefsEvent(uint256 tokenId);
  event RenameEvent(uint256 tokenId, string name);
  event RegistryEvent(address tokenAddress, uint256 tokenId);

  function renameToken(uint256 tokenId, string memory name) public {
    require (ownerOf(tokenId) == msg.sender, 'Owner permission');
    MultimediaMeta.renameToken(tokenId, name);
    emit RenameEvent(tokenId, name);
  }

  function setMetaRegistry(MultimediaSchema.TokenRef memory ref, string memory title, string memory description, string[] memory tags) public {
    MultimediaMeta.setMetaRegistry(ref, title, description, tags);
    emit RegistryEvent(ref.tokenAddress, ref.tokenId);
  }

  function createTokenRef(string memory name, string memory urlParam) public {
    MultimediaMeta.createTokenRef(name, urlParam);
    // emit CreateTokenRefEvent(name);
  }

  function setTokenRefs(uint256 tokenId, MultimediaSchema.TokenRef[][] memory refs, string[] memory contentTypes) public {
    require (ownerOf(tokenId) == msg.sender, 'Owner permission');
    MultimediaMeta.setTokenRefs(tokenId, refs, contentTypes);
    emit TokenRefsEvent(tokenId);
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    return MultimediaMeta.tokenURI(tokenId, ownerOf(tokenId));
  }

  function createDrop(MultimediaSchema.Drop memory drop) public {
    (uint256 dropCounter,) = MultimediaDrop.createDrop(drop);
    emit DropEvent(dropCounter);
  }

  function mint(uint256 dropId, uint256 quantity, uint256 tokenValue) public payable {
    uint256 tokenId = MultimediaDrop.mint(dropId, quantity, tokenValue, msg.value);
    for (uint256 i = tokenId; i < tokenId + quantity; i++) {
      _mint(msg.sender, i);
    }
  }

  function mintReserve(uint256 dropId, address[] memory to) public {
    uint256 tokenId = MultimediaDrop.mintReserve(dropId, to.length);
    for (uint256 i = 0; i < to.length; i++) {
      _mint(to[i], tokenId + i);
    }
  }

  function fulfillVRFMock() public {
    MultimediaDrop.fulfillVRFMock();
  }

  function getMetaRegistry(address tokenAddress, uint256 tokenId) public view returns (MultimediaSchema.MetaContext memory context) {
    MultimediaSchema.Storage storage ds = MultimediaMeta.multimediaStorage();
    return ds.metaRegistry[tokenAddress].tokenId[tokenId];
  }

  function getDrop(uint256 dropId) public view returns (MultimediaSchema.Drop memory drop, MultimediaSchema.DropData memory dropData) {
    MultimediaSchema.Storage storage ds = MultimediaMeta.multimediaStorage();
    return (ds.drops[dropId], ds.dropData[dropId]);
  }

  function getTokenRefs(uint256 tokenId) public view returns (MultimediaSchema.TokenRef[][] memory tokenRefs) {
    return MultimediaMeta.getTokenRefs(tokenId);
  }
}

