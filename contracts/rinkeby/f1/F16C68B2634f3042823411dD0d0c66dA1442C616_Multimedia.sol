pragma solidity ^0.8.11;

library MultimediaSchema {
  struct TokenRefMapping {
    string name;
    string urlParam;
  }

  struct TokenRef {
    address tokenAddress;
    uint256 tokenId;
  }

  struct TokenRefTypes {
    mapping (uint256 => TokenRef[]) refs; // tokenID => ref
  }

  // struct TokenRefs {
  //   TokenRef audioRef;
  //   TokenRef renderRef;
  //   TokenRef textRef;
  //   TokenRef artRef;
  // }

  struct Drop {
    string name;
    string description;
    string uri;
    address saleToken;
    uint256 salePrice;
    uint256 totalQuantity;
    uint256 reserveQuantity;
    string season;
    bool editioned;
    bool renamable;
    bool requireOwnership;
    string[] traits;
    uint16[] traitChances;
  }

  struct DropData {
    uint256 tokenIdCounter;
    uint256 reserveIdCounter;
    uint256 randomizedTraitsCounter;
    bool useVRF;
    bool initialized;
  }

  struct MetaContext {
    string authorTitle;
    string authorDescription;
    string[] authorTags;
    string title;
    string description;
    string[] tags;
    bool published;
  }

  struct MetaRegistry {
    mapping (uint256 => MetaContext) tokenId;
  }

  struct Storage {
    address daoAddress;
    uint256 dropCounter;
    uint256 tokenRefCounter;

    mapping (uint256 => Drop) drops; // starting from index 0
    mapping (uint256 => DropData) dropData; // starting from index 0
    mapping (uint256 => uint256[]) randomizedTraits; // tokenId => Drop.traits indexes

    mapping (address => MetaRegistry) metaRegistry; // contract address => tokenId
    // mapping (uint256 => TokenRefs) nftRefs; // tokenID => NFT Refs
    mapping (uint256 => TokenRefMapping) tokenRefMapping; // tokenRefId => token ref name
    mapping (string => TokenRefTypes) tokenRefTypes;
    mapping (uint256 => string) nftNames; // tokenID => custom token name
  }
}

