        require( !_paused,                              "Sale paused" );
        require( num <= maxPerTx,                       "You exceeds mint limit per transaction." );
        require( supply + num <= limit - _reserved,     "Exceeds maximum Puxxies supply" );
        require( msg.value >= _price * num,             "Ether sent is not correct" );

        for(uint256 i; i < num; i++){
            _safeMint( msg.sender, supply + i );
        }
    }

    function giveAway(address[] memory _to, uint256[] memory _amount) external onlyOwner() {
        require( _to.length == _amount.length, "Entered data is invalid");
        for(uint i;  i<_amount.length; i++){
            minto(_to[i], _amount[i]);
        }
    }
    
    function minto (address _to, uint256 _amount) private {
        uint256 supply = totalSupply();
        require( _to != address(0),    "Invalid address found");
        require( _amount <= _reserved, "Exceeds reserved Puxxies supply" );

        for(uint i; i<_amount; i++){
            _safeMint( _to, supply + i );
        }
        _reserved -= _amount;
    }
    
    function WHITELIST_MINT(uint256 num) public payable {
        uint256 supply = totalSupply();
        require( !_paused,                                       "Sale paused" );
        require( WL[msg.sender] == true,                         "Only WHITELIST can mint" );
        require( supply + num <= limit - _reserved,              "Exceeds maximum Puxxies supply" );
        require( msg.value >= _price * num,                      "Ether sent is not correct" );
        
        for(uint256 i; i < num; i++){
            _safeMint( msg.sender, supply + i );
        }
    }
    
    function walletOfOwner(address _owner) public view returns(uint256[] memory) {
        uint256 tokenCount = balanceOf(_owner);
        uint256[] memory tokensId = new uint256[](tokenCount);

        for(uint256 i; i < tokenCount; i++){
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }
    
    // Just in case Eth does some crazy stuff
    function setPrice(uint256 _newPrice) public onlyOwner() {
        _price = _newPrice;
    }
 
    function setWL(address[] memory _address) public onlyOwner() {
        for(uint256 i; i<_address.length; i++){
            require(_address[i] != address(0), "Invalid address found");
            address tempAdd = _address[i];
            WL[tempAdd] = true;
        }
    }

    function setFundWallet(address _fundWallet) public onlyOwner() {
        fundWallet = _fundWallet;
    }

    function setLimit(uint256 __limit) public onlyOwner() {
        limit = __limit;
    }

    function setMaxPerTx(uint256 __maxPerTx) public onlyOwner() {
        maxPerTx = __maxPerTx;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string memory baseURI) public onlyOwner {
        _baseTokenURI = baseURI;
    }

    function getPrice() public view returns (uint256){
        return _price;
    }

    function pause() public onlyOwner {
        _paused = !_paused;
    }

    function setWallets(address[] memory _address, int[] memory _per) public onlyOwner {
        require(_address.length == _per.length && _address.length <= maxWithdrawWallets && _per.length <= maxWithdrawWallets, "Inavalid data entered");
        int tempPer = 0;
    
        for(uint i; i <_address.length; i++){
            require(_address[i] != address(0),     "Zero address found.");
            require(_per[i] >= 0  && _per[i] <= 100, "Invalid per_centage found.");
            tempPer += _per[i];
            require(tempPer <= 100, "Percentages exceeds 100%");
        }

        per1 = _per[0]; wallet1 = _address[0];
        per2 = _per[1]; wallet2 = _address[1];
        per3 = _per[2]; wallet3 = _address[2];
        per4 = _per[3]; wallet4 = _address[3];
        per5 = _per[4]; wallet5 = _address[4];
        per6 = _per[5]; wallet6 = _address[5];
    }


    function withdrawAll() public payable onlyOwner {
        // uint BALANCE = address(this).balance;
        uint Amount1 = uint(per1*(int(address(this).balance))/(100));
        uint Amount2 = uint(per2*(int(address(this).balance))/(100));
        uint Amount3 = uint(per3*(int(address(this).balance))/(100));
        uint Amount4 = uint(per4*(int(address(this).balance))/(100));
        uint Amount5 = uint(per5*(int(address(this).balance))/(100));
        uint Amount6 = uint(per6*(int(address(this).balance))/(100));

        payable(wallet1).transfer(Amount1);
        payable(wallet2).transfer(Amount2);
        payable(wallet3).transfer(Amount3);
        payable(wallet4).transfer(Amount4);
        payable(wallet5).transfer(Amount5);
        payable(wallet6).transfer(Amount6);
    }

    function BALANCE1() public view returns (int256){
        int BALANCE = int(address(this).balance);
        return BALANCE;
    }
    function wallet0001() public view returns (uint){
        return uint(per1*(int(address(this).balance))/(100));
    }
    function wallet0002() public view returns (uint){
        return uint(per2*(int(address(this).balance))/(100));
    }
    function wallet0003() public view returns (uint){
        return uint(per3*(int(address(this).balance))/(100));
    }
    function wallet0004() public view returns (uint){
        return uint(per4*(int(address(this).balance))/(100));
    }
    function wallet0005() public view returns (uint){
        return uint(per5*(int(address(this).balance))/(100));
    }
    function wallet0006() public view returns (uint){
        return uint(per6*(int(address(this).balance))/(100));
    }

