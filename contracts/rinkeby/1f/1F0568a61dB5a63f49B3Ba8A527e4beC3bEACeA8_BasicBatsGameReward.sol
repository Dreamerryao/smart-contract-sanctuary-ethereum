pragma solidity ^0.8.0;

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import './ERC721/ERC721Enum.sol';

abstract contract BasicBats is ERC721Enum, Ownable {
    
    uint256 private basicBatUnitPrice = 0.07 ether;
    
    bool public presaleOpen = false;
    bool public publicSaleOpen = false;

    uint256 public MAX_MINT = 10;
    uint256 public MAX_SUPPLY = 6000;

    // withdraw addresses
    address projectAddr = 0x95Afac888021ae5Bc106Fb5999dC3918260FcF9d;
    address devAddr = 0xda9d7C1c84c7954Ee7F5281cDCddaD359ee072e6;
    
    /*************************************************
     * 
     *      MINT PART
     * 
     * ***********************************************/
    
    
    /*
    * The mint function
    */
    function _mintBasicBats(uint256 _num, uint256 _price) internal {
        uint256 supply = totalSupply();
        require( _num <= MAX_MINT, "Can only mint 10 bats per transaction");
        require( msg.value >= _price * _num, "Not enough ether sent" );
        require( supply + _num <= MAX_SUPPLY, "Max supply reached");

        for(uint256 i; i < _num; i++){
            _safeMint( msg.sender, supply + i );
        }
        
        delete supply;
    }
    
    function mintBasicBats(uint256 num) external payable {
        require(publicSaleOpen);
        
        _mintBasicBats(num, basicBatUnitPrice);
    }
    
    /*************************************************
     * 
     *      UTILITY PART
     * 
     * ***********************************************/
     
    function switchPublicSaleStatus() external onlyOwner {
        publicSaleOpen = !publicSaleOpen;
    }

    function withdraw() external payable onlyOwner {
        uint256 devPercent = address(this).balance / 10;
        uint256 projectPercent = address(this).balance - devPercent;
        require(payable(projectAddr).send(projectPercent));
        require(payable(devAddr).send(devPercent));
    }

}

