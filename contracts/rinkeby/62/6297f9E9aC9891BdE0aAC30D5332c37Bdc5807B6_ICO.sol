pragma solidity ^0.8.0;

import "./SpaceCoin.sol";

/// @title SpaceCoin fundraise contract
/// @author Rares Stanciu
contract ICO {
    // Emitted when ICO goes into a new pahse
    event PhaseChanged(Phases phase);

    // Emitted when the contract is paused or unpaused
    event PauseChanged(bool paused);

    // Emitted when a contribution is made
    event Contribute(address indexed user, uint256 amount);

    // Emitted when a constributor has claimed his pre-open phase tokens
    event Claim(address indexed user, uint256 tokens);

    // Enum that holds all the possible phases
    enum Phases {
        SEED,
        GENERAL,
        OPEN
    }

    // Struct for storing the limits/settigns for a particular phase (in wei)
    struct PhaseLimits {
        uint256 maxRaisedAmount;
        uint256 maxIndividualAmount;
    }

    // Mapping of phase limits
    mapping(Phases => PhaseLimits) public phaseLimits;

    // Owner
    address public owner;

    // Token
    SpaceCoin public token;

    // Current phase of the ICO
    Phases public phase;

    // If fundraising is paused
    bool public paused;

    // Mapping for addresses who are whitelisted in the SEED phase
    mapping(address => bool) public seedWhitelist;

    // Mapping for each address total contribution
    mapping(address => uint256) public userContributions;

    // Total amount of ETH raised so far (in wei)
    uint256 public totalRaised;

    // Mapping for how much each address has to claim
    mapping(address => uint256) public userClaimableTokens;

    /// @notice Checks if the owner is the one calling
    modifier onlyOwner() {
        require(owner == msg.sender, "ONLY_OWNER");
        _;
    }

    /// @notice Checks if the fundraise is active
    modifier onlyActive() {
        require(!paused, "PAUSED");
        _;
    }

    /// @param _seedWhitelist Array of whitelisted addresses in the SEED phase
    constructor(address[] memory _seedWhitelist) {
        owner = msg.sender;

        // Deploy token
        token = new SpaceCoin(msg.sender);

        // Set SEED whitelisted addresses
        for (uint256 i = 0; i < _seedWhitelist.length; i++) {
            seedWhitelist[_seedWhitelist[i]] = true;
        }

        // Set maxmum raised amounts and maximum individual contribution
        // SEED Phase
        phaseLimits[Phases.SEED] = PhaseLimits({
            maxRaisedAmount: 15_000 * 10**18,
            maxIndividualAmount: 1_500 * 10**18
        });

        // GENERAL Phase
        phaseLimits[Phases.GENERAL] = PhaseLimits({
            maxRaisedAmount: 30_000 * 10**18,
            maxIndividualAmount: 1_000 * 10**18
        });

        // OPEN Phase
        phaseLimits[Phases.OPEN] = PhaseLimits({
            maxRaisedAmount: 30_000 * 10**18,
            maxIndividualAmount: 2**256 - 1
        });
    }

    //
    // External functions
    //

    /// @notice Advance the ICO to a new phase
    /// @param _newPhase The new phase to advance the ICO to
    /// @dev Can only go forward, never backward
    function setPhase(Phases _newPhase) external onlyOwner {
        require(_newPhase > phase, "MUST_ONLY_ADVANCE");

        phase = _newPhase;

        emit PhaseChanged(_newPhase);
    }

    /// @notice Allow the owner to pause/unpause the fundraising at any time
    /// @param _paused true if the fundraise should be paused, false to resume
    /// @dev The function must always change the paused state
    function setPaused(bool _paused) external onlyOwner {
        require(paused != _paused, "PAUSED_UNCHANGED");

        paused = _paused;

        emit PauseChanged(_paused);
    }

    /// @notice Contribute to the fundraise with ETH
    /// @dev The amount contributed should not exceed the total amount limit
    /// @dev or the individual contribution
    function contribute() external payable onlyActive {
        require(_isAllowedToContribute(), "NOT_ALLOWED");

        uint256 maxContribution = _getMaximumContributionLeft();

        require(maxContribution > 0, "NO_CONTRIBUTION_LEFT");
        require(msg.value <= maxContribution, "AMOUNT_TOO_HIGH");

        totalRaised += msg.value;
        userContributions[msg.sender] += msg.value;

        _transferOrAddTokens();

        emit Contribute(msg.sender, msg.value);
    }

    /// @notice Claim SPC after contributing
    function claim() external onlyActive {
        require(phase == Phases.OPEN, "NOT_CLAIMABLE_YET");

        uint256 claimableTokens = userClaimableTokens[msg.sender];
        require(claimableTokens > 0, "NOTHING_TO_CLAIM");

        userClaimableTokens[msg.sender] = 0;

        token.transfer(msg.sender, claimableTokens);

        emit Claim(msg.sender, claimableTokens);
    }

    //
    // Private functions
    //

    /// @notice Get the remaining amount (in wei) that sender can contribute
    function _getMaximumContributionLeft() private view returns (uint256) {
        PhaseLimits storage currentPhaseLimits = phaseLimits[phase];
        uint256 userContribution = userContributions[msg.sender];

        // Check if user has already reached the maximum individual contribution for the curernt phase
        if (userContribution >= currentPhaseLimits.maxIndividualAmount) {
            return 0;
        }

        // Check if the fundraise has reached the maximum amount
        if (totalRaised >= currentPhaseLimits.maxRaisedAmount) {
            return 0;
        }

        // Compute the maximum amount allowed to be contributed, so that
        // the individual contribution limit is not reached and also that
        // the total amount will not exceed the proposed phase limit
        uint256 amountUntilFundingRoundLimit = currentPhaseLimits
            .maxRaisedAmount - totalRaised;
        uint256 amountUntilIndividualimit = currentPhaseLimits
            .maxIndividualAmount - userContribution;

        // Return the smallest amount allowed to be contributed without reaching the limits
        return
            amountUntilFundingRoundLimit < amountUntilIndividualimit
                ? amountUntilFundingRoundLimit
                : amountUntilIndividualimit;
    }

    /// @notice Check if sender is allowed to contribute
    /// @dev Only the SEED phase has a whitelist requirement
    function _isAllowedToContribute() private view returns (bool) {
        if (phase == Phases.SEED && !seedWhitelist[msg.sender]) {
            return false;
        }

        return true;
    }

    /// @notice Send SPC tokens after a user contributes, or increase its claimable amount
    /// @notice if the tokens are not yet claimable.
    function _transferOrAddTokens() private {
        uint256 tokens = msg.value * 5; // 1 ETH = 5 SPC tokens

        if (phase == Phases.OPEN) {
            token.transfer(msg.sender, tokens);
        } else {
            userClaimableTokens[msg.sender] += tokens;
        }
    }

    /// @notice Withdraw all the funds to the treasury
    function withdraw() public onlyOwner {
        // Get the treasury contract from the SpaceCoin contract
        address treasury = address(token.TREASURY());

        // Transfer all available funds
        (bool success, ) = treasury.call{value: address(this).balance}("");
        require(success, "TRANSFER_FAILED");
    }
}

