import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Factory.sol";
import "hardhat/console.sol";
import "./Math.sol";

/// @title Exchange pair contract, between ETH and an ERC20 token
/// @author Rares Stanciu
/// @dev Extends ERC20 to allow minting/burning/transferring LP tokens
contract Pair is ERC20 {
    /// @notice Event emitted when liquidity is added
    event LiquidityAdded(
        uint256 ethAdded,
        uint256 tokensAdded,
        address indexed receiver
    );

    /// @notice Event emitted when liquidity is removed
    event LiquidityRemoved(uint256 lpTokensRemoved, address indexed receiver);

    /// @notice Event emitted when a swap takes place
    event Swap(
        uint256 ethOutput,
        uint256 tokensOutput,
        address indexed receiver
    );

    /// @notice Minimum liquidity of this pair. 1000 LP tokens
    uint256 public constant MINIMUM_LIQUIDITY = 1e3;

    /// @notice Fee taken on each swap (percentage of input)
    uint256 public constant FEE_PERCENTAGE = 1;

    /// @notice Token contract address
    ERC20 public token;

    /// @notice Exchange factory contract address
    Factory public factory;

    /// @notice Amount of ETH in the pair
    uint256 private ethReserve;

    /// @notice Amount of Tokens in the pair
    uint256 private tokenReserve;

    /// @notice Used as a reentrancy guard
    bool private mutex;

    /// @notice Allow execution only if deadline was not exceeded
    modifier onlyUntil(uint256 _deadline) {
        require(block.timestamp <= _deadline, "DEADLINE_EXCEEDED");
        _;
    }

    /// @notice Do not allow reentrancy in functions with this modifier
    modifier reentrancyGuard() {
        require(!mutex, "REENTRANCY_GUARD");
        mutex = true;
        _;
        mutex = false;
    }

    /// @notice Initializes the factory and token contract addresses
    /// @param _token Token contract address
    constructor(Factory _factory, ERC20 _token) ERC20("LP Token", "LPT") {
        require(address(_factory) != address(0), "FACTORY_REQUIRED");
        require(address(_token) != address(0), "TOKEN_REQUIRED");

        factory = _factory;
        token = _token;
    }

    /// @notice Add liquidity to this trading pair
    /// @dev Mints LP tokens, corresponding to user's share of the total pool
    /// @param _to Address of the user who receives the LP tokens
    /// @param _deadline Timestamp after which the liquidity adding becomes invalid
    function addLiquidity(address _to, uint256 _deadline)
        public
        payable
        onlyUntil(_deadline)
        reentrancyGuard
    {
        uint256 tokenAmount = token.balanceOf(address(this)) - tokenReserve;
        uint256 ethAmount = address(this).balance - ethReserve;

        require(tokenAmount > 0 && ethAmount > 0, "AMOUNTS_NOT_SENT");

        uint256 lpTokensAmount;
        if (totalSupply() > 0) {
            // Compute new tokens share of the total pool
            uint256 tokensShare = (tokenAmount * totalSupply()) / tokenReserve;

            // Compute new ETH share of the total pool
            uint256 ethShare = (ethAmount * totalSupply()) / ethReserve;

            // We mint the minimum of the two, in order to be sure
            // that we have enough of both (ETH and tokens)
            lpTokensAmount = tokensShare < ethShare ? tokensShare : ethShare;
        } else {
            // From the amount of LP tokens that must be minted,
            // we subtract the minimum liquidity
            lpTokensAmount =
                Math.sqrt(ethAmount * tokenAmount) -
                MINIMUM_LIQUIDITY;

            // Since we can't mint to the 0 address,
            // we mint the minimum LP tokens to the factory contract,
            // since we know that they won't be recovered.
            _mint(address(factory), MINIMUM_LIQUIDITY);
        }

        // Make sure we mint LP tokens
        require(lpTokensAmount > 0, "NO_LIQUIDITY_MINTED");

        // Mint LP tokens
        _mint(_to, lpTokensAmount);

        // Sync reserves
        syncReserves();

        emit LiquidityAdded(ethAmount, tokenAmount, _to);
    }

    /// @notice Remove liquidity from this trading pair
    /// @param _to Address of user who should receive the ETH and tokens
    /// @param _deadline Timestamp after which the liquidity removal becomes invalid
    /// @dev Burns LP tokens, corresponding to user's share of the total pool
    function removeLiquidity(address _to, uint256 _deadline)
        public
        onlyUntil(_deadline)
        reentrancyGuard
    {
        require(totalSupply() > 0, "NO_LIQUIDITY");

        uint256 lpTokensAmount = balanceOf(address(this));
        require(lpTokensAmount > 0, "NOT_ENOUGH_TOKENS");

        // Sync reserves
        syncReserves();

        // Compute user's share of the total pool
        uint256 ethToWithdraw = (ethReserve * lpTokensAmount) / totalSupply();
        uint256 tokensToWithdraw = (tokenReserve * lpTokensAmount) /
            totalSupply();

        // Burn LP tokens
        _burn(address(this), lpTokensAmount);

        // Withdraw ETH
        (bool success, ) = payable(_to).call{value: ethToWithdraw}("");
        require(success, "TRANSFER_FAILED");

        // Withdraw tokens
        token.transfer(_to, tokensToWithdraw);

        // Sync reserves
        syncReserves();

        emit LiquidityRemoved(lpTokensAmount, _to);
    }

    /// @notice Returns the current reserves (ETH & Token) balances
    function getReserves() public view returns (uint256, uint256) {
        return (ethReserve, tokenReserve);
    }

    /// @notice Sync the reserves with the actual balance of the contract
    function syncReserves() public {
        ethReserve = address(this).balance;
        tokenReserve = token.balanceOf(address(this));
    }

    /// @notice Exchange ETH and tokens
    /// @param _ethOutput Amount of ETH to receive from the exchange
    /// @param _tokenOutput Amount of tokens to receive from the exchange
    /// @param _to Receiver of the output ETH/tokens
    /// @param _deadline Timestamp after which the swap becomes invalid
    function swap(
        uint256 _ethOutput,
        uint256 _tokenOutput,
        address _to,
        uint256 _deadline
    ) public payable onlyUntil(_deadline) reentrancyGuard {
        require(_ethOutput > 0 || _tokenOutput > 0, "OUTPUT_AMOUNT_REQUIRED");
        require(_ethOutput == 0 || _tokenOutput == 0, "ONLY_ONE_OUTPUT");
        require(
            _ethOutput < ethReserve && _tokenOutput < tokenReserve,
            "NOT_ENOUGH_LIQUIDITY"
        );

        uint256 ethAmount = address(this).balance - ethReserve;
        uint256 tokenAmount = token.balanceOf(address(this)) - tokenReserve;

        if (_ethOutput > 0) {
            // If buying ETH, compute the required tokens
            uint256 ethReserveAfter = ethReserve - _ethOutput;
            uint256 tokensReserveAfter = (ethReserve * tokenReserve) /
                ethReserveAfter;
            uint256 requiredTokenInput = tokensReserveAfter - tokenReserve;

            // Add the fee to the required input
            uint256 requiredTokenInputWithFee = requiredTokenInput *
                (100 + FEE_PERCENTAGE);

            // Make sure enough tokens were transferred
            require(
                tokenAmount * 100 >= requiredTokenInputWithFee,
                "INVALID_TOKENS_INPUT"
            );

            // Transfer ETH
            (bool success, ) = _to.call{value: _ethOutput}("");
            require(success, "TRANSFER_FAILED");
        } else {
            // If buying tokens, compute the required ETH
            uint256 tokensReserveAfter = tokenReserve - _tokenOutput;
            uint256 ethReserveAfter = (ethReserve * tokenReserve) /
                tokensReserveAfter;
            uint256 requiredEthInput = ethReserveAfter - ethReserve;

            // Add the fee to the required input
            uint256 requiredEthInputWithFee = requiredEthInput *
                (100 + FEE_PERCENTAGE);

            // Make sure enough ETH were transferred
            require(
                ethAmount * 100 >= requiredEthInputWithFee,
                "INVALID_ETH_INPUT"
            );

            // Transfer tokens
            token.transfer(_to, _tokenOutput);
        }

        // Sync reserves
        syncReserves();

        emit Swap(_ethOutput, _tokenOutput, _to);
    }
}

