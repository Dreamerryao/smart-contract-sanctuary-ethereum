pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Factory.sol";
import "./Pair.sol";

/// @title Treasury contract
/// @author Rares Stanciu
/// @dev Holds funds and allows depositing to liquidity pools
contract Treasury {
    /// @notice  Treasury owner
    address public immutable owner;

    /// @notice Token contract
    ERC20 public immutable token;

    /// @notice Checks if the owner is the one calling
    modifier onlyOwner() {
        require(owner == msg.sender, "ONLY_OWNER");
        _;
    }

    constructor(address _owner, ERC20 _token) {
        require(_owner != address(0), "OWNER_REQUIRED");
        require(address(_token) != address(0), "TOKEN_REQUIRED");

        owner = _owner;
        token = _token;
    }

    /// @notice Transfer funds to a liquidity pool
    /// @param _factory Address of the Factory contract
    function depositToLiquidityPool(Factory _factory) external onlyOwner {
        require(address(_factory) != address(0), "FACTORY_REQUIRED");

        uint256 ethBalance = address(this).balance;
        uint256 tokenBalance = token.balanceOf(address(this));

        require(ethBalance > 0 && tokenBalance > 0, "NO_FUNDS_TO_DEPOSIT");

        // Find the exchange pair
        Pair pair = _factory.pairs(token);
        if (address(pair) == address(0)) {
            // Pair does not exist. We can create the pair with the entire liquidity
            token.approve(address(_factory), tokenBalance);
            _factory.createPair{value: ethBalance}(token, tokenBalance);
        } else {
            // Pair exists. We must compute the amounts based on the
            // reserves ratio from the pair.
            (uint256 ethReserve, uint256 tokenReserve) = pair.getReserves();

            // Compute the required tokens amount if we deposit the full amount of ETH
            uint256 requiredTokens = (ethBalance * tokenReserve) / ethReserve;

            // Compute the required ETH amount if we deposit the full amount of tokens
            uint256 requiredEth = (tokenBalance * ethReserve) / tokenReserve;

            if (requiredTokens <= tokenBalance) {
                // We deposit the full amount of ETH and corresponding amount of tokens
                token.transfer(address(pair), requiredTokens);
                pair.addLiquidity{value: ethBalance}(
                    msg.sender,
                    block.timestamp + 5 minutes
                );
            } else if (requiredEth <= ethBalance) {
                // We deposit the full amount of tokens and corresponding amount of ETH
                token.transfer(address(pair), tokenBalance);
                pair.addLiquidity{value: requiredEth}(
                    msg.sender,
                    block.timestamp + 5 minutes
                );
            } else {
                // Should not be possible
                revert("CANT_SATISFY_INVARIANT");
            }
        }
    }

    receive() external payable {}
}

