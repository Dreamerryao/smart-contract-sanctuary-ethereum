pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Treasury.sol";

/// @title SpaceCoin token contract
/// @author Rares Stanciu
contract SpaceCoin is ERC20 {
    // Emitted when the tax is enabled or disabled
    event TaxEnabledChanged(bool enabled);

    // % of each transfer that goes into the treasury
    uint256 public constant TAX_PRECENTAGE = 2;

    // Marks if the 2% (TAX_PERCENTAGE) tax is applied or not
    bool public taxEnabled = false;

    // Owner
    address public owner;

    // Treasury account
    Treasury public immutable TREASURY;

    /// @notice Checks if the owner is the one calling
    modifier onlyOwner() {
        require(owner == msg.sender, "ONLY_OWNER");
        _;
    }

    constructor(address _owner) ERC20("SpaceCoin", "SPC") {
        require(_owner != address(0), "OWNER_REQUIRED");
        owner = _owner;

        // We save treasury in a local variable since we can't read immutable variables
        // in the constructor.
        Treasury treasury = new Treasury(_owner, this);
        TREASURY = treasury;

        // The token will be deployed by the ICO contract.
        // Mint 150,000 SPC for the ICO and 350,000 for the Treasury
        _mint(msg.sender, 150_000 * (10**decimals()));
        _mint(address(treasury), 350_000 * (10**decimals()));
    }

    //
    // External functions
    //

    /// @notice Enables or disables the 2% (TAX_PERCENTAGE) tax on each transfer
    /// @param _taxEnabled true if tax is applied, false otherwise
    /// @dev The function must always change the state of the tax (applied or not)
    function setTaxEnabled(bool _taxEnabled) external onlyOwner {
        require(taxEnabled != _taxEnabled, "TAX_ENABLED_UNCHANGED");

        taxEnabled = _taxEnabled;

        emit TaxEnabledChanged(_taxEnabled);
    }

    /// @notice Override ERC20 _transfer to include the 2% tax
    /// @param sender Sender address
    /// @param recipient Recipient address
    /// @param amount Amount of SPC to transfer
    /// @dev Relies on the ERC20 to do the zero adress checks and for ensuring that
    /// @dev the sender has enough balance.
    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override {
        // If tax is disabled, use regular transfer
        if (!taxEnabled) {
            super._transfer(sender, recipient, amount);
        } else {
            // Else, apply tax
            uint256 tax = (amount * TAX_PRECENTAGE) / 100;
            uint256 amountAfterTax = amount - tax;

            super._transfer(sender, address(TREASURY), tax);
            super._transfer(sender, recipient, amountAfterTax);
        }
    }
}

