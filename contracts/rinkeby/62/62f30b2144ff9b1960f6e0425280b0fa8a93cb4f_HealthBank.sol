contract HealthBank is Context, Ownable, IERC20 {
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using SafeMath for uint256;

    address private _owner;
    string private _symbol;
    string private _name; 
    uint256 private _decimals;
    uint256 private _totalSupply;
    uint256 private _tokenReward;

    mapping(address => uint256) _balances;
    mapping(address => mapping(address => uint256)) _allowances;

    // Address to Index to Text Data
    mapping(address => uint256) _numOfHealthData;
    mapping(address => mapping(uint256 => string)) _healthData;

    constructor () {
        _owner = _msgSender();
        _symbol = "HB";
        _name = "Health Bank";
        _decimals = 18;
        _totalSupply = 1e9;
        _tokenReward = 100;
        _balances[_owner] = _totalSupply;
    }

    function aquireHealthData(address _patient, string memory _text) public returns(bool) {
        require(keccak256(abi.encode(_text)) != keccak256(abi.encode("")), "Empty data!");

        uint index = _numOfHealthData[_patient];
        _healthData[_patient][index] = _text;
        _numOfHealthData[_patient] += 1;
        
        transferFrom(_owner, _patient, _tokenReward);

        emit Transfer(_owner, _patient, _tokenReward);

        return true;
    }
    
    function retrieveHealthData(address _patient, uint256 _index) public view returns(string memory) {
        uint maxIndex = _numOfHealthData[_patient];
        require(_index < maxIndex, "That record does not exist!");
        
        return _healthData[_patient][_index];
    }
    
    function retrieveAllHealthData(address _patient) public view returns(string[] memory) {
        uint maxIndex = _numOfHealthData[_patient];
        
        string[] memory allTexts = new string[](maxIndex);
        
        for (uint i = 0; i < maxIndex; i++) {
            allTexts[i] = retrieveHealthData(_patient, i);
        }
        
        return allTexts;
    }


    function decimals() public pure returns (uint8) {
        return 18;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    /**
     * @dev Returns the amount of tokens owned by `account`.
     */
    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    /**
     * @dev Moves `amount` tokens from the caller's account to `recipient`.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transfer(address recipient, uint256 amount) public override returns (bool) {
        require(_balances[_msgSender()] >= amount, "Not enough balance!");

        _balances[_msgSender()] -= amount;
        _balances[recipient] += amount;
        
        emit Transfer(_msgSender(), recipient, amount);

        return true;
    }

    /**
     * @dev Returns the remaining number of tokens that `spender` will be
     * allowed to spend on behalf of `owner` through {transferFrom}. This is
     * zero by default.
     *
     * This value changes when {approve} or {transferFrom} are called.
     */
    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _allowances[_msgSender()][spender] = amount;

        emit Approval(_msgSender(), spender, amount);

        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        require(_balances[sender] >= amount, "Not enough balance!");
        require(recipient != address(0x0), "Not to a null address!");

        _balances[sender] -= amount;
        _balances[recipient] += amount;

        emit Transfer(sender, recipient, amount);

        return true;
    }
}


