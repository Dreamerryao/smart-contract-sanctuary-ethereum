// File contracts/Graffiti6TraxxStemz.sol


pragma solidity ^0.8.2;





contract Graffiti6TraxxStemz is ERC721A, IERC2981, Pausable, AccessControl, ErrorCodes {
    /// @dev Constants
    uint16 public constant TOKEN_LIMIT = 5000; 
    uint16 public constant MAX_TOKENS_PER_MINT = 5;
    uint256 public constant MINT_PRICE = 0.0303 ether;

    /// @dev Owner of the contract (purpose: OpenSea compatibility, etc.)
    address public owner;

    /// @dev for AccessControl
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    /// @dev Royalties
    struct RoyaltyInfo {
        address recipient;
        uint24 amount;
    }    
    RoyaltyInfo private _royaltyInfo;

    /// @dev tokenURI storage support
    mapping(uint256 => string) public nftURI;
    /// @dev if nftURI hasn't been set for a tokenID, baseNftURI will be used to construct the URI.
    string private _baseNftURI;

    /// @dev for OpenSea support of contract-level metadata: https://docs.opensea.io/docs/contract-level-metadata
    string private _contractURI;

    /// @dev Events
    event TraxxStemzMinted(address indexed minter, uint256 quantity);
    event RoyaltyUpdated(address recipientAddress, uint256 royaltyBps);
    event TraxxStemzURIUpdated(string contractURI);

    modifier onlyAdmin() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, msg.sender),
            throwError(ONLY_ADMIN)
        );
        _;
    }
        
    constructor(string memory contractUri, string memory baseNftURI, uint24 defaultRoyalty) ERC721A("Graffit6TraxxStemz", "G6TS")  {
        console.log("Deploying Graffit6TraxxStemz contract..");
        _contractURI = contractUri;
        _baseNftURI = baseNftURI;
        _royaltyInfo = RoyaltyInfo(msg.sender, defaultRoyalty);
        owner = msg.sender; 
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(OWNER_ROLE, msg.sender);
        _setupRole(PAUSER_ROLE, msg.sender);
    }

    receive() external payable { }

    // Overrides

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(
            ERC721A,
            AccessControl,
            IERC165
        ) returns (bool) {
            return super.supportsInterface(interfaceId) || interfaceId == type(IERC2981).interfaceId;
    }

    // tokenURI

    /**
     * @dev Base URI for computing {tokenURI}.  This will only be used if setTokenUri has
            never been called for a given token ID. It is used by the underlying ERC-721A
            contract.
     */
    function _baseURI() internal view override virtual returns (string memory) {
        return _baseNftURI;
    }

    function setTokenURI(uint256 tokenId, string calldata _URI) external onlyAdmin {
        require(_exists(tokenId), throwError(TOKEN_NOT_EXISTS));
        nftURI[tokenId] = _URI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), throwError(URI_QUERY_NON_EXISTENT_TOKEN));
        if (bytes(nftURI[tokenId]).length != 0) {
            return nftURI[tokenId];
        } else {
            return super.tokenURI(tokenId);
        }
    }

    // Minting

    function mint(uint256 quantity) external payable {
        require(totalSupply() + quantity < TOKEN_LIMIT, throwError(INVALID_TOKEN_AMOUNT));
        require(quantity <= MAX_TOKENS_PER_MINT, throwError(INVALID_TOKEN_AMOUNT));
        // TODO: is this right error code?
        require(msg.value >= MINT_PRICE * quantity, throwError(ASSET_INSUFFICIENT_ALLOWANCE_BALANCE));

        _safeMint(msg.sender, quantity);

        emit TraxxStemzMinted(msg.sender, quantity);
    }

    // Payments

    function withdraw(uint256 amount) public onlyAdmin {  
        // TODO: Is this the right error code?
        require(amount <= address(this).balance, throwError(INSUFFICIENT_BALANCE_FOR_TRANSFER_OR_ID_NOT_FOUND));
        // TODO: is owner the right payee? This is not updatable right now. Should it be?
        address payable payableOwner = payable(owner);
        payableOwner.transfer(amount);   
    }
    
    // Pausable support

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity)
        internal
        whenNotPaused
        override(ERC721A)
    {
        super._beforeTokenTransfers(from, to, startTokenId, quantity);
    }

    // Royalties

    /// @inheritdoc IERC2981
    function royaltyInfo(uint256, uint256 value)
        external
        view
        override
        returns (address receiver, uint256 royaltyAmount)
    {
        return(_royaltyInfo.recipient, (value * _royaltyInfo.amount) / 10000);
    }

    /// @dev Sets token royalties
    /// @param recipient recipient of the royalties
    /// @param value percentage (using 2 decimals - 10000 = 100, 0 = 0)
    function setRoyaltyInfo(address recipient, uint256 value) external onlyAdmin {
        require(value <= 10000, throwError(BPS_EXCEEDS_10000));
        _royaltyInfo = RoyaltyInfo(recipient, uint24(value));

        emit RoyaltyUpdated(recipient, value); 
    }

    // ContractURI for OpenSea support

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function setContractURI(string memory _uri) external onlyAdmin {
        _contractURI = _uri;
        emit TraxxStemzURIUpdated(_uri);
    }
}