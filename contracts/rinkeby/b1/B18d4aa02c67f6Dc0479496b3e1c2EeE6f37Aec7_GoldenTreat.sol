pragma solidity ^0.8.0;

// Contracts
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "./utils/IUniswapV2Factory.sol";
import "./utils/IUniswapV2Pair.sol";
import "./utils/IUniswapV2Router02.sol";
import "hardhat/console.sol";

contract GoldenTreat is Context, AccessControl, IERC20, IERC20Metadata {
    using SafeMath for uint256;
    struct VestingData {
        uint256 time;
        uint256 amount;
    }
    struct ExchangeTax {
        uint256 marketing;
        uint256 liquidity;
        uint256 buyBack;
        uint256 burned;
    }
    struct Checkpoint {
        uint256 fromBlock;
        uint256 votes;
    }
    string private _name;
    string private _symbol;
    uint256 private _totalSupply;
    uint256 private vestingPercentage = 9000;
    uint256 private vestingLockAfterPurchase = 165 days;
    uint256 private marketingBalance;
    uint256 private liquidityBalance;
    uint256 private buyBackBalance;
    uint256 public liquifyTriggerAmount;
    address private ROUTER;
    address private distributionContract;
    address private burnAddress = 0x000000000000000000000000000000000000dEaD;
    address private babyDogeAddress;
    address private PAIR;
    bool private tokenVesting;

    mapping(address => uint256) private _balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private exchangeAddress; //if exchangeRouter[address] = true then its a router
    mapping(address => bool) private _isExcludedFromFee; // Excluded from fee
    mapping(address => bool) private _AddLiquidity; // Excluded from fee
    mapping(address => mapping(uint256 => Checkpoint)) internal checkpoints;
    mapping(address => uint256) internal numCheckpoints;
    mapping(address => address) private delegates;
    mapping(address => VestingData) private tokenVestingData;
    mapping(address => VestingData) private liquidityLock;
    mapping(address => uint256) public nonces;
    mapping(bool => ExchangeTax) public buySellTax;

    bytes32 internal constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");
    bytes32 internal constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor(
        string memory name_,
        string memory symbol_,
        uint256 amount_,
        uint256 liquifyTriggerAmount_,
        address distributionContract_,
        address router_,
        address babyDoge_,
        bool tokenVesting_
    ) {
        _name = name_;
        _symbol = symbol_;
        tokenVesting = tokenVesting_;
        liquifyTriggerAmount = liquifyTriggerAmount_;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(GOVERNANCE_ROLE, _msgSender());
        _setupRole(MINTER_ROLE, _msgSender());
        setExcludedFromFeeAddress(msg.sender, true);
        mint(msg.sender, amount_);
        distributionContract = distributionContract_;
        ROUTER = router_;
        babyDogeAddress = babyDoge_;

        PAIR = IUniswapV2Factory(IUniswapV2Router02(ROUTER).factory())
            .createPair(address(this), IUniswapV2Router02(ROUTER).WETH());

        setExchangeAddress(ROUTER, true);
        setExchangeAddress(PAIR, true);
        setExcludedFromFeeAddress(PAIR, true);
        setExcludedFromFeeAddress(address(this), true);
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

    event Received(address, uint256);

    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    /// @notice An event thats emitted when an account changes its delegate
    event DelegateChanged(
        address indexed delegator,
        address indexed fromDelegate,
        address indexed toDelegate
    );

    /// @notice An event thats emitted when a delegate account's vote balance changes
    event DelegateVotesChanged(
        address indexed delegate,
        uint256 previousBalance,
        uint256 newBalance
    );

    /**
     * @dev Returns the name of the token.
     */
    function name() public view virtual override returns (string memory) {
        return _name;
    }

    /**
     * @dev Returns the symbol of the token, usually a shorter version of the
     * name.
     */
    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual override returns (uint8) {
        return 18;
    }

    /**
     * @dev See {IERC20-totalSupply}.
     */
    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    /**
     * @dev See {IERC20-balanceOf}.
     */
    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _balances[account];
    }

    /**
     * @dev Creates `amount` new tokens for `to`.
     *
     * See {ERC20-_mint}.
     *
     * Requirements:
     *
     * - the caller must have the `MINTER_ROLE`.
     */
    function mint(address to, uint256 amount)
        public
        virtual
        onlyRole(MINTER_ROLE)
    {
        _mint(to, amount);
    }

    function transfer(address recipient, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    /**
     * @dev See {IERC20-allowance}.
     */
    function allowance(address owner, address spender)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    /**
     * @dev See {IERC20-approve}.
     *
     * Requirements:
     *
     * - `spender` cannot be the zero address.
     */
    function approve(address spender, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);

        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(
            currentAllowance >= amount,
            "ERC20: transfer amount exceeds allowance"
        );
        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amount);
        }

        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender] + addedValue
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(
            currentAllowance >= subtractedValue,
            "ERC20: decreased allowance below zero"
        );
        unchecked {
            _approve(_msgSender(), spender, currentAllowance - subtractedValue);
        }

        return true;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        require(sender != address(0));
        require(recipient != address(0));

        uint256 amountIn = _beforeTokenTransfer(sender, recipient, amount);

        _moveDelegates(
            delegates[sender],
            delegates[recipient],
            amount,
            amountIn
        );

        _afterTokenTransfer();
    }

    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0));

        _beforeTokenTransfer(address(0), account, amount);

        _totalSupply += amount;
        _balances[account] += amount;
        if (tokenVesting) {
            _tokenVestingUpdate(account, amount, false);
        }

        emit Transfer(address(0), account, amount);

        _afterTokenTransfer();
    }

    function burn(uint256 amount) public {
        _burn(msg.sender, amount);
    }

    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0));

        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount);
        unchecked {
            _balances[account] = accountBalance - amount;
        }
        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0));
        require(spender != address(0));

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function lpPair() public view virtual returns (address) {
        return PAIR;
    }

    function SetFee(
        bool _buySell,
        uint256 _marketing,
        uint256 _liquidity,
        uint256 _burned,
        uint256 _buyBack
    ) public onlyRole(GOVERNANCE_ROLE) {
        ExchangeTax storage tax = buySellTax[_buySell];
        tax.marketing = _marketing;
        tax.liquidity = _liquidity;
        tax.burned = _burned;
        tax.buyBack = _buyBack;
    }

    function setExchangeAddress(address _account, bool _excluded)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        exchangeAddress[_account] = _excluded;
    }

    function setExcludedFromFeeAddress(address _account, bool _excluded)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        _isExcludedFromFee[_account] = _excluded;
    }

    function setLiquidityAddress(address _address, bool _excluded)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        _AddLiquidity[_address] = _excluded;
    }

    function setLiquifyTriggerAmount(uint256 _liquifyTriggerAmount)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        liquifyTriggerAmount = _liquifyTriggerAmount;
    }

    function setVestingSettings(
        uint256 _vestingPercentage,
        uint256 _vestingLockAfterPurchase,
        bool _tokenVesting
    ) public onlyRole(GOVERNANCE_ROLE) {
        vestingPercentage = _vestingPercentage;
        vestingLockAfterPurchase = _vestingLockAfterPurchase;
        tokenVesting = _tokenVesting;
    }

    function _tokenVestingUpdate(
        address _recipient,
        uint256 _amount,
        bool _liquidity
    ) internal {
        VestingData storage tokenVestingDataStorage;

        if (_liquidity) {
            tokenVestingDataStorage = liquidityLock[_recipient];
        } else {
            tokenVestingDataStorage = tokenVestingData[_recipient];
        }
        if (tokenVestingDataStorage.time == 0) {
            if (_amount > 0) {
                tokenVestingDataStorage.time = block.timestamp.add(
                    vestingLockAfterPurchase
                );
                tokenVestingDataStorage.amount = _amount
                    .mul(vestingPercentage)
                    .div(10000);
            }
        }
        // If wallet has prior transactions
        else {
            // Calculate how much time is left to vest tokens
            uint256 coolDownRemaining = tokenVestingDataStorage.time.sub(
                block.timestamp
            );
            // If there is no vesting, delete
            if (coolDownRemaining == 0) {
                delete tokenVestingData[_recipient];
            }
            // If there is vesting calculate remaining tokens that need vesting.
            else {
                uint256 remainingPercentage = coolDownRemaining
                    .mul(10**9)
                    .div(vestingLockAfterPurchase)
                    .mul(10000)
                    .div(10**9);

                tokenVestingDataStorage.amount = tokenVestingDataStorage
                    .amount
                    .mul(remainingPercentage)
                    .div(10000);
            }

            tokenVestingDataStorage.time = block.timestamp.add(
                vestingLockAfterPurchase
            );
            tokenVestingDataStorage.amount = tokenVestingDataStorage.amount.add(
                _amount.mul(vestingPercentage).div(10000)
            );
        }
    }

    function viewNotVestedTokens(address recipient)
        public
        view
        returns (uint256 locked)
    {
        VestingData memory transactionlocked = tokenVestingData[recipient];

        if (transactionlocked.time == 0) {
            locked = 0;
        } else {
            uint256 coolDownRemaining = transactionlocked.time.sub(
                block.timestamp
            );
            if (coolDownRemaining == 0) {
                locked = 0;
            } else {
                uint256 remainingPercentage = coolDownRemaining
                    .mul(10**9)
                    .div(vestingLockAfterPurchase)
                    .mul(10000)
                    .div(10**9);

                locked = transactionlocked.amount.mul(remainingPercentage).div(
                    10000
                );
            }
        }
    }

    function exchangeTransfer(
        address from,
        address to,
        uint256 amount,
        bool buy,
        bool tax
    ) internal returns (uint256 amountOut) {
        _tokenVestingUpdate(from, 0, false);
        VestingData storage transactionlocked = tokenVestingData[from];
        uint256 vestedTokens = balanceOf(from).sub(transactionlocked.amount);
        uint256 senderBalance = _balances[from];
        require(senderBalance >= amount);
        unchecked {
            _balances[from] = senderBalance - amount;
        }

        // Taxes
        if (tax) {
            amount -= _takeTax(buy, amount);
        }

        // If I am sending tokens that are not vested.
        if (amount > vestedTokens) {
            amount = recoverUnVested(from, amount, vestedTokens);
        }

        _balances[to] += amount;
        emit Transfer(from, to, amount);
        // Exchanges don't vest tokens!
        if (buy == true) {
            _tokenVestingUpdate(to, amount, false);
        }
        return amountOut = amount;
    }

    function recoverUnVested(
        address from,
        uint256 _amount,
        uint256 _vested
    ) internal returns (uint256 vestedAmount) {
        uint256 toRecover = _amount.sub(_vested);
        _balances[burnAddress] += toRecover;
        emit Transfer(from, burnAddress, toRecover);
        vestedAmount = _amount.sub(toRecover);
    }

    function standardTransfer(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256 amountOut) {
        _tokenVestingUpdate(from, 0, false);
        // First we check how much of the tokens will be burned.
        VestingData storage transactionlockedFrom = tokenVestingData[from];
        uint256 senderBalance = _balances[from];
        uint256 vestedTokens = balanceOf(from).sub(
            transactionlockedFrom.amount
        );
        require(senderBalance >= amount);
        unchecked {
            _balances[from] = senderBalance - amount;
        }
        if (amount > vestedTokens) {
            amount = recoverUnVested(from, amount, vestedTokens);
        }
        _balances[to] += amount;
        emit Transfer(from, to, amount);
        return amountOut = amount;
    }

    function taxTransfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        _balances[to] += amount;
        emit Transfer(from, to, amount);
    }

    function liquidityTransfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        uint256 senderBalance = _balances[from];
        require(senderBalance >= amount);

        if (_AddLiquidity[to]) {
            _tokenVestingUpdate(from, 0, false);
            _tokenVestingUpdate(from, 0, true);
            VestingData storage transactionlocked = tokenVestingData[from];
            uint256 percentage = (((amount * 10**9) / senderBalance) * 10000) /
                10**9;
            uint256 toReduce = transactionlocked.amount.mul(percentage).div(
                10000
            );
            transactionlocked.amount = transactionlocked.amount.sub(toReduce);
            if (toReduce > 0) {
                _tokenVestingUpdate(from, toReduce, true);
            }
        } else {
            _tokenVestingUpdate(from, 0, false);
            _tokenVestingUpdate(from, 0, true);
            VestingData storage transactionlocked = liquidityLock[to];
            uint256 percentage = (((amount * 10**9) / senderBalance) * 10000) /
                10**9;
            uint256 toReduce = transactionlocked.amount.mul(percentage).div(
                10000
            );
            transactionlocked.amount = transactionlocked.amount.sub(toReduce);
        }

        unchecked {
            _balances[from] = senderBalance - amount;
        }
        _balances[to] += amount;
        emit Transfer(from, to, amount);
    }

    function transferNoVesting(
        address from,
        address to,
        uint256 amount,
        bool buy,
        bool tax,
        bool standard
    ) internal returns (uint256 amountOut) {
        uint256 senderBalance = _balances[from];
        require(senderBalance >= amount);
        unchecked {
            _balances[from] = senderBalance - amount;
        }
        if (standard == false) {
            // Taxes
            if (tax) {
                amount -= _takeTax(buy, amount);
            }
        }
        _balances[to] += amount;
        emit Transfer(from, to, amount);
        return amountOut = amount;
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _amount
    ) internal virtual returns (uint256 amountOut) {
        if (_from != address(0)) {
            if (_AddLiquidity[_from] || _AddLiquidity[_to]) {
                liquidityTransfer(_from, _to, _amount);
            } else if (_to == address(this) || _to == burnAddress) {
                taxTransfer(_from, _to, _amount);
            } else if (exchangeAddress[_to]) {
                if (!_isExcludedFromFee[_from]) {
                    if (tokenVesting) {
                        return
                            amountOut = exchangeTransfer(
                                _from,
                                _to,
                                _amount,
                                false,
                                true
                            );
                    } else {
                        return
                            amountOut = transferNoVesting(
                                _from,
                                _to,
                                _amount,
                                false,
                                true,
                                false
                            );
                    }
                } else {
                    if (tokenVesting) {
                        return
                            amountOut = exchangeTransfer(
                                _from,
                                _to,
                                _amount,
                                false,
                                false
                            );
                    } else {
                        return
                            amountOut = transferNoVesting(
                                _from,
                                _to,
                                _amount,
                                false,
                                false,
                                false
                            );
                    }
                }
            } else if (exchangeAddress[_from]) {
                if (!_isExcludedFromFee[_from]) {
                    if (tokenVesting) {
                        return
                            amountOut = exchangeTransfer(
                                _from,
                                _to,
                                _amount,
                                true,
                                true
                            );
                    } else {
                        return
                            amountOut = transferNoVesting(
                                _from,
                                _to,
                                _amount,
                                true,
                                true,
                                false
                            );
                    }
                } else {
                    if (tokenVesting) {
                        return
                            amountOut = exchangeTransfer(
                                _from,
                                _to,
                                _amount,
                                true,
                                false
                            );
                    } else {
                        amountOut = transferNoVesting(
                            _from,
                            _to,
                            _amount,
                            true,
                            true,
                            false
                        );
                    }
                }
            } else {
                if (tokenVesting) {
                    return amountOut = standardTransfer(_from, _to, _amount);
                } else {
                    return
                        amountOut = transferNoVesting(
                            _from,
                            _to,
                            _amount,
                            false,
                            false,
                            true
                        );
                }
            }
        }
    }

    function _takeTax(bool _buySell, uint256 _amount)
        internal
        returns (uint256 amountAfterFee)
    {
        ExchangeTax storage tax = buySellTax[_buySell];

        uint256 _marketingBalance = _amount.mul(tax.marketing).div(10000);
        uint256 _liquidityAmount = _amount.mul(tax.liquidity).div(10000);
        uint256 _buyBackAmount = _amount.mul(tax.buyBack).div(10000);
        uint256 _burnAmount = _amount.mul(tax.burned).div(10000);

        uint256 totalFeeAmount = _marketingBalance
            .add(_liquidityAmount)
            .add(_buyBackAmount)
            .add(_burnAmount);

        if (totalFeeAmount > _burnAmount) {
            transfer(address(this), totalFeeAmount.sub(_burnAmount));
        }
        if (_marketingBalance != 0) {
            marketingBalance += _marketingBalance;
        }
        if (_liquidityAmount != 0) {
            liquidityBalance += _liquidityAmount;
        }
        if (_buyBackAmount != 0) {
            buyBackBalance += _buyBackAmount;
        }
        if (_burnAmount != 0) {
            transfer(burnAddress, _burnAmount);
        }
        return totalFeeAmount;
    }

    function _afterTokenTransfer() internal virtual {
        if (liquidityBalance.add(buyBackBalance) > liquifyTriggerAmount) {
            _swapAndLiquify();
        }
    }

    //take amount of tokens for lp fee + buybackBabyDoge Fee
    function _swapAndLiquify() internal {
        address weth = IUniswapV2Router02(ROUTER).WETH();

        // Split the Token balance into halves
        uint256 halfForTokenLP = liquidityBalance.div(2);
        uint256 totalToSwap = halfForTokenLP.add(buyBackBalance);
        buyBackBalance = 0;
        liquidityBalance = 0;

        uint256 ethBalanceBeforeSwap = address(this).balance;
        uint256 expectedBuyBackWETH = _getQuote(
            buyBackBalance,
            address(this),
            weth
        );
        _swapTokens(address(this), weth, totalToSwap, address(this));
        uint256 ethReceivedFromSwap = address(this).balance.sub(
            ethBalanceBeforeSwap
        );
        uint256 ethToLiquidity = ethReceivedFromSwap.sub(expectedBuyBackWETH);
        _addLiquidity(halfForTokenLP, ethToLiquidity);
        if (expectedBuyBackWETH > 0)
            _swapTokens(
                weth,
                babyDogeAddress,
                expectedBuyBackWETH,
                address(this)
            );

        IERC20(babyDogeAddress).transfer(
            distributionContract,
            IERC20(babyDogeAddress).balanceOf(address(this))
        );
        emit SwapAndLiquify(
            halfForTokenLP,
            ethReceivedFromSwap,
            halfForTokenLP
        );
    }

    function _addLiquidity(uint256 _tokenAmount, uint256 _ethAmount) private {
        _approve(address(this), ROUTER, _tokenAmount);
        IUniswapV2Router02(ROUTER).addLiquidityETH{value: _ethAmount}(
            address(this),
            _tokenAmount,
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    function _getQuote(
        uint256 _amountIn,
        address _fromTokenAddress,
        address _toTokenAddress
    ) internal view returns (uint256 amountOut) {
        address pair = IUniswapV2Factory(IUniswapV2Router02(ROUTER).factory())
            .getPair(_fromTokenAddress, _toTokenAddress);
        (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(pair)
            .getReserves();
        address token0 = IUniswapV2Pair(pair).token0();
        (uint256 reserveIn, uint256 reserveOut) = token0 == _fromTokenAddress
            ? (reserve0, reserve1)
            : (reserve1, reserve0);
        uint256 amountInWithFee = _amountIn.mul(997);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        uint256 denominator = reserveIn.mul(1000).add(amountInWithFee);
        amountOut = numerator.div(denominator);
    }

    function _swapTokens(
        address _FromTokenContractAddress,
        address _ToTokenContractAddress,
        uint256 _amountIn,
        address _forWallet
    ) internal returns (uint256 tokenBought) {
        address[] memory path;
        address WETH = IUniswapV2Router02(ROUTER).WETH();
        uint256 amountOutMin;

        if (_ToTokenContractAddress == WETH) {
            path = new address[](2);
            path[0] = _FromTokenContractAddress;
            path[1] = WETH;
            IERC20(path[0]).approve(ROUTER, _amountIn);
            amountOutMin = _getQuote(_amountIn, path[0], path[1]);
            tokenBought = IUniswapV2Router02(ROUTER).swapExactTokensForETH(
                _amountIn,
                amountOutMin,
                path,
                _forWallet,
                block.timestamp + 1200
            )[path.length - 1];
        } else {
            path = new address[](2);
            path[0] = WETH;
            path[1] = _ToTokenContractAddress;
            amountOutMin = _getQuote(_amountIn, path[0], path[1]);
            amountOutMin = amountOutMin.mul(8000).div(10000);
            tokenBought = IUniswapV2Router02(ROUTER).swapExactETHForTokens{
                value: _amountIn
            }(amountOutMin, path, _forWallet, block.timestamp + 1200)[
                path.length - 1
            ];
        }
    }

    function getCurrentVotes(address _account) external view returns (uint256) {
        uint256 nCheckpoints = numCheckpoints[_account];
        return
            nCheckpoints > 0
                ? checkpoints[_account][nCheckpoints - 1].votes
                : 0;
    }

    function delegate(address _delegatee) public {
        return _delegate(msg.sender, _delegatee);
    }

    function _delegate(address _delegator, address _delegatee) internal {
        address currentDelegate = delegates[_delegator];
        uint256 delegatorBalance = _balances[_delegator];
        delegates[_delegator] = _delegatee;

        emit DelegateChanged(_delegator, currentDelegate, _delegatee);

        _moveDelegates(
            currentDelegate,
            _delegatee,
            delegatorBalance,
            delegatorBalance
        );
    }

    function _moveDelegates(
        address _srcRep,
        address _dstRep,
        uint256 _amountOut,
        uint256 _amountIn
    ) internal {
        if (_srcRep != _dstRep && _amountOut > 0) {
            if (_srcRep != address(0)) {
                uint256 srcRepNum = numCheckpoints[_srcRep];
                uint256 srcRepOld = srcRepNum > 0
                    ? checkpoints[_srcRep][srcRepNum - 1].votes
                    : 0;
                uint256 srcRepNew = srcRepOld.sub(_amountOut);
                _writeCheckpoint(_srcRep, srcRepNum, srcRepOld, srcRepNew);
            }

            if (_dstRep != address(0)) {
                uint256 dstRepNum = numCheckpoints[_dstRep];
                uint256 dstRepOld = dstRepNum > 0
                    ? checkpoints[_dstRep][dstRepNum - 1].votes
                    : 0;
                uint256 dstRepNew = dstRepOld.add(_amountIn);
                _writeCheckpoint(_dstRep, dstRepNum, dstRepOld, dstRepNew);
            }
        }
    }

    function _writeCheckpoint(
        address _delegatee,
        uint256 _nCheckpoints,
        uint256 _oldVotes,
        uint256 _newVotes
    ) internal {
        uint256 blockNumber = block.number;

        if (
            _nCheckpoints > 0 &&
            checkpoints[_delegatee][_nCheckpoints - 1].fromBlock == blockNumber
        ) {
            checkpoints[_delegatee][_nCheckpoints - 1].votes = _newVotes;
        } else {
            checkpoints[_delegatee][_nCheckpoints] = Checkpoint(
                blockNumber,
                _newVotes
            );
            numCheckpoints[_delegatee] = _nCheckpoints + 1;
        }

        emit DelegateVotesChanged(_delegatee, _oldVotes, _newVotes);
    }

    function getPriorVotes(address account, uint256 blockNumber)
        public
        view
        returns (uint256)
    {
        require(blockNumber < block.number);
        uint256 nCheckpoints = numCheckpoints[account];
        console.log(nCheckpoints);
        if (nCheckpoints == 0) {
            return 0;
        }
        if (checkpoints[account][nCheckpoints - 1].fromBlock <= blockNumber) {
            console.log(checkpoints[account][nCheckpoints - 1].votes);
            return checkpoints[account][nCheckpoints - 1].votes;
        }
        if (checkpoints[account][0].fromBlock > blockNumber) {
            console.log(checkpoints[account][0].fromBlock);
            console.log(blockNumber);
            return 0;
        }

        uint256 lower = 0;
        uint256 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2;
            Checkpoint memory cp = checkpoints[account][center];
            if (cp.fromBlock == blockNumber) {
                return cp.votes;
            } else if (cp.fromBlock < blockNumber) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return checkpoints[account][lower].votes;
    }
}

