pragma solidity ^0.8.4;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "hardhat/console.sol";

/**
 * @title LootBox
 * LootBox - a randomized and openable lootbox of GameTokens
 */
contract LootBox is
    Ownable,
    Pausable,
    ReentrancyGuard,
    VRFConsumerBase,
    ERC1155Holder
{
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;
    using ERC165Checker for address;

    bytes4 public constant IID_IERC721 = type(IERC721).interfaceId;
    bytes4 public constant IID_IERC1155 = type(IERC1155).interfaceId;

    address private _verifier;
    uint256 public lastId;

    event CreatedLootBox(uint256 indexed lootBoxId, uint256 indexed keyId, LootBoxSettings lootBox);

    // Event for logging lootbox opens
    event LootBoxOpened(
        uint256 indexed lootBoxId,
        address indexed buyer,
        uint256 boxesPurchased,
        uint256[] issuedNfts
    );
    event RemovedLootBox(uint256 lootBoxId, address user);

    enum LootBoxType {
        GENERAL,
        WHITELISTED 
    }

    struct LootBoxSettings {
        address owner;
        // key id
        uint256 keyId;
        //type of lootBox
        LootBoxType lootBoxType;
        // Number of items to send per open.
        uint256 minQuantityPerOpen;
        // Number of items to send per open.
        // Set to 0 to disable this LootBox.
        uint256 maxQuantityPerOpen;
        // Probability in basis points (out of 10,000) of receiving each class (descending)
        uint256[] classProbabilities;
        uint256[] classes;
        address nftAddress;
        bool isERC721;
        // Number of lootbox
        uint256 remainingQuantity;
        // price of lootbox
        uint256 price;
        // start date
        uint256 startTime;
        // end date
        uint256 endTime;
        // currency
        IERC20 currency;
        bool status; // 1: alive ; 0: remove
    }

    struct LootBoxSettingParam {
        uint256 _keyId;
        LootBoxType _lootBoxType;
        uint256[2] _quantityPerOpen;
        uint256[][2] _classProbabilities; // [0]: class rarities; [1]: rarities (descending)
        address _nftAddress;
        uint256[][] _tokenIds;
        uint256[][] _amount1155;
        uint256 _remainingQuantity;
        uint256 _price;
        uint256[2] _dateTime; // prevent deep stack
        address _currency;
    }

    mapping(uint256 => LootBoxSettings) public lootBoxToSettings;

    // ERC1155
    // lootBox => tokenId => class => amount
    mapping(uint256 => mapping(uint256 => mapping(uint256 => uint256)))
        public lootBoxTokenIdClassToAmount;

    // ERC721
    mapping(address => EnumerableSet.UintSet) internal nftToTokenIds;
    // lootBox => class => tokenIds
    mapping(uint256 => mapping(uint256 => EnumerableSet.UintSet))
        internal lootBoxToClassTokenIds;

    bytes32 internal keyHash;
    uint256 internal fee;
    uint256 public randomResult;
    uint256 constant INVERSE_BASIS_POINT = 10000;
    uint256 constant PERCENT_BASIS_POINT = 10000;

    /**
     * @dev Example constructor. Calls createLootBoxSettings for you with
     *      sample settings
     */
    constructor(
        address _vrfCoordinator,
        address _linkToken,
        bytes32 _keyHash,
        uint256 _linkFee,
        address verifier_
    ) VRFConsumerBase(_vrfCoordinator, _linkToken) {
        keyHash = _keyHash;
        fee = _linkFee;
        _verifier = verifier_;
    }

    modifier validLootBox(uint256 _lootboxId) {
        require(_lootboxId > 0 && _lootboxId <= lastId, "Invalid lootbox");
        _;
    }

    //////
    // GETTER FUNCTIONS
    //////
    /**
     * @dev Get number of tokenIds in a class.
     * @param _lootBoxId The lootBox id
     * @param _classId The class id
     */
    function getNumberOfTokenIds(uint256 _lootBoxId, uint256 _classId)
        public
        view
        returns (uint256)
    {
        uint256[] memory tokenIds = getTokensAtClass(_lootBoxId, _classId);

        return tokenIds.length;
    }

    /**
     * @dev Get tokenId in a class and lootBox.
     * @param _lootBoxId The lootBox id
     * @param _classId The class id
     * @param _index The tokenId index
     */
    function getTokenId(
        uint256 _lootBoxId,
        uint256 _classId,
        uint256 _index
    ) external view returns (uint256) {
        (uint256 _indexClass, bool _success) = getClassIndex(
            _lootBoxId,
            _classId
        );
        if (_success) {
            return lootBoxToClassTokenIds[_lootBoxId][_indexClass].at(_index);
        }
        return 0;
    }

    /**
     * @dev Get all token in a class and lootBox.
     * @param _lootBoxId The lootBox id
     * @param _classId The class id
     */
    function getTokensAtClass(uint256 _lootBoxId, uint256 _classId)
        public
        view
        returns (uint256[] memory)
    {
        uint256[] memory _tokenIds;
        (uint256 _index, bool _success) = getClassIndex(_lootBoxId, _classId);
        if (_success) {
            _tokenIds = lootBoxToClassTokenIds[_lootBoxId][_index].values();
        }
        return _tokenIds;
    }

    /**
     * @dev Get class probabilities.
     * @param _lootBoxId The lootBox id
     * @param _classId The class id
     */
    function getClassProbabilities(uint256 _lootBoxId, uint256 _classId)
        external
        view
        returns (uint256)
    {
        LootBoxSettings memory settings = lootBoxToSettings[_lootBoxId];
        (uint256 _index, bool _success) = getClassIndex(_lootBoxId, _classId);
        if (_success) {
            return settings.classProbabilities[_index];
        }
        return 0;
    }

    function getClassIndex(uint256 _lootBoxId, uint256 _classId)
        public
        view
        returns (uint256, bool)
    {
        LootBoxSettings memory settings = lootBoxToSettings[_lootBoxId];
        for (uint256 i = 0; i < settings.classes.length; i++) {
            if (settings.classes[i] == _classId) {
                return (i, true);
            }
        }
        return (0, false);
    }

    /**
     * @dev Get nftAddress.
     * @param _lootBoxId The lootBox id
     */
    function getNftAddressOfLootBox(uint256 _lootBoxId)
        public
        view
        returns (address)
    {
        LootBoxSettings memory settings = lootBoxToSettings[_lootBoxId];

        return settings.nftAddress;
    }

    //////
    // INITIALIZATION FUNCTIONS FOR OWNER
    //////
    /**
     * @dev Set the settings for a particular lootbox
     */
    function createLootBoxSettings(
        LootBoxSettingParam memory lootBoxSettingParam
    ) external {
        lastId++;
        uint256 totalProbability = 0;
        bool _isERC721;
        require(
            isERC721(lootBoxSettingParam._nftAddress) ||
                isERC1155(lootBoxSettingParam._nftAddress),
            "Invalid nft address"
        );
        require(
            lootBoxSettingParam._classProbabilities[1].length ==
                lootBoxSettingParam._classProbabilities[0].length &&
                lootBoxSettingParam._tokenIds.length ==
                lootBoxSettingParam._classProbabilities[0].length,
            "Invalid classProbabilities"
        );

        if (isERC721(lootBoxSettingParam._nftAddress)) {
            _isERC721 = true;
        }

        for (
            uint256 i = 0;
            i < lootBoxSettingParam._classProbabilities[1].length;
            i++
        ) {
            totalProbability += lootBoxSettingParam._classProbabilities[1][i];

            if (_isERC721) {
                for (
                    uint256 j = 0;
                    j < lootBoxSettingParam._tokenIds[i].length;
                    j++
                ) {
                    _addToken721IdToClass(
                        lootBoxSettingParam._nftAddress,
                        lootBoxSettingParam._tokenIds[i][j],
                        _msgSender(),
                        lootBoxSettingParam._classProbabilities[0][i],
                        lastId
                    );
                }
            } else {
                require(
                    lootBoxSettingParam._tokenIds.length ==
                        lootBoxSettingParam._amount1155.length,
                    "Invalid input nft1155"
                );

                require(
                    lootBoxSettingParam._tokenIds[i].length ==
                        lootBoxSettingParam._amount1155[i].length,
                    "Invalid input nft1155"
                );
                for (
                    uint256 j = 0;
                    j < lootBoxSettingParam._tokenIds[i].length;
                    j++
                ) {
                    _addToken1155IdToClass(
                        lootBoxSettingParam._nftAddress,
                        lootBoxSettingParam._tokenIds[i][j],
                        lootBoxSettingParam._amount1155[i][j],
                        _msgSender(),
                        lootBoxSettingParam._classProbabilities[0][i],
                        lastId
                    );
                }
            }
        }

        require(
            totalProbability == INVERSE_BASIS_POINT,
            "LootBox#createLootBoxSettings: WRONG_TOTAL_PROBABILITY"
        );
        require(
            lootBoxSettingParam._quantityPerOpen[0] <=
                lootBoxSettingParam._quantityPerOpen[1],
            "LootBox#createLootBoxSettings: WRONG_QUANTITY_RANGE"
        );

        LootBoxSettings memory settings = LootBoxSettings({
            owner: _msgSender(),
            keyId: lootBoxSettingParam._keyId,
            lootBoxType: lootBoxSettingParam._lootBoxType,
            minQuantityPerOpen: lootBoxSettingParam._quantityPerOpen[0],
            maxQuantityPerOpen: lootBoxSettingParam._quantityPerOpen[1],
            classProbabilities: lootBoxSettingParam._classProbabilities[1],
            classes: lootBoxSettingParam._classProbabilities[0],
            nftAddress: lootBoxSettingParam._nftAddress,
            isERC721: _isERC721,
            remainingQuantity: lootBoxSettingParam._remainingQuantity,
            price: lootBoxSettingParam._price,
            startTime: lootBoxSettingParam._dateTime[0],
            endTime: lootBoxSettingParam._dateTime[1],
            currency: IERC20(lootBoxSettingParam._currency),
            status: true
        });

        lootBoxToSettings[lastId] = settings;

        emit CreatedLootBox(lastId, lootBoxSettingParam._keyId, settings);
    }

    /**
     * @dev Remove a lootBox.
     * @param _lootBoxId The LootBox to set remove
     */
    function removeLootBox(uint256 _lootBoxId)
        public
        validLootBox(_lootBoxId)
    {
        LootBoxSettings memory settings = lootBoxToSettings[_lootBoxId];
        require(settings.owner == _msgSender(), "User is not owner lootbox");

        for (uint256 i = 0; i < settings.classes.length; i++) {
            uint256[] memory tokenIds = getTokensAtClass(
                _lootBoxId,
                settings.classes[i]
            );
            if (isERC721(settings.nftAddress)) {
                for (uint256 j = 0; j < tokenIds.length; j++) {
                    _removeTokenId721FromClass(
                        settings.nftAddress,
                        _lootBoxId,
                        settings.classes[i],
                        tokenIds[j],
                        _msgSender()
                    );
                }
            } else if (isERC1155(settings.nftAddress)) {
                for (uint256 j = 0; j < tokenIds.length; j++) {
                    _removeTokenId1155FromClass(
                        settings.nftAddress,
                        _lootBoxId,
                        settings.classes[i],
                        tokenIds[j],
                        lootBoxTokenIdClassToAmount[_lootBoxId][tokenIds[j]][
                            settings.classes[i]
                        ],
                        _msgSender()
                    );
                }
            }
        }
        lootBoxToSettings[_lootBoxId].status = false;

        emit RemovedLootBox(_lootBoxId, _msgSender());
    }

    ///////
    // MAIN FUNCTIONS
    //////

    /**
     * @dev Open a lootbox manually and send what's inside to _toAddress
     * Convenience method for contract owner.
     */
    function open(
        uint256 _lootBoxId,
        address _toAddress,
        uint256 _amount,
        uint256 _discountPercent,
        uint256 _nonce,
        bytes calldata _signature
    ) external payable {
        _mint(
            _lootBoxId,
            _toAddress,
            _amount,
            _discountPercent,
            _nonce,
            _signature
        );
    }

    /**
     * @dev Main minting logic for lootboxes
     */
    function _mint(
        uint256 _lootBoxId,
        address _toAddress,
        uint256 _amount,
        uint256 _discountPercent,
        uint256 _nonce,
        bytes calldata _signature
    ) internal whenNotPaused nonReentrant {
        // Load settings for this box lootBox
        LootBoxSettings memory settings = lootBoxToSettings[_lootBoxId];

        require(
            block.timestamp > settings.startTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            block.timestamp < settings.endTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            settings.minQuantityPerOpen <= _amount,
            "LootBox#_mint: OPEN_TOO_LESS"
        );
        require(
            settings.maxQuantityPerOpen > 0,
            "LootBox#_mint: TIER_NOT_ALLOWED"
        );
        require(
            settings.remainingQuantity >= _amount,
            "LootBox#_mint: NOT_ENOUGH_QUANTITY"
        );
        require(
            settings.maxQuantityPerOpen >= _amount,
            "LootBox#_mint: OPEN_TOO_MANY"
        );

        uint256 cost = settings.price * _amount;

        if (settings.lootBoxType == LootBoxType.GENERAL) {
            if (!verifyMessage(_lootBoxId, _toAddress, _nonce, _signature)) {
                _discountPercent = 0;
            }
        } else {
            require(verifyMessage(_lootBoxId, _toAddress, _nonce, _signature), "User not in whiteList");
        }

        // lootBox discount
        cost =
            (cost * (PERCENT_BASIS_POINT - _discountPercent)) /
            PERCENT_BASIS_POINT;

        if (address(settings.currency) == address(0)) {
            // native currency
            require(msg.value == cost, "LootBox#_mint: WRONG_FEE");
        } else {
            settings.currency.transferFrom(msg.sender, address(this), cost);
        }

        uint256 totalMinted = 0;

        // getRandomNumber();

        uint256[] memory issuedNfts = new uint256[](_amount);

        while (totalMinted < _amount) {
            uint256 quantityOfRandomized = 1;
            uint256 class = _pickRandomClass(
                _lootBoxId,
                settings.classProbabilities,
                settings.classes
            );
            uint256[] memory nfts = _sendTokenWithClass(
                _lootBoxId,
                class,
                _toAddress,
                quantityOfRandomized
            );
            for (uint256 j = 0; j < quantityOfRandomized; j++) {
                issuedNfts[totalMinted + j] = nfts[j];
            }
            totalMinted += quantityOfRandomized;
        }

        lootBoxToSettings[_lootBoxId].remainingQuantity -= totalMinted;

        // Event emissions
        emit LootBoxOpened(_lootBoxId, _toAddress, _amount, issuedNfts);
    }

    function withdraw(address _tokenAddress) public onlyOwner {
        if (_tokenAddress == address(0)) {
            // native currency
            uint256 balance = address(this).balance;
            require(balance > 0, "LootBox#withdraw: NO_BALANCE");
            payable(msg.sender).transfer(balance);
        } else {
            IERC20 withDrawToken = IERC20(_tokenAddress);
            uint256 balance = withDrawToken.balanceOf(address(this));
            require(balance > 0, "LootBox#withdraw: NO_BALANCE");
            withDrawToken.transfer(
                msg.sender,
                withDrawToken.balanceOf(address(this))
            );
        }
    }

    /////
    // HELPER FUNCTIONS
    /////

    function verifyMessage(
        uint256 _lootBoxId,
        address _toAddress,
        uint256 _nonce,
        bytes memory signature
    ) public view returns (bool) {
        if (signature.length == 0) return false;
        bytes32 dataHash = encodeData(_lootBoxId, _toAddress, _nonce);
        bytes32 signHash = ECDSA.toEthSignedMessageHash(dataHash);
        address recovered = ECDSA.recover(signHash, signature);
        return recovered == _verifier;
    }

    function encodeData(
        uint256 _lootBoxId,
        address _toAddress,
        uint256 _nonce
    ) public view returns (bytes32) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return keccak256(abi.encode(id, _lootBoxId, _toAddress, _nonce));
    }

    function isERC1155(address _contractAddr) public view returns (bool) {
        return _contractAddr.supportsInterface(IID_IERC1155);
    }

    function isERC721(address _contractAddr) public view returns (bool) {
        return _contractAddr.supportsInterface(IID_IERC721);
    }

    // Returns the tokenId sent to _toAddress
    function _sendTokenWithClass(
        uint256 _lootBoxId,
        uint256 _class,
        address _toAddress,
        uint256 _amount
    ) internal returns (uint256[] memory) {
        uint256[] memory result = new uint256[](_amount);
        for (uint256 i = 0; i < _amount; i++) {
            uint256 tokenId = _pickRandomIdForClass(_lootBoxId, _class);
            address _nftAddress = getNftAddressOfLootBox(_lootBoxId);
            // if loot box is in eth and bsc
            // no nft contract is deployed so the system will mint the token on polygon
            if(isERC721(_nftAddress)){
                _removeTokenId721FromClass(
                    _nftAddress,
                    _lootBoxId,
                    _class,
                    tokenId,
                    _toAddress
                );
            }else{
                _removeTokenId1155FromClass(
                    _nftAddress,
                    _lootBoxId,
                    _class,
                    tokenId,
                    _amount,
                    _toAddress
                );
            }
            
            result[i] = tokenId;
        }

        return result;
    }

    function _pickRandomClass(
        uint256 _lootBoxId,
        uint256[] memory _classProbabilities,
        uint256[] memory _classes
    ) internal returns (uint256) {
        uint256 value = uint16(_random().mod(INVERSE_BASIS_POINT));
        // Start at top class (length - 1)
        // skip common (0), we default to it
        for (uint256 i = _classProbabilities.length - 1; i > 0; i--) {
            uint256 probability = _classProbabilities[i];
            if (value < probability) {
                if (getNumberOfTokenIds(_lootBoxId, _classes[i]) > 0)
                    return _classes[i];
            } else {
                value = value - probability;
            }
        }

        return _classes[0];
    }

    function _pickRandomIdForClass(uint256 _lootBoxId, uint256 _class)
        internal
        returns (uint256)
    {
        uint256[] memory tokenIds = getTokensAtClass(_lootBoxId, _class);

        require(
            tokenIds.length > 0,
            "LootBox#_pickRandomIdForClass: NO_TOKEN_ON_CLASS"
        );

        uint256 randIndex = _random().mod(tokenIds.length);

        return tokenIds[randIndex];
    }

    /**
     * Requests randomness
     */
    function getRandomNumber() internal returns (bytes32 requestId) {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        return requestRandomness(keyHash, fee);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32, uint256 randomness) internal override {
        randomResult = randomness;
    }

    /**
     * @dev Pseudo-random number generator
     */
    function _random() internal returns (uint256) {
        uint256 randomNumber = uint256(
            keccak256(
                abi.encodePacked(
                    blockhash(block.number - 1),
                    msg.sender,
                    randomResult
                )
            )
        );
        randomResult = randomNumber;
        return randomNumber;
    }

    function _addToken721IdToClass(
        address _token,
        uint256 _tokenId,
        address _owner,
        uint256 _class,
        uint256 _lootBoxId
    ) internal {
        nftToTokenIds[_token].add(_tokenId);
        lootBoxToClassTokenIds[_lootBoxId][_class].add(_tokenId);

        IERC721(_token).transferFrom(_owner, address(this), _tokenId);
    }

    function _addToken1155IdToClass(
        address _token,
        uint256 _tokenId,
        uint256 _amount,
        address _owner,
        uint256 _class,
        uint256 _lootBoxId
    ) internal {
        nftToTokenIds[_token].add(_tokenId);
        lootBoxToClassTokenIds[_lootBoxId][_class].add(_tokenId);

        lootBoxTokenIdClassToAmount[_lootBoxId][_tokenId][_class] = _amount;
        IERC1155(_token).safeTransferFrom(
            _owner,
            address(this),
            _tokenId,
            _amount,
            ""
        );
    }

    function _removeTokenId721FromClass(
        address _token,
        uint256 _lootBoxId,
        uint256 _class,
        uint256 _tokenId,
        address _owner
    ) internal {
        nftToTokenIds[_token].remove(_tokenId);
        lootBoxToClassTokenIds[_lootBoxId][_class].remove(_tokenId);

        IERC721(_token).transferFrom(address(this), _owner, _tokenId);
    }

    function _removeTokenId1155FromClass(
        address _token,
        uint256 _lootBoxId,
        uint256 _class,
        uint256 _tokenId,
        uint256 _amount,
        address _owner
    ) internal {
        require(
            lootBoxTokenIdClassToAmount[_lootBoxId][_tokenId][_class] >=
                _amount,
            "Not enough amount"
        );
        lootBoxTokenIdClassToAmount[_lootBoxId][_tokenId][
            _class
        ] -= _amount;
        if (
            lootBoxTokenIdClassToAmount[_lootBoxId][_tokenId][_class] == 0
        ) {
            nftToTokenIds[_token].remove(_tokenId);
            lootBoxToClassTokenIds[_lootBoxId][_class].remove(_tokenId);
        }
        IERC1155(_token).safeTransferFrom(
            address(this),
            _owner,
            _tokenId,
            _amount,
            ""
        );
    }


    /**
     * @dev Triggers stopped state.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     */
    function unpause() external onlyOwner {
        _unpause();
    }
}

