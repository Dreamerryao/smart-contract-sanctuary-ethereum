/**
 * Toolset for deriving BOOM Town in-house NFT tokens rarities.
 * It assumes two periods: pre-reveal and post-reveal.
 * Pre-reveal period: NFT tokens are minted till the mint count cap is reached,
 * only in-house tokens mint events increase the mint counter. Token rarities cannot be derived.
 * Post-reveal period: The mint cap is reached. Token rarities can be derived.
 *
 * Upgrade: all the values (the rarity distrib table, entropy commitment and the entropy, blocknum and
 *          blockhash) are being transfered to the upgraded contract instance automatically
 * 
 * Dependencies:
 * - TagStorage
 *
 * Intructions: 
 * - after minting an in-house token, such token must be marked explicitly as the 
 *   'in-house' token by calling setInhouseNFT from BOOMTypeProcessor by minter
 * - the mint counter is set by setRevealCountLimit from BOOMTypeProcessor by minter
 * - the pre-reveal period is switched to the post-reveal one by calling fixBlocknum
 *   by minter. The fixBlocknum saves the block number when the mint cap has been reached.
 *   NOTE: the operator MUST call setBlockHash within 256 blocks after fixBlocknum was called.
 *   As the remedy of a late setBlockHash call, the operator may set explicitly the hash of the block
 *   when the post-reveal period started, though this scenario requires full trust towards the
 *   BOOM Town operator
 * - the BOOM Town operator's entropy is committed by calling init by the operator
 * - the BOOM Town operator's entropy is set/revealed by calling setEntropy by the operator
 * - the rarity distribution is set by setRarityTableLength, setRarityPopulationSize and setRarityTableElement
 *   by the operator
 */

pragma solidity ^0.8.0;

import "./common/BOOMBase.sol";
import "./TagStorage.sol";

contract BOOMRarityTable is BOOMBase {

    bytes32 public constant BOOM_RARITY_TABLE_GROUPID = keccak256(abi.encodePacked('BOOM_RARITY_TABLE_GROUPID'));

    TagStorage public tags;

    bytes32 public commitment;
    bytes32 public entropy;
    bytes32 public blockHash;
    uint256 public blockNum;

    uint256[] public tokenRarity;
    uint256   public tokenPopulationSize;

    event TAGLink(address addr);
    event Commit(bytes32 _commitment);
    event TableLength(uint256 _length);
    event TableElement(uint256 _index, uint256 _value);
    event PopulationSize(uint256 _value);
    event Entropy(bytes32 _value);
    event BlockNum(uint256 _value);
    event BlockHash(bytes32 _value);

    /**
     * Recovers all the values after the contract instance upgrade
     */
    constructor() {

    }

    function getId() override view public returns(bytes32) {
	return BOOM_RARITY_TABLE_CONTRACT_ID;
    }

    function getName() override view public returns(string memory) {
	return BOOM_RARITY_TABLE_CONTRACT_NAME;
    }

    function getVersion() override virtual view external returns(bytes32) {
	return keccak256(abi.encodePacked('v1.0')); // First release
    }

    function onListenAdded(bytes32 hname, address contractInstance, bool isNew) override public onlyManager {
	_onListenAdded(hname, contractInstance, isNew);
    }

    function _onListenAdded(bytes32 hname, address contractInstance, bool isNew) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
	    _linkTAG(contractInstance);
	    if(!isNew){
		if(commitment != bytes32(0))
		    _init(commitment);
		if(tokenRarity.length > 0){
		    _setRarityTableLength(tokenRarity.length);
		    for(uint i=0;i<tokenRarity.length;i++)
			_setRarityTableElement(i, tokenRarity[i]);
		}
		if(tokenPopulationSize > 0)
		    _setRarityPopulationSize(tokenPopulationSize);
		if(entropy != bytes32(0))
		    _setEntropy(entropy);
		if(blockNum > 0)
		    _fixBlocknum(blockNum);
		if(blockHash != bytes32(0))
		    _setBlockHash(blockHash);
	    }
	}else
	    super._onListenAdded(hname, contractInstance, isNew);
    }


    function onListenRemoved(bytes32 hname) override public onlyManager {
	_onListenRemoved(hname);
    }

    function _onListenRemoved(bytes32 hname) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
	    tags = TagStorage(address(0));
	}else
	super._onListenRemoved(hname);
    }


/**
     * @notice Sets link to Tag Storage smart contract
    **/
    function linkTAG(address tag_storage) public virtual onlyOwner {
        _linkTAG(tag_storage);
    }

    function _linkTAG(address tag_storage) internal {
        tags = TagStorage(tag_storage);

	bytes32 tag_key = _getCommitmentKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    commitment = tags.getByte32Value(tag_key);
	tag_key = _getRarityLengthKey();
	if(tags.getTagType(tag_key) != Util.StatType.None){
	    uint _length = _getRarityTableLength();
	    tokenPopulationSize = _getRarityPopulationSize();
	    for(uint8 i=0;i<_length;i++){
		uint256 _element = _getRarityTableElement(i);
		tokenRarity.push(_element);
	    }
	}
	tag_key = _getEntropyKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    entropy = tags.getByte32Value(tag_key);
	tag_key = _getBlockhashKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    blockHash = tags.getByte32Value(tag_key);
	tag_key = _getBlocknumKey();
	if(tags.getTagType(tag_key) != Util.StatType.None)
	    blockNum = tags.getIntValue(tag_key);

        emit TAGLink(tag_storage);
    }


    /**
     * Initializes the VRF with the BOOM Town operator entropy commitment.
     * The operator can commit the entropy just once
     *
     * @param _commitment Entropy commitment
     */
    function init(bytes32 _commitment) public virtual onlyOperator {
	_init(_commitment);
	commitment = _commitment;

	emit Commit(_commitment);
    }

    function _init(bytes32 _commitment) internal virtual {
	bytes32 tag_key = _getCommitmentKey();
