pragma solidity ^0.8;

import "./TagStorage.sol";
import "./BOOMMinter.sol";

import "./common/BOOMBase.sol";
import "./IFactoryERC721.sol";


contract BOOMFactory is IFactoryERC721, BOOMBase {

    bytes32 public constant FACTORY_GROUPID = keccak256(abi.encodePacked('FACTORY_GROUPID'));

    TagStorage	public tags;
    BOOMMinter	public minter;

    address	public proxyRegistryAddress;

    event Transfer(
        address indexed from,
        address indexed to,
        uint256 indexed tokenId
    );

    event TAGLink(address addr);
    event MinterLink(address addr);
    event ProxyLink(address addr);

    constructor() {

    }

    modifier isProxiedMinter() {
	// Must be sent from the owner proxy or owner.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        require(
            address(proxyRegistry.proxies(owner())) == _msgSender() ||
                owner() == _msgSender() ||
		hasRole(Util.ROLE_MINTER, _msgSender()),
	    "BOOMFactory: Must be called by the owner proxy, owner or minter"
        );
	_;
    }

    modifier isMintable(uint256 _optionId) {
	require(canMint(_optionId),"The option must be mintable");
	_;
    }

    function getId() override view public returns(bytes32) {
	return BOOM_FACTORY_CONTRACT_ID;
    }

    function getName() override view public returns(string memory) {
	return BOOM_FACTORY_CONTRACT_NAME;
    }

    function getVersion() override virtual view external returns(bytes32) {
	return keccak256(abi.encodePacked('v1.0')); // First release
    }

    function onListenAdded(bytes32 hname, address contractInstance, bool isNew) override public onlyManager {
	_onListenAdded(hname, contractInstance, isNew);
    }

    function _onListenAdded(bytes32 hname, address contractInstance, bool isNew) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
    	    _linkTAG(contractInstance);
	}else 
	if(hname == MINTER_CONTRACT_ID){
    	    _linkMinter(contractInstance);
	}else
	if(hname == PROXY_REGISTRY_CONTRACT_ID){
    	    _linkProxy(contractInstance);
	}else
    	    super._onListenAdded(hname, contractInstance, isNew);
    }

    function onListenRemoved(bytes32 hname) override public onlyManager {
	_onListenRemoved(hname);
    }

    function _onListenRemoved(bytes32 hname) internal override {
	if(hname == TAG_STORAGE_CONTRACT_ID){
	    tags = TagStorage(address(0));
	}else
	if(hname == MINTER_CONTRACT_ID){
	    minter = BOOMMinter(address(0));
	}else
	if(hname == PROXY_REGISTRY_CONTRACT_ID){
	    proxyRegistryAddress = address(0);
        }else
    	    super._onListenRemoved(hname);
    }


    /**
     * @notice Sets link to Tag Storage smart contract
    **/
    function linkTAG(address tag_storage) public virtual onlyOwner {
	_linkTAG(tag_storage);
    }

    function _linkTAG(address tag_storage) internal {
        tags = TagStorage(tag_storage);
        emit TAGLink(tag_storage);
    }


    /**
     * @notice Sets link to Tag Storage smart contract
    **/
    function linkMinter(address addr) public virtual onlyOwner {
	_linkMinter(addr);
    }

    function _linkMinter(address addr) internal {
        minter = BOOMMinter(addr);
        emit MinterLink(addr);
	fireTransferEvents(address(0), owner());
    }

    function linkProxy(address _addr) public onlyOwner {
	_linkProxy(_addr);
    }

    function _linkProxy(address _addr) internal {
	proxyRegistryAddress = _addr;
	emit ProxyLink(_addr);
    }


    function name() override public view returns (string memory) {
	return tags.getStringValue(_getNameKey());
    }

    function symbol() override public view returns (string memory) {
	return tags.getStringValue(_getSymbolKey());
    }

    function numOptions() override public view returns(uint256) {
	return minter.numOptions();
    }

    function canMint(uint256 _optionId) override public view returns(bool) {
	return minter.canMint(_optionId);
    }

    function tokenURI(uint256 _optionId) override public view returns(string memory) {
	return minter.tokenURI(_optionId);
    }

    function supportsFactoryInterface() override public pure returns (bool) {
        return true;
    }

    /**
     * Indicates the Wyvern schema name for assets in this lootbox, e.g. "ERC1155"
     */
    function factorySchemaName() public view returns (string memory) {
	return 'ERC721';
    }

    function mint(uint256 _optionId, address _toAddress) override public isProxiedMinter isMintable(_optionId) {
	minter.mint(_optionId, _toAddress);
    }

    function transferOwnership(address newOwner) override public onlyOwner {
        address _prevOwner = owner();
        super.transferOwnership(newOwner);
        fireTransferEvents(_prevOwner, newOwner);
    }

    function fireTransferEvents(address _from, address _to) private {
        for (uint256 i = 0; i < minter.numOptions(); i++) {
            emit Transfer(_from, _to, i);
        }
    }

    function setName(string calldata _name) public onlyOperator {
	tags.setTag(
	    FACTORY_GROUPID,
	    _getNameKey(),
	    _name
	);
    }

    function setSymbol(string calldata _symbol) public onlyOperator {
	tags.setTag(
	    FACTORY_GROUPID,
	    _getSymbolKey(),
	    _symbol
	);
    }

    /**
     * Hack to get things to work automatically on OpenSea.
     * Use transferFrom so the frontend doesn't have to worry about different method names.
     */
    function transferFrom(
        address,
        address _to,
        uint256 _tokenId
    ) public {
        mint(_tokenId, _to);
    }

    /**
     * Hack to get things to work automatically on OpenSea.
     * Use isApprovedForAll so the frontend doesn't have to worry about different method names.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        returns (bool)
    {
        if (owner() == _owner && _owner == _operator) {
            return true;
        }

        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (
            owner() == _owner &&
            address(proxyRegistry.proxies(_owner)) == _operator
        ) {
            return true;
        }

        return hasRole(Util.ROLE_MINTER, _msgSender());
    }

    /**
     * Hack to get things to work automatically on OpenSea.
     * Use ownerOf so the frontend doesn't have to worry about different method names.
     */
    function ownerOf(uint256) public view returns (address _owner) {
        return owner();
    }


    function _getNameKey() internal pure returns(bytes32) {
	return keccak256(abi.encodePacked('BOOM_NAME_TAG'));
    }

    function _getSymbolKey() internal pure returns(bytes32) {
	return keccak256(abi.encodePacked('BOOM_SYMBOL_TAG'));
    }

}

