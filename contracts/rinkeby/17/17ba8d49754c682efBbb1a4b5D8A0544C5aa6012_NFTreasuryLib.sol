    function getTicketBalance(Treasury storage _treasury, uint256 tokenID)
        internal
        view
        returns (uint256)
    {
        return (_treasury.remainingTicketBalance[tokenID]);
    }

    /*****************************************
     *
     * TODO: this should be called once to determine the main values
     * then just read the values and split the revenue
     * strong case to be made to use ERC155 here, because all tokens with the same ID can share same formula
     * but we are free to create different token classes identified by their tokenIDs
     *
     ******************************************/
    function splitRevenue(
        Treasury storage _treasury,
        Ticket memory _ticket,
        buyNFTicketParams memory params,
        uint256 value
    ) internal returns (uint256 transactionFee, uint256 r) {
        string memory _msg;
        uint256 f = _ticket.resellerFee.add(_ticket.serviceFee);

        // r is the ratio of reseller vs. total in per thousand, e.g 100 in case of 90 service and 10 reseller fee
        r = _ticket.resellerFee.mul(_treasury.ratioBASE);
        r = r.div(f);

        // total transaction fee = _treasury.TRANSACTIONFEE * (f = resellerFee + serviceFee)
        transactionFee = f.mul(_treasury.TRANSACTIONFEE);
        transactionFee = transactionFee.div(_treasury.ratioBASE);

        // transactionFee is split among reseller and service provider along
        // reseller: r * transactionFee
        // service provider: (1-r) * transactionFee
        uint256 resellerTransaction = transactionFee.mul(r);
        resellerTransaction = resellerTransaction.div(_treasury.ratioBASE);
        uint256 serviceTransaction = transactionFee.sub(resellerTransaction);

        // update ticket fees
        _ticket.serviceFee = _ticket.serviceFee.sub(serviceTransaction);
        _ticket.resellerFee = _ticket.resellerFee.sub(resellerTransaction);
        _ticket.transactionFee = transactionFee;

        require(value <= (_ticket.resellerFee + _ticket.serviceFee + transactionFee),"value too small");

        // Treasury keeps TX fees
        _treasury.bank[_treasury.owner] += transactionFee;

        /********* START Treasury magicK ***********
         *
         * update pools and allowances
         * bracket by verifying distribution BEFORE and AFTER
         *
         ********************************************/
        verifyDistribution(_treasury, params, _ticket);
        if ((_ticket.serviceDescriptor & IS_CERTIFICATE) == IS_CERTIFICATE) {
            distributeCertificateRevenueToPool(_treasury, params, _ticket);
        } else if ((_ticket.serviceDescriptor & IS_TICKET) == IS_TICKET) {
            distributeTicketRevenueToPool(_treasury, params, _ticket);
        } else {
            _msg = "service Descriptor ";
            _msg = string(
                abi.encodePacked(
                    _msg,
                    Strings.toHexString(_ticket.serviceDescriptor),
                    "neither CERT nor TICKET"
                )
            );
        }
        verifyDistribution(_treasury, params, _ticket);
        /********* END of Treasury magicK ***********/

        setTicketData(_treasury, _ticket.tokenID, _ticket);
        return (transactionFee, r);
    }

    function distributeCertificateRevenueToPool(
        Treasury storage _treasury,
        buyNFTicketParams memory params,
        Ticket memory _ticket
    ) internal {
        address resellerWallet = getResellerWallet(_treasury, params.serviceProvider);
        address serviceProviderWallet = getServiceProviderWallet(_treasury, params.serviceProvider);
        // serviceProvider share
        _treasury.serviceProviderAccount[params.serviceProvider].serviceProviderPool 
            += _ticket.serviceFee;
        TransferHelper.safeApprove(_treasury.ERC20Token, serviceProviderWallet,
            _treasury.serviceProviderAccount[params.serviceProvider].serviceProviderPool);

        // reseller share
        _treasury.serviceProviderAccount[params.serviceProvider].resellerPool += _ticket.resellerFee;
        TransferHelper.safeApprove(_treasury.ERC20Token, resellerWallet, 
            _treasury.serviceProviderAccount[params.serviceProvider].resellerPool);
    }

    function presentTicketFeeToPool(Treasury storage _treasury, Ticket memory _t, 
        uint256 credits, address ERC20Token) 
        internal 
    {
        string memory _msg;
        address serviceProviderWallet = getServiceProviderWallet(_treasury, _t.serviceProvider);

        uint256 originalServiceProviderPool = getServiceProviderPool(_treasury, _t.serviceProvider);
        _msg = "Service Provider pool and allowance differ ";
        uint256 originalServiceProviderAllowance = IERC20(ERC20Token).allowance(address(this), serviceProviderWallet);
        _msg = string(abi.encodePacked(_msg,Strings.toString(originalServiceProviderPool)," ", Strings.toString(originalServiceProviderAllowance)));
        require(originalServiceProviderPool == originalServiceProviderAllowance, _msg);

        uint256 valueCredited = ( _treasury.remainingTicketBalance[_t.tokenID] * credits) / _t.credits;
        require(_treasury.remainingTicketBalance[_t.tokenID] >= valueCredited, "tokenBalance insufficient");
        require(_treasury.serviceProviderAccount[_t.serviceProvider].serviceTicketPool >= valueCredited, 
            "serviceTicketPool insufficient");
      
        // Commit TX
        _treasury.remainingTicketBalance[_t.tokenID] -= valueCredited;
        _treasury.serviceProviderAccount[_t.serviceProvider].serviceTicketPool -= valueCredited;
        _treasury._totalTicketPool -= valueCredited;


        _treasury.serviceProviderAccount[_t.serviceProvider].serviceProviderPool += valueCredited;
        _t.credits -= credits;
        _treasury._ticketData[_t.tokenID] = _t;
        TransferHelper.safeApprove(ERC20Token,serviceProviderWallet, 
            _treasury.serviceProviderAccount[_t.serviceProvider].serviceProviderPool);
        TransferHelper.safeApprove(ERC20Token, _treasury.owner, 
            _treasury.bank[_treasury.owner] +
            _treasury.serviceProviderAccount[_t.serviceProvider].serviceTicketPool);

    }

    function presentCertificateRepurchase(Treasury storage _treasury, address sender,Ticket memory _t) 
        internal 
    {
        uint256 refundValue = _t.certValue;
        address serviceProviderContract = _t.serviceProvider;
        address serviceProviderWallet = getServiceProviderWallet(_treasury, _t.serviceProvider);
        uint256 availableFunds = getServiceProviderPool(_treasury, _t.serviceProvider);
        uint256 availableAllowance = IERC20(_treasury.ERC20Token).allowance(address(this), serviceProviderWallet);
        uint256 serviceProviderWalletFunds = IERC20(_treasury.ERC20Token).balanceOf(serviceProviderWallet);

        /**** START Verification */
        string memory _msg = "ServiceProvider contract ";
        string memory id = Strings.toString(
            uint256(uint160(serviceProviderContract))
        );
        _msg = string(abi.encodePacked(_msg, id, " and wallet "));
        id = Strings.toString(uint256(uint160(serviceProviderWallet)));
        _msg = string(
            abi.encodePacked(_msg, id, " have insufficient availableFunds=")
        );
        id = Strings.toString(availableFunds);
        _msg = string(abi.encodePacked(_msg, id, "/availableAllowance="));
        id = Strings.toString(availableAllowance);
        _msg = string(abi.encodePacked(_msg, id, "  plus insufficient availableWalletBalance= "));
        id = Strings.toString(serviceProviderWalletFunds);
        _msg = string(abi.encodePacked(_msg, id, " to refund certificate valued at "));
        id = Strings.toString(_t.certValue);
        _msg = string(abi.encodePacked(_msg, id, " .1"));
        require( (availableFunds + serviceProviderWalletFunds) >= refundValue, _msg);
        /**** END Verification  */

        /**********
         *
         * if allowance for service provider covers the cost of refunding
         * we transfer from our treasury and reduce the allowance of the serviceProvider
         *
         **********/
        if (refundValue <= availableFunds) {
            _treasury.serviceProviderAccount[_t.serviceProvider].serviceProviderPool -= refundValue;
            TransferHelper.safeTransferFrom(_treasury.ERC20Token,_treasury.owner,sender,refundValue);
            // reduce allowance for Service Provider
        } else if (refundValue <= (availableFunds + serviceProviderWalletFunds) ) {
            uint256 gap = refundValue - availableFunds;
            // first empty local balance
            _treasury.serviceProviderAccount[_t.serviceProvider].serviceProviderPool -= availableFunds;
            TransferHelper.safeTransferFrom(_treasury.ERC20Token, _treasury.owner, sender, availableFunds);
            // then use remote wallet allowance
            TransferHelper.safeTransferFrom(_treasury.ERC20Token, serviceProviderWallet, sender, gap);

            // TODO emit Event
        }
        _t.credits = 0;
        setTicketData(_treasury, _t.tokenID, _t);
        TransferHelper.safeApprove(_treasury.ERC20Token, serviceProviderWallet,
            _treasury.serviceProviderAccount[_t.serviceProvider].serviceProviderPool);
    }

    function distributeTicketRevenueToPool(
        Treasury storage _treasury,
        buyNFTicketParams memory params,
        Ticket memory _ticket
    ) internal {
        updateTicketPools(_treasury, params.serviceProvider, _ticket);
        // minting fee for NFTicket MSC
        TransferHelper.safeApprove(
            _treasury.ERC20Token, _treasury.owner, _treasury.bank[_treasury.owner] +
                _treasury.serviceProviderAccount[params.serviceProvider].serviceTicketPool
        );
        // reseller fee for reseller
        address resellerWallet = getResellerWallet(_treasury, params.serviceProvider);
        TransferHelper.safeApprove(_treasury.ERC20Token, resellerWallet, 
            _treasury.serviceProviderAccount[params.serviceProvider].resellerPool);
    }

    function updateTicketPools(
        Treasury storage _treasury,
        address serviceProvider,
        Ticket memory _ticket
    ) internal {
        _treasury.remainingTicketBalance[_ticket.tokenID] += _ticket.serviceFee;
        _treasury.serviceProviderAccount[serviceProvider].serviceTicketPool += _ticket.serviceFee;
        _treasury._totalTicketPool += _ticket.serviceFee;
        _treasury.serviceProviderAccount[serviceProvider].resellerPool += _ticket.resellerFee;
    }

    function verifyDistribution(
        Treasury storage _treasury,
        buyNFTicketParams memory params,
        Ticket memory _ticket
    ) internal  {
        uint256 nfticketAllowance;
        uint256 resellerAllowance;
        uint256 serviceProviderAllowance;
        uint256 contractBalance;

        // TODO this is only for one SP and one Reseller
        // so need to identify SP:reseller pair with a unique identifier and
        // keep separate accounts for each such pair

        contractBalance = IERC20(_treasury.ERC20Token).balanceOf(address(this));
        address resellerWallet = getResellerWallet(_treasury, params.serviceProvider);
        address serviceProviderWallet = getServiceProviderWallet(_treasury, params.serviceProvider);
        

        nfticketAllowance = IERC20(_treasury.ERC20Token).allowance(address(this), _treasury.owner);
        resellerAllowance = IERC20(_treasury.ERC20Token).allowance(address(this), resellerWallet);
        serviceProviderAllowance = IERC20(_treasury.ERC20Token).allowance(address(this), serviceProviderWallet);

        string memory _msg;
        _msg = "Contract balance ";
        _msg = string(abi.encodePacked(_msg, abi.encodePacked(Strings.toString(contractBalance)),
                " is insufficient for (nfticketAllowance = "));
        _msg = string(abi.encodePacked(_msg, abi.encodePacked(Strings.toString(nfticketAllowance)),
                ") + (resellerAllowance = "));
        _msg = string(abi.encodePacked(_msg, abi.encodePacked(Strings.toString(resellerAllowance)),
                ") + serviceProviderAllowance = "));
        _msg = string(abi.encodePacked(_msg, abi.encodePacked(Strings.toString(serviceProviderAllowance)),
                ") + (serviceFee = "));
        _msg = string(abi.encodePacked(_msg, abi.encodePacked(Strings.toString(_ticket.serviceFee)),")"));
        require(nfticketAllowance + resellerAllowance + serviceProviderAllowance <= contractBalance,_msg);
    }

    function isRegisteredServiceProvider(
        Treasury storage _treasury,
        address serviceProvider
    ) public view returns (bool) {
        return (_treasury._serviceDescriptors[serviceProvider] != 0);
    }

    function registerServiceProvider(
        Treasury storage _treasury,
        address serviceProvider,
        uint32 providerDescriptor,
        address serviceProviderWallet
    ) internal returns (uint16 status) {
        uint256 _serviceProvider = uint256(uint160(serviceProvider));
        if (isRegisteredServiceProvider(_treasury, serviceProvider)) {
            // this provider is already registered
            // TODO CRUD?
            return (403);
        } else {
            // register self as counterparty both as reseller as well as serviceProvider
            // this means this is a serviceProvider without a reseller
            _treasury._providers[providerDescriptor] = serviceProvider;
            _treasury._serviceDescriptors[serviceProvider] = providerDescriptor;
            _treasury.serviceProvidersReseller.set(
                _serviceProvider,
                serviceProvider
            );
            _treasury.resellersServiceProvider.set(
                _serviceProvider,
                serviceProvider
            );
            _treasury
                .serviceProviderAccount[serviceProvider]
                .serviceProviderWallet = serviceProviderWallet;
            _treasury
                .serviceProviderAccount[serviceProvider]
                .resellerWallet = serviceProviderWallet;
        }
        return (200);
    }

    function getServiceDescriptor(
        Treasury storage _treasury,
        address serviceProvider
    ) public view returns (uint32 descriptor) {
        return _treasury._serviceDescriptors[serviceProvider];
    }

    function registerServiceReseller(
        Treasury storage _treasury,
        address _serviceProvider,
        address _reseller,
        address _resellerWallet
    ) internal returns (uint16 status) {
        uint256 serviceProvider = uint256(uint160(_serviceProvider));
        uint256 reseller = uint256(uint160(_reseller));

        _treasury.serviceProvidersReseller.set(serviceProvider, _reseller);
        _treasury.resellersServiceProvider.set(reseller, _serviceProvider);
        _treasury
            .serviceProviderAccount[_serviceProvider]
            .resellerWallet = _resellerWallet;
        _treasury.serviceProviderAccount[_serviceProvider].resellerPool = 0;

        return (200);
    }

    function storeCash(
        Treasury storage _t,
        address payable _owner,
        uint256 value
    ) public {
        _t.bank[_owner] += value;
    }
}

