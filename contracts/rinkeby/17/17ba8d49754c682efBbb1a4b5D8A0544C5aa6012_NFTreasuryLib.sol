    struct Treasury {
        // these values are initiated in NFTicket constructor{}
        address owner;
        address ERC20Token;
        uint256 TRANSACTIONFEE; // promil, i.e. 1.5%
        uint256 ratioBASE;
        uint256 _totalTicketPool;
        mapping(address => uint256) _contractBalance;
        mapping(address => uint256) bank;
        mapping(uint256 => Ticket) _ticketData;
        mapping(uint32 => address) _providers;
        mapping(address => uint32) _serviceDescriptors;
        /*
         * TODO
