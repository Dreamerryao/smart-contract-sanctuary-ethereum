contract SwapProxy is Ownable, AccessControl {
    using Address for address;
    using SafeMathUniswap for uint256;
    IV3SwapRouter public immutable uniswapV3Router;
    IUniswapV2Router02 public uniswapV2Router;
    IUniswapV2Router02 public sushiSwapRouter;
    WETHERC20 weth;
    address _weth = 0xc778417E063141139Fce010982780140Aa0cD5Ab; //TODO

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    uint24 public constant poolFee = 3000;

    function isAdmin(address addy) internal view returns (bool) {
        if (hasRole(ADMIN_ROLE, addy) || addy == owner()) {
            return true;
        } else {
            return false;
        }
    }

    constructor() {
        weth = WETHERC20(_weth);
        uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );
        uniswapV3Router = IV3SwapRouter(
            0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45
        );
        sushiSwapRouter = IUniswapV2Router02(
            0xd9e1cE17f2641f24aE83637ab66a2cca9C378B9F
        );
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        addAdmin(msg.sender);
    }

    function addAdmin(address admin) public virtual onlyOwner {
        grantRole(ADMIN_ROLE, admin);
    }

    function removeAdmin(address admin) public virtual onlyOwner {
        revokeRole(ADMIN_ROLE, admin);
    }

    function recoverERC20(
        address tokenAddress,
        uint256 tokenAmount,
        address to
    ) external virtual {
        require(isAdmin(msg.sender), "Ha ha");
        require(
            tokenAmount <= IERC20(tokenAddress).balanceOf(address(this)),
            "Not enough tokens"
        );
        IERC20(tokenAddress).transfer(to, tokenAmount);
    }

    function recoverETH(address to, uint256 value) external virtual {
        require(isAdmin(msg.sender), "Ha ha");
        require(value <= address(this).balance, "Not enough ETH");
        TransferHelper.safeTransferETH(to, value);
    }

    function swapEthForWeth(uint256 ethAmount) external virtual {
        require(isAdmin(msg.sender), "Ha ha");
        require(ethAmount <= address(this).balance, "Not enough ETH");
        weth.deposit{value: ethAmount}();
        weth.transfer(address(this), ethAmount);
    }

    function swapEthForWethInternal(uint256 ethAmount) internal virtual {
        require(isAdmin(msg.sender) || msg.sender == address(this), "Ha ha");
        require(ethAmount <= address(this).balance, "Not enough ETH");
        weth.deposit{value: ethAmount}();
        weth.transfer(address(this), ethAmount);
    }

    /*
     * Uni V2 Starts
     */
    function swapTokensForEthUniV2(IERC20 token, uint256 tokenAmount)
        external
        returns (uint256)
    {
        require(isAdmin(msg.sender), "Ha ha");
        require(
            tokenAmount <= token.balanceOf(address(this)),
            "Not enough tokens"
        );
        address[] memory path = new address[](2);
        path[0] = address(token);
        path[1] = uniswapV2Router.WETH();

        uint256 balanceBefore = address(this).balance;

        TransferHelper.safeApprove(
            address(token),
            address(uniswapV2Router),
            tokenAmount
        );

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );

        uint256 ethAmount = address(this).balance.sub(balanceBefore);
        swapEthForWethInternal(ethAmount);
        return ethAmount;
    }

    function swapEthForTokensUniV2(uint256 ethAmount, IERC20 token)
        external
        returns (uint256)
    {
        require(isAdmin(msg.sender), "Ha ha");
        require(ethAmount <= address(this).balance, "Not enough ETH");
        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = address(token);

        uint256 balanceBefore = token.balanceOf(address(this));

        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: ethAmount
        }(0, path, address(this), block.timestamp);

        uint256 tokenAmount = token.balanceOf(address(this)).sub(balanceBefore);
        return tokenAmount;
    }

    function swapTokensForTokensUniV2(
        IERC20 tokenIn,
        IERC20 tokenOut,
        uint256 tokenInAmount
    ) external returns (uint256) {
        require(isAdmin(msg.sender), "Ha ha");
        require(
            tokenInAmount <= tokenIn.balanceOf(address(this)),
            "Not enough tokens"
        );
        address[] memory path = new address[](2);
        path[0] = address(tokenIn);
        path[1] = address(tokenOut);

        uint256 balanceBefore = tokenOut.balanceOf(address(this));

        TransferHelper.safeApprove(
            address(tokenIn),
            address(uniswapV2Router),
            tokenInAmount
        );

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenInAmount,
            0, // accept any amount of tokens
            path,
            address(this),
            block.timestamp
        );

        uint256 tokenAmount = tokenOut.balanceOf(address(this)).sub(
            balanceBefore
        );
        return tokenAmount;
    }

    /*
     * Uni V2 Ends
     */

    /*
     * Uni V3 Starts
     */
    function swapUniV3(
        address tokenIn,
        address tokenOut,
        uint256 tokenInAmount
    ) external returns (uint256 amountOut) {
        require(isAdmin(msg.sender), "Ha ha");
        require(
            tokenInAmount <= IERC20(tokenIn).balanceOf(address(this)),
            "Not enough tokens"
        );

        // approve the router to spend
        TransferHelper.safeApprove(
            tokenIn,
            address(uniswapV3Router),
            tokenInAmount
        );

        // The call to `exactInputSingle` executes the swap given the route.
        amountOut = uniswapV3Router.exactInputSingle(
            IV3SwapRouter.ExactInputSingleParams({
                tokenIn: tokenIn,
                tokenOut: tokenOut,
                fee: poolFee,
                recipient: address(this),
                amountIn: tokenInAmount,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            })
        );
    }

    /*
     * Uni V3 Ends
     */

    /*
     * SushiSwap Starts
     */
    function swapTokensForEthSushiSwap(IERC20 token, uint256 tokenAmount)
