contract GrittyCats is
	Context,
	ERC721Enumerable,
	VRFConsumerBase,
	Ownable,
	ReentrancyGuard
{
	using ABDKMath64x64 for uint;
	using SafeMath for uint256;
	using Strings for uint256;
	using ECDSA for bytes32;

	string public provenanceHash;
	string private _theBaseURI;
	uint256 public startingIndex;

	uint256 public mintCost = 0.03 ether;
	uint256 public constant MAX_SUPPLY = 9999;
	uint256 public constant MAX_PER_TRANSACTION = 9;
	uint256 public constant MAX_PER_WALLET_LIMIT = 9;
	mapping(address => uint256) private _mintedPerAddress;

	bool public publicSaleIsActive;
	bool public presaleIsActive;
	bool public metadataRevealed;
	bool public metadataFinalised;

	/// @dev Royalty info
	address public immutable signerAddress;
	address public immutable royaltyAddress;
	uint256 public royaltySize = 750;
	uint256 public royaltyDenominator = 10000;
	mapping(uint256 => address) private _royaltyReceivers;

	/// @dev Randomness
	bytes32 internal immutable keyHash;
	uint256 internal fee;

	/// @dev Merkle root
	bytes32 internal rootHash;

	/// @dev Errors
	/**
	 * Insufficient balance for transfer. Needed `required` but only `available` available.
	 * @param available balance available.
	 * @param required requested amount to transfer.
	 */
	error InsufficientBalance(uint256 available, uint256 required);
	/**
	 * Maximum mints exceeded. Allowed `allowed` but trying to mint `trying`.
	 * @param trying total trying to mint.
	 * @param allowed allowed amount to mint per wallet.
	 */
	error MaxPerWalletCap(uint256 trying, uint256 allowed);
	/**
	 * Maximum supply exceeded. Allowed `allowed` but trying to mint `trying`.
	 * @param trying total trying to mint.
	 * @param allowed allowed amount to mint per wallet.
	 */
	error MaxSupplyExceeded(uint256 trying, uint256 allowed);
	/**
	 * Not allowed. Address is not allowed.
	 * @param _address wallet address checked.
	 */
	error NotAlowed(address _address);
	/**
	 * Token does not exist.
	 * @param tokenId token id checked.
	 */
	error DoesNotExist(uint256 tokenId);

	/// @dev Events
	event PermanentURI(string _value, uint256 indexed _id);

	event TokensMinted(address indexed mintedBy, uint256 indexed tokensNumber);

	event StartingIndexFinalized(uint256 indexed startingIndex);

	event BaseUriUpdated(string oldBaseUri, string newBaseUri);

	event CostUpdated(uint256 oldCost, uint256 newCost);

	/// @dev Modifiers
	/**
	 * @dev Throws if public sale is NOT active.
	 */
	modifier isPublicSaleActive() {
		if (_msgSender() != owner()) {
			require(publicSaleIsActive, "Public sale is not active yet.");
		}
		_;
	}

	/**
	 * @dev Throws if max tokens per wallet
	 */
	modifier isMaxTokensPerWallet(uint256 tokensNumber) {
		if (_msgSender() != owner()) {
			uint256 mintedBalance = _mintedPerAddress[_msgSender()];
			uint256 currentMintingAmount = mintedBalance + tokensNumber;
			if (currentMintingAmount > MAX_PER_WALLET_LIMIT) {
				revert MaxPerWalletCap(
					currentMintingAmount,
					MAX_PER_WALLET_LIMIT
				);
			}
		}
		_;
	}

	/**
	 * @dev Throws if the amount sent is not equal to the total cost.
	 */
	modifier isCorrectAmountProvided(uint256 tokensNumber) {
		uint256 totalCost = tokensNumber * mintCost;
		if (totalCost == msg.value && _msgSender() != owner()) {
			revert InsufficientBalance(msg.value, totalCost);
		}
		_;
	}

	/**
	 * @dev Throws if the total token number being minted is zero
	 */
	modifier isMintingOne(uint256 tokensNumber) {
		require(tokensNumber > 0, "You must mint at least 1 token");
		_;
	}

	/**
	 * @dev Throws if the total being minted is greater than the max supply
	 */
	modifier isLessThanMaxSupply(uint256 tokensNumber) {
		if (totalSupply() + tokensNumber > MAX_SUPPLY) {
			revert MaxSupplyExceeded(totalSupply() + tokensNumber, MAX_SUPPLY);
		}
		_;
	}

	/**
	 * @dev Throws if the sender is not on the presale list
	 */
	modifier isOnPresaleList(bytes32[] memory proof) {
		if (
			!presaleIsActive ||
			_msgSender() == owner() ||
			!_onPresaleList(_msgSender(), proof)
		) {
			revert NotAlowed(_msgSender());
		}
		_;
	}
	/**
	 * @dev Throws if the sender is not on the presale list
	 */
	modifier doesExist(uint256 tokenId) {
		if (!_exists(tokenId)) {
			revert DoesNotExist(tokenId);
		}
		_;
	}

	constructor(
		address _coordinatorAddress,
		address _linkTokenAddress,
		bytes32 _keyHash,
		uint256 _fee,
		address _royaltyAddress,
		address _signerAddress,
		string memory _baseURI
	)
		ERC721("GrittyCats", "GCAT")
		VRFConsumerBase(_coordinatorAddress, _linkTokenAddress)
	{
		royaltyAddress = _royaltyAddress;
		signerAddress = _signerAddress;
		keyHash = _keyHash;
		fee = _fee;
		_theBaseURI = _baseURI;
	}

	/**
	 * @notice The private sale start function.
	 * @dev The presale mint function.
	 * @param tokensNumber Total number of tokens to mint.
	 * @param proof Cryptographic proof checked to see if the wallet address is allowed.
	 */
	function purchase(uint256 tokensNumber, bytes32[] memory proof)
		public
		payable
		isMaxTokensPerWallet(tokensNumber)
		isCorrectAmountProvided(tokensNumber)
		isMintingOne(tokensNumber)
		isLessThanMaxSupply(tokensNumber)
		isOnPresaleList(proof)
	{
		if (_msgSender() != owner()) {
			_mintedPerAddress[_msgSender()] += tokensNumber;
		}

		_safeMint(_msgSender(), tokensNumber);
		emit TokensMinted(_msgSender(), tokensNumber);
	}

	/**
	 * @notice The public sale start function.
	 * @dev The public mint function.
	 */
	function purchase(uint256 tokensNumber)
		public
		payable
		isPublicSaleActive
		isMaxTokensPerWallet(tokensNumber)
		isCorrectAmountProvided(tokensNumber)
		isMintingOne(tokensNumber)
		isLessThanMaxSupply(tokensNumber)
	{
		_safeMint(_msgSender(), tokensNumber);
		emit TokensMinted(_msgSender(), tokensNumber);
	}

	/**
	 * @dev Check to see if the address is on the presale list.
	 */
	function _onPresaleList(address claimer, bytes32[] memory proof)
		internal
		view
		returns (bool)
	{
		bytes32 leaf = keccak256(abi.encodePacked(claimer));
		return MerkleProof.verify(proof, rootHash, leaf);
	}

	/**
	 * @dev Set the presale list
	 */
	function setPresaleList(bytes32 _rootHash) public onlyOwner {
		rootHash = _rootHash;
	}

	function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
		external
		view
		returns (address receiver, uint256 royaltyAmount)
	{
		uint256 amount = _salePrice.mul(royaltySize).div(royaltyDenominator);
		address royaltyReceiver = _royaltyReceivers[_tokenId] != address(0)
			? _royaltyReceivers[_tokenId]
			: royaltyAddress;
		return (royaltyReceiver, amount);
	}

	function addRoyaltyReceiverForTokenId(address receiver, uint256 tokenId)
		public
		onlyOwner
	{
		_royaltyReceivers[tokenId] = receiver;
	}

	function setProvenanceHash(string memory _provenanceHash) public onlyOwner {
		require(
			bytes(_provenanceHash).length == 0,
			"Provenance hash has already been set"
		);
		provenanceHash = _provenanceHash;
	}

	function updatePresaleStatus(bool _presaleIsActive) public onlyOwner {
		presaleIsActive = _presaleIsActive;
	}

	function setBaseURI(string memory newBaseURI) public onlyOwner {
		require(!metadataFinalised, "Metadata already revealed");

		string memory currentURI = _theBaseURI;
		_theBaseURI = newBaseURI;
		emit BaseUriUpdated(currentURI, newBaseURI);
	}

	function setMintCost(uint256 _newCost) public onlyOwner {
		uint256 currentCost = mintCost;
		mintCost = _newCost;
		emit CostUpdated(currentCost, _newCost);
	}

	function finalizeStartingIndex()
		public
		onlyOwner
		returns (bytes32 requestId)
	{
		require(startingIndex == 0, "startingIndex already set");
		require(
			LINK.balanceOf(address(this)) >= fee,
			"Not enough LINK - fill contract with faucet"
		);
		return requestRandomness(keyHash, fee);
	}

	/**
	 * @dev Callback function used by VRF Coordinator
	 */
	function fulfillRandomness(bytes32, uint256 randomness) internal override {
		startingIndex = (randomness % MAX_SUPPLY);
		emit StartingIndexFinalized(startingIndex);
	}

	/**
	 * @dev Just in case the fee for the VRF Coordinator changes in the future
	 */
	function setVRFFee(uint256 _fee) public onlyOwner {
		fee = _fee;
	}

	function tokenURI(uint256 tokenId)
		external
		view
		virtual
		override
		doesExist(tokenId)
		returns (string memory)
	{
		if (!metadataRevealed) return _theBaseURI;
		return string(abi.encodePacked(_theBaseURI, tokenId.toString()));
	}

	function revealMetadata() public onlyOwner {
		require(!metadataRevealed, "Metadata already revealed");
		metadataRevealed = true;
	}

	function finalizeMetadata() public onlyOwner {
		require(!metadataFinalised, "Metadata already finalised");
		metadataFinalised = true;
	}

	function withdraw() external onlyOwner {
		uint256 balance = address(this).balance;
		payable(owner()).transfer(balance);
	}
}

