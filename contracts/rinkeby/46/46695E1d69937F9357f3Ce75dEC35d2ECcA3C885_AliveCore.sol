    uint32[] calldata percentAllocations) public onlyGovernor returns(address ){
        // require(alivePercentage < 500000 , "can't be more than 50%");
        (address split1) =  ISplitMain(splits).createSplit(accounts, percentAllocations, 0, address(0));

//         address[] memory newAccount;
