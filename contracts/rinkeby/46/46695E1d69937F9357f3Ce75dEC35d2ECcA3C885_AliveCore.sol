         _fixedMintPrice  // price of the fixedMintToken 
        );

        Songs memory newSong;
        newSong.nftContract = address(nft);
        newSong.songId = allSongs.length;
        newSong.splitAddressWithAlive = royalityReceiver;
        newSong.splitNonFee = royalityReceiver;
        newSong.typeOfNFT = 456;
        allSongs.push(newSong);
        return nft;

    }


