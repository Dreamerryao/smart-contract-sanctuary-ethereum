// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

import "./NewBandContract.sol";

contract AliveCore is AccessControl {
address aliveRoyaltyReceiver;
bytes32  constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");

struct Band {
    uint bandId;
    string bandName;
    address bandAddress;
    address deployer;
}

Band[] allBands;

mapping(uint => Band) mappingOfIdToBand;

event BandCreated(address , address, uint, string );

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(GOVERNANCE_ROLE,  _msgSender());
    }

    function createBand(string memory _bandName) public {
        uint id = allBands.length+1;
        N456Factory newBand = new N456Factory(id, msg.sender, _bandName);
        Band memory bandNew;
        bandNew.bandId=id;
        bandNew.bandName=_bandName;
        bandNew.bandAddress= address(newBand);
        bandNew.deployer = msg.sender;
        allBands.push(bandNew);

        mappingOfIdToBand[id] =  bandNew;

        emit BandCreated(address(newBand) , msg.sender, id, _bandName);

    }
   

      function giveGovernanceRole(address _to) public {
     require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "only Admin can grant roles");

     _grantRole(GOVERNANCE_ROLE, _to);
    }


    function retriveGovernanceRole(address _to) public {
     require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
     require(hasRole(GOVERNANCE_ROLE, _to), "addresss does not have the role");

     _revokeRole(GOVERNANCE_ROLE, _to);
    }
   
   function returnAllBands() public view returns(Band[] memory) {
       return allBands;
   }

}

