pragma solidity ^0.8.0;

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract LeesahTest721 is ERC721A {
    /* Supply Constants */
    address contractOwnerAddress;
    address lootboxContractAddress;
    string private baseURI;
    constructor(string memory name, string memory symbol) ERC721A(name, symbol) {
      contractOwnerAddress = msg.sender;
    }

    function mint(address toAddress, uint256 amount) external onlyLootbox {
        _mint(toAddress, amount);
    }

    function uri(uint256 _tokenId) public view returns (string memory) {
      return string(
        abi.encodePacked(
          baseURI,
          Strings.toString(_tokenId),
          ".json"
        )
      );
    }

    function devMint(uint256 amount) external onlyOwner {
      _mint(msg.sender, amount);
    }

    function setBaseUri(string calldata _baseURI) external onlyOwner {
      baseURI = _baseURI;
    }

    function setLootboxContractAddress(address contractAddress) external onlyOwner {
      lootboxContractAddress = contractAddress;
    }

    modifier onlyOwner() {
      require(msg.sender == contractOwnerAddress, "Not contract owner");
      _;
    }

    modifier onlyLootbox() {
      require(lootboxContractAddress != address(0), "Lootbox contract address not set");
      require(msg.sender == lootboxContractAddress, "Cannot mint");
      _;
    }
}

