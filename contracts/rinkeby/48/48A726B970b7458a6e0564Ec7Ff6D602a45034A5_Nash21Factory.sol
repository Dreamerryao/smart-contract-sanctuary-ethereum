pragma solidity ^0.8.9;

import '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import './utils/UUPSUpgradeableByRole.sol';
import './interfaces/INash21Manager.sol';
import './interfaces/INash21Guarantee.sol';
import './interfaces/INash21Factory.sol';
import 'hardhat/console.sol';

/// @title The contract of Nash21Factory
/// @notice Handles non-fungible tokens balances and actions
/// @dev Any data regarding non-fungible tokens is placed here
contract Nash21Factory is INash21Factory, UUPSUpgradeableByRole, ERC721URIStorageUpgradeable, ERC721EnumerableUpgradeable, EIP712Upgradeable {
    
    bytes32 private immutable _N21_FACTORY_TYPEHASH = keccak256(
        'Create(bytes32 hashId,address account,uint256 value,bytes32 currency,uint256 startDate,uint256 endDate,bytes32 uri,uint256 deadline)'
    );

    bytes32 private immutable _GUARANTEE_ROLE = keccak256('GUARANTEE_ROLE');

    bytes32 private immutable _AUTH_ROLE = keccak256('AUTH_ROLE');

    string private _baseTokenURI;

    string private _contractUri;

    uint256 private _id;

    /// @inheritdoc INash21Factory
    uint256 public creationFee;

    /// @inheritdoc INash21Factory
    uint256 public transferFee;
    
    /// @inheritdoc INash21Factory
    mapping(uint256 => Data) public data;

    /// @inheritdoc INash21Factory
    mapping(uint256 => address) public authorized;

    /// @inheritdoc INash21Factory
    mapping(bytes32 => bool) public unauthorized;

    /// @inheritdoc INash21Factory
    mapping(bytes32 => uint256) public hashMap;

    /// @inheritdoc INash21Factory
    mapping(bytes32 => uint256) public tokenCreationFee;

    /// @inheritdoc INash21Factory
    mapping(uint256 => uint256) public tokenTransferFee;

    /// @inheritdoc INash21Factory
    mapping(uint256 => address) public recipients;

    /** PUBLIC FUNCTIONS */

    /// @inheritdoc INash21Factory
    function initialize() public initializer {
        __ERC721_init_unchained('N21', 'Nash21 ERC721');
        __ERC721URIStorage_init_unchained();
        __ERC721Enumerable_init_unchained();
        __EIP712_init_unchained('N21_FACTORY', '1');
        __AccessControlProxyPausable_init_unchained(msg.sender);

        // _setBaseURI('https://nash21.io/nfts/');
        // _setContractURI('https://nash21.io/contractUri/');  
        _setCreationFee(1 ether);
        _id = 1;
    }

    /// @inheritdoc INash21Factory
    function getOrigin(
        uint256 id
    ) public view returns(uint256) {
        return data[id].origin;
    }

    /// @inheritdoc INash21Factory
    function getStartDate(
        uint256 id
    ) public view returns(uint256) {
        return data[id].startDate;
    }

    /// @inheritdoc INash21Factory
    function getEndDate(
        uint256 id
    ) public view returns(uint256) {
        return data[id].endDate;
    }

    /// @inheritdoc INash21Factory
    function getCreationFee(
        bytes32 hashId
    ) public view returns(uint256) {
        uint256 _creationFee = tokenCreationFee[hashId] == 0 ? creationFee : tokenCreationFee[hashId];
        if (_creationFee > 100 ether) _creationFee = 0;
        INash21Guarantee guaranteeInterface = INash21Guarantee(INash21Manager(manager).get(keccak256('GUARANTEE')));
        return guaranteeInterface.transformCurrency(getCurrency(hashMap[hashId]), getValue(hashMap[hashId]) * _creationFee / 100 ether);
    }

    /// @inheritdoc INash21Factory
    function getTransferFee(
        uint256 id
    ) public view returns(uint256) {
        uint256 _transferFee = tokenTransferFee[id] == 0 ? transferFee : tokenTransferFee[id];
        if (_transferFee > 100 ether) _transferFee = 0;
        INash21Guarantee guaranteeInterface = INash21Guarantee(INash21Manager(manager).get(keccak256('GUARANTEE')));
        return guaranteeInterface.transformCurrency(getCurrency(id), getValue(id) * _transferFee / 100 ether);
    }

    /// @inheritdoc INash21Factory
    function getValue(
        uint256 id
    ) public view returns (uint256) {
        return data[id].value;
    }

    /// @inheritdoc INash21Factory
    function getReleased(
        uint256 id
    ) public view returns (uint256) {
        return block.timestamp > getEndDate(id) ? getValue(id)
        : block.timestamp > getStartDate(id) ? getValue(id) * (block.timestamp - getStartDate(id)) / (getEndDate(id) - getStartDate(id))
        : 0;
    }

    /// @inheritdoc INash21Factory
    function getCurrency(
        uint256 id
    ) public view returns (bytes32) {
        return data[id].currency;
    }

    /// @inheritdoc INash21Factory
    function authAndCreate(
        bytes32 hashId,
        address account,
        uint256 value,
        bytes32 currency,
        uint256 startDate,
        uint256 endDate,
        string memory uri
    ) public onlyRole(_AUTH_ROLE) returns(uint256) {
        uint256 id = _authorize(hashId, account, value, currency, startDate, endDate, uri);
        _create(id);
        return id;
    }

    /// @inheritdoc INash21Factory
    function authorize(
        bytes32 hashId,
        address account,
        uint256 value,
        bytes32 currency,
        uint256 startDate,
        uint256 endDate,
        string memory uri
    ) public onlyRole(_AUTH_ROLE) returns(uint256) {
        return _authorize(hashId, account, value, currency, startDate, endDate, uri);
    } 

    /// @inheritdoc INash21Factory
    function authorizeAndSetFees(
        bytes32 hashId,
        address account,
        uint256 value,
        bytes32 currency,
        uint256 startDate,
        uint256 endDate,
        uint256 _creationFee,
        string memory uri
    ) public onlyRole(_AUTH_ROLE) returns(uint256) {
        uint256 id = _authorize(hashId, account, value, currency, startDate, endDate, uri);
        _setTokenCreationFee(_creationFee, hashId);
        return id;
    } 

    /// @inheritdoc INash21Factory
    function unauthorizeOffchainSign(
        bytes32 hash
    ) public onlyRole(_AUTH_ROLE) {
        unauthorized[hash] = true;
        emit Unauthorize(hash);
    }

    /// @inheritdoc INash21Factory
    function unauthorize(
        bytes32 hashId
    ) public onlyRole(_AUTH_ROLE) {
        uint256 id = hashMap[hashId];
        address account = authorized[id];
        require(account != address(0), 'Nash21Factory: token is unauthorized');
        authorized[id] = address(0);
        emit Unauthorize(hashId);
    }

    /// @inheritdoc INash21Factory
    function create(
        bytes32 hashId
    ) public {
        address account = msg.sender;
        IERC20 tokenInterface = IERC20(INash21Manager(manager).get(keccak256('USDT')));
        address vault = INash21Manager(manager).get(keccak256('VAULT'));
        require(vault != address(0), 'Nash21Factory: vault is not defined');
        uint256 feeRequired = getCreationFee(hashId);
        tokenInterface.transferFrom(account, vault, feeRequired);
        _create(hashMap[hashId]);
    }

    /// @inheritdoc INash21Factory
    function createWithSignature(
        bytes32 hashId,
        address account,
        uint256 value,
        bytes32 currency,
        uint256 startDate,
        uint256 endDate,
        string memory uri,
        uint256 deadline, 
        bytes memory signature, 
        address signer
    ) public {
        require(hasRole(_AUTH_ROLE, signer), 'Nash21Factory: invalid role');
        require(block.timestamp <= deadline, 'Nash21Factory: expired deadline');

        uint256 id = _authorize(hashId, account, value, currency, startDate, endDate, uri);
        bytes32 hashedUri = keccak256(abi.encodePacked(uri));

        bytes32 structHash = keccak256(abi.encode(_N21_FACTORY_TYPEHASH, hashId, account, value, currency, startDate, endDate, hashedUri, deadline));
        bytes32 hash = _hashTypedDataV4(structHash);

        bool validation = SignatureCheckerUpgradeable.isValidSignatureNow(signer, hash, signature);
        require(validation, 'Nash21Factory: invalid signature');

        require(!unauthorized[hash], 'Nash21Factory: unauthorized');
        _create(id);
    }

    /// @inheritdoc INash21Factory
    function setCreationFee(
        uint256 fee
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setCreationFee(fee);
    }

    /// @inheritdoc INash21Factory
    function setTokenCreationFee(
        uint256 fee,
        bytes32 hashId
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTokenCreationFee(fee, hashId);
    }

    /// @inheritdoc INash21Factory
    function setTransferFee(
        uint256 fee
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTransferFee(fee);
    }

    /// @inheritdoc INash21Factory
    function setTokenTransferFee(
        uint256 fee,
        uint256 id
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTokenTransferFee(fee, id);
    }

    /// @inheritdoc INash21Factory
    function contractURI(
    ) public view returns (string memory) {
        return _contractUri;
    }

    /// @inheritdoc INash21Factory
    function baseURI(
    ) public view returns (string memory) {
        return _baseURI();
    }

    /// @inheritdoc INash21Factory
    function setBaseURI(
        string memory uri
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setBaseURI(uri);
    }

    /// @inheritdoc INash21Factory
    function setTokenURI(
        uint256 id,
        string memory _tokenURI
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTokenURI(id, _tokenURI);
    }

    /// @inheritdoc INash21Factory
    function setContractURI(
        string memory contractURI_
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setContractURI(contractURI_);
    }

    /// @inheritdoc INash21Factory
    function split(
        address account,
        uint256 id,
        uint256 timestamp
    ) public whenNotPaused onlyRole(_GUARANTEE_ROLE) returns (uint256, uint256) {
        require(ownerOf(id) == account, 'Nash21Factory: only owner can split this token');
        _checkTimestamp(id, timestamp);

        uint256 id1 = _id;
        uint256 id2 = _id + 1;

        _id += 2;

        uint256 value1 = getValue(id) * (timestamp - getStartDate(id)) / (getEndDate(id) - getStartDate(id));
        uint256 value2 = getValue(id) - value1;

        data[id1] = Data(id, value1, getCurrency(id), getStartDate(id), timestamp, tokenURI(id));
        data[id2] = Data(id, value2, getCurrency(id), timestamp, getEndDate(id), tokenURI(id));

        _burn(id);
        _mint(account, id1);
        _mint(account, id2);

        _setTokenURI(id1, data[id1].uri);
        _setTokenURI(id2, data[id2].uri);

        emit Burn(account, id);
        emit Mint(account, id1);
        emit Mint(account, id2);

        return (id1, id2);
    }

    /// @inheritdoc INash21Factory
    function tokenURI(
        uint256 id
    ) public view override (INash21Factory, ERC721Upgradeable, ERC721URIStorageUpgradeable) returns (string memory) {
        return super.tokenURI(id);
    }

    /// @inheritdoc INash21Factory
    function setRecipient(
        uint256 id,
        address recipient
    ) public {
        require(msg.sender == ownerOf(id), "Nash21Factory: only owner can set recipient");
        recipients[id] = recipient;
    }

    /** PRIVATE/INTERNAL/OVERRIDE FUNCTIONS */

    function _authorize(
        bytes32 hashId,
        address account,
        uint256 value,
        bytes32 currency,
        uint256 startDate,
        uint256 endDate,
        string memory uri
    ) internal returns(uint256) {
        require(hashMap[hashId] == 0, 'Nash21Factory: token already authorized');
        require(account != address(0), 'Nash21Factory: cant authorize address zero');
        require(startDate < endDate, 'Nash21Factory: ending timestamp must be after starting timestamp');
        require(value > 0, 'Nash21Factory: value cant be null');
        uint256 id = _id;
        _id++;
        data[id] = Data(id, value, currency, startDate, endDate, uri);
        authorized[id] = account;
        hashMap[hashId] = id;
        emit Authorize(hashId, account, id, value, currency, startDate, endDate, uri);
        return id;
    }

    function _create(
        uint256 id
    ) internal whenNotPaused {
        address account = authorized[id];
        require(account != address(0), 'Nash21Factory: the creation of this token is not authorized');
        authorized[id] = address(0);
        recipients[id] = account;
        _mint(account, id);
        _setTokenURI(id, data[id].uri);
        emit Mint(account, id);
    }

    function _checkTimestamp(
        uint256 id,
        uint256 timestamp
    ) internal view {
        require(timestamp > data[id].startDate, 'Nash21Factory: timestamp is before the start timestamp');
        require(timestamp < data[id].endDate, 'Nash21Factory: timestamp is after the end timestamp');
    }

    function _baseURI(   
    ) internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function _setBaseURI(
        string memory newBaseTokenURI
    ) internal {
        _baseTokenURI = newBaseTokenURI;
    }

    function _setTokenURI(
        uint256 id,
        string memory _tokenURI
    ) internal override {
        super._setTokenURI(id, _tokenURI);
        emit SetTokenURI(id, _tokenURI);
    }

    function _setContractURI(
        string memory contractURI_
    ) internal {
        _contractUri = contractURI_;
        emit SetContractURI(contractURI_);
    }

    function _setCreationFee(
        uint256 fee
    ) internal {
        creationFee = fee;
    }

    function _setTokenCreationFee(
        uint256 fee,
        bytes32 hashId
    ) internal {
        tokenCreationFee[hashId] = fee;
        emit SetTokenCreationFee(hashId, fee);
    }

    function _setTransferFee(
        uint256 fee
    ) internal {
        transferFee = fee;
    }

    function _setTokenTransferFee(
        uint256 fee,
        uint256 id
    ) internal {
        tokenTransferFee[id] = fee;
        emit SetTokenTransferFee(id, fee);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC721Upgradeable, ERC721EnumerableUpgradeable, IERC165Upgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function _burn(
        uint256 id
    ) internal override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        super._burn(id);
    }

    function _beforeTokenTransfer(
        address startDate,
        address to,
        uint256 id
    ) internal whenNotPaused override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        INash21Guarantee guaranteeInterface = INash21Guarantee(INash21Manager(manager).get(keccak256('GUARANTEE')));
        require((guaranteeInterface.distributed(id) == 0 || to == address(0)), 'Nash21Factory: split to transfer');

        if ((startDate != address(0)) && (to != address(0))) {
            uint256 feeRequired = getTransferFee(id);
            if (feeRequired > 0) {
                IERC20 tokenInterface = IERC20(INash21Manager(manager).get(keccak256('USDT')));
                address vault = INash21Manager(manager).get(keccak256('VAULT'));
                require(vault != address(0), 'Nash21Factory: vault is not defined');
                tokenInterface.transferFrom(startDate, vault, feeRequired);
            }
        }
        
        super._beforeTokenTransfer(startDate, to, id);
    }

    // Leaving some storage for upgrades

    uint256[40] private __gap;
}

