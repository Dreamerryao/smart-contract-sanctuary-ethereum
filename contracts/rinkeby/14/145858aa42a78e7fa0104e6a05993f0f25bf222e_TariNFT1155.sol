interface IMINT{
    
    function mint(string memory uri, uint256 supply, uint96 fee)  external returns(uint256, bool);

}
contract TariNFT1155 is ERC1155 {

    uint256 newTokenId = 1;
    address public owner;
    mapping (string => bool) private usedURIs;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    constructor (string memory name, string memory symbol) ERC1155 (name, symbol) {
        owner = msg.sender;
    }

    modifier onlyOwner() {
        require(owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    /** @dev change the Ownership from current owner to newOwner address
        @param newOwner : newOwner address */    

    function transferOwnership(address newOwner) external onlyOwner returns(bool){
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        owner = newOwner;
        emit OwnershipTransferred(owner, newOwner);
        return true;
    }

    /** @dev verify the tokenURI that should be verified by owner of the contract.
        *requirements: signer must be owner of the contract
        @param uri string memory URI of token to be minted.
        @param supply uint256 number of copies to be mint.
        @param fee uint96 percentage  of royalty to be receive.

    */

    function mint(string memory uri, uint256 supply, uint96 fee) external returns(uint256, bool){
        require(!isDuplicate(uri), "Minting: Duplicate Minting");
        usedURIs[uri] = true;
        uint256 tokenId = newTokenId;
        _mint(msg.sender, newTokenId, supply, abi.encodePacked(msg.sender, newTokenId, supply));
        _setTokenURI(newTokenId, uri);
        _setTokenRoyalty(newTokenId, msg.sender, fee);
        newTokenId = newTokenId + 1;
        return (tokenId, true);
    }

    function setBaseURI(string memory _baseURI) external onlyOwner{
         _setTokenURIPrefix(_baseURI);
    }

    function isDuplicate(string memory uri) internal view returns(bool){
        return usedURIs[uri];
    }

    function burn(uint256 tokenId, uint256 supply) external {
        _burn(msg.sender, tokenId, supply);
    }

    function burnBatch(uint256[] memory tokenIds, uint256[] memory amounts) external {
        _burnBatch(msg.sender, tokenIds, amounts);
    }
}