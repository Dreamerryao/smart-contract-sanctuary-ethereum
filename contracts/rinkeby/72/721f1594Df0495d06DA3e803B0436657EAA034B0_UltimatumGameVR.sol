error UltimatumGame__InvalidProposerResponderAddress();
error UltimatumGame__NotProposer();
error UltimatumGame__NotResponder();

error UltimatumGame__ProposerNotSet();
error UltimatumGame__ResponderNotSet();
error UltimatumGame__CannotProceedAsBalanceIsZero();

error UltimatumGame__InvalidProposal();
error UltimatumGame__ProposerNeedToProposeBeforeRespond();

error UltimatumGame__TransferToProposerFailed();
error UltimatumGame__TransferToResponderFailed();
error UltimatumGame__OwnerWithdrawFailed();

/// @title Ultimatum Game
/// @notice A famous economic experiments game, see details at https://en.wikipedia.org/wiki/Ultimatum_game
/// @dev This is for learning and practise purpose
/// @custom:state-transfer-diagram
///                   (DEPOSIT)  (DEPOSIT)
///                   {must before propose}
///           DEPLOY    ->   ENROLL    ->      PROPOSE    ->       RESPOND  ->       (WITHDRAW)   ->   (NEXT) ENROLL
///                       {must be owner}  {must be proposer} {must be responder}  {must be owner}    {must be owner}
///                       {address can't 0}
///                                        {check deposited}
///                                                           {must proposed}
/// s_proposal:   0            -1                [0, 1]            [0, 1]             [0, 1]              -1
/// @custom:todo The random one can inherit this one
/// @custom:todo Min deposit and split should larger than gas cost
/// @custom:todo Not fair for responder to pay for more gas fee
/// @custom:todo Compensate gas fee for proposer and responder
/// @custom:todo Support mutli users simultaneously
contract UltimatumGame {
    enum RespondChoice {
        Accept,
        Reject
    }
    using SplitUint256 for uint256;

    /* solhint-disable var-name-mixedcase */
    address private immutable i_owner;
    address payable internal s_proposer;
    address payable internal s_responder;
    int8 private s_proposal; // Between 1 and 100, -1 means unset

    /* solhint-enable var-name-mixedcase */

    constructor() {
        i_owner = msg.sender;
    }

    modifier mustBeOwner() {
        if (msg.sender != i_owner) {
            revert UltimatumGame__NotOwner();
        }
        _;
    }

    receive() external payable {
        // TODO cannot if proposed
    }

    fallback() external payable {
        // TODO cannot if proposed
    }

    function enroll(address payable _proposer, address payable _responder)
        public
        virtual
        mustBeOwner
    {
        if (_proposer == address(0) || _responder == address(0)) {
            revert UltimatumGame__InvalidProposerResponderAddress();
        }
        s_proposer = _proposer;
        s_responder = _responder;
        s_proposal = -1;
    }

    function propose(uint8 _myshare) public virtual {
        if (address(this).balance == 0) {
            revert UltimatumGame__CannotProceedAsBalanceIsZero();
        }

        if (msg.sender != s_proposer) {
            revert UltimatumGame__NotProposer();
        }
        if (!(_myshare >= 0 && _myshare <= 100)) {
            revert UltimatumGame__InvalidProposal();
        }

        s_proposal = int8(_myshare);
    }

    function respond(RespondChoice choice) external {
        if (msg.sender != s_responder) {
            revert UltimatumGame__NotResponder();
        }
        if (s_proposal < 0) {
            revert UltimatumGame__ProposerNeedToProposeBeforeRespond();
        }

        if (choice == RespondChoice.Accept) {
            (uint256 moneyToProposer, uint256 moneyToResponder) = address(this)
                .balance
                .splitByPercent(uint8(s_proposal));

            // solhint-disable-next-line avoid-low-level-calls
            (bool callProposerSuccess, ) = s_proposer.call{
                value: moneyToProposer
            }("");
            if (!callProposerSuccess) {
                revert UltimatumGame__TransferToProposerFailed();
            }

            // solhint-disable-next-line avoid-low-level-calls
            (bool callResponderSuccess, ) = s_responder.call{
                value: moneyToResponder
            }("");
            if (!callResponderSuccess) {
                revert UltimatumGame__TransferToResponderFailed();
            }
        }

        resetGame();
    }

    function withdraw() public payable mustBeOwner {
        // solhint-disable-next-line avoid-low-level-calls
        (bool success, ) = i_owner.call{value: address(this).balance}("");
        if (!success) {
            revert UltimatumGame__OwnerWithdrawFailed();
        }
    }

    function resetGame() private {
        s_proposer = payable(address(0));
        s_responder = payable(address(0));
    }

    function getOwner() public view returns (address) {
        return i_owner;
    }

    function getProposer() public view returns (address) {
        return s_proposer;
    }

    function getResponder() public view returns (address) {
        return s_responder;
    }

    function getProposal() public view returns (int8) {
        return int8(s_proposal);
    }
}

