pragma solidity ^0.8.0;
pragma abicoder v2;

import { IERC20Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import { SafeERC20Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./interfaces/INftPriceOracle.sol";
import "./interfaces/INftPriceOracle2.sol";
import "./interfaces/IFloorPricePrediction.sol";

import "hardhat/console.sol";

/**
 * @title FloorPricePrediction
 */
contract FloorPricePrediction is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable, IFloorPricePrediction {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    INftPriceOracle public oracle;
    INftPriceOracle2 public oracle2;

    address public adminAddress; // address of the admin
    uint256 public bufferSeconds; // number of seconds for valid execution of a prediction round
    uint256 public minBetAmount; // minimum betting amount (denominated in wei)
    uint256 public treasuryFee; // treasury rate (e.g. 200 = 2%, 150 = 1.50%)
    uint256 public treasuryAmount; // treasury amount that was not claimed
    uint256 public lastTreasuryClaimedAt; // timestamp

    mapping(PositionPeriod => Period) public paramsByPeriod;
    mapping(address => Market) public markets;
    mapping(address => BetInfo[]) public userBetInfos;
    NFTCollection[] public nfts;

    struct Period {
        uint256 currentEpoch;
        uint256 intervalSeconds;
        uint256 genesisTimestamp;
        uint256 lockIntervalSeconds;
        mapping(uint256 => mapping(address => Bet[])) ledger;
        mapping(uint256 => Round) rounds;
    }
    struct Market {
        string slug;
        address nftContract;
        bool paused;
        mapping(uint256 => Period) periods; // intervalSeconds => Period
        Interval[] intervals;
    }
    struct NFTCollection {
        string slug;
        address nftContract;
    }
    struct Interval {
        uint256 intervalSeconds;
        uint256 lockIntervalSeconds;
    }
    struct PredictionParams {
        address _oracleAddress;
        address _oracle2Address;
        address _adminAddress;
        uint256 _minBetAmount;
        uint256 _treasuryFee;
        uint256 _genesisTimestamp;
        NFTCollection[] _nfts;
        Interval[] _intervals;
    }
    event BetBear(
        address indexed sender,
        uint256 indexed amount,
        address indexed market,
        uint256 intervalSeconds,
        uint256 epoch
    );
    event BetBull(
        address indexed sender,
        uint256 indexed amount,
        address indexed market,
        uint256 intervalSeconds,
        uint256 epoch
    );
    event Claim(
        address indexed sender,
        uint256 indexed amount,
        address indexed market,
        uint256 period,
        uint256 epoch,
        uint256 betNumber
    );
    event NewAdminAddress(address admin);
    event NewMinBetAmount(uint256 minBetAmount);
    event NewTreasuryFee(uint256 treasuryFee);
    event NewOracle(address oracle);
    event RewardsCalculated(
        uint256 indexed epoch,
        uint256 rewardBaseCalAmount,
        uint256 rewardAmount,
        uint256 treasuryAmount
    );
    event TokenRecovery(address indexed token, uint256 amount);
    event TreasuryClaim(uint256 amount);

    // NA: Not Admin
    modifier onlyAdmin() {
        require(msg.sender == adminAddress, "NA");
        _;
    }

    // CNA: contract not allowed
    // PCNA: Proxy contract not allowed
    modifier notContract() {
        require(!_isContract(msg.sender), "CNA");
        require(msg.sender == tx.origin, "PCNA");
        _;
    }

    modifier whenNotPaused(address market) {
        require(!markets[market].paused, "Pausable: paused");
        _;
    }

    // @custom:oz-upgrades-unsafe-allow constructor
    // constructor() initializer {}
    function initialize(PredictionParams memory params) public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
        oracle = INftPriceOracle(params._oracleAddress);
        oracle2 = INftPriceOracle2(params._oracle2Address);
        adminAddress = params._adminAddress;
        minBetAmount = params._minBetAmount;
        treasuryFee = params._treasuryFee;
        for (uint i = 0; i < params._nfts.length; i++) {
            nfts.push(params._nfts[i]);
            Market storage m = markets[params._nfts[i].nftContract];
            m.slug = params._nfts[i].slug;
            m.nftContract = params._nfts[i].nftContract;
            for (uint j = 0; j < params._intervals.length; j++) {
                m.intervals.push(params._intervals[j]);
                Period storage p = m.periods[params._intervals[j].intervalSeconds];
                p.intervalSeconds = params._intervals[j].intervalSeconds;
                p.lockIntervalSeconds = params._intervals[j].lockIntervalSeconds;
                p.genesisTimestamp = params._genesisTimestamp;
            }
        }
    }

    /**
     * @notice Bet bear position
     */

    // BMG: Bet amount must be greater than minBetAmount
    /*
      Victor
      epoch = (now - genesisTime) / time_per_round 可以從前端算好之後帶來，或是前端帶 timestamp 跟 periodIdx，在 contract 裡面計算
    */
    function betBear(address market, uint256 intervalSeconds)
        external
        payable
        whenNotPaused(market)
        nonReentrant
        notContract
    {
        require(msg.value >= minBetAmount, "BMG");

        // Update round data
        uint256 amount = msg.value;
        uint256 epoch = getCurrentEpoch(market, intervalSeconds);
        Period storage period = markets[market].periods[intervalSeconds];
        Round storage round = period.rounds[epoch];

        if (round.startTimestamp == 0) {
            _initRound(period, round, epoch);
        }

        round.totalAmount = round.totalAmount + amount;
        round.bearAmount = round.bearAmount + amount;

        // Update user data
        Bet memory bet;
        bet.position = Position.Bear;
        bet.amount = amount;
        period.ledger[epoch][msg.sender].push(bet);
        uint256 betsLength = period.ledger[epoch][msg.sender].length;
        BetInfo memory info;
        info.market = market;
        info.intervalSeconds = intervalSeconds;
        info.epoch = epoch;
        info.nth = betsLength - 1;
        userBetInfos[msg.sender].push(info);

        emit BetBear(msg.sender, amount, market, intervalSeconds, epoch);
    }

    /**
     * @notice Bet bull position
     */
    // BMG: Bet amount must be greater than minBetAmount
    function betBull(address market, uint256 intervalSeconds)
        external
        payable
        whenNotPaused(market)
        nonReentrant
        notContract
    {
        require(msg.value >= minBetAmount, "BMG");

        // Update round data
        uint256 amount = msg.value;
        uint256 epoch = getCurrentEpoch(market, intervalSeconds);
        Period storage period = markets[market].periods[intervalSeconds];
        Round storage round = period.rounds[epoch];

        if (round.startTimestamp == 0) {
            _initRound(period, round, epoch);
        }

        round.totalAmount = round.totalAmount + amount;
        round.bullAmount = round.bullAmount + amount;

        // Update user data
        Bet memory bet;
        bet.position = Position.Bull;
        bet.amount = amount;
        period.ledger[epoch][msg.sender].push(bet);
        uint256 betsLength = period.ledger[epoch][msg.sender].length;
        BetInfo memory info;
        info.market = market;
        info.intervalSeconds = intervalSeconds;
        info.epoch = epoch;
        info.nth = betsLength - 1;
        userBetInfos[msg.sender].push(info);

        emit BetBull(msg.sender, amount, market, intervalSeconds, epoch);
    }

    function _initRound(
        Period storage period,
        Round storage round,
        uint256 epoch
    ) internal {
        uint256 genesisTimestamp = period.genesisTimestamp;
        uint256 intervalSeconds = period.intervalSeconds;
        uint256 lockIntervalSeconds = period.lockIntervalSeconds;
        round.startTimestamp = genesisTimestamp + ((epoch - 1) * lockIntervalSeconds);
        round.lockTimestamp = genesisTimestamp + (epoch * lockIntervalSeconds);
        round.closeTimestamp = genesisTimestamp + (epoch * lockIntervalSeconds) + intervalSeconds;
        round.epoch = epoch;
        round.totalAmount = 0;
    }

    /**
     * @notice Claim reward for an array of epochs
     */
    // NC: Not claimable
    function claim(BetInfo[] memory betInfos) external nonReentrant notContract {
        uint256 reward; // Initializes reward
        for (uint256 i = 0; i < betInfos.length; i++) {
            BetInfo memory info = betInfos[i];
            Market storage m = markets[info.market];
            Round storage r = m.periods[info.intervalSeconds].rounds[info.epoch];
            require(r.closeTimestamp != 0 && r.lockTimestamp != 0, "RNI"); //RNI: round not init
            require(block.timestamp > r.closeTimestamp, "RNC"); // RNC:round not closed
            if (r.closePrice == 0) {
                r.closePrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.closeTimestamp));
                r.lockPrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.lockTimestamp));
            }
            Bet storage bet = m.periods[info.intervalSeconds].ledger[info.epoch][msg.sender][info.nth];
            require(claimable(r, bet), "NC");
            uint addedReward = _calculateRewards(r, bet);
            reward += addedReward;
            bet.claimed = true;
            emit Claim(msg.sender, addedReward, m.nftContract, info.intervalSeconds, r.epoch, info.nth);
        }
        if (reward > 0) {
            _safeTransfer(address(msg.sender), reward);
        }
    }

    /**
     * @notice Claim all rewards in treasury
     * @dev Callable by admin
     */
    function claimTreasury() external nonReentrant onlyAdmin {
        uint256 currentTreasuryAmount = 0;
        uint256 currentTimestamp = block.timestamp;
        for (uint256 i = 0; i < nfts.length; i++) {
            Market storage m = markets[nfts[i].nftContract];
            uint256 intervalsLengh = m.intervals.length;

            for (uint256 j = 0; j < intervalsLengh; j++) {
                Interval memory interval = m.intervals[j];
                Period storage p = m.periods[interval.intervalSeconds];
                uint256 numOfRounds = (currentTimestamp - p.genesisTimestamp) / p.lockIntervalSeconds + 1;

                uint _lastTreasuryClaimedAt = lastTreasuryClaimedAt;
                for (uint256 k = numOfRounds; k > 0; k--) {
                    Round memory r = p.rounds[k];
                    if (r.closeTimestamp == 0) continue;
                    if (r.closeTimestamp < _lastTreasuryClaimedAt) {
                        break;
                    }
                    if (r.closeTimestamp != 0 && r.closeTimestamp < currentTimestamp && r.totalAmount > 0) {
                        if (r.closePrice == 0) {
                            r.closePrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.closeTimestamp));
                            r.lockPrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.lockTimestamp));
                        }
                        if (r.closePrice == r.lockPrice) {
                            currentTreasuryAmount += r.totalAmount;
                        } else {
                            currentTreasuryAmount += (r.totalAmount * treasuryFee) / 10000;
                        }
                    }
                }
            }
        }
        lastTreasuryClaimedAt = currentTimestamp;
        _safeTransfer(adminAddress, currentTreasuryAmount);
        emit TreasuryClaim(currentTreasuryAmount);
    }

    /**
     * @notice Set minBetAmount
     * @dev Callable by admin
     */
    // BAMS0: Bet Mount must be superior to 0
    function setMinBetAmount(uint256 _minBetAmount) external onlyAdmin {
        require(_minBetAmount != 0, "BAMS0");
        minBetAmount = _minBetAmount;

        emit NewMinBetAmount(minBetAmount);
    }

    /**
     * @notice Set treasury fee
     * @dev Callable by admin
     */
    function setTreasuryFee(uint256 _treasuryFee) external onlyAdmin {
        treasuryFee = _treasuryFee;

        emit NewTreasuryFee(treasuryFee);
    }

    /**
     * @notice It allows the owner to recover tokens sent to the contract by mistake
     * @param _token: token address
     * @param _amount: token amount
     * @dev Callable by owner
     */
    function recoverToken(address _token, uint256 _amount) external onlyOwner {
        IERC20Upgradeable(_token).safeTransfer(address(msg.sender), _amount);

        emit TokenRecovery(_token, _amount);
    }

    /**
     * @notice Set admin address
     * @dev Callable by owner
     */
    // CBZA: Cannot be zero address
    function setAdmin(address _adminAddress) external onlyOwner {
        require(_adminAddress != address(0), "CBZA");
        adminAddress = _adminAddress;

        emit NewAdminAddress(_adminAddress);
    }

    /**
     * @notice Returns user bets history length
     * @param user: user address
     */
    function getUserRoundsLength(address user) public view returns (uint256) {
        return userBetInfos[user].length;
    }

    /**
     * @notice Returns round epochs and bet information for a user that has participated
     * @param user: user address
     * @param cursor: cursor
     * @param size: size
     */
    function getUserRounds(
        address user,
        uint256 cursor,
        uint256 size
    )
        external
        view
        returns (
            BetInfo[] memory betInfos,
            Round[] memory rounds,
            Bet[] memory bets,
            int256 nextCursor
        )
    {
        uint256 length = cursor + 1 - size >= 0 ? size : cursor + 1;
        betInfos = new BetInfo[](length);
        rounds = new Round[](length);
        bets = new Bet[](length);
        for (uint i = 0; i < length; i++) {
            BetInfo memory info = userBetInfos[user][cursor - i];
            Round memory r = markets[info.market].periods[info.intervalSeconds].rounds[info.epoch];
            Bet memory b = markets[info.market].periods[info.intervalSeconds].ledger[info.epoch][user][info.nth];
            if (r.closePrice == 0 && block.timestamp > r.closeTimestamp) {
                r.closePrice = int(oracle2.getNftPriceByTimestamp(markets[info.market].nftContract, r.closeTimestamp));
                r.lockPrice = int(oracle2.getNftPriceByTimestamp(markets[info.market].nftContract, r.lockTimestamp));
            }
            betInfos[i] = info;
            rounds[i] = r;
            bets[i] = b;
        }
        nextCursor = int256(cursor) - int256(size);
    }

    function getUserBets(
        address user,
        address market,
        uint256 intervalSeconds,
        uint256 betTimestamp
    ) external view returns (Bet[] memory bets) {
        uint256 epoch = getEpochByTimeStamp(market, intervalSeconds, betTimestamp);
        bets = markets[market].periods[intervalSeconds].ledger[epoch][user];
    }

    function getEpochByTimeStamp(
        address market,
        uint256 intervalSeconds,
        uint256 timestamp
    ) public view returns (uint256 epoch) {
        epoch =
            (timestamp - markets[market].periods[intervalSeconds].genesisTimestamp) /
            markets[market].periods[intervalSeconds].lockIntervalSeconds +
            1;
    }

    /**
     * @notice Get the claimable stats of specific epoch and user account
     * @param round: round
     * @param bet: bet
     */
    function claimable(Round memory round, Bet memory bet) public pure returns (bool) {
        if (round.lockPrice == round.closePrice || bet.claimed == true) {
            return false;
        }
        return
            bet.amount != 0 &&
            ((round.closePrice > round.lockPrice && bet.position == Position.Bull) ||
                (round.closePrice < round.lockPrice && bet.position == Position.Bear));
    }

    function _calculateRewards(Round memory round, Bet memory bet) internal view returns (uint256) {
        // Bull
        if (round.closePrice > round.lockPrice) {
            return (bet.amount * round.totalAmount * (10000 - treasuryFee)) / 10000 / round.bullAmount;
        }
        return (bet.amount * round.totalAmount * (10000 - treasuryFee)) / 10000 / round.bearAmount;
    }

    /**
     * @notice Get current epoch by market and period
     * Current timestamp must be within the epoch
     */
    function getCurrentEpoch(address market, uint256 intervalSeconds) public view returns (uint256 epoch) {
        epoch =
            (block.timestamp - markets[market].periods[intervalSeconds].genesisTimestamp) /
            markets[market].periods[intervalSeconds].lockIntervalSeconds +
            1;
    }

    /**
     * @notice Returns true if `account` is a contract.
     * @param account: account address
     */
    function _isContract(address account) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(account)
        }
        return size > 0;
    }

    // FIXME: 改成從epoch往前拿{length}個rounds
    /**
     * @notice Returns true if `account` is a contract.
     * @param epoch: To get rounds from epoch, set to 0 will get current epoch
     *
     */
    function getRounds(
        address market,
        uint256 intervalSeconds,
        uint256 epoch,
        uint256 length
    ) external view returns (Round[] memory) {
        Round[] memory rounds = new Round[](length);
        uint256 _epoch = epoch == 0 ? getCurrentEpoch(market, intervalSeconds) : epoch;
        for (uint i = 0; i < length; i++) {
            rounds[i] = markets[market].periods[intervalSeconds].rounds[_epoch - i];
        }
        return rounds;
    }

    function getRoundsAndUserBets(
        address user,
        address market,
        uint256 intervalSeconds,
        uint256 epoch,
        uint256 length
    ) external view returns (Round[] memory, Bet[][] memory) {
        Round[] memory _rounds = new Round[](length);
        Bet[][] memory _userRoundBets = new Bet[][](length);
        uint256 _epoch = epoch == 0 ? getCurrentEpoch(market, intervalSeconds) : epoch;
        for (uint i = 0; i < length; i++) {
            _rounds[i] = markets[market].periods[intervalSeconds].rounds[_epoch - i];
            Bet[] memory _userBets = markets[market].periods[intervalSeconds].ledger[_epoch - i][user];
            _userRoundBets[i] = new Bet[](_userBets.length);
            for (uint j = 0; j < _userBets.length; j++) {
                _userRoundBets[i][j] = _userBets[j];
            }
        }
        return (_rounds, _userRoundBets);
    }

    function getCurrentEpochStartTimestamp() external view override returns (uint256 startTimestamp) {
        uint256 currentEpoch = paramsByPeriod[PositionPeriod(0)].currentEpoch;
        startTimestamp = paramsByPeriod[PositionPeriod(0)].rounds[currentEpoch].startTimestamp;
    }

    // STF: TransferHelper: safe transfer failed
    function _safeTransfer(address to, uint256 value) internal {
        (bool success, ) = to.call{ value: value }("");
        require(success, "STF");
    }

    function pauseMarket(address market) external {
        markets[market].paused = true;
    }

    function unpausedMarket(address market) external {
        markets[market].paused = false;
    }

    function addMarket(
        NFTCollection calldata _nft,
        Interval[] calldata _intervals,
        uint256 _genesisTimestamp
    ) external {
        nfts.push(_nft);
        Market storage m = markets[_nft.nftContract];
        m.slug = _nft.slug;
        m.nftContract = _nft.nftContract;
        for (uint i = 0; i < _intervals.length; i++) {
            m.intervals.push(_intervals[i]);
            Period storage p = m.periods[_intervals[i].intervalSeconds];
            p.intervalSeconds = _intervals[i].intervalSeconds;
            p.lockIntervalSeconds = _intervals[i].lockIntervalSeconds;
            p.genesisTimestamp = _genesisTimestamp;
        }
    }

    function calculateUserRewards(address user) external view returns (uint256) {
        BetInfo[] memory infos = userBetInfos[user];
        uint256 reward = 0;
        for (uint i = infos.length - 1; i >= 0; i--) {
            BetInfo memory info = infos[i];
            Market storage m = markets[info.market];
            Round memory r = markets[info.market].periods[info.intervalSeconds].rounds[info.epoch];
            Bet storage b = markets[info.market].periods[info.intervalSeconds].ledger[info.epoch][user][info.nth];
            if (b.claimed) {
                break;
            }
            if (!b.claimed && block.timestamp > r.closeTimestamp) {
                if (r.closePrice == 0) {
                    r.closePrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.closeTimestamp));
                    r.lockPrice = int(oracle2.getNftPriceByTimestamp(m.nftContract, r.lockTimestamp));
                }
                if (claimable(r, b)) {
                    reward += _calculateRewards(r, b);
                }
            }
        }
        return reward;
    }
}

