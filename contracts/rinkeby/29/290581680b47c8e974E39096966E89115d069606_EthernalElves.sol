contract EthernalElves is ERC721, ElfDataStore {

    /*///////////////////////////////////////////////////////////////
    ///     Alas, We are the Ethernal. The Ethernal Elves         ///
    //////////////////////////////////////////////////////////////*/

    function name() external pure returns (string memory) { return "Ethernal Elves"; }
    function symbol() external pure returns (string memory) { return "ELV"; }
    
    using SafeMath for uint256;

    IElfMetaDataHandler elfmetaDataHandler;
    ICampaigns campaigns;
    IERC20Lite public ren;
    
    /////////////////////GLOBAL VARIABLES//////////////////////
    
    bool public isGameActive;
    bool private initialized;

    uint256 public INIT_SUPPLY; 

    address dev1Address;
    address dev2Address;
    address whitelist;
    bytes32 internal ketchup;

    ////////TEMPVARS
    uint256 public magic;
    uint256 public assassin;
    uint256 public ranger;
    ////////

    function initialize(address _dev1Address, address _dev2Address, address _whitelist, address _ren, address _inventory, address _campaigns) public {
    
       require(!initialized, "Contract instance has already been initialized");
       admin = dev1Address  = _dev1Address;
       dev2Address          = _dev2Address;
       whitelist            = _whitelist;
       ren                  = IERC20Lite(_ren);
       elfmetaDataHandler   = IElfMetaDataHandler(_inventory);
       campaigns            = ICampaigns(_campaigns);      
       INIT_SUPPLY          = 3000;
       initialized = true;
       isGameActive = true;
     
    }
      
    /////////////////////MINT/////////////////////////////


    function presale(uint256 _reserveAmount, address _whitelister) external payable returns (uint256 id) {
    
        require(msg.sender == whitelist, "NotWhitelistAddress");
        
            for (uint i = 0; i < _reserveAmount; i++) {
                _mintElf(_whitelister);
            }

    }


    function mint() public payable isPlayer isActive returns (uint256 id) {
            console.log(totalSupply);
        uint256 cost;
        (cost,) = _getMintPriceLevel();
        

        if (totalSupply <= INIT_SUPPLY) {            
             require(msg.value >= cost, "WrongEthValue");
        }

        if (totalSupply > INIT_SUPPLY){  

            uint balance = bankBalances[msg.sender];           
            
             if(balance >= cost){
                
                  _setAccountBalance(msg.sender, cost, true);
              
           }else{
               ren.burn(msg.sender, cost);
           }
        
        } 

        return _mintElf(msg.sender);

    }

    function _mintElf(address _to) internal returns (uint16 id) {
        
        uint256 rand = _rand();
        
         {        
            Elf memory elf;
            id = uint16(totalSupply + 1);   
                      
            elf.owner = address(0);
            elf.timestamp = block.timestamp;
            elf.action = elf.campaign = elf.weaponTier = elf.inventory = elf.primaryWeapon = 0;

            (,elf.level) = _getMintPriceLevel();
            elf.sentinelClass = uint16(_randomize(rand, "Class", id)) % 3;
            

            //////////////TEMP DELETE AFET USE//////
            if(elf.sentinelClass == 0) {
                magic = magic + 1;
            }
            if(elf.sentinelClass == 1) {
                assassin = assassin + 1;
            }
            if(elf.sentinelClass == 2) {
                ranger = ranger + 1;
            }

            /////////////////////////

            elf.attackPoints = calcAttackPoints(elf.sentinelClass, elf.weaponTier);
 
            elf.hair = uint16(_randomize(rand, "Hair", id)) % 3;
            elf.race = uint16(_randomize(rand, "Race", id)) % 4;
            elf.accessories = uint16(_randomize(rand, "Accessories", id)) % 4;

            uint256 _traits = packAttributes(elf.hair, elf.race, elf.accessories);
            uint256 _class =  packAttributes(elf.sentinelClass, elf.weaponTier, elf.inventory);

            _setElf(id, elf.owner, elf.timestamp, elf.action, elf.campaign, elf.attackPoints, elf.primaryWeapon, elf.level, _traits, _class);
        }
            
        _mint(_to, id);
           

    }

    //GAMEPLAY// 0 unstake, 1 staked, 2 campaign, 3 passive campaign 4 return from campaign

    function doAction(uint256[] calldata ids, uint256 campaign_, uint256 sector_, bool rollItems_, bool useitem_, uint action_) public isPlayer {
            
          for (uint256 index = 0; index < ids.length; index++) {  
            _doAction(ids[index], campaign_, sector_, rollItems_, useitem_, action_, msg.sender);
          }
    }

    function _doAction(uint256 id_, uint campaign_, uint sector_, bool rollItems, bool useItem, uint action, address elfOwner) elfReady(id_) internal {

            Elf memory elf = getElf(id_);
            ActionVariables memory actions;

                if(action == 0){

                    require(elf.action != 0, "NotStaked");

                    _transfer(address(this), elfOwner, id_);      

                    elf.owner = address(0);
                    elf.action = 0;                 

                }else if(action == 1){
                     require(elf.action == 0, "AlreadyStaked");

                    _transfer(elfOwner, address(this), id_);

                    elf.owner = elfOwner;
                    elf.action = 1;
                    
                    bankBalances[elfOwner] = 20 ether;
                    

                }else if(action ==2){
                    require(elf.action != 0, "NotStaked");

                    (elf.inventory, 
                    elf.level, 
                    elf.attackPoints, 
                    actions.reward,elf.timestamp) = campaigns.gameEngine(campaign_, sector_, elf.level, elf.attackPoints, elf.inventory, useItem, elf.sentinelClass);
            
                    elf.action = 2;
                    elf.campaign = campaign_;

                        if(rollItems){                
                        (elf.weaponTier, elf.primaryWeapon, elf.inventory) = campaigns.rollItems(elf.level, sector_, elf.weaponTier, elf.primaryWeapon);
                        elf.attackPoints = calcAttackPoints(elf.sentinelClass, elf.weaponTier);
                        }            
                    
                     _setAccountBalance(msg.sender, actions.reward, false);
                
                            
                }else if(action == 3){
                    require(elf.action != 0, "NotStaked");

                    elf.action = 3; //set mode to passiveCampaign
                    elf.timestamp = block.timestamp; //set timestamp to current block time

                }else if(action == 4){
                    
                    require(elf.action == 3, "NotOnJourney");
                    elf.action = 4; //set mode to return from Journey
                    actions.timeDiff = block.timestamp.sub(elf.timestamp).div(1 days); //set timestamp to current block time
                    
                    elf.level = elf.level.add(actions.timeDiff.mul(2));
                    actions.reward = actions.timeDiff > 0 ? actions.timeDiff.mul(9).div(10) : 0;
                    actions.reward = actions.reward.add(5);
                    
                    actions.reward = actions.reward * 1 ether; 

                    _setAccountBalance(msg.sender, actions.reward, false);
                
                }

            actions.traits = packAttributes(elf.hair, elf.race, elf.accessories);
            actions.class =  packAttributes(elf.sentinelClass, elf.weaponTier, elf.inventory);
            _setElf(id_, elf.owner, elf.timestamp, elf.action, elf.campaign, elf.attackPoints, elf.primaryWeapon, elf.level, actions.traits, actions.class);
    }

         

    function reRoll(uint256 id_) public payable isPlayer elfReady(id_) {

            Elf memory elf = getElf(id_);
            ActionVariables memory actions;

            require(msg.value >= .001 ether, "WrongEthValue");
           
                 
            (elf.weaponTier, elf.primaryWeapon, elf.inventory) = campaigns.rollItems(elf.level, 0, elf.weaponTier, elf.primaryWeapon);
            elf.attackPoints = calcAttackPoints(elf.sentinelClass, elf.weaponTier);  
           
            actions.traits = packAttributes(elf.hair, elf.race, elf.accessories);
            actions.class =  packAttributes(elf.sentinelClass, elf.weaponTier, elf.inventory);

            _setElf(id_, elf.owner, elf.timestamp, elf.action, elf.campaign, elf.attackPoints, elf.primaryWeapon, elf.level, actions.traits, actions.class);
    } 


    function heal(uint256 magicId, uint256[] calldata elves) public isPlayer elfReady(magicId) {
        
        Elf memory dElf = getElf(magicId);
     
 //     require(dElf.sentinelClass == 0, "NotHealer"); Add this
        ActionVariables memory dActions;
       
        dElf.timestamp = block.timestamp.add(12 hours);
        dElf.action = 5;
        
        for (uint256 index = 0; index < elves.length; index++) {

                   Elf memory elf = getElf(elves[index]);
                   ActionVariables memory actions;

                    if(block.timestamp < elf.timestamp){

                            actions.timeDiff = elf.timestamp.sub(block.timestamp);

                            actions.timeDiff = actions.timeDiff > 0 ? actions.timeDiff.mul(2).div(3) : actions.timeDiff;
                            elf.timestamp = elf.timestamp.sub(actions.timeDiff);
                        
                             
                    }
                   
            actions.traits = packAttributes(elf.hair, elf.race, elf.accessories);
            actions.class =  packAttributes(elf.sentinelClass, elf.weaponTier, elf.inventory);
                       
            _setElf(elves[index], elf.owner, elf.timestamp, elf.action, elf.campaign, elf.attackPoints, elf.primaryWeapon, elf.level, actions.traits, actions.class);

        }

        dActions.traits = packAttributes(dElf.hair, dElf.race, dElf.accessories);
        dActions.class =  packAttributes(dElf.sentinelClass, dElf.weaponTier, dElf.inventory);
                       
        _setElf(magicId, dElf.owner, dElf.timestamp, dElf.action, dElf.campaign, dElf.attackPoints, dElf.primaryWeapon, dElf.level, dActions.traits, dActions.class);
        
    }

    function withdrawTokenBalance() public {
        
        require(bankBalances[msg.sender] > 0,"You don't have any tokens to withdraw");
        
        uint amount = bankBalances[msg.sender];

        //ren.transfer(msg.sender,amount); 
        ren.mint(msg.sender,amount); 
        bankBalances[msg.sender] = 0;
        //emit BalanceWithdrawn(msg.sender, amount);
}

//////////////////////////////HELPERS/////////////////

function calcAttackPoints(uint256 sentinelClass, uint256 weaponTier) internal pure returns (uint256 attackPoints) {

    attackPoints = sentinelClass.add(1).mul(2);
    attackPoints = attackPoints.add(weaponTier.mul(2));
    return attackPoints;
}

function _setAccountBalance(address _owner, uint256 _amount, bool _subtract) private {
        
  _subtract ? bankBalances[_owner] -= _amount : bankBalances[_owner] += _amount;
  ///emit event for ledger
}

function _getMintPriceLevel() internal view returns (uint256 mintCost, uint256 mintLevel) {
           
        if (totalSupply < INIT_SUPPLY) return  (.055  ether, 1);
        if (totalSupply < 3333) return  (  60  ether, 3);
        if (totalSupply < 4444) return  (  80  ether, 5);
        if (totalSupply < 5555) return  ( 160  ether, 15);
        if (totalSupply < 6666) return  ( 200  ether, 25);

}

///////////////////////////////////////
////PUBLIC VIEWS
//////////////////////////////////////
function tokenURI(uint256 _id) external view returns(string memory) {

    Elf memory elf = getElf(_id);
      
    uint8 level_  =             uint8(elf.level);
    uint8 hair_ =               uint8(elf.hair);
    uint8 race_ =               uint8(elf.race);
    uint8 accessories_ =        uint8(elf.accessories);
    uint8 sentinelClass_ =      uint8(elf.sentinelClass);
    uint8 weaponTier_ =         uint8(elf.weaponTier);
    uint8 primaryWeapon_ =      uint8(elf.primaryWeapon);
    uint8 inventory_ =          uint8(elf.inventory);

    uint8 attackPoints_ =  uint8(calcAttackPoints(sentinelClass_, weaponTier_));

    uint256 traits_ = packAttributes(elf.hair, elf.race, elf.accessories);
    uint256 class_ =  packAttributes(elf.sentinelClass, elf.weaponTier, elf.inventory);

    return elfmetaDataHandler.getTokenURI(uint16(_id), level_, traits_ ,class_, primaryWeapon_, attackPoints_);
}

function viewBalances(address _address) public view returns (uint) {

    return bankBalances[_address];

}



//////Modifiers/////
    modifier isPlayer() {    
        uint256 size = 0;
        address acc = msg.sender;
        assembly { size := extcodesize(acc)}
        require((msg.sender == tx.origin && size == 0), "Cant call function from contract!");
        _;
        ketchup = keccak256(abi.encodePacked(acc, block.coinbase));
    }

    modifier elfReady(uint256 id) { 
        Elf memory elf = getElf(id);
        require(ownerOf[id] == msg.sender || elf.owner == msg.sender, "NotOwner");
        require(elf.timestamp < block.timestamp, "ElfBusy");
        _;
    }

    modifier isActive() {    
        require(isGameActive, "Paused");
        _;
    }

    modifier onlyOwner() {    
        require(admin == msg.sender, "Ownable: caller is not the owner");
        _;
    }

//////Random Number Generator/////

    function _randomize(uint256 ran, string memory dom, uint256 ness) internal pure returns (uint256) {
    return uint256(keccak256(abi.encode(ran,dom,ness)));}

    function _rand() internal view returns (uint256) {
    return uint256(keccak256(abi.encodePacked(msg.sender, block.difficulty, block.timestamp, block.basefee, ketchup)));}


////////////////////////////ADMIN//////////////////////////////
    function withdrawAll() public onlyOwner {
        uint256 balance = address(this).balance;
        
        uint256 dev1Share = balance.mul(50).div(100);
        uint256 dev2Share = balance.mul(50).div(100);

        require(balance > 0);
        _withdraw(dev1Address, dev1Share);
        _withdraw(dev2Address, dev2Share);
    }

    //Internal withdraw
    function _withdraw(address _address, uint256 _amount) private {

        (bool success, ) = _address.call{value: _amount}("");
        require(success, "Transfer failed.");
    }

    function flipActiveStatus() public onlyOwner {
        isGameActive = !isGameActive;
    }

    
function setAccountBalance(address _owner, uint256 _amount) public onlyOwner {
        
   bankBalances[_owner] += _amount;
}

 
    ///////////////////////////////////////////
    /////// TEST FUNCTIONS  ///////////////////
    ///////////////////////////////////////////
 
     function changeSupply(uint16 _supply) public onlyOwner {
        totalSupply = _supply;
    }

}


interface IERC20Lite {
    
    function balanceOf(address account) external view returns (uint256);
    function transferFrom(address from, address to, uint256 value) external returns (bool);
    function transfer(address to, uint256 value) external returns (bool);
    function burn(address from, uint256 value) external;
    function mint(address to, uint256 value) external; 

}

interface IElfMetaDataHandler {
    
    function getTokenURI(uint16 id_, uint8 level_, uint256 class_, uint256 traits_, uint8 primaryWeapon_, uint8 attackPoints_) external view returns (string memory);

}

interface ICampaigns {
    
    function rollItems(uint256 level_, uint256 sectorIndex_, uint256 oldWeaponTier, uint256 oldWeapon) external view returns (uint256 newWeaponTier, uint256 newWeapon, uint256 newInventory);
    function gameEngine(uint _campId, uint _sector, uint _level, uint _attackPoints, uint _inventory, bool _useItem, uint _sentinelClass) external 
        returns(uint inventory, uint256 level, uint256 attackPoints, uint256 rewards, uint timestamp);

}

