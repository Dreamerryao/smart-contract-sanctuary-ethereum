pragma solidity 0.8.7;

//import "hardhat/console.sol";

contract ElfDataStore {

mapping(uint256 => uint256) sentinels; //memory slot for Elfs
mapping(address => uint256) bankBalances; //memory slot for bank balances

/////////////DATA STRUCTURES///////////////////////////////
    struct Elf {
            address owner;  
            uint256 timestamp; 
            uint256 action; 
            uint256 campaign;
            uint256 attackPoints; 
            uint256 primaryWeapon; 
            uint256 level;
            uint256 hair;
            uint256 race; 
            uint256 accessories; 
            uint256 sentinelClass; 
            uint256 weaponTier; 
            uint256 inventory; 
    }

    struct Attributes { 
            uint256 hair; //MAX 3
            uint256 race;  //MAX 6 Body
            uint256 accessories; //MAX 7
            uint256 sentinelClass; //MAX 3
            uint256 weaponTier; //MAX 6
            uint256 inventory; //MAX 7
    }

    struct ActionVariables {

            uint256 reward;
            uint256 timeDiff;
            uint256 traits; 
            uint256 class;  
    } 

//////////////////////////Elf Storage/////////////////////////////////////////

function getElf(uint256 _id) public view returns(Elf memory _elf) {
    
    uint256 character = sentinels[_id];

    _elf.owner =          address(uint160(uint256(character)));
    _elf.timestamp =      uint256(uint40(character>>160));
    _elf.action =         uint256(uint8(character>>200));
    _elf.campaign =       uint256(uint8(character>>208));
    _elf.attackPoints =   uint256(uint8(character>>216));
    _elf.primaryWeapon =  uint256(uint8(character>>224));
    _elf.level =          uint256(uint8(character>>232));

    uint _traits =        uint256(uint8(character>>240));
    uint _class =         uint256(uint8(character>>248));

    Attributes memory expandAttributes = getAttributes(_traits, _class);

    _elf.hair           = expandAttributes.hair;
    _elf.race           = expandAttributes.race;
    _elf.accessories    = expandAttributes.accessories;
    _elf.sentinelClass  = expandAttributes.sentinelClass;
    _elf.weaponTier     = expandAttributes.weaponTier;
    _elf.inventory      = expandAttributes.inventory;

}


function _setElf(
    uint256 _id, 
    address owner, 
    uint256 timestamp, 
    uint256 action, 
    uint256 campaign, 
    uint256 attackPoints, 
    uint256 primaryWeapon, 
    uint256 level, 
    uint256 traits, 
    uint256 class 
    ) internal {

    uint256 character = uint256(uint160(address(owner)));
    
    character |= timestamp<<160;
    character |= action<<200;
    character |= campaign<<208;
    character |= attackPoints<<216;
    character |= primaryWeapon<<224;
    character |= level<<232;
    character |= traits<<240;
    character |= class<<248;
    
    sentinels[_id] = character;
}

//////////////////////////////HELPERS/////////////////

function packAttributes(uint hundreds, uint tens, uint ones) internal pure returns (uint256 attributes) {
    attributes = uint256(hundreds*100 + tens*10 + ones);
    return attributes;
}

function getAttributes(uint256 _trait, uint256 _class) internal pure returns (Attributes memory _attributes) {

    _attributes.hair              =       (_trait / 100) % 10;
    _attributes.race              =       (_trait / 10) % 10;
    _attributes.accessories       =       (_trait) % 10;   
    _attributes.sentinelClass     =       (_class / 100) % 10;
    _attributes.weaponTier        =       (_class / 10) % 10;
    _attributes.inventory         =       (_class) % 10; 

}

}

