pragma solidity ^0.8.13;

import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
// import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
// import "./interfaces/IMasterChef.sol";
import "./interfaces/IStaking.sol";
import "hardhat/console.sol";
import {ISwapperEx, SwapExample} from "./Swapper.sol";

contract TokenVault is Ownable, Pausable {
    // using SafeERC20 for IERC20;
    // using SafeMath for uint256;

    struct UserInfo {
        uint256 shares; // number of shares for a user
        uint256 lastDepositedTime; // keeps track of deposited time for potential penalty
        uint256 lastUserActionTime; // keeps track of the last user action time
    }

    IERC20 public immutable token; // Cake token
    // ISwapperEx public swapper;
    // IERC20 public immutable receiptToken; // Syrup token

    IStaking public immutable masterchef;

    mapping(address => UserInfo) public userInfo;
    uint256 public totalShares;
    // uint256 public lastHarvestedTime;
    address public admin;
    // address public treasury;

    uint256 public lastHarvest;
    uint256 public constant MAX_PERFORMANCE_FEE = 500; // 5%
    uint256 public constant MAX_CALL_FEE = 1000; // 10%
    uint256 public constant MAX_WITHDRAW_FEE = 100; // 1%
    uint256 public constant MAX_WITHDRAW_FEE_PERIOD = 72 hours; // 3 days
    uint256 public pid;
    // uint256 public performanceFee = 200; // 2%
    uint256 public callFee = 25; // 0.25%
    // uint256 public withdrawFee = 10; // 0.1%
    // uint256 public withdrawFeePeriod = 72 hours; // 3 days

    event Deposit(
        address indexed sender,
        uint256 amount,
        uint256 shares,
        uint256 lastDepositedTime
    );
    event Withdraw(address indexed sender, uint256 amount, uint256 shares);
    event EmergencyWithdraw(
        address indexed sender,
        uint256 amount,
        uint256 shares
    );

    event Harvest(address indexed sender, uint256 callFee);
    event Pause();
    event Unpause();

    constructor(
        IERC20 _token,
        IStaking _masterchef,
        uint256 _pid,
        address _admin // address _treasury // ISwapperEx _swapper
    ) {
        token = _token;
        masterchef = _masterchef;
        pid = _pid;
        admin = _admin;

        uint256 maxInt = 0;
        unchecked {
            maxInt = maxInt - 1;
        }
        // govno, vse ravno mozhet ne hvatit
        IERC20(_token).approve(address(_masterchef), maxInt);
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "admin: wut?");
        _;
    }

    /**
     * @notice Checks if the msg.sender is a contract or a proxy
     */
    modifier notContract() {
        require(!_isContract(msg.sender), "contract not allowed");
        require(msg.sender == tx.origin, "proxy contract not allowed");
        _;
    }

    /**
     * @notice Deposits funds into the Cake Vault
     * @dev Only possible when contract not paused.
     * @param _amount: number of tokens to deposit (in CAKE)
     */
    function deposit(uint256 _amount) external whenNotPaused notContract {
        require(_amount > 0, "Nothing to deposit");

        UserInfo storage user = userInfo[msg.sender];

        uint256 currentShares;
        uint256 amountToStake = _amount;
        uint256 totalPoolAmount = balanceOf();
        uint256 poolReward = masterchef.pendingReward(pid, address(this));

        if (poolReward != 0) {
            masterchef.withdraw(pid, 0);
            totalPoolAmount += poolReward;
            amountToStake += poolReward;
        }

        token.transferFrom(msg.sender, address(this), _amount);

        if (totalShares != 0) {
            currentShares = (_amount * totalShares) / totalPoolAmount;
            // console.log(
            //     "-ap-currentShares =  amount(%s) * totalShares(%s) / totalPoolAmount ",
            //     _amount,
            //     totalShares,
            //     totalPoolAmount
            // );
        } else {
            currentShares = _amount;
        }

        user.shares += currentShares;
        totalShares += currentShares;

        user.lastDepositedTime = block.timestamp;
        user.lastUserActionTime = block.timestamp;

        // console.log("-ap-Deposit to staking during deposit: ", amountToStake);

        IStaking(masterchef).deposit(pid, amountToStake);

        emit Deposit(msg.sender, _amount, currentShares, block.timestamp);
    }

    function withdrawAll() external notContract {
        withdrawShares(userInfo[msg.sender].shares);
    }

    /**
     * @notice Reinvests CAKE tokens into MasterChef
     * @dev Only possible when contract not paused.
     */
    function harvest() external notContract whenNotPaused {
        _earn(true);
    }

    /**
     * @notice Sets admin address
     * @dev Only callable by the contract owner.
     */
    function setAdmin(address _admin) external onlyOwner {
        require(_admin != address(0), "Cannot be zero address");
        admin = _admin;
    }

    /**
     * @notice Sets call fee
     * @dev Only callable by the contract admin.
     */
    function setCallFee(uint256 _callFee) external onlyAdmin {
        require(
            _callFee <= MAX_CALL_FEE,
            "callFee cannot be more than MAX_CALL_FEE"
        );
        callFee = _callFee;
    }

    function emergencyWithdraw() external onlyAdmin whenPaused {
        masterchef.emergencyWithdraw(pid);
    }

    function emergencyUserWithdraw() external notContract whenPaused {
        UserInfo storage user = userInfo[msg.sender];
        require(user.shares > 0, "Nothing to withdraw");

        uint256 stakingBalance = token.balanceOf(address(this));
        uint256 currentAmountToWithdraw = (stakingBalance * user.shares) /
            totalShares;

        user.lastUserActionTime = block.timestamp;
        user.shares = user.shares - user.shares;
        totalShares = totalShares - user.shares;

        token.transfer(msg.sender, currentAmountToWithdraw);

        emit EmergencyWithdraw(
            msg.sender,
            currentAmountToWithdraw,
            user.shares
        );
    }

    function inCaseTokensGetStuck(address _token) external onlyAdmin {
        require(
            _token != address(token),
            "Token cannot be same as deposit token"
        );
        // require(_token != address(receiptToken), "Token cannot be same as receipt token");

        uint256 amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).transfer(msg.sender, amount);
    }

    /**
     * @notice Triggers stopped state
     * @dev Only possible when contract not paused.
     */
    function pause() external onlyAdmin whenNotPaused {
        _pause();
        emit Pause();
    }

    /**
     * @notice Returns to normal state
     * @dev Only possible when contract is paused.
     */
    function unpause() external onlyAdmin whenPaused {
        _unpause();
        emit Unpause();
    }

    /**
     * @notice Calculates the expected harvest reward from third party
     * @return Expected reward to collect in CAKE
     */
    function calculateHarvestRewards() external view returns (uint256) {
        uint256 amount = IStaking(masterchef).pendingReward(pid, address(this));
        // amount = amount.add(available());
        uint256 currentCallFee = (amount * callFee) / (10000);
        // button click reward
        return currentCallFee;
    }

    /**
     * @notice Calculates the total pending rewards that can be restaked
     * @return Returns total pending cake rewards
     */
    function calculateTotalPendingRewards() external view returns (uint256) {
        return IStaking(masterchef).pendingReward(0, address(this));
    }

    function calculateCurrentBalance(address _of)
        external
        view
        returns (uint256)
    {
        UserInfo storage user = userInfo[_of];
        uint256 pending = (balanceOf() * user.shares) / (totalShares);

        return pending;
    }

    /**
     * @notice Calculates the price per share
     */
    function getPricePerFullShare() external view returns (uint256) {
        //                 if=0, then 1e18 else ratio
        return totalShares == 0 ? 1e18 : (total() * 1e18) / (totalShares);
    }

    function getTotalStakeOf(address _of) external view returns (uint256) {
        UserInfo storage user = userInfo[_of];
        uint256 pending = (total() * user.shares) / (totalShares);

        return pending;
    }

    /**
     * @notice Withdraws from funds from the Cake Vault
     * @param _shares: Number of shares to withdraw
     */
    function withdrawShares(uint256 _shares) public notContract {
        UserInfo storage user = userInfo[msg.sender];
        require(_shares > 0, "Nothing to withdraw");
        require(_shares <= user.shares, "Withdraw amount exceeds balance");
        //
        //          reward + stake

        _earn(false);

        uint256 stakingBalance = total();
        // uint256 currentAmount = (balanceOf().mul(_shares)).div(totalShares);
        uint256 currentAmountToWithdraw = (stakingBalance * _shares) /
            totalShares;

        console.log(
            "-ap-currentAmountToWithdraw is: ",
            currentAmountToWithdraw,
            " \n Staking balance is: ",
            stakingBalance
        );

        user.lastUserActionTime = block.timestamp;
        user.shares = user.shares - _shares;
        totalShares = totalShares - _shares;

        masterchef.withdraw(pid, currentAmountToWithdraw);
        token.transfer(msg.sender, currentAmountToWithdraw);

        emit Withdraw(msg.sender, currentAmountToWithdraw, _shares);
    }

    function total() internal view returns (uint256) {
        (uint256 amount, ) = masterchef.userInfo(pid, address(this));
        amount += masterchef.pendingReward(pid, address(this));
        return amount;
    }

    function balanceOf() public view returns (uint256) {
        (uint256 amount, ) = masterchef.userInfo(pid, address(this));
        // return token.balanceOf(address(this))+(amount);
        // amount += IStaking(masterchef).pendingReward(pid,address(this));

        return amount;
    }

    function _earn(bool _fromHarvest) internal {
        // console.log("-ap-I am earning");
        // withdraw 0 to get reward
        uint256 pendingReward = masterchef.pendingReward(pid, address(this));

        if (pendingReward != 0) {
            masterchef.withdraw(pid, 0);

            console.log("-ap-stakingReward is: ", pendingReward);
            // if it's harvest, give reward to caller
            if (_fromHarvest) {
                uint256 callerFee = ((pendingReward) * callFee) / 10000;
                pendingReward -= callerFee;
                pendingReward -= callerFee;
                // console.log("-ap-Caller reward: ", callerFee);
                token.transfer(msg.sender, callerFee);
                emit Harvest(msg.sender, callerFee);
            }
            // console.log("-ap-Deposit to staking: ", withRewardBalance);
            IStaking(masterchef).deposit(pid, pendingReward);
        }else{
            if (_fromHarvest){
                require(pendingReward>0,"Sorry, someone took your reward");
            }
        }
    }

    function _isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(addr)
        }
        return size > 0;
    }
}

