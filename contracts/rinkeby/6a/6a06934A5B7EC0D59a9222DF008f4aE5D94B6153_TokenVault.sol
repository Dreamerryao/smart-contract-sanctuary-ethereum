pragma solidity ^0.8.13;

import "hardhat/console.sol";
import "./interfaces/IUniswapper.sol";
import "./interfaces/IUniswapV2pair.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

// contract LiquidityValueCalculator is ILiquidityValueCalculator {
interface ISwapperEx {
    function PAIR() external view;

    function GEAR() external view;

    function WETH() external view;

    function UNISWAP_ROUTER_ADDRESS() external view;

    function ETHtoGEAR(address _to) external payable;

    function GEARtoETH(address _to, uint256 _amount) external payable;

    receive() external payable;

    fallback() external payable;
}

contract SwapExample {
    address public constant PAIR = 0xdD5d1A256b25e1087Fc3B098b443e96Cfa73237d;
    address public constant GEAR = 0x1b980e05943dE3dB3a459C72325338d327B6F5a9;
    address public constant WETH = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    address public constant UNISWAP_ROUTER_ADDRESS =
        0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    ISwapper public uniswapRouter;
    IERC20 public token;

    // router 01 0x1b980e05943dE3dB3a459C72325338d327B6F5a9
    // router 02 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
    constructor() {
        token = IERC20(GEAR);
        uniswapRouter = ISwapper(UNISWAP_ROUTER_ADDRESS);
    }

    receive() external payable {}

    fallback() external payable {}

    function getPriceInEther(uint256 amountInEther) public view returns(uint256) {
        IUniswapV2Pair pair = IUniswapV2Pair(PAIR);
        (uint256 Reserves0, uint256 Reserves1, ) = pair.getReserves();

        // decimals
        uint256 res1 = Reserves1 * (10**18);
        return ((amountInEther * res1) / Reserves0); // return amount of token0 needed to buy token1
    }
