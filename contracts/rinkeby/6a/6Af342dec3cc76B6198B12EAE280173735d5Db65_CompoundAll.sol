        require(
            msg.sender == _user ||
                msg.sender == IPowerCityUserFactory(factory).getUserAdmin(),
            "msgSender != user or userAdmin"
        );

        address contract_ = IPowerCityUserFactory(factory).getContract(_user);

        uint256 ethGainfromStabilityPool = IPowerCityUser(contract_).getDepositorETHGain();
        uint256 ethGainfromStakingPool = IPowerCityUser(contract_).getPendingETHGain();
            
        uint256 lqtyGain_ = IPowerCityUser(contract_).getDepositorLQTYGain();
        uint256 lusdGain_ = IPowerCityUser(contract_).getPendingLUSDGain();

        uint256 lusdBalBefore = IERC20(lusd).balanceOf(address(this));
        uint256 balBefore = address(this).balance;

        // Claim Rewards and Stake lqtyGain_

        if(ethGainfromStabilityPool > 0 || lqtyGain_ > 0 || lusdGain_ > 0) {
            IPowerCityUser(contract_).withdrawFromSP(0);

            IERC20(lqty).safeApprove(contract_, lqtyGain_);
            IPowerCityUser(contract_).stake(lqtyGain_);
        }

        // Provide lusdGain_ to SP

        if(lusdGain_ > 0) {
            uint256 lusdBalAfter = IERC20(lusd).balanceOf(address(this));

            IERC20(lusd).safeApprove(contract_, lusdBalAfter - lusdBalBefore);
            IPowerCityUser(contract_).provideToSP(lusdBalAfter - lusdBalBefore, address(0));

        }

        // Add ETH as collateral in Trove

        if(ethGainfromStabilityPool + ethGainfromStakingPool > 0) {
            
            address upperHint_;
            address lowerHint_;

            // using local scope to avoid stack-too-deep error
            {
                uint256 seed_ = _seed;
                uint256 balAfter = address(this).balance;

                (uint256 debt_, uint256 coll_, , ) = ITroveManager(
                    IPowerCityUserFactory(factory).getTroveManager()
                ).getEntireDebtAndColl(contract_);

                (upperHint_, lowerHint_) = IPowerCityUser(contract_).getAddresses(
                    coll_,
                    debt_,
                    seed_
                );

                IPowerCityUser(contract_).adjustTrove{value: balAfter - balBefore}(
                    0,
                    0,
                    0,
                    false,
                    upperHint_,
                    lowerHint_
                );
            }
        }
    }
}