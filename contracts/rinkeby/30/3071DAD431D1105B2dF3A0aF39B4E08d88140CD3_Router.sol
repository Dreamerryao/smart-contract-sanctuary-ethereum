pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Pair.sol";

/// @title Exchange factory contract
/// @author Rares Stanciu
/// @notice Creates and manages exchange pairs
contract Factory {
    /// @notice Event emitted when creating a new pair
    event PairCreated(
        address indexed token,
        uint256 initialEthLiquidity,
        uint256 initialTokenLiquidity
    );

    /// @notice Mapping of available pair contracts
    mapping(ERC20 => Pair) public pairs;

    /// @notice Create a pair between ETH and an ERC20 token
    /// @param _token Address of the ERC20 token contract
    /// @param _tokenAmount Initial token liquidity
    /// @dev Must also supply initial ETH liquidity via msg.value
    function createPair(ERC20 _token, uint256 _tokenAmount) external payable {
        require(pairs[_token] == Pair(address(0)), "PAIR_ALREADY_EXISTS");
        require(
            _tokenAmount > 0 && msg.value > 0,
            "INITIAL_LIQUIDITY_REQUIRED"
        );
        require(address(_token) != address(0), "TOKEN_REQUIRED");

        // Create pair
        Pair pair = new Pair(this, _token);

        // Save pair to mapping
        pairs[_token] = pair;

        // Add liquidity
        _token.transferFrom(msg.sender, address(pair), _tokenAmount);
        pair.addLiquidity{value: msg.value}(msg.sender, block.timestamp);

        emit PairCreated(address(_token), msg.value, _tokenAmount);
    }
}

