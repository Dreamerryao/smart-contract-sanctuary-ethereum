        _;
        mutex = false;
    }

    /// @notice Initializes the factory and token contract addresses
    /// @param _token Token contract address
    constructor(Factory _factory, ERC20 _token) ERC20("LP Token", "LPT") {
        require(address(_factory) != address(0), "FACTORY_REQUIRED");
        require(address(_token) != address(0), "TOKEN_REQUIRED");

        factory = _factory;
        token = _token;
    }

    /// @notice Add liquidity to this trading pair
    /// @dev Mints LP tokens, corresponding to user's share of the total pool
    /// @param _to Address of the user who receives the LP tokens
    /// @param _deadline Timestamp after which the liquidity adding becomes invalid
    function addLiquidity(address _to, uint256 _deadline)
        public
        payable
        onlyUntil(_deadline)
        reentrancyGuard
    {
        uint256 tokenAmount = token.balanceOf(address(this)) - tokenReserve;
        uint256 ethAmount = address(this).balance - ethReserve;

        require(tokenAmount > 0 && ethAmount > 0, "AMOUNTS_NOT_SENT");

        uint256 lpTokensAmount;
        if (totalSupply() > 0) {
            // Compute new tokens share of the total pool
            uint256 tokensShare = (tokenAmount * totalSupply()) / tokenReserve;

            // Compute new ETH share of the total pool
            uint256 ethShare = (ethAmount * totalSupply()) / ethReserve;

            // We mint the minimum of the two, in order to be sure
            // that we have enough of both (ETH and tokens)
            lpTokensAmount = tokensShare < ethShare ? tokensShare : ethShare;
        } else {
            // From the amount of LP tokens that must be minted,
            // we subtract the minimum liquidity
            lpTokensAmount =
                Math.sqrt(ethAmount * tokenAmount) -
                MINIMUM_LIQUIDITY;

            // Since we can't mint to the 0 address,
            // we mint the minimum LP tokens to the factory contract,
            // since we know that they won't be recovered.
            _mint(address(factory), MINIMUM_LIQUIDITY);
        }

        // Make sure we mint LP tokens
        require(lpTokensAmount > 0, "NO_LIQUIDITY_MINTED");

        // Mint LP tokens
        _mint(_to, lpTokensAmount);

        // Sync reserves
        syncReserves();

        emit LiquidityAdded(ethAmount, tokenAmount, _to);
