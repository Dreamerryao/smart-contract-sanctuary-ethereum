pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Factory.sol";
import "./Pair.sol";

/// @title Router contract
/// @author Rares Stanciu
/// @notice Allows exchanging ETH and tokens through the exchange
contract Router {
    /// @notice Event emitted when a swap takes place
    event Swap(
        address indexed token,
        uint256 ethInput,
        uint256 tokensInput,
        uint256 ethOutput,
        uint256 tokensOutput,
        address indexed receiver
    );

    /// @notice Event emitted when liquidity is added
    event LiquidityAdded(
        uint256 ethAdded,
        uint256 tokensAdded,
        address indexed receiver
    );

    /// @notice Event emitted when liquidity is removed
    event LiquidityRemoved(uint256 lpTokensRemoved, address indexed receiver);

    /// @notice Address of the factory contract
    Factory public factory;

    /// @notice Used as a reentrancy guard
    bool private mutex;

    /// @notice Allow execution only if deadline was not exceeded
    modifier onlyUntil(uint256 _deadline) {
        require(block.timestamp <= _deadline, "DEADLINE_EXCEEDED");
        _;
    }

    /// @notice Do not allow reentrancy in functions with this modifier
    modifier reentrancyGuard() {
        require(!mutex, "REENTRANCY_GUARD");
        mutex = true;
        _;
        mutex = false;
    }

    constructor(Factory _factory) {
        require(address(_factory) != address(0), "FACTORY_REQUIRED");

        factory = _factory;
    }

    /// @notice Compute the expected output for a given input amount
    /// @param _token Address of the ERC20 token contract
    /// @param _ethInput Amount of ETH to exchange
    /// @param _tokenInput Amount of tokens to exchange
    /// @dev _ethInput and _tokenInput can't both be > 0
    function getExpectedOutput(
        ERC20 _token,
        uint256 _ethInput,
        uint256 _tokenInput
    ) public view returns (uint256) {
        require(address(_token) != address(0), "TOKEN_REQUIRED");
        require(_ethInput > 0 || _tokenInput > 0, "ONE_INPUT_REQUIRED");
        require(_ethInput == 0 || _tokenInput == 0, "ONLY_ONE_INPUT");

        // Find pair from the Factory contract
        Pair pair = factory.pairs(_token);
        require(address(pair) != address(0), "PAIR_NOT_FOUND");

        (uint256 ethReserve, uint256 tokenReserve) = pair.getReserves();
        // There's no point in checking if pair has liquidity,
        // since having a pair in the Factory contract implies it was
        // created with factory.createPair() which requires
        // initial liquidity to be sent at creation.
        // Other manually created pairs, that may not have initial liquidity set,
        // would not be inserted in the factory.pairs mapping.

        // Set correct input and output values in order to allow
        // for exchanges both ways.
        uint256 inputReserve;
        uint256 outputReserve;
        uint256 inputAmount;
        if (_ethInput > 0) {
            inputReserve = ethReserve;
            outputReserve = tokenReserve;
            inputAmount = _ethInput;
        } else {
            inputReserve = tokenReserve;
            outputReserve = ethReserve;
            inputAmount = _tokenInput;
        }

        // Account for swap fee
        inputAmount = inputAmount * (100 - pair.FEE_PERCENTAGE());

        uint256 inputReserveAfter = inputReserve * 100 + inputAmount;
        uint256 outputReserveAfter = ((ethReserve * tokenReserve) * 100) /
            inputReserveAfter;

        uint256 outputAmount = outputReserve - outputReserveAfter;

        return outputAmount;
    }

    /// @notice Exchange ETH for Tokens or Tokens for ETH
    /// @param _token Address of desired ERC20 token contract
    /// @param _ethInput The exact amount of ETH to exchange
    /// @param _minTokenOutput If buying tokens, this is the minimum amount of tokens to buy (defines slippage)
    /// @param _tokenInput The exact amount of tokens to exchange
    /// @param _minEthOutput If buying ETH, this is the minimum amount of ETH to buy (defines slippage)
    /// @param _deadline Timestamp after which the liquidity adding becomes invalid
    /// @dev _ethInput and _tokenInput can't be both non-zero
    /// @dev Even though _ethInput is redundant, we use it for clarity.
    function swap(
        ERC20 _token,
        uint256 _ethInput,
        uint256 _minTokenOutput,
        uint256 _tokenInput,
        uint256 _minEthOutput,
        uint256 _deadline
    ) external payable onlyUntil(_deadline) reentrancyGuard {
        require(address(_token) != address(0), "TOKEN_REQUIRED");
        require(_ethInput > 0 || _tokenInput > 0, "ONE_INPUT_REQUIRED");
        require(_ethInput == 0 || _tokenInput == 0, "ONLY_ONE_INPUT");

        if (_ethInput > 0) {
            require(msg.value == _ethInput, "WRONG_TX_VALUE_SENT");
        }

        // Find pair from the Factory contract
        Pair pair = factory.pairs(_token);
        require(address(pair) != address(0), "PAIR_NOT_FOUND");

        // In order to allow for fee-on-transfer tokens,
        // we compute the token input using the delta of the balance
        uint256 tokenAmount;

        if (_tokenInput > 0) {
            uint256 balanceBefore = _token.balanceOf(address(pair));
            _token.transferFrom(msg.sender, address(pair), _tokenInput);
            uint256 balanceAfter = _token.balanceOf(address(pair));
            tokenAmount = balanceAfter - balanceBefore;
        }

        // Compute the expected output amount
        uint256 ethOutput;
        uint256 tokenOutput;
        {
            uint256 output = getExpectedOutput(_token, _ethInput, tokenAmount);
            ethOutput = _ethInput > 0 ? 0 : output;
            tokenOutput = _tokenInput > 0 ? 0 : output;
        }

        // Compute user's balances before the swap
        uint256 ethBalanceBefore = address(msg.sender).balance;
        uint256 tokenBalanceBefore = _token.balanceOf(address(msg.sender));

        // Do the swap
        pair.swap{value: _ethInput}(
            ethOutput,
            tokenOutput,
            msg.sender,
            _deadline
        );

        // Compute amounts received
        {
            uint256 ethReceived = address(msg.sender).balance -
                ethBalanceBefore;
            uint256 tokensReceived = _token.balanceOf(address(msg.sender)) -
                tokenBalanceBefore;

            // Ensure user doesn't get less than the minimum requested
            require(ethReceived >= _minEthOutput, "SLIPPAGE_TOO_HIGH");
            require(tokensReceived >= _minTokenOutput, "SLIPPAGE_TOO_HIGH");
        }

        emit Swap(
            address(_token),
            _ethInput,
            _tokenInput,
            ethOutput,
            tokenOutput,
            msg.sender
        );
    }

    /// @notice Add liquidity to a pair, setting desired slippage
    /// @param _token Address of desired ERC20 token contract
    /// @param _ethInput Desired amount of ETH to add
    /// @param _minEthInput Minimum amount of ETH allowed (defines slippage)
    /// @param _tokenInput Desired amount of tokens to add
    /// @param _minTokenInput Minimum amonut of tokens allowed (defines slippage)
    /// @param _deadline Timestamp after which the liquidity adding becomes invalid
    /// @dev Even though _ethInput is redundant, we use it for clarity.
    function addLiquidity(
        ERC20 _token,
        uint256 _ethInput,
        uint256 _minEthInput,
        uint256 _tokenInput,
        uint256 _minTokenInput,
        uint256 _deadline
    ) external payable onlyUntil(_deadline) reentrancyGuard {
        require(address(_token) != address(0), "TOKEN_REQUIRED");
        require(_ethInput > 0 && _tokenInput > 0, "BOTH_INPUTS_REQUIRED");
        require(
            _ethInput >= _minEthInput && _tokenInput >= _minTokenInput,
            "WRONG_MIN_INPUTS"
        );

        require(msg.value == _ethInput, "WRONG_TX_VALUE_SENT");

        // Find pair from the Factory contract
        Pair pair = factory.pairs(_token);
        require(address(pair) != address(0), "PAIR_NOT_FOUND");

        (uint256 ethReserve, uint256 tokenReserve) = pair.getReserves();

        // Compute new tokens share of the total pool
        uint256 tokensShare = (_tokenInput * pair.totalSupply()) / tokenReserve;

        // Compute new ETH share of the total pool
        uint256 ethShare = (_ethInput * pair.totalSupply()) / ethReserve;

        // In order to be sure that we have enough of both inputs,
        // we choose the minimum of the two
        uint256 ethAmount;
        uint256 tokenAmount;

        if (tokensShare < ethShare) {
            // Compute the required ETH amount if we deposit the full amount of tokens
            uint256 requiredEth = (_tokenInput * ethReserve) / tokenReserve;

            require(requiredEth >= _minEthInput, "SLIPPAGE_TOO_HIGH");

            ethAmount = requiredEth;
            tokenAmount = _tokenInput;
        } else {
            // Compute the required token amount if we deposit the full amount of ETH
            uint256 requiredTokens = (_ethInput * tokenReserve) / ethReserve;

            require(requiredTokens >= _minTokenInput, "SLIPPAGE_TOO_HIGH");

            ethAmount = _ethInput;
            tokenAmount = requiredTokens;
        }

        // Add liquidity
        _token.transferFrom(msg.sender, address(pair), tokenAmount);
        pair.addLiquidity{value: ethAmount}(msg.sender, _deadline);

        emit LiquidityAdded(ethAmount, tokenAmount, msg.sender);
    }

    /// @notice Remove liquidity frrom a pair, setting desired slippage
    /// @param _token Address of desired ERC20 token contract
    /// @param _lpTokensInput Amount of LP tokens to remove from the pool
    /// @param _minEthOutput Minimum amount of ETH allowed to receive (defines slippage)
    /// @param _minTokenOutput Minimum amonut of tokens allowed to receive (defines slippage)
    /// @param _deadline Timestamp after which the liquidity removal becomes invalid
    function removeLiquidity(
        ERC20 _token,
        uint256 _lpTokensInput,
        uint256 _minEthOutput,
        uint256 _minTokenOutput,
        uint256 _deadline
    ) external onlyUntil(_deadline) reentrancyGuard {
        require(address(_token) != address(0), "TOKEN_REQUIRED");
        require(_lpTokensInput > 0, "LP_TOKEN_INPUT_REQUIRED");

        // Find pair from the Factory contract
        Pair pair = factory.pairs(_token);
        require(address(pair) != address(0), "PAIR_NOT_FOUND");

        // Compute user's share of the total pool
        {
            (uint256 ethReserve, uint256 tokenReserve) = pair.getReserves();
            uint256 supply = pair.totalSupply();
            uint256 ethToWithdraw = (ethReserve * _lpTokensInput) / supply;
            uint256 tokensToWithdraw = (tokenReserve * _lpTokensInput) / supply;

            require(ethToWithdraw >= _minEthOutput, "SLIPPAGE_TOO_HIGH");
            require(tokensToWithdraw >= _minTokenOutput, "SLIPPAGE_TOO_HIGH");
        }

        uint256 ethBalanceBefore = address(msg.sender).balance;
        uint256 tokenBalanceBefore = _token.balanceOf(msg.sender);

        // Transfer LP tokens and remove liquidity
        pair.transferFrom(msg.sender, address(pair), _lpTokensInput);
        pair.removeLiquidity(msg.sender, _deadline);

        // Check also the amount of ETH/tokens transferred
        uint256 ethWithdrawn = address(msg.sender).balance - ethBalanceBefore;
        uint256 tokensWithdrawn = _token.balanceOf(msg.sender) -
            tokenBalanceBefore;

        require(ethWithdrawn >= _minEthOutput, "SLIPPAGE_TOO_HIGH");
        require(tokensWithdrawn >= _minTokenOutput, "SLIPPAGE_TOO_HIGH");

        emit LiquidityRemoved(_lpTokensInput, msg.sender);
    }
}

