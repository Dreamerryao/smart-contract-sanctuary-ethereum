    }

    function changeWhiteListStatusForRound(
        uint256 _roundId,
        bool _whitelistOnly
    ) external onlyOwner {
        require(_roundId < rounds.length, "NFT Sale: round id invalid");

        Round storage round = rounds[_roundId];

        round.whitelistOnly = _whitelistOnly;

        emit ChangeRound(
            round.startTime,
            round.endTime,
            round.mintFee,
            round.supply,
            round.maxTokensPerWallet,
            round.whitelistOnly
        );
    }

    function mint() public payable whenNotPaused nonReentrant {
        _calculateRound();

        require(
            _status(lastRoundIndex) == STATUS.ACTIVE,
            "NFTSale: sale is not started yet or ended"
        );

        Round memory round = rounds[lastRoundIndex];

        require(
            tokensPerWallet[lastRoundIndex][_msgSender()] <
                round.maxTokensPerWallet,
            "NFT Sale: max tokens per wallet reached"
        );

        uint256 mintFee = msg.value;

        require(
            mintFee >= round.mintFee,
            "NFTSale: cannot enough ETH for mint"
        );

        if (mintFee > round.mintFee) {
            //refund extra
            _deliverFunds(
                _msgSender(),
                mintFee - round.mintFee,
                "NFTSale: failed transfer ETH to address"
            );
        }

        inSwap = true;
        _processVRFFee(round.mintFee);
        inSwap = false;

        _deliverFunds(
            teamAddress,
            address(this).balance,
            "NFTSale: failed transfer ETH to address"
        );

        /*
            request to VRF for mint fee
        */

        _processVRFRequest();

        round.collected += 1;
    }

    function getUserRequestsLength(address _account)
        external
        view
        returns (uint256 length)
    {
        length = userRequests[_account].length;
    }

    function getTokenIdByRequestIndex(address _account, uint256 _requestIndex)
        external
        view
        returns (RandomnessRequestData memory request)
    {
        uint256 requestID = userRequests[_account][_requestIndex];

        request = randomnessRequests[requestID];
    }

    function _calculateRound() internal {
        Round memory round = rounds[lastRoundIndex];

        if (
            (block.timestamp > round.endTime) &&
            (lastRoundIndex + 1 < rounds.length)
        ) {
            lastRoundIndex += 1;
        }
    }

    function getLastRoundIndex()
        external
        view
        returns (uint256 _lastRoundIndex)
    {
        Round memory round = rounds[lastRoundIndex];

        _lastRoundIndex = lastRoundIndex;
        if (
            (block.timestamp > round.endTime) &&
            (lastRoundIndex + 1 < rounds.length)
        ) {
            _lastRoundIndex += 1;
        }
    }

    function getRoundSupply(uint256 _roundIndex)
        public
        view
        returns (uint256 supply)
    {
        if (_roundIndex == 0) {

            Round memory round = rounds[0];

            supply = round.supply;

        } else {

            Round memory round = rounds[_roundIndex];

            supply = round.supply;

            for (uint256 j = _roundIndex - 1; j > 0; j--) {
                Round memory prevRound = rounds[j];
                supply += prevRound.supply - prevRound.collected;
            }

            Round memory firstRound = rounds[0];

            supply += firstRound.supply - firstRound.collected;
        }
    }

    function _processVRFFee(uint256 _amount) internal {
        //buy link
        address[] memory path = new address[](2);

        path[0] = WETH;
        path[1] = address(LINKTOKEN);

        (bool success, ) = payable(uniswapRouter).call{value: _amount}(
            abi.encodeWithSignature(
                "swapETHForExactTokens(uint256,address[],address,uint256)",
                linkFee,
                path,
                address(this),
                UNISWAP_ROUTER_DEADLINE + block.timestamp
            )
        );

        if (LINKTOKEN.balanceOf(address(this)) > 0) {
            LINKTOKEN.transferAndCall(
                address(COORDINATOR),
                linkFee,
                abi.encode(subscriptionId)
            );
        }
    }

    function _processVRFRequest() internal {
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            REQUEST_CONFIRMATIONS,
            CALLBACK_GAS_LIMIT,
            NUM_WORDS
        );

        RandomnessRequestData memory data = RandomnessRequestData({
            account: _msgSender(),
            roundId: lastRoundIndex,
            number: 0,
            tokenId: 0,
            fullfilled: false
        });

        randomnessRequests[requestId] = data;

        userRequests[_msgSender()].push(requestId);

        emit CreateRequest(_msgSender(), requestId);
    }

    /// sale status
    function status(uint256 _roundId) external view returns (STATUS) {
        return _status(_roundId);
    }

    function _status(uint256 _roundIndex) internal view returns (STATUS) {
        Round memory round = rounds[_roundIndex];

        if (
            (block.timestamp >= round.startTime) &&
            (block.timestamp <= round.endTime)
        ) {
            if (round.collected == getRoundSupply(_roundIndex)) {
                console.log(getRoundSupply(_roundIndex));

                return STATUS.FINISHED;
            }

            if (!round.whitelistOnly) {
                return STATUS.ACTIVE; // ACTIVE - mint enabled
            } else {
                //whitelisted
                if (whitelistAddresses[_msgSender()]) {
                    return STATUS.ACTIVE;
                } else {
                    return STATUS.FAILED;
                }
            }
        }

        if (block.timestamp > round.endTime) {
            return STATUS.FINISHED;
        }

        return STATUS.QUED; // QUED - awaiting start time
    }

    function fulfillRandomWords(
        uint256 requestId, /* requestId */
        uint256[] memory randomWords
    ) internal override {
        RandomnessRequestData storage data = randomnessRequests[requestId];
        require(!data.fullfilled, "VRF: request already processed");

        data.fullfilled = true;
        data.number = randomWords[0];

        _processWithRandomness(requestId, data);
    }

    function _processWithRandomness(
        uint256 _requestId,
        RandomnessRequestData storage _data
    ) internal {
        uint256 roundId = _data.roundId;

        address user = _data.account;

        Round storage round = rounds[roundId];

        require(
            tokensPerWallet[roundId][user] < round.maxTokensPerWallet,
            "NFT Sale: max tokens per wallet reached"
        );

        uint256 randomness = _data.number;

        uint256 batch = randomness % NUM_OF_BATCHES;

        if (!_activeBatches.contains(batch)) {
            batch = _activeBatches.at(_activeBatches.length() - 1);
        }

        uint256 supply = batchSupply[batch];

        uint256 tokenId = batch * NUM_OF_BATCHES + supply;

        ISNC(sncAddress).mint(user, tokenId);

        tokensPerWallet[roundId][user] += 1;

        batchSupply[batch] += 1;

        _data.tokenId = tokenId;
        if (batchSupply[batch] == ELEM_IN_BATCHES) {
            _activeBatches.remove(batch);
        }

        emit MintToken(roundId, user, _requestId, tokenId);
    }

