    ) external onlyOwner {
        Round memory round = Round(
            _startTime,
            _endTime,
            _mintFee,
            _supply,
            0,
            _maxTokensPerWallet,
            _whitelistOnly
        );

        rounds.push(round);

        emit AddRound(
            _startTime,
            _endTime,
            _mintFee,
            _supply,
            _maxTokensPerWallet,
            _whitelistOnly
