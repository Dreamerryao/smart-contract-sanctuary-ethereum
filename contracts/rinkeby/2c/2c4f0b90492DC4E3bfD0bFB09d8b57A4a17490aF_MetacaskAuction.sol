pragma solidity 0.8.14;

interface IKYC {
    struct KYCDetails {
        Status status;
        uint256 expire;
        bytes16 id;
        bytes32 country;
    }

    enum Status {
        ACTIVE,
        SUSPENDED,
        BLOCKED
    }

    /**
     * @notice Returns boolean value is the user verified, KYC valid, status active
     * @param account user address
     */
    function isVerified(address account) external view returns (bool result);

    /**
     * @dev Emmitted when KYC country locked
     * @param caller transaction
     */
    event Locked(address indexed caller);

    /**
     * @dev Emmitted when metacask auction contract updated
     * @param auction contract address
     */
    event MetacaskAuctionUpdated(address auction);

    /**
     * @dev Emmitted when KYC country unlocked
     * @param caller transaction
     */
    event Unlocked(address indexed caller);

    /**
     * @dev Emmitted when KYC details updated
     * @param data KYC details
     * @param holder who owns the token
     */
    event Updated(KYCDetails data, address indexed holder);

    /**
     * @dev Emmitted when verifier approved KYC user
     * @param data KYC details
     * @param countryName the name of the country
     * @param to user to whom verification was confirmed
     * @param tokenId identifier id
     */
    event Verified(KYCDetails data, string countryName, address indexed to, uint256 tokenId);
}

