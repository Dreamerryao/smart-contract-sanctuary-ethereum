pragma solidity 0.8.14;

import "./BaseERC721.sol";
import "./interfaces/IKYC.sol";
import "./interfaces/ISyntheticToken.sol";
import "./interfaces/ISyntheticProxyAdmin.sol";
import "./interfaces/IMetacaskNFT.sol";
import "./utils/Queue.sol";

/**
 * @dev {MetacaskAuction} A smart contract with auction logic.
 */
contract MetacaskAuction is Queue {
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.Bytes32Set;

    struct Lot {
        uint256 tokenId;
        address seller;
        IERC20 baseCurrency;
        uint256 reservePrice;
        uint256 step;
        uint256 startTimestamp;
        uint256 endTimestamp;
        uint256 cancelTimestamp;
        uint256 extensionTimestamp;
        bool active;
    }

    address public constant ZERO_ADDRESS = address(0);
    uint256 public constant HUNDRED_PERCENT = 10000;

    IMetacaskNFT private _nft;
    IKYC private _kyc;
    ISyntheticProxyAdmin private _syntheticProxyAdmin;

    uint256 private _lotsCount;
    uint256 private _fee;
    address private _feeRecipient;

    mapping(uint256 => Lot) private _lots;

    /**
     * @notice Returns the value of the auction commission
     */
    function fee() external view returns (uint256) {
        return _fee;
    }

    /**
     * @notice Returns the address of the commission recipient
     */
    function feeRecipient() external view returns (address) {
        return _feeRecipient;
    }

    /**
     * @notice Returns KYC contract address
     */
    function kycContract() external view returns (IKYC) {
        return _kyc;
    }

    /**
     * @notice Returns the number of lots
     */
    function lotsCount() external view returns (uint256) {
        return _lotsCount;
    }

    /**
     * @notice Returns lot data by ID
     */
    function lotItem(uint256 lotId) external view returns (Lot memory) {
        return _lots[lotId];
    }

    /**
     * @notice Returns address NFT collection
     */
    function nftContract() external view returns (IERC721) {
        return _nft;
    }

    /**
     * @notice Returns synthetic proxy admin address
     */
    function syntheticProxyAdmin() external view returns (ISyntheticProxyAdmin) {
        return _syntheticProxyAdmin;
    }

    /**
     * @dev Emmitted when tokens are distributed after the end of the auction
     * @param lot data
     * @param winner details of the auction winner
     */
    event AssetsDistributed(Lot lot, Bidder winner);

    /**
     * @dev Emmitted when auction finished
     * @param lotId identifier lot
     * @param winner details of the auction winner
     */
    event AuctionFinished(uint256 lotId, Bidder winner);

    /**
     * @dev Emmitted when bidder made a bet
     * @param lot data
     * @param bidder the account that placed the bet
     * @param newBidValue current bid
     * @param isSynthetic which token is the bet made synthetic or not
     */
    event Bidded(Lot lot, address bidder, uint256 newBidValue, bool isSynthetic);

    /**
     * @dev Emmitted when auction created
     * @param lot data
     */
    event Created(Lot lot);

    /**
     * @dev Emmitted when auction cancelled
     * @param lot data
     */
    event Canceled(Lot lot);

    /**
     * @dev Emmitted when the commissions are distributed
     * @param baseCurrency the token for which the lot was put up
     * @param amountIn the final bid that won the lot
     * @param remains the balance after the distribution of the commission
     * @param tokenId id of the sold NFT token
     */
    event FeeDistributed(IERC20 baseCurrency, uint256 amountIn, uint256 remains, uint256 tokenId);

    /**
     * @dev Emmitted when commission updated
     * @param fee the commission that will be deducted after the auction ends
     */
    event FeeUpdated(uint256 fee);

    /**
     * @dev Emmitted when commission recipient updated
     * @param recipient commission recipient
     */
    event FeeRecipientUpdated(address recipient);

    /**
     * @dev Emmitted when KYC contract updated
     * @param kyc contract
     */
    event KYCUpdated(address indexed kyc);

    /**
     * @dev Emmitted when NFT contract updated
     * @param nft contract
     */
    event NFTUpdated(address indexed nft);

    /**
     * @dev Emmitted when Synthetic proxy contract contract updated
     * @param synthetic contract
     */
    event SyntheticProxyAdminUpdated(address indexed synthetic);

    /**
     * @notice Initialize auction contract
     * @param fee_ the commission that will be deducted after the auction ends
     * @param feeRecipient_ address of the commission recipient
     * @param nft_ collection contract NFT
     * @param kyc_ contract KYC
     * @param syntheticProxyAdmin_ contract synthetic proxy admin
     * @param queueLength_ maximum length of the betting queue
     */
    constructor(
        uint256 fee_,
        address feeRecipient_,
        address nft_,
        address kyc_,
        address syntheticProxyAdmin_,
        uint256 queueLength_
    ) {
        require(feeRecipient_ != ZERO_ADDRESS, "Recipient is zero");
        require(nft_ != ZERO_ADDRESS, "NFT is zero address");
        require(kyc_ != ZERO_ADDRESS, "KYC is zero address");
        require(syntheticProxyAdmin_ != ZERO_ADDRESS, "SyntheticProxyAdmin is zero address");
        require(queueLength_ > 0, "Queue is zero");
        _fee = fee_;
        _feeRecipient = feeRecipient_;
        _nft = IMetacaskNFT(nft_);
        _kyc = IKYC(kyc_);
        _syntheticProxyAdmin = ISyntheticProxyAdmin(syntheticProxyAdmin_);
        updateQueueLength(queueLength_);
    }

    /**
     * @notice Place a bet for a token (not synthetic)
     * If the bid is made for a native token,
     * the token is sent with a method call
     *
     * @param lotId identifier lot item
     * @param value the number of tokens for the bet
     */
    function bid(uint256 lotId, uint256 value) external payable returns (bool) {
        uint256 amountIn = msg.value > 0 ? msg.value : value;
        return _bid(lotId, payable(msg.sender), amountIn, false);
    }

    /**
     * @notice The bet that is made for a synthetic token
     * Only the operator role can call the method
     *
     * @param lotId identifier lot item
     * @param bidder the account for which the bet is being made
     * @param amountIn the number of tokens for the bet
     */
    function bid(
        uint256 lotId,
        address payable bidder,
        uint256 amountIn
    ) external onlyOperator returns (bool) {
        return _bid(lotId, bidder, amountIn, true);
    }

    /**
     * @notice Put the NFT up for sale
     * It is possible only if the user has passed KYC
     *
     * @param tokenId the ID of the token offered for sale
     * @param baseCurrency the address of the token for which the lot is put up for sale
     * @param reservePrice starting price per lot
     * @param step minimum bid step
     * @param startTimestamp auction start time
     * @param endTimestamp auction end time
     * @param cancelTimestamp the timestamp before which the auction can be canceled
     * @param extensionTimestamp auction extension time if the auction is over and there are no bids
     */
    function create(
        uint256 tokenId,
        address baseCurrency,
        uint256 reservePrice,
        uint256 step,
        uint256 startTimestamp,
        uint256 endTimestamp,
        uint256 cancelTimestamp,
        uint256 extensionTimestamp
    ) external returns (bool) {
        address seller = msg.sender;
        address owner = _nft.ownerOf(tokenId);
        require(owner == seller, "Seller is not token owner");
        require(endTimestamp > startTimestamp, "EndTimestamp lte StartTimestamp");
        require(_kyc.isVerified(seller), "Seller is not verified KYC");
        _nft.transferFrom(seller, address(this), tokenId);
        Lot storage lot = _lots[_lotsCount];
        lot.tokenId = tokenId;
        lot.seller = seller;
        lot.baseCurrency = IERC20(baseCurrency);
        lot.reservePrice = reservePrice;
        lot.step = step;
        lot.startTimestamp = startTimestamp;
        lot.endTimestamp = endTimestamp;
        lot.cancelTimestamp = cancelTimestamp;
        lot.extensionTimestamp = extensionTimestamp;
        lot.active = true;
        _lotsCount++;
        emit Created(lot);
        return true;
    }

    /**
     * @notice Cancel the NFT offered for sale
     * @param lotId identifier lot item
     */
    function cancel(uint256 lotId) external returns (bool) {
        require(lotId < _lotsCount, "Lot not found");
        Lot storage lot = _lots[lotId];
        uint256 timestamp = getTimestamp();
        require(msg.sender == lot.seller, "Caller is not seller");
        require(lot.active, "Lot is not active");
        require(length(lotId) == 0, "There are active bids");
        require(timestamp <= lot.cancelTimestamp || lot.cancelTimestamp == 0, "Lot is active");
        _nft.transferFrom(address(this), lot.seller, lot.tokenId);
        lot.active = false;
        emit Canceled(lot);
        return true;
    }

    /**
     * @notice Update fee auction
     * @param value new auction commission
     */
    function updateFee(uint256 value) external onlyOwner returns (bool) {
        _fee = value;
        emit FeeUpdated(value);
        return true;
    }

    /**
     * @notice Update fee recipient auction
     * @param recipient new address fee recipient
     */
    function updateFeeRecipient(address recipient) external onlyOwner returns (bool) {
        _feeRecipient = recipient;
        emit FeeRecipientUpdated(recipient);
        return true;
    }

    /**
     * @notice Update NFT contract address
     * @param nft new NFT contract address
     */
    function updateNft(address nft) external onlyOwner returns (bool) {
        require(nft != ZERO_ADDRESS, "NFT is zero address");
        _nft = IMetacaskNFT(nft);
        emit NFTUpdated(nft);
        return true;
    }

    /**
     * @notice Update KYC contract address
     * @param kyc new KYC contract address
     */
    function updateKYC(address kyc) external onlyOwner returns (bool) {
        require(kyc != ZERO_ADDRESS, "KYC is zero address");
        _kyc = IKYC(kyc);
        emit KYCUpdated(kyc);
        return true;
    }

    /**
     * @notice Update synthetic proxy admin contract address
     * @param synthetic new synthetic proxy admin address
     */
    function updateSyntheticProxyAdmin(address synthetic) external onlyOwner returns (bool) {
        require(synthetic != ZERO_ADDRESS, "SyntheticProxyAdmin is zero address");
        _syntheticProxyAdmin = ISyntheticProxyAdmin(synthetic);
        emit SyntheticProxyAdminUpdated(synthetic);
        return true;
    }

    /**
     * @notice End the auction
     * @param lotId identifier lot item
     */
    function finishAuction(uint256 lotId) external returns (bool) {
        require(lotId < _lotsCount, "Lot not found");
        Lot storage lot = _lots[lotId];
        uint256 timestamp = getTimestamp();
        uint256 bidsLength = length(lotId);
        require(lot.active, "Lot is not active");
        if (bidsLength > 0) {
            require(timestamp >= lot.endTimestamp, "Lot is active");
        } else {
            require(
                timestamp >= lot.endTimestamp && timestamp >= lot.extensionTimestamp,
                "Extension timestamp timeout"
            );
            _nft.safeTransferFrom(address(this), lot.seller, lot.tokenId);
            return true;
        }
        Bidder[] memory bids = bidItems(lotId, 0, bidsLength);
        Bidder memory winner = _distributeAssets(bids, lot, false);
        lot.active = false;
        emit AuctionFinished(lotId, winner);
        return true;
    }

    /**
     * @notice Complete the auction before it ends and return all funds to bidders,
     * as well as to the NFT seller
     *
     * @param lotId identifier lot item
     */
    function rejectAuction(uint256 lotId) external nonReentrant onlyOperator returns (bool) {
        require(lotId < _lotsCount, "Lot not found");
        Lot storage lot = _lots[lotId];
        uint256 bidsLength = length(lotId);
        require(lot.active, "Lot is not active");
        Bidder[] memory bids = bidItems(lotId, 0, bidsLength);
        Bidder memory winner = _distributeAssets(bids, lot, true);
        lot.active = false;
        emit AuctionFinished(lotId, winner);
        return true;
    }

    /**
     * @notice Private method for betting
     * @param lotId identifier lot item
     * @param bidder the account for which the bet is being made
     * @param amountIn the number of tokens for the bet
     * @param isSynthetic which token is the bet made synthetic or not
     */
    function _bid(
        uint256 lotId,
        address payable bidder,
        uint256 amountIn,
        bool isSynthetic
    ) private returns (bool) {
        require(_kyc.isVerified(bidder), "Bidder is not verified KYC");
        require(lotId < _lotsCount, "Lot not found");
        Lot storage lot = _lots[lotId];
        uint256 timestamp = getTimestamp();
        require(lot.active, "Lot is not active");
        require(amountIn > 0, "New bid is not positive");
        require(amountIn >= lot.reservePrice, "Bid lt reserve price");
        require(timestamp >= lot.startTimestamp, "Auction has not started");
        Bidder memory bidderData = bidderDataByAccount(lotId, bidder);
        uint256 bidsLength = length(lotId);
        uint256 lastBidValue;
        if (bidsLength > 0) {
            require(timestamp <= lot.endTimestamp, "Auction ended");
            lastBidValue = bidItem(lotId, bidsLength - 1).value;
        } else {
            require(
                timestamp <= lot.endTimestamp || timestamp <= lot.extensionTimestamp,
                "Extension timestamp timeout"
            );
        }
        uint256 newBidValue = lastBidValue + amountIn;
        require(newBidValue >= lastBidValue + lot.step, "New bid lt min step");
        if (isSynthetic) {
            ISyntheticToken synthetic = ISyntheticToken(
                _syntheticProxyAdmin.syntheticByToken(address(lot.baseCurrency))
            );
            require(address(synthetic) != ZERO_ADDRESS, "Synthetic is zero address");
            uint256 limit = synthetic.limitSyntheticByAddress(bidder);
            require(bidderData.value + amountIn <= limit, "Bet limit exceeded");
            if (!bidderData.isSynthetic) withdraw(bidder, bidderData.value, lot.baseCurrency);
        } else {
            uint256 accountBid = bidderData.isSynthetic ? newBidValue : newBidValue - bidderData.value;
            if (address(lot.baseCurrency) == ZERO_ADDRESS) {
                require(msg.value == accountBid, "Native coin bid insufficient");
            } else {
                IERC20(lot.baseCurrency).transferFrom(bidder, address(this), accountBid);
            }
        }
        add(lotId, bidder, lot.baseCurrency, newBidValue, isSynthetic);
        emit Bidded(lot, bidder, newBidValue, isSynthetic);
        return true;
    }

    /**
     * @notice Distribute funds after the auction ends or reject auction
     * @param bids array data bids
     * @param lot data
     * @param isReject boolean value that indicates that the auction is being forcibly terminated
     */
    function _distributeAssets(
        Bidder[] memory bids,
        Lot memory lot,
        bool isReject
    ) private returns (Bidder memory winner) {
        for (uint256 i = bids.length - 1; i >= 0; i--) {
            if (i == 0 && (bids[i].status == PaymentState.FAIL || isReject)) {
                _nft.safeTransferFrom(address(this), lot.seller, lot.tokenId);
            }
            if (!bids[i].isSynthetic) {
                if (bids[i].status == PaymentState.FAIL || winner.account != ZERO_ADDRESS || isReject) {
                    withdraw(bids[i].account, bids[i].value, lot.baseCurrency);
                } else {
                    uint256 sellerAmount = _distributeFee(lot.baseCurrency, lot.tokenId, bids[i].value);
                    _nft.safeTransferFrom(address(this), bids[i].account, lot.tokenId);
                    withdraw(payable(lot.seller), sellerAmount, lot.baseCurrency);
                    winner = bids[i];
                }
            } else if (bids[i].status == PaymentState.SUCCESS && winner.account == ZERO_ADDRESS && !isReject) {
                _nft.safeTransferFrom(address(this), bids[i].account, lot.tokenId);
                winner = bids[i];
            }
        }
        emit AssetsDistributed(lot, winner);
    }

    /**
     * @notice Calculate and distribute the commission at the end of the auction
     * @param baseCurrency the token for which the lot was purchased
     * @param tokenId the NFT token that was sold
     * @param amountIn the winning amount of the bet
     */
    function _distributeFee(
        IERC20 baseCurrency,
        uint256 tokenId,
        uint256 amountIn
    ) private returns (uint256 remains) {
        uint256 feeAuction = (amountIn * _fee) / HUNDRED_PERCENT;
        remains = amountIn - feeAuction;
        withdraw(payable(_feeRecipient), feeAuction, baseCurrency);
        IMetacaskNFT.Commission[] memory commissions = _nft.commissionsByTokenId(tokenId);
        for (uint256 i = 0; i < commissions.length; i++) {
            uint256 commission = (amountIn * commissions[i].amount) / HUNDRED_PERCENT;
            if (commission > remains) revert("Commission amount gt hundred percent");
            remains -= commission;
            withdraw(payable(commissions[i].receiver), commission, baseCurrency);
        }
        emit FeeDistributed(baseCurrency, amountIn, remains, tokenId);
    }

    /**
     * @notice Сurrent timestamp
     */
    function getTimestamp() internal view virtual returns (uint256) {
        return block.timestamp;
    }
}

