     * @notice Returns the allowed amount of synthetic tokens to bid by account
     * @param account for get balance
     */
    function limitSyntheticByAddress(address account) external view returns (uint256);

    /**
     * @notice Returns decimlas synthetic token
     */
    function decimals() external view returns (uint256);

    /**
     * @notice Mint token, and set max value for a bet
     * @dev Creates `amount` new tokens for `to`, of token type `id`
     *
     * See {ERC721-_mint}
     *
     * Requirements:
     *
     * - the caller must have the `ADMIN_ROLE`
     * @param to address to whom will be mint
     * @param value threshold value of tokens that are allowed to be used in stakes
     */
    function mint(address to, uint256 value) external returns (uint256 id);

    /**
     * @notice Update user limit for bid threshold
     * @dev For success works:
     *  - The user must have an nft token
     */
    function updateLimit(address to, uint256 value) external returns (bool);
}

