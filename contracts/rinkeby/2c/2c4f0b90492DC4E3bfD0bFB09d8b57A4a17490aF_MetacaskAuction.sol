pragma solidity 0.8.14;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "../access/Operator.sol";

/**
 * @dev {Queue} Smart contract for working with a queue of bidders:
 *
 * This is an abstract contract that allows you to work with queues of bidders
 * The maximum queue length is configurable
 * The maximum bid is counted as the last index in the array (MAX_BID = _bids[lotId][_bids[lotId].length - 1])
 */
abstract contract Queue is Operator, ReentrancyGuard {
    struct Bidder {
        address payable account;
        uint256 value;
        IERC20 tokenIn;
        bool isSynthetic;
        PaymentState status;
    }

    enum PaymentState {
        PROCESSING,
        SUCCESS,
        FAIL
    }

    uint256 private _queueLength;

    mapping(uint256 => Bidder[]) private _bids;
    mapping(uint256 => mapping(address => uint256)) private _bidIndexes;

    /**
     * @notice Returns bid item data
     * @param lotId identifier lot item
     * @param index lot
     */
    function bidItem(uint256 lotId, uint256 index) public view returns (Bidder memory bidData) {
        if (length(lotId) > index) bidData = _bids[lotId][index];
    }

    /**
     * @notice Returns bid items data array
     * @param lotId identifier lot item
     * @param offset number of missing elements
     * @param limit what is the number of elements to output
     */
    function bidItems(
        uint256 lotId,
        uint256 offset,
        uint256 limit
    ) public view returns (Bidder[] memory bidsData) {
        uint256 bidsLength = _bids[lotId].length;
        if (offset >= bidsLength) return new Bidder[](0);
        uint256 to = offset + limit;
        if (bidsLength < to) to = bidsLength;
        bidsData = new Bidder[](to - offset);
        for (uint256 i = 0; i < bidsData.length; i++) bidsData[i] = _bids[lotId][offset + i];
    }

    /**
     * @notice Returns bid data by account
     * @param lotId identifier lot item
     * @param account for which will be provided bid data
     */
    function bidderDataByAccount(uint256 lotId, address account) public view returns (Bidder memory bidData) {
        uint256 index = _bidIndexes[lotId][account];
        if (index > 0) bidData = _bids[lotId][index - 1];
    }

    /**
     * @notice Returns boolean value if bid by account contains
     * @param lotId identifier lot item
     * @param account of the bidder being checked
     */
    function contains(uint256 lotId, address account) public view returns (bool) {
        return _bidIndexes[lotId][account] != 0;
    }

    /**
     * @notice Returns count bidders to lot
     * @param lotId identifier lot item
     */
    function length(uint256 lotId) public view returns (uint256) {
        return _bids[lotId].length;
    }

    /**
     * @notice Returns max queue length for bids
     */
    function queueLength() public view returns (uint256) {
        return _queueLength;
    }

    /**
