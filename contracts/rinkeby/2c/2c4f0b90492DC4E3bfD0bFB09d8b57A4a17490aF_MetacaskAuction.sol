pragma solidity 0.8.14;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

/**
 * @dev {BaseERC721} including:
 *
 *  - opportunity for holders to burn their tokens
 *  - uri setter role allowing uri to be updated
 *
 * This contract uses {AccessControlEnumerable} to lock permissioned functions using the
 * different roles.
 *
 * The account on which the contract is deployed will receive and a pause
 * roles, as well as the default administrator role, which allows
 * and suspend roles for other accounts.
 */
contract BaseERC721 is ERC721Enumerable, ERC721Burnable, AccessControlEnumerable {
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIds;

    /**
     * @notice Returns last token id number
     */
    function tokenIds() external view returns (uint256) {
        return _tokenIds.current();
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable, AccessControlEnumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @notice Initializes BaseERC721
     * @dev Grants `DEFAULT_ADMIN_ROLE`
     * to the account that deploys the contract
     * @param name_ name of the token
     * @param symbol_ symbol of the token
     */
    constructor(string memory name_, string memory symbol_) ERC721(name_, symbol_) {
        address deployer = msg.sender;
        _grantRole(DEFAULT_ADMIN_ROLE, deployer);
    }

    /**
     * @dev See {ERC721Burnable}
     */
    function _burn(uint256 tokenId) internal virtual override {
        super._burn(tokenId);
    }

    /**
     * @notice Mint token
     * @dev Creates `amount` new tokens for `to`, of token type `id`
     *
     * See {ERC721-_mint}
     *
     * @param to address to whom will be mint
     */
    function _mint(address to) internal virtual returns (uint256 id) {
        _tokenIds.increment();
        id = _tokenIds.current();
        _mint(to, id);
    }

    /**
     * @notice See {ERC721Supply-_beforeTokenTransfer}
     * @dev For success works:
     *  - The contract should be not paused
     *  - The address to whom the mint should be verified
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }
}

