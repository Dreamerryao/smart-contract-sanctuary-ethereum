contract Election is AccessControl {
    using Base64 for bytes;
    mapping(uint256 => uint256) private voterCount;
    mapping(uint256 => Contestant[]) private contestants;
    mapping(uint256 => mapping(address => Voter)) private registeredVoters;
    mapping(uint256 => ElectionData) private electionMetaData;
    uint256[] private activeElections;
    uint256[] private unReleasedElections;
    uint256[] private awaitingElections;
    uint256 private _electionIds = 0;
    struct Contestant {
        bytes32 name;
        uint256 voteCount;
        bool isQualified;
    }

    struct Voter {
        bool hasVoted;
        uint256 weight;
        uint256 vote;
        address delegate;
    }

    struct ElectionData {
        string data;
        uint16 category;
        bool isTakingVotes;
        bool isResultReleased;
        uint256 minEndDate;
        uint256 duration;
        address creator;
    }

    event ElectionCreated(uint256 id, address creator);
    event RegistrationStatus(uint256 id, address voter, bool isSuccess);

    /**
     * @param _data strigified json metadata of the election
     * @param _duration  the length of election in minutes
     * @param category the category the election would belong to.
     *                 check accessControl contract for possible values
     */
    function createElection(
        string calldata _data,
        uint256 _duration,
        uint16 category
    ) external onlyAuthorizedCreator(category) {
        _electionIds++;
        uint256 newElectionId = _electionIds;

        ElectionData memory electionData_ = ElectionData(
            _data,
            category,
            false,
            false,
            0,
            _duration,
            msg.sender
        );
        electionMetaData[newElectionId] = electionData_;
        awaitingElections.push(newElectionId);
        emit ElectionCreated(newElectionId, msg.sender);
    }

    /**
     *@notice if election minEndDate is 0, chairperson  has not started election.Also 
              if election.isTakingVotes is true election has already started.
     
     
     *@param electionId Id of the election contestants would be added to.
     *@param contestantNames The name of contestants to be added to election with Id electionId
     */
    function addContestants(
        bytes32[] memory contestantNames,
        uint256 electionId
    ) public whenNotPaused onlyCreator(electionId) {
        require(
            !electionMetaData[electionId].isTakingVotes &&
                electionMetaData[electionId].minEndDate == 0,
            "Election:operation closed."
        );

        for (uint256 i = 0; i < contestantNames.length; i++) {
            if (contestantNames[i] == 0) continue;
            contestants[electionId].push(
                Contestant({
                    name: contestantNames[i],
                    voteCount: 0,
                    isQualified: true
                })
            );
        }
    }

    /**
     * @notice Voters that do not meet election access requirement would be skipped.
     * @param voters The addresses of allowed voters.
     * @param electionId The Id of election to add voters to.
     */
    function registerVoters(address[] memory voters, uint256 electionId)
        public
        whenNotPaused
        onlyCreator(electionId)
    {
        _registerVoters(voters, electionId);
    }

    /**
     * @notice election must have registered voters and contestants before start.
     * @param electionId The Id of election to start
     */
    function startElection(uint256 electionId)
        public
        onlyChairperson
        whenNotPaused
        onlyValidElection(electionId)
    {
        require(
            !electionMetaData[electionId].isTakingVotes,
            "Election: Already started"
        );
        require(contestants[electionId].length > 0, "Election:No contestants");
        require(voterCount[electionId] > 0, "Election:No Voters");
        electionMetaData[electionId].isTakingVotes = true;
        electionMetaData[electionId].minEndDate =
            block.timestamp +
            electionMetaData[electionId].duration *
            60 seconds;
        uint256 len = awaitingElections.length;
        for (uint256 i = 0; i < len; i++) {
            if (awaitingElections[i] != electionId) continue;

            activeElections.push(awaitingElections[i]);
            awaitingElections[i] = awaitingElections[len - 1];
            awaitingElections.pop();
            break;
        }
    }

    /**
     * @notice contestantIndex is the index of contestant in the contestant array
     * @param contestantIndex contestant Id in the particular election with Id electionId
     * @param electionId election Id of the election user wants to vote.
     */
    /**
     *@notice If isTakingVotes property of electionMetaData is true election have started

     */
    function vote(uint256 contestantIndex, uint256 electionId)
        public
        whenNotPaused
        onlyValidElection(electionId)
        onlyQualifiedCandidate(electionId, contestantIndex)
    {
        require(
            electionMetaData[electionId].isTakingVotes &&
                electionMetaData[electionId].minEndDate != 0,
            "Election:Not in progress."
        );

        require(
            registeredVoters[electionId][msg.sender].weight > 0,
            "Election:Not registered."
        );

        require(
            !registeredVoters[electionId][msg.sender].hasVoted,
            "Election:Already voted."
        );

        for (uint256 i = 0; i < contestants[electionId].length; i++) {
            if (contestantIndex == i) {
                contestants[electionId][i].voteCount++;
                registeredVoters[electionId][msg.sender].hasVoted = true;
            }
        }
    }

    /**
     * @notice election can only be ended after the required election minEndDate is reached.
     * @param electionId Id of election that chairperson wants to end.
     * @param _releaseResult should be true if election be released too else false.
     */
    function endElection(uint256 electionId, bool _releaseResult)
        public
        onlyChairperson
        whenNotPaused
        validateElection(electionId)
    {
        electionMetaData[electionId].isTakingVotes = false;
        uint256 len = activeElections.length;
        for (uint256 i = 0; i < len; i++) {
            if (activeElections[i] != electionId) continue;

            activeElections[i] = activeElections[len - 1];
            activeElections.pop();
            break;
        }

        if (_releaseResult)
            electionMetaData[electionId].isResultReleased = true;
        else unReleasedElections.push(electionId);
    }

    /**
     * @notice election must be ended before election results could be released
     * @dev allows users view results of the election with Id electionId
     * @param electionId electionId of election to be released
     */
    function releaseResult(uint256 electionId)
        public
        onlyChairperson
        whenNotPaused
        onlyValidElection(electionId)
    {
        require(
            electionMetaData[electionId].minEndDate != 0 &&
                !electionMetaData[electionId].isTakingVotes,
            "Election: Election not ended."
        );
        electionMetaData[electionId].isResultReleased = true;
        uint256 len = unReleasedElections.length;
        for (uint256 i = 0; i < len; i++) {
            if (unReleasedElections[i] != electionId) continue;
            unReleasedElections[i] = unReleasedElections[len - 1];
            unReleasedElections.pop();
            break;
        }
    }

    /**
     * @notice New owner must meet access level requirement for election.
     * @dev transfers ownership of an election to the new owner
     * @param electionId the election Id of the election that ownership would be transfered
     * @param newOwner the address ownership is transferred to.
     */
    function transferOwnership(uint256 electionId, address newOwner)
        public
        onlyCreator(electionId)
    {
        _isNewOwnerAuthorized(electionMetaData[electionId].category, newOwner);
        electionMetaData[electionId].creator = newOwner;
    }

    /** 
    @notice This function gets the metadata of an election.
    */
    function getElectionMetaData(uint256 electionId)
        public
        view
        onlyValidElection(electionId)
        returns (string memory)
    {
        string memory metadata = electionMetaData[electionId].data;
        bytes memory jsonBytes = bytes(string(abi.encodePacked(metadata)));
        string memory jsonString = jsonBytes.encode();

        return
            string(
                abi.encodePacked("data:application/json;base64,", jsonString)
            );
    }

    /** 
    @notice This function gets the name of a contestant.
    @param electionId the Id of the election
    @param index the index of the contestant in the elections contestant array
    */
    function getContestant(uint256 electionId, uint256 index)
        public
        view
        onlyValidElection(electionId)
        returns (bytes32)
    {
        if (index >= contestants[electionId].length) return 0;

        return contestants[electionId][index].name;
    }

    /** 
    @notice This function gets the electionIds of ongoing elections.
    */
    function getActiveElections() public view returns (uint256[] memory) {
        return activeElections;
    }

    /** 
    @notice This function gets the electionIds of elections awaiting approval.
    */
    function getAwaitingElections() public view returns (uint256[] memory) {
        return awaitingElections;
    }

    /** 
    @notice This function gets the electionIds of elections that have being ended but results are not released.
    */
    function getUnReleasedElections() public view returns (uint256[] memory) {
        return unReleasedElections;
    }

    /** 
    @notice This function gets the number of contestants for an election.
    @param electionId the Id of the election
    */
    function getContestantCount(uint256 electionId)
        public
        view
        onlyValidElection(electionId)
        returns (uint256)
    {
        return contestants[electionId].length;
    }

    /** 
    @notice This function gets the electionIds of completed elections (elections that results are released).
    */
    function getCompletedElections() public view returns (uint256[] memory) {
        uint256[] memory releasedElections = new uint256[](_electionIds);
        for (uint256 i = 1; i <= _electionIds; i++) {
            if (electionMetaData[i].isResultReleased)
                releasedElections[i - 1] = i;
        }
        return releasedElections;
    }

    /** 
    @notice This function gets the number of registered voters of an election.
    @param electionId the ID of the election.
    */
    function getRegisteredVotersCount(uint256 electionId)
        public
        view
        onlyValidElection(electionId)
        returns (uint256)
    {
        return voterCount[electionId];
    }

    /** 
    @notice This function gets the votecount of a contestant.
    @param electionId the ID of the election.
    @param index the index of the contestant in the contestants array
    */
    function getContestantVoteCount(uint256 electionId, uint256 index)
        public
        view
        onlyValidElection(electionId)
        returns (uint256)
    {
        require(
            index < contestants[electionId].length,
            "Election:contestant not exist."
        );
        return contestants[electionId][index].voteCount;
    }

    /** 
    @notice This function gets the winner of an election.
    @param electionId the ID of the election your getting the winner of.
    */
    function getWinnerName(uint256 electionId)
        public
        view
        onlyValidElection(electionId)
        returns (bytes32 winnerName_)
    {
        require(
            electionMetaData[electionId].isResultReleased,
            "Election:Awaiting result"
        );
        winnerName_ = contestants[electionId][_getWinner(electionId)].name;
    }

    function getElectionMinEndDate(uint256 electionId)
        public
        view
        onlyValidElection(electionId)
        returns (uint256)
    {
        return electionMetaData[electionId].minEndDate;
    }

    /** 
    @notice This function gets the winner of an election.
    @param electionId the ID of the election your getting the winner of.
    */
    function _getWinner(uint256 electionId)
        private
        view
        onlyValidElection(electionId)
        returns (uint256 winningContestant)
    {
        uint256 winningVoteCount = 0;
        for (uint256 p = 0; p < contestants[electionId].length; p++) {
            if (contestants[electionId][p].voteCount > winningVoteCount) {
                winningVoteCount = contestants[electionId][p].voteCount;
                winningContestant = p;
            }
        }
    }

    function _registerVoters(address[] memory voters, uint256 electionId)
        private
        onlyCreator(electionId)
    {
        _requireElectionNotStarted(electionId);
        (electionId);
        for (uint256 i = 0; i < voters.length; i++) {
            if (
                voters[i] == address(0) ||
                registeredVoters[electionId][voters[i]].hasVoted ||
                registeredVoters[electionId][voters[i]].weight != 0 ||
                _isVoterNotAuthorized(
                    electionMetaData[electionId].category,
                    voters[i]
                )
            ) {
                emit RegistrationStatus(electionId, voters[i], false);
                continue;
            }

            registeredVoters[electionId][voters[i]].weight = 1;
            voterCount[electionId] = voterCount[electionId] + 1;
            emit RegistrationStatus(electionId, voters[i], true);
        }
    }

    modifier validateElection(uint256 electionId) {
        _requireValidElection(electionId);
        require(
            electionMetaData[electionId].minEndDate != 0,
            "Election:Not started."
        );

        require(
            electionMetaData[electionId].minEndDate != 0 &&
                electionMetaData[electionId].isTakingVotes,
            "Election: Already ended"
        );

        require(
            block.timestamp >= electionMetaData[electionId].minEndDate,
            "Election:minEndDate not reached."
        );

        _;
    }

    /**
     *@notice requires that an election is not started.
     */
    function _requireElectionNotStarted(uint256 electionId) private view {
        require(
            electionMetaData[electionId].minEndDate == 0 &&
                !electionMetaData[electionId].isTakingVotes,
            "Election: Already started"
        );
    }

    /**
     *@notice requires that an election with Id electionId exists.
     */
    function _requireValidElection(uint256 electionId) private view {
        require(
            electionId <= _electionIds && electionId != 0,
            "Election: Election not exist."
        );
    }

    /**
     *@notice requires that an election with Id electionId exists.
     */
    modifier onlyValidElection(uint256 electionId) {
        _requireValidElection(electionId);
        _;
    }

    modifier onlyCreator(uint256 electionId) {
        _requireValidElection(electionId);
        require(
            msg.sender == chairPerson ||
                msg.sender == electionMetaData[electionId].creator,
            "Election: Not Authorized."
        );
        _;
    }

    /** 
    @dev This function disqualifies a candidate.
    @param electionId the ID of the election.
    @param index the index of the contestant you are disqualifying
    */
    function disqualifyContestant(uint256 electionId, uint256 index)
        public
        whenNotPaused
        onlyCreator(electionId)
        onlyValidElection(electionId)
    {
        require(
            index < contestants[electionId].length,
            "Election:contestant not exist."
        );
        contestants[electionId][index].isQualified = false;
        contestants[electionId][index].voteCount = 0;
    }

    /** 
    @notice This function requires the candidate hasn't been disqualified.
    @param electionId the ID of the election you are checking.
    @param index the index of the contestant you are checking
    */
    modifier onlyQualifiedCandidate(uint256 electionId, uint256 index) {
        require(
            contestants[electionId][index].isQualified,
            "This candidate has been disqualified!"
        );
        _;
    }
}

