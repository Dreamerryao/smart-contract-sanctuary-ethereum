// We first import some OpenZeppelin Contracts.

// We inherit the contract we imported. This means we'll have access
// to the inherited contract's methods.
contract ZuriCertificate is ERC721 {
    // Magic given to us by OpenZeppelin to help us keep track of tokenIds.
    using Counters for Counters.Counter;
    using Base64 for bytes;

    Counters.Counter private certificateIds;
    mapping(uint256 => string) private certificateMetaData;

    constructor() ERC721("ZuriCertificate", "ZCRT") {
        console.log("This is my NFT contract. Woah!");
    }

    // A function our user will hit to get their NFT.
    function graduateStudents(
        address electionContract,
        address[] memory addresses,
        string[] memory metaData
    ) public {
        IElection election = IElection(electionContract);
        election.removeStudent(addresses);

        // Actually mint certificates to students
        for (uint256 i; i < addresses.length; i++) {
            // Get the current tokenId, this starts at 0.
            uint256 newItemId = certificateIds.current();
            _safeMint(addresses[i], newItemId);
            certificateMetaData[newItemId] = metaData[i];
            certificateIds.increment();
        }
    }

    function getCurrentCertificateId() public view returns (uint256) {
        return certificateIds.current();
    }

    function tokenURI(uint256 certificateId)
        public
        view
        override(ERC721)
        returns (string memory)
    {
        string memory metadata = certificateMetaData[certificateId];
        bytes memory jsonBytes = bytes(string(abi.encodePacked(metadata)));
        string memory jsonString = jsonBytes.encode();

        return
            string(
                abi.encodePacked("data:application/json;base64,", jsonString)
            );
    }
}