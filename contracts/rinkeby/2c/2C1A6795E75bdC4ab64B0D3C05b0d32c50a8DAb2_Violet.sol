/**
 *  @title Violet
 *  @author NicolÃ¡s Acosta (nicoacosta.eth) @0xnico_
 *          linktr.ee/nicoacosta.eth
 *  @notice Jadyn Violet Genesis Collection
 *          NFT ERC721.
 *          Different editions will be added over time.
 *          Each edition will have its own contract implementing token minting and token URIs.
 *  @dev Inherits from @openzeppelin/contracts `ERC721` and `Ownable`
 *       Delegates each edition minting and URIs to other contracts implementing `IVioletEdition`
 */
contract Violet is ERC721, Ownable, IViolet {
    /// ------------
    /// 1. Events
    /// ------------

    /// @notice A Violet edition was added
    /// @param editionId Edition id
    /// @param editionContract Edition contract address
    event EditionAdded(uint256 editionId, address editionContract);

    /// @notice Edition of a new token minted
    /// @param token Token id
    /// @param edition Edition id
    event TokenEdition(uint256 token, uint256 edition);

    /// @notice Collection metadata was updated
    /// @param oldURI Old metadata IPFS URI
    /// @param newURI New metadata IPFS URI
    event MetadataUpdated(string oldURI, string newURI);

    /// @notice ETH was received
    /// @param from sender
    /// @param amount ETH amount received
    event ETHReceived(address from, uint256 amount);

    /// ---------------------
    /// 2. State variables
    /// ---------------------

    /// @notice Artist address
    address private constant ARTIST =
        0xB5D94cDD855f587b5f90C7699e6018419Ee02571; // jadynviolet.eth

    /// @notice Developer address
    address private constant DEV = 0x623236cC70D5EF2c0233d13EC906b6E69658a5e4; // nicoacosta.eth

    /// @notice Collection editions' contract addresses
    /// @dev Edition id => Edition contract implementing IVioletEdition
    mapping(uint256 => IVioletEdition) private _editions;

    /// @notice Edition of each token
    /// @dev Token id => Edition id
    mapping(uint256 => uint256) private _tokenEdition;

    /// @notice Collection metadata IPFS URI
    string private _collectionURI;

    /// @notice Last minted token id
    uint256 private _lastId;

    /// @notice Addresses for ETH and tokens withdrawal
    address private immutable _withdrawalAddress1;
    address private immutable _withdrawalAddress2;

    /// -----------------
    /// 3. Constructor
    /// -----------------

    constructor(
        // uint256 _firstEditionMintingStartDate,
        // string memory _firstEditionURI,
        address withdrawalAddress1_,
        address withdrawalAddress2_
    ) ERC721("Violet", "VIOLET") {
        // Mint first edition tokens for artist and dev
        _mintToken(1, ARTIST, 1);
        _mintToken(1, DEV, 2);
        _mintToken(1, ARTIST, 3);
        _mintToken(1, ARTIST, 4);

        // Set last id to #4. First public minting call will mint #5
        _lastId = 4;

        // Set withdrawal addresses
        _withdrawalAddress1 = withdrawalAddress1_;
        _withdrawalAddress2 = withdrawalAddress2_;

        // VioletFirstEdition firstEdition = new VioletFirstEdition(
        //     address(this),
        //     msg.sender,
        //     _firstEditionMintingStartDate,
        //     _firstEditionURI,
        //     withdrawalAddress1_,
        //     withdrawalAddress2_
        // );

        // addEdition(1, address(firstEdition));
    }

    /// -----------------------
    /// 4. ERC721 extensions
    /// -----------------------

    /// @notice Total amount of tokens minted so far
    /// @return totalSupply Total amount of tokens minted so far
    function totalSupply() external view returns (uint256) {
        return _lastId;
    }

    /// @notice Checks if token has been minted.
    /// @dev Returns ERC721's internal `_exists`
    /// @param tokenId Token id
    /// @return Bool: whether the token has been minted or not
    function exists(uint256 tokenId) external view returns (bool) {
        return _exists(tokenId);
    }

    /// --------------
    /// 5. Editions
    /// --------------

    /// @notice Returns whether an edition has been released
    /// @param editionId Edition id
    /// @return bool whether an edition has been released
    function editionExists(uint256 editionId) public view returns (bool) {
        // If edition does not exist, _editions[editionId] would return address(0)
        return address(_editions[editionId]) != address(0);
    }

    /// @notice If edition exists, returns edition contract
    /// @param editionId Edition id
    /// @return _editionContract edition contract
    function editionContract(uint256 editionId)
        external
        view
        returns (address _editionContract)
    {
        _editionContract = address(_editions[editionId]);
        require(
            _editionContract != address(0),
            "Violet: Contract query for nonexistent edition"
        );
    }

    /// @notice Adds edition contract for minting and token uris
    /// @param id Edition id
    /// @param contract_ Edition contract address
    function addEdition(uint256 id, address contract_) public onlyOwner {
        _editions[id] = IVioletEdition(contract_);
        emit EditionAdded(id, contract_);
    }

    /// @notice Returns the edition of a token
    /// @param tokenId Token id
    /// @return tokenEdition Edition id
    function tokenEdition(uint256 tokenId) external view returns (uint256) {
        require(_exists(tokenId), "Violet: URI query for nonexistent token");

        return _tokenEdition[tokenId];
    }

    /// @notice Reverts if caller is not the edition contract
    /// @param editionId Edition id
    modifier onlyEditionContract(uint256 editionId) {
        require(
            msg.sender == address(_editions[editionId]),
            "Violet: Caller is not the edition contract"
        );
        _;
    }

    /// -------------
    /// -------------
    /// 6. Minting
    /// -------------
    /// -------------

    /// ------------------------
    /// 6.A. Public minting
    /// ------------------------

    /// @notice Mints a new token
    /// @dev Reverts if caller is not the edition contract
    /// @param editionId Edition id
    /// @param to Token recipient
    /// @return id Minted id
    function mintViolet(uint256 editionId, address to)
        external
        override
        onlyEditionContract(editionId)
        returns (uint256 id)
    {
        _lastId++;

        _mintToken(editionId, to, _lastId);

        return _lastId;
    }

    /// @notice Mints new tokens
    /// @dev Reverts if caller is not the edition contract
    /// @param editionId Edition id
    /// @param to Token recipient
    /// @param amount Amount of tokens to mint
    /// @return id First minted id
    function mintViolets(
        uint256 editionId,
        address to,
        uint256 amount
    ) external override onlyEditionContract(editionId) returns (uint256 id) {
        id = _lastId;

        for (uint256 i = 1; i <= amount; i++) {
            _mintToken(editionId, to, id + i);
        }

        _lastId = id + amount;
    }

    /// --------------------------
    /// 6.B. Internal minting
    /// --------------------------

    /// @notice Mints new token and save its edition
    /// @param _editionId Edition id
    /// @param _to Token recipient
    /// @param _tokenId Token id
    function _mintToken(
        uint256 _editionId,
        address _to,
        uint256 _tokenId
    ) private {
        _safeMint(_to, _tokenId);
        _tokenEdition[_tokenId] = _editionId;
        emit TokenEdition(_tokenId, _editionId);
    }

    /// --------------
    /// --------------
    /// 7. Metadata
    /// --------------
    /// --------------

    /// ----------------------
    /// 7.A. Contract URI
    /// ----------------------

    /// @notice Collection metadata IPFS URI
    /// @return string Collection metadata IPFS URI
    function contractURI() external view returns (string memory) {
        return _collectionURI;
    }

    /// @notice Set/update collection metadata IPFS URI
    /// @param newURI a parameter just like in doxygen (must be followed by parameter name)
    function setContractURI(string memory newURI) external onlyOwner {
        emit MetadataUpdated(_collectionURI, newURI);
        _collectionURI = newURI;
    }

    /// -------------------
    /// 7.B. Token URI
    /// -------------------

    /// @notice Returns a token metadata IPFS URI
    /// @dev Calls token's edition contract to get its metadata
    /// @param tokenId Token id
    /// @return string Token metadata IPFS URI
    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(tokenId), "Violet: URI query for nonexistent token");

        uint256 _editionId = _tokenEdition[tokenId];
        return IVioletEdition(_editions[_editionId]).tokenURI(tokenId);
    }

    /// ----------------
    /// ----------------
    /// 8. Withdrawal
    /// ----------------
    /// ----------------

    /// @notice Enable contract to receive ETH
    receive() external payable {
        emit ETHReceived(msg.sender, msg.value);
    }

    /// @notice Verifies if caller is authorized to withdraw funds
    modifier onlyWithdrawalAddresses() {
        require(
            msg.sender == owner() ||
                msg.sender == _withdrawalAddress1 ||
                msg.sender == _withdrawalAddress2,
            "Splitter: Caller cannot withdraw funds"
        );
        _;
    }

    /// @notice Amount to be transfered to _withdrawalAddress1
    /// @param _totalBalance Total balance to be withdrawn
    /// @return uint256 Amount to be transfered to _withdrawalAddress1
    function _withdrawalAmountForWithdrawalAddress1(uint256 _totalBalance)
        private
        pure
        returns (uint256)
    {
        return (_totalBalance * 15) / 100;
    }

    /// @notice Withdraw contract's ETH balance to withdrawal addresses
    function withdrawETH() external onlyWithdrawalAddresses {
        uint256 _balance = address(this).balance;
        require(_balance > 0, "Splitter: No ETH balance to transfer");

        uint256 _amount1 = _withdrawalAmountForWithdrawalAddress1(_balance);

        payable(_withdrawalAddress1).transfer(_amount1);
        payable(_withdrawalAddress2).transfer(_balance - _amount1);
    }

    /// @notice Withdraw contract's ERC20 balance to withdrawal addresses
    function withdrawERC20(address erc20) external onlyWithdrawalAddresses {
        IERC20 token = IERC20(erc20);
        uint256 _balance = token.balanceOf(address(this));
        require(_balance > 0, "Splitter: No token balance to transfer");

        uint256 _amount1 = _withdrawalAmountForWithdrawalAddress1(_balance);

        token.transfer(_withdrawalAddress1, _amount1);
        token.transfer(_withdrawalAddress2, _balance - _amount1);
    }
}

