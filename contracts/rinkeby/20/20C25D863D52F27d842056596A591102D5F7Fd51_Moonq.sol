pragma solidity ^0.8.0;

import "./Interfaces.sol";
import "./Moonq.sol";
import "./MoonqPlot.sol";
contract Elector {

    struct Vote {
        address dao;
        uint64 votes;
    }

    Moonq public token;

    bool public collectionOpened;
    bool public votingOpened;
    bool public controllerSet;

    address public controller;
    address public moonPlot;
    uint public maxVotes ;
    uint public creationTime ;
    uint public startTime;

    uint public constant WARM_UP_PERIOD = 2*30*24*3600;
    uint public constant PROPOSALS_PERIOD = 6*30*24*3600;
    uint public constant DAO_PERIOD = 12*30*24*3600;
    
    Vote[] public results;
    mapping(address => uint) public deposits; 
    mapping(address => address) public favourite; // voter =>candidate
    mapping(address => uint64) public voteCounts;

    constructor(address _moonPlot, address tokAddress){
        //todo, set LiquidityBuyBack contract as temporary controller here
        collectionOpened = false;
        token = Moonq(tokAddress);
        startTime = block.timestamp;
        votingOpened = false;
        controllerSet = false;
        moonPlot = _moonPlot;
    }

    function getResult(uint idx) public view returns (address dao,uint64 voteCount){
        dao = results[idx].dao;
        voteCount = results[idx].votes;
    }

    function getResultByAddress(address candidate) public view returns (address dao,uint64 voteCount){
        return getResult(findIndex(candidate));
    }

    function getFavOfAddress(address user) public view returns (address dao,uint64 voteCount){
        return getResult(findIndex(favourite[user]));
    }

    function getVoteCount(address voter) public view returns(uint64 voteCount){
        return voteCounts[voter] ;
    }

    function addVotes(address person, uint64 _voteCount) external {
        require(msg.sender == moonPlot,"only NFT can change votes");
        voteCounts[person] = voteCounts[person]+_voteCount;
    }

    function getController() public view returns(address){
        if(controllerSet)
            return controller;
        else
            return address(0);
    }

    function tryOpenCandidateCollection() private{
        require(block.timestamp-startTime>WARM_UP_PERIOD,"still in Warm'up period");
        require(getController() == address(0),"controller set");
        require(votingOpened == false,"voting opened");
        collectionOpened = true;
    }

    function addCandidate(address candidate, string calldata detailsUrl) external{
        tryOpenCandidateCollection();
        uint requiredDeposit = token.totalSupply()/10000;
        require(token.balanceOf(msg.sender)>=requiredDeposit, "1/10000 of total supply required as deposit to register candidate");
        require(collectionOpened,"cannot add candidates yet");
        require(voteCounts[candidate]==0,"already added");
        require(deposits[msg.sender]==0,"only one candidate per address");
        deposits[msg.sender] = requiredDeposit;
        token.transferFrom(msg.sender, address(this),deposits[msg.sender] );
        results.push(Vote(candidate,0));
        voteCounts[candidate]=1;

        emit NewCandidate(candidate, detailsUrl);
    }

    function proposalsCount() public view returns (uint){
        return results.length;
    }

    function releaseDeposit() external{

        require(deposits[msg.sender]>0,"no deposit");
        require(controllerSet,"voting not finished");
        uint bal =  deposits[msg.sender];
        deposits[msg.sender] = 0;
        token.transferFrom(address(this), msg.sender,bal );
    }

    function tryOpenVoting() private{
        require(block.timestamp-startTime>PROPOSALS_PERIOD,"still before voting period");
        require(getController() == address(0),"cannot reopen voting");
        if(votingOpened == false){
            votingOpened = true;
            collectionOpened = false;
        }
    }

    function findIndex(address _x) private view returns(uint){
        for(uint i=0;i<results.length;i++){
            address actual = results[i].dao;
            if(actual==_x){
                return i;
            }
        }
        
        revert("address not found");
    }

    function voteCanditate(address adr) external{
        tryOpenVoting();
        if(block.timestamp-startTime>DAO_PERIOD){
            closeVoting();
            return;
        }
        require(votingOpened,"cannot vote yet");
        uint candidateIndex = findIndex(adr);
        require(results[candidateIndex].dao == adr,"candidate list shuffled");
        require(adr!=address(0),"invalid adr");
            if(favourite[msg.sender]!=address(0)){
                
                uint favIndex = findIndex(favourite[msg.sender]);
                if(favIndex!=candidateIndex){
                    results[favIndex].votes = results[favIndex].votes - voteCounts[msg.sender];
                    fixDown(favIndex);
                    results[candidateIndex].votes =  results[candidateIndex].votes + voteCounts[msg.sender];
                    fixUp(candidateIndex);
                }
            }else{
                results[candidateIndex].votes =  results[candidateIndex].votes + voteCounts[msg.sender];
                favourite[msg.sender] = results[candidateIndex].dao;
                fixUp(candidateIndex);
            }
    }

    function closeVoting() private{
        controller = results[0].dao;
        controllerSet = true;
        votingOpened = false;
    }

    function moveVotes(address from, address to, uint64 amount) external{
        require(msg.sender == moonPlot,"only NFT can change votes");
        require(voteCounts[from]>=amount, "not sufficient amount of votes");
        voteCounts[to] = voteCounts[to]+amount;
        voteCounts[from] = voteCounts[from]-amount;

        if(favourite[from]!=address(0)){
            if(favourite[from]!=address(0)){
                uint favIndex = findIndex(favourite[from]);
                results[favIndex].votes=results[favIndex].votes-amount;
                fixDown(favIndex);
            }
            if(favourite[to]!=address(0)){
                uint favIndex2 = findIndex(favourite[to]);
                results[favIndex2].votes=results[favIndex2].votes+amount;
                fixUp(favIndex2);
            }
        }
    }

    function fixUp(uint index) internal{
        if(index >0){
            uint parentIndex = index/2;
            if(results[parentIndex].votes<results[index].votes){
                Vote memory tmp = results[parentIndex];
                results[parentIndex].votes = results[index].votes;
                results[parentIndex].dao = results[index].dao;
                results[index].votes = tmp.votes;
                results[index].dao = tmp.dao;
                fixUp(parentIndex);
            }
        }
    }

    function fixDown(uint index) internal{
        Vote memory up = results[index];
        uint maxIndex = index;
        if(index*2+1<results.length){
            if(results[index*2+1].votes>up.votes){
                maxIndex = index*2+1;
                up = results[index*2+1];
            }else{
            }
        }else{
        }

        if(index*2+2<results.length){
            if(results[index*2+2].votes>up.votes){
                maxIndex = index*2+2;
                up = results[index*2+2];
            }
        }
        if(maxIndex!=index){
            results[index] = results[maxIndex];
            results[maxIndex] = up;
            fixDown(maxIndex);
        }
    }

    event NewCandidate(address candidate, string details);

}

