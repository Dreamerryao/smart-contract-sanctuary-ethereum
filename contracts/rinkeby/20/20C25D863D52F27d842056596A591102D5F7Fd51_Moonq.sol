pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./Tools.sol";
import "./Elector.sol";
import "./Interfaces.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";



contract Moonq is ERC20{
  

  uint256 public constant WARM_UP_DURATION = 3600*24*60; 
  IUniswapV2Factory public UNISWAP_V2_FACTORY = IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);
    // if Uniswap pool value is not 100 000 USD before that time, allow redeem

  uint256 public creationTime ;
  bool public initialized = false;
  bool public failed = false;
  bool public assesed = false;
  address public elector;
  Tools public tools;


  mapping(address=>bool) trustedOperators;

  function verifyController() private{
    require(Elector(elector).getController() == msg.sender,"not allowed");
  }


  constructor(address _marketing1, address _marketing2,address _tools) ERC20("ToTheMoon.quest","$MOON") payable {
    require(msg.value==0.5 ether,"must be exact 0.5 eth");
    _mint(_tools,9*10**(5+decimals()));// 900 000 tokens
    _mint(address(this),10**(5+decimals()));// 100 000 tokens
    uint marketingHalf = balanceOf(address(this))/2;
    tools = Tools(_tools);
    _transfer(address(this),_marketing1, marketingHalf);
    _transfer(address(this),_marketing2, marketingHalf);
    tools.swap{value:msg.value}(address(tools.PAIR_TOKEN()),_tools);
    tools.createPair(address(this),address(tools.PAIR_TOKEN()));
  }

  function updatePrice() external{
    tools.updatePrice();
  }

  function setElector(address _elector) public{
    require(elector == address(0),"elector set");
    elector = _elector;
    trustedOperators[_elector] = true;
  }

  function mint(uint amount, address target) public{ //only after DAO will be setup aility to mint $MOON will be enabled
    verifyController();
    _mint(target,amount);
  }

  function execute(address target, bytes memory data) public{
    verifyController();
    (bool status,) = target.call(data);
    require(status,"reverted");
  }

  function getPoolAddress() public view returns (address){
    return tools.getPair(address(this),address(tools.PAIR_TOKEN()));
  }

  function addLiquidity() public{
    require(creationTime == 0,"called before");
    tools.addLiquidity(address(tools.PAIR_TOKEN()),address(this));
    creationTime = block.timestamp;
  }

  function decimals() public view virtual override returns (uint8) {
    return 6;
  }

  function asses() private{
    uint256 amount = IERC20(tools.PAIR_TOKEN()).balanceOf(tools.getPair(address(tools.PAIR_TOKEN()), address(this)));
    if(amount>10**(18+5)){
      failed = false;
    }
    else{
      failed = true;
    }
  }

  function burn(uint amount) public {
    _burn(msg.sender, amount);
  }

  function redeem(uint amount) public virtual {
    require(block.timestamp - creationTime> WARM_UP_DURATION,"too soon");
    if(assesed == false){
      asses();
      assesed = true;
    }
    require(failed,"Moonq reached goal");
    address pair = UNISWAP_V2_FACTORY.getPair(address(tools.PAIR_TOKEN()), address(this));
    uint256 lpBalance = IERC20(pair).balanceOf(address(this));
    uint lpToBurn = lpBalance*amount/totalSupply();
    _burn(msg.sender,amount);
    IERC20(pair).transferFrom(msg.sender,address(pair),lpToBurn);
    IUniswapV2Pair(pair).burn(address(this));
    tools.PAIR_TOKEN().transfer(msg.sender,tools.PAIR_TOKEN().balanceOf(address(this)));
  }

  function finalize() external{
      initialized = true;
  }

  function addTrustedOperator(address _op) external{
      require(initialized == false || trustedOperators[msg.sender],"initialization completed, no further changes possible");
      trustedOperators[_op] = true;
      emit TrustedOperatorAdded(_op);
  }
  
  function clearTrustedOperator(address _op) external{
      require(initialized == false || trustedOperators[msg.sender],"initialization completed, no further changes possible");
      trustedOperators[_op] = false;
      emit TrustedOperatorAdded(_op);
  }
  
  function transferFrom(address sender, address recipient, uint256 amount) public virtual override returns (bool) {
    if(trustedOperators[msg.sender]){
        _transfer(sender, recipient, amount);
    }else{
        return super.transferFrom(sender, recipient, amount);
    }
    return true;
  }

  event TrustedOperatorAdded(address indexed operator);

}

