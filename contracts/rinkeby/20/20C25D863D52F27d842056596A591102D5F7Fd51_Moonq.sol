        view
        returns (int56[] memory tickCumulatives, uint160[] memory secondsPerLiquidityCumulativeX128s);
}

contract Tools {
    IUniswapV2Factory public UNISWAP_V2_FACTORY = IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);
    ERC20 public PAIR_TOKEN ; //ERC20(0x13915621F7227924Cfb4C5cfEC54F4391b7d1eD8); //
    IUniswapV2Router01 public exchange = IUniswapV2Router01(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

    uint private ethPriceCached;
    uint private cacheBlock;

    constructor(address _pairToken){
        PAIR_TOKEN = ERC20(_pairToken);
    }

    function getChainID() private view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function updatePrice() external{
        if(block.number - cacheBlock > 100){
            ethPriceCached = getEthPrice();
            cacheBlock = block.number ;
        }
    }

    function getEthPrice() public view returns (uint){
        
        if(block.number - cacheBlock <= 100){
            return ethPriceCached;
        }
        if(getChainID()==4){
            return 3200;
        }
        uint32[] memory input = new uint32[](2) ;
        input[0]=180;
        input[1]=0;
        (int56[] memory tickCumulatives,) = IObserve(0xC2e9F25Be6257c210d7Adf0D4Cd6E3E881ba25f8).observe(input);
        int56 ticsNum = tickCumulatives[0]-tickCumulatives[1];
        uint result = 10**30;
        uint base = 10**30+10**26; //1.0001 * 10**30
        ticsNum = ticsNum / 180;
        while (ticsNum>0){
            if(ticsNum % 2 == 1){
                result = (result * base)/10**30;    
            }
            ticsNum = ticsNum/2;
            base =base*base / 10**30;
        }
        result = result/10**30;
        return result;
    }

    function createPair(address tok1, address tok2) external{
        UNISWAP_V2_FACTORY.createPair(tok1, tok2);
    }

    function getPair(address tok1, address tok2) public view returns(address){
       return UNISWAP_V2_FACTORY.getPair(tok1, tok2);
    }
    
    function approveAll(address destination, address token) private returns(uint){
        uint bal = IERC20(token).balanceOf(address(this));
        ERC20(token).approve(address(destination), bal);
        return bal;
    }

    function addLiquidity(address tok1,address tok2) public {

        uint bal = approveAll(address(exchange), tok1);
        uint bal2 = approveAll(address(exchange), tok2);
        address pair = getPair(tok1,tok2);
        uint balPair1 = IERC20(tok1).balanceOf(pair) ;
        uint balPair2 = IERC20(tok2).balanceOf(pair) ;

        if( balPair2>0 && balPair1>0){
            if(bal*balPair2>bal2*balPair1)  {
                bal = balPair1 * bal2 / balPair2;
            }else{
                bal2 = balPair2 * bal / balPair1;
            }
        }
        
        exchange.addLiquidity(
        tok1, 
        tok2,  
        bal,bal2,
        0,0,
        tok2, block.timestamp+1);

        transferAll(payable(tx.origin),tok1);
        transferAll(payable(tx.origin),tok1);
        transferAll(payable(msg.sender),pair);

    }

    function transferAll(address payable destination, address token) private{
        uint bal = IERC20(token).balanceOf(destination);
        if(token == address(PAIR_TOKEN)){
            IWeth9(token).withdraw(bal);
            destination.transfer(bal);
        }else{
