pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./Interfaces.sol";
import "./Elector.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";



contract MoonqPlot is ERC721{

  uint public totalSupply;
  address public store ;  
  Elector public elector;
  bool public notUpdated;
  address public notRealOwner = address(0x00FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF);
  
  constructor() ERC721("ToTheMoon.quest Plots","$MOONPlots") {
    totalSupply = 0;
    notUpdated = true;
  }

  function ValidateIsStoreOrSelf() private view{
      require(msg.sender == address(this) || msg.sender == store, "illegal caller");
  }

    /// @notice allows to create a plot, only invocable via Store
    /// @dev every 1 deg of both lat and long are multiplied by 2**10 = 1024 to support divisibility
    /// 0-7 bits - granularity level 0, biggest cell 1deg squared
    /// 8 - 39 bits - int32 lat multiplied by 1024
    /// 40 - 71 bits - int32 long multiplird by 1024
    /// @param lat latitiude (-90,90) multiplied by 1024
    /// @param lat longitiude (-180,180) multiplied by 1024
  function mint(int32 lat,int32 long, address _owner) external returns(uint){
      ValidateIsStoreOrSelf();
      uint index = internalMint(lat, long, 0, _owner);
      return index;
  }

  function internalMint(int32 lat,int32 long,uint lvl, address _owner) internal returns(uint){
      uint index = encode(lat,long, lvl,0);
      totalSupply = totalSupply+1;
      _mint(_owner,index);
      elector.addVotes(_owner,1024*1024);
      emit NewMoonPlot(lat,long,_owner, index);
      emit PlotTransfer(address(0), _owner, index);
      return index;
  }

  function mintBulk(int32[] calldata lat, int32[] calldata long, address _owner) external{
    require(lat.length == long.length,"invalid input");
    require(store == address(0), "already initialized");
    for(uint i=0;i<lat.length;i++){
      this.mint(lat[i],long[i],_owner);
    }
  }

  function hasOwner(uint tokenId) external view returns (bool){
    return _exists(tokenId);
  }
  

  function split(uint256 index) external {
      ValidateIsStoreOrSelf();
      address owner = ownerOf(index);
      (int lat,int long,uint lvl,) = decode(index);
      require(index<=10,"min plot size 1");
      int deltaNextLvl = int(1024/(2**(lvl+1)));
      _mint(owner,encode(lat,long,lvl+1,0));
      _mint(owner,encode(lat+deltaNextLvl,long,lvl+1,0));
      _mint(owner,encode(lat+deltaNextLvl,long+deltaNextLvl,lvl+1,0));
      _mint(owner,encode(lat,long+deltaNextLvl,lvl+1,0));
      _transfer(owner, notRealOwner, index);
  }

  function setStore(address _store) external {
      require(store == address(0) || msg.sender == elector.getController(),"store already set");
      if(store!=address(0)){
        notUpdated = false;
      }
      store = _store;
  }

  function setElector(address _elector) external {
      require(address(elector) == address(0),"elector already set");
      elector = Elector(_elector);
  }

  function transferFrom(address sender, address recipient, uint256 tokenId) public virtual override {
    emit PlotTransfer(sender, recipient, tokenId);
    if((store == msg.sender && notUpdated == true) || address(this) == msg.sender){
        _transfer(sender, recipient, tokenId);
    }else{
        super.transferFrom(sender, recipient, tokenId);
    }
    (,,uint index,) = decode(tokenId);
    elector.moveVotes(sender, recipient, uint32(4**(10-index)));
  }

  function safeTransferFrom(address sender,address recipient, uint tokenId) public virtual override {

    (,,uint index,) = decode(tokenId);
    emit PlotTransfer(sender, recipient, tokenId);
    elector.moveVotes(sender, recipient, uint32(4**(10-index)));
    super.safeTransferFrom(sender, recipient, tokenId);
  }

    /// @notice allows to create a plot, only invocable via Store
    /// @dev every 1 deg of both lat and long are multiplied by 2**10 = 1024 to support divisibility
    /// 0-7 bits - granularity level 0, biggest cell 1deg squared
    /// 8 - 39 bits - int32 lat multiplied by 1024
    /// 40 - 71 bits - int32 long multiplird by 1024
    /// @param lat latitiude (-90,90) multiplied by 1024
    /// @param lat longitiude (-180,180) multiplied by 1024
  function decode(uint index) pure public returns (int lat,int long, uint level, uint reminder){
    level = index / (2**(256-8));
    lat = int(index % (2**(256-8)))/ (2**(256-8-32)) - 90*1024;
    long = int(index % (2**(256-8-32)))/ (2**(256-8-64)) - 180*1024;
    reminder = index % (2**(256-8-64))/  (2**(256-8-64-10));
    return(lat,long,level, reminder);
  }

    /// @notice allows to create a plot, only invocable via Store
    /// @dev every 1 deg of both lat and long are multiplied by 2**10 = 1024 to support divisibility
    /// 0-7 bits - granularity level 0, biggest cell 1deg squared
    /// 8 - 39 bits - int32 lat multiplied by 1024
    /// 40 - 71 bits - int32 long multiplird by 1024
    /// @param lat latitiude (-90,90) multiplied by 1024
    /// @param lat longitiude (-180,180) multiplied by 1024
  function encode(int lat,int long, uint level, uint reminder) pure public returns(uint index){
    return uint((lat+90*1024)* (2**(256-8-32))+(long+180*1024)*(2**(256-8-64)))+reminder*(2**(256-8-64-10))+level*2**(256-8);
  }
  
  event NewMoonPlot(int32 lat,int32 long,address indexed _owner, uint256 tokenId);
  event PlotTransfer(address from, address to, uint256 tokenId);

}

