  function addApprovedContract(address _contract) public {
    LibDiamond.enforceIsContractOwner();
    require(s.t.approvedContractIndexes[_contract] == 0, "FraktalDeFiFacet: Approved contract already exists");
    s.t.approvedContracts.push(_contract);
    s.t.approvedContractIndexes[_contract] = s.t.approvedContracts.length;
  }

  function removeApprovedContract(address _contract) public {
    LibDiamond.enforceIsContractOwner();
    uint256 index = s.t.approvedContractIndexes[_contract];
    require(index > 0, "FraktalDeFiFacet: Approved contract does not exist");
    uint256 lastIndex = s.t.approvedContracts.length;
    if (index != lastIndex) {
        address lastContract = s.t.approvedContracts[lastIndex - 1];
        s.t.approvedContracts[index - 1] = lastContract;
        s.t.approvedContractIndexes[lastContract] = index;
    }
    s.t.approvedContracts.pop();
    delete s.t.approvedContractIndexes[_contract];
  }

  function approvedContracts() public view returns (address[] memory contracts_) {
    contracts_ = s.t.approvedContracts;
  }

  function addContract (
    address _addr,
    string memory name,
    Storage.Platform calldata platform,
    Storage.ContractType[] calldata _types,
    bool active
  ) public {

    require(!LibModifiers._hasContract(_addr), 'Contract already exists, and you wasted gas because you did not do a sanity check');
    uint platformContractsCount = s.platformContractsCount;

    Storage.ContractInfo storage _c = s.platformContracts[platformContractsCount + 1];
    _c._addr = _addr;
    _c.name = name;
    _c.platform = platform;
    // _c._types = new Storage.ContractType[](_types.length);
    for(uint i = 0; i < _types.length; i++) {
      // TODO: add checks for if type exists is types and if type exists is in platformContract
      // Storage.ContractType memory _t = _c._types[i];
      _c._types.push(_types[i]);
    }
    _c.active = active;
    
    s.platformContractsCount++;
    emit LibDataHelper.AddedPlatformContract(_addr, name, platform, _types, active);
  }
  function addContractType (Storage.ContractType calldata _type) internal returns(bool success) {
    success = false;
    uint i;
    for (i = 0; i < s.contractTypes.length; i++) {
      if (Utils._stringCompare(
        Utils._toLower(_type.name),
        Utils._toLower(s.contractTypes[i].name)
      )) {
        success = true;
      }
    }
    if (!success) {
      return success;
    }
    s.contractTypes.push(_type);
    emit LibDataHelper.AddedContractType(_type);
    
  }

  function _authorizeContract (uint id) internal {
    s.platformContracts[id].active = true;
  }
  function _deauthContract (uint id) internal {
    s.platformContracts[id].active = false;
  }
  function _getContractByName(string memory name) internal view returns(Storage.ContractInfo memory _contract) {
    uint i;
    for (i = 0; i < _totalContracts(); i++) {
      if ( Utils._stringCompare( Utils._toLower(name), Utils._toLower(s.platformContracts[i].name) ) ) {
        _contract = s.platformContracts[i];
        return _contract;
      }
    }
  }
  function _getContractByNameWithId (string memory name) internal view returns(Storage.ContractInfo memory _contract, uint id) {
    uint i;
    for (i = 0; i < _totalContracts(); i++) {
      if ( Utils._stringCompare(Utils._toLower(name), Utils._toLower(s.platformContracts[i].name))) {
        _contract = s.platformContracts[i];
        id = i;
        return (_contract, id);
      }
    }
  }
  function _getContractByAddress(address _addr) internal view returns(Storage.ContractInfo memory _contract) {
    uint i;
    for (i = 0; i < _totalContracts(); i++) {
      if ( _addr == s.platformContracts[i]._addr ) {
        _contract = s.platformContracts[i];
        return _contract;
      }
    }
  }
  function _getContractByAddressWithId (address _addr) internal view returns(Storage.ContractInfo memory _contract, uint id) {
    uint i;
    for (i = 0; i < _totalContracts(); i++) {
      if ( _addr == s.platformContracts[i]._addr ) {
        _contract = s.platformContracts[i];
        id = i;
        return (_contract, id);
