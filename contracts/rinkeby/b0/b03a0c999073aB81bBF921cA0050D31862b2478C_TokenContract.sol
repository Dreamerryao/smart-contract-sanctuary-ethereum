import "../Error.sol";
import "../structs/Model.sol";
import "../structs/Car.sol";
import "../structs/Engine.sol";
import "../interfaces/tokens/ICarToken.sol";
import "../interfaces/tokens/IEngineToken.sol";
import "../chainlink/VRFConsumer.sol";

contract CarToken is ICarToken, ERC721, IERC2981, Pausable, Ownable, VRFConsumer {
  using SafeMath for uint256;
  using Strings for uint256;
  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;

  string public baseURI;
  uint256 public cost = 0.0005 ether;
  mapping(uint256 => address) internal _receiverAddresses;
  string constant private BASE_EXTENSION = ".json";
  uint256 constant private AUTHOR_ROYALTY = 250; // 2.5%
  uint256 constant private DEV_ROYALTY = 250; // 2.5%
  mapping (uint => Car) private cars;
  IEngineToken private _engineToken;
  IERC721 private _engineERC721;

  Model[] private _models;
  uint256[] private _modelsRarity;

  // Chainlink
  uint32 internal callbackGasLimit = 100000;
  uint16 internal requestConfirmations = 3;
  uint32 internal numWords = 2;
  mapping(uint256 => uint256[]) public requestIdToRandomWords;
  mapping(uint256 => uint) internal requestIdToMintAmount;
  mapping(uint256 => uint256[]) internal requestIdToTokenIds;

  constructor(
    string memory name,
    string memory symbol,
    string memory initBaseURI
  ) ERC721(name, symbol) {
    setBaseURI(initBaseURI);
    _pause();
    setupModel(Model("Vw Golf", 1, 1020), 240);
    setupModel(Model("Toyota Celica", 1, 1030), 236);
    setupModel(Model("Ford Focus", 1, 1050), 232);
    setupModel(Model("Mazda 3", 1, 1060), 226);
    setupModel(Model("Honda Civic", 1, 1070), 224);
    setupModel(Model("Mazda Rx8", 1, 1080), 220);
    setupModel(Model("Mazda Rx7", 1, 1090), 214);

    setupModel(Model("Chevrolet Corvette zl1", 2, 2000), 134);
    setupModel(Model("koenigsegg agera 2010", 2, 2020), 132);
    setupModel(Model("Lamborghini huracan", 2, 2040), 130);
    setupModel(Model("Pagani Huayra", 2, 2060), 128);
    setupModel(Model("MAclaren 650", 2, 2080), 126);
    setupModel(Model("Porsche 911 gt3", 2, 2100), 124);
    setupModel(Model("Mercedes E6.3", 2, 2120), 122);
    setupModel(Model("BMW m5 2020", 2, 2140), 120);

    setupModel(Model("Buggati Veyron", 3, 3100), 58);
    setupModel(Model("lamborghini aventador", 3, 3140), 56);
    setupModel(Model("Koenigsegg agera 2015", 3, 3160), 54);
    setupModel(Model("CAPARO t1", 3, 3180), 52);
    setupModel(Model("Hennessy Venom", 3, 3200), 50);
    setupModel(Model("koenigsegg jesko 2019", 3, 3220), 48);
    setupModel(Model("SSC tuatara  2018", 3, 3250), 46);
  }

  function configure(address engineAddress, uint64 subId) public onlyOwner {
    _engineToken = IEngineToken(engineAddress);
    _engineERC721 = IERC721(engineAddress);
    subscriptionId = subId;
  }

  function setupModel(Model memory model, uint256 rarity) internal {
    _models.push(model);
    _modelsRarity.push(rarity);
    // console.log("Setup Rarity", model.name, rarity);
  }

  // function devMint(uint mintAmount) public onlyOwner {
  //   if (mintAmount <= 0) { revert MintAmountNotPositive(mintAmount); }
  //   for (uint256 i = 1; i <= mintAmount; i++) {
  //     _tokenIds.increment();
  //     uint256 tokenId = _tokenIds.current();
  //     _safeMint(msg.sender, tokenId);
  //   }
  // }

  // function mint(uint mintAmount) public payable whenNotPaused {
  //   if (mintAmount <= 0) { revert MintAmountNotPositive(mintAmount); }
  //   if (msg.sender != owner() && msg.value < cost * mintAmount) {
  //     revert InsufficientBalance(msg.value, cost * mintAmount);
  //   }
  //   for (uint256 i = 1; i <= mintAmount; i++) {
  //     _tokenIds.increment();
  //     uint256 tokenId = _tokenIds.current();
  //     _safeMint(msg.sender, tokenId);
  //     Model memory model = _modelFactory.getRandomModel();
  //     Engine memory engine = _engineToken.getCommonEngine();
  //     cars[tokenId] = Car(model, engine);
  //     _receiverAddresses[tokenId] = _msgSender();
  //   }
  // }

  function royaltyInfo(uint256 tokenId, uint256 value) external view override
    returns (address receiver, uint256 royaltyAmount)
  {
    receiver = _receiverAddresses[tokenId];
    uint256 authorRoyaltyAmount = value.mul(AUTHOR_ROYALTY).div(10000); // (value * AUTHOR_ROYALTY) / 10000;
    uint256 devRoyaltyAmount = value.mul(DEV_ROYALTY).div(10000);
    royaltyAmount = authorRoyaltyAmount.add(devRoyaltyAmount);
  }

  function requestMint(uint mintAmount) public payable whenNotPaused {
    if (mintAmount <= 0) { revert MintAmountNotPositive(mintAmount); }
    if (msg.sender != owner() && msg.value < cost * mintAmount) {
      revert InsufficientBalance(msg.value, cost * mintAmount);
    }
    uint256 requestId = COORDINATOR.requestRandomWords(
      keyHash,
      subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      numWords
    );
    requestIdToMintAmount[requestId] = mintAmount;
    requestIdToTokenIds[requestId] = new uint256[](mintAmount);
    for (uint256 i = 0; i < mintAmount; i++) {
      _tokenIds.increment();
      uint256 tokenId = _tokenIds.current();
      requestIdToTokenIds[requestId][i] = tokenId;
      _safeMint(_msgSender(), tokenId);
      _receiverAddresses[tokenId] = _msgSender();
    }
  }


  function getRandomModel(uint256 randomNumber) internal view returns (Model memory) {
    uint256 size = 0;
    for (uint i = 0; i < _models.length; i++) {
      uint256 rarity = _modelsRarity[i];
      size = size.add(rarity);
    }
    uint256 idx = 0;
    Model[] memory result = new Model[](size);
    for (uint i = 0; i < _models.length; i++) {
      Model memory m = _models[i];
      uint256 rarity = _modelsRarity[i];
      for (uint ii = 0; ii < rarity; ii++) {
        result[idx] = m;
        idx = idx.add(1);
      }
    }
    // shuffle result
    // for (uint i = 0; i < result.length; i++) {
    //   uint n = i + randomNumber % (result.length - i);
    //   Model memory temp = result[n];
    //   result[n] = result[i];
    //   result[i] = temp;
    // }
    uint n = randomNumber.mod(result.length);
    return result[n];
  }

  function fulfillRandomWords(
    uint256 requestId,
    uint256[] memory randomWords
  ) internal override {
    requestIdToRandomWords[requestId] = randomWords;
    uint mintAmount = requestIdToMintAmount[requestId];
    for (uint256 i = 0; i < mintAmount; i++) {
      uint256 randomNumber = randomWords[i];
      uint256 tokenId = requestIdToTokenIds[requestId][i];
      // Model memory model = getRandomModel(randomNumber);
      Model memory model = _models[randomNumber % _models.length];
      Engine memory engine = _engineToken.getCommonEngine();
      cars[tokenId] = Car(model, engine);
    }
  }


  function totalSupply() public view returns (uint256) {
    return _tokenIds.current();
  }

   // Equip car details
  function equipEngine(uint carId, uint engineId) external {
    if(!_exists(carId)) { revert NonexistentCar(carId); }
    address carOwner = ownerOf(carId);
    address engineOwner;
    try _engineERC721.ownerOf(engineId) returns (address result) {
      engineOwner = result;
    } catch {
      revert NonexistentEngine(engineId);
    }
    if (carOwner != _msgSender()) { revert NotOwner(carOwner, _msgSender()); }
    if (engineOwner != _msgSender()) { revert NotOwner(engineOwner, _msgSender()); }
    Car memory car = cars[carId];
    car.engine = _engineToken.engineOf(engineId);
    _engineToken.equip(engineId, carId);
    cars[carId] = car;
  }

  function unequipEngine(uint carId) external {
    if(!_exists(carId)) { revert NonexistentCar(carId); }
    address carOwner = ownerOf(carId);
    if (carOwner != _msgSender()) { revert NotOwner(carOwner, _msgSender()); }
    Car memory car = cars[carId];
    require(car.engine.id > 0, "Engine not equipped.");
    _engineToken.unequip(car.engine.id);
    car.engine = _engineToken.getCommonEngine();
    cars[carId] = car;
  }

  function carOf(uint tokenId) external view returns(Car memory) {
    if(!_exists(tokenId)) { revert NonexistentCar(tokenId); }
    return cars[tokenId];
  }

  // Transfer
  function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal whenNotPaused override {
    super._beforeTokenTransfer(from, to, tokenId);
  }

  function transferFrom(address from, address to, uint256 tokenId) public override {
    if(!_exists(tokenId)) { revert NonexistentCar(tokenId); }
    Car memory car = cars[tokenId];
    if (car.engine.id > 0) {
      _engineERC721.transferFrom(from, to, car.engine.id);
    }
    super.transferFrom(from, to, tokenId);
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
    string memory base = _baseURI();
    return bytes(base).length > 0 ? string(abi.encodePacked(base, tokenId.toString(), BASE_EXTENSION)) : "";
  }

  // Only Owner
  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function withdraw() public onlyOwner {
    if (!payable(msg.sender).send(address(this).balance)) {
      revert CanNotSended(msg.sender, address(this).balance);
    }
  }

  function setBaseURI(string memory newBaseURI) public onlyOwner {
    baseURI = newBaseURI;
  }

  function setCost(uint256 newCost) public onlyOwner() {
    cost = newCost;
  }
}

