pragma solidity ^0.8.2;

import "@openzeppelin/contracts/utils/Context.sol";

abstract contract Equipable is Context {

  event Equipped(address account, uint tokenId, uint carId);
  event Unequipped(address account, uint tokenId);

  mapping (uint => uint) private equipped;

  function _equip(uint tokenId, uint carId) internal virtual {
    equipped[tokenId] = carId;
    emit Equipped(_msgSender(), tokenId, carId);
  }

  function _unequip(uint256 tokenId) internal virtual {
    delete equipped[tokenId];
    emit Unequipped(_msgSender(), tokenId);
  }

  function isEquipped(uint256 tokenId) public view virtual returns (bool) {
    return equipped[tokenId] > 0;
  }

  function equipOf(uint tokenId) public view virtual returns (uint) {
    require(isEquipped(tokenId), "Not Equipped.");
    return equipped[tokenId];
  }

  modifier whenNotEquipped(uint256 tokenId) {
    require(!isEquipped(tokenId), "Equipable: is equipped.");
    _;
  }
}

