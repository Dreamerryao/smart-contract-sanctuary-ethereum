pragma solidity ^0.8.0;

import "hardhat/console.sol";

// import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

import "../chainlink/VRFConsumer.sol";
import "../structs/Engine.sol";


contract TokenContract is ERC721, VRFConsumerBaseV2, Ownable {
  using SafeMath for uint256;
  using Strings for uint256;
  using Counters for Counters.Counter;

  // constructor() {}

  // enum MintRequestStatus { PENDING, FULFILLED, PROCESSED }

  struct Model {
    string name;
    uint tier;
    uint horsepower;
  }

  // mapping(uint256 => MintRequest) internal mintRequests;
  // mapping(uint256 => uint256[]) internal requestIdToTokenIds;
  mapping(uint256 => uint256[]) public requestIdToRandomWords;
  // mapping(uint256 => address) internal requestIdToSender;
  // mapping(uint256 => uint256) internal requestIdToPendingRequestIndex;
  // mapping(uint256 => uint256) internal requestIdToFulfilledRequestIndex;
  uint256[] internal _pendingRequests;
  uint256[] internal _fulfilledRequests;

  struct RandomnessRequest {
    address account;
    uint256 number;
    uint256 tokenId;
    bool fulfilled;
  }
  mapping(address => uint256[]) public userRequests;

  // Chainlink
  VRFCoordinatorV2Interface COORDINATOR;
  LinkTokenInterface LINKTOKEN;
  bytes32 keyHash;
  uint32 public constant CALLBACK_GAS_LIMIT = 1000000;
  uint16 public constant REQUEST_CONFIRMATIONS = 3;
  uint32 public constant NUM_WORDS = 1;


  // Engine
  Counters.Counter private _tokenIds;
  mapping(uint => Engine) private engines;
  Model[] private _models;
  uint256[] private _modelsRarity;

  mapping(uint256 => uint256) public tokenIdToRandomNumber;
  uint256[] public rndNums;
  uint64 public subscriptionId;
  mapping(uint256 => RandomnessRequest) public randomnessRequests;

  // Event
  event CreateRequest(address account, uint256 requestId);
  event MintToken(address account, uint256 requestId, uint256 tokenId);

  constructor(
    string memory name,
    string memory symbol,
    string memory initBaseURI,
    address vrfCoordinator,
    address linkTokenAddress,
    bytes32 initialKeyHash
  )
    ERC721(name, symbol)
    VRFConsumerBaseV2(vrfCoordinator)
  {
    COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    LINKTOKEN = LinkTokenInterface(linkTokenAddress);
    keyHash = initialKeyHash;
    createNewSubscription();
    setupModel(Model("Engine 1.1", 1, 50), 240);
    setupModel(Model("Engine 1.2", 1, 55), 236);
    setupModel(Model("Engine 1.3", 1, 60), 232);
    setupModel(Model("Engine 1.4", 1, 65), 226);
    setupModel(Model("Engine 1.5", 1, 70), 224);
    setupModel(Model("Engine 1.6", 1, 75), 220);
    setupModel(Model("Engine 1.7", 1, 80), 214);

    setupModel(Model("Engine 2.1", 2, 100), 134);
    setupModel(Model("Engine 2.2", 2, 110), 132);
    setupModel(Model("Engine 2.3", 2, 120), 130);
    setupModel(Model("Engine 2.4", 2, 130), 128);
    setupModel(Model("Engine 2.5", 2, 140), 126);
    setupModel(Model("Engine 2.6", 2, 150), 124);
    setupModel(Model("Engine 2.7", 2, 160), 122);
    setupModel(Model("Engine 2.8", 2, 170), 120);

    setupModel(Model("Engine 3.1", 3, 200), 58);
    setupModel(Model("Engine 3.2", 3, 210), 56);
    setupModel(Model("Engine 3.3", 3, 220), 54);
    setupModel(Model("Engine 3.4", 3, 230), 52);
    setupModel(Model("Engine 3.5", 3, 240), 50);
    setupModel(Model("Engine 3.6", 3, 250), 48);
    setupModel(Model("Engine 3.7", 3, 260), 46);
  }


  // Create a new subscription when the contract is initially deployed.
  function createNewSubscription() private onlyOwner {
    // Create a subscription with a new subscription ID.
    address[] memory consumers = new address[](1);
    consumers[0] = address(this);
    subscriptionId = COORDINATOR.createSubscription();
    // Add this contract as a consumer of its own subscription.
    COORDINATOR.addConsumer(subscriptionId, consumers[0]);
  }

  function cancelSubscription(address receivingWallet) external onlyOwner {
    // Cancel the subscription and send the remaining LINK to a wallet address.
    COORDINATOR.cancelSubscription(subscriptionId, receivingWallet);
    subscriptionId = 0;
  }

  function topUpSubscription(uint256 amount) external onlyOwner {
    LINKTOKEN.transferAndCall(
      address(COORDINATOR),
      amount,
      abi.encode(subscriptionId)
    );
  }

  function updateKeyHash(bytes32 _keyHash) external onlyOwner {
    keyHash = _keyHash;
    // emit UpdateKeyHash(keyHash);
  }

  function requestMint() public payable {
    uint256 requestId = COORDINATOR.requestRandomWords(
      keyHash,
      subscriptionId,
      REQUEST_CONFIRMATIONS,
      CALLBACK_GAS_LIMIT,
      NUM_WORDS
    );
    RandomnessRequest memory data = RandomnessRequest({
      account: _msgSender(),
      number: 0,
      tokenId: 0,
      fulfilled: false
    });
    randomnessRequests[requestId] = data;
    userRequests[_msgSender()].push(requestId);
    emit CreateRequest(_msgSender(), requestId);
  }

  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    RandomnessRequest storage data = randomnessRequests[requestId];
    require(!data.fulfilled, "VRF: request already processed");
    data.fulfilled = true;
    data.number = randomWords[0];
    _tokenIds.increment();
    uint256 tokenId = _tokenIds.current();
    Model memory engineModel = getRandomModel(data.number);
    engines[tokenId] = Engine(tokenId, engineModel.name, engineModel.tier, engineModel.horsepower);
    _safeMint(_msgSender(), tokenId);
    emit MintToken(data.account, requestId, tokenId);
  }

  function pendingRequests() public view returns(uint256[] memory) {
    return _pendingRequests;
  }

  function fulfilledRequests() public view returns(uint256[] memory) {
    return _fulfilledRequests;
  }

  function setupModel(Model memory model, uint256 rarity) internal {
    _models.push(model);
    _modelsRarity.push(rarity);
  }

  function getRandomModel(uint256 randomNumber) internal returns (Model memory) {
    uint256 size = 0;
    for (uint i = 0; i < _models.length; i++) {
      uint256 rarity = _modelsRarity[i];
      size = size.add(rarity);
    }
    uint256 idx = 0;
    Model[] memory result = new Model[](size);
    for (uint i = 0; i < _models.length; i++) {
      Model memory m = _models[i];
      uint256 rarity = _modelsRarity[i];
      for (uint ii = 0; ii < rarity; ii++) {
        result[idx] = m;
        idx = idx.add(1);
      }
    }
    // shuffle result
    // for (uint i = 0; i < result.length; i++) {
    //   uint s = i + shuffleRandomNumber % (result.length - i);
    //   Model memory temp = result[s];
    //   result[s] = result[i];
    //   result[i] = temp;
    // }
    uint n = randomNumber.mod(result.length);
    rndNums.push(n);
    return result[n];
  }

  function totalSupply() public view returns (uint256) {
    return _tokenIds.current();
  }

  function engineOf(uint tokenId) external view returns(Engine memory) {
    return engines[tokenId];
  }
}

