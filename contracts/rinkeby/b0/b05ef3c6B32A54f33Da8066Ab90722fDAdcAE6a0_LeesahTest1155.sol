pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import { MerkleProof } from "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import { IERC721 } from "@openzeppelin/contracts/interfaces/IERC721.sol";
import { LeesahTestMadBoy721 } from "./LeesahTestMadBoy721.sol";
import { LeesahTestVehicles721 } from "./LeesahTestVehicles721.sol";
import { LeesahTestKeys721 } from "./LeesahTestKeys721.sol";

contract LeesahTest1155 is ERC1155Supply, VRFConsumerBaseV2 {
    /** VRF STUFF */
    VRFCoordinatorV2Interface COORDINATOR;

    // Your subscription ID.
    uint64 s_subscriptionId;

    // The gas lane to use, which specifies the maximum gas price to bump to.
    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    bytes32 keyHash = 0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 callbackGasLimit = 100000;

    // The default is 3, but you can set this higher.
    uint16 requestConfirmations = 3;

    uint256 public s_randomWord;
    address s_owner;

    // s_rollers stores a mapping between the requestID (returned when a request
    // is made), and the address of the roller. This is so the contract can keep
    // track of who to assign the result to when it comes back.
    mapping(uint256 => address) private s_rollers;

    // s_context stores the requestId and whether it's for the 1155 or 721 randomization

    mapping(uint256 => string) private s_context;
    mapping(address => uint256) private s_results;

    uint256[] public randomWordsArray;

    /** END VRF STUFF */

    /* 1155 Supply Constants */
    uint256 public immutable maxChuggernauts = 490;
    uint256 public immutable maxBigChuggos = 1360;
    uint256 public immutable maxLilChugs = 7950;

    /* WL Constants */
    uint256 public immutable maxAllowlistMintPerAddress = 2;
    uint256 public immutable maxPublicSaleMintPerAddress = 5;

    /* Contracts */
    LeesahTestMadBoy721 public immutable madBoyNFTContract;
    LeesahTestVehicles721 public immutable vehiclesNFTContract;
    LeesahTestKeys721 public immutable keysNFTContract;

    /* Lootbox Constants */
    uint256 public constant LilChug = 1;
    uint256 public constant BigChuggo = 2;
    uint256 public constant Chuggernaut = 3;
    uint256[] public lootboxTypes = new uint256[](3);

    string private baseURI;
    bytes32 private _allowlistMerkleRoot;

    constructor(
      string memory _baseURI,
      uint64 subscriptionId,
      address _vrfCoordinator,
      LeesahTestMadBoy721 madBoyNFTContract_,
      LeesahTestVehicles721 vehiclesNFTContract_,
      LeesahTestKeys721 keysNFTContract_
    ) ERC1155(_baseURI) VRFConsumerBaseV2(_vrfCoordinator) {
        // Set NFT contract addresses
        madBoyNFTContract = madBoyNFTContract_;
        vehiclesNFTContract = vehiclesNFTContract_;
        keysNFTContract = keysNFTContract_;

        // Set lootbox types
        lootboxTypes[0] = LilChug;
        lootboxTypes[1] = BigChuggo;
        lootboxTypes[2] = Chuggernaut;

        // Set base URI for metadata
        baseURI = _baseURI;

        // Chainlink VRF
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        s_owner = msg.sender;
        s_subscriptionId = subscriptionId;
    }

    function uri(uint256 _lootboxTypeId) public override view returns (string memory) {
      return string(
        abi.encodePacked(
          baseURI,
          Strings.toString(_lootboxTypeId),
          ".json"
        )
      );
    }

    /***
      WRITE OPERATIONS
    ***/

    // request to mint - request randomness
    function mint(address _toAddress, uint32 _amount) external {
      uint256 requestId = COORDINATOR.requestRandomWords(
        keyHash,
        s_subscriptionId,
        requestConfirmations,
        callbackGasLimit,
        _amount
      );
      s_rollers[requestId] = _toAddress;
      s_context[requestId] = "1155";
    }

    function mintBatch(address _toAddress, uint256[] calldata _amounts) external {
      _mintBatch(_toAddress, lootboxTypes, _amounts, "");
      emit TransferSingle(msg.sender, address(0), _toAddress, 1, 1);
    }


    function allowlistMint(address _fromAddress, bytes32[] memory proof, uint256 amount) external {
      // TODO: research gas optimized minting allowlist proof
      // TODO: change whitelistClaimed state
      bytes32 leaf = keccak256(abi.encodePacked(_fromAddress, amount));
      require(MerkleProof.verify(proof, _allowlistMerkleRoot, leaf), "Invalid proof");
      _mint(_fromAddress, LilChug, amount, "");
    }

    function openLootbox(uint256 _lootboxTypeId, address _fromAddress, uint256 _amount) public {
      // TODO: add protections and validations for the minting process
      // validate valid lootboxTypeId
      // TODO: is there a smarter way of checking this
      require(
        (_lootboxTypeId == LilChug || _lootboxTypeId == BigChuggo || _lootboxTypeId == Chuggernaut),
        "Cannot open invalid lootbox type"
      );
      _burn(_fromAddress, _lootboxTypeId, _amount);
    }

    function openAllLootboxes(address _fromAddress) external {
      // TODO; see if there's a better way of assigning these values
      address[] memory addresses = new address[](3);
      addresses[0] = _fromAddress;
      addresses[1] = _fromAddress;
      addresses[2] = _fromAddress;

      // Check that the _fromAddress is able to open a lootbox
      uint256[] memory lootboxes = balanceOfBatch(addresses, lootboxTypes);

      // If LilChug
      if(lootboxes[0] > 0) {
        madBoyNFTContract.mint(_fromAddress);
        _burn(_fromAddress, LilChug, lootboxes[0]);
      }
      // If Chuggernaut
      if(lootboxes[2] > 0){
        madBoyNFTContract.mint(_fromAddress);
        vehiclesNFTContract.mint(_fromAddress);
        keysNFTContract.mint(_fromAddress);
        _burn(_fromAddress, Chuggernaut, lootboxes[2]);
      }
      // If BigChuggo
      if(lootboxes[1] > 0) {
        uint256 requestId = COORDINATOR.requestRandomWords(
          keyHash,
          s_subscriptionId,
          requestConfirmations,
          callbackGasLimit,
          uint32(lootboxes[1])
        );
        s_rollers[requestId] = _fromAddress;
        s_context[requestId] = "721";
      }
    }

    /***
      ADMIN OPERATIONS
    ***/

    // TODO: Add admin-privilege gate to these functions
    function setBaseUri(string calldata _baseURI) external onlyOwner{
      baseURI = _baseURI;
    }

    function setAllowlistMerkleRoot(bytes32 merkleRoot) external onlyOwner{
      _allowlistMerkleRoot = merkleRoot;
    }


    /***
      CHAINLINK VRF STUFF
    ***/
    
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
      address toAddress = s_rollers[requestId];
      string memory context = s_context[requestId];

      // Randomly mint lootboxes
      if(keccak256(abi.encodePacked(context)) == keccak256(abi.encodePacked("1155"))) {
        for(uint32 i = 0; i < randomWords.length; i++) {

          uint256 randomWord = randomWords[i] % 9800 + 1;

          if (randomWord > 0 && randomWord <= 7950) {
            _mint(toAddress, LilChug, 1, "");
          } else if (randomWord > 7950 && randomWord <= 9310) {
            _mint(toAddress, BigChuggo, 1, "");
          } else {
            _mint(toAddress, Chuggernaut, 1, "");
          }

        }
      }

      // Randomly mint NFTs for BigChuggo lootbox opening
      if(keccak256(abi.encodePacked(context)) == keccak256(abi.encodePacked("721"))) {
        for(uint32 i = 0; i < randomWords.length; i++) {
          madBoyNFTContract.mint(toAddress);

          uint256 nftType = (randomWords[i] % 2340) + 1;
          if (nftType > 0 && nftType <= 1752) {
            vehiclesNFTContract.mint(toAddress);
          } else {
            keysNFTContract.mint(toAddress);
          }
          uint256 bigChuggoBoxes = balanceOf(toAddress, BigChuggo);
          _burn(toAddress, BigChuggo, bigChuggoBoxes);
        }
      }
    }

    modifier onlyOwner() {
      require(msg.sender == s_owner);
      _;
    }
}

