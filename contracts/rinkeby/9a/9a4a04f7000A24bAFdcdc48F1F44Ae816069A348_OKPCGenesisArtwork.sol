pragma solidity ^0.8.8;

import {IOKPCGenesisArtwork} from './interfaces/IOKPCGenesisArtwork.sol';
import {IOKPC} from './interfaces/IOKPC.sol';
import {Ownable} from '@openzeppelin/contracts/access/Ownable.sol';
import '@0xsequence/sstore2/contracts/SSTORE2.sol';
import 'hardhat/console.sol';

contract OKPCGenesisArtwork is IOKPCGenesisArtwork, Ownable {
  /* --------------------------------- ****** --------------------------------- */

  /* -------------------------------------------------------------------------- */
  /*                                   STORAGE                                  */
  /* -------------------------------------------------------------------------- */
  address public genesisArtworkForPC;
  IOKPC _okpc;

  /* --------------------------------- ****** --------------------------------- */

  /* -------------------------------------------------------------------------- */
  /*                                   ERRORS                                   */
  /* -------------------------------------------------------------------------- */
  error OKPCNotFound();
  error InvalidDataLength();

  /* --------------------------------- ****** --------------------------------- */

  /* -------------------------------------------------------------------------- */
  /*                               INITIALIZATION                               */
  /* -------------------------------------------------------------------------- */
  constructor(bytes memory artworks, address okpcAddress) {
    if (artworks.length != 8192) revert InvalidDataLength();
    genesisArtworkForPC = SSTORE2.write(artworks);

    _okpc = IOKPC(okpcAddress);
  }

  /* --------------------------------- ****** --------------------------------- */

  /* -------------------------------------------------------------------------- */
  /*                              GENESIS ARTWORK                               */
  /* -------------------------------------------------------------------------- */

  /// @notice
  /// @param
  function getGenesisArtwork(uint256 pcId)
    public
    view
    override
    returns (uint256 artId, IOKPC.Art memory)
  {
    if (pcId == 0 || pcId > 8192) revert OKPCNotFound();

    artId = uint256(
      abi.decode(SSTORE2.read(genesisArtworkForPC, pcId - 1, pcId), (uint8))
    );

    return (artId, _okpc.getGalleryArt(artId));
  }

  function getGenesisArtwork2(uint256 pcId) public view returns (uint8 artId) {
    // if (pcId == 0 || pcId > 8192) revert OKPCNotFound();

    artId = abi.decode(
      SSTORE2.read(genesisArtworkForPC, pcId - 1, pcId),
      (uint8)
    );

    return (artId);
  }
}

