pragma solidity ^0.8.7;
//SPDX-License-Identifier: Unlicensed

import "./libraries/Ownable.sol";
import "./interfaces/IRaffleWorldStorage.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "hardhat/console.sol";

contract RaffleWorldStorage is IRaffleWorldStorage, Ownable {
    struct Raffle {
        address tokenContract;
        uint256 ticketPrice;
        uint256 ticketsNumber;
        string raffleType;
        uint256 raffleStartDate;
        uint256 lockDays;
        string raffleName;
        uint256 totalAmount;
        bool canceled;
        string status;
    }

    struct RaffleIndices {
        uint256 raffles_index;
        uint256 active_raffles_index;
        uint256 raffles_by_type_index;
        uint256 active_raffles_by_type_index;
    }

    struct Partner {
        string name;
        address partnerAddress;
        uint256 amount;
    }

    struct Winner {
        address winnerAddress;
        address nftAddress;
        uint256 nftId;
    }

    struct Discount {
        uint256 minTickets;
        uint256 percentage;
    }

    struct Entry {
        address buyer;
        uint256 buy_time;
    }

    struct RaffleRandom {
        uint256 raffleId;
        uint256 random;
    }

    Raffle[] public raffles;
    mapping(uint256 => RaffleIndices) raffleIndices;
    mapping(uint256 => Partner) rafflePartners;
    mapping(uint256 => Winner[]) raffleWinners;
    mapping(uint256 => Discount[]) raffleDiscounts;
    mapping(uint256 => Entry[]) raffleEntries;
    mapping(uint256 => RaffleRandom) raffleRandom;

    uint256[] public active_raffles;
    uint256[] public bronze_raffles;
    uint256[] public active_bronze_raffles;
    uint256[] public silver_raffles;
    uint256[] public active_silver_raffles;
    uint256[] public gold_raffles;
    uint256[] public active_gold_raffles;
    uint256[] public diamond_raffles;
    uint256[] public active_diamond_raffles;

    address private raffleWorldContractAddress;

    constructor() {
        raffleWorldContractAddress = address(0);
    }

    modifier beforeRaffleStart(uint256 _raffleId) {
        require(
            block.timestamp < raffles[_raffleId].raffleStartDate,
            "You cannot update raffle params after it started!"
        );
        _;
    }

    modifier onlyAllowed() {
        _checkAllowed();
        _;
    }

    function raffleContract() public view returns (address) {
        return raffleWorldContractAddress;
    }

    function _checkAllowed() internal view {
        require(
            raffleContract() == _msgSender() || owner() == _msgSender(),
            "Caller is not awllowed!"
        );
    }

    function changeRaffleContractAddress(address _newRaffleContractAddress)
        public
        onlyOwner
    {
        require(
            _newRaffleContractAddress != address(0),
            "new raffle contract address is zero address!"
        );
        raffleWorldContractAddress = _newRaffleContractAddress;
    }

    function _checkRaffleParams(
        string memory _raffleType,
        uint256 _raffleStartDate
    ) internal view returns (bool) {
        bool raffleTypeValidity = false;
        if (keccak256(bytes(_raffleType)) == keccak256(bytes("bronze")))
            raffleTypeValidity = true;
        else if (keccak256(bytes(_raffleType)) == keccak256(bytes("silver")))
            raffleTypeValidity = true;
        else if (keccak256(bytes(_raffleType)) == keccak256(bytes("gold")))
            raffleTypeValidity = true;
        else if (keccak256(bytes(_raffleType)) == keccak256(bytes("diamond")))
            raffleTypeValidity = true;
        bool raffleStartDayValidity = false;
        if (_raffleStartDate >= block.timestamp) raffleStartDayValidity = true;
        return raffleTypeValidity && raffleStartDayValidity;
    }

    /**
     * @dev Used to create a new raffle
     */
    function AddRaffle(
        address _tokenContract,
        uint256 _ticketPrice,
        uint256 _ticketsNumber,
        string memory _raffleType,
        uint256 _raffleStartDate,
        uint256 _lockDays,
        string memory _raffleName,
        uint256 _totalAmount
    ) external override onlyAllowed {
        // checks if the type of the raffle and the start date provided are valid
        require(
            _checkRaffleParams(_raffleType, _raffleStartDate) == true,
            "Raffle type or start date incorrect!"
        );

        raffles.push(
            Raffle({
                tokenContract: _tokenContract,
                ticketPrice: _ticketPrice,
                ticketsNumber: _ticketsNumber,
                raffleType: _raffleType,
                raffleStartDate: _raffleStartDate,
                lockDays: _lockDays,
                raffleName: _raffleName,
                totalAmount: _totalAmount,
                canceled: false,
                status: "ongoing"
            })
        );

        emit NewRaffle(
            raffles.length - 1,
            _tokenContract,
            _ticketPrice,
            _ticketsNumber,
            _raffleType,
            _raffleStartDate,
            _lockDays,
            _raffleName,
            _totalAmount
        );

        ActivateRaffle(raffles.length - 1);
    }

    function ActivateRaffle(uint256 _raffleId) internal {
        raffles[_raffleId].canceled = false;

        raffleIndices[_raffleId].active_raffles_index = active_raffles.length;
        active_raffles.push(_raffleId);

        if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("bronze"))
        ) {
            bronze_raffles.push(_raffleId);
            active_bronze_raffles.push(_raffleId);
            raffleIndices[_raffleId].active_raffles_by_type_index =
                active_bronze_raffles.length -
                1;
        } else if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("silver"))
        ) {
            silver_raffles.push(_raffleId);
            active_silver_raffles.push(_raffleId);
            raffleIndices[_raffleId].active_raffles_by_type_index =
                active_silver_raffles.length -
                1;
        } else if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("gold"))
        ) {
            gold_raffles.push(_raffleId);
            active_gold_raffles.push(_raffleId);
            raffleIndices[_raffleId].active_raffles_by_type_index =
                active_gold_raffles.length -
                1;
        } else {
            diamond_raffles.push(_raffleId);
            active_diamond_raffles.push(_raffleId);
            raffleIndices[_raffleId].active_raffles_by_type_index =
                active_diamond_raffles.length -
                1;
        }

        emit RaffleActivated(_raffleId);
    }

    function GetRaffleStatus(uint256 _raffleId)
        external
        view
        override
        returns (string memory)
    {
        return raffles[_raffleId].status;
    }

    function CancelRaffle(uint256 _raffleId) external override onlyAllowed {
        require(raffles[_raffleId].canceled == false, "Raflfle is not active!");

        raffles[_raffleId].canceled = true;

        active_raffles[
            raffleIndices[_raffleId].active_raffles_index
        ] = active_raffles[
            raffleIndices[active_raffles[active_raffles.length - 1]]
                .active_raffles_index
        ];

        raffleIndices[active_raffles[active_raffles.length - 1]]
            .active_raffles_index = raffleIndices[_raffleId]
            .active_raffles_index;

        active_raffles.pop();

        if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("bronze"))
        ) {
            active_bronze_raffles[
                raffleIndices[_raffleId].active_raffles_by_type_index
            ] = active_bronze_raffles[active_bronze_raffles.length - 1];

            raffleIndices[
                active_bronze_raffles[active_bronze_raffles.length - 1]
            ].active_raffles_by_type_index = raffleIndices[_raffleId]
                .active_raffles_by_type_index;

            raffleIndices[_raffleId].active_raffles_by_type_index = 2**256 - 1;

            active_bronze_raffles.pop();
        } else if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("silver"))
        ) {
            active_silver_raffles[
                raffleIndices[_raffleId].active_raffles_by_type_index
            ] = active_silver_raffles[active_silver_raffles.length - 1];

            raffleIndices[
                active_silver_raffles[active_silver_raffles.length - 1]
            ].active_raffles_by_type_index = raffleIndices[_raffleId]
                .active_raffles_by_type_index;

            raffleIndices[_raffleId].active_raffles_by_type_index = 2**256 - 1;

            active_silver_raffles.pop();
        } else if (
            keccak256(bytes(raffles[_raffleId].raffleType)) ==
            keccak256(bytes("gold"))
        ) {
            active_gold_raffles[
                raffleIndices[_raffleId].active_raffles_by_type_index
            ] = active_gold_raffles[active_gold_raffles.length - 1];

            raffleIndices[active_gold_raffles[active_gold_raffles.length - 1]]
                .active_raffles_by_type_index = raffleIndices[_raffleId]
                .active_raffles_by_type_index;

            raffleIndices[_raffleId].active_raffles_by_type_index = 2**256 - 1;

            active_gold_raffles.pop();
        } else {
            active_diamond_raffles[
                raffleIndices[_raffleId].active_raffles_by_type_index
            ] = active_diamond_raffles[active_diamond_raffles.length - 1];

            raffleIndices[
                active_diamond_raffles[active_diamond_raffles.length - 1]
            ].active_raffles_by_type_index = raffleIndices[_raffleId]
                .active_raffles_by_type_index;

            raffleIndices[_raffleId].active_raffles_by_type_index = 2**256 - 1;

            active_diamond_raffles.pop();
        }

        emit RaffleCanceled(_raffleId);
    }

    function EndRaffle(uint256 _raffleId) external override {
        require(raffleContract() == _msgSender(), "Invalid caller!");
        raffles[_raffleId].status = "ended";
    }

    function IsCanceled(uint256 _raffleId)
        external
        view
        override
        returns (bool)
    {
        return raffles[_raffleId].canceled;
    }

    function SetRaffleTokenContract(uint256 _raffleId, address _tokenContract)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        raffles[_raffleId].tokenContract = _tokenContract;
        emit TokenContractAddressChanged(_raffleId, _tokenContract);
    }

    function GetRaffleTokenContract(uint256 _raffleId)
        external
        view
        override
        returns (address)
    {
        return raffles[_raffleId].tokenContract;
    }

    function SetRaffleTicketPrice(uint256 _raffleId, uint256 _ticketPrice)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        raffles[_raffleId].ticketPrice = _ticketPrice;
        emit TicketPriceChanged(_raffleId, _ticketPrice);
    }

    function GetRaffleTicketPrice(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffles[_raffleId].ticketPrice;
    }

    function SetRaffleTicketsNumber(uint256 _raffleId, uint256 _ticketsNumber)
        external
        override
        onlyAllowed
    {
        require(
            _ticketsNumber >= raffleEntries[_raffleId].length,
            "Needs to be bigger than bought tickets!"
        );
        raffles[_raffleId].ticketsNumber = _ticketsNumber;

        emit TicketsNumberChanged(_raffleId, _ticketsNumber);
    }

    function GetRaffleTicketsNumber(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffles[_raffleId].ticketsNumber;
    }

    function SetRaffleType(uint256 _raffleId, string memory _raffleType)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        require(
            keccak256(bytes(_raffleType)) == keccak256(bytes("bronze")) ||
                keccak256(bytes(_raffleType)) == keccak256(bytes("silver")) ||
                keccak256(bytes(_raffleType)) == keccak256(bytes("gold")) ||
                keccak256(bytes(_raffleType)) == keccak256(bytes("diamond")),
            "Invalid raffle type!"
        );
        raffles[_raffleId].raffleType = _raffleType;

        emit TypeChanged(_raffleId, _raffleType);
    }

    function GetRaffleType(uint256 _raffleId)
        external
        view
        override
        returns (string memory)
    {
        return raffles[_raffleId].raffleType;
    }

    function SetRaffleStartDate(uint256 _raffleId, uint256 _raffleStartDate)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        require(_raffleStartDate > block.timestamp, "Invalid start date!");
        raffles[_raffleId].raffleStartDate = _raffleStartDate;

        emit StartDateChanged(_raffleId, _raffleStartDate);
    }

    function GetRaffleStartDate(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffles[_raffleId].raffleStartDate;
    }

    function SetRaffleLockDays(uint256 _raffleId, uint256 _lockDays)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        raffles[_raffleId].lockDays = _lockDays;
        emit LockDaysChanged(_raffleId, _lockDays);
    }

    function GetRaffleLockDays(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffles[_raffleId].lockDays;
    }

    function SetRaffleName(uint256 _raffleId, string memory _raffleName)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        raffles[_raffleId].raffleName = _raffleName;
        emit NameChanged(_raffleId, _raffleName);
    }

    function GetRaffleName(uint256 _raffleId)
        external
        view
        override
        returns (string memory)
    {
        return raffles[_raffleId].raffleName;
    }

    function SetRaffleTotalAmount(uint256 _raffleId, uint256 _totalAmount) 
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
        {
            raffles[_raffleId].totalAmount = _totalAmount;
        }

    function GetRaffleTotalAmount(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffles[_raffleId].totalAmount;
    }

    function SetRafflePartner(
        uint256 _raffleId,
        string memory _name,
        address _partnerAddress,
        uint256 _amount
    ) external override onlyAllowed beforeRaffleStart(_raffleId) {
        rafflePartners[_raffleId].name = _name;
        rafflePartners[_raffleId].partnerAddress = _partnerAddress;
        rafflePartners[_raffleId].amount = _amount;

        emit PartnerAdded(_raffleId, _name, _partnerAddress, _amount);
    }

    function ResetRafflePartner(uint256 _raffleId)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        rafflePartners[_raffleId].name = "Raffle World";
        rafflePartners[_raffleId].partnerAddress = address(this);
        rafflePartners[_raffleId].amount = 0;

        emit PartnerReseted(_raffleId);
    }

    function GetRafflePartner(uint256 _raffleId)
        external
        view
        override
        returns (
            string memory,
            address,
            uint256
        )
    {
        return (
            rafflePartners[_raffleId].name,
            rafflePartners[_raffleId].partnerAddress,
            rafflePartners[_raffleId].amount
        );
    }

    function GetAllRafflesLength() external view override returns (uint256) {
        return raffles.length;
    }

    function GetRafflesLength(string memory _raffleType)
        external
        view
        override
        returns (uint256)
    {
        if (keccak256(bytes(_raffleType)) == keccak256(bytes("bronze"))) {
            return bronze_raffles.length;
        } else if (
            keccak256(bytes(_raffleType)) == keccak256(bytes("silver"))
        ) {
            return silver_raffles.length;
        } else if (keccak256(bytes(_raffleType)) == keccak256(bytes("gold"))) {
            return gold_raffles.length;
        } else {
            require(
                keccak256(bytes(_raffleType)) == keccak256(bytes("diamond")),
                "Incorrect raffle type!"
            );
            return diamond_raffles.length;
        }
    }

    function GetActiveRafflesLength(string memory _raffleType)
        external
        view
        override
        returns (uint256)
    {
        if (keccak256(bytes(_raffleType)) == keccak256(bytes("bronze"))) {
            return active_bronze_raffles.length;
        } else if (
            keccak256(bytes(_raffleType)) == keccak256(bytes("silver"))
        ) {
            return active_silver_raffles.length;
        } else if (keccak256(bytes(_raffleType)) == keccak256(bytes("gold"))) {
            return active_gold_raffles.length;
        } else {
            require(
                keccak256(bytes(_raffleType)) == keccak256(bytes("diamond")),
                "Incorrect raffle type!"
            );
            return active_diamond_raffles.length;
        }
    }

    function AddRafflePrize(
        uint256 _raffleId,
        address _nftAddress,
        uint256 _nftId
    ) external override onlyAllowed beforeRaffleStart(_raffleId) {
        IERC721 nftContract = IERC721(_nftAddress);
        require(
            nftContract.ownerOf(_nftId) == address(address(this)),
            "NFT needs to be trasnfered the contract first!"
        );
        raffleWinners[_raffleId].push(
            Winner({
                winnerAddress: address(0x00),
                nftAddress: _nftAddress,
                nftId: _nftId
            })
        );

        emit PrizeAdded(_raffleId, _nftAddress, _nftId);
    }

    function RemoveRafflePrize(uint256 _raffleId, uint256 _index)
        external
        override
        onlyAllowed
        beforeRaffleStart(_raffleId)
    {
        require(_index < raffleWinners[_raffleId].length, "Invalid index");
        emit PrizeRemoved(
            _raffleId,
            raffleWinners[_raffleId][_index].nftAddress,
            raffleWinners[_raffleId][_index].nftId
        );
        IERC721 nftContract = IERC721(
            raffleWinners[_raffleId][_index].nftAddress
        );
        nftContract.safeTransferFrom(
            address(this),
            _msgSender(),
            raffleWinners[_raffleId][_index].nftId,
            ""
        );
        raffleWinners[_raffleId][_index] = raffleWinners[_raffleId][
            raffleWinners[_raffleId].length - 1
        ];
    }

    function GetRafflePrize(uint256 _raffleId, uint256 _index)
        external
        view
        override
        returns (
            address,
            address,
            uint256
        )
    {
        return (
            raffleWinners[_raffleId][_index].winnerAddress,
            raffleWinners[_raffleId][_index].nftAddress,
            raffleWinners[_raffleId][_index].nftId
        );
    }

    function GetRafflePrizesLength(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffleWinners[_raffleId].length;
    }

    function AddRaffleWinner(
        uint256 _raffleId,
        uint256 _prizeIndex,
        uint256 _ticketIndex
    ) external override {
        require(
            raffleContract() == _msgSender(),
            "Only callable by the raffle contract!"
        );
        raffleWinners[_raffleId][_prizeIndex].winnerAddress = raffleEntries[
            _raffleId
        ][_ticketIndex].buyer;

        IERC721 nftContract = IERC721(
            raffleWinners[_raffleId][_prizeIndex].nftAddress
        );
        nftContract.safeTransferFrom(
            address(this),
            raffleWinners[_raffleId][_prizeIndex].winnerAddress,
            raffleWinners[_raffleId][_prizeIndex].nftId,
            ""
        );

        emit WinnerComputed(
            _raffleId,
            raffleWinners[_raffleId][_prizeIndex].winnerAddress,
            raffleWinners[_raffleId][_prizeIndex].nftAddress,
            raffleWinners[_raffleId][_prizeIndex].nftId
        );
    }

    function AddRaffleDiscount(
        uint256 _raffleId,
        uint256 _minTickets,
        uint256 _percentage
    ) external override onlyAllowed {
        require(_percentage <= 10000, "Invalid percentage!");
        raffleDiscounts[_raffleId].push(
            Discount({minTickets: _minTickets, percentage: _percentage})
        );
        emit DiscountAdded(_raffleId, _minTickets, _percentage);
    }

    function RemoveRaffleDiscount(uint256 _raffleId, uint256 _index)
        external
        override
        onlyAllowed
    {
        require(_index < raffleDiscounts[_raffleId].length, "Invalid index!");
        emit DiscountRemoved(
            _raffleId,
            raffleDiscounts[_raffleId][_index].minTickets,
            raffleDiscounts[_raffleId][_index].percentage
        );
        raffleDiscounts[_raffleId][_index] = raffleDiscounts[_raffleId][
            raffleDiscounts[_raffleId].length - 1
        ];
        raffleDiscounts[_raffleId].pop();
    }

    function GetRaffleDiscount(uint256 _raffleId, uint256 _index)
        external
        view
        override
        returns (uint256, uint256)
    {
        return (
            raffleDiscounts[_raffleId][_index].minTickets,
            raffleDiscounts[_raffleId][_index].percentage
        );
    }

    function GetMaximumApplicableDiscount(
        uint256 _raffleId,
        uint256 _ticketsNumber
    ) external view override returns (uint256) {
        uint256 discount = 0;
        for (uint256 i = 0; i < raffleDiscounts[_raffleId].length; i++) {
            if (raffleDiscounts[_raffleId][i].minTickets <= _ticketsNumber) {
                discount = raffleDiscounts[_raffleId][i].percentage;
            } else {
                break;
            }
        }
        return discount;
    }

    function GetRaffleDiscountsLength(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffleDiscounts[_raffleId].length;
    }

    function AddRaffleEntry(uint256 _raffleId, address _buyer)
        external
        override
        onlyAllowed
    {
        raffleEntries[_raffleId].push(
            Entry({buyer: _buyer, buy_time: block.timestamp})
        );
        emit TicketBought(_raffleId, _buyer);
    }

    function RemoveRaffleEntry(uint256 _raffleId, uint256 _index)
        external
        override
        onlyAllowed
        returns (address)
    {
        require(_index < raffleEntries[_raffleId].length, "Invalid index!");
        emit TicketWithdrawed(
            _raffleId,
            raffleEntries[_raffleId][_index].buyer,
            _index
        );
        raffleEntries[_raffleId][_index] = raffleEntries[_raffleId][
            raffleEntries[_raffleId].length - 1
        ];
        address buyer = raffleEntries[_raffleId][_index].buyer;
        raffleEntries[_raffleId].pop();

        return buyer;
    }

    function GetRaffleEntriesLength(uint256 _raffleId)
        external
        view
        override
        returns (uint256)
    {
        return raffleEntries[_raffleId].length;
    }

    function GetBuyTimeOfTicket(uint256 _raffleId, uint256 _index)
        external
        view
        override
        returns (uint256)
    {
        return raffleEntries[_raffleId][_index].buy_time;
    }

