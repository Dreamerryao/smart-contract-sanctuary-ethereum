pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";


import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

import "./interfaces/irc20.sol";
import "./interfaces/IKotoMoIIIHoctb.sol";
import "./interfaces/ICatVsElephant.sol";
import "./interfaces/IMarketplace.sol";

import "./ERC1155Receiver.sol";
import "hardhat/console.sol";

contract MarketplaceBasic is AccessControl, ERC1155Receiver {

    using ERC165Checker for address; 

    // bytes4 public constant IID_IMARKETPLACE = type(IMarketplace).interfaceId;
    bytes4 public constant IID_IERC20 = type(IERC20).interfaceId;
    bytes4 public constant IID_IERC721 = type(IERC721).interfaceId;
    bytes4 public constant IID_IERC1155 = type(IERC1155).interfaceId;

    // Events
    event itemCreated(address indexed _contract, uint256 _amount, string _uri);

    event itemListed(address indexed _contract, uint indexed _tokenId, uint256 indexed _amount, uint _price);
    event itemCanceled(address indexed _contract, address indexed _seller, uint _tokenId);
    event itemSold(address indexed _contract, address indexed _seller, address indexed _buyer, uint _tokenId, ERCType _tokenType);

    event itemAuctioned(address indexed _contract, uint indexed _tokenId, uint256 indexed _amount, uint _startPrice);
    event itemBidded(address _buyer, uint _bidAmount, address _contract, address _seller, uint _tokenId, ERCType _tokenType);
    event AuctionFinished(bool indexed _success, address indexed _seller, address indexed _lastBidder, address _tokenContract, uint _tokenId);

    enum ERCType {
        Erc721,
        Erc1155
    }

    struct saleTokenInfo {
        uint amount;
        uint price;
    }
    
    struct auctionTokenInfo {
        uint amount;
        uint startPrice;
        uint bidCounter;
        uint lastBid;
        address lastBidder;
        uint startDate;
        address ercContract;
    }

    uint public auctionDelay = 3 days;
    uint public minBidsForSuccessAuction = 3;

    mapping(address => mapping(ERCType => mapping(uint => saleTokenInfo))) public onSale;
    mapping(address => mapping(ERCType => mapping(uint => auctionTokenInfo))) public onAuction;

    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl, ERC1155Receiver) returns (bool) {
        return super.supportsInterface(interfaceId);// || interfaceId == IID_IMARKETPLACE;
    }

    function isERC721(address _contract) internal view returns (bool) {
        return _contract.supportsInterface(IID_IERC721);
    }    
    
    function isERC1155(address _contract) internal view returns (bool) {
        return _contract.supportsInterface(IID_IERC1155);
    }

    function createItem(address _contract, uint _amount, string memory _uri) public {
        if (isERC721(_contract)) {
            IKotoMoIIIHoctb(_contract).safeMint(msg.sender, _uri);
            emit itemCreated(_contract, _amount, _uri);
        } else if (isERC1155(_contract)) {
            ICatVsElephant(_contract).mintToken(msg.sender, _amount, "0x00",  _uri);
            emit itemCreated(_contract, _amount, _uri);
        } else {
            revert("Contract 404");
        }
    }
}

