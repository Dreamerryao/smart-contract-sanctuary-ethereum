pragma solidity ^0.8.0;

import "./interfaces/IMarketplace.sol";
import "./MarketplaceAuction.sol";

contract Marketplace is MarketplaceAuction {

    bytes4 public constant IID_IMARKETPLACE = type(IMarketplace).interfaceId;

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return super.supportsInterface(interfaceId) || interfaceId == IID_IMARKETPLACE;
    }

    // Sale

    function listItem(address _contract, uint _tokenId, uint _amount, uint _price) public {
        return listItemInt(_contract, _tokenId, _amount, _price);
    }

    function cancel(address _contract, uint _tokenId) public {
        return cancelInt(_contract, _tokenId);
    }

    function buyItem(address _erc20, address _contract, address _saller, uint _tokenId) public {
        return buyItemInt(_erc20, _contract, _saller, _tokenId);
    }

    // Token auction

    function listItemOnAuction(address _contract, uint _tokenId, uint _amount, uint _price) public {
        return listItemOnAuctionInt(_contract, _tokenId, _amount, _price);
    }

    function makeBid(address _erc20, uint _bidAmount, address _contract, address _seller, uint _tokenId) public {
        return makeBidInt(_erc20, _bidAmount, _contract, _seller, _tokenId);
    }

    function finishAuction(address _contract, address _seller, uint _tokenId) public {
        return finishAuctionInt(_contract, _seller, _tokenId);
    }

}

