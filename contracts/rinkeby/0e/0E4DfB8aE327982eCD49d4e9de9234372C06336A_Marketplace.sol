pragma solidity ^0.8.0;

interface IMarketplace {

    function createItem(address _contract, uint _amount, string memory _uri) external;
    function listItem(address _contract, uint _tokenId, uint _amount, uint _price) external;
    function cancel(address _contract, uint _tokenId) external;
    function buyItem(address _erc20, address _contract, address _saller, uint _tokenId) external;
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "./MarketplaceSell.sol";

contract MarketplaceAuction is MarketplaceSell {

    function listItemOnAuctionInt(address _contract, uint _tokenId, uint _amount, uint _price) internal {
        if (isERC721(_contract)) {
            IERC721(_contract).transferFrom(msg.sender, address(this), _tokenId);
            onAuction[msg.sender][ERCType.Erc721][_tokenId] = auctionTokenInfo(1, _price, 0, 0, address(0), block.timestamp, address(0)); 
            emit itemAuctioned(_contract, _tokenId, _amount, _price);
        } else if (isERC1155(_contract)) {
            IERC1155(_contract).safeTransferFrom(msg.sender, address(this), _tokenId, _amount, "0x00");
            onAuction[msg.sender][ERCType.Erc1155][_tokenId] = auctionTokenInfo(_amount, _price, 0, 0, address(0), block.timestamp, address(0));
            emit itemAuctioned(_contract, _tokenId, _amount, _price);
        } else {
            revert("Contract 404");
        }
    }

    function makeBidInt(address _erc20, uint _bidAmount, address _contract, address _seller, uint _tokenId) internal {
        if (isERC721(_contract)) {
            _makeBid(_erc20, _bidAmount, _contract, _seller, _tokenId, ERCType.Erc721);
        } else if (isERC1155(_contract)) {
            _makeBid(_erc20, _bidAmount, _contract, _seller, _tokenId, ERCType.Erc1155);
        } else {
            revert("Contract 404");
        }
    }

    function _makeBid(address _erc20, uint _bidAmount, address _contract, address _seller, uint _tokenId, ERCType _tokenType) internal {
            require(
                onAuction[_seller][_tokenType][_tokenId].amount > 0,
                "Can`t bid, token not on sale"
            );

            uint lastBid = onAuction[_seller][_tokenType][_tokenId].lastBid;

            if (lastBid == 0) {
                require(
                    IERC20(_erc20).balanceOf(msg.sender) > onAuction[_seller][_tokenType][_tokenId].startPrice,
                    "ERC20 amount is low, can't bid"
                );
                IERC20(_erc20).transferFrom(msg.sender, address(this), _bidAmount);
            } else {
                require(
                    IERC20(_erc20).balanceOf(msg.sender) > lastBid,
                    "ERC20 amount is low, can't bid"
                );
                require(
                    _bidAmount > lastBid,
                    "Bid lower than last bid"
                );

                IERC20(_erc20).transferFrom(msg.sender, address(this), _bidAmount);
                IERC20(_erc20).approve(msg.sender, lastBid);
                IERC20(_erc20).transfer(
                    onAuction[_seller][_tokenType][_tokenId].lastBidder,
                    lastBid
                );            
            }

            onAuction[_seller][_tokenType][_tokenId].lastBid = _bidAmount;
            onAuction[_seller][_tokenType][_tokenId].bidCounter += 1;
            onAuction[_seller][_tokenType][_tokenId].lastBidder = msg.sender;
            onAuction[_seller][_tokenType][_tokenId].ercContract = _erc20;
            emit itemBidded(msg.sender, _bidAmount, _contract, _seller, _tokenId, _tokenType);
    }

    function finishAuctionInt(address _contract, address _seller, uint _tokenId) internal {
        if (isERC721(_contract)) {
            _finishAuction(_contract, _seller, _tokenId, ERCType.Erc721);
        } else if (isERC1155(_contract)){
            _finishAuction(_contract, _seller, _tokenId, ERCType.Erc1155);
        } else {
            revert("Contract 404");
        }
    }

    function _finishAuction(address _contract, address _seller, uint _tokenId, ERCType _tokenType) internal {
        require(
            onAuction[_seller][_tokenType][_tokenId].amount > 0,
            "Can`t finish, token is not on sale"
        );

        if ((block.timestamp - onAuction[_seller][_tokenType][_tokenId].startDate) > auctionDelay) {
            if (onAuction[_seller][_tokenType][_tokenId].bidCounter >= minBidsForSuccessAuction) {
                // Success finish

                IERC20(onAuction[_seller][_tokenType][_tokenId].ercContract).transfer(
                    _seller,
                    onAuction[_seller][_tokenType][_tokenId].lastBid
                );
            

                if (_tokenType == ERCType.Erc721) {
                    IERC721(_contract).transferFrom(
                        address(this),
                        onAuction[_seller][_tokenType][_tokenId].lastBidder,
                        _tokenId
                    );
                } else {
                    IERC1155(_contract).safeTransferFrom(
                        address(this), 
                        onAuction[_seller][_tokenType][_tokenId].lastBidder,
                        _tokenId,
                        onAuction[_seller][ERCType.Erc1155][_tokenId].amount,
                        "0x00"
                    );
                }

                emit AuctionFinished(
                    true,
                    _seller,
                    onAuction[_seller][_tokenType][_tokenId].lastBidder,
                    _contract, 
                    _tokenId
                );

            } else {
                // Unsuccess finish
                if (onAuction[_seller][_tokenType][_tokenId].lastBid > 0) {
                    IERC20(onAuction[_seller][_tokenType][_tokenId].ercContract).transfer(
                        onAuction[_seller][_tokenType][_tokenId].lastBidder,
                        onAuction[_seller][_tokenType][_tokenId].lastBid
                    );
                }

                if (_tokenType == ERCType.Erc721) {
                    IERC721(_contract).transferFrom(
                        address(this),
                        _seller,
                        _tokenId
                    );
                } else {
                    IERC1155(_contract).safeTransferFrom(
                        address(this), 
                        _seller,
                        _tokenId,
                        onAuction[msg.sender][ERCType.Erc1155][_tokenId].amount,
                        "0x00"
                    );
                }

                emit AuctionFinished(
                    false,
                    _seller,
                    onAuction[_seller][_tokenType][_tokenId].lastBidder,
                    _contract, 
                    _tokenId
                );
            }
        } else {
            revert("Can`t finish, delay not pass");
        }
    }

}

