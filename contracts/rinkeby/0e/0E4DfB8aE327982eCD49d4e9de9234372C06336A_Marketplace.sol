pragma solidity ^0.8.0;

import "./MarketplaceBasic.sol";

contract MarketplaceSell is MarketplaceBasic {
    function listItemInt(address _contract, uint _tokenId, uint _amount, uint _price) internal {
        if (isERC721(_contract)) {
            IERC721(_contract).transferFrom(msg.sender, address(this), _tokenId);
            onSale[msg.sender][ERCType.Erc721][_tokenId] = saleTokenInfo(1, _price); 
            emit itemListed(_contract, _tokenId, _amount, _price);
        } else if (isERC1155(_contract)) {
            IERC1155(_contract).safeTransferFrom(msg.sender, address(this), _tokenId, _amount, "0x00");
            onSale[msg.sender][ERCType.Erc1155][_tokenId] = saleTokenInfo(_amount, _price);
            emit itemListed(_contract, _tokenId, _amount, _price);
        } else {
            revert("Contract 404");
        }
    }

    function cancelInt(address _contract, uint _tokenId) internal {
        if (isERC721(_contract)) {
            require(
                onSale[msg.sender][ERCType.Erc721][_tokenId].amount > 0,
                "Can`t cancel, token is not on sale"
            );
            // IERC721(_contract).approve(msg.sender, _tokenId);
            IERC721(_contract).transferFrom(address(this), msg.sender, _tokenId);
            
            onSale[msg.sender][ERCType.Erc721][_tokenId] = saleTokenInfo(0, 0);
            emit itemCanceled(_contract, msg.sender, _tokenId);
        } else if (isERC1155(_contract)) {
            uint amountToSale = onSale[msg.sender][ERCType.Erc1155][_tokenId].amount;

            require(
                amountToSale > 0,
                "Can`t cancel, token is not on sale"
            );

            IERC1155(_contract).safeTransferFrom(address(this), msg.sender, _tokenId, amountToSale, "0x00");
            onSale[msg.sender][ERCType.Erc1155][_tokenId] = saleTokenInfo(0, 0);
            emit itemCanceled(_contract, msg.sender, _tokenId);
        } else {
            revert("Contract 404");
        }
    }

    function buyItemInt(address _erc20, address _contract, address _saller, uint _tokenId) internal {
        // if (isERC20(_erc20) && isERC721(_contract)) {
        if (isERC721(_contract)) {
            _buyItem(_erc20, _contract, _saller, _tokenId, ERCType.Erc721);
        // } else if (isERC20(_erc20) && isERC1155(_contract)) {
        } else if (isERC1155(_contract)) {
            _buyItem(_erc20, _contract, _saller, _tokenId, ERCType.Erc1155);
        } else {
            revert("Contract 404");
        }
    }

    function _buyItem(address _erc20, address _contract, address _saller, uint _tokenId, ERCType _tokenType) internal {
            uint amountToSale = onSale[_saller][_tokenType][_tokenId].amount;
            uint tokenPrice = onSale[_saller][_tokenType][_tokenId].price;

            require(
                amountToSale > 0,
                "4"
            );
            require(
                IERC20(_erc20).balanceOf(msg.sender) > onSale[_saller][_tokenType][_tokenId].price,
                "ERC20 amount is not enought"
            );

            if (_tokenType == ERCType.Erc721) {
                IERC20(_erc20).transferFrom(msg.sender, _saller, tokenPrice);
                IERC721(_contract).transferFrom(address(this), msg.sender, _tokenId);
            } else {
                IERC20(_erc20).transferFrom(msg.sender, _saller, tokenPrice);
                IERC1155(_contract).safeTransferFrom(address(this), msg.sender, _tokenId, amountToSale, "0x00");
            }

            onSale[_saller][_tokenType][_tokenId] = saleTokenInfo(0, 0);
            emit itemSold(_contract, _saller, msg.sender, _tokenId, _tokenType);
    }

}

