contract ClaimNFTs is Ownable {
    using SafeMath for uint256;

    address[] public nftAddresses;
    uint256[] public tokenIDs;
    uint256 public claimNFTPrice = 0;
    uint256 public maxPerClaim;
    uint256 public claimedAmount;
    address public ownerOfNFT = 0x29028ec042Bf7648d61aBDFa27BF2E6147Fa1C5B;


    event Stake(address indexed user, uint256 amount);
    event Claim(address indexed user, uint256 amount);



    constructor(address[] memory tokenAddress, uint256[] memory ids) {
        nftAddresses = tokenAddress;
        tokenIDs = ids;
        maxPerClaim = 5;
        claimedAmount = 0;
    }

    function setNFTAddresses(address[] memory tokenAddress) public onlyOwner {
        nftAddresses = tokenAddress;
    }

    function setTokenIDs(uint256[] memory ids) public onlyOwner {
        tokenIDs = ids;
    }

    function setClaimPrice(uint256 price) public onlyOwner {
        claimNFTPrice = price;
    }

    function setMaxClaimAmount(uint256 amount) public onlyOwner {
        maxPerClaim = amount;
    }

    function getNFTPrice() public view returns (uint256) {
        return claimNFTPrice;
    }

    function claimNFT(uint256 cnt) public payable{
        require(claimNFTPrice.mul(cnt) == msg.value, "ETH value sent is not correct");
        require(cnt < maxPerClaim, "Exceed MaxPerClaimable Amount");

        for (uint256 i = 0; i < cnt; i++){
            IERC721(nftAddresses[claimedAmount]).transferFrom(ownerOfNFT, address(this), tokenIDs[claimedAmount]);
            IERC721(nftAddresses[claimedAmount]).transferFrom(address(this), msg.sender, tokenIDs[claimedAmount]);
            emit Claim(msg.sender, 1);
            claimedAmount++;
        }
        
    }

    function withdraw() public onlyOwner {
        uint balance = address(this).balance;
        address payable ownerAddress = payable(msg.sender);
        ownerAddress.transfer(balance);
    }

}