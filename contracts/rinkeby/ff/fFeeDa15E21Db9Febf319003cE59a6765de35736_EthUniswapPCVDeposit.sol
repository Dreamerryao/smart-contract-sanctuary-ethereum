// File: contracts/token/Tai.sol

pragma solidity 0.6.6;



contract Tai is ERC20, AccessControl {

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE");

    modifier onlyGovernor {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Tai: Caller is not the governor");
        _;
    }

    modifier onlyMinter() {
        require(hasRole(MINTER_ROLE, msg.sender), "Tai: Caller is not the minter");
        _;
    }

    modifier onlyBurner() {
        require(hasRole(BURNER_ROLE, msg.sender), "Tai: Caller is not the burner");
        _;
    }

    constructor() public ERC20("Tai Chi USD", "TAI") {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function mint(address to, uint256 amount) public onlyMinter {
        _mint(to, amount);
    }

    function burnFrom(address from, uint256 amount) public onlyBurner {
        _burn(from, amount);
    }

    function grantMinter(address minter) public onlyGovernor {
        _setupRole(MINTER_ROLE, minter);

    }

    function grantBurner(address burner) public onlyGovernor {
        _setupRole(BURNER_ROLE, burner);
    }

    function revokeMinter(address minter) public onlyGovernor{
        revokeRole(MINTER_ROLE, minter);
    }

    function revokeBurner(address burner) public onlyGovernor{
        revokeRole(BURNER_ROLE, burner);
    }

}

