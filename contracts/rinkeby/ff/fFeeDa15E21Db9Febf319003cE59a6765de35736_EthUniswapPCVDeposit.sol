pragma solidity ^0.6.6;














abstract contract PCVRef {
    using SafeERC20 for Tai;
    using Decimal for Decimal.D256;
    using Babylonian for uint256;
    using SignedSafeMath for int256;
    using SafeMathCopy for uint256;
    using SafeCast for uint256;
    using SafeCast for int256;
    using Address for address payable;
    using SafeERC20 for Tai;
//----------------event -----------------------
    event PairUpdate(address indexed _pair);

    /// @notice the Uniswap router contract
    IUniswapV2Router02 public  router;

    /// @notice the referenced Uniswap pair contract
    IUniswapV2Pair public  pair;

    IOracle public oracle;

    Tai tai;


    IUniswapV2Factory public constant UNISWAP_FACTORY = IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);
    

    constructor(
        address _token,
        address _router,
        address _oracle,
        address _tai
    ) public  {
       
        address _pair = UNISWAP_FACTORY.getPair(_tai, _token);
        oracle = IOracle(_oracle);
        _setupPair(_pair);
        tai = Tai(_tai);
        router = IUniswapV2Router02(_router);

        _approveToken(_tai);
        _approveToken(token());
        _approveToken(_pair);
    }

    function _setupPair(address _pair) internal {
        pair = IUniswapV2Pair(_pair);
        emit PairUpdate(_pair);
    }

    /// @notice approves a token for the router
    function _approveToken(address _token) internal {
        uint256 maxTokens = uint256(-1);
        IERC20(_token).approve(address(router), maxTokens);
    }

    function taiBalance() public view returns (uint256) {
        return tai.balanceOf(address(this));
    }

    function _burnTaiHeld() internal {
        tai.burnFrom(address(this), taiBalance());
    }

    /// @notice pair reserves with tai listed first
    function getReserves()
        public
        view
        returns (uint256 taiReserves, uint256 tokenReserves)
    {
        address token0 = pair.token0(); //tai地址
        (uint256 reserve0, uint256 reserve1, ) = pair.getReserves(); //tai 的储备量 另一个token的储备量
        (taiReserves, tokenReserves) = address(tai) == token0
            ? (reserve0, reserve1)
            : (reserve1, reserve0);
        return (taiReserves, tokenReserves);
    }

    /// @notice the address of the non-tai underlying token
    function token() public view returns (address) {
        address token0 = pair.token0();
        if (address(tai) == token0) {
            return pair.token1();
        }
        return token0;
    }

    function peg() public view returns (Decimal.D256 memory) {
        (Decimal.D256 memory _peg, bool valid) = oracle.read();
        require(valid, "OracleRef: oracle invalid");
        return _peg;
    }

    /// @notice returns true if price is below the peg
    function _isBelowPeg(Decimal.D256 memory peg) internal view returns (bool) {
        (Decimal.D256 memory price, , ) = _getUniswapPrice();
        return peg.lessThan(price);
    }

    /// @notice invert a peg price
    /// @param price the peg price to invert
    /// @return the inverted peg as a Decimal
    /// @dev the inverted peg would be X per tai
    function invert(Decimal.D256 memory price)
        public
        pure
        returns (Decimal.D256 memory)
    {
        return Decimal.one().div(price);
    }

    /// @notice calculate amount of the not tai token needed to trade back to the peg
    function _getAmountToPegOther(
        uint256 taiReserves,
        uint256 tokenReserves,
        Decimal.D256 memory peg
    ) internal pure returns (uint256) {
        return _getAmountToPeg(tokenReserves, taiReserves, invert(peg));
    }

    /// @notice return current percent distance from peg  返回当前距离peg距离的百分比
    /// @dev will return Decimal.zero() if above peg  大于peg 返回0
    function _getDistanceToPeg()
        internal
        view
        returns (Decimal.D256 memory distance)
    {
        (Decimal.D256 memory price, , ) = _getUniswapPrice(); //tai储备量和eth储备量的比率计算成price
        return _deviationBelowPeg(price, peg());  //peg() 是oracle 的价格
    }

    /// @notice utility for calculating absolute distance from peg based on reserves
    /// @param reserveTarget pair reserves of the asset desired to trade with
    /// @param reserveOther pair reserves of the non-traded asset
    /// @param peg the target peg reported as Target per Other
    function _getAmountToPeg(
        uint256 reserveTarget,
        uint256 reserveOther,
        Decimal.D256 memory peg
    ) internal pure returns (uint256) {
        uint256 radicand = peg.mul(reserveTarget).mul(reserveOther).asUint256();
        uint256 root = radicand.sqrt();
        if (root > reserveTarget) {
            return (root - reserveTarget).mul(1000).div(997);
        }
        return (reserveTarget - root).mul(1000).div(997);
    }

    /// @notice calculate amount of tai needed to trade back to the peg
    function _getAmountToPegTai(
        uint256 taiReserves,
        uint256 tokenReserves,
        Decimal.D256 memory peg
    ) internal pure returns (uint256) {
        return _getAmountToPeg(taiReserves, tokenReserves, peg);
    }

    /// @notice get uniswap price and reserves
    /// @return price reported as tai per X
    /// @return reserveTai tai reserves
    /// @return reserveOther non-tai reserves
    function _getUniswapPrice()
        internal
        view
        returns (
            Decimal.D256 memory,
            uint256 reserveTai,
            uint256 reserveOther
        )
    {
        (reserveTai, reserveOther) = getReserves();
        return (
            Decimal.ratio(reserveTai, reserveOther),
            reserveTai,
            reserveOther
        );
    }

    /// @notice get final uniswap price after hypothetical tai trade
    /// @param amountTai a signed integer representing tai trade. Positive=sell, negative=buy
    /// @param reserveTai tai reserves
    /// @param reserveOther non-tai reserves
    function _getFinalPrice(
        int256 amountTai,
        uint256 reserveTai,
        uint256 reserveOther
    ) internal pure returns (Decimal.D256 memory) {
        uint256 k = reserveTai.mul(reserveOther);
        int256 signedReserveTai = reserveTai.toInt256();
        int256 amountTaiWithFee = amountTai > 0 ? amountTai.mul(997).div(1000) : amountTai; // buys already have fee factored in on uniswap's other token side

        uint256 adjustedReserveTai = signedReserveTai.add(amountTaiWithFee).toUint256();
        uint256 adjustedReserveOther = k / adjustedReserveTai;
        return Decimal.ratio(adjustedReserveTai, adjustedReserveOther); // alt: adjustedReserveTai^2 / k
    }

    /// @notice return the percent distance from peg before and after a hypothetical trade
    /// @param amountIn a signed amount of tai to be traded. Positive=sell, negative=buy
    /// @return initialDeviation the percent distance from peg before trade
    /// @return finalDeviation the percent distance from peg after hypothetical trade
    /// @dev deviations will return Decimal.zero() if above peg
    function _getPriceDeviations(int256 amountIn)
        internal
        view
        returns (
            Decimal.D256 memory initialDeviation,
            Decimal.D256 memory finalDeviation,
            Decimal.D256 memory _peg,
            uint256 taiReserves,
            uint256 tokenReserves
        )
    {
        _peg = peg();

        (Decimal.D256 memory price, uint256 reserveTai, uint256 reserveOther) =
            _getUniswapPrice();
        initialDeviation = _deviationBelowPeg(price, _peg);

        Decimal.D256 memory finalPrice =
            _getFinalPrice(amountIn, reserveTai, reserveOther);
        finalDeviation = _deviationBelowPeg(finalPrice, _peg);

        return (
            initialDeviation,
            finalDeviation,
            _peg,
            reserveTai,
            reserveOther
        );
    }


    /// @notice get deviation from peg as a percent given price
    /// @dev will return Decimal.zero() if above peg
    function _deviationBelowPeg(
        Decimal.D256 memory price,
        Decimal.D256 memory peg
    ) internal pure returns (Decimal.D256 memory) {
        // If price <= peg, then tai is more expensive and above peg
        // In this case we can just return zero for deviation
        //如果价格<=peg，则tai更贵且高于peg
        //在这种情况下，我们可以只返回0作为偏差
        if (price.lessThanOrEqualTo(peg)) {
            return Decimal.zero();
        }
        Decimal.D256 memory delta = price.sub(peg, "Impossible underflow");
        return delta.div(peg);
    }


    /// @notice get deviation from peg as a percent given price
    /// @dev will return Decimal.zero() if above peg
    function deviationBelowPeg(
        Decimal.D256 calldata price,
        Decimal.D256 calldata peg
    ) external pure returns (Decimal.D256 memory) {
        return _deviationBelowPeg(price, peg);
    }

    /// @notice amount of pair liquidity owned by this contract
    /// @return amount of LP tokens
    function liquidityOwned() public view returns (uint256) {
        return pair.balanceOf(address(this));
    }

    /// @notice ratio of all pair liquidity owned by this contract  本合约拥有的所有交易对的流动性比率
    function _ratioOwned() internal view returns (Decimal.D256 memory) {
        uint256 balance = liquidityOwned();    //合约拥有的交易对的balanceOf  0.5* 100 
        uint256 total = pair.totalSupply();    //交易对的总供应
        return Decimal.ratio(balance, total); //返回比例
    }

    function _mintTai(uint256 amount) internal {
        tai.mint(address(this), amount);
    }
      
}

