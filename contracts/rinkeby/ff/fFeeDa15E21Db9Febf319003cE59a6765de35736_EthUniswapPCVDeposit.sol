contract EthUniswapPCVDeposit is IPCVDeposit,PCVRef {
    using Address for address payable;

    mapping (address => uint) public accounts;
    mapping (address => uint) public account_block;
    event getlog(uint256 a,uint256 b,uint256 c,uint256 d);

    uint256 PLEDGE = 46523;

    /// @notice Uniswap PCV Deposit constructor
    /// @param _router Uniswap Router
    /// @param _oracle oracle for reference
    /// @param _tai tai address
    /// @param _token WETH address
    constructor(
        address _router,
        address _oracle,
        address _tai,
        address _token
    ) public  PCVRef(_token, _router, _oracle, _tai){}

    receive() external payable {}

    /// @notice deposit tokens into the PCV allocation
    /// @param ethAmount of tokens deposited   //postGenesis 代表创世结束
    function deposit(uint256 ethAmount) external payable override{
        require(
            ethAmount == msg.value,
            "Bonding Curve: ent value does not equal inputS"
        );

        uint256 ethAmount = address(this).balance; // include any ETH dust from prior LP

        uint256 taiAmount = _getAmountTaiToDeposit(ethAmount); //根据存储eth数量*当前预言机价格 获得tai数量

        _addLiquidity(ethAmount, taiAmount); //添加流动性池

        _burnTaiHeld(); // burn any tai dust from LP 销毁tai

        emit Deposit(msg.sender, ethAmount);
    }

    function _removeLiquidity(uint256 liquidity)
        internal
        returns (uint256)
    {
        uint256 endOfTime = uint256(-1); //固定写法,代表没时效性
        (, uint256 amountWithdrawn) =
            router.removeLiquidityETH(
                address(tai), //tai合约地址
                liquidity, //移除的流动性的量
                0, //抽取代币的最小个数
                0, //抽取代币的最小个数
                address(this), //移除代币的目标地址
                endOfTime //时效性
            );
        return amountWithdrawn;
    }

    function _transferWithdrawn(address to, uint256 amount) internal  {
        payable(to).sendValue(amount);
    }

    function _addLiquidity(uint256 ethAmount, uint256 taiAmount) internal {
        _mintTai(taiAmount); //铸造tai

        uint256 endOfTime = uint256(-1); //没时效性
        router.addLiquidityETH{value: ethAmount}( //参数和remove 一样
            address(tai),
            taiAmount,
            0,
            0,
            address(this),
            endOfTime
        );
    }

    /// @notice withdraw tokens from the PCV allocation  撤销
    /// @param amountUnderlying of tokens withdrawn
    /// @param to the address to send PCV to   onlyPCVController
    function withdraw(address to, uint256 amountUnderlying)
        external override
    {
        uint256 totalUnderlying = totalValue();  //获得合约拥有的ETH存储量
        require(
            amountUnderlying <= totalUnderlying,
            "UniswapPCVDeposit: Insufficient underlying"
        );

        uint256 totalLiquidity = liquidityOwned(); ////合约拥有的交易对的balanceOf

        // ratio of LP tokens needed to get out the desired amount
        Decimal.D256 memory ratioToWithdraw =
            Decimal.ratio(amountUnderlying, totalUnderlying); //计算所需LPtoken的比率
        
        // amount of LP tokens factoring in ratio
        uint256 liquidityToWithdraw =
            ratioToWithdraw.mul(totalLiquidity).asUint256(); //计算所需的LPtoken数量

        uint256 amountWithdrawn = _removeLiquidity(liquidityToWithdraw); //移除流动性

        _transferWithdrawn(to, amountWithdrawn);

        _burnTaiHeld(); //销毁Tai

        emit Withdrawal(msg.sender, to, amountWithdrawn);
    }



    /// @notice returns total value of PCV in the Deposit  
    function totalValue() public view override returns (uint256) {
        (, uint256 tokenReserves) = getReserves(); //获得ETH 存储量
        return _ratioOwned().mul(tokenReserves).asUint256(); // 获得合约拥有的ETH存储量
    }

    function _getAmountTaiToDeposit(uint256 amountToken)
        internal
        view
        returns (uint256 amountTai)
    {
        return peg().mul(amountToken).asUint256();
    }

