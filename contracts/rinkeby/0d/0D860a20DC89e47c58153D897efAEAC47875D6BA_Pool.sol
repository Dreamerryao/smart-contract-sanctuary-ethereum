pragma solidity ^0.8.0;

import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import './ERC20.sol';
import './interfaces/IPool.sol';
import './libraries/Math.sol';
import './libraries/SafeTransfers.sol';

contract Pool is ERC20, IPool {
  using Math for uint;

  uint constant FEE = 1;
  uint public override constant MINIMUM_LIQUIDITY = 10**3;
  address public override token;
  address public override router;
  uint private ethReserve;
  uint private ercReserve;
  bool internal locked = false;

  modifier onlyRouter() {
    require(msg.sender == router, 'Pool:onlyRouter: Unauthorized Access');
    _;
  }

  modifier lock() {
    require(!locked, 'Pool: No re-entrancy');
    locked = true;
    _;
    locked = false;
  }

  constructor(address _token, address _router) ERC20('SpacePool', 'SPCP') {
    token = _token;
    router = _router;
  }

  function getReserves() public override view returns (uint _ethReserve, uint _ercReserve) {
    return (ethReserve, ercReserve);
  }

  function mint(address to) external override lock onlyRouter returns (uint liquidity) {
    uint eth = address(this).balance - ethReserve;
    uint erc = IERC20(token).balanceOf(address(this)) - ercReserve;
    uint ts = totalSupply();

    if (ts == 0) {
      liquidity = Math.sqrt(eth * erc) - MINIMUM_LIQUIDITY;
      _mint(address(0), MINIMUM_LIQUIDITY);
    } else {
      liquidity = Math.min(
        (eth * ts) / ethReserve,
        (erc * ts) / ercReserve
      );
    }

    require(liquidity > 0, 'Pool::deposit: Insufficient liquidity');
    _mint(to, liquidity);
    sync();
  }

  function burn(address to) external override lock onlyRouter returns (uint ethOut, uint ercOut) {
    uint ts = totalSupply();
    uint liquidity = balanceOf(to);

    ethOut = (liquidity * ethReserve) / ts;
    ercOut = (liquidity * ercReserve) / ts;

    require(ethOut > 0 && ercOut > 0, 'Pool::burn: Insufficient liquidity');

    _burn(to, liquidity);

    SafeTransfers.safeTransfer(token, to, ercOut);
    SafeTransfers.safeTransferETH(to, ethOut);
    sync();
  }

  function getAmountOut(uint eth, uint erc) public override view returns (uint out) {
    require((eth > 0 || erc > 0) && (eth == 0 || erc == 0), 'Pool::getAmountOut: Insufficient output amount');
    require(eth < ethReserve && erc < ercReserve, 'Pool::getAmountOut: Insufficient liquidity');

    uint x = ethReserve;
    uint y = ercReserve;
    uint k = x * y;

    if (eth > 0) {
      uint fee = eth * FEE / 100;
      uint y2  = k / (x + eth - fee);
      out = y - y2;
    } else {
      uint fee = erc * FEE / 100;
      uint x2  = k / (y + erc - fee);
      out = x - x2;
    }
  }

  function swap(address to) external override lock onlyRouter returns (uint amountOut) {
    uint eth = address(this).balance - ethReserve;
    uint erc = IERC20(token).balanceOf(address(this)) - ercReserve;
    uint amountOut = getAmountOut(eth, erc);

    require(amountOut > 0, "Pool::swap: Insufficient amount");
    require(to != token && to != address(this), 'Pool::swap: Invalid to destination');

    if (erc > 0) {
      ethReserve = ethReserve - amountOut;
      SafeTransfers.safeTransferETH(to, amountOut);
    } else {
      ercReserve = ercReserve - amountOut;
      SafeTransfers.safeTransfer(token, to, amountOut);
    }
    sync();
    emit Swap(to, eth, erc);

    return amountOut;
  }

  function sync() internal {
    ethReserve = address(this).balance;
    ercReserve = IERC20(token).balanceOf(address(this));
    emit Sync(ethReserve, ercReserve);
  }

  receive() external payable onlyRouter {}
}

