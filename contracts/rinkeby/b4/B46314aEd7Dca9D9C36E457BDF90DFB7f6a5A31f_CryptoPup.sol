pragma solidity ^0.8.13;

import "https://github.com/chiru-labs/ERC721A/blob/1843596cf863557fcd3bf0105222a7c29690af5c/contracts/ERC721A.sol";
import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/7a14f6c5953a1f2228280e6eb1dfee8e5c28d79a/contracts/token/common/ERC2981.sol";
import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/7a14f6c5953a1f2228280e6eb1dfee8e5c28d79a/contracts/security/ReentrancyGuard.sol";
import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/7a14f6c5953a1f2228280e6eb1dfee8e5c28d79a/contracts/access/Ownable.sol";
import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/7a14f6c5953a1f2228280e6eb1dfee8e5c28d79a/contracts/utils/Address.sol";
import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/7a14f6c5953a1f2228280e6eb1dfee8e5c28d79a/contracts/utils/Strings.sol";
import "https://github.com/Brechtpd/base64/blob/4d85607b18d981acff392d2e99ba654305552a97/base64.sol";

contract CryptoPup is ERC721A, ERC2981, Ownable, ReentrancyGuard {
    using Strings for uint256;

    /* Private Variables */
    struct MintTypes {
        uint256 _presaleMintsByAddress;
    }

    struct Coupon {
        bytes32 r;
        bytes32 s;
        uint8 v;
    }

    enum CouponType {
        Presale
    }

    enum SalePhase {
        Locked,
        Presale,
        PublicSale
    }

    address private immutable _teamAddress =
        0x20cF43601e6AE07457Db2333C94793A354bA9241;
    address private immutable _splitterAddress =
        0xE9BfCDDE27400194c1009049d913c7661e3A7cCE;
    address private _couponSigner;

    /* Public Variables */

    SalePhase public phase = SalePhase.Locked;
    bool public revealed;

    string public uriPrefix = "";
    string public uriSuffix = "";
    string public hiddenMetadataUri;

    uint256 public mintPrice = 0.05 ether;
    uint256 public maxSupply = 11111;
    uint256 public reserveSupply = 35;
    uint256 public maxMintAmountPerTx = 5;

    string public contractUri;

    uint256 public teamTokensMinted;
    mapping(address => MintTypes) public addressToMinted;

    constructor(
        address couponSigner,
        uint96 _royaltyFeesInBips
    ) ERC721A("Crypto Pup", "CPUP") {
        _couponSigner = couponSigner;
        setRoyaltyInfo(_splitterAddress, _royaltyFeesInBips);
        setContractURI("{\"name\":\"CryptoPup\",\"description\":\"DESCRIPTIONHERE\",\"image\":\"https://puu.sh/JlOxn/47d32a0788.png\",\"external_link\":\"cryptopupcrew.com\",\"seller_fee_basis_points\":1000,\"fee_recipient\":\"0x7F75b173F7bE4bA80b7fC6e08317B7d17A2b76be\"}");
    }

    /* Modifiers */

    modifier mintCompliance(uint256 quantity) {
        require(
            quantity > 0 && quantity <= maxMintAmountPerTx,
            "Invalid Mint Amount!"
        );
        require(totalSupply() + quantity <= maxSupply, "Max Supply Reached!");
        _;
    }

    /* Admin Functions */

    /* Self-Destruct the smart contract
     * @dev Remove this function for production
     * @notice self destructs the smart contract
     */
    function kill() public onlyOwner {
        selfdestruct(payable(owner()));
    }

    function withdraw() public onlyOwner nonReentrant {
        Address.sendValue(payable(_splitterAddress), address(this).balance);
    }

    /**
     * Reserve tokens for the team + marketing
     * @dev mints the number of tokens passedin as `quantity` to the _teamAddress
     * @param quantity the number of tokens to mint
     */
    function devReserveTokens(uint256 quantity)
        external
        onlyOwner
        nonReentrant
    {
        require(totalSupply() + quantity <= maxSupply, "Max Supply Reached!");
        require(
            quantity + teamTokensMinted <= reserveSupply,
            "Exceeds Reserved Amount for Team"
        );

        teamTokensMinted += quantity;

        _mint(_teamAddress, quantity);
    }

    /**
     * Advance Phase
     * @dev advance the sale phase state
     * @notice advances sale phase state incrementally
     */
    function enterPhase(SalePhase _phase) external onlyOwner {
        require(uint8(_phase) > uint8(phase), "Can Only Advance Phases");
        phase = _phase;
    }

    /**
     * Set Coupon Signer
     * @dev set the coupon signing wallet
     * @param couponSigner the new coupon signing wallet address
     */
    function setCouponSigner(address couponSigner) external onlyOwner {
        _couponSigner = couponSigner;
    }

    /**
     * Set Revealed Status
     * @dev set the revealed status of the contract
     * @param state the new revealed status
     */
    function setRevealed(bool state) public onlyOwner {
        revealed = state;
    }

    /**
     * Set Hidden Metadata URI
     * @dev set the hidden metadata URI
     * @param _hiddenMetadataUri the new hidden metadata URI
     */
    function setHiddenMetadataUri(string memory _hiddenMetadataUri)
        public
        onlyOwner
    {
        hiddenMetadataUri = _hiddenMetadataUri;
    }

    /**
     * Set URI Prefix
     * @dev set the URI prefix
     * @param _uriPrefix the new URI prefix
     */
    function setUriPrefix(string memory _uriPrefix) public onlyOwner {
        uriPrefix = _uriPrefix;
    }

    /**
     * Set URI Suffix
     * @dev set the URI suffix
     * @param _uriSuffix the new URI suffix
     */
    function setUriSuffix(string memory _uriSuffix) public onlyOwner {
        uriSuffix = _uriSuffix;
    }

    /**
     * Set Royalty Info
     * @dev set the ERC2981 royalty information
     * @param _receiver the receiving address for royalties
     * @param _royaltyFeesInBips the royalty fees in Bips
     */
    function setRoyaltyInfo(address _receiver, uint96 _royaltyFeesInBips)
        public
        onlyOwner
    {
        _setDefaultRoyalty(_receiver, _royaltyFeesInBips);
    }

    /**
     * Set Contract URI
     * @dev set the contract URI
     * @notice encodes the contract information into a base64 string
     * @param contractInfo the new contract information
     */
    function setContractURI(string memory contractInfo) public onlyOwner {
        contractUri = string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(bytes(contractInfo))
            )
        );
    }

    /* Helper Function */

    /**
     * Verify Coupon
     * @dev verify that the coupon sent was signed by the coupon signer
     * @notice valid coupons will include coupon signer, type [Presale], address, and allotted mints
     * @notice returns a boolean value
     * @param digest the digest
     * @param coupon the coupon
     */
    function _isVerifiedCoupon(bytes32 digest, Coupon memory coupon)
        internal
        view
        returns (bool)
    {
        address signer = ecrecover(digest, coupon.v, coupon.r, coupon.s);
        require(signer != address(0), "Zero Address");
        return signer == _couponSigner;
    }

    /* Minting Functions */

    /**
     * Mint Public Sale Tokens
     * @dev Minting function for public sale tokens
     * @param quantity the number of tokens being minted by sender
     */
    function mint(uint256 quantity)
        external
        payable
        mintCompliance(quantity)
        nonReentrant
    {
        require(phase == SalePhase.PublicSale, "Public Sale Not Active");

        _mint(_msgSender(), quantity);

        payable(_splitterAddress).call{value: msg.value}("");
    }

    /**
     * Mint Presale tokens
     * @dev minting function for tokens available during the Presale phase
     * @notice minting presale tokens requires a valid coupon, associated with wallet and allotted amount
     * @param quantity the number of tokens being minted by the sender
     * @param allotted the alloted number of tokens specified in the Presale coupon
     * @param coupon the signed coupon
     */
    function mintPresale(
        uint256 quantity,
        uint256 allotted,
        Coupon memory coupon
    ) external payable nonReentrant {
        require(phase == SalePhase.Presale, "Presale Not Active");
        bytes32 digest = keccak256(
            abi.encode(CouponType.Presale, allotted, _msgSender())
        );
        require(_isVerifiedCoupon(digest, coupon), "Invalid Coupon");
        require(
            quantity + addressToMinted[_msgSender()]._presaleMintsByAddress <
                allotted + 1,
            "Exceeds Max Allotted"
        );

        addressToMinted[_msgSender()]._presaleMintsByAddress += quantity;

        _mint(_msgSender(), quantity);

        payable(_splitterAddress).call{value: msg.value}("");
    }

    /**
     * Owner only minting to other addresses
     * @dev mints to a particular address
     * @notice mints tokens to the specified address
     * @param quantity number of tokens to mint in a single transaction
     * @param receiver address of the target destination
     */
    function mintForAddress(uint256 quantity, address receiver)
        public 
        mintCompliance(quantity)
        onlyOwner
        nonReentrant
    {
        _mint(receiver, quantity);
    }

    /* Read Only Functions */

    /**
     * Retrieval of a token's URI
     * @dev retrieves the URI of a token, returning the hidden URI if not yet revealed
     * @notice retrieves the URI of a token
     * @param tokenId the id of the token to retrieve the URI for
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        if (revealed == false) {
            return hiddenMetadataUri;
        }

        string memory currentBaseURI = _baseURI();
        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        tokenId.toString(),
                        uriSuffix
                    )
                )
                : "";
    }
    
    /**
     * Sale Phase Retrieval
     * @dev retrieves the current sale phase
     */
    function salePhase() public view returns (SalePhase) {
        return phase;
    }

    /**
     * Contract URI Retrieval
     * @dev retrieves the URI for the contract info
     */
    function contractURI() public view returns (string memory) {
        return contractUri;
    }

    /**
     * Base URI Override
     * @dev overrided to return the uriPrefix
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return uriPrefix;
    }

    /* Interface Override */

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721A, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

