    function checkBackupWallet(
        address[] calldata _backUpWallet,
        string memory uid
    ) internal view returns (bool) {
        address[] memory backupWallets = members[uid].backupWallets;
        bool walletExists = false;
        for (uint256 i = 0; i < backupWallets.length; i++) {
            for (uint256 j = 0; j < _backUpWallet.length; j++) {
                if (backupWallets[i] == _backUpWallet[j]) {
                    walletExists = true;
                }
            }
        }
        return walletExists;
    }

