    event walletUpdated(
        string uid,
        uint256 dateCreated,
        address[] deployedFactories,
        address[] backupWallets,
        address[] wallets,
        uint256 primaryWallet
    );
    // Event when updating factories
    event factoryUpdated(
        string uid,
        uint256 dateCreated,
        address[] deployedFactories,
        address[] backupWallets,
        address[] wallets,
        uint256 primaryWallet
    );

