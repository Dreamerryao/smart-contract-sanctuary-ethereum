    {
        string memory memberuid;
        for (uint256 i = 0; i < allMembers.length; i++) {
            address[] memory _wallets = allMembers[i].wallets;
            if (_wallets.length != 0) {
                for (uint256 j = 0; j < _wallets.length; j++) {
