import "@uniswap/v2-periphery/contracts/interfaces/IWETH.sol";

contract SwapDapp {
    mapping(address => bool) public acceptedTokens;
    address private owner;
    address public factory;
    address public router;
    uint256 public nftPriceInETH;

    address private WETH;
    address private GKC_CONTRACT;

    modifier onlyOwner() {
        require(msg.sender == owner, "Only owner can call this function");
        _;
    }

    constructor(
        address _factory,
        address _router,
        address _WETH,
        address _GKC_CONTRACT
    ) public {
        owner = msg.sender;
        factory = _factory;
        router = _router;
        WETH = _WETH;
        GKC_CONTRACT = _GKC_CONTRACT;
        nftPriceInETH = 0.02 ether;
    }

    function addToken(address _tokenAddress) public onlyOwner {
        acceptedTokens[_tokenAddress] = true;
    }

    function mintUsingToken(address _tokenAddress, uint256 _amountIn) public {
        require(acceptedTokens[_tokenAddress], "Token not accepted");
        require(
            calculateMinAmountOfTokenNeeded(_tokenAddress, nftPriceInETH) >=
                _amountIn,
            "Not enough tokens"
        );
        require(
            calculateMininumTargetAmountReceived(_tokenAddress, _amountIn) >=
                nftPriceInETH,
            "Not enough tokens will be received after swap"
        );

        // Collect the token from the user
        IERC20 token = IERC20(_tokenAddress);
        token.transferFrom(msg.sender, address(this), _amountIn);

        // Swap the token for ETH
        address[] memory path = new address[](2);
        path[0] = _tokenAddress;
        path[1] = WETH;

        UniswapV2Router02(payable(router)).swapTokensForExactETH(
            nftPriceInETH,
            _amountIn,
            path,
            address(this),
            block.timestamp
        );

        // Create Instance of the GKC contract
        IGambaKittiesClub gkc_contract = IGambaKittiesClub(GKC_CONTRACT);
        gkc_contract.mint{value: nftPriceInETH}(1);

        // Send the NFT to the buyer
        gkc_contract.transferFrom(address(this), msg.sender, 1);
    }

    // REMOVE BEFORE DEPLOYMENT
    // function convertETHToToken(address _tokenAddress) public payable onlyOwner {
    //     // IWETH(WETH).deposit{value: msg.value}();

    //     address[] memory path = new address[](2);
    //     path[0] = WETH;
    //     path[1] = _tokenAddress;

    //     UniswapV2Router02(payable(router)).swapExactETHForTokens{value: msg.value}(
    //         0,
    //         path,
    //         msg.sender,
    //         block.timestamp
    //     );
    // }

    function calculateMinAmountOfTokenNeeded(
        address _tokenAddress,
        uint256 _targetVolume
    ) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = _tokenAddress;
        path[1] = WETH;

        uint256[] memory amountInMins = UniswapV2Router02(payable(router))
            .getAmountsIn(_targetVolume, path);
        console.log("calculateMinAmountOfTokenNeeded %s", amountInMins[0]);
        return amountInMins[0];
    }

    function calculateMininumTargetAmountReceived(
        address _tokenAddress,
        uint256 _amountIn
    ) public view returns (uint256) {
        address[] memory path = new address[](2);
        path[0] = _tokenAddress;
        path[1] = WETH;

        uint256[] memory amountOutMins = UniswapV2Router02(payable(router))
            .getAmountsOut(_amountIn, path);
        console.log(
            "calculateMininumTargetAmountReceived %s",
            amountOutMins[path.length - 1]
        );
        return amountOutMins[path.length - 1];
    }

    function pairInfo(address tokenA, address tokenB)
        internal
        view
        returns (
            uint256 reserveA,
            uint256 reserveB,
            uint256 totalSupply
        )
    {
        IUniswapV2Pair pair = IUniswapV2Pair(
            UniswapV2Library.pairFor(factory, tokenA, tokenB)
        );

        totalSupply = pair.totalSupply();
        (uint256 reserves0, uint256 reserves1, ) = pair.getReserves();
        (reserveA, reserveB) = tokenA == pair.token0()
            ? (reserves0, reserves1)
            : (reserves1, reserves0);
    }

    function printReserve(address tokenA, address tokenB) public view {
        (uint256 reserveA, uint256 reserveB, uint256 totalSupply) = pairInfo(
            tokenA,
            tokenB
        );
        console.log("'%s' '%s' '%s'", reserveA, reserveB, totalSupply);
        uint256 price = getPrice(1 ether, tokenA, tokenB);
        console.log("1 WETH is equal to %d", price);
        console.log(
            "%s BGLD is equal to %s WETH",
            price,
            getPrice(price, tokenB, tokenA)
        );
        // console.log(reserveA, " ", reserveB, " ", totalSupply);
    }

    function getPrice(
        uint256 fromVol,
        address fromToken,
        address toToken
    ) public view returns (uint256) {
        (uint256 reserveA, uint256 reserveB, ) = pairInfo(fromToken, toToken);
        uint256 toVolume = (fromVol * reserveB) / (fromVol + reserveA);
        return toVolume;
    }

    function getOwner() public view returns (address) {
        return owner;
    }

    // function computeLiquidityShareValue(uint liquidity, address tokenA, address tokenB) pure external returns (uint tokenAAmount, uint tokenBAmount) {
    //     revert('TODO');
    // }
}

