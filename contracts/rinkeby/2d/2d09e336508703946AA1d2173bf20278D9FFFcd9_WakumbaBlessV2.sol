pragma solidity ^0.8.17;

import '@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import '../utils/WakumbaOwnedUpgradeable.sol';
import '../airdrop/SwordOfBravery.sol';
import '../staking/Faith.sol';
import '../DoDoFrensNFT.sol';

contract WakumbaBlessV2 is WakumbaOwnedUpgradeable, PausableUpgradeable, ReentrancyGuardUpgradeable {
    using ECDSAUpgradeable for bytes32;

    mapping(address => uint32) private __claimedSwordAmt;
    mapping(address => uint256) private __claimedFaithAmt;

    Faith private __faithContract;
    SwordOfBravery private __swordContract;

    // v2
    DoDoFrensNFT private __dodoContract;

    // init
    function initialize(address swordAddress, address faithAddress) public initializer {
        __WakumbaOwned_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        __swordContract = SwordOfBravery(swordAddress);
        __faithContract = Faith(faithAddress);
    }

    function setSwordContract(address addr) external onlyOwner {
        __swordContract = SwordOfBravery(addr);
    }

    function setFaithContract(address addr) external onlyOwner {
        __faithContract = Faith(addr);
    }

    function setDoDoContract(address addr) external onlyOwner {
        __dodoContract = DoDoFrensNFT(addr);
    }

    function balanceOf(address addr) public view virtual returns (uint256) {
        return __dodoContract.balanceOf(addr);
    }

    function isValidSignature(
        address addr,
        uint256 amount,
        uint256 limit,
        bytes32 nonce,
        bytes calldata signature
    ) public view returns (bool) {
        bytes32 messageHash = keccak256(abi.encodePacked(addr, amount, limit, nonce));
        return owner() == messageHash.toEthSignedMessageHash().recover(signature);
    }

    function airdropFaith(
        address to,
        uint256 amount,
        uint256 maxAmt,
        bytes32 nonce,
        bytes calldata signature
    ) public nonReentrant whenNotPaused {
        require(_msgSender() == to, 'Invalid caller');
        require(isValidSignature(to, amount, maxAmt, nonce, signature), 'Invalid signature');
        __claimedFaithAmt[_msgSender()] += amount;
        __faithContract.mint(to, amount);
        require(__claimedFaithAmt[_msgSender()] <= maxAmt, 'max amount exceeded');
    }

    function getClaimedFaithAmt(address addr) external view returns (uint256) {
        return __claimedFaithAmt[addr];
    }

    function airdropSword(
        address to,
        uint16 amount,
        uint16 maxAmt,
        bytes32 nonce,
        bytes calldata signature
    ) public nonReentrant whenNotPaused {
        require(_msgSender() == to, 'Invalid caller');
        require(isValidSignature(to, amount, maxAmt, nonce, signature), 'Invalid signature');
        __claimedSwordAmt[_msgSender()] += amount;
        __swordContract.mint(
            to,
            0, /* Sword ID, only airdrop the common sword */
            amount,
            abi.encodePacked(maxAmt)
        );
        require(__claimedSwordAmt[_msgSender()] <= maxAmt, 'max amount exceeded');
    }

    function getClaimedSwordAmt(address addr) external view returns (uint32) {
        return __claimedSwordAmt[addr];
    }

    // TODO: Claim Mountain
}

