pragma solidity ^0.8.13;
pragma experimental ABIEncoderV2;

import "solmate/tokens/ERC1155.sol";
import "solmate/tokens/ERC721.sol";
import "solmate/auth/Owned.sol";
import "openzeppelin/contracts/proxy/utils/Initializable.sol";
import "openzeppelin/contracts/utils/Strings.sol";

import "forge-std/console.sol";

contract Plus1 is ERC1155, Initializable, Owned {
    error MintingDisabled();
    error Unauthorized();
    error NotSupported();
    error AlreadyMinted();
    error NotFound();

    string public name;
    string public symbol;
    string public _baseURI;
    ERC721 public original;
    uint8 public limit;
    bool public mintEnabled;

    mapping(uint256 => address[]) private _owners;

    constructor() Owned(address(0)) {}

    function init(
        address _owner,
        string memory _name,
        string memory _symbol,
        ERC721 _original,
        uint8 _limit,
        string memory initialBaseURI
    ) external initializer {
        owner = _owner;
        name = _name;
        symbol = _symbol;
        original = _original;
        limit = _limit;
        _baseURI = initialBaseURI;
        mintEnabled = true;
    }

    function uri(uint256 id)
        public
        view
        virtual
        override
        returns (string memory)
    {
        return string.concat(_baseURI, Strings.toString(id));
    }

    ////////////////////////////////////////////////////////////////
    //                  MINT & BURN THE PLUS1 TOKENS
    ////////////////////////////////////////////////////////////////

    function mint(uint256 tokenID) public {
        if (!mintEnabled) {
            revert MintingDisabled();
        }
        if (original.ownerOf(tokenID) != msg.sender) {
            revert Unauthorized();
        }
        if (_owners[tokenID].length != 0) {
            revert AlreadyMinted();
        }
        _owners[tokenID].push(msg.sender);

        uint256[] memory ids = new uint256[](1);
        ids[0] = tokenID;

        uint256[] memory amounts = new uint256[](1);
        amounts[0] = limit;

        _batchMint(msg.sender, ids, amounts, "");
    }

    ////////////////////////////////////////////////////////////////
    //                        ERC-721 OVERRIDES
    ////////////////////////////////////////////////////////////////

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        require(
            msg.sender == original.ownerOf(id) ||
                msg.sender == from ||
                isApprovedForAll[from][msg.sender],
            "NOT_AUTHORIZED"
        );

        balanceOf[from][id] -= amount;
        balanceOf[to][id] += amount;
        _updateOwners(id, amount, from, to);

        emit TransferSingle(msg.sender, from, to, id, amount);

        require(
            to.code.length == 0
                ? to != address(0)
                : ERC1155TokenReceiver(to).onERC1155Received(
                    msg.sender,
                    from,
                    id,
                    amount,
                    data
                ) == ERC1155TokenReceiver.onERC1155Received.selector,
            "UNSAFE_RECIPIENT"
        );
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] calldata ids,
        uint256[] calldata amounts,
        bytes memory data
    ) public virtual override {
        require(ids.length == amounts.length, "LENGTH_MISMATCH");

        require(
            msg.sender == from || isApprovedForAll[from][msg.sender],
            "NOT_AUTHORIZED"
        );

        // Storing these outside the loop saves ~15 gas per iteration.
        uint256 id;
        uint256 amount;

        for (uint256 i = 0; i < ids.length; ) {
            require(msg.sender == original.ownerOf(id), "NOT_AUTHORIZED");

            id = ids[i];
            amount = amounts[i];

            balanceOf[from][id] -= amount;
            balanceOf[to][id] += amount;

            _updateOwners(id, amount, from, to);

            // An array can't have a total length
            // larger than the max uint256 value.
            unchecked {
                ++i;
            }
        }

        emit TransferBatch(msg.sender, from, to, ids, amounts);

        require(
            to.code.length == 0
                ? to != address(0)
                : ERC1155TokenReceiver(to).onERC1155BatchReceived(
                    msg.sender,
                    from,
                    ids,
                    amounts,
                    data
                ) == ERC1155TokenReceiver.onERC1155BatchReceived.selector,
            "UNSAFE_RECIPIENT"
        );
    }

    // Ensure the array of owners reflects the new ownership after a transfer
    function _updateOwners(
        uint256 id,
        uint256 amount,
        address from,
        address to
    ) internal {
        uint256 fromBalance = balanceOf[from][id];
        uint256 toBalance = balanceOf[to][id];
        address[] storage newOwners = _owners[id];

        // If the new balance is 0, remove `from` from the array
        if (fromBalance == 0) {
            address[] memory currentOwners = _owners[id];
            delete _owners[id];
            for (uint256 i = 0; i < currentOwners.length; ++i) {
                address o = currentOwners[i];
                if (o == from) {
                    continue;
                }
                newOwners.push(o);
            }
        }

        // If the new toBalance is the same as the amount,
        // they are a new owner so need a new entry

        if (toBalance == amount) {
            newOwners.push(to);
        }
    }

    ////////////////////////////////////////////////////////////////
    //                        PLUS1 FUNCTIONS
    ////////////////////////////////////////////////////////////////

    function owners(uint256 tokenID) external view returns (address[] memory) {
        return _owners[tokenID];
    }

    function originalOwner(uint256 tokenID) external view returns (address) {
        return original.ownerOf(tokenID);
    }

    function recall(uint256 tokenID) external {
        if (original.ownerOf(tokenID) != msg.sender) {
            revert Unauthorized();
        }
        address recaller = msg.sender;
        bool isContract = recaller.code.length != 0;

        address[] memory currentOwners = _owners[tokenID];
        for (uint256 i = 0; i < currentOwners.length; i++) {
            address from = currentOwners[i];
            uint256 amount = balanceOf[from][tokenID];
            balanceOf[recaller][tokenID] += amount;
            delete balanceOf[from][tokenID];

            emit TransferSingle(msg.sender, from, recaller, tokenID, amount);

            if (isContract) {
                require(
                    ERC1155TokenReceiver(recaller).onERC1155Received(
                        msg.sender,
                        from,
                        tokenID,
                        amount,
                        ""
                    ) == ERC1155TokenReceiver.onERC1155Received.selector,
                    "UNSAFE_RECIPIENT"
                );
            }
        }
        delete _owners[tokenID];
        _owners[tokenID].push(recaller);
    }

    ////////////////////////////////////////////////////////////////
    //                        OWNER OPS
    ////////////////////////////////////////////////////////////////

    function setBaseURI(string calldata newBaseURI) external onlyOwner {
        _baseURI = newBaseURI;
    }

    function setMintEnabled(bool enabled) external onlyOwner {
        mintEnabled = enabled;
    }

    ////////////////////////////////////////////////////////////////
    //                        PRIVATE UTILS
    ////////////////////////////////////////////////////////////////

    // function _exists(uint256 originalID) internal view returns (bool) {
    //     return derivedIDs[originalID].length != 0;
    // }
}

