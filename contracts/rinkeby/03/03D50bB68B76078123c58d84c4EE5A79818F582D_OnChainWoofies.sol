    mapping(uint16 => uint256) private tokenIdToTraitHash;


    uint16  public constant MAX_DOGS                = 50;
    uint8   public constant MAX_TEAM_RESERVE        = 10;
    uint8   public constant MAX_MINT_PER_TX         = 5;
    uint256 public constant MINT_PRICE              = 1000000000000000;       // 0.001 eth follow 18 decimal standard



    uint16  public  m_uTokenCounter   = 0;
    uint8   public  m_uTokensReserved = 0;
    uint16  private m_uNonce = 0;


    enum MintState{eDev, eOpen, eClosed}
    MintState public m_eMintState = MintState.eDev;


    address private m_withdrawAddress;

    constructor(address _withdrawAddress) public
    ERC721("On Chain Woofies","WOOFZ")
    {
        m_withdrawAddress = _withdrawAddress;
    }

    modifier onlyOwnerOf(uint16 _tokenId)
    {
        require(ownerOf(_tokenId) == msg.sender, "You are not the owner of this token!");
        _;
    }

    function ReserveMint(uint8 _qty) external onlyOwner
    {
        require(m_uTokenCounter + _qty <= MAX_DOGS, "Minting the desired quantity exceeds supply!");
        require(m_uTokensReserved + _qty <= MAX_TEAM_RESERVE, "You are the dev, you know you cannot mint anymore.");
        m_uTokensReserved += _qty;
        _Mint(_qty, msg.sender);
    }

    function MintForSelf(uint256 _qty) external payable
    {
        require(m_eMintState == MintState.eOpen, "Public sale is not open!");
        require(m_uTokenCounter + _qty <= MAX_DOGS, "Minting the desired quantity exceeds supply!");
        require(_qty <= MAX_MINT_PER_TX, "Only up to x tokens per transaction!");
        require(msg.value >= MINT_PRICE * _qty, "Not enough ether for the desired quantity, check your math!");
        _Mint(_qty, msg.sender);
    }

    function MintForFriend(uint256 _qty, address _address) external payable
    {
        require(m_eMintState == MintState.eOpen, "Public sale is not open!");
        require(m_uTokenCounter + _qty <= MAX_DOGS, "Minting the desired quantity exceeds supply!");
        require(_qty <= MAX_MINT_PER_TX, "Only up to x tokens per transaction!");
        require(msg.value >= MINT_PRICE * _qty, "Not enough ether for the desired quantity, check your math!");
        _Mint(_qty, _address);
    }

    function _Mint(uint256 _qty, address _sendTo) private
    {
        for(uint16 i = 0; i < _qty; i++)
        {
            m_uTokenCounter++;
            uint16 tokenId = m_uTokenCounter;

            m_uNonce++;
            tokenIdToTraitHash[tokenId] = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, tokenId, msg.sender, m_uNonce)));
            _safeMint(_sendTo, tokenId);
        }
        if(m_uTokenCounter == MAX_DOGS)
        {
            closeMint();
        }
    }

    function closeMint() private
    {
        m_eMintState = MintState.eClosed;
    }

    function setMintState(uint8 _mintState) external onlyOwner
    {
        require(_mintState > uint8(m_eMintState), "No Back stepping mint order!");
        require(((_mintState >= 0) && (_mintState <= 2)), "Not a valid mint state!");
        m_eMintState = MintState(_mintState);
    }

    function tokenURI(uint256 _tokenId) override public view returns (string memory)
    {
        require(_exists(_tokenId), "This token does not exist!");
        ImageObject memory dog = constructDogFromHash(tokenIdToTraitHash[SafeCast.toUint16(_tokenId)]);
        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "On Chain Woofies #', toString(_tokenId), '", "description": "These WOOFZ were generated entirely on chain!"', getTraitsString(dog), '"image": "data:image/svg+xml;base64,', Base64.encode(bytes(getSVG(dog))), '"}'))));
        json = string(abi.encodePacked('data:application/json;base64,', json));
        return json;
    }

    function withdraw() external onlyOwner
    {
        payable(m_withdrawAddress).transfer(address(this).balance);
    }

    function getLayer0(uint256 _hash) internal pure returns (uint256)
    {
        uint256 rand = random(string(abi.encodePacked("Layer 0", toString(_hash))));
        rand %= 3;

        return rand;
    }

    function getLayer1(uint256 _hash) internal pure returns (uint256)
    {
        uint256 rand = random(string(abi.encodePacked("Layer 1", toString(_hash))));
        rand %= 3;

        return rand;
    }

    function getLayer2(uint256 _hash) internal pure returns (uint256)
    {
        uint256 rand = random(string(abi.encodePacked("Layer 2", toString(_hash))));
        rand %= 3;

        return rand;
    }

    function constructDogFromHash(uint256 _hash) private view returns (ImageObject memory)
    {
        ImageObject memory dog;

        dog.layer0                = getLayer0(_hash);
        dog.layer1                = getLayer1(_hash);
        dog.layer2                = getLayer2(_hash);

        return dog;
    }

    function getSVG(ImageObject memory dog) internal view returns (string memory) {
        string[5] memory parts;

        // open the xml+svg object
        parts[0] = '<svg id="x" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 30 30">';

        // overlay layers
        parts[1] = layer0Art[dog.layer0];
        parts[2] = layer1Art[dog.layer1];
        parts[3] = layer2Art[dog.layer2];

        // close the xml+svg object
        parts[4] = '<style>#x{shape-rendering: crispedges;}</style></svg>';

        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4]));

        return output;
    }

    function getTraitsString(ImageObject memory dog) internal view returns (string memory)
    {
        string[7] memory parts;

        parts[0] = ', "attributes": [{"trait_type": "Background","value": "';
        parts[1] = layer0Names[dog.layer0];

        parts[2] = '"}, {"trait_type": "Type","value": "';
        parts[3] = layer1Names[dog.layer1];

        parts[4] = '"}, {"trait_type": "Swagg","value": "';
        parts[5] = layer2Names[dog.layer2];

        parts[6] = '"}], ';

        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4], parts[5], parts[6]));
        return output;
    }


    function random(string memory input) internal pure returns (uint256)
    {
        return uint256(keccak256(abi.encodePacked(input)));
    }

    function toString(uint256 value) internal pure returns (string memory)
    {
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

library Base64 {
    bytes internal constant TABLE = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";

    function encode(bytes memory data) internal pure returns (string memory) {
        uint256 len = data.length;
        if (len == 0) return "";

        uint256 encodedLen = 4 * ((len + 2) / 3);

        bytes memory result = new bytes(encodedLen + 32);

        bytes memory table = TABLE;

        assembly {
            let tablePtr := add(table, 1)
            let resultPtr := add(result, 32)

            for {
                let i := 0
            } lt(i, len) {

            } {
                i := add(i, 3)
                let input := and(mload(add(data, i)), 0xffffff)

                let out := mload(add(tablePtr, and(shr(18, input), 0x3F)))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(12, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(6, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(input, 0x3F))), 0xFF))
                out := shl(224, out)

                mstore(resultPtr, out)

                resultPtr := add(resultPtr, 4)
            }

            switch mod(len, 3)
            case 1 {
                mstore(sub(resultPtr, 2), shl(240, 0x3d3d))
            }
            case 2 {
                mstore(sub(resultPtr, 1), shl(248, 0x3d))
            }

            mstore(result, encodedLen)
        }

        return string(result);
    }
}

