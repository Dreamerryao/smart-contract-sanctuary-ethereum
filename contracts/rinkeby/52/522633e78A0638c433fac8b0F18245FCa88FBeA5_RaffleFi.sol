pragma solidity ^0.8.0;

import "./IERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "hardhat/console.sol";

struct Raffle {
    bool started;
    bool finished;
    bool canceled;
    uint32 deadline;
    address owner;
    address collection;
    address winner;
    uint256 reservePrice;
    uint256 tokenId;
    uint256 storedBlockNumber;
    bytes32 seed;
}

contract RaffleFi is IERC721Receiver {
    using Counters for Counters.Counter;
    Counters.Counter public _raffleCount;

    event TicketsBougth(address account, uint256 tickets);
    event RaffleCreated(
        uint256 id,
        address owner,
        address collection,
        uint256 tokenId
    );
    event RaffleFinished(uint256 id);
    event RaffleWinner(uint256 id, address winner);
    event RaffleCanceled(uint256 id);

    bool internal locked;
    address public owner;

    mapping(uint256 => Raffle) public raffles;
    mapping(address => uint256[]) public ownerRaffles;
    mapping(uint256 => address[]) players;
    mapping(uint256 => mapping(address => uint256)) numOfTickets;

    uint256 public ticketPrice = .01 ether;
    uint256 public fee = 25; //2.5%

    constructor() {
        owner = msg.sender;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    modifier onlyCreator(uint256 raffleId) {
        require(msg.sender == raffles[raffleId].owner);
        _;
    }

    modifier noReentrant() {
        require(!locked, "No re-entrancy");
        locked = true;
        _;
        locked = false;
    }

    /**
     * Buy as many tickets as gas limit allows
     */
    function buy(uint256 raffleId) external payable {
        Raffle storage raffle = raffles[raffleId];
        require(
            raffle.started && !raffle.finished && !raffle.canceled,
            "Not started"
        );
        require(
            msg.value % ticketPrice == 0 && msg.value >= ticketPrice,
            "Incorrect value"
        );

        uint256 numberOfEntries = msg.value / ticketPrice;

        for (uint256 index = 0; index < numberOfEntries; index++) {
            players[raffleId].push(msg.sender);
        }
        numOfTickets[raffleId][msg.sender] += numberOfEntries;
        emit TicketsBougth(msg.sender, numberOfEntries);
    }

    function numPlayers(uint256 _raffleId) external view returns (uint256) {
        return players[_raffleId].length;
    }

    /**
    * Create raffle
    * todo add onlyOwner modifier for v1
    */
    function createRaffle(
        address _collection,
        uint256 tokenId,
        uint256 reservePrice,
        uint32 deadline
    ) external returns (uint256 _raffleId) {
        //deadline to be bigger than current timestamp
        require(deadline > block.timestamp + 60 * 60 * 24, "Min 1 day");
        require(reservePrice > ticketPrice, "Bad reserve price");
        IERC721 collection = IERC721(_collection);
        //transfer NFT to this contract
        collection.safeTransferFrom(msg.sender, address(this), tokenId);
        require(
            collection.ownerOf(tokenId) == address(this),
            "Failed with NFT transfer"
        );

        Raffle memory raffle;
        raffle.deadline = deadline;
        raffle.owner = msg.sender;
        raffle.collection = _collection;
        raffle.tokenId = tokenId;
        raffle.reservePrice = reservePrice;
        raffle.started = true;

        _raffleId = _raffleCount.current();
        _raffleCount.increment();

        raffles[_raffleId] = raffle;
        ownerRaffles[msg.sender].push(_raffleId);
        emit RaffleCreated(_raffleId, msg.sender, _collection, tokenId);
    }

    /**
     * Finish raffle when deadline is reached
     */
    function finishRaffle(uint256 raffleId, bytes32 _seed) external onlyOwner {
        Raffle storage raffle = raffles[raffleId];
        require(!raffle.finished && !raffle.canceled, "Invalid state");
        require(raffle.deadline <= block.timestamp, "Deadline not reached");

        raffle.finished = true;
        raffle.seed = _seed;
        raffle.storedBlockNumber = block.number + 1;

        emit RaffleFinished(raffleId);
    }

    /**
     * Internal function to pick random winner. If reserve price is not met when deadline is reached NFT is returned to creator and winner receives ETH. Otherwise winner gets NFT and creator receives ETH.
     */
    function pickWinner(uint256 raffleId) external onlyOwner {
        Raffle storage raffle = raffles[raffleId];
        require(raffle.canceled || raffle.finished, "Not finished");
        require(raffle.storedBlockNumber < block.number, "Block number");

        IERC721 collection = IERC721(raffle.collection);
        if (players[raffleId].length == 0) {
            collection.safeTransferFrom(
                address(this),
                raffle.owner,
                raffle.tokenId
            );
            return;
        }

        uint256 randomNumber = uint256(
            keccak256(
                abi.encodePacked(
                    raffle.seed,
                    blockhash(raffle.storedBlockNumber)
                )
            )
        );
        uint256 index = randomNumber % players[raffleId].length;
        address winner = players[raffleId][index];

        uint256 devFee = (address(this).balance * fee) / 1000;
        uint256 amount = address(this).balance - devFee;
        console.log("Amount for winner: %s devFee: %s", amount, devFee);

        if (players[raffleId].length * ticketPrice >= raffle.reservePrice) {
            (bool success, ) = raffle.owner.call{value: amount}("");
            require(success, "Failed to send Ether");
            collection.safeTransferFrom(address(this), winner, raffle.tokenId);
        } else {
            (bool success, ) = winner.call{value: amount}("");
            require(success, "Failed to send Ether");
            collection.safeTransferFrom(
                address(this),
                raffle.owner,
                raffle.tokenId
            );
        }

        (bool sent, ) = owner.call{value: devFee}("");
        require(sent, "Failed to send Ether");

        delete players[raffleId];

        emit RaffleWinner(raffleId, winner);
    }

    /**
     * Creator can cancel rafle at any time if there is no tickets sold. Otherwise he should wait for deadline
     */
    function cancel(uint256 raffleId, bytes32 _seed)
        external
        onlyCreator(raffleId)
    {
        require(
            players[raffleId].length == 0,
            "Raffle is active till deadline"
        );
        Raffle storage raffle = raffles[raffleId];
        require(!raffle.canceled && !raffle.finished);

        raffle.canceled = true;
        raffle.seed = _seed;
        raffle.storedBlockNumber = block.number + 1;

        emit RaffleCanceled(raffleId);
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) override external pure returns (bytes4) {
        return
            bytes4(
                keccak256("onERC721Received(address,address,uint256,bytes)")
            );
    }
}

