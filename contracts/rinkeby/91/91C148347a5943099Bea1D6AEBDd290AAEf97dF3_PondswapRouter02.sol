pragma solidity >=0.6.6;

import './interfaces/IPondswapFactory.sol';
import './libraries/TransferHelper.sol';

import './interfaces/IPondswapRouter02.sol';
import './libraries/PondswapLibrary.sol';
import './libraries/SafeMath.sol';
import './interfaces/IERC20.sol';
import './interfaces/IWGLMR.sol';
import "hardhat/console.sol";

contract PondswapRouter02 is IPondswapRouter02 {
    using SafeMath for uint;

    address public immutable override factory;
    address public immutable override WGLMR;

    modifier ensure(uint deadline) {
        require(deadline >= block.timestamp, 'PondswapRouter: EXPIRED');
        _;
    }

    constructor(address _factory, address _WGLMR) public {
        factory = _factory;
        WGLMR = _WGLMR;
    }

    receive() external payable {
        assert(msg.sender == WGLMR); // only accept GLMR via fallback from the WGLMR contract
    }

    // **** ADD LIQUIDITY ****
    function _addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin
    ) internal virtual returns (uint amountA, uint amountB) {
        // create the pair if it doesn't exist yet
        address _addr = IPondswapFactory(factory).getPair(tokenA, tokenB);
        console.log("getPair: %s", _addr);
        if (IPondswapFactory(factory).getPair(tokenA, tokenB) == address(0)) {
            console.log("createPair");
            IPondswapFactory(factory).createPair(tokenA, tokenB);
        }
        (uint reserveA, uint reserveB) = PondswapLibrary.getReserves(factory, tokenA, tokenB);
        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountADesired, amountBDesired);
        } else {
            uint amountBOptimal = PondswapLibrary.quote(amountADesired, reserveA, reserveB);
            if (amountBOptimal <= amountBDesired) {
                require(amountBOptimal >= amountBMin, 'PondswapRouter: INSUFFICIENT_B_AMOUNT');
                (amountA, amountB) = (amountADesired, amountBOptimal);
            } else {
                uint amountAOptimal = PondswapLibrary.quote(amountBDesired, reserveB, reserveA);
                assert(amountAOptimal <= amountADesired);
                require(amountAOptimal >= amountAMin, 'PondswapRouter: INSUFFICIENT_A_AMOUNT');
                (amountA, amountB) = (amountAOptimal, amountBDesired);
            }
        }
    }
    function addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external virtual override ensure(deadline) returns (uint amountA, uint amountB, uint liquidity) {
        (amountA, amountB) = _addLiquidity(tokenA, tokenB, amountADesired, amountBDesired, amountAMin, amountBMin);
        address pair = PondswapLibrary.pairFor(factory, tokenA, tokenB);
        TransferHelper.safeTransferFrom(tokenA, msg.sender, pair, amountA);
        TransferHelper.safeTransferFrom(tokenB, msg.sender, pair, amountB);
        liquidity = IPondswapPair(pair).mint(to);
    }
    function addLiquidityGLMR(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountGLMRMin,
        address to,
        uint deadline
    ) external virtual override payable ensure(deadline) returns (uint amountToken, uint amountGLMR, uint liquidity) {
        (amountToken, amountGLMR) = _addLiquidity(
            token,
            WGLMR,
            amountTokenDesired,
            msg.value,
            amountTokenMin,
            amountGLMRMin
        );
        address pair = PondswapLibrary.pairFor(factory, token, WGLMR);
        TransferHelper.safeTransferFrom(token, msg.sender, pair, amountToken);
        IWGLMR(WGLMR).deposit{value: amountGLMR}();
        assert(IWGLMR(WGLMR).transfer(pair, amountGLMR));
        liquidity = IPondswapPair(pair).mint(to);
        // refund dust GLMR, if any
        if (msg.value > amountGLMR) TransferHelper.safeTransferGLMR(msg.sender, msg.value - amountGLMR);
    }

    // **** REMOVE LIQUIDITY ****
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountA, uint amountB) {
        address pair = PondswapLibrary.pairFor(factory, tokenA, tokenB);
        IPondswapPair(pair).transferFrom(msg.sender, pair, liquidity); // send liquidity to pair
        (uint amount0, uint amount1) = IPondswapPair(pair).burn(to);
        (address token0,) = PondswapLibrary.sortTokens(tokenA, tokenB);
        (amountA, amountB) = tokenA == token0 ? (amount0, amount1) : (amount1, amount0);
        require(amountA >= amountAMin, 'PondswapRouter: INSUFFICIENT_A_AMOUNT');
        require(amountB >= amountBMin, 'PondswapRouter: INSUFFICIENT_B_AMOUNT');
    }
    function removeLiquidityGLMR(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountGLMRMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountToken, uint amountGLMR) {
        (amountToken, amountGLMR) = removeLiquidity(
            token,
            WGLMR,
            liquidity,
            amountTokenMin,
            amountGLMRMin,
            address(this),
            deadline
        );
        TransferHelper.safeTransfer(token, to, amountToken);
        IWGLMR(WGLMR).withdraw(amountGLMR);
        TransferHelper.safeTransferGLMR(to, amountGLMR);
    }
    function removeLiquidityWithPermit(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountA, uint amountB) {
        address pair = PondswapLibrary.pairFor(factory, tokenA, tokenB);
        uint value = approveMax ? uint(-1) : liquidity;
        IPondswapPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        (amountA, amountB) = removeLiquidity(tokenA, tokenB, liquidity, amountAMin, amountBMin, to, deadline);
    }
    function removeLiquidityGLMRWithPermit(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountGLMRMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountToken, uint amountGLMR) {
        address pair = PondswapLibrary.pairFor(factory, token, WGLMR);
        uint value = approveMax ? uint(-1) : liquidity;
        IPondswapPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        (amountToken, amountGLMR) = removeLiquidityGLMR(token, liquidity, amountTokenMin, amountGLMRMin, to, deadline);
    }

    // **** REMOVE LIQUIDITY (supporting fee-on-transfer tokens) ****
    function removeLiquidityGLMRSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountGLMRMin,
        address to,
        uint deadline
    ) public virtual override ensure(deadline) returns (uint amountGLMR) {
        (, amountGLMR) = removeLiquidity(
            token,
            WGLMR,
            liquidity,
            amountTokenMin,
            amountGLMRMin,
            address(this),
            deadline
        );
        TransferHelper.safeTransfer(token, to, IERC20(token).balanceOf(address(this)));
        IWGLMR(WGLMR).withdraw(amountGLMR);
        TransferHelper.safeTransferGLMR(to, amountGLMR);
    }
    function removeLiquidityGLMRWithPermitSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountGLMRMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external virtual override returns (uint amountGLMR) {
        address pair = PondswapLibrary.pairFor(factory, token, WGLMR);
        uint value = approveMax ? uint(-1) : liquidity;
        IPondswapPair(pair).permit(msg.sender, address(this), value, deadline, v, r, s);
        amountGLMR = removeLiquidityGLMRSupportingFeeOnTransferTokens(
            token, liquidity, amountTokenMin, amountGLMRMin, to, deadline
        );
    }

    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(uint[] memory amounts, address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = PondswapLibrary.sortTokens(input, output);
            uint amountOut = amounts[i + 1];
            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOut) : (amountOut, uint(0));
            address to = i < path.length - 2 ? PondswapLibrary.pairFor(factory, output, path[i + 2]) : _to;
            IPondswapPair(PondswapLibrary.pairFor(factory, input, output)).swap(
                amount0Out, amount1Out, to, new bytes(0)
            );
        }
    }
    function swapExactTokensForTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external virtual override ensure(deadline) returns (uint[] memory amounts) {
        amounts = PondswapLibrary.getAmountsOut(factory, amountIn, path);
        require(amounts[amounts.length - 1] >= amountOutMin, 'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT');
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]
        );
        _swap(amounts, path, to);
    }
    function swapTokensForExactTokens(
        uint amountOut,
        uint amountInMax,
        address[] calldata path,
        address to,
        uint deadline
    ) external virtual override ensure(deadline) returns (uint[] memory amounts) {
        amounts = PondswapLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= amountInMax, 'PondswapRouter: EXCESSIVE_INPUT_AMOUNT');
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]
        );
        _swap(amounts, path, to);
    }
    function swapExactGLMRForTokens(uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        payable
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(path[0] == WGLMR, 'PondswapRouter: INVALID_PATH');
        amounts = PondswapLibrary.getAmountsOut(factory, msg.value, path);
        require(amounts[amounts.length - 1] >= amountOutMin, 'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT');
        IWGLMR(WGLMR).deposit{value: amounts[0]}();
        assert(IWGLMR(WGLMR).transfer(PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]));
        _swap(amounts, path, to);
    }
    function swapTokensForExactGLMR(uint amountOut, uint amountInMax, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(path[path.length - 1] == WGLMR, 'PondswapRouter: INVALID_PATH');
        amounts = PondswapLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= amountInMax, 'PondswapRouter: EXCESSIVE_INPUT_AMOUNT');
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]
        );
        _swap(amounts, path, address(this));
        IWGLMR(WGLMR).withdraw(amounts[amounts.length - 1]);
        TransferHelper.safeTransferGLMR(to, amounts[amounts.length - 1]);
    }
    function swapExactTokensForGLMR(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(path[path.length - 1] == WGLMR, 'PondswapRouter: INVALID_PATH');
        amounts = PondswapLibrary.getAmountsOut(factory, amountIn, path);
        require(amounts[amounts.length - 1] >= amountOutMin, 'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT');
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]
        );
        _swap(amounts, path, address(this));
        IWGLMR(WGLMR).withdraw(amounts[amounts.length - 1]);
        TransferHelper.safeTransferGLMR(to, amounts[amounts.length - 1]);
    }
    function swapGLMRForExactTokens(uint amountOut, address[] calldata path, address to, uint deadline)
        external
        virtual
        override
        payable
        ensure(deadline)
        returns (uint[] memory amounts)
    {
        require(path[0] == WGLMR, 'PondswapRouter: INVALID_PATH');
        amounts = PondswapLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= msg.value, 'PondswapRouter: EXCESSIVE_INPUT_AMOUNT');
        IWGLMR(WGLMR).deposit{value: amounts[0]}();
        assert(IWGLMR(WGLMR).transfer(PondswapLibrary.pairFor(factory, path[0], path[1]), amounts[0]));
        _swap(amounts, path, to);
        // refund dust GLMR, if any
        if (msg.value > amounts[0]) TransferHelper.safeTransferGLMR(msg.sender, msg.value - amounts[0]);
    }

    // **** SWAP (supporting fee-on-transfer tokens) ****
    // requires the initial amount to have already been sent to the first pair
    function _swapSupportingFeeOnTransferTokens(address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = PondswapLibrary.sortTokens(input, output);
            IPondswapPair pair = IPondswapPair(PondswapLibrary.pairFor(factory, input, output));
            uint amountInput;
            uint amountOutput;
            { // scope to avoid stack too deep errors
            (uint reserve0, uint reserve1,) = pair.getReserves();
            (uint reserveInput, uint reserveOutput) = input == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
            amountInput = IERC20(input).balanceOf(address(pair)).sub(reserveInput);
            amountOutput = PondswapLibrary.getAmountOut(amountInput, reserveInput, reserveOutput);
            }
            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOutput) : (amountOutput, uint(0));
            address to = i < path.length - 2 ? PondswapLibrary.pairFor(factory, output, path[i + 2]) : _to;
            pair.swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }
    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external virtual override ensure(deadline) {
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amountIn
        );
        uint balanceBefore = IERC20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IERC20(path[path.length - 1]).balanceOf(to).sub(balanceBefore) >= amountOutMin,
            'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT'
        );
    }
    function swapExactGLMRForTokensSupportingFeeOnTransferTokens(
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    )
        external
        virtual
        override
        payable
        ensure(deadline)
    {
        require(path[0] == WGLMR, 'PondswapRouter: INVALID_PATH');
        uint amountIn = msg.value;
        IWGLMR(WGLMR).deposit{value: amountIn}();
        assert(IWGLMR(WGLMR).transfer(PondswapLibrary.pairFor(factory, path[0], path[1]), amountIn));
        uint balanceBefore = IERC20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IERC20(path[path.length - 1]).balanceOf(to).sub(balanceBefore) >= amountOutMin,
            'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT'
        );
    }
    function swapExactTokensForGLMRSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    )
        external
        virtual
        override
        ensure(deadline)
    {
        require(path[path.length - 1] == WGLMR, 'PondswapRouter: INVALID_PATH');
        TransferHelper.safeTransferFrom(
            path[0], msg.sender, PondswapLibrary.pairFor(factory, path[0], path[1]), amountIn
        );
        _swapSupportingFeeOnTransferTokens(path, address(this));
        uint amountOut = IERC20(WGLMR).balanceOf(address(this));
        require(amountOut >= amountOutMin, 'PondswapRouter: INSUFFICIENT_OUTPUT_AMOUNT');
        IWGLMR(WGLMR).withdraw(amountOut);
        TransferHelper.safeTransferGLMR(to, amountOut);
    }

    // **** LIBRARY FUNCTIONS ****
    function quote(uint amountA, uint reserveA, uint reserveB) public pure virtual override returns (uint amountB) {
        return PondswapLibrary.quote(amountA, reserveA, reserveB);
    }

    function getAmountOut(uint amountIn, uint reserveIn, uint reserveOut)
        public
        pure
        virtual
        override
        returns (uint amountOut)
    {
        return PondswapLibrary.getAmountOut(amountIn, reserveIn, reserveOut);
    }

    function getAmountIn(uint amountOut, uint reserveIn, uint reserveOut)
        public
        pure
        virtual
        override
        returns (uint amountIn)
    {
        return PondswapLibrary.getAmountIn(amountOut, reserveIn, reserveOut);
    }

    function getAmountsOut(uint amountIn, address[] memory path)
        public
        view
        virtual
        override
        returns (uint[] memory amounts)
    {
        return PondswapLibrary.getAmountsOut(factory, amountIn, path);
    }

    function getAmountsIn(uint amountOut, address[] memory path)
        public
        view
        virtual
        override
        returns (uint[] memory amounts)
    {
        return PondswapLibrary.getAmountsIn(factory, amountOut, path);
    }
}

