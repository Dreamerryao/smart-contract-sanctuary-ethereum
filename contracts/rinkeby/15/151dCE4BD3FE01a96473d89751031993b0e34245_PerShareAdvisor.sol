contract PerShareAdvisor is Ownable {

    using BoringMath for uint256;

    uint256 public claimed;
    // 디플레이션간 뉴런 블록당 보상
    uint[] public defNeuronPerShare;
    // 디플레이션시작블록
    uint[] public deflation;
    // 디플레이션구간계산블록수(런칭일~디플레이션1 // 디플레이션1~2 블록)
    uint[] public claimableAmountCal;
    uint256 public totalToken;
    address public neuronToken;
    address public advisorAddress;
    uint256 public launchingBlock;
    event Claim(uint256 cliamed);



    modifier claimableAddress() {
    // router지정된 주소만가능
        require(msg.sender == advisorAddress ||  msg.sender == owner );
        _;
    }

    constructor(address _neuronToken, uint256 _accNeuronPerShare, address _advisorAddress, uint256 _launchingBlock) public {
        neuronToken = _neuronToken;
        defNeuronPerShare.push(_accNeuronPerShare);
        // 뉴런 디플레이션구간 블록당 보상
        // for(uint i =1; i< 6; i++) {
        //     defNeuronPerShare[i] = defNeuronPerShare[i-1].sub(defNeuronPerShare[i-1].mul(10).div(100));
        // }
        advisorAddress = _advisorAddress;
        launchingBlock = _launchingBlock;

        uint ctr = 6;
        for (uint i = 0; i < ctr; i++) {
            if (i == 0) {
                deflation.push(_launchingBlock.add(20));
            } else {
                deflation.push(deflation[i - 1].add(20));
            }
        }

        for(uint j = 1; j< ctr; j++) {
            defNeuronPerShare.push(defNeuronPerShare[j - 1].sub(defNeuronPerShare[j - 1].mul(10).div(100)));
        }

        claimableAmountCal.push(defNeuronPerShare[0].mul(20));
        // claimableAmountCal[0] = deAmountBlock[0].mul(defNeuronPerShare[0]);
        for(uint k=1; k < 5 ; k++) {
            claimableAmountCal.push(claimableAmountCal[k-1].add(defNeuronPerShare[k].mul(20)));
        }
    }


    function tokensClaimable() external view returns (uint256 claimableAmount) {
        claimableAmount = _tokensClaimable();
    }

    function _tokensClaimable() private view returns (uint256 claimableAmount) {
           if(_getunClaimToken() > 0 ) {   
            uint256 blocks = block.number.sub(deflation[0]);


            if(block.number >= deflation[0] && block.number < deflation[1]) {
                claimableAmount = blocks.mul(defNeuronPerShare[0]).sub(claimed);
            }else if(block.number >= deflation[1] && block.number < deflation[2]) {
                uint256 de1amountBlock = block.number.sub(deflation[1]);
                claimableAmount = claimableAmountCal[0].add(de1amountBlock.mul(defNeuronPerShare[1])).sub(claimed);
            }else if(block.number >= deflation[2] && block.number < deflation[3]) {
                uint256 de2amountBlock = block.number.sub(deflation[2]);
                claimableAmount = claimableAmountCal[1].add(de2amountBlock.mul(defNeuronPerShare[2])).sub(claimed);
            }else if(block.number >= deflation[3] && block.number < deflation[4]) {
                uint256 de3amountBlock = block.number.sub(deflation[3]);
                claimableAmount = claimableAmountCal[2].add(de3amountBlock.mul(defNeuronPerShare[3])).sub(claimed);
            }else if(block.number >= deflation[4] && block.number < deflation[5]) {
                uint256 de4amountBlock = block.number.sub(deflation[4]);
                claimableAmount = claimableAmountCal[3].add(de4amountBlock.mul(defNeuronPerShare[4])).sub(claimed);
            } else if(block.number > deflation[5] && block.number < deflation[5].add(10512000)) {
                uint256 de5amountBlock = block.number.sub(deflation[5]);
                claimableAmount = claimableAmountCal[4].add(de5amountBlock.mul(defNeuronPerShare[5])).sub(claimed);
            } else {
                claimableAmount = _getunClaimToken();
            }
            
        } else claimableAmount = 0;
    }



    function getunClaimToken() external view returns (uint256 unClaimToken) {
        unClaimToken = _getunClaimToken();
    }

    function _getunClaimToken() private view returns (uint256 unClaimToken) {
        unClaimToken =  IERC20(neuronToken).balanceOf(address(this));
    }


    function setNeuronPerShare(uint256 _neuronPerShare) claimableAddress() external {
        defNeuronPerShare[0] = _neuronPerShare;
        // 뉴런 디플레이션구간 블록당 보상
        for(uint i =1; i< 6; i++) {
            defNeuronPerShare[i] = defNeuronPerShare[i-1].sub(defNeuronPerShare[i-1].mul(10).div(100));
        }
    }


    function claim() claimableAddress() external {
        require(block.number >= deflation[0]);
        uint256 claimAmount = _tokensClaimable();

        TransferHelper.safeTransfer(neuronToken , advisorAddress , claimAmount);
        claimed = claimed.add(claimAmount);

        emit Claim(claimAmount);
    } 




}

