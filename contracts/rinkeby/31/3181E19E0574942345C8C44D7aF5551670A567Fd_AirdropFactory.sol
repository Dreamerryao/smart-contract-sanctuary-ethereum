contract AirdropContract is ERC721Holder, ERC1155Holder, Ownable {

    using SafeERC20 for IERC20;

    struct InitialParameters {
        uint256 launchpassId;
    }

    struct ERC20Airdrop {
        address addr;
        uint256 quantity;
    }

    struct ERC721Airdrop {
        address addr;
        uint256[] tokenIds;
    }

    struct ERC1155Airdrop {
        address addr;
        uint256 id;
        uint256 quantity;
    }

    uint256 public launchpassId;

    constructor(
        InitialParameters memory initialParameters
    ) {
        launchpassId = initialParameters.launchpassId;
    }

    function airdropERC20(IERC20 _token, ERC20Airdrop[] memory _airdrops) public onlyOwner {
        for (uint256 i = 0; i < _airdrops.length; i++) {
            SafeERC20.safeTransfer(_token, _airdrops[i].addr, _airdrops[i].quantity);
        }
    }

