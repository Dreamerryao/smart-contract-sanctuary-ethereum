        ERC721NFT nft = ERC721NFT(_contract);
        for (uint256 i = 0; i < _airdrops.length; i++) {
            for (uint256 j = 0; j < _airdrops[i].tokenIds.length; j++) {
                nft.safeTransferFrom(address(this), _airdrops[i].addr, _airdrops[i].tokenIds[j]);
            }
        }
    }

    function airdropERC1155(address _contract, ERC1155Airdrop[] memory _airdrops) public onlyOwner {
        ERC1155NFT nft = ERC1155NFT(_contract);
        for (uint256 i = 0; i < _airdrops.length; i++) {
            nft.safeTransferFrom(address(this), _airdrops[i].addr, _airdrops[i].id, _airdrops[i].quantity, "");
        }
    }
}

