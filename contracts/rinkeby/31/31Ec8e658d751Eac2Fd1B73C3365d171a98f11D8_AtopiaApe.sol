pragma solidity ^0.8.0;

import "./utils/Base.sol";
import "./utils/WithOG.sol";
import "./libs/BatchCounters.sol";
import "./libs/Base64.sol";
import "./interfaces/IDrawer.sol";

contract AtopiaApe is AtopiaBase, WithOG {
	using Base64 for *;
	using BatchCounters for BatchCounters.Counter;
	BatchCounters.Counter private _tokenIds;

	uint256 public constant maxSale = 10_000;
	uint256 public constant saleFee = 0.069 ether;
	uint256 public constant maxSupply = 50_000;
	uint256 public constant nameFee = 1000_000_000;

	mapping(uint256 => string) names;

	mapping(uint256 => uint256) public tokenTraits;
	mapping(uint256 => uint256) specials;
	uint256[] blockHashes;
	uint256 seed;

	IDrawer public drawer;

	bool public paused = true;

	constructor(address bucks) AtopiaBase(bucks) ERC721("Atopia Apes", "ATPAPE") {
		seed = uint256(keccak256(abi.encodePacked(block.difficulty, block.coinbase, block.timestamp)));
	}

	modifier onlyPaused(bool _paused) {
		require(paused == _paused, "Not Allowed");
		_;
	}

	function totalSupply() public view returns (uint256) {
		return _tokenIds.current();
	}

	function nextGenInfo(uint256 last) public pure returns (uint256, uint256) {
		if (last < 20_000) {
			return (10_100_000_000, (3 * 365 days) / 10); // 10k ABUCKS
		} else if (last < 30_000) {
			return (15_100_000_000, (2 * 365 days) / 10); // 15k ABUCKS
		} else if (last < 40_000) {
			return (20_100_000_000, (1 * 365 days) / 10); // 20k ABUCKS
		} else {
			return (25_100_000_000, (1 * 365 days) / 10); // 25k ABUCKS
		}
	}

	function enter(
		address to,
		uint256 tokenId,
		uint256 _seed
	) internal {
		uint256 tokenTrait;
		for (uint8 i = 0; i < drawer.traitCount() - 1; i++) {
			tokenTrait = (tokenTrait << 16) | ((_seed & 0xFFFF) % drawer.itemCount(i));
			_seed = _seed >> 16;
		}

		// Furry Body & Face Colors
		if (((tokenTrait >> 128) & 0xFFFF) == ((tokenTrait >> 112) & 0xFFFF)) {
			tokenTrait = (tokenTrait << 16) | 1;
		} else {
			tokenTrait = tokenTrait << 16;
		}

		tokenTraits[tokenId] = tokenTrait;
		claims[tokenId] = block.timestamp;
		_mint(to, tokenId);
	}

	function batch(
		address to,
		uint256 amount,
		uint256 age
	) internal returns (uint256 newSeed) {
		newSeed = seed;
		(uint256 start, uint256 end) = _tokenIds.increment(amount);
		for (uint256 i = start; i <= end; i++) {
			birth[i] = block.timestamp - age;
			claims[i] = block.timestamp;

			newSeed = uint256(keccak256(abi.encodePacked(i, birth[i], newSeed)));
			enter(to, i, newSeed);

			if (i % 1000 == 0) {
				blockHashes.push(newSeed);
			}
		}
	}

	function mint(uint256 amount) external payable onlyPaused(false) {
		require(totalSupply() + amount <= maxSale);
		require(msg.value >= saleFee * amount);
		seed = batch(msg.sender, amount, (5 * 365 days) / 10);
	}

	function mintOG(uint256 amount) external onlyPaused(false) withMembership(msg.sender, amount) {
		require(totalSupply() + amount <= maxSale);
		seed = batch(msg.sender, amount, (5 * 365 days) / 10);
	}

	function mintNextGen(uint256 amount) external onlyPaused(false) {
		uint256 last = totalSupply();
		require(last >= maxSale);
		require(last + amount <= maxSupply);
		(uint256 price, uint256 age) = nextGenInfo(last);
		bucks.burnFrom(msg.sender, price * amount);
		seed = batch(msg.sender, amount, age);
	}

	function setName(uint256 tokenId, string memory name) external {
		require(ownerOf(tokenId) == msg.sender);
		bucks.burnFrom(msg.sender, nameFee);
		names[tokenId] = name;
	}

	function useItem(
		uint256 tokenId,
		uint256 itemId,
		uint256 amount
	) external onlyTokenOwner(tokenId) {
		uint256 itemInfo = IShop(shop).itemInfo(itemId - 1);
		// Min Age
		require(getAge(tokenId) >= itemInfo & 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF);
		IShop(shop).burn(msg.sender, itemId, amount);

		itemInfo = itemInfo >> 128;
		// Bonus Trait
		if (itemInfo & 0xFFFFFFFF > 0) {
			uint16 traitId = uint16(itemInfo & 0xFFFF);
			uint16 traitPos = uint16(((itemInfo >> 16) & 0xFFFF) * 16);
			tokenTraits[tokenId] = (tokenTraits[tokenId] & (0xFFFF << traitPos)) | (traitId << traitPos);
		}

		itemInfo = itemInfo >> 64;
		// Bonus Age
		useItemInternal(tokenId, itemInfo * amount);
	}

	function setPaused(bool _paused) external onlyOwner {
		paused = _paused;
	}

	function setDrawer(address _drawer) external onlyOwner {
		drawer = IDrawer(_drawer);
	}

	function addMembership(address[] calldata members) public override onlyOwner {
		WithOG.addMembership(members);
	}

	function withdraw() external onlyOwner {
		payable(owner()).transfer(address(this).balance);
	}

	function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
		require(_exists(tokenId), "Token Invalid");
		return drawer.tokenURI(tokenId, names[tokenId], tokenTraits[tokenId]);
	}
}

