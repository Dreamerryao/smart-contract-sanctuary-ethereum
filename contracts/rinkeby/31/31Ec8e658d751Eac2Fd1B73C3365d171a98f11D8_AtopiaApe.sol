// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "../interfaces/IBucks.sol";
import "../interfaces/IShop.sol";
import "../interfaces/ICenter.sol";

abstract contract AtopiaBase is ERC721, Ownable {
	IBucks public bucks;
	address public shop;
	address public space;

	mapping(uint256 => uint256) public birth;
	mapping(uint256 => uint256) public grow;
	mapping(uint256 => uint256) public claims;
	mapping(uint256 => uint256) public pendings;

	mapping(uint256 => uint256) lives;

	constructor(address _bucks) {
		bucks = IBucks(_bucks);
	}

	function getAge(uint256 tokenId) public view returns (uint256) {
		return (block.timestamp - birth[tokenId]) + grow[tokenId];
	}

	function getRewardsInternal(uint256 tokenId) internal view returns (uint256) {
		uint256 duration = block.timestamp - claims[tokenId];
		uint256 averageSpeed = block.timestamp + claims[tokenId] + grow[tokenId] * 2 - birth[tokenId] * 2;
		return (averageSpeed * duration * 500_000_000) / 365 days / 1 days;
	}

	function getRewards(uint256 tokenId) external view returns (uint256) {
		return getRewardsInternal(tokenId) + pendings[tokenId];
	}

	modifier onlyTokenOwner(uint256 tokenId) {
		require(ownerOf(tokenId) == msg.sender);
		_;
	}

	function claimRewards(uint256 tokenId) public onlyTokenOwner(tokenId) {
		uint256 rewards = getRewardsInternal(tokenId);
		if (pendings[tokenId] > 0) {
			rewards += pendings[tokenId];
			pendings[tokenId] = 0;
		}
		claims[tokenId] = block.timestamp;
		bucks.mint(msg.sender, rewards);
	}

	modifier onlyShop() {
		require(shop == msg.sender);
		_;
	}

	function setShop(address _shop) external onlyOwner {
		shop = _shop;
	}

	function useItemInternal(uint256 tokenId, uint256 bonusAge) internal {
		pendings[tokenId] += getRewardsInternal(tokenId);
		claims[tokenId] = block.timestamp;
		grow[tokenId] += bonusAge;
	}

	modifier onlySpace() {
		require(space == msg.sender);
		_;
	}

	function setSpace(address _space) external onlyOwner {
		space = _space;
	}

	function getLife(uint256 tokenId) public view returns (uint256 job, uint256 data) {
		uint256 life = lives[tokenId];
		job = life >> 192;
		data = life & (1 << (129 - 1));
	}

	modifier notBusy(uint256 tokenId) {
		(uint256 job, ) = getLife(tokenId);
		require(job == 0);
		_;
	}

	function setLife(uint256 tokenId, uint256 life) external onlySpace notBusy(tokenId) {
		lives[tokenId] = life;
		pendings[tokenId] += getRewardsInternal(tokenId);
		claims[tokenId] = block.timestamp;
	}

	function exitCenter(
		uint256 tokenId,
		address center,
		uint256 grown
	) external onlySpace {
		uint256 rewards = getRewardsInternal(tokenId);
		uint256 fee = (rewards * ICenter(center).enjoyFee()) / 10000;
		claims[tokenId] = block.timestamp;
		bucks.mint(center, fee);
		pendings[tokenId] += rewards - fee;
		grow[tokenId] += grown;
		lives[tokenId] = 0;
	}
}

