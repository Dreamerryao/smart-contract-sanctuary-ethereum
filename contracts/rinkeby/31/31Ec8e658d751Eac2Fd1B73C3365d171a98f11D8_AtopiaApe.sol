pragma solidity ^0.8.0;

abstract contract WithOG {
	uint256 public constant MAX_OG_MINT = 2;
	mapping(address => uint256) memberships;

	modifier withMembership(address member, uint256 amount) {
		require(memberships[member] >= amount);
		memberships[member] -= amount;
		_;
	}

	function addMembership(address[] calldata members) public virtual {
		for (uint256 i = 0; i < members.length; i++) {
			memberships[members[i]] = MAX_OG_MINT;
		}
	}
}