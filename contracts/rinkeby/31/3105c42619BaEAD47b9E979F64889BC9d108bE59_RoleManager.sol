contract Util {
    uint256 public constant MAX_UINT = (~uint256(0)-1);

    string public constant	APP_NAME='BOOM_CITY';

    bytes32 public constant	ROLE_MINTER = keccak256(abi.encodePacked('ROLE_MINTER'));
    bytes32 public constant	ROLE_OPERATOR = keccak256(abi.encodePacked('ROLE_OPERATOR'));
    bytes32 public constant	ROLE_VRF_MANAGER = keccak256(abi.encodePacked('ROLE_VRF_MANAGER'));
    bytes32 public constant	ROLE_META_MANAGER = keccak256(abi.encodePacked('ROLE_META_MANAGER'));
    bytes32 public constant	ROLE_FACTORY = keccak256(abi.encodePacked('ROLE_FACTORY'));

//    string public constant X_CONTRACT_NAME = 'contract_name';

//    bytes32 public constant X_CONTRACT_ID = keccak256(abi.encodePacked(X_CONTRACT_NAME));

    enum StatType{ None, Integer, String, ByteArray, Boolean, Address }

    enum NFTRarity{ Epic, Rare, Common }

    struct Stat{
	string name;
	StatType statType;
	bool is_mutable;
    }

    struct StatValue{
	StatType statType;
	uint256  int_val;
	string   str_val;
	bytes32  bta_val;
	bool     bool_val;
	address	 addr_val;
    }

/*    function getTagKey(uint256 nft_id, string calldata name) public pure returns(bytes32) {
	return keccak256(abi.encodePacked(nft_id, name));
    }*/

    function getTokenURIKey(uint256 nft_id) public pure returns(bytes32) {
	return keccak256(abi.encodePacked(APP_NAME, nft_id));
    }

    function getTokenURIFrozenKey(uint256 nft_id) public pure returns(bytes32) {
	return keccak256(abi.encodePacked(APP_NAME, '_FROZEN_URI', nft_id));
    }

    function getBaseURIKey() public pure returns(bytes32) {
	return keccak256(abi.encodePacked(APP_NAME, 'BASE_URI'));
    }

    function getDefaultTokenURIKey() public pure returns(bytes32) {
	return keccak256(abi.encodePacked(APP_NAME, 'DEFAULT_TOKEN_URI'));
    }

    function getDefaultTokenURIasIDKey() public pure returns(bytes32) {
	return keccak256(abi.encodePacked(APP_NAME, 'DEFAULT_TOKEN_URI_AS_ID'));
    }


    function getRandomItem(uint256 rvalue, uint256[] memory distribution, uint256 size) public pure returns(uint256) {
	uint256 ratio = MAX_UINT/size;
	uint256 svalue = 0;
	for(uint256 i=0;i<distribution.length;i++){
	    svalue+=ratio*distribution[i];
	    if(rvalue < svalue)
		return i;
	}
	return distribution.length;
    }

/*      function deriveCommitment(bytes32 entropy) public pure returns (bytes32){
        return keccak256(abi.encodePacked(entropy));
    }*/

}

