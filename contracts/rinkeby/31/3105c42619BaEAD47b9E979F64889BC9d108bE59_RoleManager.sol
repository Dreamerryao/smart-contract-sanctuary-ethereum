//contract RoleManager is ContextMixin, AccessControlEnumerable, Ownable {
contract RoleManager is BOOMBase {
    using Strings for uint256;

    mapping(bytes32 => bytes32[]) public roles;
    mapping(bytes32 => mapping(bytes32 => uint256)) public roleIds;

    constructor(){
	_grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function getId() override view public returns(bytes32) {
	return ROLE_MANAGER_CONTRACT_ID;
    }

    function getName() override view public returns(string memory) {
	return ROLE_MANAGER_CONTRACT_NAME;
    }

    function getVersion() override virtual view external returns(bytes32) {
	return keccak256(abi.encodePacked('v1.0')); // First release
    }

    function setRoleAdmin(bytes32 role, bytes32 adminRole) public virtual onlyRole(DEFAULT_ADMIN_ROLE) {
	_setRoleAdmin(role, adminRole);
    }

    function grantRole(bytes32 role, address account) public override virtual {
	super.grantRole(role,account);

	if(manager == address(0))return;
	bytes32 hname = CrossContractMan(manager).getContractId(account);
	if(hname == bytes32(0))return;

	roleIds[hname][role] = roles[hname].length;
	roles[hname].push(role);

//	console.log("ADDED ROLE %s for %s",uint256(role).toHexString(),uint256(uint160(account)).toHexString());
    }

    function revokeRole(bytes32 role, address account) public override virtual {
	super.revokeRole(role,account);

	if(manager == address(0))return;
	bytes32 hname = CrossContractMan(manager).getContractId(account);
	if(hname == bytes32(0))return;

	delete roles[hname][roleIds[hname][role]];
	delete roleIds[hname][role];
//	console.log("REVOKED ROLE %s for %s",uint256(role).toHexString(),uint256(uint160(account)).toHexString());
    }

    function transferRoles(bytes32 hname, address old_instance, address new_instance) public virtual onlyManager {
//	console.log("MOVING ROLES FOR %s from %s to %s",uint256(hname).toHexString(),uint256(uint160(old_instance)).toHexString(),
//		uint256(uint160(new_instance)).toHexString());

	for(uint i=0;i<roles[hname].length;i++){
	    _revokeRole(roles[hname][i],old_instance);
	    _grantRole(roles[hname][i],new_instance);
//	    console.log("MOVED ROLE %s from %s to %s",roles[hname][i],old_instance,new_instance);
//	    console.log("MOVED ROLE %s from %s to %s",uint256(roles[hname][i]).toHexString(),uint256(uint160(old_instance)).toHexString(),
//		uint256(uint160(new_instance)).toHexString());
	}
    }


