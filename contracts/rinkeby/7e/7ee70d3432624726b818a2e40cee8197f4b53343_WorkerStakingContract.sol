library StakingUtils {
    /// @notice The struct used to represent a staked piece or puzzle
    /// @dev Note that there is no evidence if its a piece or puzzle
    /// @dev This differentiation is made via the generateKey function and the variables that store the actual stake
    /// @dev `amount` is used to store the number of workers associated with a certain piece or puzzle
    struct StakedPiecePuzzle {
        uint256 tokenId;
        address owner;
        uint256 amount;
        uint256[] workers;
        uint256 taxRate;
    }

    /// @notice The struct used to represent a staked worker
    /// @dev `home` and `isPuzzle` are used to identify the CASTLE token that the worker is asociated with.
    /// @dev `home` represents the tokenId, while `isPuzzle` shows if its a piece or puzzle token
    /// @dev `landlord` and `taxesCollectedByLandlord` are used for the renting functionality.
    struct Worker {
        uint256 tokenId;
        uint256 timestamp;
        address owner;
        uint256 stolen;
        uint256 taxesCollectedByLandlord;
        uint256 home;
        address landlord;
        bool isPuzzle;
        uint256 raided;
    }

    /**
     * @notice Generates a hash that identifies the stake of an address
     *
     * @dev The hash is used in a mapping to access the staked tokens one by one
     * @dev 'isPuzzle` identifies if the token is a piece or puzzle
     * @dev This way only a single variable is needed to keep track of both token types
     *
     * @param _address The address of the staker
     * @param tokenId TokenId of the staked token
     * @param isPuzzle Information wheter the token is a piece or puzzle
     *
     * @return key The hash used to locate the stake
     */
    function generateKey(
        address _address,
        uint256 tokenId,
        bool isPuzzle
    ) internal pure returns (uint256 key) {
        key = uint256(keccak256(abi.encode(_address, tokenId, isPuzzle)));
    }

    /**
     * @notice Casts a boolean to its numerical representation
     * @notice true evaluates to 1, while false evaluates to 0
     *
     * @dev Is used to access the indexed tokens of an address
     * @dev A mapping is used in the form address => uint => uint[], where the first uint can only be 0 or 1
     * @dev This way only a single variable is needed to keep track of both token types
     *
     * @param x The boolean to be casted
     * @return r The numerical representation of the boolean
     */
    function toUint(bool x) internal pure returns (uint256 r) {
        assembly {
            r := x
        }
    }

    /**
     * @notice Maps a tokenId + isPuzzle to the respective earning rate it has
     *
     * @param tokenId The tokenId of the staked token
     * @param isPuzzle Information wheter the token is a piece or puzzle
     *
     * @return baseRate The earning rate of the staked token
     */
    function getBaseRate(uint256 tokenId, bool isPuzzle)
        internal
        pure
        returns (uint256 baseRate)
    {
        isPuzzle
            ? require(0 < tokenId && tokenId <= 4, "INVALID")
            : require(0 < tokenId && tokenId <= 100, "INVALID");

        if (!isPuzzle) {
            if (tokenId <= 25) {
                baseRate = 0.1041666666666666 ether; // GOLD PIECE   150 CROWNS per day (24h)
            } else if (tokenId <= 50) {
                baseRate = 0.0520833333333333 ether; // SILVER PIECE  75 CROWNS per day (24h)
            } else if (tokenId <= 75) {
                baseRate = 0.0416666666666666 ether; // BRONZE PIECE  60 CROWNS per day (24h)
            } else if (tokenId <= 100) {
                baseRate = 0.0312500000000000 ether; // IRON PIECE    45 CROWNS per day (24h)
            }
        } else {
