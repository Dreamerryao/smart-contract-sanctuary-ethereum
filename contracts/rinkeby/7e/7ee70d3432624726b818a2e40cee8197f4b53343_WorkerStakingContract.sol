pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "./base/ChainLinkBase.sol";

import "./interfaces/CastleStakingContractInterface.sol";
import "./interfaces/RaidersContractInterface.sol";
import "./interfaces/CastleStakingContractInterface.sol";
import "./interfaces/CrownsContractInterface.sol";

import "./libraries/ArrayUtils.sol";
import "./libraries/StakingUtils.sol";

import "hardhat/console.sol";

/**
 * @title WorkerStakingContract
 */
contract WorkerStakingContract is OwnableUpgradeable, ERC721HolderUpgradeable {
    using ArrayUtils for uint256[];

    modifier onlyCastleStakingContract() {
        require(
            _msgSender() == address(castleStakingContr),
            "INVALIDOPERATION"
        );
        _;
    }

    modifier onlyRaidersContract() {
        require(_msgSender() == address(raidersContr), "INVALIDOPERATION");
        _;
    }

    IERC721EnumerableUpgradeable internal citizenContr;
    CastleStakingContractInterface internal castleStakingContr;
    RaidersContractInterface internal raidersContr;
    CrownsContractInterface internal crownsContr;

    mapping(uint256 => StakingUtils.Worker) public stakedWorkers;
    mapping(address => uint256[]) public indexedWorkersByOwner;

    uint256 public claimableRaidersTaxes;
    uint8 public constant RAIDER_TAX_RATE = 15;

    function initialize(
        CrownsContractInterface _crowns,
        IERC721EnumerableUpgradeable _citizenContr,
        CastleStakingContractInterface _castleStakingContr,
        RaidersContractInterface _raidersContr
    ) public initializer {
        __Ownable_init();
        __ERC721Holder_init();

        crownsContr = _crowns;
        citizenContr = _citizenContr;
        castleStakingContr = _castleStakingContr;
        raidersContr = _raidersContr;
    }

    /** @notice Assigns given tokenIds to an address stake and transfers token ownership to the contract
     *
     * @param _workerTokenIds Array of token Ids to be staked
     */
    function stakeManyWorkers(uint256[] calldata _workerTokenIds) external {
        for (uint8 i; i < _workerTokenIds.length; i++) {
            require(!raidersContr.isRaider(_workerTokenIds[i]), "NOTAWORKER");

            indexedWorkersByOwner[_msgSender()].push(_workerTokenIds[i]);

            StakingUtils.Worker storage existingStake = stakedWorkers[
                _workerTokenIds[i]
            ];
            existingStake.timestamp = block.timestamp;
            existingStake.owner = _msgSender();
            existingStake.tokenId = _workerTokenIds[i];

            citizenContr.safeTransferFrom(
                _msgSender(),
                address(this),
                _workerTokenIds[i]
            );
        }
    }

    /** @notice Withdraws rewards for given `workerTokenIds` and optionally removes them
     * from their respective piecePuzzles and/or unstakes the tokens from the contract
     *
     * @param _tokenIds Ids of staked tokens to be managed
     * @param _removeFromPiecePuzzle Whether or not to remove the tokens from their respective piecePuzzles
     * @param _unstake Whether or not to unstake the tokens from the contract
     */
    function manageWorkers(
        uint256[] calldata _tokenIds,
        bool _unstake,
        bool _removeFromPiecePuzzle
    ) external {
        (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        ) = calculateRewardsForManyWorkers(_tokenIds);

        for (uint256 i; i < _tokenIds.length; i++) {
            StakingUtils.Worker storage existingStake = stakedWorkers[
                _tokenIds[i]
            ];

            require(existingStake.owner == _msgSender(), "NOTYOURWORKER");
            require(
                citizenContr.ownerOf(_tokenIds[i]) == address(this),
                "WORKERNOTSTAKED"
            );

            existingStake.stolen = 0;
            existingStake.taxesCollectedByLandlord = 0;
            existingStake.timestamp = block.timestamp;

            if (existingStake.home != 0) {
                uint256 key = StakingUtils.generateKey(
                    existingStake.landlord,
                    existingStake.home,
                    existingStake.isPuzzle
                );

                if (existingStake.landlord == _msgSender()) {
                    rewards[i] += landlordTaxes[i];
                } else {
                    crownsContr.mint(existingStake.landlord, landlordTaxes[i]);
                }

                if (_removeFromPiecePuzzle || _unstake) {
                    castleStakingContr.removeWorker(key, _tokenIds[i]);
                    existingStake.home = 0;
                    existingStake.landlord = address(0);
                }
            }

            if (_unstake) {
                indexedWorkersByOwner[_msgSender()].deleteValueInArray(
                    _tokenIds[i]
                );
                existingStake.owner = address(0);

                citizenContr.safeTransferFrom(
                    address(this),
                    _msgSender(),
                    _tokenIds[i]
                );
            }
        }

        if (raidersContr.getRaidersCount() > 0) {
            claimableRaidersTaxes +=
                raidersTaxes.sum() /
                raidersContr.getRaidersCount();
            crownsContr.mint(_msgSender(), rewards.sum());
        } else {
            crownsContr.mint(_msgSender(), rewards.sum() + raidersTaxes.sum());
        }
    }

    /** @notice Returns owner of a staked worker
     *
     * @param _tokenId TokenId to be checked
     */
    function getWorkersOwner(uint256 _tokenId)
        external
        view
        returns (address owner)
    {
        return stakedWorkers[_tokenId].owner;
    }

    /** @notice Used by CastleStakingContract when a worker gets associated with a piecePuzzle
     *
     * @param _workerTokenId TokenId of the worker
     * @param _piecePuzzleTokenId TokenId of the piecePuzzle
     * @param _isPuzzle Indicates if its a piece or puzzle
     * @param _landlord Owner of the piecePuzzle
     */
    function updateStakedWorker(
        uint256 _workerTokenId,
        uint256 _piecePuzzleTokenId,
        bool _isPuzzle,
        address _landlord
    ) external onlyCastleStakingContract {
        StakingUtils.Worker storage worker = stakedWorkers[_workerTokenId];

        worker.home = _piecePuzzleTokenId;
        worker.isPuzzle = _isPuzzle;
        worker.landlord = _landlord;
    }

    /** @notice Calculates the collectable staking reward for a batch of workers
     *
     * @param _tokenIds Worker tokenId
     *
     * @return rewards Amount of CROWNS earned by the workers
     * @return landlordTaxes Amount of CROWNS earned by the workers belonging to their landlords
     * @return raidersTaxes Amount of CROWNS earned by the workers belonging to the raiders
     */
    function calculateRewardsForManyWorkers(uint256[] memory _tokenIds)
        public
        view
        returns (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        )
    {
        rewards = new uint256[](_tokenIds.length);
        landlordTaxes = new uint256[](_tokenIds.length);
        raidersTaxes = new uint256[](_tokenIds.length);

        for (uint256 i; i < _tokenIds.length; i++) {
            StakingUtils.Worker memory existingStake = stakedWorkers[
                _tokenIds[i]
            ];
            uint256 timeStaked = getTimeStaked(_tokenIds[i]);

            if (timeStaked < 1 days) {
                rewards[i] = 0;
                landlordTaxes[i] = 0;
                raidersTaxes[i] = 0;
            } else {
                uint256 baseRatePerMinute = 0.0138888888888 ether;
                uint256 landlordTaxRate = 0;

                if (existingStake.home != 0) {
                    baseRatePerMinute = StakingUtils.getBaseRate(
                        existingStake.home,
                        existingStake.isPuzzle
                    );

                    landlordTaxRate = existingStake.landlord != _msgSender()
                        ? castleStakingContr.getTaxRate(
                            StakingUtils.generateKey(
                                existingStake.landlord,
                                existingStake.home,
                                existingStake.isPuzzle
                            )
                        )
                        : 0;
                }

                uint256 untaxedReward = (baseRatePerMinute *
                    (timeStaked / 1 minutes)) - existingStake.stolen;

                raidersTaxes[i] = existingStake.home != 0 &&
                    existingStake.isPuzzle
                    ? 0
                    : (((untaxedReward * RAIDER_TAX_RATE)) / 100);
                landlordTaxes[i] = (((untaxedReward - raidersTaxes[i]) *
                    landlordTaxRate) / 100);

                rewards[i] = untaxedReward - raidersTaxes[i] - landlordTaxes[i];
            }
        }
    }

    /**
     * @notice Aggregates an array of `StakingUtils.Worker` structs for all tokenIds staked by the sender
     *
     * @return workers Array of `StakingUtils.Worker` structs
     */
    function getAllStakedWorkers(address _account)
        external
        view
        returns (StakingUtils.Worker[] memory workers)
    {
        workers = new StakingUtils.Worker[](
            indexedWorkersByOwner[_account].length
        );

        for (uint256 i; i < workers.length; i++) {
            workers[i] = stakedWorkers[indexedWorkersByOwner[_account][i]];
        }

        return workers;
    }

    /**
     * @notice Returns a `StakingUtils.Worker` struct for a given tokenId
     *
     * @param _tokenId Id of the staked worker
     */
    function getStakedWorker(uint256 _tokenId)
        external
        view
        returns (StakingUtils.Worker memory worker)
    {
        worker = stakedWorkers[_tokenId];
    }

    /**
     * @notice Returns the landlord of a staked worker
     *
     * @param _tokenId Id of the staked worker
     *
     * @return landlord Owner of the associated piecePuzzle
     */
    function getStakedWorkersLandlord(uint256 _tokenId)
        public
        view
        returns (address landlord)
    {
        return stakedWorkers[_tokenId].landlord;
    }

    /**
     * @notice Returns the owner of a staked worker
     *
     * @param _tokenId Id of the staked worker
     *
     * @return landlord Owner of the staked tokenId
     */
    function getStakedWorkersOwner(uint256 _tokenId)
        public
        view
        returns (address)
    {
        return stakedWorkers[_tokenId].owner;
    }

    /**
     * @notice Used by the {CastleStakingContract} to update a staked worker when the landlord collects earned taxes
     *
     * @dev This value (`taxesCollectedByLandlord`) gets deducted from the reward when `calculateRewardsForManyWorkers`
     * @dev is called
     *
     * @param _workerTokenId Id of the staked worker
     */
    function setTaxesCollectedByLandLordForWorker(
        uint256 _workerTokenId,
        uint256 _taxesCollectedByLandlord
    ) public onlyCastleStakingContract {
        stakedWorkers[_workerTokenId]
            .taxesCollectedByLandlord = _taxesCollectedByLandlord;
    }

    /**
     * @notice Used by the {RaidersContract} to update a staked worker when a raid happened
     *
     * @dev This value (`stolen`) gets deducted from the reward when `calculateRewardsForManyWorkers`
     * @dev is called
     *
     * @param _tokenId Id of the staked worker
     * @param _stolen Amount that was stolen
     */
    function setStolenForWorker(uint256 _tokenId, uint256 _stolen)
        external
        onlyRaidersContract
    {
        stakedWorkers[_tokenId].stolen = _stolen;
        stakedWorkers[_tokenId].raided++;
    }

    function getTimeStaked(uint256 tokenId)
        public
        view
        returns (uint256 timeStaked)
    {
        timeStaked = uint256(
            block.timestamp - stakedWorkers[tokenId].timestamp
        );
    }

    /*** @notice Whenever an {IERC721} `tokenId` token is transferred to this contract via {IERC721-safeTransferFrom}
     * by `operator` from `from`, this function is called.
     *
     * It must return its Solidity selector to confirm the token transfer.
     * If any other value is returned or the interface is not implemented by the recipient,
     * the transfer will be reverted.
     *
     * The selector can be obtained in Solidity with `IERC721Receiver.onERC721Received.selector`.
     */
    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes memory data
    ) public pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[49] private __gap;
}

