interface IUsdbNFT {
    function getUsdbAmount(uint256 tokenId) external view returns(uint);
}
contract StakingBackedNFTPool is Ownable, ERC721Holder {
    
    using SafeMath for uint256;

    /* ======== EVENTS ========== */
    event stakeEvent(address _depositor, uint _tokenId);
    event unstakeEvent(address _recipient, uint _amount);
    event claimEvent(address _recipient, uint _amount);

    /* ======== STATE VARIABLES ======== */
    address public immutable AMPS; //AMPS Token Address
    address public immutable USDBNFT; //Backed NFT Address

    mapping( address => Staking ) _stakingInfo;
    mapping( address => bool) _stakedInfo;

    Terms public terms; // stores terms for new pool
    uint generationRate; //AMPS token emission rate per seconds

    /* ======== STRUCTS ======== */

    // Info for creating new POOL 
    struct Terms {
        uint vestingTermSeconds; // in seconds
        uint vestingTerm; // in blocks
    }
    //Info for NFT holder
    struct Staking {
        uint tokenId; //staked NFT token Id
        uint vestingSeconds; //seconds left to vest
        uint vesting; //blocks left to vest
        uint lastTimestamp; // Last Interaction of Time
        uint lastBlock; //Last Interaction of Block
        uint lastClaimTimestamp; //Last Interaction of Time for claim
    }

    /* ======== INITIALIZATION ======== */
    constructor(
        address _AMPS,
        address _USDBNFT
    ) {
        require(_AMPS != address(0), "No Zero Address");
        AMPS = _AMPS;
        require(_USDBNFT != address(0), "No Zero Address");
        USDBNFT = _USDBNFT;
    }
    
    /**
        @notice initilaize the staking term and pool emission rate
        @param _vestingSeconds uint
        @param _vesting uint
        @param _generationRate uint
     */
    function initializePool(
        uint _vestingSeconds,
        uint _vesting,
        uint _generationRate
    ) public onlyOwner {
        terms = Terms({
            vestingTermSeconds: _vestingSeconds,
            vestingTerm: _vesting
        });
        generationRate = _generationRate;
    }
    /* =========== OnlyOwner Functions =========*/
    /**
        @notice set generationRate
        @param _generationRate uint
     */
    function setGenerationRate(uint _generationRate) public onlyOwner {
        generationRate = _generationRate;
    }

    /* ======== USER FUNCTIONS ======== */
    /**
        @notice stake Backed NFT
        @param _tokenId uint
        @param _depositor address
    */
    function stake(uint _tokenId, address _depositor) external {
       require(IERC721(USDBNFT).ownerOf(_tokenId) == msg.sender, "No Owner of NFT");
       require(_stakedInfo[msg.sender] == false, "Already staked");
       //send NFT token from user to contract
       IERC721(USDBNFT).safeTransferFrom(msg.sender, address(this), _tokenId);

       //staking info is stored
       _stakingInfo[_depositor] = Staking({
           tokenId: _tokenId,
           vestingSeconds: terms.vestingTermSeconds,
           vesting: terms.vestingTerm,
           lastTimestamp: block.timestamp,
           lastBlock: block.number,
           lastClaimTimestamp: block.timestamp
       });
       _stakedInfo[_depositor] = true;
       emit stakeEvent(_depositor, _tokenId);
    }
    /**
        @notice claim AMPS Token
        @param  _recipient address 
        @return uint
    */
    function claim( address _recipient) public returns(uint){
        Staking memory info = _stakingInfo[msg.sender];
        uint claimAmpsAmount = _calcAmount( msg.sender, info.tokenId );
        IAmpsToken(AMPS).mint(_recipient, claimAmpsAmount);
        _stakingInfo[msg.sender].lastClaimTimestamp = block.timestamp;

        emit claimEvent(_recipient, claimAmpsAmount);
        return claimAmpsAmount;
    }
    /**
        @notice unstake NFT
        @param _recipient address
        @return uint
     */
    function unstake( address _recipient) public returns(uint) {
        require(_stakedInfo[msg.sender] == true, "No Staked NFT");
        Staking memory info = _stakingInfo[msg.sender];

        uint percentVested = percentVestedFor( msg.sender ); // (seconds since last interaction / vesting term remaining)
        uint percentVestedBlocks = percentVestedBlocksFor( msg.sender ); // (blocks since last interaction / vesting term remaining)

        require ( percentVested >= 10000, "Wait for lockup period");
        require ( percentVestedBlocks >= 10000, "Wait for lockup period");

        //send NFT token to user
        IERC721(USDBNFT).safeTransferFrom(address(this), _recipient, info.tokenId);

        uint _amount = IAmpsToken(AMPS).balanceOf(msg.sender);
        //burn Amps token of user
        IAmpsToken(AMPS).burn(msg.sender, _amount);

        delete _stakingInfo[msg.sender];

        //set the user's staked info
        _stakedInfo[msg.sender] = false;

        emit unstakeEvent(_recipient, _amount);

        return _amount;

    }
    /**
        @notice calc AMPS token amount to user
        @param _depositor address
        @param _tokenId uint
        @return uint
    */
    function _calcAmount( address _depositor, uint _tokenId ) public view returns(uint) {
        Staking memory info = _stakingInfo[_depositor];
        require(_stakedInfo[_depositor] == true, "No Staked NFT");
        //uint usdbAmount = IUsdbNFT(USDBNFT).getUsdbAmount(_tokenId);
        //uint amount = generationRate.mul(usdbAmount).mul(block.timestamp.sub(info.lastClaimTimestamp));
        uint amount = generationRate.mul(100000).mul(block.timestamp.sub(info.lastClaimTimestamp));
        return amount;
    }
    /**
        @notice calculate the time remaining from timestamp
        @param _depositor address
        @return percentVested_ uint
     */
     function percentVestedFor(address _depositor) public view returns(uint percentVested_) {
        Staking memory info = _stakingInfo[_depositor];
        uint secondsSinceLast = block.timestamp.sub( info.lastTimestamp );
        uint vestingSeconds = info.vestingSeconds;

        if ( vestingSeconds > 0 ) {
            percentVested_ = secondsSinceLast.mul( 10000 ).div(vestingSeconds);
        } else {
            percentVested_ = 10000;
        }
     }
     /**
        @notice calculate the block remaining from block number
        @param _depositor address
        @return percentVested_ uint
     */
    function percentVestedBlocksFor( address _depositor ) public view returns ( uint percentVested_ ) {
        Staking memory info = _stakingInfo[ _depositor ];
        uint blocksSinceLast = block.number.sub( info.lastBlock );
        uint vesting = info.vesting;

        if ( vesting > 0 ) {
            percentVested_ = blocksSinceLast.mul( 10000 ).div( vesting );
        } else {
            percentVested_ = 10000;
        }
    }
}