        name = _name;
        symbol = _symbol;
        _setURI(myUri);

        _registerInterface(type(IERC1155).interfaceId);
        _registerInterface(type(IERC1155Metadata).interfaceId);
        _registerInterface(type(IERC1155MetadataURI).interfaceId);
    }

