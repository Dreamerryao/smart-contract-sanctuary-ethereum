pragma solidity 0.8.9;

import "./../common/ERC1155/ERC1155.sol";
import "./../common/access/AccessControl.sol";
import "./../common/Withdrawable.sol";
import "./../common/OpenSeaCompatible.sol";

/**
 * @dev FEAR NFT test.
 */
contract FearERC1155BridgedTest is ERC1155, AccessControl, Withdrawable, OpenSeaCompatible {

    /**
    * @dev Minter role identifier.
    */
    bytes32 public constant MINTER_ROLE = "Minter";

    /**
    * @dev Bridger role identifier.
    */
    bytes32 public constant BRIDGER_ROLE = "Bridger";

    constructor(string memory uri) ERC1155("FEAR ERC1155 Bridged Test", "F1155TEST", uri) {
        _setRoleAdmin(MINTER_ROLE, ROOT_ROLE);
        _setRoleAdmin(BRIDGER_ROLE, ROOT_ROLE);
    }

    function initialMint(address initialOwner, uint256 id, uint256 amount) external onlyRole(MINTER_ROLE) {
        require(initialOwner != address(0), "Zero address");
        require(amount > 0, "Cannot mint zero tokens");
        require(block.chainid == 4, "Initial minting available only on Rinkeby");

        _mint(initialOwner, id, amount, "");
    }

    // MUON Bridging functions

    function mint(address to, uint256 id, uint256 amount, bytes memory data) external onlyRole(BRIDGER_ROLE) {
        _mint(to, id, amount, data);
    }

    function burn(address account, uint256 id, uint256 amount) external onlyRole(BRIDGER_ROLE) {
        require(isApprovedForAll(account, _msgSender()), "caller is not approved");
        _burn(account, id, amount);
    }

    // tokens of owner?
}

