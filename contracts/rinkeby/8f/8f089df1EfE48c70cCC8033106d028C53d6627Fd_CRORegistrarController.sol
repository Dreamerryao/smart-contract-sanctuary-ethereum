import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";
import "./interfaces/IPriceBase.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "../library/StringUtils.sol";

contract StableERC20PriceBase is IPriceBase, Ownable, ReentrancyGuard, Proxied {
    using SafeMath for uint256;
    using SafeERC20 for ERC20;
    using SafeERC20 for IERC20;
    using StringUtils for *;

    // uint256 immutable GRACE_PERIOD = 90 days;
    address public treasury;
    // Rent in base price units by length. Element 0 is for 1-length names, and so on.
    uint256[] public rentPrices;
    ERC20 public feeCurrency;
    mapping(address => bool) public whitelist;

    event RentPriceChanged(uint256[] prices);
    event FeeCurrencyChanged(
        address indexed previousToken,
        address indexed nextToken
    );
    event TransferFee(
        string indexed name,
        uint256 indexed expires,
        uint256 duration,
        address indexed from,
        uint256 fees
    );
    event WhitelistAdd(address indexed user);
    event WhitelistRemove(address indexed user);
    event TreasuryChanged(
        address indexed previousTreasury,
        address indexed nextTreasury
    );

    function init(
        uint256[] memory _rentPrices,
        address _feeCurrency,
        address _treasury
    ) public proxied {
        require(
            address(_treasury) != address(0),
            "StableERC20PriceBase: _treasury ZERO ADDRESS"
        );
        require(
            address(_feeCurrency) != address(0),
            "StableERC20PriceBase: _feeCurrency ZERO ADDRESS"
        );

        rentPrices = _rentPrices;
        feeCurrency = ERC20(_feeCurrency);
        treasury = _treasury;

        emit RentPriceChanged(rentPrices);
        emit FeeCurrencyChanged(address(0), address(feeCurrency));
    }

    constructor(
        uint256[] memory _rentPrices,
        address _feeCurrency,
        address _treasury
    ) {
        init(_rentPrices, _feeCurrency, _treasury);
    }

    /// @dev Get the price for registerng/renewing name
    /// @param name name string
    /// @param expires expiration timestamp
    /// @param duration duration for lease
    /// @return Price in 10^18 decimals
    function price(
        string calldata name,
        uint256 expires,
        uint256 duration
    ) public view override returns (uint256) {
        return _adjustForTokenDecimals(_price(name, expires, duration));
    }

    /**
     * @dev Returns the pricing premium in wei.
     */
    function premium(
        string calldata name,
        uint256 expires,
        uint256 duration
    ) external view returns (uint256) {
        return _adjustForTokenDecimals(_premium(name, expires, duration));
    }

    /// @dev Transfer the rent fee from user to treasury
    /// @param name name string
    /// @param expires expiration timestamp
    /// @param duration duration for lease
    /// @param from user address to transfer money from
    function transferFee(
        string calldata name,
        uint256 expires,
        uint256 duration,
        address from
    ) external override nonReentrant {
        if (whitelist[from]) {
            emit TransferFee(name, expires, duration, from, 0);
        } else {
            uint256 fees = price(name, expires, duration);
            emit TransferFee(name, expires, duration, from, fees);
            feeCurrency.safeTransferFrom(from, treasury, fees);
        }
    }

    function addToWhitelist(address _user) external onlyOwner {
        whitelist[_user] = true;
        emit WhitelistAdd(_user);
    }

    function removeFromWhitelist(address _user) external onlyOwner {
        whitelist[_user] = false;
        emit WhitelistRemove(_user);
    }

    function setTreasury(address treasury_) external onlyOwner {
        require(
            treasury_ != address(0),
            "[email protected])PriceBase::setTreasury: ZERO ADDRESS"
        );
        address previousTreasury = treasury;
        treasury = treasury_;
        emit TreasuryChanged(previousTreasury, treasury);
    }

    /**
     * @dev Sets rent prices.
     * @param _rentPrices The price array. Each element corresponds to a specific
     *                    name length; names longer than the length of the array
     *                    default to the price of the last element. Values are
     *                    in base price units, equal to one attodollar (1e-18
     *                    dollar) each.
     */
    function setPrices(uint256[] memory _rentPrices) public onlyOwner {
        rentPrices = _rentPrices;
        emit RentPriceChanged(_rentPrices);
    }

    function setCurrency(ERC20 _feeCurrency) external onlyOwner {
        IERC20 previousCurrency = feeCurrency;
        feeCurrency = _feeCurrency;
        emit FeeCurrencyChanged(
            address(previousCurrency),
            address(feeCurrency)
        );
    }

    function feeCurrencyDecimals() external view returns (uint256) {
        return feeCurrency.decimals();
    }

    function withdraw() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawERC20(IERC20 _token, uint256 _amount) public onlyOwner {
        _token.safeTransfer(msg.sender, _amount);
    }

    function withdrawFees() public onlyOwner {
        feeCurrency.safeTransfer(msg.sender, feeCurrency.balanceOf(msg.sender));
    }

    /// @dev Get the price for registerng/renewing name
    /// @param name name string
    /// @param expires expiration timestamp
    /// @param duration duration for lease
    /// @return Price in 10^18 decimals
    function _price(
        string calldata name,
        uint256 expires,
        uint256 duration
    ) internal view returns (uint256) {
        uint256 len = name.strlen();
        if (len > rentPrices.length) {
            len = rentPrices.length;
        }
        require(len > 0, "StableERC20PriceBase::_price: empty rentPrices");

        uint256 basePrice = rentPrices[len - 1].mul(duration);

        basePrice = basePrice.add(_premium(name, expires, duration));

        return basePrice;
    }

    /**
     * @dev Returns the pricing premium in internal base units.
     */
    function _premium(
        string memory,
        uint256,
        uint256
    ) internal view virtual returns (uint256) {
        return 0;
    }

    function _adjustForTokenDecimals(uint256 amount)
        internal
        view
        returns (uint256)
    {
        uint256 feeDecimals = feeCurrency.decimals();
        if (feeDecimals > 18) {
            return amount.mul(10**(feeDecimals.sub(18)));
        } else {
            return amount.div(10**(uint256(18).sub(feeDecimals)));
        }
    }

    function supportsInterface(bytes4 interfaceID)
        public
        view
        virtual
        returns (bool)
    {
        return
            interfaceID == type(IERC165).interfaceId ||
            interfaceID == type(IPriceBase).interfaceId;
    }
}

