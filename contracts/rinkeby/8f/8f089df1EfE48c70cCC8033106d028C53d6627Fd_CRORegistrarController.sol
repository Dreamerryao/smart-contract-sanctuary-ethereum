        return _register(id, nameOwner, duration, false);
    }

    function _register(
        uint256 id,
        address nameOwner,
        uint256 duration,
        bool updateRegistry
    ) internal live onlyController returns (uint256) {
        require(available(id), "BaseRegistrar::_register(): not available");
        require(
            block.timestamp + duration + GRACE_PERIOD >
                block.timestamp + GRACE_PERIOD,
            "BaseRegistrar::_register(): duration less than grace period"
        ); // Prevent future overflow

        expiries[id] = block.timestamp + duration;
        if (_exists(id)) {
            // Name was previously owned, and expired
            _burn(id);
        }
        _mint(nameOwner, id);
        if (updateRegistry) {
            ens.setSubnodeOwner(baseNode, bytes32(id), nameOwner);
        }

        emit NameRegistered(id, nameOwner, block.timestamp + duration);

        return block.timestamp + duration;
    }

    function renew(uint256 id, uint256 duration)
        external
        override
        live
        onlyController
        returns (uint256)
    {
        require(
            expiries[id] + GRACE_PERIOD >= block.timestamp,
            "BaseRegistrar: name not found"
        ); // Name must be registered here or in grace period
        require(
            expiries[id] + duration + GRACE_PERIOD > duration + GRACE_PERIOD,
            "BaseRegistrar: name not found"
        ); // Prevent future overflow

        expiries[id] += duration;
        emit NameRenewed(id, expiries[id]);
        return expiries[id];
    }

    /**
     * @dev Reclaim ownership of a name in ENS, if you own it in the registrar.
     */
    function reclaim(uint256 id, address nameOwner) external override live {
        require(
            _isApprovedOrOwner(msg.sender, id),
            "BaseRegistrar:reclaim(): sender not approved"
        );
        ens.setSubnodeOwner(baseNode, bytes32(id), nameOwner);
    }

    function supportsInterface(bytes4 interfaceID)
        public
        pure
        override(ERC721Upgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return
            interfaceID == INTERFACE_META_ID ||
            interfaceID == ERC721_ID ||
            interfaceID == RECLAIM_ID;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity >=0.8.4;

import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";

import "./StableERC20PriceBase.sol";
import "./interfaces/IPriceBase.sol";
import "./BaseRegistrarImplementationUpgradeable.sol";
import "../library/StringUtils.sol";
import "../resolver/interfaces/IResolver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

/**
 * @dev A registrar controller for registering and renewing names at fixed cost.
 */
contract CRORegistrarController is Ownable, Proxied {
    using StringUtils for *;
    using SafeERC20 for IERC20;

    uint256 public constant MIN_REGISTRATION_DURATION = 28 days;

    bytes4 private constant INTERFACE_META_ID =
        bytes4(keccak256("supportsInterface(bytes4)"));
    bytes4 private constant COMMITMENT_CONTROLLER_ID =
        bytes4(
            keccak256("rentPrice(string,uint256)") ^
                keccak256("available(string)") ^
                keccak256("makeCommitment(string,address,bytes32)") ^
                keccak256("commit(bytes32)") ^
                keccak256("register(string,address,uint256,bytes32)") ^
                keccak256("renew(string,uint256)")
        );

    bytes4 private constant COMMITMENT_WITH_CONFIG_CONTROLLER_ID =
        bytes4(
            keccak256(
                "registerWithConfig(string,address,uint256,bytes32,address,address)"
            ) ^
                keccak256(
                    "makeCommitmentWithConfig(string,address,bytes32,address,address)"
                )
        );

    BaseRegistrarImplementationUpgradeable public base;
    IPriceBase public priceBase;
    uint256 public minCommitmentAge;
    uint256 public maxCommitmentAge;

    mapping(bytes32 => uint256) public commitments;

    event NameRegistered(
        string name,
        bytes32 indexed label,
        address indexed nameOwner,
        uint256 cost,
        uint256 expires
    );
    event NameRenewed(
        string name,
        bytes32 indexed label,
        uint256 cost,
        uint256 expires
    );
    event NewPriceBase(address indexed oracle);
    event CommitmentAgeChanged(
        uint256 indexed minCommitmentAge,
        uint256 indexed maxCommitmentAge
    );

    function init(
        BaseRegistrarImplementationUpgradeable _base,
        IPriceBase _priceBase,
        uint256 _minCommitmentAge,
        uint256 _maxCommitmentAge
    ) public proxied {
        require(
            _maxCommitmentAge > _minCommitmentAge,
            "CROController: max is less than min age"
        );

        base = _base;
        priceBase = _priceBase;
        minCommitmentAge = _minCommitmentAge;
        maxCommitmentAge = _maxCommitmentAge;
        emit CommitmentAgeChanged(minCommitmentAge, maxCommitmentAge);
    }

    constructor(
        BaseRegistrarImplementationUpgradeable _base,
        IPriceBase _priceBase,
        uint256 _minCommitmentAge,
        uint256 _maxCommitmentAge
    ) {
        init(_base, _priceBase, _minCommitmentAge, _maxCommitmentAge);
    }

    function rentPrice(string memory name, uint256 duration)
        public
        view
        returns (uint256)
    {
        bytes32 hash = keccak256(bytes(name));
        uint256 expires = base.nameExpires(uint256(hash));
        return priceBase.price(name, expires, duration);
    }

    function _transferRentPrice(string memory name, uint256 duration) internal {
        bytes32 hash = keccak256(bytes(name));
        priceBase.transferFee(
            name,
            base.nameExpires(uint256(hash)),
            duration,
            msg.sender
        );
    }

    function valid(string memory name) public pure returns (bool) {
        return name.strlen() >= 3;
    }

    function available(string memory name) public view returns (bool) {
        bytes32 label = keccak256(bytes(name));
        return valid(name) && base.available(uint256(label));
    }

    function makeCommitment(
        string memory _name,
        address _nameOwner,
        bytes32 _secret
    ) public pure returns (bytes32) {
        return
            makeCommitmentWithConfig(
                _name,
                _nameOwner,
                _secret,
                address(0),
                address(0)
            );
    }

    function makeCommitmentWithConfig(
        string memory name,
        address nameOwner,
        bytes32 secret,
        address resolver,
        address addr
    ) public pure returns (bytes32) {
        bytes32 label = keccak256(bytes(name));
        if (resolver == address(0) && addr == address(0)) {
            return keccak256(abi.encodePacked(label, nameOwner, secret));
        }
        require(resolver != address(0), "CROController: resolver ZERO ADDRESS");
        return
            keccak256(
                abi.encodePacked(label, nameOwner, resolver, addr, secret)
            );
    }

    function commit(bytes32 commitment) public {
        require(
            commitments[commitment] + maxCommitmentAge < block.timestamp,
            "CROController: Commitment already exists"
        );
        commitments[commitment] = block.timestamp;
    }

    function register(
        string calldata name,
        address nameOwner,
        uint256 duration,
        bytes32 secret
    ) external payable {
        registerWithConfig(
            name,
            nameOwner,
            duration,
            secret,
            address(0),
            address(0)
        );
    }

    function registerWithConfig(
        string memory name,
        address nameOwner,
        uint256 duration,
        bytes32 secret,
        address resolver,
        address addr
    ) public payable {
        bytes32 commitment = makeCommitmentWithConfig(
            name,
            nameOwner,
            secret,
            resolver,
            addr
        );
        uint256 cost = _consumeCommitment(name, duration, commitment);
        // transfer fee token
        _transferRentPrice(name, duration);

        bytes32 label = keccak256(bytes(name));
        uint256 tokenId = uint256(label);

        uint256 expires;
        if (resolver != address(0)) {
            // Set this contract as the (temporary) owner, giving it
            // permission to set up the resolver.
            expires = base.register(tokenId, address(this), duration);

            // The nodehash of this label
            bytes32 nodehash = keccak256(
                abi.encodePacked(base.baseNode(), label)
            );

            // Set the resolver
            base.ens().setResolver(nodehash, resolver);

            // Configure the resolver
            if (addr != address(0)) {
                IResolver(resolver).setAddr(nodehash, addr);
            }

            // Now transfer full ownership to the expeceted owner
            base.reclaim(tokenId, nameOwner);

            base.transferFrom(address(this), nameOwner, tokenId);
        } else {
            require(
                addr == address(0),
                "CRORegistrarController::registerWithConfig(): addr is not zero"
            );
            expires = base.register(tokenId, nameOwner, duration);
        }

        emit NameRegistered(name, label, nameOwner, cost, expires);

        // // Refund any extra payment
        // if (msg.value > cost) {
        //     payable(msg.sender).transfer(msg.value - cost);
        // }
    }

    /// @notice Bulk Reserve names
    /// @param names array for names to reserve
    /// @param nameOwners array for owners in same order as names array
    /// @param duration duration to register for
    /// @param resolver resolver address
    function reserve(
        string[] memory names,
        address[] memory nameOwners,
        uint256 duration,
        address resolver
    ) public onlyOwner {
        require(
            names.length == nameOwners.length,
            "CRORegistrarController::reserve: names and owners should have equal length"
        );

        for (uint256 i = 0; i < names.length; i++) {
            string memory name = names[i];
            address nameOwner = nameOwners[i];
            bytes32 label = keccak256(bytes(name));
            uint256 tokenId = uint256(label);

            uint256 expires;
            // Set this contract as the (temporary) owner, giving it
            // permission to set up the resolver.
            expires = base.register(tokenId, address(this), duration);

            // The nodehash of this label
            bytes32 nodehash = keccak256(
                abi.encodePacked(base.baseNode(), label)
            );

            // Set the resolver
            base.ens().setResolver(nodehash, resolver);

            // Configure the resolver
            // IResolver(resolver).setAddr(nodehash, nameOwner);

            // Now transfer full ownership to the expected owner
            base.reclaim(tokenId, nameOwner);

            base.transferFrom(address(this), nameOwner, tokenId);

            emit NameRegistered(name, label, nameOwner, 0, expires);
        }
    }

    function renew(string calldata name, uint256 duration) external payable {
        uint256 cost = rentPrice(name, duration);
        // transfer the fee token
        _transferRentPrice(name, duration);
        // require(msg.value >= cost);

        bytes32 label = keccak256(bytes(name));
        uint256 expires = base.renew(uint256(label), duration);

        // if (msg.value > cost) {
        //     payable(msg.sender).transfer(msg.value - cost);
        // }

        emit NameRenewed(name, label, cost, expires);
    }

    function setPriceBase(IPriceBase _priceBase) public onlyOwner {
        priceBase = _priceBase;
        emit NewPriceBase(address(priceBase));
    }

    function setCommitmentAges(
        uint256 _minCommitmentAge,
        uint256 _maxCommitmentAge
    ) public onlyOwner {
        minCommitmentAge = _minCommitmentAge;
        maxCommitmentAge = _maxCommitmentAge;
        emit CommitmentAgeChanged(minCommitmentAge, maxCommitmentAge);
    }

    function withdraw() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawERC20(IERC20 _token, uint256 _amount) public onlyOwner {
        _token.safeTransfer(msg.sender, _amount);
    }

    function supportsInterface(bytes4 interfaceID)
        external
        pure
        returns (bool)
    {
        return
            interfaceID == INTERFACE_META_ID ||
            interfaceID == COMMITMENT_CONTROLLER_ID ||
            interfaceID == COMMITMENT_WITH_CONFIG_CONTROLLER_ID;
    }

    function _consumeCommitment(
        string memory name,
        uint256 duration,
        bytes32 commitment
    ) internal returns (uint256) {
        // Require a valid commitment
        require(
            commitments[commitment] + minCommitmentAge <= block.timestamp,
            "CRORegistrarController::_consumeCommitment(): commitment doesn't exist or too early to consume"
        );

        // If the commitment is too old, or the name is registered, stop
        require(
            commitments[commitment] + maxCommitmentAge > block.timestamp,
            "CRORegistrarController::_consumeCommitment(): commitment too old"
        );
        require(
            available(name),
            "CRORegistrarController::_consumeCommitment(): Name not available"
        );

        delete (commitments[commitment]);

        uint256 cost = rentPrice(name, duration);
        require(
            duration >= MIN_REGISTRATION_DURATION,
            "CRORegistrarController::_consumeCommitment(): less than min duration"
        );
        // require(msg.value >= cost);

        return cost;
    }
}

