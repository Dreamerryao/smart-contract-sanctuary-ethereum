pragma solidity ^0.8.0;
//import de alguns contratos da OpenZeppelin
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";

//heranca contrato ERC721URIStorage
contract MyEpicNFT is ERC721URIStorage {
    //magica dada pelo o OpenZeppelin para nos ajudar a observar os tokensIds
    //aqui ele esta usando a biblioteca counters para criar uma variavel
    
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    //Precisa passar o nome do token NFT e o simbolo dele
    constructor() ERC721("PixeledWordsNFT", "PWN"){
        console.log("Esse e meu contrato | PixeledWordsNFT: ");
    }

    //funcao para o usuario pegar (mintar) sua NFT
    function makeAnEpicNFT() public {
        //Pega o tokenId atual, que come√ßa com 0
        uint256 newItemId = _tokenIds.current();
        //Minta ("cunha/forja") o NFT para o sender (quem ativa o contrato - msg.sender)
        _safeMint(msg.sender, newItemId);
        //designa os dados NFT
        _setTokenURI(newItemId, "data:application/json;base64,");
        console.log("Uma NFT com ID %s foi mintada para %s", newItemId, msg.sender);
        //incrementa o contador para o proximo nft
        _tokenIds.increment();
    }
}

