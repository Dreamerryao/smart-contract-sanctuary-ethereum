pragma solidity ^0.8.9;

import './IUniswapV2Router02.sol';


contract SendToken {

    function SednEther(address receiver) external payable{
        receiver.call{value: msg.value}("");
    }

    function getBalance() public view returns(uint) {
        return address(this).balance;
    }

    IUniswapV2Router02 uni = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

    address TokenOut = 0xeb8f08a975Ab53E34D8a0330E0D34de942C95926;

    function swapExactETHforTokens(  uint256 amountOut,address receiver) external payable {
        address[] memory path = new address[](2);
        path[0] = uni.WETH();
        path[1] = TokenOut;

            uni.swapExactETHForTokens{value: msg.value}(
                amountOut,
                path,
                receiver,
                block.timestamp + 25
            );
    }

    function swapTokensForExactTokens( uint256 amountOut, uint256 amountInMax,  address receiver) external payable{
        address[] memory path = new address[](2);
        path[0] = TokenOut;
        path[1] = TokenOut;

            uni.swapTokensForExactTokens(
                amountOut,
                amountInMax,
                path,
                receiver,
                block.timestamp + 25
            );
    }
    
    
}

pragma solidity ^0.8.9;


import './IUniswapV2Router01.sol';

