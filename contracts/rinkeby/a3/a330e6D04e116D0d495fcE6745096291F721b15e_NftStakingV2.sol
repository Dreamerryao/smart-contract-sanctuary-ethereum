pragma solidity ^0.8.7;
pragma experimental ABIEncoderV2;

import "../lib/UintSet.sol";
import "../lib/AdressSet.sol";
import "../Interfaces/IFarmV2.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "hardhat/console.sol";

contract NftStakingV2 is AccessControl, IERC1155Receiver {
    using AddressSet for AdSet;
    using UintSet for Set;
    using SafeMath for uint256;

    event RewardClaimed(
        uint256[] tokenIds,
        address staker,
        uint256 stoneAmount
    );
    event TokenStaked(address staker, uint256 tokenId);
    event BatchTokenStaked(address staker, uint256[] tokenIds);
    event TokenUnstaked(address staker, uint256[] tokenIds);
    event SetWhitelist(address whitelisted);
    event SetWeight(uint256 weight);
    event StakingFunded(uint256 fundedAmount, uint256 contractBalance);
    event SetFarm(address farm);
    event Initialize(bool isTrue);

    bytes4 internal constant _ERC1155_RECEIVED = 0xf23a6e61;
    bytes4 internal constant _ERC1155_BATCH_RECEIVED = 0xbc197c81;
    bytes4 internal constant _ERC1155_REJECTED = 0xffffffff;

    IFarmV2 public farm;
    uint256 public cardWeight;
    address public whitelistedAddress;

    struct StakedNFT {
        address staker;
        uint256 totalClaimedReward;
        uint256 lastClaimedTimestamp;
        uint256 enterTimestamp;
        uint256 exitTimestamp;
    }

    // Token ID => StakedNFT
    mapping(uint256 => StakedNFT) private stakeNFT;
    // Staker Address => Staked Token IDs
    mapping(address => Set) private stakedTokenIds;

    constructor(
        uint256 _cardWeight,
        address _farmV2,
        address _collection,
        address _admin
    ) {
        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        cardWeight = _cardWeight;
        whitelistedAddress = _collection;

        setFarmAddress(_farmV2);

        emit Initialize(true);
    }

    function setWhiteList(address collection)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        whitelistedAddress = collection;

        emit SetWhitelist(whitelistedAddress);
    }

    function setCardWeight(uint256 _cardWeight)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        cardWeight = _cardWeight;

        emit SetWeight(cardWeight);
    }

    function setFarmAddress(address _farm) public onlyRole(DEFAULT_ADMIN_ROLE) {
        farm = IFarmV2(_farm);

        emit SetFarm(_farm);
    }

    function fundStaking(uint256 amount) internal {
        uint256 contractBalance = farm.rewardedStones(address(this));
        uint256 fundAmount = (5000000 ether + amount);

        contractBalance += fundAmount;
        farm.giveAway(address(this), contractBalance);

        emit StakingFunded(fundAmount, contractBalance);
    }

    function claimReward(uint256[] memory tokenIds) public {
        uint256 claimableReward;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            StakedNFT storage stakedNFT = stakeNFT[tokenIds[i]];

            require(msg.sender == stakedNFT.staker, "You do not own this card");

            uint256 reward = _calculateReward(tokenIds[i]);
            claimableReward += reward;

            stakedNFT.totalClaimedReward += reward;
            stakedNFT.lastClaimedTimestamp = block.timestamp;
        }

        _sendStone(claimableReward);

        emit RewardClaimed(tokenIds, msg.sender, claimableReward);
    }

    function unstake(uint256 tokenId) external {
        StakedNFT storage stakedNFT = stakeNFT[tokenId];
        require(
            stakedNFT.staker == msg.sender,
            "NftStaking: not staked for owner"
        );
        uint256[] memory tokenIds = new uint256[](1);
        tokenIds[0] = tokenId;

        uint256 claimableReward = _calculateReward(tokenId);

        stakedNFT.staker = address(0);
        stakedNFT.exitTimestamp = block.timestamp;

        stakedTokenIds[msg.sender].remove(tokenId);

        IERC1155 whiteListedCollection = IERC1155(whitelistedAddress);

        _sendStone(claimableReward);
        whiteListedCollection.safeTransferFrom(
            address(this),
            msg.sender,
            tokenId,
            1,
            ""
        );

        emit RewardClaimed(tokenIds, msg.sender, claimableReward);
        emit TokenUnstaked(msg.sender, tokenIds);
    }

    function batchUnStake(uint256[] memory tokenIds) external {
        uint256 numTokens = tokenIds.length;
        require(numTokens != 0, "NftStaking: no tokens");

        uint256[] memory values = new uint256[](numTokens);
        uint256 claimableReward;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            StakedNFT storage stakedNFT = stakeNFT[tokenIds[i]];
            require(
                stakedNFT.staker == msg.sender,
                "NftStaking: not staked for owner"
            );

            values[i] = 1;

            claimableReward = _calculateReward(tokenIds[i]);

            stakedNFT.staker = address(0);
            stakedNFT.exitTimestamp = block.timestamp;
            stakedTokenIds[msg.sender].remove(tokenIds[i]);
        }

        IERC1155 whiteListedCollection = IERC1155(whitelistedAddress);

        _sendStone(claimableReward);
        whiteListedCollection.safeBatchTransferFrom(
            address(this),
            msg.sender,
            tokenIds,
            values,
            ""
        );

        emit RewardClaimed(tokenIds, msg.sender, claimableReward);
        emit TokenUnstaked(msg.sender, tokenIds);
    }

    /**
     * @return the NFTs that the 'staker' has staked on this contract.
     */
    function getUserTokenIds(address staker)
        public
        view
        returns (uint256[] memory)
    {
        return stakedTokenIds[staker].getArray();
    }

    function getReward(uint256[] memory tokenIds)
        public
        view
        returns (uint256)
    {
        uint256 totalReward;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            totalReward += _calculateReward(tokenIds[i]);
        }
        return totalReward;
    }

    function getTokenInfo(uint256 tokenId)
        public
        view
        returns (StakedNFT memory)
    {
        return stakeNFT[tokenId];
    }

    function _registerStaker(uint256 tokenId, address staker) internal {
        StakedNFT storage stakedNFT = stakeNFT[tokenId];

        stakedTokenIds[staker].add(tokenId);

        stakedNFT.staker = staker;
        stakedNFT.lastClaimedTimestamp = block.timestamp;
        stakedNFT.enterTimestamp = block.timestamp;
        stakedNFT.exitTimestamp = 0;
    }

    function _stake(uint256 tokenId, address staker) internal {
        _registerStaker(tokenId, staker);

        emit TokenStaked(staker, tokenId);
    }

    function _batchStake(uint256[] memory tokenIds, address staker) internal {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _registerStaker(tokenIds[i], staker);
        }
        emit BatchTokenStaked(staker, tokenIds);
    }

    function _calculateReward(uint256 tokenId) internal view returns (uint256) {
        StakedNFT memory stakedNFT = stakeNFT[tokenId];
        require(
            stakedNFT.staker != address(0),
            "This token is not staked currently on this contract"
        );
        uint256 stakeTime = block.timestamp - stakedNFT.lastClaimedTimestamp;

        return cardWeight.mul(stakeTime).mul(11574074074074000);
    }

    function _sendStone(uint256 amount) internal {
        require(amount > 0, "You don't have enough rewards to collect");

        uint256 contractBalance = farm.rewardedStones(address(this));

        if (contractBalance < amount) {
            fundStaking(amount);
        }

        farm.sell(amount, address(this), msg.sender);
    }

    /**
     * ERC1155Receiver hook for single transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155Received(
        address, /*operator*/
        address from,
        uint256 id,
        uint256, /*value*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        if (whitelistedAddress == msg.sender) {
            _stake(id, from);
            return _ERC1155_RECEIVED;
        }
        require(false, "NftStaking: contract not whitelisted");
    }

    /**
     * ERC1155Receiver hook for batch transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155BatchReceived(
        address, /*operator*/
        address from,
        uint256[] calldata ids,
        uint256[] calldata, /*values*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        if (whitelistedAddress == msg.sender) {
            _batchStake(ids, from);
            return _ERC1155_BATCH_RECEIVED;
        }
        require(false, "NftStaking: contract not whitelisted");
    }
}

