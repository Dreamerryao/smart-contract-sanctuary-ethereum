pragma solidity 0.8.9;

import "./AlexGCoin.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";
import "./VRFConsumerBaseUpgradeable.sol";

interface Students {
    function getStudentsList() external view returns (string[] memory);
}

contract TokenVendor is UUPSUpgradeable, OwnableUpgradeable, VRFConsumerBaseUpgradeable, ChainlinkClient {
    using Chainlink for Chainlink.Request;
    AlexGCoin internal ownToken;
    Students internal students;
    ERC721 internal ownableToken;

    event BuyTokens(address buyer, uint256 paidAmount, uint256 sellAmount);
    event NotEnoughTokens(address buyer, uint256 paidAmount, uint256 sellAmount);

    struct OrderData {
        address buyer;
        address payableTokenAddress;
        uint payableAmount;
        bytes32 priceRequestId;
        bytes32 randomNumberRequestId;
        uint priceData;
        uint randomNumberData;
    }

    mapping(bytes32 => OrderData) private priceRequestMap;
    mapping(bytes32 => OrderData) private randomNumberRequestMap;
    mapping(address => OrderData) private orderMap;
    //VRFConsumer properties
    bytes32 internal keyHash;
    uint256 internal fee;

    event NumberRequested(bytes32 indexed requestId, address indexed user);
    event NumberProvided(bytes32 indexed requestId, uint256 indexed result);
    //ChainlinkClient properties
    address private oracle;
    bytes32 private jobId;

    event PriceRequested(bytes32 indexed requestId, address indexed user);
    event PriceProvided(bytes32 indexed requestId, bytes32 indexed result);

    function initialize(
        address tokenAddress,
        address studentsAddress,
        address ownableTokenAddress,
        address vrfCoordinator,
        address linkAddress,
        bytes32 _keyHash,
        uint256 _fee,
        address _oracle/*,
        bytes32 _jobId*/)
    public initializer
    {
        ownToken = AlexGCoin(tokenAddress);
        students = Students(studentsAddress);
        ownableToken = ERC721(ownableTokenAddress);
        keyHash = _keyHash;
        fee = _fee;
        oracle = _oracle;
//        jobId = _jobId;
        jobId = "6e053707957748e2b022121c775f9efe";
        __VRFConsumerBaseUpgradeable_init(vrfCoordinator, linkAddress);
        __UUPSUpgradeable_init();
        __Ownable_init();
    }

    function _authorizeUpgrade(address newImpl) internal override onlyOwner {
    }

    modifier onlyTokenOwner {
        uint256 tokenBalance = ownableToken.balanceOf(msg.sender);
        require(tokenBalance > 0, string(abi.encodePacked("Only owner of ", ownableToken.name(), " can call this function")));
        _;
    }

    function buyTokensForETH() public payable onlyTokenOwner {
        require(msg.value > 0, "Send ETH to buy some tokens");
        createNewOrder(msg.sender, msg.value, address(0));
    }

    function buyTokensForERC20(address payableTokenAddress, uint256 payableTokenAmount) public virtual onlyTokenOwner {
        require(payableTokenAmount > 0, "payableTokenAmount must be greater than zero");
        uint256 allowance = ERC20(payableTokenAddress).allowance(msg.sender, address(this));
        require(allowance >= payableTokenAmount, "Allowance must be greater or equals to payableTokenAmount");
        createNewOrder(msg.sender, payableTokenAmount, payableTokenAddress);
    }

    function createNewOrder(address buyer, uint payableAmount, address payableTokenAddress) internal {
        require(orderMap[buyer].buyer != buyer, "Order already in progress");
        bytes32 priceRequestId = requestETHPrice(buyer);
        bytes32 randomNumberRequestId = requestRandomNumber(buyer);
        OrderData memory order;
        order.payableTokenAddress = payableTokenAddress;
        order.buyer = buyer;
        order.payableAmount = payableAmount;
        order.priceRequestId = priceRequestId;
        order.randomNumberRequestId = randomNumberRequestId;
        priceRequestMap[priceRequestId] = order;
        randomNumberRequestMap[randomNumberRequestId] = order;
        orderMap[buyer] = order;
    }

    function completeOrder(OrderData memory orderData) internal {
        if (orderData.randomNumberData == 0 || orderData.priceData == 0) {
            return;
        }
        uint256 currentPrice = calculateTokenPrice(orderData.priceData, orderData.randomNumberData);
        uint256 amountToBuy = orderData.payableAmount * currentPrice / (10 ** 18);
        uint256 vendorBalance = ownToken.balanceOf(address(this));
        if (orderData.payableTokenAddress != address(0)) {
            //complete ERC20 order type
            require(vendorBalance >= amountToBuy, "Not enough tokens for sell");
            bool paid = ERC20(orderData.payableTokenAddress).transferFrom(orderData.buyer, address(this), orderData.payableAmount);
            require(paid, "Failed to transfer payable tokens from buyer to vendor wallet");
            bool sent = ownToken.transfer(orderData.buyer, amountToBuy);
            require(sent, "Failed to transfer tokens to buyer");
            emit BuyTokens(orderData.buyer, orderData.payableAmount, amountToBuy);
        } else {
            //complete ETH order type
            if (vendorBalance >= amountToBuy) {
                bool sent = ownToken.transfer(orderData.buyer, amountToBuy);
                require(sent, "Failed to transfer token to user");
                emit BuyTokens(orderData.buyer, orderData.payableAmount, amountToBuy);
            } else {
                (bool sent,) = orderData.buyer.call{value : orderData.payableAmount}("Sorry, there is not enough tokens to buy");
                require(sent, "Failed to return back ETH to user");
                emit NotEnoughTokens(orderData.buyer, orderData.payableAmount, amountToBuy);
            }
        }
    }

    function calculateTokenPrice(uint priceData, uint multiplier) internal view returns (uint256) {
        return (priceData / students.getStudentsList().length) * multiplier;
    }

    function requestRandomNumber(address requester) internal returns (bytes32) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK to pay fee");
        bytes32 requestId = requestRandomness(keyHash, fee);
        emit NumberRequested(requestId, requester);
        return requestId;
    }

    function requestETHPrice(address requester) internal returns (bytes32) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK to pay fee");
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfillETHPrice.selector);
        request.add("get", "https://min-api.cryptocompare.com/data/price?fsym=ETH&tsyms=USD");
        request.add("path", "USD");
        request.addInt("times", 100);
        bytes32 requestId = sendChainlinkRequestTo(oracle, request, fee);
        emit PriceRequested(requestId, requester);
        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 min = 1;
        uint256 max = 10;
        uint256 randomNumberResult = randomness % (max - min + 1) + min;
        randomNumberRequestMap[requestId].randomNumberData = randomNumberResult;
        emit NumberProvided(requestId, randomNumberResult);
        completeOrder(randomNumberRequestMap[requestId]);
    }

    function fulfillETHPrice(bytes32 _requestId, bytes32 _data) public recordChainlinkFulfillment(_requestId) {
        priceRequestMap[_requestId].priceData = uint256(_data);
        emit PriceProvided(_requestId, _data);
        completeOrder(priceRequestMap[_requestId]);
    }
}

