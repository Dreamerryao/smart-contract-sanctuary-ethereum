import "hardhat/console.sol";

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

import "./WagerDAORngProxyInterface.sol";

contract Jolt is Ownable {
    event wagerPlaced(address _from, uint256 _blockNumber, uint256 _amount);

    fallback() external {
        revert("Jolt Fallback!!");
    }

    using Strings for uint256;

    struct Wager {
        uint256 blockNumber;
        uint256 numerator;
        uint256 amount;
    }

    struct WagerSet {
        Wager[] wagers;
        uint32 numWagers;
        bool initialized;
    }

    /**
     * Multiplier = Numerator/100
     */
    uint256 public MIN_NUMERATOR = 200;
    uint256 public MAX_NUMERATOR = 10000;
    uint256 public DENOMINATOR = 100;

    mapping(address => WagerSet) private wagerMap;

    WagerDAORngProxyInterface internal immutable RNG_PROXY;

    constructor(address _rngProxy) Ownable() {
        RNG_PROXY = WagerDAORngProxyInterface(_rngProxy);
    }

    function calculateNumerator(uint256 _word) public pure returns (uint256) {
        bytes32 b32 = bytes32(_word);
        uint256 bs0 = uint8(b32[0]);
        uint256 bs1 = uint8(b32[1]);
        uint256 bs2 = uint8(b32[2]);
        uint256 bs3 = uint8(b32[3]);
        uint256 b0 = uint256(bs0) * (10**32);
        uint256 b1 = uint256(bs1) * (10**32);
        uint256 b2 = uint256(bs2) * (10**32);
        uint256 b3 = uint256(bs3) * (10**32);
        uint256 added = ((b0 / (256**1)) +
            (b1 / (256**2)) +
            (b2 / (256**3)) +
            (b3 / (256**4)));
        uint256 mul = added * 100000000;
        uint256 div = (mul + 10**32);
        uint256 top = 100000000 * 10**35;
        uint256 op1 = top / div;
        uint256 f = (op1 * 99) / 1000;
        return f;
    }

    function placeWager(uint256 _multiplier) public payable {
        require(_multiplier >= MIN_NUMERATOR, "Multiplier below minimum.");
        require(_multiplier <= MAX_NUMERATOR, "Multiplier above maximum.");
        if (wagerMap[msg.sender].initialized) {
            if (
                wagerMap[msg.sender].numWagers ==
                wagerMap[msg.sender].wagers.length
            ) {
                wagerMap[msg.sender].wagers.push(
                    Wager(block.number, _multiplier, msg.value)
                );
            } else {
                wagerMap[msg.sender].wagers[wagerMap[msg.sender].numWagers] = (
                    Wager(block.number, _multiplier, msg.value)
                );
            }
        } else {
            wagerMap[msg.sender].wagers.push(
                Wager(block.number, _multiplier, msg.value)
            );
            wagerMap[msg.sender].initialized = true;
        }
        wagerMap[msg.sender].numWagers++;
        WagerDAORngProxyInterface.BlockEntry memory blockEntry = RNG_PROXY
            .blockEntryMap(block.number);
        if (blockEntry.requestId == 0) {
            generateNumeratorForBlock(block.number);
        }
        emit wagerPlaced(msg.sender, block.number, msg.value);
    }

    function claim() public {
        for (uint256 i = 0; i < wagerMap[msg.sender].numWagers; i++) {
            require(
                wagerMap[msg.sender].wagers[i].blockNumber < block.number,
                "Cannot claim unsettled wagers!"
            );
            if (
                getNumeratorForBlock(
                    wagerMap[msg.sender].wagers[i].blockNumber
                ) >= wagerMap[msg.sender].wagers[i].numerator
            ) {
                address payable sender = payable(msg.sender);
                sender.transfer(
                    (wagerMap[msg.sender].wagers[i].numerator *
                        wagerMap[msg.sender].wagers[i].amount) / DENOMINATOR
                );
            }
            delete wagerMap[msg.sender].wagers[i];
        }
        // TODO: Implement reentrancy guard.
        wagerMap[msg.sender].numWagers = 0;
    }

    function generateNumeratorForBlock(uint256 _blockNumber) public {
        require(
            RNG_PROXY.blockEntryMap(_blockNumber).requestId == 0,
            "Generation request already sent!"
        );
        RNG_PROXY.requestWordForBlock(_blockNumber);
    }

    function getNumeratorForBlock(uint256 _blockNumber)
        public
        view
        returns (uint256)
    {
        require(
            RNG_PROXY.getWordForBlock(_blockNumber) != 0,
            "RNG not generated for block."
        );
        return calculateNumerator(RNG_PROXY.getWordForBlock(_blockNumber));
    }

    function getWagers(address _wallet) public view returns (Wager[] memory) {
        Wager[] memory wagers = new Wager[](wagerMap[_wallet].numWagers);
        for (uint256 i = 0; i < wagerMap[_wallet].numWagers; i++) {
            Wager memory wager = wagerMap[_wallet].wagers[i];
            wagers[i] = wager;
        }
        return wagers;
    }

    function fundEth() public payable {}

    function withdraw(uint256 _amount) public {
        require(msg.sender == owner(), "Must be owner");
        payable(msg.sender).transfer(_amount);
    }
}

