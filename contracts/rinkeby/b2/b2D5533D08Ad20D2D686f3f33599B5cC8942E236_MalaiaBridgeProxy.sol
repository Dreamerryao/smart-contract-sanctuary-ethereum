pragma solidity ^0.8.0;

import "../contracts/Consortium.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract MalaiaBridgeStorage is Consortium, Pausable {
	event TokenBridgeModified(string indexed _token, address indexed _mainchainContract, address indexed _sidechainContract);
    event ValidatorModified(address indexed _validator, bool indexed _status);
    event TokenDeposited(uint256 indexed _depositId, address indexed _owner, string indexed _token, uint256 _amount);
    event TokenWithdrawn(uint256 indexed _withdrawalId, address indexed _owner, string indexed _token, uint256 _amount);

    struct Deposit {
        address owner;
        address tokenContract;
        address sidechainAddress;
        uint256 amount;
    }

    struct Withdrawal {
        address owner;
        string token;
        uint256 tokenNumber;
    }

	struct TokenLink {
        address mainchainContract;
        address sidechainContract;
    }

    Deposit[] public deposits;
    mapping(uint256 => Withdrawal) public withdrawals;
	mapping(string => TokenLink) public tokenLinks;
    address _mainContract;

    /**
    *** Bridge link functions
    **/

	function linkToken(string calldata _token, address _mainchainContract, address _sidechainContract) external onlyAdmin {
        tokenLinks[_token] = TokenLink(_mainchainContract, _sidechainContract);

        emit TokenBridgeModified(_token, _mainchainContract, _sidechainContract);
    }

    function unlinkToken(string calldata _token) external onlyAdmin {
        delete tokenLinks[_token];

		emit TokenBridgeModified(_token, address(0), address(0));
    }

    function getTokenLink(string calldata _token) public view returns (address _mainchainContract, address _sidechainContract) {
        _mainchainContract = tokenLinks[_token].mainchainContract;
        _sidechainContract = tokenLinks[_token].sidechainContract;
    }

    modifier onlyLinked(string memory _token) {
        require(tokenLinks[_token].mainchainContract != address(0) && tokenLinks[_token].sidechainContract != address(0), "MalaiaBridge: Token does not have a link to the malaia chain");
        _;
    }

    modifier onlyNewWithdrawal(uint256 _withdrawalId) {
        require(withdrawals[_withdrawalId].owner == address(0), "MalaiaBridge: This withdrawal has already been executed");
        _;
    }
}

