pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import "./MedievalNFT.sol";
import "./access/MedievalAccessControlled.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "./interface/IDistributor.sol";

contract NFTStakingPool is Initializable, ReentrancyGuard, MedievalAccessControlled, ERC721Upgradeable{
    using EnumerableSet for EnumerableSet.AddressSet;
    using Address for address;
    
    address public distributor;
    MedievalNFT public medivalNFT;

    uint256 public totalWeight;
    // account => weight
    mapping(address => uint256) weight;

    // tokenId => weight
    mapping(uint256 => uint256) nftWeight; // cache the weight;

    mapping(address=>uint256) index;
    mapping(address=>mapping(address=>uint256)) claimed;

    EnumerableSet.AddressSet rewardTokens;

    uint256 public occupation;
    uint256 constant INDEX_UNIT = 10**18;

    string constant NOT_IMPLEMENTED_MESSAGE = "Not implemented!";

    function initialize(
        address _controlCenter,
        address _medivalNFT,
        uint256 _occupation,
        address[] calldata _rewards
    ) public initializer {
        __ERC721_init("Staked MedivalNFT", "sMNFT"); // TODO

        _setControlCenter(_controlCenter, tx.origin);
        medivalNFT = MedievalNFT(_medivalNFT);
        occupation = _occupation;

        for(uint256 i=0;i<_rewards.length; i++){
            rewardTokens.add(_rewards[i]);
        }
    }

    function _transfer(address from, address to, uint256 tokenId) internal pure override{
        revert(NOT_IMPLEMENTED_MESSAGE);
    }

    function transferFrom(address from, address to, uint256 tokenId) public pure override {
        revert(NOT_IMPLEMENTED_MESSAGE);
    }

    function approve(address to, uint256 tokenId) public pure override {
        revert(NOT_IMPLEMENTED_MESSAGE);
    }

    function setApprovalForAll(address operator, bool approved) public pure override {
        revert(NOT_IMPLEMENTED_MESSAGE);
    }

    function addReward(address reward) external onlyAdmin {
        rewardTokens.add(reward);
    }

    function removeReward(address reward) external onlyAdmin {
        rewardTokens.remove(reward);
    }

    function distributeReward(address reward, uint256 amount) external nonReentrant {
        if(amount > 0) {
            require(rewardTokens.contains(reward), "Not in the rewardList");
            uint256 beforeAmount = IERC20(reward).balanceOf(address(this));
            IERC20(reward).transferFrom(msg.sender, address(this), amount);
            uint256 afterAmount = IERC20(reward).balanceOf(address(this));
            index[reward] += (afterAmount - beforeAmount) * INDEX_UNIT / totalWeight;
        }
    }

    function pendingReward(address reward, address to) public view returns(uint256) {
        uint256 amount =  weight[to] * index[reward] / INDEX_UNIT;
        amount -= claimed[reward][to];
        return amount;
    }

    function _claimFor(address reward, address to) internal {
        require(rewardTokens.contains(reward), "Not in the rewardList");
        uint256 amount = pendingReward(reward, to);
        IERC20(reward).transfer(to, amount);
        claimed[reward][to] += amount;
    }

    function claim(address reward) external nonReentrant {
        _claimFor(reward, msg.sender);
    }

    function _claimAll(address to) internal{
        for(uint256 i=0; i < rewardTokens.length(); i++) {
            _claimFor(rewardTokens.at(i), to);
        }
    }

    function claimAll() external nonReentrant {
        _claimAll(msg.sender);
    }

    function stake(uint256 tokenId) external nonReentrant {
        // Distribute the reward before staking.
        _triggerRewardDistribution();

        require(
            (medivalNFT.tokenOccupation(tokenId) & occupation) > 0,
            "Wrong Occupation!"
        );

        medivalNFT.transferFrom(msg.sender, address(this), tokenId);
        _mint(msg.sender, tokenId);

        uint256 _weight = medivalNFT.weight(tokenId);
        weight[msg.sender] += _weight;
        nftWeight[tokenId] = _weight;
        totalWeight += _weight;

        for(uint256 i=0; i < rewardTokens.length(); i++) {
            address reward = rewardTokens.at(i);
            claimed[reward][msg.sender] += _weight * index[reward] / INDEX_UNIT;
            //TODO make sure staking NFT will not change the pending reward.
        }
    }

    function unstake(uint256 tokenId) external nonReentrant {
        _triggerRewardDistribution();
        _claimAll(msg.sender);

        require(ownerOf(tokenId) == msg.sender, "msg.sender is not NFT Owner");
        _burn(tokenId);

        uint256 _weight = nftWeight[tokenId];
        
        weight[msg.sender] -= _weight;
        totalWeight -= _weight;
        delete nftWeight[tokenId];

        medivalNFT.transferFrom(address(this), msg.sender, tokenId);
    }

    function _triggerRewardDistribution() internal {
        if(address(distributor) != address(0)) {
            IDistributor(distributor).distributeReward();
        }
    }

    function setDistributor(address _distributor) external onlyAdmin {
        distributor = _distributor;
    }

    // View functions

    function rewardTokenList() view public returns (address []  memory){
        address[] memory tokenList = new address[](rewardTokens.length());
        for(uint256 i=0; i < rewardTokens.length(); i++) {
            tokenList[i] = rewardTokens.at(i);
        }
        return tokenList;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

}

