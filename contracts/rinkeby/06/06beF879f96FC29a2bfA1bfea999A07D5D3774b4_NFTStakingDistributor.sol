pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "./chainlink/VRFConsumerBaseUpgradable.sol";
import "./access/MedievalAccessControlled.sol";
import "./NFTStakingPool.sol";
import "./interface/IDistributor.sol";
import "./interface/IStaking.sol";


contract NFTStakingDistributor is Initializable, IDistributor, MedievalAccessControlled {
    using EnumerableSet for EnumerableSet.UintSet;

    bytes32 public COORDINATOR_ROLE = keccak256("COORDINATOR_ROLE");

    IStaking public staking; // (3,3) staking
    IERC20 public food;

    Farm public farm;
    uint256 farmID;

    struct Farm {
        NFTStakingPool farmerPool;
        NFTStakingPool thiefPool;
        NFTStakingPool guardPool;
        uint256 guardShareRatio;
        uint256 minStealingRate;
    }

    uint256 constant BPS_UNIT = 10**4;

    function initialize(
        address _controlCenter,
        address _staking,
        address _food,
        uint256 _farmID
    ) external initializer {
        _setControlCenter(_controlCenter, tx.origin);
        staking = IStaking(_staking);
        food = IERC20(_food);
        farmID = _farmID;
    }

    function setStaking(address _staking) external onlyAdmin {
        staking = IStaking(_staking);
    }

    function setFarm(address _farmerPool, 
                     address _thiefPool, 
                     address _guardPool,
                     uint256 _guardShareRatio,
                     uint256 _minStealingRate
                    ) external onlyAdmin {
        
        farm.farmerPool = NFTStakingPool(_farmerPool);
        farm.thiefPool = NFTStakingPool(_thiefPool);
        farm.guardPool = NFTStakingPool(_guardPool);
        farm.guardShareRatio = _guardShareRatio;
        farm.minStealingRate = _minStealingRate;

        // infinite approve rewardPools;
        food.approve(_farmerPool, type(uint256).max);
        food.approve(_thiefPool, type(uint256).max);
        food.approve(_guardPool, type(uint256).max);
    }

    function setGuardShareRatio(uint256 _guardShareRatio) 
        external onlyRole(COORDINATOR_ROLE) {
        farm.guardShareRatio = _guardShareRatio;
    }


    function distributeReward() public override {
        if(address(staking) != address(0)){
            staking.rebase();
        }

        uint256 totalFood = food.balanceOf(address(this));
        if(totalFood > 0) {
            ( uint256 length, uint256 number, uint256 endBlock, uint256 distribute ) = staking.epoch();
            // TODO verify
            uint256 lastEpochBlock = endBlock - length;
                         
            uint256 famerAmount = 0;
            uint256 thiefAmount = 0;
            uint256 guardAmount = 0;
            uint256 seed = _random(
                uint256(blockhash(lastEpochBlock)),
                farmID);

            if(stealingHappened(seed, 
                    farm.farmerPool.totalWeight(), 
                    farm.thiefPool.totalWeight(), 
                    farm.guardPool.totalWeight(),
                    farm.minStealingRate
                    )
            ){
                thiefAmount = totalFood;
            } else {
                guardAmount = totalFood * farm.guardShareRatio / BPS_UNIT;
                famerAmount = totalFood - guardAmount;
            }

            farm.farmerPool.distributeReward(address(food), famerAmount);
            farm.thiefPool.distributeReward(address(food), thiefAmount);
            farm.guardPool.distributeReward(address(food), guardAmount);
        }
    }


    function _random(uint256 seed, uint256 offset) pure internal returns(uint256){
        return uint256(keccak256(abi.encode(seed, offset)));
    }


    function stealingHappened(uint256 seed, uint256 weightFarmer, uint256 weightThief, uint256 weightGuard, uint256 minStealingRate) public pure returns (bool){
        if(weightThief == 0) {
            return false;
        }
        
        if(seed % BPS_UNIT < minStealingRate) {
            return true;
        }  

        seed % (2 * weightThief + 10 * weightGuard) < weightThief;
        // without guard the stealing rate is capped at 50%
    }
}

