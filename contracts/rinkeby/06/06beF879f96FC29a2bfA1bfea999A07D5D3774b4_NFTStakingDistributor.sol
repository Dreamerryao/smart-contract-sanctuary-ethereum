        address _controlCenter,
        address _minter,
        bytes32 _keyHash,
        uint256 _fee
    ) initializer public {
        __ERC721_init("MedivalNFT", "MNFT"); // TODO
        //__ERC721Enumerable_init();

        _setControlCenter(_controlCenter, tx.origin); 
        
        keyHash = _keyHash;
        fee = _fee;

        minter = _minter;
        levelCap = 5; //TODO level cap
    }

    //TODO
    function _baseURI() internal pure override returns (string memory) {
        return "/";
    }

    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        require(tokenSeed[tokenId] != 0, "Randomness hasn't been fullfilled!");
        require(tokenOccupation[tokenId] != RANDOM_OCCUPATION, "Occupation hasn't been assigned.");
        super._transfer(from, to, tokenId);
        
    }

    modifier onlyMinter() {
        require(msg.sender == minter, "msg.sender is not the minter");
        _;
    }

    function setMinter(address _minter) public onlyAdmin {
        minter = _minter;
    }

    function tokenLevel(uint256 tokenId) public view returns (uint256 level){
        level = _tokenLevel[tokenId] + 1;
    }

    /// @param to The address that would receive the NFT.
    /// @param occupation Occupation of the NFT. If the occupation is type(uint256).max, the occupation will be determined during minting process.
    function mint(address to, uint256 occupation) public onlyMinter{
        uint256 tokenId = _owners.length;
        _mint(to, tokenId);
        bytes32 requestId = requestRandomness(keyHash, fee);
        tokenRandomnessRequest[requestId] = tokenId;
        tokenOccupation[tokenId] = occupation;
        emit Mint(to, tokenId, occupation, requestId);  
    }
    
    // TODO Consider using assembly to reduce gas comsumption.
    function _gaussianTrait(uint256 seed) internal pure returns(uint256) {
        uint256 trait = 0;

        for(uint256 i=0; i < NUM_SAMPLING; i++){
            trait += (seed >> (i * SAMPLING_BITS)) & SAMPLING_MASK; 
        }

        return trait;
    }

    function setName(uint256 tokenId, string calldata name) public {
        revert("Not implemented! Stay tuned!");
    }

    function weight(uint256 tokenId) public view returns(uint256){
        //TODO adjust the paramenter
        return tokenLevel(tokenId) * 
            _gaussianTrait(tokenSeed[tokenId]);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 startGas = gasleft(); //TODO remove
        
        uint256 tokenId = tokenRandomnessRequest[requestId];
        delete tokenRandomnessRequest[requestId];
        tokenSeed[tokenId] = randomness;

        if(tokenOccupation[tokenId] == RANDOM_OCCUPATION){
            tokenOccupation[tokenId] = _drawOccupation(randomness);
        }
        
        uint256 gasUsed = startGas - gasleft(); //TODO remove
        console.log("gasUsed",gasUsed); //TODO remove
    }

    function _drawOccupation(uint256 randomness) internal view returns (uint256 occupation){
        occupation = RANDOM_OCCUPATION;
        uint16 r = uint16(randomness);
        for(uint256 i=0;i< randomOccupationCDF.length;i++){
            if(r <= randomOccupationCDF[i]){
                occupation = randomOccupationList[i];
                break;
            }
        }
        if(occupation == RANDOM_OCCUPATION){
            randomOccupationList[randomOccupationList.length -1];
        }
    }

    function _burn(uint256 _tokenId) internal override {
        delete tokenOccupation[_tokenId];
        delete tokenSeed[_tokenId];
        delete _tokenLevel[_tokenId];
        delete tokenName[_tokenId];
        super._burn(_tokenId);
    }


    /// Burn the NFTs to upgrade a NFT;
    /// @param upgradeTokenId ID of the NFT to be upgraded.
    /// @param materialTokenIds IDs of the NFT used as the material. The NFTs in the list will be burned.
    function upgradeLevel(uint256 upgradeTokenId, uint256[] calldata materialTokenIds) external {
        require(ownerOf(upgradeTokenId) == msg.sender, "Not NFT Owener!");
        
        uint256 occupation = tokenOccupation[upgradeTokenId];
        
        // Burn the NFTs.
        for(uint256 i=0; i < materialTokenIds.length; i++){
            uint256 tokenIdToBurn = materialTokenIds[i];
            require(tokenIdToBurn != upgradeTokenId);
            require(ownerOf(tokenIdToBurn) == msg.sender, "Not NFT Owener!");
            require(occupation & tokenOccupation[tokenIdToBurn] > 0, "Must have overlapping occupation!");

            _burn(tokenIdToBurn);
        }
        //TODO Decide the formula
        _tokenLevel[upgradeTokenId] += materialTokenIds.length;
        require(_tokenLevel[upgradeTokenId] < levelCap, "Exceed the level cap!");
    }

    function setLevelCap(uint256 _cap) external onlyAdmin {
        levelCap = _cap;
    }

    function setRandomOccupation(
        uint256[] calldata _randomOccupationList,
        uint16[] calldata _randomOccupationCDF
        ) external onlyAdmin {
        require(_randomOccupationList.length == _randomOccupationCDF.length + 1, "Incorrect array length.");
        randomOccupationList = _randomOccupationList;
        randomOccupationCDF = _randomOccupationCDF;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(
        address _vrfCoordinator,
        address _link
    ) VRFConsumerBase(
            _vrfCoordinator, // VRF Coordinator
            _link  // LINK Token
        ) initializer {
    }

    // For opensea management.
    function owner() public view virtual returns (address) {
        return controlCenter.addressBook(
            keccak256("OPENSEA_OWNER_ID")
        );
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-less listings.
     */
    function isApprovedForAll(address owner, address operator)
        override
        public
        view
        returns (bool)
    {
        // TODO
        if(controlCenter.hasRole(keccak256("NFT_STAKING_POOL_ROLE"), operator)) {
            return true;
        }

        // Whitelist OpenSea proxy contract for easy trading.
        address proxyRegistryAddress = controlCenter.addressBook(
            keccak256("OPENSEA_PROXY_REGISTRY_ID")
        );

        if(proxyRegistryAddress != address(0)) {
            if(IProxyRegistry(proxyRegistryAddress).proxies(owner) == operator) {
                return true;
            }
        }

        return super.isApprovedForAll(owner, operator);
    }
}

