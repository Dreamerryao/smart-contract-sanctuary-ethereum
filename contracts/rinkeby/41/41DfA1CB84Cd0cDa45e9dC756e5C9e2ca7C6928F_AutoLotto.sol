pragma solidity >=0.7.0 <0.9.0;

import "hardhat/console.sol";

import "./AutoLottoHelper.sol";

contract AutoLotto is AutoLottoHelper {
    bool private canBuyTickets;
    uint256 private numberOfTicketsPurchased;

    uint256 private difficultyLevel;
    // uint256 private winningNumber;
    uint8[] winningNumbers;
    uint256 totalJackpot;

    uint8[5] winningDistributionPercentages;
    address[][4] winners; //dynamic index first https://ethereum.stackexchange.com/questions/11870/create-a-two-dimensional-array-in-solidity
    enum DISTRIBUTION_TYPE {
        THREE_NUMBER_WINNER,
        FOUR_NUMBER_WINNER,
        FIVE_NUMBER_WINNER,
        SIX_NUMBER_WINNER,
        ADMIN_COSTS
    }

    address[] private players;
    mapping(address => uint256[]) ticketsByAddress;

    uint256 private ticketPrice;

    event TicketPurchased(address buyer, uint256 numberPick);

    event NumberOfTicketsPurchasedForDrawing(uint256 _numberOfTicketsPurchased);

    event DrawingWinners(address[] _drawingWinners, uint8[] _winningNumbers);

    event WinningNumbers(uint8[] _winningNumbers);

    function allowTicketPurchase(bool _buyFlag) public onlyOwner {
        canBuyTickets = _buyFlag;
        console.log("can buy tickets is now: ", canBuyTickets );
    }

    function setLottoParams(
        uint256 _difficultyLevel,
        uint8[5] calldata _winningDistributionPercentages
    ) public onlyOwner {
        uint256 totalPercent = 0;
        for (uint256 i=0; i<5; i++) {
            totalPercent += _winningDistributionPercentages[i];
        } 
        // console.log("Total percent is", totalPercent);
        // console.log("Difficulty level", _difficultyLevel);
        // console.log("6 number %", _winningDistributionPercentages[3]);
        require(_difficultyLevel == 5 || _difficultyLevel == 6, "Difficulty level must be 5 or 6");
        require(totalPercent == 100, "Distribution percentages must add up to 100");
        require(_difficultyLevel == 5 || _winningDistributionPercentages[3] > 0, "6 number winnings % not set, but difficulty is 6");
        difficultyLevel = _difficultyLevel;
        winningDistributionPercentages = _winningDistributionPercentages;
        console.log("lotto params updated");
    }

    //pulled out into a separate function so that it can be easily overridden for unit testing
    //though maybe this isn't necessary at all after refactor
    // function setWinningNumbers() internal virtual {
    //     winningNumbers = getVRFRandomPick(difficultyLevel);
    // }

     function setWinningNumbers() public onlyOwner {
        winningNumbers = getVRFRandomPick(difficultyLevel);
    }



    function findWinners() public onlyOwner {
        //console.log("solidity contract balance", address(this).balance);
        //console.log("AutoLotto address: ", address(this));
        console.log("Number of tickets purchased: ", numberOfTicketsPurchased);
        require(numberOfTicketsPurchased > 0, "No tickets were purchased for this drawing");

        //cleanup from previous drawing
        delete winners;

        emit NumberOfTicketsPurchasedForDrawing(numberOfTicketsPurchased);

        setWinningNumbers();

        // for(uint256 y = 0; y < winningNumbers.length; y++)
        // {
        //     console.log("localWiningNumber: " , winningNumbers[y]);
        // }

        emit WinningNumbers(winningNumbers);

        uint256[] memory tickets;
        uint256[] memory currentTicket;
        uint256 numbersMatched;

        for (uint256 i = 0; i < players.length; i++) {
            //all players loop
            tickets = ticketsByAddress[players[i]];
            for (uint256 j = 0; j < tickets.length; j++) {
                //unpack int into individual picks
                currentTicket = unpackIntInto2DigitNums(
                    tickets[j],
                    difficultyLevel
                );
                //how many matched?
                numbersMatched = 0;
                for (uint256 k = 0; k < currentTicket.length; k++) {
                    //current pick
                    for (uint256 x = 0; x < winningNumbers.length; x++) {
                        //winning number
                        if (currentTicket[k] == winningNumbers[x]) {
                            numbersMatched++;
                        }
                    }
                }
                if (numbersMatched == 3) {
                    winners[uint256(DISTRIBUTION_TYPE.THREE_NUMBER_WINNER)].push(
                        players[i]
                    );
                } else if (numbersMatched == 4) {
                    winners[uint256(DISTRIBUTION_TYPE.FOUR_NUMBER_WINNER)].push(
                        players[i]
                    );
                } else if (numbersMatched == 5) {
                    winners[uint256(DISTRIBUTION_TYPE.FIVE_NUMBER_WINNER)].push(
                        players[i]
                    );
                } else if (numbersMatched == 6) {
                    winners[uint256(DISTRIBUTION_TYPE.SIX_NUMBER_WINNER)].push(
                        players[i]
                    );
                }
            }
        }

        // console.log("3 number winners: ", winners[uint256(DISTRIBUTION_TYPE.THREE_NUMBER_WINNER)].length );
        // console.log("4 number winners: ", winners[uint256(DISTRIBUTION_TYPE.FOUR_NUMBER_WINNER)].length );
        // console.log("5 number winners: ", winners[uint256(DISTRIBUTION_TYPE.FIVE_NUMBER_WINNER)].length );
        // console.log("6 number winners: ", winners[uint256(DISTRIBUTION_TYPE.SIX_NUMBER_WINNER)].length );
        //viewWinningNumbersAndWinners(localWinningNumbers);  //test only
        payWinners();

        //console.log("solidity contract balance POST payout", address(this).balance);

        cleanUp();
    }

    function payWinners() public {
       // console.log("contract balance: ",  address(this).balance );
        totalJackpot =
            address(this).balance -
            ((address(this).balance *
                winningDistributionPercentages[
                    uint256(DISTRIBUTION_TYPE.ADMIN_COSTS)
                ]) / 100);

               // console.log("totalJackpot: ", totalJackpot);
        payWinnerCohort(DISTRIBUTION_TYPE.THREE_NUMBER_WINNER);
        payWinnerCohort(DISTRIBUTION_TYPE.FOUR_NUMBER_WINNER);
        payWinnerCohort(DISTRIBUTION_TYPE.FIVE_NUMBER_WINNER);
        if (difficultyLevel == 6) {
           // console.log("paying 6-number winner");
            payWinnerCohort(DISTRIBUTION_TYPE.SIX_NUMBER_WINNER);
        }
    }

    function payWinnerCohort(DISTRIBUTION_TYPE _distributionType) private {
        address[] memory winningCohort = winners[uint256(_distributionType)];
        if (winningCohort.length > 0) {
            emit DrawingWinners(winningCohort, winningNumbers);
            uint256 jackpot = ((totalJackpot *
                winningDistributionPercentages[uint256(_distributionType)]) /
                100) / winningCohort.length;
            for (uint256 i = 0; i < winningCohort.length; i++) {
                payable(winningCohort[i]).transfer(jackpot);
              //  console.log("Payable jackpot: ", jackpot);
              //  console.log("Remaining contract balance: ", address(this).balance);
            }
        }
    }

    function setTicketPrice(uint256 _newPrice) public onlyOwner {
        ticketPrice = _newPrice;
        console.log("Changed ticket price to this ether value:", ticketPrice);
    }

    function purchaseManualTicket(uint256[] memory numberPick)
        external
        payable
    {
        // console.log("solidity contract balance PRE purchase", address(this).balance);
        // console.log("Message value: ",msg.value);
        // console.log("ticket price: ", ticketPrice);
        // console.log("pick length: ", numberPick.length);
        // console.log("ticket cost: ", ticketPrice * numberPick.length);
        require(canBuyTickets, "Ticket purchase functionality is turned off");
        require(msg.value == ticketPrice * numberPick.length, "Not enough ether to purchase ticket(s)");
        
        if (ticketsByAddress[msg.sender].length == 0) {
            players.push(msg.sender);
        }
        for (uint256 i = 0; i < numberPick.length; i++) {
            ticketsByAddress[msg.sender].push(numberPick[i]);
            numberOfTicketsPurchased++;
           // console.log("Purchased manual ticket: ", numberPick[i]);
        }
        emit TicketPurchased(msg.sender, numberPick.length);
        //console.log("solidity contract balance POST purchase", address(this).balance);
    }

    function purchaseRandomTicket(uint256 numberOfPicks) external payable {
        require(canBuyTickets, "Ticket purchase functionality is turned off");
        require(msg.value == ticketPrice * numberOfPicks, "Not enough ether to purchase ticket(s)");
        uint256[] memory finalRandomPicks = new uint256[](numberOfPicks);
        finalRandomPicks = getRandomPick(numberOfPicks, difficultyLevel);
        if (ticketsByAddress[msg.sender].length == 0) {
            players.push(msg.sender); 
        }
        for (uint256 i = 0; i < finalRandomPicks.length; i++) {
            ticketsByAddress[msg.sender].push(finalRandomPicks[i]);
            numberOfTicketsPurchased++;
            //console.log("Purchased random ticket: ", finalRandomPicks[i]);
        }
        emit TicketPurchased(msg.sender, numberOfPicks);
    }

    function cleanUp() private {
        numberOfTicketsPurchased = 0;

        for (uint256 i = 0; i < players.length; i++) {
            delete ticketsByAddress[players[i]];
        }

        delete players;
    }

    //   function viewWinningNumbersAndWinners (uint256[] memory WinningNumbers) public view {    //test only
    //    for (uint256 i = 0; i < WinningNumbers.length; i++)
    //     console.log("winning number: ", WinningNumbers[i]);

    //     for(uint256 j = 0; j < threeNumberWinners.length; j++){
    //         console.log("three number winner: ", threeNumberWinners[j]);
    //     }

    //      for(uint256 k = 0; k < fourNumberWinners.length; k++){
    //         console.log("four number winner: ", fourNumberWinners[k]);
    //     }

    //      for(uint256 n = 0; n < fiveNumberWinners.length; n++){
    //         console.log("five number winner: ", fiveNumberWinners[n]);
    //     }

    //      for(uint256 m = 0; m < sixNumberWinners.length; m++){
    //         console.log("six number winner: ", sixNumberWinners[m]);
    //     }
    //  }
}

