pragma solidity >=0.7.0 <0.9.0;

import "hardhat/console.sol";
import "./ownable.sol";
import "./RandomNumberConsumer.sol";

contract AutoLottoHelper is Ownable, RandomNumberConsumer {
    uint256 randNonce = 0;
    address[] uniqueWinners;

    // modifier validNumber(uint256 numberPick) {
    //     require(numberPick >= 123456 && numberPick <= 444546474849);
    //     _;
    // }

    function get2digitVRFnumber(uint256 VRFResult)
        public
        pure
        returns (uint256)
    {
        uint256 currentNumber;

        if (VRFResult < 100) {
            currentNumber = VRFResult;
        } else {
            currentNumber = VRFResult % 100;
        }

        if (currentNumber > 49) {
            currentNumber = currentNumber % 50;
        }
        if (currentNumber == 0) {
            currentNumber += 1;
        }

        return currentNumber;
    }

    function getVRFRandomPick(uint256 difficultyLevel)
        internal
        returns (uint8[] memory)
    {
        getVRFRandomNumber();
        uint8[] memory randomVRFPick = new uint8[](difficultyLevel);
        bool isDuplicate = true;
        uint256 currentNumber;
        uint256 VRFResult = randomVRFResult;

        //VRFResult = 3671752977260297; //test only
        require(VRFResult > 0, "VRF result is equal to 0");

        for (uint256 i = 0; i < randomVRFPick.length; i++) {
            currentNumber = get2digitVRFnumber(VRFResult);
            VRFResult = (VRFResult - (VRFResult % 100)) / 100;
            while (isDuplicate) {
                for (uint256 j = 0; j < randomVRFPick.length; j++) {
                    if (randomVRFPick[j] != currentNumber) {
                        isDuplicate = false;
                    } else {
                        isDuplicate = true;
                        if (VRFResult < 100) {
                            currentNumber = get2digitVRFnumber(VRFResult);
                        } else {
                            VRFResult = (VRFResult - (VRFResult % 100)) / 100;
                            currentNumber = get2digitVRFnumber(VRFResult);
                        }
                    }
                }
            }
            isDuplicate = true;
            randomVRFPick[i] = uint8(currentNumber);
        }
        return sortArray(randomVRFPick);
    }

    function getUniqueArray(address[] memory winners)
        internal
        returns (address[] memory)
    {
        bool hasDupes = false;
        for (uint256 i = 0; i < winners.length; i++) {
            if (i == 0) {
                uniqueWinners.push(winners[i]);
            } else {
                for (uint256 j = 0; j < uniqueWinners.length; j++) {
                    if (winners[i] == uniqueWinners[j]) {
                        hasDupes = true;
                        break;
                    }
                }
                if (!hasDupes) {
                    uniqueWinners.push(winners[i]);
                } else {
                    hasDupes = false;
                }
            }
        }
        return uniqueWinners;
    }

    function getRandomPick(uint256 NumberOfPicks, uint256 difficultyLevel)
        public
        returns (uint256[] memory)
    {
        uint256[] memory finalRandomPicks = new uint256[](NumberOfPicks);
        uint8[] memory randomPick = new uint8[](difficultyLevel);
        bool isDuplicate = true;
        uint256 currentNumber;

        for (uint256 k = 0; k < finalRandomPicks.length; k++) {
            for (uint256 i = 0; i < randomPick.length; i++) {
                currentNumber = getRandomNumber();
                while (isDuplicate) {
                    for (uint256 j = 0; j < randomPick.length; j++) {
                        if (randomPick[j] != currentNumber) {
                            isDuplicate = false;
                        } else {
                            isDuplicate = true;
                            currentNumber = getRandomNumber();
                        }
                    }
                }
                isDuplicate = true;
                randomPick[i] = uint8(currentNumber);
            }

            //randomPick = sortArray(randomPick);

            //finalRandomPicks[k] = convertArraytouint256(randomPick);
            finalRandomPicks[k] = pack2DigitNumsIntoInt(randomPick);
        }

        return finalRandomPicks;
    }

    // function convertArraytouint256(uint256[] memory randomPick) internal pure returns(uint256) {
    //     uint256 exponent = 6;
    //     uint256 randomPickNumber;
    //     for(uint256 i = 0; i < randomPick.length; i++){
    //         randomPickNumber = randomPickNumber + randomPick[i] * 100 ** exponent;
    //         exponent--;
    //     }
    //     randomPickNumber = randomPickNumber / 100;
    //     return randomPickNumber;
    // }

    function sortArray(uint8[] memory randomPick)
        internal
        pure
        returns (uint8[] memory)
    {
        for (uint256 i = 0; i < randomPick.length - 1; i++) {
            for (uint256 j = 0; j < randomPick.length - 1; j++) {
                if (randomPick[j] > randomPick[j + 1]) {
                    uint256 currentValue = randomPick[j];
                    randomPick[j] = randomPick[j + 1];
                    randomPick[j + 1] = uint8(currentValue);
                }
            }
        }
        return randomPick;
    }

    function getRandomNumber() internal returns (uint256) {
        uint256 randomnumber = uint256(
            keccak256(abi.encodePacked(block.timestamp, msg.sender, randNonce))
        ) % 50;
        if (randomnumber == 0) {
            randomnumber = randomnumber + 1;
        }
        randNonce++;
        return randomnumber;
    }

    function pack2DigitNumsIntoInt(uint8[] memory numbers)
        public
        pure
        returns (uint256)
    {
        uint256 packed = 0;
        numbers = sortArray(numbers);
        for (uint256 i = 0; i < numbers.length; i++) {
            // console.log("First Number: ", numbers[i]);
            packed += numbers[i] * (100**i);
            // console.log("Packed Number: ", packed);
        }
        return packed;
    }

    function unpackIntInto2DigitNums(uint256 packed, uint256 difficultyLevel)
        public
        pure
        returns (uint256[] memory)
    {
        uint256[] memory numbers = new uint256[](difficultyLevel);
        for (uint256 i = 0; i < difficultyLevel; i++) {
            if (packed < 100) {
                numbers[i] = packed;
            } else {
                numbers[i] = packed % 100;
                packed = (packed - numbers[i]) / 100;
            }
        }
        return numbers;
    }
}

