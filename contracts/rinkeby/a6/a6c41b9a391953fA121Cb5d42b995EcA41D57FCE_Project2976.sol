pragma solidity ^0.8.14;

import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract Project2976 is ERC721Enumerable, VRFConsumerBaseV2, Pausable, Ownable {
    /// @dev IPFS base URI holding actual metadata
    string private baseURI;
    /// @dev IPFS URI of the not revealed image
    string public notRevealedUri;
    /// @dev Cost per nft in pre sale
    uint256 public preSaleCost = 0.01 ether;
    ///@dev Cost per nft in public sale
    uint256 public pubSaleCost = 0.1 ether;
    /// @dev max supply of nfts in public sale
    uint256 public maxSupply = 1210;
    /// @dev max supply of nfts in pre sale
    uint256 public preSaleMaxSupply = 10;
    /// @dev max nft per address
    uint256 public nftPerAddressLimit = 5;
    /// @dev pre sale start date
    uint256 public preSaleStartDate = 1660242600;
    /// @dev pre sale end date
    uint256 public preSaleEndDate = 1660262400;
    /// @dev public sale start date
    uint256 public pubSaleDate = 1660264200;
    /// @dev Mapping for whitelisted addresses
    mapping(address => bool) private whitelistedAddresses;
    /// @dev Mapping for address minted balance
    mapping(address => uint256) public addressMintedBalance;
    /// @dev Maximum amount of tokens that can be minted in a single transaction
    uint256 public constant MAX_MINT_AMOUNT = 5;
    using Counters for Counters.Counter;
    /// @dev Token id counter for sequential token id generation
    Counters.Counter private tokenIdCounter;
    /// @dev Metadata index offset that will be used to set the offset for the metadata URI linked to token id
    uint256 public metadataIndexOffset;
    /// @dev Reserved number of tokens that could be minted by the owner of the contract
    uint256 public reservedCount = 50;
    VRFCoordinatorV2Interface private coordinator;
    LinkTokenInterface private linktoken;
    // Your subscription ID.
    uint64 private subscriptionId;
    // Rinkeby coordinator.
    address private constant VRF_COORDINATOR_ADDRESS = 0x6168499c0cFfCaCD319c818142124B7A15E857ab;
    // Rinkeby LINK token contract.
    address private constant LINK_ADDRESS = 0x01BE23585060835E02B77ef475b0Cc51aA1e0709;
    // The gas lane to use, which specifies the maximum gas price to bump to.
    bytes32 private keyHash = 0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;
    // Depends on the number of requested values that you want to be sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 private callBackGasLimit = 100000;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    // How many random words to request from the coordinator.
    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
    uint32 private constant NUMRAND = 1;
    /// @dev Request id generated by VRF Chainlink
    uint256 public vrfRequestId;
    /// @dev Fee denominator for royalty calculation
    uint256 private feeDenominator = 10000;
    /// @dev Fee numerator for royalty calculation
    uint256 public feeNumerator = 500;

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _initNotRevealedUri,
        uint64 subId
    ) ERC721(_name, _symbol) VRFConsumerBaseV2(VRF_COORDINATOR_ADDRESS) {
        notRevealedUri = _initNotRevealedUri;
        coordinator = VRFCoordinatorV2Interface(VRF_COORDINATOR_ADDRESS);
        linktoken = LinkTokenInterface(LINK_ADDRESS);
        subscriptionId = subId;
    }

    function _isOwner() private view {
        require(msg.sender == owner(), "Ownable: caller is not the owner");
    }

    /**
     * @dev Modifier to check if the caller is the owner
     */
    modifier allowOwner() {
        _isOwner();
        _;
    }

    function _notPaused() private view {
        require(!paused(), "Pausable: paused");
    }

    /**
     * @dev Modifier to check if the contract is paused
     */
    modifier notPaused() {
        _notPaused();
        _;
    }

    /**
     * @dev Sets the VRF Chainlink configuration parameters for the contract.
     * @param _subscriptionId The subscription ID.
     * @param _keyHash The key hash.
     * @param _callBackGasLimit The call back gas limit.
     */
    function setVRFConfig(uint64 _subscriptionId, bytes32 _keyHash, uint32 _callBackGasLimit) public allowOwner {
        subscriptionId = _subscriptionId;
        keyHash = _keyHash;
        callBackGasLimit = _callBackGasLimit;
    }

    /**
     * @dev Sets the contract pause status. Its an emergency stop mechanism that can be triggered by an authorized account.
     * Once paused all the transfer functionality would be blocked. Ex: mint, burn, transfer, etc.
     * Only the owner can call this function.
     * @param toPause new pause status
     */
    function setPause(bool toPause) external allowOwner {
        if (toPause) {
            _pause();
        } else {
            _unpause();
        }
    }

    /**
     * @dev Queries base uri of the meta data set by the owner
     */
    function _baseURI() internal view virtual override(ERC721) returns (string memory) {
        return baseURI;
    }

    /**
     * @dev Queries base uri of the meta data set by the owner.
     */
    function getBaseURI() external view returns (string memory) {
        return baseURI;
    }

    /**
     * @dev Queries for the token metadata based on token id
     * @param tokenId NFT token id
     */
    function tokenURI(uint256 tokenId) public view virtual override(ERC721) returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        if (metadataIndexOffset == 0) {
            return notRevealedUri;
        }
        uint256 rawIndex = tokenId + metadataIndexOffset;
        if (rawIndex > maxSupply) {
            rawIndex = rawIndex - maxSupply;
        }

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0 ? string(abi.encodePacked(currentBaseURI, Strings.toString(rawIndex), ".json")) : "";
    }

    /**
     * @dev Gets accumulated ETH balance of this contract
     */
    function getContractBalance() external view allowOwner returns (uint256) {
        return address(this).balance;
    }

    /**
     * @dev Sets token limit per address. Only the owner can call this function.
     * @param newSaleLimit new limit
     */
    function setAddressLimits(uint256 newSaleLimit) external allowOwner {
        nftPerAddressLimit = newSaleLimit;
    }

    /**
     * @dev Sets sale price. Only the owner can call this function.
     * @param newPreSaleCost new price
     * @param newPublicSaleCost new price
     */
    function setSalePrice(uint256 newPreSaleCost, uint256 newPublicSaleCost) external allowOwner {
        preSaleCost = newPreSaleCost;
        pubSaleCost = newPublicSaleCost;
    }

    /**
     * @dev Sets new base uri for metadata.
     * Make sure to call this function just before the metadata offset is revealed and after all the NFTs have been minted.
     * Only the owner can call this function.
     * @param newBaseURI new base uri
     */
    function setBaseURI(string memory newBaseURI) external allowOwner {
        baseURI = newBaseURI;
    }

    /**
     * @dev Sets new not revealed uri. Only the owner can call this function.
     * @param newNotRevealedURI new not revealed uri
     */
    function setNotRevealedURI(string memory newNotRevealedURI) external allowOwner {
        notRevealedUri = newNotRevealedURI;
    }

    /**
     * @dev Sets token max supply for sales. Only the owner can call this function.
     * @param newPreSaleMaxSupply new pre sale max supply
     * @param newPublicMaxSupply new public sale max supply
     */
    function setSaleMaxSupply(uint256 newPreSaleMaxSupply, uint256 newPublicMaxSupply) external allowOwner {
        preSaleMaxSupply = newPreSaleMaxSupply;
        maxSupply = newPublicMaxSupply;
    }

    /**
     * @dev Sets sale dates. Only the owner can call this function.
     * @param newPreSaleStartDate new pre sale start date
     * @param newPreSaleEndDate new pre sale end date
     * @param newPublicSaleDate new public sale start date
     */
    function setSaleDates(
        uint256 newPreSaleStartDate,
        uint256 newPreSaleEndDate,
        uint256 newPublicSaleDate
    ) external allowOwner {
        preSaleStartDate = newPreSaleStartDate;
        preSaleEndDate = newPreSaleEndDate;
        pubSaleDate = newPublicSaleDate;
    }

    /**
     * @dev Sets whitelisted addresses. Only the owner can call this function.
     * @param users new whitelisted addresses
     */
    function setWhitelistAddresses(address[] memory users) external allowOwner {
        for (uint256 i = 0; i < users.length; i++) {
            whitelistedAddresses[users[i]] = true;
        }
    }

    /**
     * @dev Checks whether the address has been whitelisted
     * @param user address to check
     */
    function isWhitelistAddress(address user) public view returns (bool) {
        return whitelistedAddresses[user];
    }

    /**
     * @dev Removes the whitelisted addresses
     * @param users addresses to remove
     */
    function removeWhiteListAddresses(address[] memory users) external allowOwner {
        for (uint256 i = 0; i < users.length; i++) {
            delete whitelistedAddresses[users[i]];
        }
    }

    /**
     * @dev Changes the reserve count of NFT for airdrops. Only the owner can call this function.
     */
    function setReserve(uint256 newReserveCount) external allowOwner {
        reservedCount = newReserveCount;
    }

    /**
     * @dev Requests Chainlink VRF for the random number which is used to set the token metadata index offset. Only the owner can call this function.
     * This function is to be used by the owner to reveal the actual metadata URI once all the tokens have been minted or in technically when max supply of token is reached.
     */
    function revealMetadata() public allowOwner {
        require(tokenIdCounter.current() == maxSupply, "Not ready for reveal");
        require(metadataIndexOffset == 0, "Already requested");
        // Will revert if subscription is not set and funded.
        vrfRequestId = coordinator.requestRandomWords(keyHash, subscriptionId, REQUEST_CONFIRMATIONS, callBackGasLimit, NUMRAND);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        require(requestId == vrfRequestId, "Bad Request");
        metadataIndexOffset = randomWords[0] % maxSupply;
        if (metadataIndexOffset == 0) {
            // in the 1 per MAX_TOKENS chance that we get a 0 modulus,
            // we assign the index offset to 1
            metadataIndexOffset = 1;
        }
    }

    /**
     * @dev Withdraws accumulated ETH balance from the contract. Only the owner can call this function.
     */
    function withdraw() external payable allowOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    /**
     * @dev Overriden to block operations when the contract is paused.
     * Hook that is called before any token transfer. This includes minting
     * and burning.
     * Handle the extra logic after calling super.
     */
    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId
    ) internal override(ERC721Enumerable) notPaused {
        super._beforeTokenTransfer(_from, _to, _tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721Enumerable) returns (bool) {
        return interfaceId == this.royaltyInfo.selector || super.supportsInterface(interfaceId);
    }

    /**
     * Sets royalty calculation parameters. Only the owner can call this function.
     */
    function setRoyaltyCalcInfo(uint256 newFeeNumerator, uint256 newFeeDenominator) external allowOwner {
        feeNumerator = newFeeNumerator;
        feeDenominator = newFeeDenominator;
    }

    /**
     * Signals royalty information as per ERC-2981.
     */
    function royaltyInfo(uint256, uint256 amount) public view returns (address, uint256) {
        return (owner(), (amount * feeNumerator) / feeDenominator);
    }

    /**
     * @dev Burns the tokenId. Only the owner of the tokenId or the approved operator can burn tokens.
     */
    function burn(uint256 tokenId) external {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "Not owner nor approved");
        addressMintedBalance[ownerOf(tokenId)]--;
        _burn(tokenId);
    }

    function _mintMultiple(address to, uint256 mintAmount) internal {
        require(mintAmount > 0, "mintAmount cannot be zero");
        for (uint256 i = 0; i < mintAmount; i++) {
            tokenIdCounter.increment();
            uint256 _tokenId = tokenIdCounter.current();
            _safeMint(to, _tokenId);
        }
    }

    /**
     * @dev Validate if specified token number can be minted.
     * @param mintAmount number of tokens to mint
     */
    function _validateMint(uint256 mintAmount) internal {
        uint256 supply = tokenIdCounter.current();
        require(supply + mintAmount <= maxSupply - reservedCount, "NFT supply maxed out");
        require(block.timestamp >= preSaleStartDate, "Sale has not started yet");
        require(mintAmount <= MAX_MINT_AMOUNT, "Max mint amount exceeded");
        uint256 ownerMintedCount = addressMintedBalance[msg.sender];
        require(ownerMintedCount + mintAmount <= nftPerAddressLimit, "Max NFT per address exceeded");

        // validate pre-sale and public-sale parameters
        if (block.timestamp < pubSaleDate) {
            require(block.timestamp < preSaleEndDate, "Public sale has not started");
            require(isWhitelistAddress(msg.sender), "Address is not whitelisted");
            require(msg.value >= preSaleCost * mintAmount, "Insufficient funds sent");
            require(supply + mintAmount <= preSaleMaxSupply, "Max NFT presale limit exceeded");
        } else {
            require(msg.value >= pubSaleCost * mintAmount, "Insufficient funds sent");
        }
    }

    /**
     * @dev Reserves (mints) a specified number of tokens for a specified address. Only the owner can call this function.
     * @param toAddress address to mint the token to
     * @param mintAmount number of tokens to mint
     */
    function reserve(address toAddress, uint256 mintAmount) external allowOwner {
        uint256 supply = tokenIdCounter.current();
        require(supply + mintAmount <= maxSupply, "NFT supply maxed out");
        require(reservedCount > 0, "Reserve depleted");
        addressMintedBalance[msg.sender] += mintAmount;
        reservedCount -= mintAmount;
        _mintMultiple(toAddress, mintAmount);
    }

    /**
     * @dev Mint mutiple tokens at once.
     * @param mintAmount number of tokens to mint.
     */
    function mint(uint256 mintAmount) external payable {
        _validateMint(mintAmount);
        addressMintedBalance[msg.sender] += mintAmount;
        _mintMultiple(msg.sender, mintAmount);
    }

    /**
     * @dev Paginated query to get all the token ids of the owner or all the tokens
     * @param owned bool to specify whether the query is for owned or all NFTs
     * @param from starting index
     * @param length number of token ids to return
     */
    function getTokens(
        bool owned,
        uint256 from,
        uint256 length
    ) external view returns (uint256[] memory) {
        uint256 numActive = owned ? balanceOf(msg.sender) : totalSupply();
        if (from + length > numActive) {
            length = numActive - from;
        }
        uint256[] memory tokenIds = new uint256[](length);
        for (uint256 i = 0; i < length; i++) {
            tokenIds[i] = owned ? tokenOfOwnerByIndex(msg.sender, from + i) : tokenByIndex(from + i);
        }
        return tokenIds;
    }
}

