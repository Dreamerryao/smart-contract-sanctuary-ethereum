pragma solidity ^0.7.0;

import '@openzeppelin/contracts/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

import '../interfaces/IRiddleBrain.sol';

contract RiddleBrainBuySell is Ownable {
	using SafeMath for uint;
    using Address for address;

    struct Batch {
        uint count;
        uint price;
    }

    IRiddleBrain internal _riddleBrain;
    mapping(uint => Batch) internal _batchByNum; // batchNum => Batch;

    constructor(address riddleBrain) public onlyOwner {
        _riddleBrain = IRiddleBrain(riddleBrain);
    }

    function getRiddleBrain() public view returns (address) {
		return address(_riddleBrain);
	}

	/** 
	* @dev set riddle brain address
	* @param riddleBrain riddle brain
	*/
	function setRiddleBrain(address riddleBrain) public onlyOwner {
		_riddleBrain = IRiddleBrain(riddleBrain);
	}

	function getBatchByNumber(uint batchNum) external view returns(uint count, uint price) {
		count = _batchByNum[batchNum].count;
		price = _batchByNum[batchNum].price;
	}

	/** 
	* @dev set brain batch info
	* @param batchNum batch number that is agreed
	* @param count the count of brains in the batch
	* @param price the price of this batch
	*/
	function setBrainBatch(uint batchNum, uint count, uint price) external onlyOwner {
		_batchByNum[batchNum] = Batch({
			count: count,
			price: price
		});
	}

	function _burnBrainsOf(address user, uint amount) internal {
		_riddleBrain.burn(user, amount);
	}

	function _transferBrainsTo(address to, uint amount) internal {
		_riddleBrain.mint(to, amount);
	}

	/** 
	* @dev transfer brain batch to a given user. internal function that will be called in riddle core
	* @param to a user who is supposed to get brains of the given batch
	* @param batchNum batch number
	*/
	function _transferBrainBatchTo(address to, uint batchNum) internal {
		_riddleBrain.mint(to, _batchByNum[batchNum].count);
	}
}

