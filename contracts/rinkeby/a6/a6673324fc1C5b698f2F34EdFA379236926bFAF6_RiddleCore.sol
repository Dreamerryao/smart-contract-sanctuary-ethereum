pragma solidity ^0.7.0;

import '@openzeppelin/contracts/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC1155/IERC1155.sol';
import "@openzeppelin/contracts/token/ERC1155/ERC1155Holder.sol";
import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import 'hardhat/console.sol';

import './RiddleKeyBuySell.sol';
import './RiddleBrainBuySell.sol';
import '../interfaces/IRiddleKey.sol';
import '../libraries/SafeMath32.sol';
import '../utilities/AdminRole.sol';

contract RiddleCore is RiddleBrainBuySell, RiddleKeyBuySell, ERC1155Holder {
	using SafeMath for uint;
	using SafeMath32 for uint32;
	using Address for address;
	using SafeERC20 for IERC20;

	string public constant BRAIN = 'riddle';
	string public constant KEY = 'KEY';

	event AddedBrain(address indexed sender, uint amount);
	event AddedKey(address indexed sender, uint amount);
	event ConfirmedSubmitRiddle(address user, uint level);
	event ConfirmedBuyHint(address user, uint level);
	event LockedLevel(uint level);
	event UnlockedLevel(uint level);
	event PassedAllLevels(address user);

	struct KeyInfo {
		uint index;
		string rKey;
	}

	struct UserInfo {
		uint256 brainAmount;
		uint32 level;
		mapping(uint => KeyInfo) keyInfo;	// level => keyInfo
		mapping(uint => bool) hasKeyOfLevel;   // level => result
	}

	struct LevelInfo {
		bool locked;
		uint256 brainAmountNeed;
		uint256 startedTime;
		uint256 cooldownTime;
		uint256 periodTime;
		mapping(uint => uint) brainAmountNeedForHint;	// hint number => amount of brains
	}

	mapping(address => UserInfo) private _userInfo;
	mapping(uint => LevelInfo) private _levelInfo;
	uint private _currentLevel;
	uint private _currentSeason = 1;
	address payable private _wallet;

	constructor(address riddleBrain, address riddleKey, address payable wallet) public onlyOwner RiddleKeyBuySell(riddleKey) RiddleBrainBuySell(riddleBrain) {
		_wallet = wallet;
		initializeLevelInfo();
	}

	/** 
	* @dev start new season. will initialize old data from the previous season
	* @param immediatelyStartSale if sale starts immediately after moving the next season
	*/
	function startNewSeason(uint season, bool immediatelyStartSale) external onlyOwner {
		initializeLevelInfo();
		_riddleKey.initialize();
		if (immediatelyStartSale) {
				setSellEvent(7 days, 24 hours); // default sale period time and delay time
		}
		_currentSeason = season;
	}

	/** 
	* @dev initialize the level info. unlocks all level beside of level 1 and rest current level to 1
	*/
	function initializeLevelInfo() public onlyOwner {
		_setLevelInfo(1, true, 0, 0, 0);
		_setLevelInfo(2, true, 0, 0, 0);
		_setLevelInfo(3, true, 0, 0, 0);
		_setLevelInfo(4, true, 0, 0, 0);
		_currentLevel = 0;
	}

	function getWallet() external view returns (address) {
		return _wallet;
	}

	/** 
	* @dev send the user info of given user address
	* @param user user address
	*/
	function getUserInfo(address user) public view returns (uint brainAmount, uint32 level, bool hasKey, uint keyIndex) {
		brainAmount = _userInfo[user].brainAmount;
		level = _userInfo[user].level;
		hasKey = _userInfo[user].hasKeyOfLevel[level];
		if (hasKey) {
			keyIndex = _userInfo[user].keyInfo[level].index;
		}
	}

	// return key index and key own status if a given user has the key in a given level. 
	// It might be used to determine if a user has special key (diamond key) in level 5 for the next season.
	function ownedKeyOfUserByLevel(address user, uint level) public view returns (bool hasKey, uint keyIndex) {
		hasKey = _userInfo[user].hasKeyOfLevel[level];
		keyIndex = _userInfo[user].keyInfo[level].index;
	}

	/** 
	* @dev return current level
	*/
	function getCurrentLevel() external view returns (uint) {
		return _currentLevel;
	}

	function getCurrentSeason() external view returns (uint) {
		return _currentSeason;
	}

	/** 
	* @dev return locked status of the given level
	* @param level level
	*/
	function isStartedOf(uint level) external view returns (bool) {
		return !_levelInfo[level].locked;
	}

	function getLevelInfo(uint level) external view returns (
		bool locked,
		uint startedTime,
		uint cooldownTime,
		uint periodTime,
		uint brainAmountNeed,
		uint brainAmountNeedForHint1,
		uint brainAmountNeedForHint2
	) {
		locked = _levelInfo[level].locked;
		startedTime = _levelInfo[level].startedTime;
		cooldownTime = _levelInfo[level].cooldownTime;
		periodTime = _levelInfo[level].periodTime;
		brainAmountNeed = _levelInfo[level].brainAmountNeed;
		brainAmountNeedForHint1 = _levelInfo[level].brainAmountNeedForHint[1];
		brainAmountNeedForHint2 = _levelInfo[level].brainAmountNeedForHint[2];
	}

	/** 
	* @dev buy a key for level one before starting season
	* @param tokenId token id for level one
	* @param rKey rKey of key token. determined by tokenId and supplied index
	*/
	function buyKeyForLevelOne(uint tokenId, string memory rKey) external payable {
		UserInfo storage user = _userInfo[msg.sender];
		require(user.level < 2, 'Can not buy the key in this level');
		require(_riddleKey.levelOf(tokenId) == 1, 'Not permitted to buy this key');
		require(user.hasKeyOfLevel[_riddleKey.levelOf(tokenId)] == false, 'Already bought one');

		_buyKey(tokenId, msg.value);
		_wallet.transfer(msg.value);
		user.hasKeyOfLevel[_riddleKey.levelOf(tokenId)] = true;
		user.keyInfo[_riddleKey.levelOf(tokenId)] = KeyInfo({
			index: _riddleKey.currentSupplyOf(_riddleKey.levelOf(tokenId)).sub(1),
			rKey: rKey
		});
		user.level = 1;
		_transferBrainsTo(msg.sender, _levelInfo[_riddleKey.levelOf(tokenId)].brainAmountNeed);
		emit AddedKey(msg.sender, 1);
	}

	/** 
	* @dev sell brains of given batch with the batch number. payable function
	* @param batchNum batch number
	*/
	function buyBrainBatch(uint batchNum) external payable {
		require(_batchByNum[batchNum].count > 0 && _batchByNum[batchNum].price > 0, 'Not set the price for this batch or wrong batch number sent');
		require(msg.value == _batchByNum[batchNum].price, 'Not sufficient price for buying this batch');

		_transferBrainBatchTo(msg.sender, batchNum);
		_wallet.transfer(msg.value);
		UserInfo storage user = _userInfo[msg.sender];
		user.brainAmount = _batchByNum[batchNum].count;
	}

	/** 
	* @dev allow a user to buy a hint. required: user should have at least amount of brains for buying a hint in this level
	* @param level the level where user buy a hint in
	*/
	function buyHint(uint level) external {
		require(_userInfo[msg.sender].brainAmount >= _levelInfo[level].brainAmountNeed, 'Have not sufficient brains to buy a hint in this level');

		UserInfo storage userInfo = _userInfo[msg.sender];
		_burnBrainsOf(msg.sender, _levelInfo[level].brainAmountNeed);
		userInfo.brainAmount = userInfo.brainAmount.sub(_levelInfo[level].brainAmountNeed);
		emit ConfirmedBuyHint(msg.sender, level);
	}

	/**
	* @dev allow a user to submit a riddle. should remove the amount of brains of the given level from the amount of user's brains
	* @param level the level of solved riddle
	*/
	function submitRiddle(uint level) external {
		UserInfo storage user = _userInfo[msg.sender];
		require(_levelInfo[level].locked == false, 'level is not started yet');
		require(user.level == level || (user.level == 5 && level == 1), 'level mismatch');
		require(user.brainAmount >= _levelInfo[level].brainAmountNeed, 'has not sufficient brains');
		
		_burnBrainsOf(msg.sender, _levelInfo[level].brainAmountNeed);
		user.brainAmount = user.brainAmount.sub(_levelInfo[level].brainAmountNeed);
		emit ConfirmedSubmitRiddle(msg.sender, level);
	}

	/** 
	* @dev give a user with chest new key of next level & update user's information
	* @param level the level that users reached
	* @param prevRKey previous rKey
	* @param nextRKey next rKey
	*/
	function openChest(uint level, string memory prevRKey, string memory nextRKey) external {
		UserInfo storage user = _userInfo[msg.sender];
		require(user.level == level || (level == 1 && user.level == 5), 'level mismatch');
		require(keccak256(abi.encodePacked(user.keyInfo[level].rKey)) == keccak256(abi.encodePacked(prevRKey)), 'rKey mismatch');

		uint nextToken = _riddleKey.tokenOf(user.level.add(1));
		if (user.level == 4) {
				emit PassedAllLevels(msg.sender);
		}
		user.level = user.level.add(1);
		user.hasKeyOfLevel[_riddleKey.levelOf(nextToken)] = true;
		_riddleKey.safeTransferFrom(address(this), msg.sender, nextToken, 1, '');
		user.keyInfo[level.add(1)] = KeyInfo({
			index: _riddleKey.currentSupplyOf(level.add(1)).sub(1),
			rKey: nextRKey
		});
	}

	/**
	* @dev locks given level
	* @param level level to be locked
	*/
	function lockLevel(uint level) external onlyAdmin {
		_setLevelInfo(level, true, 0, 0, 0);
		emit LockedLevel(level);
	}

	/**
	* @dev unlocks given level and set cooldown time of level and period time to end level
	* @param level level number to unlock
	* @param cooldown seconds of cooldown hour
	* @param period seconds of period days
	* @param brainAmountNeed amount of brain needed in this level
	*/
	function unlockLevel(uint level, uint cooldown, uint period, uint brainAmountNeed) external onlyAdmin {
		require(level > 0 && level < 5, 'Level incorrect!');

		_setLevelInfo(level, false, cooldown, period, brainAmountNeed);
		_currentLevel = level;
		emit UnlockedLevel(level);
	}

	/** 
	* @dev set hint price of given level with amount of brains needed
	* @param level level of hint
	* @param hintNum hint number
	*/
	function setHintPriceByBrain(uint level, uint hintNum, uint brainAmountNeed) external {
		require(hintNum < 3, 'invalid hint number');
		require(level > 0 && level < 5, 'invalid level');

		LevelInfo storage levelInfo = _levelInfo[level];
		levelInfo.brainAmountNeedForHint[hintNum] = brainAmountNeed;
	}

	/**
	* @dev set cooldown time of the level
	* @param level level
	* @param cooldown unix time of cooldown as seconds
	*/
	function setCooldownTimeOfLevel(uint level, uint cooldown) external onlyAdmin {
		require(level > 0 && level < 5, 'Level incorrect');
		require(cooldown > 0, 'Cooldown time incorrect');

		LevelInfo storage levelInfo = _levelInfo[level];
		levelInfo.cooldownTime = cooldown;
	}

	/**
	* @dev set period time of the level
	* @param level level
	* @param period unix time of period as seconds
	*/
	function setPeriodTimeOfLevel(uint level, uint period) external onlyAdmin {
		require(level > 0 && level < 5, 'level incorrect');
		require(period > 0, 'cooldown time incorrect');

		LevelInfo storage levelInfo = _levelInfo[level];
		levelInfo.periodTime = period;
	}

	/** 
	* @dev set level information
	* @param level level number
	* @param locked whether given level is locked or not
	* @param cooldown cooldown time after level has been finished
	* @param period period time of given level
	*/
	function _setLevelInfo(uint level, bool locked, uint cooldown, uint period, uint brainAmountNeed) private {
		LevelInfo storage levelInfo = _levelInfo[level];
		levelInfo.locked = locked;
		levelInfo.startedTime = locked ? 0 : block.timestamp;
		levelInfo.brainAmountNeed = brainAmountNeed;
		if (locked == false) {
			if (cooldown > 0) {
				levelInfo.cooldownTime = cooldown;
			}
			if (period > 0) {
				levelInfo.periodTime = period;
			}
		}
	}
}

