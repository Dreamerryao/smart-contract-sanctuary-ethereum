pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract Marketplace is ReentrancyGuard {
    address payable public immutable feeAccount;
    uint256 public feePercent;
    // uint256 public itemCount;
    using SafeMath for uint256;
    using Counters for Counters.Counter;

    Counters.Counter private itemCount;
    // Counters.Counter private _itemsSold;

    // uint256 listingPrice = 0.025 ether;

    // enum State { Created, Release, Inactive }

    struct MarketItem {
        uint itemId;
        address nftContract;
        uint tokenID;
        uint price;
        address payable seller;
        address payable owner;
        bool sold;
    }

    event Offered(
        uint itemId,
        address indexed nftContract,
        uint tokenId,
        uint price,
        address indexed seller
    );

    event Bought(
        uint itemId,
        address indexed nftContract,
        uint tokenId,
        uint price,
        address indexed seller,
        address indexed buyer
    );

    mapping(uint256 => MarketItem) public items;

    constructor(uint256 _feePercent) {
        feeAccount = payable(msg.sender);
        feePercent = _feePercent;
    }

    function getItemCount() public view returns (uint256) {
        return itemCount.current();
    }

    function makeItem(
        // IERC721 _nft,
        address nftContract,
        uint _tokenID,
        uint256 _price
    ) external nonReentrant {
        // require(msg.sender == feeAccount);
        require(_price > 0, "Price must be greater than zero");
        // increment ItemCount;
        itemCount.increment();
        uint256 id = itemCount.current();
        //transfer nft
        IERC721(nftContract).transferFrom(msg.sender, address(this), id);

        // _nft.transferFrom(msg.sender, address(this),id);
        // add new item ot item mapping
        items[id] = MarketItem(
            id,
            nftContract,
            _tokenID,
            _price,
            payable(msg.sender),
            payable(address(this)),
            false
        );

        emit Offered(
            id,
            address(nftContract),
            _tokenID,
            _price,
            payable(msg.sender)
        );
    }

    /* allows someone to resell a token they have purchased */
    function resellToken(uint256 _itemId, uint256 price) public {
        require(
            items[_itemId].owner == msg.sender,
            "Only item owner can perform this operation"
        );
        items[_itemId].sold = false;
        items[_itemId].price = price;
        items[_itemId].seller = payable(msg.sender);
        items[_itemId].owner = payable(address(this));
        //   _itemsSold.decrement();

        IERC721(items[_itemId].nftContract).transferFrom(
            msg.sender,
            address(this),
            items[_itemId].tokenID
        );
    }

    function purchaseItem(uint256 _itemId) external payable nonReentrant {
        uint _totalPrice = getTotalPrice(_itemId);
        uint256 id = itemCount.current();

        MarketItem storage item = items[_itemId];
        require(_itemId > 0 && _itemId <= id, "Item doesn't exist");
        require(
            msg.value >= _totalPrice,
            "Not enought ether to cover item prive and market"
        );
        require(!item.sold, "Item already sold");

        // pay the seller and feeAccount
        item.seller.transfer(item.price);
        feeAccount.transfer(_totalPrice.sub(item.price));
        // Update item asa sold
        item.sold = true;
        // transfer nft
        IERC721(item.nftContract).transferFrom(
            address(this),
            msg.sender,
            item.tokenID
        );
        //emit bought
        emit Bought(
            _itemId,
            address(item.nftContract),
            item.tokenID,
            item.price,
            address(item.seller),
            msg.sender
        );
        // update the owner and seller
        item.owner = payable(msg.sender);
        item.seller = payable(address(0));
    }

    function getTotalPrice(uint _itemId) public view returns (uint256) {
        return ((items[_itemId].price.mul((100 + feePercent)) / 100));
    }

    function updateFeePercentage(uint _feePercent) public {
        require(
            feeAccount == msg.sender,
            "Only marketplace owner can update listing price."
        );
        feePercent = _feePercent;
    }

    /* Returns only items that a user has purchased */
    function fetchMyNFTs() public view returns (MarketItem[] memory) {
        uint256 itemCounter = itemCount.current();
        uint256 counter = 0;
        uint currentIndex = 0;

        for (uint i = 0; i < itemCounter; i++) {
            if (items[i + 1].owner == msg.sender) {
                counter++;
            }
        }

        MarketItem[] memory storageItems = new MarketItem[](counter);

        for (uint i = 1; i <= itemCounter; i++) {
            if (
                items[i].owner == msg.sender
                // &&
                // items[i].state != State.Inactive
            ) {
                MarketItem storage currentItem = items[i];
                storageItems[currentIndex] = currentItem;
                currentIndex++;
            }
        }

        return storageItems;
    }

    /* Returns only items a user has listed */
    function fetchItemsListed() public view returns (MarketItem[] memory) {
        uint256 itemCounter = itemCount.current();
        uint256 counter = 0;
        uint currentIndex = 0;

        for (uint i = 0; i < itemCounter; i++) {
            if (items[i + 1].seller == msg.sender) {
                counter++;
            }
        }

        MarketItem[] memory storageItems = new MarketItem[](counter);
        for (uint i = 1; i <= itemCounter; i++) {
            if (items[i].seller == msg.sender) {
                MarketItem storage currentItem = items[i];
                storageItems[currentIndex] = currentItem;
                currentIndex++;
            }
        }
        return storageItems;
    }
}

