pragma solidity ^0.8.9;

import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {AvantArteStoreInterfaceV1} from "../libraries/AvantArte/AvantArteStoreInterfaceV1.sol";
import {Erc721Holder} from "../libraries/Holders/Erc721Holder.sol";
import {MintersSafeListController, Props as MSLCProps} from "../libraries/Controllers/MintersSafeListController.sol";
import {BasicFunctionality, Props as BFProps} from "../contractsBase/BasicFunctionality.sol";
import {WithdrawSplitter, WithdrawSplit} from "../libraries/Withdraws/WithdrawSplitter.sol";

struct Props {
    address proxyAddr;
    address owner;
    uint256 costInWei;
    address[] safeList;
    uint256 maxMintAmount;
    WithdrawSplit[] withdrawSplits;
}

contract SafeListErc721HolderV1 is
    ReentrancyGuard,
    Erc721Holder,
    MintersSafeListController,
    BasicFunctionality,
    WithdrawSplitter,
    AvantArteStoreInterfaceV1
{
    address public proxyAddr;

    constructor(Props memory props)
        MintersSafeListController(
            MSLCProps(props.safeList, props.maxMintAmount)
        )
        Erc721Holder()
        BasicFunctionality(BFProps(props.owner, props.costInWei))
        WithdrawSplitter(props.withdrawSplits)
    {
        proxyAddr = props.proxyAddr;
    }

    /// @dev allows to purchase a token
    function purchase(uint256 tokenId)
        public
        payable
        override
        onlyRunning
        onlyEnabled
        onlySafeListedMinter
        nonReentrant
    {
        require(_getErc721TokenCount(proxyAddr, tokenId) > 0, "no supply");
        require(msg.value >= costInWei, "no funds");
        _incrementCount();
        _incrementAddressMintedCount(1);

        _splitWithdraw(msg.value);
        emit OnPurchase(msg.sender, tokenId);
        _safeTransferErc721Token(proxyAddr, tokenId, msg.sender, msg.data);
    }

    /// @dev allows admins to take the remaining tokens in the end of the draw
    function withdrawTokens(uint256[] calldata tokenIds)
        external
        onlyOwner
        nonReentrant
    {
        uint256 length = tokenIds.length;
        for (uint256 i = 0; i < length; i++) {
            _safeTransferErc721Token(
                proxyAddr,
                tokenIds[i],
                msg.sender,
                msg.data
            );
        }
    }

    function isActive(uint256 tokenId) public view override returns (bool) {
        return count(tokenId) > 0 && isEnabled && _isTimerRunning();
    }

    function count(uint256 tokenId) public view override returns (uint256) {
        return _getErc721TokenCount(proxyAddr, tokenId);
    }

    function cost(uint256) public view override returns (uint256) {
        return costInWei;
    }

    function expectedRoyalties(uint256) public pure override returns (uint256) {
        return 0;
    }

    function setWithdrawSplit(WithdrawSplit[] calldata _withdrawSplits)
        external
        onlyOwner
        nonReentrant
    {
        _setWithdrawSplit(_withdrawSplits);
    }
}

