pragma solidity ^0.8.9;

import {ERC721Holder as OzERC721Holder} from "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

/*
 * in order to use, the giving contracts must call
 * function setApprovalForAll(THE_ADDRESS_OF_THIS_CONTRACT, true)
 */

contract Erc721Holder is OzERC721Holder {
    function _getErc721TokenCount(address contractAddr, uint256 tokenId)
        internal
        view
        returns (uint256)
    {
        ERC721 nft = ERC721(contractAddr);
        return nft.ownerOf(tokenId) == address(this) ? 1 : 0;
    }

    function _safeTransferErc721Token(
        address contractAddr,
        uint256 tokenId,
        address to,
        bytes calldata data
    ) internal {
        ERC721 nft = ERC721(contractAddr);
        nft.safeTransferFrom(address(this), to, tokenId, data);
    }
}

