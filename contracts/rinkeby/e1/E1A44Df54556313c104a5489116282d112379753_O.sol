    function renderBackground(Peeper memory peeper)
        private
        view
        returns (string memory)
    {
        string
            memory bgAnimation = '<animateTransform attributeName="transform" type="translate" dur="2s" repeatCount="indefinite" to="0 0" ';
        if (bytes(peeper.flower).length > 0) {
            bgAnimation = string(
                abi.encodePacked(bgAnimation, 'from="-250 -440"/>')
            );
        } else {
            bgAnimation = string(
                abi.encodePacked(bgAnimation, 'from="-310 -270"/>')
            );
        }

        string memory pattern = string(
            abi.encodePacked(
                '<rect width="200%" height="200%" fill="url(#bg-pattern)">',
                bgAnimation,
                "</rect>"
            )
        );
        string memory output = string(
            abi.encodePacked(
                '<rect id="bg" width="',
                Helpers.toString(WIDTH),
                '" height="',
                Helpers.toString(HEIGHT),
                '" fill="',
                colors[getAColorID(peeper.id, "BG")][1],
                '" />',
                pattern
            )
        );
        return output;
    }

    function renderCommonPattern(Peeper memory peeper)
        private
        view
        returns (string memory)
    {
        // If has flower, add flower pattern
        string memory flowerPattern = "";
        if (bytes(peeper.flower).length > 0) {
            flowerPattern = string(
                abi.encodePacked(
                    flowerPattern,
                    '<g id="flower-bg">',
                    peeper.flower,
                    '</g><g id="flower">',
                    peeper.flower,
                    "</g><style>#flower rect { display: none; } #flower-bg use, #flower-bg circle { display: none; }</style>"
                )
            );
        }

        string
            memory skinPattern = '<pattern id="skin-pattern" x="0" y="0" width="500" height="500" patternUnits="userSpaceOnUse" patternTransform="rotate(-30)">';

        // SKIN PATTERN
        if (bytes(peeper.flower).length == 0) {
            skinPattern = string(
                abi.encodePacked(
                    skinPattern,
                    '<rect width="500" height="500" fill="',
                    peeper.skinColor[1],
                    '" /></pattern>'
                )
            );
        } else {
            skinPattern = string(
                abi.encodePacked(
                    skinPattern,
                    '<use xlink:href="#flower-bg" /></pattern>'
                )
            );
        }

        // BG PATTERN
        string
            memory bgPattern = '<pattern id="bg-pattern" x="0" y="0" patternUnits="userSpaceOnUse" patternTransform="rotate(-30)" ';
        if (bytes(peeper.flower).length == 0) {
            bgPattern = string(
                abi.encodePacked(
                    bgPattern,
                    'width="130" height="130">',
                    peeper.weather[1],
                    colors[getAColorID(peeper.id, "BGPATTERN")][1],
                    peeper.weather[2],
                    "</pattern>"
                )
            );
        } else {
            bgPattern = string(
                abi.encodePacked(
                    bgPattern,
                    'width="500" height="500">',
                    '<use xlink:href="#flower" /></pattern>'
                )
            );
        }

        return
            string(
                abi.encodePacked(
                    "<defs>",
                    flowerPattern,
                    skinPattern,
                    bgPattern,
                    "</defs>"
                )
            );
    }

    function renderBody() private pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    '<ellipse cx="510" cy="860" rx="38%" ry="20%" fill="url(#skin-pattern)"',
                    STROKE,
                    "/>"
                )
            );
    }

    function renderHands(Peeper memory peeper)
