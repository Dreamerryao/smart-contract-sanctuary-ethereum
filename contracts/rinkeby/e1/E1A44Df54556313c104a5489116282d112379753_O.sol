    // TODO: - Add utility function to mint with multiple peepers
    function capturePeeperWithFlowers(uint256[] calldata flowerIds)
        external
        payable
        nonReentrant
    {
        require(huntBegins, "Hunt has yet begun");
        require((totalSupply() + flowerIds.length) < MAXIMUM_PEEPER, "No more peeper");
        require((CAPTURE_BOUNTY * flowerIds.length) <= msg.value, "Bounty value is not correct");
        for (uint256 index = 0; index < flowerIds.length; index++) {
            require(
                FlowersInterface(flowersAddress).ownerOf(flowerIds[index]) == msg.sender,
                "You don't own the flower"
            );
            uint256 tokenId = totalSupply() + 1;
            peeperToFlowerId[tokenId] = flowerIds[index];
            _safeMint(msg.sender, tokenId);
        }
    }

    function capturePeeper(uint256 amount) public payable nonReentrant {
        require(huntBegins, "Hunt has yet begun");
        require(totalSupply() + amount < MAXIMUM_PEEPER, "No more peeper");
        require(amount <= 10, "You are too greedy");
        require(
            (CAPTURE_BOUNTY * amount) <= msg.value,
            "Bounty value is not correct"
        );
        for (uint256 index = 0; index < amount; index++) {
            _safeMint(msg.sender, totalSupply() + 1);
        }
    }

    // Sacrifice a peeper for another peeper
    function sacrificePeeper(uint256[] calldata tokenIds)
        external
        nonReentrant
    {
        require(huntBegins, "Hunt has yet begun");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                ownerOf(tokenIds[i]) == msg.sender,
                "You must own the Peeper"
            );
        }
        for (uint256 i = 0; i < tokenIds.length; i++) {
            // Burn and mint
            _burn(tokenIds[i]);
            _safeMint(msg.sender, totalSupply() + 1);
        }
    }

    // Inspired by Alt-nouns contract by On-Chain-Collective https://etherscan.io/address/0x971a6ff4f5792f3e0288f093340fb36a826aae96
    function getFlower(uint256 flowerTokenId)
        public
        view
        returns (string memory)
    {
        if (flowerTokenId == 0) {
            return "";
        }

        string memory tokenUri = FlowersInterface(flowersAddress).tokenURI(flowerTokenId);
        string memory decodedFlower = string(
            Base64.decode(
                Helpers.substring(
                    tokenUri,
                    int256(Helpers.length(tokenUri) - 29),
                    29
                )
            )
        );

        // Finds the index of ';' in decoded tokenURI & adds 8 to it (for 'base64,') to set the starting index for the encoded SVG
        uint256 index = uint256(Helpers.indexOf(decodedFlower, ";")) + 8;

        // Substring subtracts 2 from (length - index) to skip the last two characters, i.e. '"}'
        string memory flowerSvg = Helpers.substring(
            decodedFlower,
            int256(Helpers.length(decodedFlower) - index - 2),
            int256(index)
        );

        // Decoding base64 to svg string. Skips the opening and closing <svg> tags
        bytes memory svgBytes = Base64.decode(flowerSvg);
        bytes memory result = new bytes(svgBytes.length - 117 - 6);
        for (uint256 i = 0; i < result.length; i++) {
            result[i] = svgBytes[i + 117];
        }
        string memory resultStr = string(result);
        return resultStr;
    }

