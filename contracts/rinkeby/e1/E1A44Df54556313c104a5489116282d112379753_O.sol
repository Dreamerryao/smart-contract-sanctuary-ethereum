    function getAColorID(uint256 tokenId, string memory seed)
        private
        view
        returns (uint256)
    {
        return Helpers.pluckNum(tokenId, seed, 0, NUM_COLORS - 1, msg.sender);
    }

    function randomPeeper(uint256 tokenId)
        private
        view
        returns (Peeper memory)
    {
        string[3] memory mouth = mouths[
            Helpers.pluckNum(tokenId, "MOUTH", 0, NUM_MOUTHS - 1, msg.sender)
        ];
        string[2] memory mouthAccessory = mouthAccessories[0];

        if (keccak256(bytes(mouth[2])) == keccak256("1")) {
            mouthAccessory = mouthAccessories[
                Helpers.pluckNum(
                    tokenId,
                    "MOUTHACCESSORY",
                    0,
                    NUM_MOUTH_ACCESSORIES - 1,
                    msg.sender
                )
            ];
        }

        return
            Peeper(
                tokenId,
                getFlower(peeperToFlowerId[tokenId]),
                colors[getAColorID(tokenId, "SKIN")],
                bgPatterns[
                    Helpers.pluckNum(
                        tokenId,
                        "BGPATTERN",
                        0,
                        NUM_PATTERNS - 1,
                        msg.sender
                    )
                ],
                hands[
                    Helpers.pluckNum(
                        tokenId,
                        "HAND",
                        0,
                        NUM_HANDS - 1,
                        msg.sender
                    )
                ],
                noses[
                    Helpers.pluckNum(
                        tokenId,
                        "NOSE",
                        0,
                        NUM_NOSE - 1,
                        msg.sender
                    )
                ],
                eyes[
                    Helpers.pluckNum(
                        tokenId,
                        "EYE",
                        0,
                        NUM_EYES - 1,
                        msg.sender
                    )
                ],
                mouth,
                mouthAccessory
            );
    }

