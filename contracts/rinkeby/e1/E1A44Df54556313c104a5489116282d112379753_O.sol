        return
            string(
                abi.encodePacked(
                    peeper.hand[1],
                    '<g transform="translate(718)">',
                    peeper.hand[1],
                    "</g>"
                )
            );
    }

    function renderImage(uint256 tokenId) private view returns (string memory) {
        Peeper memory peeper = randomPeeper(tokenId);
        string memory image = string(
            abi.encodePacked(
                '<svg width="',
                Helpers.toString(WIDTH),
                '" height="',
                Helpers.toString(HEIGHT),
                '" viewBox="0 0 ',
                Helpers.toString(WIDTH),
                " ",
                Helpers.toString(HEIGHT)
            )
        );
        image = string(
            abi.encodePacked(
                image,
                '" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">',
                renderCommonPattern(peeper),
                renderBackground(peeper),
                renderBody(),
                renderWall(peeper),
                renderHands(peeper),
                renderHead(peeper),
                "</svg>"
            )
        );
        return image;
    }

    function singleAttribute(string memory trait, string memory value)
        private
        pure
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    '{"trait_type": "',
                    trait,
                    '","value": "',
                    value,
                    '"}'
                )
            );
    }

    function getAttributes(uint256 tokenId)
        private
        view
        returns (string memory)
    {
        Peeper memory peeper = randomPeeper(tokenId);
        string memory output = string(
            abi.encodePacked(
                '"attributes": [',
                singleAttribute("Skin", peeper.skinColor[0]),
                ",",
                singleAttribute("Weather", peeper.weather[0]),
                ",",
                singleAttribute("Hands", peeper.hand[0]),
                ",",
                singleAttribute("Nose", peeper.nose[0]),
                ",",
                singleAttribute("Eyes", peeper.eye[0]),
                "]"
            )
        );
        return output;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"Peeper #',
                                Helpers.toString(tokenId),
                                '","description":"Peeper are on chain creatures. Nobody really knows what they are. Pay a bounty to capture your own Peeper, and you might find out...',
                                '","image": "data:image/svg+xml;base64,',
                                Base64.encode(bytes(renderImage(tokenId))),
                                '"',
                                // '",',
                                // getAttributes(tokenId),
                                "}"
                            )
                        )
                    )
                )
            );
    }
}

