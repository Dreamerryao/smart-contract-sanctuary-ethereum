pragma solidity >=0.8.0 <0.9.0;
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

interface IERC165 {
    function supportsInterface(bytes4 interfaceID) external view returns(bool);
}

interface IERC721 is IERC165 {

    event Transfer(address from, address to, uint tokenId);
    event Approval(address owner, address approved, uint tokenId);
    event ApprovalForAll(address owner, address operator, bool approved);

    function balanceOf(address owner) external view returns(uint balance);

    function ownerOf(uint tokenId) external view returns(address owner);

    function safeTransferFrom(address from, address to, uint tokenId) external;

    function transferFrom(address from, address to, uint tokenId) external;

    function approve(address to, uint tokenId) external;

    function getApproved(uint tokenId) external returns(address operator);

    function setApprovalForAll(address operator, bool _approved) external;

    function isApprovedForAll(address owner, address operator) view external returns(bool);

    function safeTransferFrom(address from, address to, uint tokenId, bytes calldata data) external;
}

contract MyNft is IERC165, IERC721{
    string public name;
    string public symbol;
    using Strings for uint;

    using Counters for Counters.Counter;

    Counters.Counter public tokenIndex;

    using Address for address;

    mapping(address => uint) balances;
    mapping(uint => address) tokens;

    mapping(uint => address ) approvals_for_token;

    // owner => controller
    mapping(address => address) approvals_all_token;

    function tokenURI(uint tokenId) external view returns (string memory){
       string memory uri = string(
           abi.encodePacked(
               _baseUrl(), tokenId.toString()
           )
       );
       return uri;
    }

    function _baseUrl() private pure returns(string memory){
        return 'http://';
    }


    function balanceOf(address owner) external view override returns(uint balance){
        return balances[owner];
    }

    function ownerOf(uint tokenId) external view override returns(address owner){
        return tokens[tokenId];
    }

    function approve(address controller, uint tokenId) override external{
        require(tokens[tokenId] != address(0), "tokenId must exist");
        address owner = tokens[tokenId];
        require(owner == msg.sender ||
            approvals_for_token[tokenId] == msg.sender,
            "The caller must own the token or be an approved operator");

        approvals_for_token[tokenId] = controller;
        emit Approval(owner, controller, tokenId);
    }

    function getApproved(uint tokenId) external override view returns(address operator){
        return approvals_for_token[tokenId] ;
    }

    function setApprovalForAll(address operator, bool _approved) override external{
        if(_approved == true){
            approvals_all_token[msg.sender] = operator;
        }else{
            approvals_all_token[msg.sender] = address(0);
        }
    }

    function isApprovedForAll(address owner, address operator) override view external returns(bool){
        return approvals_all_token[owner] == operator;
    }

    function allowedToControl(address owner, address controller, uint tokenId) private returns(bool){
        if(controller == owner ||
            approvals_for_token[tokenId] == controller ||
            approvals_all_token[owner] == controller
        ){
            return true;
        }else {
            return false;
        }
    }

    function transferFrom(address from, address to, uint tokenId) override external {
        require(from != address(0), "from cannot be the zero address");
        require(to != address(0), 'to cannot be the zero address');
        require(tokens[tokenId] == from, "tokenId token must be owned by from");
        require(allowedToControl(from, msg.sender, tokenId), "caller does not approval");
        _transfer(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint tokenId, bytes calldata data)
        override external {
            require(from != address(0), "from cannot be the zero address");
            require(to != address(0), 'to cannot be the zero address');
            require(tokens[tokenId] == from, "tokenId token must be owned by from");
            require(allowedToControl(from, msg.sender, tokenId), "caller does not approval");
            if(Address.isContract(to)){
                require(
                    IERC721Receiver(to).onERC721Received(msg.sender, from, tokenId, data) ==
                    IERC721Receiver.onERC721Received.selector, 'did not return 721 selector back');
                _transfer(from, to, tokenId);
            }else {
                _transfer(from, to, tokenId);
            }
    }

    function safeTransferFrom(address from, address to, uint tokenId) override external {
        require(from != address(0), "from cannot be the zero address");
        require(to != address(0), 'to cannot be the zero address');
        require(tokens[tokenId] == from, "tokenId token must be owned by from");
        require(allowedToControl(from, msg.sender, tokenId), "caller does not approval");
        if(Address.isContract(to)){
            require(
                IERC721Receiver(to).onERC721Received(msg.sender, from, tokenId, '') ==
                IERC721Receiver.onERC721Received.selector, 'did not return 721 selector back');
            _transfer(from, to, tokenId);
        }else {
            _transfer(from, to, tokenId);
        }
    }

    function _transfer(address from, address to, uint tokenId) private {
        tokens[tokenId] = to;
        balances[from] -= 1;
        balances[to] += 1;
        approvals_for_token[tokenId] = address(0);
        emit Transfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) external override view returns(bool){
        console.logBytes4(interfaceId);
        console.logBytes4(type(IERC165).interfaceId);
        console.logBytes4(type(IERC721).interfaceId);

        if(interfaceId == type(IERC165).interfaceId ||
            interfaceId == type(IERC721).interfaceId){
                return true;
        }else {
            return false;
        }
    }

    function mint() public payable{
        require(msg.value >= 1 * 10**18, 'at least 1 Eth is required');
        tokenIndex.increment();
        tokens[tokenIndex.current()] = msg.sender;
        balances[msg.sender] += 1;
