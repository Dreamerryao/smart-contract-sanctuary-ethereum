import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./MyNft.sol";

// works of an erc20 token, not eth
// pledge your tokens in
// if reached goal campaign builder gets all the tokens
// if unable to reach goal, pledger can withdraw their tokens
// multiple crowd funding campaigns, all individually tracked
// all campaigns exist in the same contract

contract CrowdFund {

    address public immutable fooTokenAddress;
    IERC20 public immutable FooToken;

    constructor(address _addr){
        fooTokenAddress = _addr;
        FooToken = IERC20(fooTokenAddress);
    }

    using Counters for Counters.Counter;

    struct Campaign {
        string name;
        uint goalAmount;
        uint endTime;
        address launcher;
        uint campaignNumber;
        bool isCampaign;
        uint amountReached;
        bool claimed;
    }


    // campaignId => pledger'sAddress => amount pledged
    mapping(uint => mapping(address => uint)) public pledgedAmounts;

    mapping(uint => Campaign) public campaigns;
    Counters.Counter numberOfCampaigns;

    event CampaignLaunched(address, uint);
    event MoneyIn(address, uint, uint);
    event MoneyOut(address, uint, uint);

    modifier campaignExists(uint campaignId){
        require(campaigns[campaignId].isCampaign == true, "no such campaign");
        _;
    }

    modifier amountGreaterThanMinimum(uint amount){
        require(amount > 0, "amount below minimum ");
        _;
    }

    modifier hasEnoughTokens(uint amount){
        require(FooToken.balanceOf(msg.sender) >= amount, "you dont have enough tokens");
        _;
    }

    modifier hasAllowedUsAccessToTokens(uint amount){
        require(FooToken.allowance(msg.sender, address(this)) >= amount, "allowance missing");
        _;
    }

    modifier hasNotExpired(uint campaignId){
        require(block.timestamp < campaigns[campaignId].endTime, "campaign has expired");
        _;
    }

    modifier hasExpired(uint campaignId){
        require(block.timestamp >= campaigns[campaignId].endTime, "campaign is still active");
        _;
    }

    modifier hasReachedGoal(uint campaignId){
        Campaign memory camp = campaigns[campaignId];
        require(camp.amountReached >= camp.goalAmount, 'goal not reached');
        _;
    }

    modifier hasNotReachedGoal(uint campaignId){
        Campaign memory camp = campaigns[campaignId];
        require(camp.amountReached <= camp.goalAmount, 'goal exceeded');
        _;
    }

    modifier hasNotBeenClaimed(uint campaignId){
        Campaign memory camp = campaigns[campaignId];
        require(camp.claimed == false, 'has already been claimed');
        _;
    }

    function createCampaign(uint _goalAmount, uint _lengthInMinutes, string calldata _name) public{
        numberOfCampaigns.increment();

        Campaign memory newCampaign = Campaign({
            goalAmount: _goalAmount,
            endTime: block.timestamp + _lengthInMinutes * (1 minutes),
            launcher: msg.sender,
            campaignNumber: numberOfCampaigns.current(),
            name: _name,
            isCampaign: true,
            amountReached: 0,
            claimed: false
        });

        campaigns[newCampaign.campaignNumber] = newCampaign;
        console.log("new campaign with id below has been created");
        console.log(numberOfCampaigns.current());
        emit CampaignLaunched(msg.sender, numberOfCampaigns.current());
    }

    function pledge(uint campaignId, uint amount)
        campaignExists(campaignId)
        amountGreaterThanMinimum(amount)
        hasEnoughTokens(amount)
        hasAllowedUsAccessToTokens(amount)
        hasNotExpired(campaignId)
        external {
            bool transferResult = FooToken.transferFrom(msg.sender, address(this), amount);
            require(transferResult == true, "transfer failed");

            pledgedAmounts[campaignId][msg.sender] += amount;
            campaigns[campaignId].amountReached += amount;
            emit MoneyIn(msg.sender, campaignId, amount);
    }

    function withdrawByPledger(uint campaignId)
        campaignExists(campaignId)
        hasExpired(campaignId)
        hasNotReachedGoal(campaignId)
        external {
            uint amount = pledgedAmounts[campaignId][msg.sender];
            pledgedAmounts[campaignId][msg.sender] = 0;
            bool transferResult = FooToken.transfer(msg.sender, amount);
            require(transferResult == true, "transfer failed");
            emit MoneyOut(msg.sender, campaignId, amount);
    }

    function withdrawByCampaignLauncher(uint campaignId)
        campaignExists(campaignId)
        hasExpired(campaignId)
        hasReachedGoal(campaignId)
        hasNotBeenClaimed(campaignId)
        external {
            Campaign memory camp = campaigns[campaignId];
            require(msg.sender == camp.launcher, "only campaign launcer can call");
            camp.claimed = true;
            bool transferResult = FooToken.transfer(msg.sender, camp.amountReached);
            require(transferResult == true, "transfer failed");
            emit MoneyOut(msg.sender, campaignId, camp.amountReached);
    }

    function currentCampaignCount() view public returns(uint){
        return numberOfCampaigns.current();
    }

    function getCampaignName(uint num) view public returns(string memory){
        return campaigns[num].name;
    }

    function myTotalPledge(uint campaignId) view public returns(uint){
        return pledgedAmounts[campaignId][msg.sender];
    }

    function campaignGoal(uint campaignId) view public returns(uint) {
        return campaigns[campaignId].goalAmount;
    }

    function campaignReached(uint campaignId) view public returns(uint) {
        return campaigns[campaignId].amountReached;
    }

    function myFooTokenBalance() view public returns(uint){
        return FooToken.balanceOf(msg.sender);
    }

    function expired(uint campaignId) view external returns(bool){
        Campaign memory camp = campaigns[campaignId];
        return block.timestamp > camp.endTime;
    }
}

