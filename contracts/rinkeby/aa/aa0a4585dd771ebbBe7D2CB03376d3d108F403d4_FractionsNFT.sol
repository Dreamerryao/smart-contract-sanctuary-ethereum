pragma solidity ^0.8.1;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Royalty.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

import "hardhat/console.sol";
/**
 * @title Fractions NFT collection contract
 * @dev Extends ERC721 Non-Fungible Token Standard basic implementation and use ERC721Royalty as royalties system
 * @author maxper
 */
contract FractionsNFT is ERC721Royalty, Ownable, ReentrancyGuard, VRFConsumerBaseV2 {

    //struct to hold not yet claimed rewards for each NFT
    struct Draw {
        mapping(uint256 => uint256) rewardsByNFTs;
    }

    struct Giveaway {
        uint256 chainlinkRequestId;
        uint timestamp;
        uint[] winnersNb;
        uint[] amountToBeWon;
        uint[] winners;
        bool finalized;
    }

    string private baseURI;                 //baseURI for metadatas
    uint256 public percentsDecimals = 9;
    mapping(uint256 => uint256) public nftsPercents;       //percent on royalties for each NFT
    uint256[] public nftsPercentsCumulated;
    uint16 public nftPercentsFilling = 0;

    bool public mintOpen = false;
    bool public revealed = false;

    uint256 public maxMintPerTx = 5;
    uint256 public immutable maxSupply;
    uint256 public mintCost;
    uint256 public minted = 0;

    string public provenanceHash = "";
    uint256 public offsetIndexBlock;
    uint256 public offsetIndex;

    //Chainlink
    uint64 private chainlinkSubscriptionId = 1390;
    bytes32 private chainlinkKeyHash = 0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;
    VRFCoordinatorV2Interface public vrfCoordinator = VRFCoordinatorV2Interface(0x6168499c0cFfCaCD319c818142124B7A15E857ab);
    IERC20 private linkToken = IERC20(0x01BE23585060835E02B77ef475b0Cc51aA1e0709);
    uint32 private vrfCallbackGasLimit = 2000000;
    uint16 private vrfRequestConfirmations = 3;

    address payable public teamWallet;

    mapping(uint256 => Giveaway) giveaways;
    uint256[] public giveawaysIds;

    Draw private _lastDraw;     //pending rewards

    event MintState(bool state);
    event Mint(uint256 supply);
    event SoldOut();
    event Reveal();

    constructor(uint256 supply, uint256 cost, address payable wallet, uint96 royalties) ERC721("Fractions NFT", "FRAC") Ownable() 
        VRFConsumerBaseV2(0x6168499c0cFfCaCD319c818142124B7A15E857ab){
        maxSupply = supply;
        mintCost = cost;
        teamWallet = wallet;
        _setDefaultRoyalty(wallet, royalties);
    }

    function setProvenanceHash(string memory phash) public onlyOwner {
        require(!mintOpen && minted == 0, "Too late");
        provenanceHash = phash;
    }

    function updateChainlinkParams(bytes32 newHash, uint64 newSubscriptionId, address coordinator, address token, uint32 callbackGasLimit, uint16 requestConfirmations) external onlyOwner {
        chainlinkKeyHash = newHash;
        chainlinkSubscriptionId = newSubscriptionId;
        vrfCoordinator = VRFCoordinatorV2Interface(coordinator);
        linkToken = IERC20(token);
        vrfCallbackGasLimit = callbackGasLimit;
        vrfRequestConfirmations = requestConfirmations;
    }

    function setTeamWallet(address payable wallet) external onlyOwner {
        teamWallet = wallet;
    }

    function updateMintParams(uint256 newMax, uint256 newCost) external onlyOwner {
        require(!mintOpen, "Sale is active");
        maxMintPerTx = newMax;
        mintCost = newCost;
    }

    function setMintState(bool value) external onlyOwner {
        require(mintOpen != value, "State already set.");
        mintOpen = value;

        emit MintState(value);
    }

    function setRoyalty(address receiver, uint96 feeNumerator) external onlyOwner {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    /**
     * Set base URI for metadatas
     */
    function setBaseURI(string memory uri) public onlyOwner {
        baseURI = uri;
    }

    function totalSupply() public view returns (uint256) {
        return minted;
    }

    /**
     * Get metadatas URI for tokenId.
     */
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        if(!revealed || bytes(baseURI).length == 0)
        {
            return getMetadatasNotRevealed(tokenId);
        }
        else
        {
            return string(abi.encodePacked(baseURI, Strings.toString((offsetIndex + tokenId) % maxSupply), ".json"));
        }
    }

    function tokenURIs(uint256[] memory tokensIds) public view returns (string[] memory) {
        string[] memory uris = new string[](tokensIds.length);
        for(uint256 i = 0; i < tokensIds.length; i++) {
            uris[i] = tokenURI(tokensIds[i]);
        }

        return uris;
    }

    function getMetadatasNotRevealed(uint tokenId) internal pure returns (string memory) {
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "Fraction ', Strings.toString(tokenId), '", "description": "Collect fractions, earn rewards...", "image": "ipfs://QmPnz4YFGsBRAFZJ5nSRD1G5K9YQaLTivVzyaHZCCTjHA9"}'
                    )
                )
            )
        );

        return string(abi.encodePacked("data:application/json;base64,", json));
    }

    function _safeMint(address to, uint256 tokenId) internal override virtual {
        super._safeMint(to, tokenId);
        minted++;
    }

    function addFirstPercent(uint32 percent) external onlyOwner {
        require(nftPercentsFilling == 0, "already done");
        nftsPercents[0] = percent;
        nftsPercentsCumulated.push(percent);
        nftPercentsFilling = 1;
    }

    function addPercents(uint32[] calldata percents) external onlyOwner {
        uint256 percentLen = percents.length;
        require(nftPercentsFilling >= 1, "call addFirstPercent first");
        require(nftPercentsFilling + percentLen <= maxSupply, "Too much percents");

        uint256 len = nftsPercentsCumulated.length;
        
        unchecked {
            for(uint256 i = 0; i < percentLen; ++i) {
                uint256 percent = percents[i];
                nftsPercents[nftPercentsFilling + i] = percent;
                nftsPercentsCumulated.push(nftsPercentsCumulated[len + i - 1] + percent);
            }
            nftPercentsFilling += (uint16)(percentLen);
        }
    }

    function setOffsetIndex() public {
        require(offsetIndex == 0, "Starting index is already set");
        require(offsetIndexBlock != 0, "Starting index block must be set");
        
        offsetIndex = uint(blockhash(offsetIndexBlock)) % maxSupply;
        // Just a sanity case in the worst case if this function is called late (EVM only stores last 256 block hashes)
        if (block.number - offsetIndexBlock > 255) {
            offsetIndex = uint(blockhash(block.number - 1)) % maxSupply;
        }
        // Prevent default sequence
        if (offsetIndex == 0) {
            offsetIndex = 1;
        }
    }

    /**
     * Set the starting index block for the collection, essentially unblocking
     * setting starting index
     */
    function emergencySetOffsetIndexBlock() public onlyOwner {
        require(offsetIndex == 0, "Starting index is already set");
        
        offsetIndexBlock = block.number;
    }

    function reveal(string memory uri) external onlyOwner {
        require(!revealed, "Reveal already done");

       //if we did not soldout
        if(minted < maxSupply)
        {
            uint256 remaining = maxSupply - minted;
            unchecked {
                for (uint256 i = 0; i < remaining; i++) {       //mint remaining tokens to owners
                    _safeMint(msg.sender, minted);
                }
            }

            if (offsetIndexBlock == 0) {
                offsetIndexBlock = block.number;
            } 
            mintOpen = false;
        }

        if(offsetIndex == 0) {
            setOffsetIndex();
        }

        setBaseURI(uri);

        emit Reveal();
        revealed = true;
    }

    /**
     * Mint function usable while mint is opened
     */
    function mint(uint256 mintAmount) payable external nonReentrant {
        require(mintOpen, "Mint closed.");
        require(mintAmount <= maxMintPerTx, "Max by tx exceed.");
        require(minted + mintAmount <= maxSupply, "Sold out.");
        require(msg.value == mintCost * mintAmount, "Incorrect amount sent.");

        unchecked {
            for (uint256 i = 0; i < mintAmount; ++i) {
                _safeMint(msg.sender, minted);
            }
        }

        if(minted == maxSupply) {
            mintOpen = false;
            emit SoldOut();

            if (offsetIndexBlock == 0) {
                offsetIndexBlock = block.number;
            } 
        }

        emit Mint(minted);
    }

    //TODO where is the reward coming from ?
    /**
     * Make the rewards from royalties claimables.
     */
    function releaseRoyalties() external payable onlyOwner {
        require(revealed, "Not revealed yet");
        uint256 fullReward = msg.value;
        
        for(uint i = 0; i < maxSupply; ++i) {
            _lastDraw.rewardsByNFTs[i] = _lastDraw.rewardsByNFTs[i] + fullReward * percentOfNft(i) / (10 ** percentsDecimals);
        }
    }

    /**
     * Make giveway according to percent of NFT.
     * @dev use Chainlink VRF
     */
    function makeGiveaway(uint[] calldata winnersNb, uint[] calldata amountToBeWon) external payable onlyOwner {
        require(winnersNb.length == amountToBeWon.length, "Winners and amounts not matching.");

        uint fullReward = 0;
        for(uint i = 0; i < amountToBeWon.length; i++) {
            fullReward += amountToBeWon[i] * winnersNb[i];
        }

        require(fullReward == msg.value, "Incorrect amount sent.");

        uint256 requestId = vrfCoordinator.requestRandomWords(chainlinkKeyHash, chainlinkSubscriptionId, vrfRequestConfirmations, vrfCallbackGasLimit, 1);
        
        Giveaway storage giveaway = giveaways[requestId];
        giveaway.winnersNb = winnersNb;
        giveaway.amountToBeWon = amountToBeWon;
        giveaway.timestamp = block.timestamp;
        giveaway.finalized = false;
        giveaway.chainlinkRequestId = requestId;

        giveawaysIds.push(requestId);
    }

    /**
     * Reception of randomness, finalize the giveaway
     * @dev use Chainlink VRF
     */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        Giveaway memory giveaway = giveaways[requestId];
        if(giveaway.finalized == false)
        {
            uint256 randomness = randomWords[0];
            uint256 randomnessDraw = 0;
            for (uint256 winnersCategory = 0; winnersCategory < giveaway.winnersNb.length; ++winnersCategory) {
                for (uint256 winner = 0; winner < giveaway.winnersNb[winnersCategory]; ++winner) {
                    ++randomnessDraw;
                    uint256 random = uint256(keccak256(abi.encode(randomness, randomnessDraw))) % (nftsPercentsCumulated[nftPercentsFilling - 1] + 1);
                    uint256 nftWinnerId = getRandomNft(random);
                    
                    _lastDraw.rewardsByNFTs[nftWinnerId] += giveaway.amountToBeWon[winnersCategory];
                    giveaways[requestId].winners.push(nftWinnerId);
                }
            }

            giveaway.finalized = true;
            giveaways[requestId] = giveaway;
        }
    }

    //TODO make internal after tests
    function getRandomNft(uint256 randomness) public view returns (uint256) {
        unchecked {
            bool found = false;
            uint256 indexMin = 0;
            uint256 indexMax = maxSupply - 1;
            uint256 index = (indexMin + indexMax) / 2;

            while(!found) {
                if(nftsPercentsCumulated[index] >= randomness && (index == 0 || nftsPercentsCumulated[index - 1] < randomness))
                {
                    found = true;
                }
                else
                {
                    if(nftsPercentsCumulated[index] < randomness)
                    {
                        indexMin = index;
                    }
                    else
                    {
                        indexMax = index;
                    }
                    index = (indexMin + indexMax) / 2;
                }
            }

            return found ? index : maxSupply - 1;
        }
    }

    /**
     * @notice Allows owners to recover NFT sent to the contract by mistake
     * @param _token: NFT token address
     * @param _tokenId: tokenId
     * @dev Callable by owner
     */
    function recoverNonFungibleToken(address _token, uint256 _tokenId) external onlyOwner {
        IERC721(_token).transferFrom(address(this), address(msg.sender), _tokenId);
    }

    /**
     * @notice Allows owners to recover tokens sent to the contract by mistake
     * @param _token: token address
     * @dev Callable by owner
     */
    function recoverToken(address _token) external onlyOwner {
        uint256 balance = IERC20(_token).balanceOf(address(this));
        require(balance != 0, "Operations: Cannot recover zero balance");

        IERC20(_token).transferFrom(address(this), address(msg.sender), balance);
    }

    /**
     *  Allows owners to withdraw mint funds
     */
    function withdraw() external onlyOwner {
        teamWallet.transfer(address(this).balance);
    }

    function claimRewards(uint256 tokenId) external payable nonReentrant {
        require(ownerOf(tokenId) == msg.sender, "You need to own this NFT to claim its rewards.");

        uint256 amountToClaim = avalaibleRewardsForNFT(tokenId);
        require(amountToClaim > 0, "No reward to claim");

        _lastDraw.rewardsByNFTs[tokenId] = 0;
        payable(msg.sender).transfer(amountToClaim);
    }

    function _claimManyRewards(uint256[] memory tokens) private {
        uint256 allRewards = 0;
        for(uint i = 0; i < tokens.length; i++) {
            require(ownerOf(tokens[i]) == msg.sender, "You need to own this NFT to claim its rewards.");
            uint256 amountToClaim = avalaibleRewardsForNFT(tokens[i]);
            _lastDraw.rewardsByNFTs[tokens[i]] = 0;
            allRewards += amountToClaim;
        }

        require(allRewards > 0, "No reward to claim");

        payable(msg.sender).transfer(allRewards);
    }

    function claimManyRewards(uint256[] memory tokens) external payable nonReentrant {
        _claimManyRewards(tokens);
    }

    function getSenderTokens() public view returns (uint256[] memory) {
        uint256[] memory tokens = new uint256[](balanceOf(msg.sender));
        uint256 index = 0;
        for(uint i = 0; i < maxSupply; i++) {
            if(_exists(i) && ownerOf(i) == msg.sender)
            {
                tokens[index] = i;
                index++;
            }
        }

        return tokens;
    }

    function claimAllSenderTokens() public payable nonReentrant {
        _claimManyRewards(getSenderTokens());
    }

    function avalaibleRewardsForNFT(uint256 tokenId) public view returns (uint256) {
        return _lastDraw.rewardsByNFTs[tokenId];
    }

    function availableRewardsForSender() public view returns (uint256[] memory, uint256[] memory) {
        uint256[] memory tokens = getSenderTokens();
        uint256[] memory rewards = new uint256[](tokens.length);
        for (uint256 i = 0; i < tokens.length; i++) {
            rewards[i] = avalaibleRewardsForNFT(tokens[i]);
        }
        return (tokens, rewards);
    }

    function percentOfNft(uint tokenId) public view returns (uint256) {
        return nftsPercents[(offsetIndex + tokenId) % maxSupply];
    }

    function getLastGiveaway() public view returns(Giveaway memory) {
        require(giveawaysIds.length > 0, "No giveaway");
        return giveaways[giveawaysIds[giveawaysIds.length - 1]];
    }
}

