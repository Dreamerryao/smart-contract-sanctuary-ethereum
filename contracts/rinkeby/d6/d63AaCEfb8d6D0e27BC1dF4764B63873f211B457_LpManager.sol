pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "OZ/token/ERC20/IERC20.sol";

import "./interfaces/IUniswapV2ERC20.sol";
import "./StakingRewards.sol";

/**
 * @title LpStaking.
 * @notice Upgradeable contract that allows you to stake a UniswapV2ERC20 and obtain a reward in return.
 *
 * It is allowed as an extra option to perform staking using the Uniswap permit function.
 */
contract LpStaking is OwnableUpgradeable{

// VARIABLES V1

    /**
     * @notice reward token address.
     */
    address public rewardToken;

    /**
     * @notice staking token address (UniswapV2).
     */
    address public stakingToken;

    /**
     * @notice staking contract address.
     */
    address public stakingRewards;

// FUNCTIONS V1

    /**
     * @notice Initializer function to set owner rewardToken & stakingToken values.
     *         Also create staking contract.
	 * @param _rewardToken ERC20 reward token address.
	 * @param _stakingToken ERC20 staking token address (UniswapV2).
     */
    function __LpStaking_init(address _rewardToken, address _stakingToken) public initializer {
        __Ownable_init();

        rewardToken = _rewardToken;
        stakingToken = _stakingToken;

        stakingRewards = address(
            new StakingRewards(address(this), _rewardToken, _stakingToken)
        );
    }

    /**
     * @notice Send the rewardToken to the staking contract and start staking or update actual staking.
     *         If it is not called, users will be able to stake tokens but will not be able to claim rewards.
     *
	 * @param _rewardAmount Amount of rewardToken to send to the staking contract.
	 * @param _rewardsDuration Staking event duration.
     */
    function update(uint _rewardAmount, uint256 _rewardsDuration) public onlyOwner {
        require(
            IERC20(rewardToken).transfer(stakingRewards, _rewardAmount),
            'Transfer failed!'
        );
        
        StakingRewards(stakingRewards).notifyRewardAmount(_rewardAmount, _rewardsDuration);
    }

    /**
     * @notice Withdraw ERC20 tokens in the contract to the owner.
     */
    function pullExtraTokens(address _token, uint256 _amount) public onlyOwner {
        IERC20(_token).transfer(msg.sender, _amount);
    }
}

