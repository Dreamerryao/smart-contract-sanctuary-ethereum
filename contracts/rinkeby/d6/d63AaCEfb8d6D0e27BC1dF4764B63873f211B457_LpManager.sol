import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol';
import '@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol';
import "OZ/token/ERC20/IERC20.sol";
import "hardhat/console.sol";
import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol';
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

/***
  @notice Contract that handles adding liquidity for a pool pair of token/ETH
 */

contract Lpcreator is AccessControlUpgradeable{
    //WETH = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    address public constant factory = 0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f;
    address public constant router = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    address public WETH;

    /***
      @notice contract used to test the correct setup of the contract
     */
    function __Lpcreator_init(address _WETH) public initializer{
      WETH = _WETH;
    }

    function sayhello() external returns(string memory){
        return "Hello test";
    }

    /***
      @notice function that gets the tokenaddres of the provided pair
      @dev this function requires 2 tokens, but since only pool pairs of the type token/ETH are implemented
      @param tokenA this value is the token used for the pool pair in this case is DAI
      @param tokenB this value is the token used for the pool pair and is always WETH for this contract
     */
    function getToken(address tokenA, address tokenB) public view returns (address pair){
     return (IUniswapV2Factory(factory).getPair(tokenA,tokenB));
   }

    /***
      @notice payable function that allows the contract to receive the remainder eth used in the
        add liquidity function
     */
    receive() payable external {}
    
    /***
      @notice function that swaps and exact amount of eth for as many tokens as possible
      @param tokenout token output desired
     */
    function swapEthforToken(address tokenout) public payable{
       address[] memory path;
       path = new address[](2);
       path[0] = WETH;
       path[1] = tokenout;
       uint[] memory amounts = IUniswapV2Router02(router).swapExactETHForTokens{value: msg.value}(
         0,
         path,
         msg.sender,
         block.timestamp
       );
        console.log("Amount of eth traded for tokens:", amounts[0]);
    }

    /***
      @notice function that swaps an exact amount of tokens for as many eth as possible
      @param tokenIn address of the token that wants to be swapped
      @param amountIn exact amount of tokens that wants to be swapped for eth
     */
    function swapTokenforEth(address tokenIn, uint amountIn) public{
       address[] memory path;
       path = new address[](2);
       path[0] = tokenIn;
       path[1] = WETH;

       IERC20(tokenIn).transferFrom(msg.sender, address(this), amountIn);
       IERC20(tokenIn).approve(router, amountIn);

       uint[] memory amounts = IUniswapV2Router02(router).swapExactTokensForETH(
         amountIn,
         0,
         path,
         msg.sender,
         block.timestamp
       );
        console.log("Amount of tokens traded for eth", amounts[0]);
    }

    /***
      @notice function that adds the liquidity to the pair pool defined by the provided token and returns
        the amount of tokens, eth and the liquidity minted by the pool
      @param token token used for the lp token creation
      @param amount amount of tokens provided
      @dev in this case since we are using ETH(WETH) the second token is passed using msg.value  
     */
    function addLqdETH(
    address token,
    uint amount) public payable 
    returns (uint amountToken, uint amountETH, uint liquidity){
        IERC20(token).transferFrom(msg.sender, address(this), amount);
        console.log("tokens in contract: ",IERC20(token).balanceOf(address(this)));
        console.log("amount of ether ",msg.value);
        IERC20(token).approve(router, amount);
        
    (uint amountA, uint amountB, uint liquidity) =
      IUniswapV2Router02(router).addLiquidityETH{ value: msg.value }(
        token,
        amount,
        0,
        0,
        address(this),
        block.timestamp
      );
    //console.log("liquidity is: ", liquidity );
    //console.log("balance of lp: ",IERC20(getToken(0x6B175474E89094C44Da98b954EedeAC495271d0F,
    //0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2)).balanceOf(address(this)));
  
  }
    /***
      @notice function that lets the msg.sender remove the dai and eth used to create the lp tokens,
        if the fee gained by the pool pairing isnt enough you can get less tokens when the fee is substracted
      @param _tokenA token used to get lp tokens
      @param _tokenB address of the other token used to get lp tokens, ETH(WETH) address for this contract
     */
  function removeLqdEth(address _tokenA, address _tokenB) public {
    address pair = IUniswapV2Factory(factory).getPair(_tokenA, _tokenB);

    uint liquidity = IERC20(pair).balanceOf(address(this));
    IERC20(pair).approve(router, liquidity);

    (uint amountToken, uint amountETH) = 
      IUniswapV2Router02(router).removeLiquidityETH(
        _tokenA,
        liquidity,
        0,
        0,
        address(this),
        block.timestamp
      );
    //console.log("AmountToken recovered: ",amountToken);
    //console.log("amountETH recovered: ",amountETH);

   }
   //transform dai to eth
}

