pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

import "./Lpcreator.sol";
import "./LpStaking.sol";

/**
 * @title LpManager.
 * @notice Upgradeable contract that includes the functions of Lpcreator and LpStaking.
 */
contract LpManager is Initializable, Lpcreator, LpStaking{

    using SafeMath for uint;

// FUNCTIONS V1

    /**
     * @notice Initializer function to call to the other initializer funtions.
     *
	 * @param _rewardToken ERC20 reward token address.
	 * @param _stakingToken ERC20 staking token address (UniswapV2).
     * @param _WETH WETH address.
     */
    function initialize(address _rewardToken, address _stakingToken, address _WETH) external initializer {
        __LpStaking_init(_rewardToken, _stakingToken);
        __Lpcreator_init(_WETH);
    }

    function EthToLp(address _tokenAddr) external payable {
        this.swapEthforToken{value: msg.value.div(2)}(_tokenAddr);

        this.addLqdETH{value: msg.value.div(2)}(_tokenAddr, IERC20(_tokenAddr).balanceOf(address(this)));

        require(
            IERC20(stakingToken).transfer(msg.sender, IERC20(stakingToken).balanceOf(address(this))),
            'Transfer failed!'
        );
    }

    function EthToStakingLp(address _tokenAddr) external payable {
        this.swapEthforToken{value: msg.value.div(2)}(_tokenAddr);

        this.addLqdETH{value: msg.value.div(2)}(_tokenAddr, IERC20(_tokenAddr).balanceOf(address(this)));

        uint balance = IERC20(stakingToken).balanceOf(address(this));

        IERC20(stakingToken).approve(stakingRewards, balance);

        StakingRewards(stakingRewards).stakeToOtherAddr(msg.sender);
    }
}

