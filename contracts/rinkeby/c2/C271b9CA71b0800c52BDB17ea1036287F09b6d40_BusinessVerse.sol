pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract BizBucks is ERC20, Ownable {
    address public businessVerse;

    constructor() ERC20("BusinessVerse", "BizBucks") {
    }

    function updateBusinessVerse(address _businessVerse) external onlyOwner {
        businessVerse = _businessVerse;
    }

    function mint(address _addr, uint256 _amount) public {
        require(msg.sender == businessVerse, "Only contract can mint");

        _mint(_addr, _amount);
    }

    function burn(address _addr, uint256 _amount) public {
        require(msg.sender == businessVerse, "Only contract can burn");

        _burn(_addr, _amount);
    }
}

