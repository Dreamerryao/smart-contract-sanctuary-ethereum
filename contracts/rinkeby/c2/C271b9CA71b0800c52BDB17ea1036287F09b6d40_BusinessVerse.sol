pragma solidity ^0.8.0;

import "./BizBucks.sol";
import "./Base64.sol";
import "erc721a/contracts/extensions/ERC721AQueryable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

error NotHuman();
error BusinessDoesntExist();
error BusinessTypeDoesntExist();
error BusinessLevelDoesntExist();
error BusinessRarityDoesntExist();
error BusinessModifierDoesntExist();
error MintingNotEnabled();
error IncorrectPayment();
error MustMintAtLeast1();
error NotTheOwner();
error MaxNumberOfWorkersHired();
error NotEnoughBizBucks();
error IsAtMaxLevel();

contract BusinessVerse is ERC721AQueryable, Ownable {
    /* Events */
    event CollectedIncome(uint _businessID, uint _collectedIncome);
    event CollectedMultipleIncome(uint[] _businessIDs, uint _collectedIncome);
    event HiredWorker(uint _businessID, uint _workerCost, uint _newWorkerCount);
    event UpgradedLevel(uint _businessID, uint _levelCost, uint _newLevel);

    /* Addresses */
    BizBucks public bizBucks;

    /* Metadata */
    string private metadataDescription = "BusinessVerse is a business simulator game entirely on the Ethereum blockchain. Players can own businesses, hire workers, and earn $BIZBUCKS.";
    string private metadataExternalURL;
    string private metadataImageBase = "ipfs://QmRs6DiAhZT4y4Ujn61N7HBQBsDvP1arZkqKyizEGVzti5/";
    string private metadataImageExtension = ".png";

    /* Misc stuff */
    bool public mintingEnabled = true;
    uint private constant maxNumLevels = 10; // Number of business levels allocated on chain for each business type, not all need to be used

    /* Structs that define the game structure */
    struct BusinessType {
        bool typeExists;
        uint typeID;
        string typeName;
        uint typeLevel0ID; // Level 0 doesn't exit, add business level to this
        uint typeNumMinted;
        uint typeMintPrice;
    }
    struct BusinessLevel {
        bool levelExists;
        uint levelID;
        uint levelBusinessTypeID;
        string levelName;
        uint levelLevel;
        uint levelUpgradeCost; // Cost to upgrade to this level, not from this level
        uint levelMaxWorkers;
        uint levelIncomeTime;
    }
    struct BusinessRarity {
        bool rarityExists;
        uint rarityID;
        string rarityName;
        uint rarityNumModifiers;
        uint[] rarityPossibleModifierCombinations;
    }
    struct BusinessModifier {
        bool modifierExists;
        uint modifierID;
        string modifierName;
        uint modifierEffectPercent;
    }
    struct Business {
        bool businessExists;
        uint businessID;
        uint businessTypeID;
        uint businessLevel; // Starts at 1
        uint businessNumWorkers; // Starts at 1
        uint businessRarityID;
        uint businessModifierIDs; // Last n bits are modifiers
        uint businessLastCollected; // Starts at mint timestamp
    }

    /* Storages */
    mapping(uint => BusinessType) private businessTypes;
    mapping(uint => BusinessLevel) private businessLevels;
    mapping(uint => BusinessRarity) private businessRarities;
    mapping(uint => BusinessModifier) private businessModifiers;
    mapping(uint => uint) private businessTypesIncomePerWorkerPerTime; // Type ID => Income
    mapping(uint => mapping(uint => uint)) private businessTypesWorkerCosts; // Type ID => Num workers + 1 => Cost
    mapping(uint => Business) private businesses;

    /* Number of items in storages */
    uint private numBusinessTypes;
    uint private numBusinessLevels;
    uint private numBusinessRarities;
    uint private numBusinessModifiers;
    uint private numBusinesses;

    /* Constructor work moved to BusinessVerseInitializer */
    constructor() ERC721A("BusinessVerse", "BUSINESSES") {}

    /* Update addresses */
    function updateBizBucks(address _bizBucks) external onlyOwner {
        bizBucks = BizBucks(_bizBucks);
    }

    /* Update misc stuff */
    function updateMintingEnabled(bool _mintingEnabled) external onlyOwner {
        mintingEnabled = _mintingEnabled;
    }

    /* Modifiers */
    modifier onlyHuman() {
        if(msg.sender != tx.origin) {
            revert NotHuman();
        }
        _;
    }
    modifier businessExists(uint _businessID) {
        Business storage business = businesses[_businessID];
        if(!business.businessExists) {
            revert BusinessDoesntExist();
        }
        _;
    }
    modifier businessTypeExists(uint _typeID) {
        BusinessType storage bType = businessTypes[_typeID];
        if(!bType.typeExists) {
            revert BusinessTypeDoesntExist();
        }
        _;
    }
    modifier businessLevelExists(uint _levelID) {
        BusinessLevel storage bLevel = businessLevels[_levelID];
        if(!bLevel.levelExists) {
            revert BusinessLevelDoesntExist();
        }
        _;
    }
    modifier businessRarityExists(uint _rarityID) {
        BusinessRarity storage bRarity = businessRarities[_rarityID];
        if(!bRarity.rarityExists) {
            revert BusinessRarityDoesntExist();
        }
        _;
    }
    modifier businessModifierExists(uint _modifierID) {
        BusinessModifier storage bModifier = businessModifiers[_modifierID];
        if(!bModifier.modifierExists) {
            revert BusinessModifierDoesntExist();
        }
        _;
    }
    modifier onlyOwnerOfBusiness(uint _businessID) {
        if(ownerOf(_businessID) != msg.sender) {
            revert NotTheOwner();
        }
        _;
    }

    /* Minting */
    function mint(uint _businessTypeID, uint _amount) payable external onlyHuman businessTypeExists(_businessTypeID) {
        if(!mintingEnabled) {
            revert MintingNotEnabled();
        }

        BusinessType storage bType = businessTypes[_businessTypeID];
        if(msg.value < bType.typeMintPrice * _amount) {
            revert IncorrectPayment();
        }

        doMint(_businessTypeID, msg.sender, _amount);
    }
    function mintAdmin(uint _businessTypeID, address _to, uint _amount) external onlyOwner onlyHuman businessTypeExists(_businessTypeID) {
        doMint(_businessTypeID, _to, _amount);
    }
    function doMint(uint _businessTypeID, address _to, uint _amount) private businessTypeExists(_businessTypeID) {
        if(_amount == 0) {
            revert MustMintAtLeast1();
        }

        for(uint i = 1; i <= _amount; i++) {
            uint businessID = numBusinesses + i;
            createBusiness(businessID, _businessTypeID);
        }
        numBusinesses += _amount;
        _mint(_to, _amount);
    }

    /* Game struct creation and modification */
    function createBusinessType(string memory _typeName, uint _typeMintPrice) public onlyOwner returns(uint) {
        numBusinessTypes++;
        uint typeID = numBusinessTypes;

        // Create business levels
        for(uint i = 1; i <= maxNumLevels; i++) {
            uint levelID = numBusinessLevels + i;
            businessLevels[levelID] = BusinessLevel(
                false, // Exists
                levelID, // ID
                typeID, // Business type ID
                "", // Name
                i, // Level
                0, // Upgrade cost
                0, // Max workers
                0 // Income time
            );
        }

        // Create business type
        businessTypes[typeID] = BusinessType(
            true, // Exists
            typeID, // ID
            _typeName, // Name
            numBusinessLevels, // Level 0 ID
            0, // Num minted
            _typeMintPrice // Mint price
        );

        numBusinessLevels += maxNumLevels;

        return typeID;
    }
    function editBusinessType(uint _typeID, bool _typeExists, string memory _typeName, uint _typeMintPrice) public onlyOwner {
        businessTypes[_typeID].typeExists = _typeExists;
        businessTypes[_typeID].typeName = _typeName;
        businessTypes[_typeID].typeMintPrice = _typeMintPrice;
    }
    function editBusinessLevel(uint _levelID, bool _levelExists, string memory _levelName, uint _levelUpgradeCost, uint _levelMaxWorkers, uint _levelIncomeTime) public onlyOwner {
        businessLevels[_levelID].levelExists = _levelExists;
        businessLevels[_levelID].levelName = _levelName;
        businessLevels[_levelID].levelUpgradeCost = _levelUpgradeCost;
        businessLevels[_levelID].levelMaxWorkers = _levelMaxWorkers;
        businessLevels[_levelID].levelIncomeTime = _levelIncomeTime;
    }
    function createBusinessRarity(string memory _rarityName, uint _rarityNumModifiers) public onlyOwner {
        numBusinessRarities++;
        uint rarityID = numBusinessRarities;

        uint[] memory possibleModifierCombinations;

        businessRarities[rarityID] = BusinessRarity(
            true, // Exists
            rarityID, // ID
            _rarityName, // Name
            _rarityNumModifiers, // Num modifiers
            possibleModifierCombinations // Possible modifier combinations
        );
    }
    function editBusinessRarityPossibleModifierCombinations(uint _rarityID, uint[] memory _rarityPossibleModifierCombinations) public onlyOwner {
        businessRarities[_rarityID].rarityPossibleModifierCombinations = _rarityPossibleModifierCombinations;
    }
    function editBusinessRarity(uint _rarityID, bool _rarityExists, string memory _rarityName, uint _rarityNumModifiers) public onlyOwner {
        businessRarities[_rarityID].rarityExists = _rarityExists;
        businessRarities[_rarityID].rarityName = _rarityName;
        businessRarities[_rarityID].rarityNumModifiers = _rarityNumModifiers;
    }
    function createBusinessModifier(string memory _modifierName, uint _modifierEffectPercent) public onlyOwner {
        numBusinessModifiers++;
        uint modifierID = numBusinessModifiers;

        businessModifiers[modifierID] = BusinessModifier(
            true, // Exists
            modifierID, // ID
            _modifierName, // Name
            _modifierEffectPercent // Effect percent
        );
    }
    function editBusinessModifier(uint _modifierID, bool _modifierExists, string memory _modifierName, uint _modifierEffectPercent) public onlyOwner {
        businessModifiers[_modifierID].modifierExists = _modifierExists;
        businessModifiers[_modifierID].modifierName = _modifierName;
        businessModifiers[_modifierID].modifierEffectPercent = _modifierEffectPercent;
    }
    function editBusinessTypeIncomePerWorkerPerTime(uint _businessTypeID, uint _incomePerWorkerPerTime) public onlyOwner {
        businessTypesIncomePerWorkerPerTime[_businessTypeID] = _incomePerWorkerPerTime;
    }
    function editBusinessTypeWorkerCost(uint _businessTypeID, uint _businessNumWorkers, uint _cost) public onlyOwner {
        businessTypesWorkerCosts[_businessTypeID][_businessNumWorkers] = _cost;
    }
    function createBusiness(uint _businessID, uint _businessTypeID) private businessTypeExists(_businessTypeID) {
        uint rarityID = generateRarityID(_businessID);
        uint modifierIDs = generateModifierIDs(_businessID, rarityID);

        businesses[_businessID] = Business(
            true, // Exists
            _businessID, // ID
            _businessTypeID, // Type ID
            1, // Level
            1, // Num workers
            rarityID, // Rarity ID
            modifierIDs, // Modifier IDs
            block.timestamp // Last collected
        );

        businessTypes[_businessTypeID].typeNumMinted++;
    }

    /* Game struct getters */
    function getBusinessType(uint _typeID) external view businessTypeExists(_typeID) returns(BusinessType memory) {
        return businessTypes[_typeID];
    }
    function getBusinessLevelByLevelID(uint _levelID) external view businessLevelExists(_levelID) returns(BusinessLevel memory) {
        return businessLevels[_levelID];
    }
    function getBusinessLevelByBusinessID(uint _businessID) external view businessExists(_businessID) returns(BusinessLevel memory) {
        return businessLevels[getLevelIDOfBusiness(_businessID)];
    }
    function getBusinessRarity(uint _rarityID) external view businessRarityExists(_rarityID) returns(BusinessRarity memory) {
        return businessRarities[_rarityID];
    }
    function getBusinessModifier(uint _modifierID) external view businessModifierExists(_modifierID) returns(BusinessModifier memory) {
        return businessModifiers[_modifierID];
    }
    function getBusinessTypeIncomePerWorkerPerTime(uint _typeID) external view businessTypeExists(_typeID) returns(uint) {
        return businessTypesIncomePerWorkerPerTime[_typeID];
    }
    function getBusinessTypeWorkerCost(uint _typeID, uint _numWorkers) external view businessTypeExists(_typeID) returns(uint) {
        return businessTypesWorkerCosts[_typeID][_numWorkers];
    }
    function getBusiness(uint _businessID) external view businessExists(_businessID) returns(Business memory) {
        return businesses[_businessID];
    }

    /* Helper functions */
    function generateRarityID(uint _businessID) private pure returns(uint) { // Maybe you will be rewarded for figuring out how this works?
        if(_businessID % 20 == 0) { // 5% Legendary
            return 4;
        }
        else if(_businessID % 5 == 0) { // 15% Epic
            return 3;
        }
        else if(_businessID % 3 == 0) { // 26.7% Rare
            return 2;
        }
        // 53.3% Common
        return 1;
    }
    function generateModifierIDs(uint _businessID, uint _rarityID) private view returns(uint) {
        BusinessRarity storage bRarity = businessRarities[_rarityID];
        return bRarity.rarityPossibleModifierCombinations[_businessID % bRarity.rarityPossibleModifierCombinations.length];
    }
    function getBusinessHasModifier(uint _businessID, uint _modifierID) public view businessExists(_businessID) businessModifierExists(_modifierID) returns(bool) {
        Business storage business = businesses[_businessID];
        if((business.businessModifierIDs >> _modifierID) & 1 == 1) {
            return true;
        }
        return false;
    }
    function getLevelIDOfBusiness(uint _businessID) public view businessExists(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];
        BusinessType storage bType = businessTypes[business.businessTypeID];
        return bType.typeLevel0ID + business.businessLevel;
    }

    /* Main gameplay */
    function getIncomePerTimePrivate(uint _businessTypeID, uint _businessNumWorkers) private view returns(uint) {
        uint incomePerTime = businessTypesIncomePerWorkerPerTime[_businessTypeID] * _businessNumWorkers;
        // uint modifiersPercent = 0;
        // if(getBusinessHasModifier(_businessID, 1)) {
        //     modifiersPercent += businessModifiers[1].modifierEffectPercent;
        // }
        // if(getBusinessHasModifier(_businessID, 2)) {
        //     modifiersPercent += businessModifiers[2].modifierEffectPercent;
        // }
        // if(modifiersPercent != 0) {
        //     incomePerTime = (incomePerTime * (100 + modifiersPercent)) / 100;
        // }
        return incomePerTime;
    }
    function getIncomePerTimePublic(uint _businessID) public view businessExists(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];
        return getIncomePerTimePrivate(business.businessTypeID, business.businessNumWorkers);
    }
    function getIncomeTime(uint _businessID) public view businessExists(_businessID) returns(uint) {
        BusinessLevel storage bLevel = businessLevels[getLevelIDOfBusiness(_businessID)];
        return bLevel.levelIncomeTime;
    }
    function getUncollectedTimes(uint _businessID) public view businessExists(_businessID) returns(uint, uint) {
        Business storage business = businesses[_businessID];
        uint incomeTime = getIncomeTime(_businessID);
        uint secondsSinceLastCollect = block.timestamp - business.businessLastCollected;
        uint uncollectedTimes = secondsSinceLastCollect / incomeTime;
        uint newLastCollected = business.businessLastCollected + uncollectedTimes * incomeTime;
        return (uncollectedTimes, newLastCollected);
    }
    function getIncomePerTimeOld(uint _businessID) public view businessExists(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];
        uint incomePerTime = businessTypesIncomePerWorkerPerTime[business.businessTypeID] * business.businessNumWorkers;
        return incomePerTime;
    }
    function collectIncomeOld(uint _businessID) public onlyHuman businessExists(_businessID) onlyOwnerOfBusiness(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];

        uint incomePerTime = getIncomePerTimeOld(_businessID);
        (uint uncollectedTimes, uint newLastCollected) = getUncollectedTimes(_businessID);
        uint income = incomePerTime * uncollectedTimes;
        businesses[_businessID].businessLastCollected = newLastCollected;

        bizBucks.mint(msg.sender, income);
        emit CollectedIncome(_businessID, income);
        return income;
    }
    function collectIncome(uint _businessID) public onlyHuman businessExists(_businessID) onlyOwnerOfBusiness(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];

        uint incomePerTime = getIncomePerTimePrivate(business.businessTypeID, business.businessNumWorkers);
        (uint uncollectedTimes, uint newLastCollected) = getUncollectedTimes(_businessID);
        uint income = incomePerTime * uncollectedTimes;
        businesses[_businessID].businessLastCollected = newLastCollected;

        bizBucks.mint(msg.sender, income);
        emit CollectedIncome(_businessID, income);
        return income;
    }
    function collectMultipleIncome(uint[] memory _businessIDs) external onlyHuman returns(uint){
        uint income = 0;
        for(uint i = 0; i < _businessIDs.length; i++) {
            uint businessID = _businessIDs[i];

            Business storage business = businesses[businessID];
            if(!business.businessExists) {
                revert BusinessDoesntExist();
            }
            if(ownerOf(businessID) != msg.sender) {
                revert NotTheOwner();
            }

            uint incomePerTime = getIncomePerTimePrivate(business.businessTypeID, business.businessNumWorkers);
            (uint uncollectedTimes, uint newLastCollected) = getUncollectedTimes(businessID);
            income += incomePerTime * uncollectedTimes;
            businesses[businessID].businessLastCollected = newLastCollected;
        }

        bizBucks.mint(msg.sender, income);
        emit CollectedMultipleIncome(_businessIDs, income);
        return income;
    }
    function getMaxWorkersForThisLevelByBusinessID(uint _businessID) public view businessExists(_businessID) returns(uint) {
        BusinessLevel storage bLevel = businessLevels[getLevelIDOfBusiness(_businessID)];
        return bLevel.levelMaxWorkers;
    }
    function getMaxWorkersForThisLevelByLevelID(uint _levelID) private view returns(uint) {
        BusinessLevel storage bLevel = businessLevels[_levelID];
        return bLevel.levelMaxWorkers;
    }
    function getIsAtMaxWorkersForThisLevel(uint _businessID) public view businessExists(_businessID) returns(bool) {
        Business storage business = businesses[_businessID];
        if(business.businessNumWorkers >= getMaxWorkersForThisLevelByBusinessID(_businessID)) {
            return true;
        }
        return false;
    }
    function getNextWorkerCost(uint _businessID) public view businessExists(_businessID) returns(uint) {
        Business storage business = businesses[_businessID];
        uint nextWorkerCost = businessTypesWorkerCosts[business.businessTypeID][business.businessNumWorkers + 1];
        // uint modifiersPercent = 0;
        // if(getBusinessHasModifier(_businessID, 3)) {
        //     modifiersPercent += businessModifiers[3].modifierEffectPercent;
        // }
        // if(modifiersPercent != 0) {
        //     nextWorkerCost = (nextWorkerCost * (100 - modifiersPercent)) / 100;
        // }
        return nextWorkerCost;
    }
    function hireWorker(uint _businessID) external onlyHuman businessExists(_businessID) onlyOwnerOfBusiness(_businessID) returns(uint, uint) {
        if(getIsAtMaxWorkersForThisLevel(_businessID)) {
            revert MaxNumberOfWorkersHired();
        }

        uint nextWorkerCost = getNextWorkerCost(_businessID);
        if(nextWorkerCost == 0) {
            revert MaxNumberOfWorkersHired();
        }

        uint collectedIncome = collectIncome(_businessID);

        if(bizBucks.balanceOf(msg.sender) < nextWorkerCost) {
            revert NotEnoughBizBucks();
        }

        businesses[_businessID].businessNumWorkers++;
        bizBucks.burn(msg.sender, nextWorkerCost);
        emit HiredWorker(_businessID, nextWorkerCost, businesses[_businessID].businessNumWorkers);
        return (collectedIncome, nextWorkerCost);
    }
    function getIsAtMaxLevel(uint _businessID) public view businessExists(_businessID) returns(bool) {
        Business storage business = businesses[_businessID];

        if(business.businessLevel >= maxNumLevels) {
            return true;
        }

        BusinessLevel storage nextLevel = businessLevels[getLevelIDOfBusiness(_businessID) + 1];
        return !nextLevel.levelExists;
    }
    function getNextLevelCost(uint _businessID) public view businessExists(_businessID) returns(uint) {
        if(getIsAtMaxLevel(_businessID)) {
            revert IsAtMaxLevel();
        }

        BusinessLevel storage nextLevel = businessLevels[getLevelIDOfBusiness(_businessID) + 1];
        uint nextLevelCost = nextLevel.levelUpgradeCost;
        // uint modifiersPercent = 0;
        // if(getBusinessHasModifier(_businessID, 4)) {
        //     modifiersPercent += businessModifiers[4].modifierEffectPercent;
        // }
        // if(modifiersPercent != 0) {
        //     nextLevelCost = (nextLevelCost * (100 - modifiersPercent)) / 100;
        // }
        return nextLevelCost;
    }
    function getNextLevelCostWithoutReverting(uint _businessID) private view businessExists(_businessID) returns(uint) {
        if(getIsAtMaxLevel(_businessID)) {
            return 0;
        }
        return getNextLevelCost(_businessID);
    }
    function upgradeLevel(uint _businessID) external onlyHuman businessExists(_businessID) onlyOwnerOfBusiness(_businessID) returns(uint, uint) {
        if(getIsAtMaxLevel(_businessID)) {
            revert IsAtMaxLevel();
        }

        uint collectedIncome = collectIncome(_businessID);

        uint nextLevelCost = getNextLevelCost(_businessID);
        if(bizBucks.balanceOf(msg.sender) < nextLevelCost) {
            revert NotEnoughBizBucks();
        }

        businesses[_businessID].businessLevel++;
        bizBucks.burn(msg.sender, nextLevelCost);
        emit UpgradedLevel(_businessID, nextLevelCost, businesses[_businessID].businessLevel);
        return (collectedIncome, nextLevelCost);
    }
    // function getBusinessInfo(uint _businessID) external view businessExists(_businessID) returns(Business memory, string memory, uint, uint, uint, uint, uint) {
    function getBusinessInfo(uint _businessID) external view businessExists(_businessID) returns(Business memory, string memory, uint, uint, uint, uint) {
        BusinessLevel storage bLevel = businessLevels[getLevelIDOfBusiness(_businessID)];
        // return (businesses[_businessID], bLevel.levelName, bLevel.levelIncomeTime, getIncomePerTimePublic(_businessID), getMaxWorkersForThisLevelByLevelID(bLevel.levelID), getNextWorkerCost(_businessID), getNextLevelCostWithoutReverting(_businessID));
        return (businesses[_businessID], bLevel.levelName, bLevel.levelIncomeTime, getIncomePerTimePublic(_businessID), getMaxWorkersForThisLevelByLevelID(bLevel.levelID), getNextWorkerCost(_businessID));
    }

    /* Withdraw funds */
    function withdraw() external onlyOwner {
        uint balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    /* URI-related functions */
    function setMetadataDescription(string memory _metadataDescription) external onlyOwner {
        metadataDescription = _metadataDescription;
    }
    function setMetadataExternalURL(string memory _metadataExternalURL) external onlyOwner {
        metadataExternalURL = _metadataExternalURL;
    }
    function setMetadataImageBase(string memory _metadataImageBase) external onlyOwner {
        metadataImageBase = _metadataImageBase;
    }
    function setMetadataImageExtension(string memory _metadataImageExtension) external onlyOwner {
        metadataImageExtension = _metadataImageExtension;
    }

    /* ERC721A overrides */
    function _startTokenId() internal pure override returns(uint256) {
        return 1;
    }
