contract MyToken is Ownable {
    IRouter02 public v2Router;
    IERC20 immutable Emaxl2 = IERC20(0x0165b733e860b1674541BB7409f8a4743A564157);
    uint256 slippageDivisor = 1000;
    uint8 addingLp;

    constructor(){
        IRouter02 _v2Router= IRouter02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        v2Router = _v2Router;
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount, uint256 slippage, uint256 deadline) public returns(bool) {
        require(addingLp == 0, "reentrant");
        addingLp = 1;
        //transfer l2EMAX from holder to contract required aproval
        Emaxl2.transferFrom(msg.sender, address(this), tokenAmount);
        //approve the router to spend these tokens
        Emaxl2.approve(address(v2Router), tokenAmount);
        //calculate amount out min
        uint256 tokenAmountOutMinimmun = tokenAmount * slippage * slippageDivisor;
        uint256 ethAmountOutMinimun = ethAmount * slippage * slippageDivisor;
        // add the liquidity
        v2Router.addLiquidityETH{value: ethAmount}(
            address(Emaxl2),
            tokenAmount,
            tokenAmountOutMinimmun, // slippage
            ethAmountOutMinimun, // slippage
            msg.sender,
            deadline
        );
        addingLp = 0;
        return true;
    }

    receive() external payable {}
}