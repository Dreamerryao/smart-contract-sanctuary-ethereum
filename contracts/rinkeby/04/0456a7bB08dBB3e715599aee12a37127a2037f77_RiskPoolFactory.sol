pragma solidity 0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./RiskPoolERC20.sol";
import "./interfaces/IRiskPool.sol";
import "./libraries/TransferHelper.sol";
import "hardhat/console.sol";

contract RiskPool is IRiskPool, RiskPoolERC20 {
    // ERC20 attributes
    string public name;
    string public symbol;

    address public masterChef;
    address public override currency; // for now we should accept only UNO
    uint256 public override lpPrice;

    event LogCancelWithdrawRequest(address indexed _user, uint256 _amount, uint256 _amountInUno);
    event LogPolicyClaim(address indexed _user, uint256 _amount);

    constructor(
        string memory _name,
        string memory _symbol,
        address _masterChef,
        address _currency
    ) {
        name = _name;
        symbol = _symbol;
        masterChef = _masterChef;
        currency = _currency;
        lpPrice = 1e18;
    }

    modifier onlyMasterChef() {
        require(msg.sender == masterChef, "UnoRe: RiskPool Forbidden");
        _;
    }

    /**
     * @dev Users can stake only through Cohort
     */
    function enter(address _from, uint256 _amount) external override onlyMasterChef {
        _mint(_from, (_amount * 1e18) / lpPrice);
    }

    /**
     * @param _amount UNO amount to withdraw
     */
    function leaveFromPoolInPending(address _to, uint256 _amount) external override onlyMasterChef {
        require(totalSupply() > 0, "UnoRe: There's no remaining in the pool");
        uint256 requestAmountInLP = (_amount * 1e18) / lpPrice;
        require(
            (requestAmountInLP + uint256(withdrawRequestPerUser[_to].pendingAmount)) <= balanceOf(_to),
            "UnoRe: lp balance overflow"
        );
        _withdrawRequest(_to, requestAmountInLP, _amount);
    }

    function leaveFromPending(address _to) external override onlyMasterChef {
        uint256 cryptoBalance = IERC20(currency).balanceOf(address(this));
        require(cryptoBalance > 0, "UnoRe: zero uno balance");
        require(balanceOf(_to) > uint256(withdrawRequestPerUser[_to].pendingAmount), "UnoRe: lp balance overflow");
        uint256 pendingAmountInUno = (uint256(withdrawRequestPerUser[_to].pendingAmount) * lpPrice) / 1e18;
        if (pendingAmountInUno < cryptoBalance) {
            TransferHelper.safeTransfer(currency, _to, pendingAmountInUno);
        } else {
            TransferHelper.safeTransfer(currency, _to, cryptoBalance);
        }
        _withdrawImplement(_to);
    }

    function cancelWithrawRequest(address _to) external override onlyMasterChef {
        uint256 _pendingAmount = uint256(withdrawRequestPerUser[_to].pendingAmount);
        require(_pendingAmount > 0, "UnoRe: zero amount");
        _cancelWithdrawRequest(_to);
        emit LogCancelWithdrawRequest(_to, _pendingAmount, (_pendingAmount * lpPrice) / 1e18);
    }

    function policyClaim(address _to, uint256 _amount) external override onlyMasterChef {
        uint256 cryptoBalance = IERC20(currency).balanceOf(address(this));
        require(cryptoBalance > 0 && totalSupply() > 0, "UnoRe: zero uno and lp balance");
        if (cryptoBalance > _amount) {
            TransferHelper.safeTransfer(currency, _to, _amount);
            emit LogPolicyClaim(_to, _amount);
        } else {
            TransferHelper.safeTransfer(currency, _to, cryptoBalance);
            emit LogPolicyClaim(_to, cryptoBalance);
        }
        cryptoBalance = IERC20(currency).balanceOf(address(this));
        lpPrice = (cryptoBalance * 1e18) / totalSupply(); // UNO value per lp
    }

    function getWithdrawRequest(address _to)
        external
        view
        override
        onlyMasterChef
        returns (
            uint256,
            uint256,
            uint256
        )
    {
        return (
            uint256(withdrawRequestPerUser[_to].pendingAmount),
            uint256(withdrawRequestPerUser[_to].requestTime),
            withdrawRequestPerUser[_to].pendingUno
        );
    }

    function getTotalWithdrawRequestAmount() external view override onlyMasterChef returns (uint256) {
        return totalWithdrawPending;
    }
}

