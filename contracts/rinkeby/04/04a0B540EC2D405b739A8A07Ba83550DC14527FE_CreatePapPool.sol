pragma solidity ^0.8.0;

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "hardhat/console.sol";

interface IPapStaking {
    function UserInfo(address user) external view returns (uint,uint, uint);
}

contract PAPpool{

    using EnumerableSet for EnumerableSet.AddressSet;

    // Declare a set state variable
    EnumerableSet.AddressSet private addressSet;

    address public owner;
//    bool public poolStarted;
    uint public nextAtIndex;

    bool public poolStarted;
    bool public poolEnded;

    IERC20 lotteryToken;
    IERC20 poolToken;

    IPapStaking papStaking;

    // Pool infomation
    struct Pool{
        uint poolID;
        uint timeStart;
        uint timeEnd;
        uint minAllocation;
        uint maxAllocation;
        uint tokensAmount; // TODO: Remove
        uint tokenPrice; // TODO: Rename -> IDO Swap amount (price) e.g.
        bool poolEnded;
    }
    struct participationRegistration{
        uint participationId;
        bool participate;
    }

    struct infoParticipants{
        address _participationAddress;
        uint _participatedAmount;
        uint _tier;
        bool _didWin;
        bool _claimedBack;
    }

    // Mapping string with Pool's Information
    mapping(string => Pool) public pool;

    // Mapping to check user's participation Information
    mapping(uint => infoParticipants) public participationBook;

    // Participation index;
    mapping(address => participationRegistration) public didParticipate;

    // participation Array of addresses
    address[] public participationAddress;

    constructor (Pool memory _Tier1, Pool memory _Tier2, Pool memory _Tier3, address _owner, address _poolTokenAddress, address _lotteryTokenAddress, address _papStakingAddress, uint _tokenAmountT1, uint _tokenAmountT2, uint _tokenAmountT3) {
        // Owner of the contract
        owner = _owner;

        // DAITokenInfo
        lotteryToken = IERC20(_lotteryTokenAddress);

        // poolTokenInfo
        poolToken = IERC20(_poolTokenAddress);
        papStaking = IPapStaking(_papStakingAddress);

        pool['tierOne'] = Pool({poolID: _Tier1.poolID,
                                    timeStart: _Tier1.timeStart,
                                    timeEnd: _Tier1.timeEnd,
                                    minAllocation: _Tier1.minAllocation,
                                    maxAllocation: _Tier1.maxAllocation,
                                    tokensAmount: _tokenAmountT1,
                                    tokenPrice: _Tier1.tokenPrice,
                                    poolEnded: false});

        pool['tierTwo'] = Pool({poolID: _Tier2.poolID,
                                    timeStart: _Tier2.timeStart,
                                    timeEnd: _Tier2.timeEnd,
                                    minAllocation: _Tier2.minAllocation,
                                    maxAllocation: _Tier2.maxAllocation,
                                    tokensAmount: _tokenAmountT2,
                                    tokenPrice: _Tier2.tokenPrice,
                                    poolEnded: false});

        pool['tierThree'] = Pool({poolID: _Tier3.poolID,
                                    timeStart: _Tier3.timeStart,
                                    timeEnd: _Tier3.timeEnd,
                                    minAllocation: _Tier3.minAllocation,
                                    maxAllocation: _Tier3.maxAllocation,
                                    tokensAmount: _tokenAmountT3,
                                    tokenPrice: _Tier3.tokenPrice,
                                    poolEnded: false});

//        poolStarted = true;
    }

    modifier onlyOwner() {
        require(msg.sender == owner, 'Restricted only to owner');
        _;
    }

    function _getTierNumber(uint _tierNumber) private pure returns(string memory){
        string memory _tierIdentifier;
        if(_tierNumber == 1){_tierIdentifier = 'tierOne';}
        if(_tierNumber == 2){_tierIdentifier = 'tierTwo';}
        if(_tierNumber == 3){_tierIdentifier = 'tierThree';}
        return _tierIdentifier;
    }

    // function starting Pool
    function startPool(uint _TierNumber) public onlyOwner {
        // getting Amount to tansfer;
        string memory _tier = _getTierNumber(_TierNumber);
        require(poolEnded == false, 'pool already ended');
        require(poolStarted == false, 'pool already started');
        require(pool[_tier].timeStart >= block.timestamp,'To late to set');
        uint _idoToDeposit = pool[_tier].tokensAmount * pool[_tier].tokenPrice;
        lotteryToken.transferFrom(msg.sender,address(this),_idoToDeposit);
        poolStarted = true;
    }

    function participate(uint _amount) public {
        // Retrieving Information about participant and pool
        (uint _amountStaked, ,uint _tierNumber) = papStaking.UserInfo(msg.sender);
        require(_amountStaked != 0,"You don't have any staked amount");
        require(_tierNumber > 0, "You don't valid tier");

        string memory _tier = _getTierNumber(_tierNumber);

        uint _poolEndTime = pool[_tier].timeEnd;
        uint _poolStartTime = pool[_tier].timeStart;

        uint _maxParticipationAmount = pool[_tier].maxAllocation;
        uint _minParticipationAmount = pool[_tier].minAllocation;

        require(block.timestamp >=_poolStartTime, 'Pool not started');
        require(_poolEndTime >= block.timestamp, 'Pool already Ended');

        require(_maxParticipationAmount >= _amount && _amount >=_minParticipationAmount, "Participation is out of range");

        require(didParticipate[msg.sender].participate == false, 'You have already participated');

        poolToken.transferFrom(msg.sender,address(this),_amount);

        // Updating participationBook information
        participationBook[nextAtIndex]._participationAddress = msg.sender;
        participationBook[nextAtIndex]._participatedAmount = _amount;

        // Updating participation status
        didParticipate[msg.sender].participationId = nextAtIndex;
        didParticipate[msg.sender].participate = true;
        //Adding to addressSet
        addressSet.add(msg.sender);
        //adding msg.senders to address book
        participationAddress.push(msg.sender);
        nextAtIndex ++;
    }

    function getParticipationAddress() view public returns(address[] memory){
        return participationAddress;
    }


    function _generateRandom(uint256 range) internal view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty))) % range;
    }

    // declaring winners w.r.t pools
    function declareWinner(uint _tierNumber) public onlyOwner {

        string memory _tier = _getTierNumber(_tierNumber);

        uint amountOfDAIsToClaim;

        // Only when poolEnds
        require(block.timestamp >= pool[_tier].timeEnd, 'PoolTime not ended yet');

        // Numbers of winners = TokenAmount / maxAllocation
        uint _numberOfWinners = pool[_tier].tokensAmount / pool[_tier].maxAllocation;
//        console.log('_numberOfWinners',_numberOfWinners);
//        console.log('participation Length',addressSet.length());
//
//
//        console.log('participation Length',addressSet.remove(0x70997970C51812dc3A010C7d01b50e0d17dc79C8));
//        console.log('participation Length',addressSet.length());
//
//        console.log('participation Length',addressSet.remove(0x90F79bf6EB2c4f870365E785982E1f101E93b906));
//        console.log('participation Length',addressSet.length());

        uint numberOfParticipants = participationAddress.length;
        for (uint i=0; i < numberOfParticipants; i++ ){
            if(i < _numberOfWinners){
                uint _randomIndex = _generateRandom(addressSet.length());
                // getting address at that index in addressSet
                address _selectedAddress = addressSet.at(_randomIndex);

//                getting Index of that selected address
                uint _winnerAddressId = didParticipate[_selectedAddress].participationId;
                amountOfDAIsToClaim = amountOfDAIsToClaim + participationBook[_winnerAddressId]._participatedAmount;

                participationBook[_winnerAddressId]._didWin = true;

//               Popping winning address
                addressSet.remove(_selectedAddress);
            }
        }

        // leave awards to claim (in DAI) here and send the remaining balance of DAI back to Owner
        // dai to return = balance(dai) - (amountOfDAIsToClaim * swapprice)
        uint dai_return = lotteryToken.balanceOf(address(this))-(amountOfDAIsToClaim * pool[_tier].tokenPrice);
//        console.log(lotteryToken.balanceOf(msg.sender));
        lotteryToken.transfer(msg.sender,dai_return);
//        console.log(lotteryToken.balanceOf(msg.sender));

//      claim DFTY Token
//        console.log('DFTY in Contract', poolToken.balanceOf(address(this)));
//        console.log('But DFTY pool creator can claim', amountOfDAIsToClaim);
//        console.log('IDO Token in pool', lotteryToken.balanceOf(address(this)));
        poolToken.transfer(msg.sender,amountOfDAIsToClaim);
//        console.log('DFTY in to return (remaining)', poolToken.balanceOf(address(this)));
        poolEnded = true;
    }

    function claimWinToken() public {
        require(poolEnded == true,'To early to claim');
//      checking if it is winner
//      getting msg.sender if pariticpated or not
        require(didParticipate[msg.sender].participate == true,"You did not participate");
//      getting participation ID
        uint Id = didParticipate[msg.sender].participationId;
//        console.log('Id',Id);
//      checking winner or not:
        require(participationBook[Id]._didWin == true, 'You Did not win');
        require(participationBook[Id]._claimedBack == false, 'You already claimed');
        require(participationBook[Id]._participationAddress == msg.sender, 'You cannot claim');

        (, ,uint _tierNumber) = papStaking.UserInfo(msg.sender);

        string memory _tier = _getTierNumber(_tierNumber);
//      claiming amount amoutput * swap price
        uint amountWon = participationBook[Id]._participatedAmount * pool[_tier].tokenPrice;
//        console.log('participationBook[Id]._participatedAmount',pool[_tier].tokenPrice);

        lotteryToken.transfer(msg.sender,amountWon);
//        console.log(lotteryToken.balanceOf(address(this)));
//        console.log(amountWon);

        participationBook[Id]._claimedBack = true;
    }

    function claimDFTY() public {
        require(poolEnded == true,'To early to claim');
        //      checking if it is winner
        //      getting msg.sender if pariticpated or not
        require(didParticipate[msg.sender].participate == true,"You did not participate");

        //      getting participation ID
        uint participationId = didParticipate[msg.sender].participationId;
        //      checking winner or not:
        require(participationBook[participationId]._claimedBack == false, 'You already claimed');
        require(participationBook[participationId]._didWin == false, 'You Won');
        require(participationBook[participationId]._participationAddress == msg.sender, 'You cannot claim');
        //      claiming amount amoutput * swap price
        uint refundamount = participationBook[participationId]._participatedAmount;
//        console.log(poolToken.balanceOf(address(this)));
        poolToken.transfer(msg.sender,refundamount);
        participationBook[participationId]._claimedBack = true;
//        console.log(poolToken.balanceOf(address(this)));
    }

}


contract CreatePapPool is Ownable {

    struct Counter{
        uint counter;
    }

    PAPpool[] public papAddressPool;
    address public papStakingAddress;

    // Indexing for each user separate PAP contract addresses
    mapping(address => Counter) public NextCounterAt;
    // For individual pap address, there is index(incremented) that stores address
    mapping (address => mapping(uint => address)) public OwnerPoolBook;
    // Array that will store all the PAP addresses, regardless of Owner

    event papStakingChanged(address newPapStaking, uint256 time);

    constructor(address _papStaking){
        transferOwnership(msg.sender);
        setPapStaking(_papStaking);
    }

    mapping (address => bool) public poolAdmin;

    function setPapStaking(address _papStaking) public onlyOwner{
        require(_papStaking != address(0), "Can't set 0x0 address!");
        papStakingAddress = _papStaking;
        emit papStakingChanged(_papStaking, block.timestamp);
    }

    function setAdmin(address _address) public onlyOwner {
        require(poolAdmin[_address] == false,'Already Admin');
        poolAdmin[_address] = true;
    }

    function revokeAdmin(address _address) public onlyOwner {
        require(poolAdmin[_address] == true,'Not Admin');
        poolAdmin[_address] = false;
    }

    modifier isAdmin() {
        require(poolAdmin[msg.sender] == true,'restricted to Admins!');
        _;
    }

    /*
    @dev: function to allocate information as per pool, this function can be called once. It will create all tire at once.
    @Params _Tier: Array of parameter(from front end), that is passed as transaction that will create pool
    */
    function createPool(PAPpool.Pool memory _Tier1, PAPpool.Pool memory _Tier2, PAPpool.Pool memory _Tier3, address _lotteryTokenAddress, address _poolTokenAddress) external isAdmin {

        // checking Parameters length
//        require(_Tier1.length == 6 && _Tier2.length == 6 && _Tier3.length == 6, 'Wrong Tier Parameters');
        // StartTime to be more than endTime
        require(_Tier1.timeStart <=_Tier1.timeEnd && _Tier2.timeStart <=_Tier2.timeEnd && _Tier3.timeStart <=_Tier3.timeEnd,'EndTime to be more than startTime');
        // _timeStart Cannot be in past
        require(_Tier1.timeStart >=block.timestamp && _Tier2.timeStart >=block.timestamp && _Tier3.timeStart >=block.timestamp,'Time cannot be in Past');
        // maxAllocation >= minAllocation
        require(_Tier1.maxAllocation >=_Tier1.minAllocation && _Tier2.maxAllocation >=_Tier2.minAllocation && _Tier3.maxAllocation >=_Tier3.minAllocation,'maxAllocation >= minAllocation');

        uint _tokenAmountT1 =  _Tier1.maxAllocation * _Tier1.tokenPrice;
        uint _tokenAmountT2 =  _Tier2.maxAllocation * _Tier2.tokenPrice;
        uint _tokenAmountT3 =  _Tier3.maxAllocation * _Tier3.tokenPrice;

        // get current counter
        uint _nextCounter = NextCounterAt[msg.sender].counter;

        PAPpool pappool = new PAPpool(_Tier1, _Tier2, _Tier3 , msg.sender, _poolTokenAddress, _lotteryTokenAddress, papStakingAddress, _tokenAmountT1, _tokenAmountT2, _tokenAmountT3);
        papAddressPool.push(pappool);

        OwnerPoolBook[msg.sender][_nextCounter] = address(pappool);
        NextCounterAt[msg.sender].counter++;
    }
    //
    function PAPAddresses() view public returns(PAPpool[] memory){
        return papAddressPool;
    }
}

