import "@openzeppelin/contracts/math/SafeMath.sol";

// WecowBar is the coolest bar in town. You come in with some Wecow, and leave with more! The longer you stay, the more Wecow you get.
//
// This contract handles swapping to and from xWecow, WecowSwap's staking token.
contract WecowBar is ERC20("WecowBar", "xWDT"){
    using SafeMath for uint256;
    IERC20 public wdt;

    // Define the Wecow token contract
    constructor(IERC20 _wdt) public {
        wdt = _wdt;
    }

    // Enter the bar. Pay some WDTs. Earn some shares.
    // Locks Wecow and mints xWecow
    function enter(uint256 _amount) public {
        // Gets the amount of Wecow locked in the contract
        uint256 totalWecow = wdt.balanceOf(address(this));
        // Gets the amount of xWecow in existence
        uint256 totalShares = totalSupply();
        // If no xWecow exists, mint it 1:1 to the amount put in
        if (totalShares == 0 || totalWecow == 0) {
            _mint(msg.sender, _amount);
        } 
        // Calculate and mint the amount of xWecow the Wecow is worth. The ratio will change overtime, as xWecow is burned/minted and Wecow deposited + gained from fees / withdrawn.
        else {
            uint256 what = _amount.mul(totalShares).div(totalWecow);
            _mint(msg.sender, what);
        }
        // Lock the Wecow in the contract
        wdt.transferFrom(msg.sender, address(this), _amount);
    }

    // Leave the bar. Claim back your WDTs.
    // Unclocks the staked + gained Wecow and burns xWecow
    function leave(uint256 _share) public {
        // Gets the amount of xWecow in existence
        uint256 totalShares = totalSupply();
        // Calculates the amount of Wecow the xWecow is worth
        uint256 what = _share.mul(wdt.balanceOf(address(this))).div(totalShares);
        _burn(msg.sender, _share);
        wdt.transfer(msg.sender, what);
    }
}