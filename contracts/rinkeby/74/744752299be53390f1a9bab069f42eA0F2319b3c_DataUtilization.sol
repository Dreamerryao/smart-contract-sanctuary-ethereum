pragma solidity 0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./interface/IDataUtilization.sol";
import "./interface/IGFNHolder.sol";
import "./interface/IGFNConfig.sol";
import "./interface/IGNFT.sol";
import "./interface/IUSDToken.sol";
import "./interface/IReservePool.sol";
import "./interface/IEvents.sol";
import "hardhat/console.sol";

contract DataUtilization is Ownable, IDataUtilization, IEvents {

    IGFNHolder public holderContract;
    IGNFT public gnft;
    IGFNConfig public gfnConfig;
    IUSDToken public usdToken;
    IReservePool public reservePool;
    uint256 public constant MAX_SIZE = 500;

    constructor(address owner, IGNFT _gnft, IGFNConfig _gfnConfig, IGFNHolder _holderContract, IUSDToken _usd, IReservePool _reservePool) {
        gnft = _gnft;
        holderContract = _holderContract;
        gfnConfig = _gfnConfig;
        usdToken = _usd;
        reservePool = _reservePool;
        transferOwnership(owner);
    }

    function setUSDToken(IUSDToken _usdToken) external override onlyOwner {
        usdToken = _usdToken;
        emit SetUSDToken(address(_usdToken));
    }

    function setGNFT(IGNFT _gnft) external override onlyOwner {
        gnft = _gnft;
        emit SetGNFTToken(address(_gnft));
    }

    function setGFNConfig(IGFNConfig _gfnConfig) external override onlyOwner {
        gfnConfig = _gfnConfig;
        emit SetGFNConfig(address(_gfnConfig));
    }

    function setHolderContract(IGFNHolder _holderContract) external override onlyOwner {
        holderContract = _holderContract;
        emit SetHolder(address(_holderContract));
    }

    function setReservePool(IReservePool _reservePool) external override onlyOwner {
        reservePool = _reservePool;
        emit SetReservePool(address(_reservePool));
    }

    function _pay(string memory documentId, uint256 amount) private {
        address ownerOfDoc = gnft.getOwnerOfDoc(documentId);
        uint256 borrowNumber = holderContract.getDebt(ownerOfDoc);
        string memory poolOfUser = holderContract.getPoolOfUser(ownerOfDoc);

        if (bytes(poolOfUser).length > 0) { // check user is borrow or not
            address tmpStore = reservePool.getStoreInPoolId(poolOfUser);
            if(borrowNumber >= amount) {
                holderContract.decreaseDebtOfUser(ownerOfDoc, amount);
                usdToken.mintUSD(tmpStore, amount);
            } else {
                holderContract.decreaseDebtOfUser(ownerOfDoc, borrowNumber);
                uint256 profitNumber = amount - borrowNumber;

                IGFNConfig.Config memory tmpGFNConfig = gfnConfig.getConfig();
                uint256 amoutToUser = profitNumber * tmpGFNConfig.percentOfUserInBorrowUSD / 100;

                usdToken.mintUSD(address(holderContract), amoutToUser); //send percent USDToken to holder of user
                usdToken.mintUSD(tmpStore, profitNumber - amoutToUser + borrowNumber);

                holderContract.increaseProfitOfUser(ownerOfDoc, amoutToUser);
            }
        } else {
            usdToken.mintUSD(address(holderContract), amount); //send 100% USDToken to holder of user
            holderContract.increaseProfitOfUser(ownerOfDoc, amount);
        }
        emit Pay(documentId, amount);
    }
    
    function payToAccess(address du, string[] memory documentIds, uint256[] memory amounts) public override onlyOwner{
        require(documentIds.length == amounts.length && 
        documentIds.length > 0 &&
        documentIds.length <= MAX_SIZE, "Invalid length");
        for (uint256 index = 0; index < documentIds.length; index++) {
            _pay(documentIds[index], amounts[index]);
        }
        emit PayToAccess(du, documentIds, amounts);
    }

}

