pragma solidity >=0.8.4;

import "./IVirtualPawnHandler.sol";
import "./SVirtualPawnHandler.sol";
import "../PAWN-Token/IPAWN.sol";
import "../Uniswap/constants.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol";
import "@uniswap/v3-core/contracts/interfaces/callback/IUniswapV3MintCallback.sol";
import "@uniswap/v3-core/contracts/libraries/TickMath.sol";
import "@uniswap/v3-core/contracts/libraries/FixedPoint96.sol";
import "@uniswap/v3-core/contracts/libraries/FullMath.sol";
import "@uniswap/v3-core/contracts/libraries/SafeCast.sol";
import "@uniswap/v3-periphery/contracts/interfaces/INonfungiblePositionManager.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v3-periphery/contracts/libraries/LiquidityAmounts.sol";

import "hardhat/console.sol";

contract VirtualPawnHandler is
    OwnableUpgradeable,
    IVirtualPawnHandler,
    SVirtualPawnHandler,
    ERC721Upgradeable,
    PausableUpgradeable
{
    /// MODIFIERS

    /// @notice reverts when given a non whitelisted asset
    /// @param asset the ERC20 asset to check whitelisting for
    modifier onlyWhitelistedAsset(IERC20 asset) {
        if (address(whitelistedAssets[asset]) == address(0)) {
            revert VirtualPawnHandler__AssetNotWhitelisted();
        }
        _;
    }

    /// @notice reverts when given a non whitelisted user AND the userWhitelist is enabled
    /// @param user the user address to check whitelisting for
    modifier onlyWhitelistedUserPassthroughDisabled(address user) {
        if (userWhitelistEnabled == true && whitelistedUsers[user] == false) {
            revert VirtualPawnHandler__UserNotWhitelisted();
        }
        _;
    }

    /// @notice reverts when given a non whitelisted user
    /// @param user the user address to check whitelisting for
    modifier onlyWhitelistedUser(address user) {
        if (whitelistedUsers[user] == false) {
            revert VirtualPawnHandler__UserNotWhitelisted();
        }
        _;
    }
    /// STRUCTURES

    struct Slot0 {
        // the current price
        uint160 sqrtPriceX96;
        // the current tick
        int24 tick;
        // the most-recently updated index of the observations array
        uint16 observationIndex;
        // the current maximum number of observations that are being stored
        uint16 observationCardinality;
        // the next maximum number of observations to store, triggered in observations.write
        uint16 observationCardinalityNext;
        // the current protocol fee as a percentage of the swap fee taken on withdrawal
        // represented as an integer denominator (1/x)%
        uint8 feeProtocol;
        // whether the pool is locked
        bool unlocked;
    }

    struct Position {
        // Permits nonce
        uint96 nonce;
        // Addess approved for spending the position
        address operator;
        // Address of the token0 for the underlying pool
        address token0;
        // Address of the token1 for the underlying pool
        address token1;
        // Fee associated with the pool
        uint24 fee;
        // The lower end of the tick range for the position
        int24 tickLower;
        // The Higher end of the tick range for the position
        int24 tickUpper;
        // The liquidity of the position
        uint128 liquidity;
        // The fee growth of token0
        uint256 feeGrowthInside0LastX128;
        // The fee frowth of token1
        uint256 feeGrowthInside1LastX128;
        // The uncollected amount of token0
        uint128 tokensOwed0;
        // The uncollected amount of token1
        uint128 tokensOwed1;
    }

    /// CONSTRUCTOR
    function initialize() external initializer {
        __Ownable_init();
        __ERC721_init("VPH Positions", "VPHP");
        __Pausable_init();
        userWhitelistEnabled = false;
        maxSlippagePercentage = 10;
        cooldownPeriod = 7 days;
        lowerTick = TickMath.MIN_TICK;
        upperTick = TickMath.MAX_TICK;
    }

    /// NON-CONSTANT FUNCTIONS

    /// @inheritdoc IVirtualPawnHandler
    function setPawnProxy(address _pawn) external override onlyOwner {
        if (address(pawn) != address(0)) {
            revert VirtualPawnHandler__PawnProxyAlreadySet();
        }

        pawn = IPAWN(_pawn);
    }

    /// @inheritdoc IVirtualPawnHandler
    function setLiquidityRange(int24 _lowerTick, int24 _upperTick) external override onlyOwner whenNotPaused {
        if (_lowerTick < TickMath.MIN_TICK || _lowerTick > TickMath.MAX_TICK) {
            revert VirtualPawnHandler__LowerTickOutOfRange();
        }

        if (_upperTick < TickMath.MIN_TICK || _upperTick > TickMath.MAX_TICK) {
            revert VirtualPawnHandler__UpperTickOutOfRange();
        }

        if (_upperTick < _lowerTick) {
            revert VirtualPawnHandler__TicksWrongOrder();
        }

        lowerTick = _lowerTick;
        upperTick = _upperTick;
    }

    /// @inheritdoc IVirtualPawnHandler
    function setCooldownPeriod(uint256 _cooldownPeriod) external override onlyOwner whenNotPaused {
        if (_cooldownPeriod > 30 days) {
            revert VirtualPawnHandler__CooldownPeriodOutOfRange();
        }

        cooldownPeriod = _cooldownPeriod;
    }

    /// @inheritdoc IVirtualPawnHandler
    function addAssetToWhitelist(IERC20 asset) external override onlyOwner whenNotPaused {
        if (address(whitelistedAssets[asset]) != address(0)) {
            revert VirtualPawnHandler__AssetAlreadyWhitelisted();
        }

        IUniswapV3Pool pool = IUniswapV3Pool(uniswapFactory.getPool(address(pawn), address(asset), defaultUniswapFee));

        if (address(pool) == address(0)) {
            revert VirtualPawnHandler__PoolNotCreated();
        }

        (, , , , , , bool unlocked) = pool.slot0();

        if (unlocked == false) {
            revert VirtualPawnHandler__PoolNotInitialized();
        }

        whitelistedAssets[asset] = pool;

        emit WhitelistedAsset(asset);
    }

    /// @inheritdoc IVirtualPawnHandler
    function removeAssetFromWhitelist(IERC20 asset)
        external
        override
        onlyOwner
        whenNotPaused
        onlyWhitelistedAsset(asset)
    {
        delete whitelistedAssets[asset];
        emit RemovedAssetFromWhitelist(asset);
    }

    /// @inheritdoc IVirtualPawnHandler
    function addUserToWhitelist(address user) external override onlyOwner whenNotPaused {
        if (whitelistedUsers[user] != false) {
            revert VirtualPawnHandler__UserAlreadyWhitelisted();
        }

        whitelistedUsers[user] = true;
    }

    /// @inheritdoc IVirtualPawnHandler
    function removeUserFromWhitelist(address user) external override onlyOwner whenNotPaused onlyWhitelistedUser(user) {
        delete whitelistedUsers[user];
        emit RemovedUserFromWhitelist(user);
    }

    /// @inheritdoc IVirtualPawnHandler
    function enableUserWhitelist() external override onlyOwner whenNotPaused {
        if (userWhitelistEnabled == true) {
            revert VirtualPawnHandler__UserWhitelistAlreadyEnabled();
        }

        userWhitelistEnabled = true;
    }

    function disableUserWhitelist() external override onlyOwner whenNotPaused {
        if (userWhitelistEnabled == false) {
            revert VirtualPawnHandler__UserWhitelistAlreadyDisabled();
        }

        userWhitelistEnabled = false;
    }

    /// @inheritdoc IVirtualPawnHandler
    function deposit(IERC20 asset, uint256 amount)
        external
        override
        whenNotPaused
        onlyWhitelistedUserPassthroughDisabled(msg.sender)
        onlyWhitelistedAsset(asset)
    {
        if (amount == 0) {
            revert VirtualPawnHandler__DepositZeroAmount();
        }

        if (asset.transferFrom(msg.sender, address(this), amount) == false) {
            revert VirtualPawnHandler__DepositFailed();
        }

        IUniswapV3Pool pool = whitelistedAssets[asset];
        Slot0 memory slot0 = _getSlot0(pool);
        bool pawnFirst = false;

        if (pool.token0() == address(pawn)) {
            pawnFirst = true;
        }

        uint256 amountPawn = _computeAmountFromPrice(slot0.sqrtPriceX96, pawnFirst, amount);
        pawn.mint(amountPawn);

        IERC20(address(pawn)).approve(address(nonFungiblePositionManager), amountPawn);
        asset.approve(address(nonFungiblePositionManager), amount);

        // Mint the Uniswap v3 position NFT
        (uint256 tokenId, , uint256 gotAmountPawn, uint256 gotAmountAsset) = _mintPosition(
            amountPawn,
            amount,
            pool,
            maxSlippagePercentage
        );

        uint256 pawnBalance = IERC20(address(pawn)).balanceOf(address(this));

        if (pawnBalance > 0) {
            pawn.burn(pawnBalance);
        }

        // Transfer back any exceeding asset to msg.sender
        if (amount - gotAmountAsset > 0) {
            asset.transfer(msg.sender, amount - gotAmountAsset);
        }

        deposits[tokenId] = Deposit({
            asset: asset,
            pool: pool,
            pawnAmount: gotAmountPawn,
            assetAmount: gotAmountAsset,
            depositTime: block.timestamp
        });

        // Mint the proxy position NFT
        _safeMint(msg.sender, tokenId);

        emit NewPosition(msg.sender, tokenId, gotAmountPawn, gotAmountAsset);
    }

    /// @inheritdoc IVirtualPawnHandler
    function addLiquidity(uint256 tokenId, uint256 amount)
        external
        override
        whenNotPaused
        onlyWhitelistedUserPassthroughDisabled(msg.sender)
    {
        if (ownerOf(tokenId) != msg.sender) {
            revert VirtualPawnHandler__NotThePositionOwner();
        }

        Deposit memory userDeposit = deposits[tokenId];

        if (address(whitelistedAssets[userDeposit.asset]) == address(0)) {
            revert VirtualPawnHandler__AssetNotWhitelisted();
        }

        if (amount == 0) {
            revert VirtualPawnHandler__AddZeroAmount();
        }

        if (userDeposit.asset.transferFrom(msg.sender, address(this), amount) == false) {
            revert VirtualPawnHandler__AddLiquidityFailed();
        }

        IUniswapV3Pool pool = userDeposit.pool;
        Slot0 memory slot0 = _getSlot0(pool);
        bool pawnFirst = false;

        if (pool.token0() == address(pawn)) {
            pawnFirst = true;
        }

        uint256 amountPawn = _computeAmountFromPrice(slot0.sqrtPriceX96, pawnFirst, amount);
        pawn.mint(amountPawn);

        IERC20(address(pawn)).approve(address(nonFungiblePositionManager), amountPawn);
        userDeposit.asset.approve(address(nonFungiblePositionManager), amount);

        (, uint256 gotAmountPawn, uint256 gotAmountAsset) = _addLiquidity(
            tokenId,
            amountPawn,
            amount,
            maxSlippagePercentage
        );

        uint256 pawnBalance = IERC20(address(pawn)).balanceOf(address(this));

        if (pawnBalance > 0) {
            pawn.burn(pawnBalance);
        }

        // Transfer back any exceeding asset to msg.sender
        if (amount - gotAmountAsset > 0) {
            userDeposit.asset.transfer(msg.sender, amount - gotAmountAsset);
        }

        deposits[tokenId].pawnAmount = deposits[tokenId].pawnAmount + gotAmountPawn;
        deposits[tokenId].assetAmount = deposits[tokenId].assetAmount + gotAmountAsset;

        emit AddedToPosition(msg.sender, tokenId, gotAmountPawn, gotAmountAsset);
    }

    /// @inheritdoc IVirtualPawnHandler
    function rebalance(uint256 tokenId)
        external
        override
        whenNotPaused
        onlyWhitelistedUserPassthroughDisabled(msg.sender)
    {
        if (ownerOf(tokenId) != msg.sender) {
            revert VirtualPawnHandler__NotThePositionOwner();
        }

        Deposit memory userDeposit = deposits[tokenId];
        IUniswapV3Pool pool = whitelistedAssets[userDeposit.asset];

        if (address(pool) == address(0)) {
            revert VirtualPawnHandler__AssetNotWhitelisted();
        }

        Position memory position = _getPosition(tokenId);

        (uint256 pawnFees, uint256 assetFees) = _collectFees(tokenId, position, type(uint128).max, type(uint128).max);

        if (pawnFees == 0 || assetFees == 0) {
            revert VirtualPawnHandler__RebalanceZeroFees();
        }

        IERC20(address(pawn)).approve(address(nonFungiblePositionManager), pawnFees);
        userDeposit.asset.approve(address(nonFungiblePositionManager), assetFees);
        (, uint256 gotAmountPawn, uint256 gotAmountAsset) = _addLiquidity(tokenId, pawnFees, assetFees, 100);

        uint256 userOwedPawnAmount = _computeUserShare(0, 0, pawnFees - gotAmountPawn);
        uint256 userOwedAssetAmount = _computeUserShare(0, 0, assetFees - gotAmountAsset);

        if (userOwedPawnAmount > 0) {
            IERC20(address(pawn)).transfer(msg.sender, userOwedPawnAmount);
        }

        if (userOwedAssetAmount > 0) {
            userDeposit.asset.transfer(msg.sender, userOwedAssetAmount);
        }

        _swapAllAssetsForPawnAndBurn(pool);

        deposits[tokenId].pawnAmount = deposits[tokenId].pawnAmount + gotAmountPawn;
        deposits[tokenId].assetAmount = deposits[tokenId].assetAmount + gotAmountAsset;

        emit RebalancedPosition(msg.sender, tokenId, gotAmountPawn, gotAmountAsset);
    }

    /// @inheritdoc IVirtualPawnHandler
    function partialWithdraw(uint256 tokenId, uint256 amountAsset) external override whenNotPaused {
        if (ownerOf(tokenId) != msg.sender) {
            revert VirtualPawnHandler__NotThePositionOwner();
        }

        Deposit memory userDeposit = deposits[tokenId];

        if (block.timestamp - userDeposit.depositTime < cooldownPeriod) {
            revert VirtualPawnHandler__CooldownPeriodNotExpired();
        }

        if (amountAsset == 0) {
            revert VirtualPawnHandler__PartialWithdrawZeroAmount();
        }

        Slot0 memory slot0 = _getSlot0(userDeposit.pool);
        Position memory position = _getPosition(tokenId);

        uint256 amountPawn = _computeAmountFromPrice(slot0.sqrtPriceX96, position.token0 == address(pawn), amountAsset);
        uint256 protocolAmountPawn;
        uint256 protocolAmountAsset;

        {
            _collectFees(tokenId, position, type(uint128).max, type(uint128).max);
            (uint256 currentPawnAmount, uint256 currentAssetAmount) = _getCurrentPositionAmounts(position, slot0);
            protocolAmountPawn = _computeProtocolAmount(currentPawnAmount, userDeposit.pawnAmount, amountPawn);
            protocolAmountAsset = _computeProtocolAmount(currentAssetAmount, userDeposit.assetAmount, amountAsset);
        }

        uint128 liquidity = _getLiquidityForAmounts(
            slot0.sqrtPriceX96,
            position,
            amountPawn + protocolAmountPawn,
            amountAsset + protocolAmountAsset
        );

        if (liquidity > position.liquidity) {
            revert VirtualPawnHandler__NotEnoughLiquidity();
        }

        (uint256 gotAmountPawn, uint256 gotAmountAsset) = _decreaseLiquidity(tokenId, liquidity, position);
        _collectFees(tokenId, position, toUint128(gotAmountPawn), toUint128(gotAmountAsset));

        uint256 userOwedPawnAmount = _computeUserShare(gotAmountPawn, amountPawn, 0);
        uint256 userOwedAssetAmount = _computeUserShare(gotAmountAsset, amountAsset, 0);

        userDeposit.asset.transfer(msg.sender, userOwedAssetAmount);
        IERC20(address(pawn)).transfer(msg.sender, userOwedPawnAmount);

        _swapAllAssetsForPawnAndBurn(userDeposit.pool);

        deposits[tokenId].pawnAmount -= gotAmountPawn;
        deposits[tokenId].assetAmount -= gotAmountAsset;

        emit PartialWithdraw(msg.sender, tokenId, userOwedPawnAmount, userOwedAssetAmount);
    }

    /// @inheritdoc IVirtualPawnHandler
    function withdraw(uint256 tokenId) external override {
        if (ownerOf(tokenId) != msg.sender) {
            revert VirtualPawnHandler__NotThePositionOwner();
        }

        Deposit memory userDeposit = deposits[tokenId];
        IUniswapV3Pool pool = userDeposit.pool;

        if (block.timestamp - userDeposit.depositTime < cooldownPeriod) {
            revert VirtualPawnHandler__CooldownPeriodNotExpired();
        }

        if (getApproved(tokenId) != address(this)) {
            revert VirtualPawnHandler__NotApprovedForPosition();
        }

        // Take ownership of the proxy position token
        transferFrom(msg.sender, address(this), tokenId);

        // Double check that the VirtualPawnHandler is the owner of the true
        // underlying uniswapv3 position
        if (nonFungiblePositionManager.ownerOf(tokenId) != address(this)) {
            revert VirtualPawnHandler__NotAVirtualPawnHandlerPosition();
        }

        (uint256 pawnAmount, uint256 assetAmount, uint256 pawnFees, uint256 assetFees) = _closePosition(tokenId);
        uint256 userOwedPawnAmount = _computeUserShare(userDeposit.pawnAmount, pawnAmount, pawnFees);
        uint256 userOwedAssetAmount = _computeUserShare(userDeposit.assetAmount, assetAmount, assetFees);

        if (userOwedPawnAmount > 0) {
            IERC20(address(pawn)).transfer(msg.sender, userOwedPawnAmount);
        }

        if (userOwedAssetAmount > 0) {
            userDeposit.asset.transfer(msg.sender, userOwedAssetAmount);
        }

        _swapAllAssetsForPawnAndBurn(pool);

        // Burn both the uniswapv3 and VirtualPawnHandler proxy position token
        nonFungiblePositionManager.burn(tokenId);
        _burn(tokenId);

        // Remove the deposit from storage
        delete deposits[tokenId];

        emit ClosedPosition(msg.sender, userOwedPawnAmount, userOwedAssetAmount);
    }

    /// @inheritdoc IVirtualPawnHandler
    function pause() external override onlyOwner {
        _pause();
    }

    /// @inheritdoc IVirtualPawnHandler
    function unpause() external override onlyOwner whenPaused {
        _unpause();
    }

    /// INTERNAL FUNCTIONS

    /// @notice Helper function to get the Slot0 struct from a pool
    /// @notice this function is necessary to avoid Stack Too Deep errors
    /// @dev the pool address is assumbed to be valid
    /// @param pool the address of the uniswap v3 pool
    /// @return slot0 the Slot0 struct of the pool
    function _getSlot0(IUniswapV3Pool pool) internal view returns (Slot0 memory slot0) {
        (
            uint160 sqrtPriceX96,
            int24 tick,
            uint16 observationIndex,
            uint16 observationCardinality,
            uint16 observationCardinalityNext,
            uint8 feeProtocol,
            bool unlocked
        ) = pool.slot0();

        return
            Slot0({
                sqrtPriceX96: sqrtPriceX96,
                tick: tick,
                observationIndex: observationIndex,
                observationCardinality: observationCardinality,
                observationCardinalityNext: observationCardinalityNext,
                feeProtocol: feeProtocol,
                unlocked: unlocked
            });
    }

    /// @notice Helper function to get a Position struct from a tokenId
    /// @notice this function is necessary to avoid Stack Too Deep errors
    /// @dev the tokenId is assumed to be valid
    /// @param tokenId the id of the uniswap v3 position token
    /// @return position the Position struct assciated with the token
    function _getPosition(uint256 tokenId) internal view returns (Position memory position) {
        (
            uint96 nonce,
            address operator,
            address token0,
            address token1,
            uint24 fee,
            int24 tickLower,
            int24 tickUpper,
            uint128 liquidity,
            uint256 feeGrowthInside0LastX128,
            uint256 feeGrowthInside1LastX128,
            uint128 tokensOwed0,
            uint128 tokensOwed1
        ) = nonFungiblePositionManager.positions(tokenId);

        return
            Position({
                nonce: nonce,
                operator: operator,
                token0: token0,
                token1: token1,
                fee: fee,
                tickLower: tickLower,
                tickUpper: tickUpper,
                liquidity: liquidity,
                feeGrowthInside0LastX128: feeGrowthInside0LastX128,
                feeGrowthInside1LastX128: feeGrowthInside1LastX128,
                tokensOwed0: tokensOwed0,
                tokensOwed1: tokensOwed1
            });
    }

    /// @notice Helper function to compute the minimum amnount to get given allowed slippage percentage
    /// @param amount the requested amount of tokens
    /// @param maxSlippage the maximim slippage in percent
    /// @return amountMin the minimum amount to get in order to respect the slippage percentage
    function _computeAmountMin(uint256 amount, uint256 maxSlippage) internal pure returns (uint256 amountMin) {
        return (amount * (100 - maxSlippage)) / 100;
    }

    /// @notice Helper function to compute the amount of PAWN to mint
    /// @param price the SqrtPriceX96 of the pool
    /// @param pawnFirst the indicator about the tokens order
    /// @param amount the amount of input token
    /// @return amountOther the equivalent amount of the other token according to the price and direction
    function _computeAmountFromPrice(
        uint256 price,
        bool pawnFirst,
        uint256 amount
    ) internal pure returns (uint256 amountOther) {
        // Remove the square root from the price
        price = FullMath.mulDiv(price, price, FixedPoint96.Q96);

        if (pawnFirst) {
            amountOther = FullMath.mulDiv(amount, FixedPoint96.Q96, price);
        } else {
            amountOther = FullMath.mulDiv(
                price,
                amount << FixedPoint96.RESOLUTION,
                FixedPoint96.Q96 * FixedPoint96.Q96
            );
        }
    }

    /// @notice helper function to mint a new uniswap v3 position
    /// @dev pool and amounts are assumed to be valid and approved
    /// @param amountPawn the amount of token0 to request
    /// @param amountAsset the amount of token1 to request
    /// @param maxSlippage the maximum slippage allowed in percent
    /// @param pool the pool to
    function _mintPosition(
        uint256 amountPawn,
        uint256 amountAsset,
        IUniswapV3Pool pool,
        uint256 maxSlippage
    )
        internal
        returns (
            uint256 tokenId,
            uint128 liquidity,
            uint256 gotAmountPawn,
            uint256 gotAmountAsset
        )
    {
        int24 tickSpacing = pool.tickSpacing();
        bool pawnFirst = pool.token0() == address(pawn);

        INonfungiblePositionManager.MintParams memory params = INonfungiblePositionManager.MintParams({
            token0: pool.token0(),
            token1: pool.token1(),
            fee: pool.fee(),
            tickLower: lowerTick - (TickMath.MIN_TICK % tickSpacing),
            tickUpper: upperTick - (TickMath.MAX_TICK % tickSpacing),
            amount0Desired: pawnFirst ? amountPawn : amountAsset,
            amount1Desired: pawnFirst ? amountAsset : amountPawn,
            amount0Min: _computeAmountMin(pawnFirst ? amountPawn : amountAsset, maxSlippage),
            amount1Min: _computeAmountMin(pawnFirst ? amountAsset : amountPawn, maxSlippage),
            recipient: address(this),
            deadline: block.timestamp
        });

        // Mint the Uniswap v3 position NFT
        (uint256 _tokenId, uint128 _liquidity, uint256 gotAmount0, uint256 gotAmount1) = nonFungiblePositionManager
            .mint(params);

        if (pawnFirst) {
            return (_tokenId, _liquidity, gotAmount0, gotAmount1);
        }

        return (_tokenId, _liquidity, gotAmount1, gotAmount0);
    }

    /// @notice Helper function to empty a uniswap v3 position
    /// @dev the tokenId is assumed to be valid
    /// @param tokenId the id of the uniswap v3 position token
    /// @param liquidity the amount of liquidity to remove
    /// @param position the Position struct describing the uniswap v3 position
    /// @return amountPawn the output amount of PAWN tokens
    /// @return amountAsset the output amount of asset tokens
    function _decreaseLiquidity(
        uint256 tokenId,
        uint128 liquidity,
        Position memory position
    ) internal returns (uint256 amountPawn, uint256 amountAsset) {
        // Collect all liquidity
        INonfungiblePositionManager.DecreaseLiquidityParams memory decreaseLiquidityParams = INonfungiblePositionManager
            .DecreaseLiquidityParams({
                tokenId: tokenId,
                liquidity: liquidity,
                amount0Min: 0,
                amount1Min: 0,
                deadline: block.timestamp
            });

        (uint256 amount0, uint256 amount1) = nonFungiblePositionManager.decreaseLiquidity(decreaseLiquidityParams);

        if (position.token0 == address(pawn)) {
            return (amount0, amount1);
        }

        return (amount1, amount0);
    }

    /// @notice Helper function to collect the fees of a uniswap v3 position
    /// @dev the tokenId is assumed to be valid
    /// @param tokenId the id of the uniswap v3 position token
    /// @param position the Position struct describing the uniswap v3 position
    /// @return pawnFees the output amount of fee for token0
    /// @return assetFees the output amount of fee for token1
    function _collectFees(
        uint256 tokenId,
        Position memory position,
        uint128 amountPawnMax,
        uint128 amountAssetMax
    ) internal returns (uint256 pawnFees, uint256 assetFees) {
        uint128 amount0Max;
        uint128 amount1Max;

        if (position.token0 == address(pawn)) {
            amount0Max = amountPawnMax;
            amount1Max = amountAssetMax;
        } else {
            amount0Max = amountAssetMax;
            amount1Max = amountPawnMax;
        }

        INonfungiblePositionManager.CollectParams memory collectParams = INonfungiblePositionManager.CollectParams({
            tokenId: tokenId,
            recipient: address(this),
            amount0Max: amount0Max,
            amount1Max: amount1Max
        });

        if (position.token0 == address(pawn)) {
            (pawnFees, assetFees) = nonFungiblePositionManager.collect(collectParams);
        } else {
            (assetFees, pawnFees) = nonFungiblePositionManager.collect(collectParams);
        }
    }

    /// @notice Helper function add liquidity to a uniswap v3 position
    /// @dev the tokenId is assumed to be valid
    /// @param tokenId the id of the uniswap v3 position token
    /// @param amountPawn the amount of PAWN tokens to add
    /// @param amountAsset the amount of Asset tokens to add
    /// @param maxSlippage the maximum slippage allowed
    /// @return liquidity the amount of liquidity added
    /// @return addedPawn the amount of PAWN tokens added
    /// @return addedAsset the amount of Asset tokens added
    function _addLiquidity(
        uint256 tokenId,
        uint256 amountPawn,
        uint256 amountAsset,
        uint256 maxSlippage
    )
        internal
        returns (
            uint256 liquidity,
            uint256 addedPawn,
            uint256 addedAsset
        )
    {
        Position memory position = _getPosition(tokenId);
        bool pawnFirst = position.token0 == address(pawn) ? true : false;

        INonfungiblePositionManager.IncreaseLiquidityParams memory params = INonfungiblePositionManager
            .IncreaseLiquidityParams({
                tokenId: tokenId,
                amount0Desired: pawnFirst ? amountPawn : amountAsset,
                amount1Desired: pawnFirst ? amountAsset : amountPawn,
                amount0Min: _computeAmountMin(pawnFirst ? amountPawn : amountAsset, maxSlippage),
                amount1Min: _computeAmountMin(pawnFirst ? amountAsset : amountPawn, maxSlippage),
                deadline: block.timestamp
            });

        (uint256 _liquidity, uint256 gotAmount0, uint256 gotAmount1) = nonFungiblePositionManager.increaseLiquidity(
            params
        );

        if (pawnFirst) {
            return (_liquidity, gotAmount0, gotAmount1);
        }

        return (_liquidity, gotAmount1, gotAmount0);
    }

    /// @notice Helper function to close (i.e., liquidate) a uniswap v3 position
    /// @dev the tokenId is assumed to be valid
    /// @param tokenId the id of the uniswap v3 position token
    /// @return pawnAmount the amount of withdrawn PAWN
    /// @return assetAmount the output of withdrawn asset
    /// @return pawnFees the amount of PAWN fees
    /// @return assetFees the amount of asset fees
    function _closePosition(uint256 tokenId)
        internal
        returns (
            uint256 pawnAmount,
            uint256 assetAmount,
            uint256 pawnFees,
            uint256 assetFees
        )
    {
        Position memory position = _getPosition(tokenId);

        // Collect all available fees
        (pawnFees, assetFees) = _collectFees(tokenId, position, type(uint128).max, type(uint128).max);

        // Empty position : passes liquidity into owed fees
        (pawnAmount, assetAmount) = _decreaseLiquidity(tokenId, position.liquidity, position);

        // Collect fees again (here it's actually the deposited liquidity that we are collecting)
        _collectFees(tokenId, position, type(uint128).max, type(uint128).max);
    }

    /// @notice Helper function to compute the user shares after liquidating a position
    /// @param depositedAmount the amount of token deposited by the user
    /// @param withdrawnAmount the amount of token withdrawn from the position (not including the fees)
    /// @param feesAmount the amount of token generated by the fees
    /// @return userOwedAmount the amount of tokens owed to the user
    function _computeUserShare(
        uint256 depositedAmount,
        uint256 withdrawnAmount,
        uint256 feesAmount
    ) internal pure returns (uint256 userOwedAmount) {
        if (withdrawnAmount <= depositedAmount) {
            userOwedAmount = withdrawnAmount;
        } else {
            userOwedAmount = depositedAmount + ((withdrawnAmount - depositedAmount) / 2);
        }

        if (feesAmount > 0) {
            userOwedAmount = userOwedAmount + feesAmount / 2;
        }

        return userOwedAmount;
    }

    /// @notice Swap all of the asset balance for pawn and burn it
    function _swapAllAssetsForPawnAndBurn(IUniswapV3Pool pool) internal returns (uint256 burned) {
        IERC20 asset;

        if (pool.token0() == address(pawn)) {
            asset = IERC20(pool.token1());
        } else {
            asset = IERC20(pool.token0());
        }

        uint256 assetBalance = asset.balanceOf(address(this));

        if (assetBalance > 0) {
            asset.approve(address(uniswapRouter), assetBalance);
            // We don't care about the amount received after swapping, everything will be burned right after.
            _swapAllAssetForPawn(assetBalance, pool);
        }

        uint256 pawnAmountToBurn = IERC20(address(pawn)).balanceOf(address(this));

        if (pawnAmountToBurn > 0) {
            pawn.burn(pawnAmountToBurn);
        }

        return pawnAmountToBurn;
    }

    /// @notice Swap a given amount of tokens from asset held by the VirtualPawnHandler to PAWN
    /// @param amountIn the amount of tokens to swap from asset to PAWN
    /// @param pool the uniswap v3 pool to use for the swap
    function _swapAllAssetForPawn(uint256 amountIn, IUniswapV3Pool pool) internal returns (uint256 amountPawn) {
        address tokenIn;

        if (pool.token0() == address(pawn)) {
            tokenIn = pool.token1();
        } else {
            tokenIn = pool.token0();
        }

        ISwapRouter.ExactInputSingleParams memory swapParams = ISwapRouter.ExactInputSingleParams({
            tokenIn: tokenIn,
            tokenOut: address(pawn),
            fee: pool.fee(),
            recipient: address(this),
            deadline: block.timestamp,
            amountIn: amountIn,
            amountOutMinimum: 0,
            sqrtPriceLimitX96: 0
        });

        return uniswapRouter.exactInputSingle(swapParams);
    }

    /// @notice computes the actual amounts of token held into a position according to the current price of the pool
    /// @param position the position to compute the amounts for.
    /// @param slot0 the slot0 of the associated pool
    function _getCurrentPositionAmounts(Position memory position, Slot0 memory slot0)
        internal
        view
        returns (uint256 amountPawn, uint256 amountAsset)
    {
        (uint256 amount0, uint256 amount1) = LiquidityAmounts.getAmountsForLiquidity(
            slot0.sqrtPriceX96,
            TickMath.getSqrtRatioAtTick(position.tickLower),
            TickMath.getSqrtRatioAtTick(position.tickUpper),
            position.liquidity
        );

        if (position.token0 == address(pawn)) {
            return (amount0, amount1);
        }

        return (amount1, amount0);
    }

    /// @notice computes the liquidity representing the required amounts of both PAWN and ASSET given the current price
    /// @param sqrtRatioX96 the current price of the pool
    /// @param position the position to compute the amounts for
    /// @param amountPawn the amount of PAWN required
    /// @param amountAsset the amount of ASSET required
    /// @return liquidity : the liquidity to cover both amounts
    function _getLiquidityForAmounts(
        uint160 sqrtRatioX96,
        Position memory position,
        uint256 amountPawn,
        uint256 amountAsset
    ) internal view returns (uint128 liquidity) {
        uint256 amount0 = position.token0 == address(pawn) ? amountPawn : amountAsset;
        uint256 amount1 = position.token0 == address(pawn) ? amountAsset : amountPawn;

        return
            LiquidityAmounts.getLiquidityForAmounts(
                sqrtRatioX96,
                TickMath.getSqrtRatioAtTick(position.tickLower),
                TickMath.getSqrtRatioAtTick(position.tickUpper),
                amount0,
                amount1
            );
    }

    /// @notice computes the protocol shares when a user attempds to do a partial withdrawal
    /// @param currentAmount the current amount of token held in the position
    /// @param depositedAmount the amount of asset deposited by the user
    /// @param withdrawAmount the amount to withdraw from the position
    /// @return protocolAmount the protocol share of the withdrawal given the current position situation
    function _computeProtocolAmount(
        uint256 currentAmount,
        uint256 depositedAmount,
        uint256 withdrawAmount
    ) internal pure returns (uint256 protocolAmount) {
        int256 difference = SafeCast.toInt256(currentAmount) - SafeCast.toInt256(depositedAmount);

        if (difference < 0) {
            return 0;
        }

        uint256 uDifference = uint256(difference);
        uint256 balance = uDifference / 2 + depositedAmount;
        uint256 ratio = (100 * withdrawAmount) / balance;

        protocolAmount = ((uDifference / 2) * ratio) / 100;
    }

    function toUint128(uint256 x) private pure returns (uint128 y) {
        require((y = uint128(x)) == x);
    }
}

