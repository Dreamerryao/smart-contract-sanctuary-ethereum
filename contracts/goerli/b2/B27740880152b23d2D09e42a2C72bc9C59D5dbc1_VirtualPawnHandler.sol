pragma solidity >=0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface IVirtualPawnHandler {
    /// ERRORS

    /// @notice Emitted when attempting to whitelist an already whitelisted token
    error VirtualPawnHandler__AssetAlreadyWhitelisted();

    /// @notice Emitted when attempting an operation on a non whitelisted token
    error VirtualPawnHandler__AssetNotWhitelisted();

    /// @notice Emitted when attempting to whitelist an already whitelisted user
    error VirtualPawnHandler__UserAlreadyWhitelisted();

    /// @notice Emitted when a non whitelisted user attempts a protected operation
    error VirtualPawnHandler__UserNotWhitelisted();

    /// @notice Emitted when attempting to enable the user whitelist if it is already enabled
    error VirtualPawnHandler__UserWhitelistAlreadyEnabled();

    /// @notice Emitted when attempting to disable the user whitelist if it is already disabled
    error VirtualPawnHandler__UserWhitelistAlreadyDisabled();

    /// @notice Emitted when attempting to deposit a zero amount of tokens
    error VirtualPawnHandler__DepositZeroAmount();

    /// @notice Emitted when a deposit fails to get the tokens from the user
    error VirtualPawnHandler__DepositFailed();

    /// @notice Emitted when attempting to withdraw a zero amount of tokens
    error VirtualPawnHandler__WithdrawZeroAmount();

    /// @notice Emitted when attempting to add a 0 amount of tokens to an existing position
    error VirtualPawnHandler__AddZeroAmount();

    /// @notice Emitted when addLiquidity fails to get the tokens from the user
    error VirtualPawnHandler__AddLiquidityFailed();

    /// @notice Emitted when attempting to withdraw from a position owned by someone else
    error VirtualPawnHandler__NotThePositionOwner();

    /// @notice Emitted when the caller did not approve the VirtualPawnHandler for a given position
    error VirtualPawnHandler__NotApprovedForPosition();

    /// @notice Emitted when attempting to withdraw from a position that is not bound to
    /// a VirtualPawnHandler owned underlying position
    error VirtualPawnHandler__NotAVirtualPawnHandlerPosition();

    /// @notice Emitted when a withdrawal failed
    error VirtualPawnHandler__WithdrawalFailed();

    /// @notice Emitted when the VPH was not approved on withdrawal
    error VirtualPawnHandler__NoOpenPosition();

    /// @notice Emitted when a whitelisted token has no associated pool
    error VirtualPawnHandler__PoolNotCreated();

    /// @notice Emitted when attempting to whitelist a token with an uninitialized pool
    error VirtualPawnHandler__PoolNotInitialized();

    /// @notice Emitted when the swap amount cannot be safely converted to int256
    error VirtualPawnHandler__SwapAmountInOverflow();

    /// @notice Emitted when updating the liquidity range with out of range upper Tick
    error VirtualPawnHandler__UpperTickOutOfRange();

    /// @notice Emitted when updating the liquidity range with out of range lower Tick
    error VirtualPawnHandler__LowerTickOutOfRange();

    /// @notice Emitted when updating the liquidity range with lower Tick > upper Tick or the opposite
    error VirtualPawnHandler__TicksWrongOrder();

    /// @notice Emitted when attempting to set the pawn proxy when it was already set
    error VirtualPawnHandler__PawnProxyAlreadySet();

    /// @notice Emitted when attempting to (partial) withdraw before the cooldown period expiration
    error VirtualPawnHandler__CooldownPeriodNotExpired();

    /// @notice Emitted when attempting to set the cooldown period above the upper range (30 days)
    error VirtualPawnHandler__CooldownPeriodOutOfRange();

    /// @notice Emitted when attempting to rebalance a position with 0 fees accumulated in at least one asset
    error VirtualPawnHandler__RebalanceZeroFees();

    /// @notice Emitted when attempting to do a partial withdraw for more than the position holds
    error VirtualPawnHandler__NotEnoughLiquidity();

    /// @notice Emitted when attempting to do a partial withdraw with a zero amount
    error VirtualPawnHandler__PartialWithdrawZeroAmount();

    /// EVENTS

    /// @notice Emitted when a new asset is whitelisted
    event WhitelistedAsset(IERC20 indexed asset);

    /// @notice Emitted when an asset is removed from the assets whitelist
    event RemovedAssetFromWhitelist(IERC20 indexed asset);

    /// @notice Emitted when a new user is whitelisted
    event WhitelistedUser(address indexed user);

    /// @notice Emitted when a user is removed from the user whitelist
    event RemovedUserFromWhitelist(address indexed user);

    /// @notice Emitted when a new position is successfully minted
    event NewPosition(address indexed owner, uint256 indexed tokenId, uint256 amountPawn, uint256 amountAsset);

    /// @notice Emitted when liquidity is successfully added to a position
    event AddedToPosition(address indexed owner, uint256 indexed tokenId, uint256 amountPawn, uint256 amountAsset);

    /// @notice Emitted when a position is successfully rebalanced
    event RebalancedPosition(address indexed owner, uint256 indexed tokenId, uint256 amountPawn, uint256 amountAsset);

    /// @notice Emitted when a position is partially closed
    event PartialWithdraw(address indexed owner, uint256 indexed tokenId, uint256 amountPawn, uint256 amountAsset);

    /// @notice Emitted when an existing position is successfully closed
    event ClosedPosition(address indexed owner, uint256 amountPawn, uint256 amountAsset);

    /// NON-CONSTANT FUNCTIONS

    /// @notice set the PAWN proxy implementation
    /// @notice this function is requried due to the inter dependency of PAWN and VirtualPawnHandler
    /// @dev reverts if the caller is not the owner
    /// @dev reverts if the PAWN proxy is already set
    /// @param _pawn the address of the PAWN proxy
    function setPawnProxy(address _pawn) external;

    /// @notice set the liquidity range for all deposits
    /// @dev reverts if the caller is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if any of the inputs is outside MIN/MAX TICKS
    /// @param _lowerTick the lower tick for the liquidity range
    /// @param _upperTick the upper tick for the liquidity range
    function setLiquidityRange(int24 _lowerTick, int24 _upperTick) external;

    /// @notice set the cooldown perdiod between deposit and (partial) withdrawal
    /// @dev reverts if the caller is not the owner
    /// @dev reverts if the contract is paused
    /// @param _cooldownPeriod the new cooldown period in
    function setCooldownPeriod(uint256 _cooldownPeriod) external;

    /// @notice Whitelist a new ERC20 token
    /// @dev reverts the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the asset is already whitelisted
    /// @param asset the ERC20 token to whitelist
    function addAssetToWhitelist(IERC20 asset) external;

    /// @notice Remove an ERC20 token from the whitelist
    /// @dev reverts if the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the asset is not whitelisted
    /// @param asset the ERC20 token to remove from the whitelist
    function removeAssetFromWhitelist(IERC20 asset) external;

    /// @notice whitelist a new user
    /// @dev reverts if the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the user is already whitelisted
    /// @param user the address of the user to whitelist
    function addUserToWhitelist(address user) external;

    /// @notice Remove a user from the whitelist
    /// @dev reverts if the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the user is not whitelisted
    /// @param user the address of the user to remove from the whitelist
    function removeUserFromWhitelist(address user) external;

    /// @notice enable the user whitelisting protection
    /// @dev reverts if the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the user whitelist is already enabled
    function enableUserWhitelist() external;

    /// @notice disable the user whitelisting protection
    /// @dev reverts if the calling account is not the owner
    /// @dev reverts if the contract is paused
    /// @dev reverts if the user whitelist is already disabled
    function disableUserWhitelist() external;

    /// @notice the deposit function to deposit a specific amount of a whitelisted token
    /// @dev reverts if the asset is not whitelisted
    /// @dev reverts if the user whitelist is enabled and the user is not whitelisted
    /// @dev reverts if the contract is paused
    /// @param asset the ERC20 token used for the deposit operation
    /// @param amount the amount of asset tokens to deposit
    function deposit(IERC20 asset, uint256 amount) external;

    /// @notice the withdraw function to withdraw a specific amount of a whitelisted token
    /// @dev reverts if the asset is not whitelisted
    /// @dev reverts if the calling account has no existing position on the asset
    /// @dev reverts if the cooldown period for the associated deposit is not expired
    /// @param tokenId the Id of the proxy position NFT to withdraw from
    function withdraw(uint256 tokenId) external;

    /// @notice the partial withdraw function to partially close a position
    /// @dev reverts if the calling account has no existing position on the asset
    function partialWithdraw(uint256 tokenId, uint256 amount) external;

    /// @notice the rebalance function collects all fees accumulated by a position and adds them to the position
    /// @dev reverts if the asset is not whitelisted
    /// @dev reverts if the calling account is not the owner of the position
    /// @param tokenId the tokenId of the position to rebalance
    function rebalance(uint256 tokenId) external;

    /// @notice the addLiquidity function adds the given amount of asset liquidity to the position with tokenId
    /// @dev reverts if the asset is not whitelisted
    /// @dev reverts if the calling account is not the owner of the position
    /// @param tokenId the tokenId of the position to add to
    /// @param amount the amount of asset to add to the position
    function addLiquidity(uint256 tokenId, uint256 amount) external;

    /// @notice pause all effect functions of the VirtualPawnHandler except withdrawals
    /// @dev reverts if the caller is not the owner
    /// @dev revers if the contract is already paused
    function pause() external;

    /// @notice unpause all effect functions
    /// @dev revers if the caller is not the owner
    /// @dev reverts if the contract is not paused
    function unpause() external;
}

