pragma solidity ^0.8.10;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

import "../interfaces/IComplexAdmin.sol";
import "../interfaces/IBlock.sol";
import "../factories/CloneFactory.sol";
import "../interfaces/IPassport.sol";
import "../interfaces/IBadge.sol";

import "hardhat/console.sol";

contract Community is Initializable, AccessControlUpgradeable, CloneFactory {
    bytes32 public constant SUPER_ADMIN = keccak256("SUPER_ADMIN");
    bytes32 public constant ADMINISTRATOR = keccak256("ADMIN");
    bytes32 public constant MEMBER = keccak256("MEMBER");
    bytes32 public constant ACTION_BLOCK = keccak256("ACTION_BLOCK");

    bool disabled = false;

    IComplexAdmin complexAdmin;
    IPassport passport;
    IBadge badgeContract;

    struct Badge {
        uint256 id;
        string badgeName;
        string badgeCategory;
        bool private_;
        uint256 cap;
        uint256 index;
        uint256 privateIndex;
    }

    Badge[] public publicBadges;
    Badge[] private _allBadges;

    string public communityName;

    address public badgeImplementation;
    address passportAddress;

    uint256 constant MAX_CAP = 2**256 - 1;

    mapping(address => string) public blockByAddress;
    mapping(string => address) public blockByName;
    mapping(uint256 => address) public membersByPassport;
    mapping(address => uint256) public membersByAddressToPassportId;
    mapping(string => Badge) public badgesByName;
    mapping(uint256 => Badge) public badgesById;

    event BlockAdded(address community, address block_, string blockName);
    event BlockRemoved(string blockName, address blockAddress);
    event MemberAdded(uint256 memberPassportId, bytes32 role);
    event MemberRemoved(uint256 memberPassport);
    event FunctionCallDelegated(address target, bytes data);
    event CommunityDisabled(string communityName, address communityAddress);
    event CommunityEnabled(string communityName, address communityAddress);

    modifier notDisabled() {
        require(!disabled, "ERR: Community Disabled");
        _;
    }

    modifier adminOrActionBlock() {
        require(
            hasRole(ADMINISTRATOR, _msgSender()) ||
                hasRole(ACTION_BLOCK, _msgSender()),
            "ERR: Insufficient Permissions"
        );
        _;
    }

    /**
    @notice Initializer. Sets up ADMINISTRATOR and MEMBER roles, assigns to complex Admin contract and admin address
    Sets community name, inits badge contract, issues passport to admin if one doesn't exist, adds admin to 
    member mapping
    @param adminContract_ The address of the ComplexAdmin contract
    @param admin_  Address of the initial administrator/member of the community (typically the creator)
    @param communityName_ The name of the community
    @dev Breakdown of accessControl roles:
    DEFAULT_ADMIN_ROLE: Only assigned to complex admin contract
    SUPER_ADMIN: Assigned to initial admin_, as well as admin contract. Administrator for this role is other SUPER-ADMINs
    ADMINISTRATOR: Assigned to initial admin_ as well as admin contract. Administrator for this role is SUPER_ADMIN
    MEMBER: Assigned to initial admin as well as admin contract. Administrator is ADMINISTRATOR
     */
    function initialize(
        address adminContract_,
        address admin_,
        string memory communityName_,
        address badge_,
        address passportAddress_
    ) external initializer {
        // Set super admin role, assign to admin contract and make the super admin the admin of superadmin
        _grantRole(DEFAULT_ADMIN_ROLE, adminContract_);
        _grantRole(SUPER_ADMIN, admin_);
        _grantRole(SUPER_ADMIN, adminContract_);
        _setRoleAdmin(SUPER_ADMIN, SUPER_ADMIN);
        // Set Administrator role, assign to admin and admin contract. Admin of role: Super admin and Action Block
        _grantRole(ADMINISTRATOR, adminContract_);
        _grantRole(ADMINISTRATOR, admin_);
        _setRoleAdmin(ADMINISTRATOR, SUPER_ADMIN);
        _setRoleAdmin(ADMINISTRATOR, ACTION_BLOCK);
        //Set Member role, assign to admin and admin contract. Admin of role: Administrator
        _grantRole(MEMBER, adminContract_);
        _grantRole(MEMBER, admin_);
        _setRoleAdmin(MEMBER, ADMINISTRATOR);

        passportAddress = passportAddress_;
        passport = IPassport(passportAddress_);
        complexAdmin = IComplexAdmin(adminContract_);
        communityName = communityName_;
        uint256 adminPassportId = complexAdmin.getOrIssuePassport(admin_);
        membersByPassport[adminPassportId] = admin_;
        membersByAddressToPassportId[admin_] = adminPassportId;
        //Create and init badge contract
        _createAndInitBadgeContract(badge_, adminPassportId);
    }

    function _createAndInitBadgeContract(
        address badgeImplementation_,
        uint256 passportId_
    ) internal {
        badgeImplementation = _createClone(badgeImplementation_);
        badgeContract = IBadge(badgeImplementation);
        badgeContract.initialize(address(this), communityName);
        complexAdmin.addCommunityToPassport(
            passportId_,
            communityName,
            address(this),
            badgeImplementation,
            true
        );
        _mintBadge(passportId_, "Admin", "base", false, MAX_CAP, 1);
        _mintBadge(passportId_, "Member", "base", false, MAX_CAP, 1);
    }

    function mintBadge(
        uint256 passportId_,
        string memory badgeName_,
        string memory badgeCategory_,
        bool private_,
        uint256 cap_,
        uint256 amount_
    ) external notDisabled adminOrActionBlock {
        require(
            membersByPassport[passportId_] != address(0x0),
            "ERR: Not a member"
        );
        _mintBadge(
            passportId_,
            badgeName_,
            badgeCategory_,
            private_,
            cap_,
            amount_
        );
    }

    function _mintBadge(
        uint256 passportId_,
        string memory badgeName_,
        string memory badgeCategory_,
        bool private_,
        uint256 cap_,
        uint256 amount_
    ) internal {
        uint256 _id = badgeContract.mint(
            passportAddress,
            passportId_,
            badgeName_,
            badgeCategory_,
            private_,
            cap_,
            amount_
        );
        Badge memory badge = Badge({
            id: _id,
            badgeName: badgeName_,
            badgeCategory: badgeCategory_,
            private_: private_,
            cap: cap_,
            index: publicBadges.length,
            privateIndex: _allBadges.length
        });
        badgesByName[badgeName_] = badge;
        badgesById[_id] = badge;
        if (!private_) {
            publicBadges.push(badge);
        }
        _allBadges.push(badge);
    }

    function burnBadge(string memory badgeName_, uint256 amount_)
        external
        notDisabled
        adminOrActionBlock
    {
        badgeContract.burn(passportAddress, badgeName_, amount_);
    }

    function removeBadgeFromCommunity(string memory badgeName_)
        external
        notDisabled
        adminOrActionBlock
    {
        Badge memory badge = badgesByName[badgeName_];
        delete publicBadges[badge.index];
        delete _allBadges[badge.privateIndex];
        delete badgesByName[badgeName_];
    }

    function removeBadgeFromMember(
        uint256 tokenId_,
        address communityImplementation_,
        string memory badgeName_
    ) external notDisabled adminOrActionBlock {
        complexAdmin.removeBadgeFromPassportId(
            tokenId_,
            communityImplementation_,
            badgeName_
        );
    }

    //Complex Admin Control Functionality========================================
    function disableCommunity() external onlyRole(DEFAULT_ADMIN_ROLE) {
        disabled = true;
        // Need to remove community and badges from all member passports.
        // burn badges?
        emit CommunityDisabled(communityName, address(this));
    }

    function enableCommunity() external onlyRole(DEFAULT_ADMIN_ROLE) {
        disabled = false;
        emit CommunityEnabled(communityName, address(this));
    }

    //MEMBER MANAGEMENT============================================

    /**
        @notice Adds a member to the community and assigns appropriate role
        @param member_ The address of the new member
        @param role_ The keccak representation of the role to be assigned to new member
        @dev Must be called by either administrator or action block (to allow module to add members)
        @dev emits a MemberAdded event
     */
    function addMemberAndAssignRole(address member_, bytes32 role_)
        public
        notDisabled
        adminOrActionBlock
    {
        // Make sure new member is not already a member
        require(
            membersByAddressToPassportId[member_] == 0,
            "ERR: Already a member"
        );
        // get or issue passport to new member
        uint256 passportId = complexAdmin.getOrIssuePassport(member_);
        // Add member by passport ID to community
        membersByPassport[passportId] = member_;
        // add member by address to passport id
        membersByAddressToPassportId[member_] = passportId;
        complexAdmin.addCommunityToPassport(
            passportId,
            communityName,
            address(this),
            badgeImplementation,
            true
        );
        // Mint appropriate badge
        if (role_ == MEMBER) {
            _mintBadge(passportId, "Member", "base", false, 2**256 - 1, 1);
            //! Add badge to passport
        }
        if (role_ == ADMINISTRATOR) {
            _mintBadge(passportId, "Admin", "base", false, 2**256 - 1, 1);
            //! Add badge to passport
        }

        // Emit event
        emit MemberAdded(passportId, role_);
    }

    /**
        @notice Removes an existing member and revokes all roles. Also will burn badges associated with membership
