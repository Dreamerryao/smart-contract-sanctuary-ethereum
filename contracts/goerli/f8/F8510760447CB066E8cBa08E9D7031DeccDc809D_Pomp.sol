/**
 * @title CoreLogic
 * @author Pomp Protocol
 *
 * @notice This is the library that contains the logic for profile creation & mission.
 *
 */
library CoreLogic {
  uint8 internal constant MAX_HANDLE_LENGTH = 31;

  event Aborted(uint256 indexed profileId, uint256 indexed missionId, uint256 timestamp);
  event Completed(
    uint256 indexed profileId,
    uint256 indexed missionId,
    bytes32 challenge,
    uint256 timestamp
  );
  event ProfileCreated(
    uint256 indexed profileId,
    address indexed creator,
    string handle,
    uint256 timestamp
  );
  event Started(
    uint256 indexed profileId,
    uint256 indexed missionId,
    string slug,
    uint256 endtime,
    uint256 timestamp
  );
  event Verified(
    uint256 indexed profileId,
    uint256 indexed missionId,
    address verifier,
    uint256 timestamp
  );
  event Failed(
    uint256 indexed profileId,
    uint256 indexed missionId,
    address verifier,
    string reason,
    uint256 timestamp
  );

  error Claimed();
  error ContainsInvalidChars();
  error Expired();
  error InvalidLength();
  error HandleTaken();
  error ChallengeTaken();
  error NotFound();
  error NotMatched();
  error NotOwner();
  error NotVerifiable();
  error NotReady();
  error NotVerifier();
  error NotWhitelisted();
  error SecurityViolated();
  error SignatureExpired();
  error SignatureInvalid();
  error SlugTaken();

  function abortMission(
    uint256 profileId,
    uint256 missionId,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile
  ) public {
    _missionByIdByProfile[profileId][missionId].state = DataTypes.State.aborted;

    emit Aborted(profileId, missionId, block.timestamp);
  }

  function canAbortOrComplete(
    uint256 profileId,
    uint256 missionId,
    bool checkExpiry,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile
  ) public view {
    if (_missionByIdByProfile[profileId][missionId].starttime == 0) revert NotFound();
    if (_missionByIdByProfile[profileId][missionId].state != DataTypes.State.nftDeposited)
      revert NotReady();
    if (
      checkExpiry == true && _missionByIdByProfile[profileId][missionId].endtime < block.timestamp
    ) revert Expired();
  }

  function canClaimProfileWithRevert(
    mapping(address => bool) storage claimed,
    address claimer,
    bytes32[] calldata proof,
    string calldata handle,
    bytes32 merkleroot,
    mapping(bytes32 => uint256) storage _profileIdByHandleHash
  ) public view {
    if (claimed[claimer]) revert Claimed();
    if (!MerkleProof.verify(proof, merkleroot, keccak256(abi.encodePacked(claimer))))
      revert NotWhitelisted();
    validateHandleOrSlug(handle);
    if (_profileIdByHandleHash[keccak256(bytes(handle))] != 0) revert HandleTaken();
  }

  function canClaimProfile(
    mapping(address => bool) storage claimed,
    address claimer,
    bytes32[] calldata proof,
    string calldata handle,
    bytes32 merkleroot,
    mapping(bytes32 => uint256) storage _profileIdByHandleHash
  ) public view returns (bool) {
    return
      !claimed[claimer] &&
      (MerkleProof.verify(proof, merkleroot, keccak256(abi.encodePacked(claimer)))) &&
      (_profileIdByHandleHash[keccak256(bytes(handle))] == 0);
  }

  function canVerifyOrFail(
    uint256 profileId,
    uint256 missionId,
    string calldata challenge,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile
  ) public view {
    if (_missionByIdByProfile[profileId][missionId].starttime == 0) revert NotFound();
    if (_missionByIdByProfile[profileId][missionId].verifier != msg.sender) revert NotVerifier();
    if (_missionByIdByProfile[profileId][missionId].state != DataTypes.State.completed)
      revert NotVerifiable();
    if (_missionByIdByProfile[profileId][missionId].challengeHash != keccak256(bytes(challenge)))
      revert NotMatched();
  }

  function completeMission(
    uint256 profileId,
    uint256 missionId,
    string calldata challenge,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile,
    mapping(bytes32 => uint256) storage _missionIdByChallengeHash
  ) public {
    bytes32 challengeHash = keccak256(bytes(challenge));
    _missionByIdByProfile[profileId][missionId].state = DataTypes.State.completed;
    _missionByIdByProfile[profileId][missionId].challengeHash = challengeHash;
    _missionIdByChallengeHash[challengeHash] = missionId;

    emit Completed(profileId, missionId, challengeHash, block.timestamp);
  }

  function createProfile(
    mapping(address => bool) storage claimed,
    address to,
    string calldata handle,
    uint256 profileId,
    mapping(bytes32 => uint256) storage _profileIdByHandleHash,
    mapping(uint256 => DataTypes.ProfileStruct) storage _profileById
  ) external {
    _profileIdByHandleHash[keccak256(bytes(handle))] = profileId;
    _profileById[profileId].handle = handle;
    _profileById[profileId].owner = to;

    claimed[to] = true;

    emit ProfileCreated(
      profileId,
      to, // Creator is always the msg sender
      handle,
      block.timestamp
    );
  }

  function getMissionIdByChallengeHash(
    bytes32 hash,
    mapping(bytes32 => uint256) storage _missionIdByChallengeHash
  ) public view returns (uint256) {
    uint256 missionId = _missionIdByChallengeHash[hash];
    if (missionId == 0) revert NotFound();
    return missionId;
  }

  function failMission(
    uint256 profileId,
    uint256 missionId,
    string calldata reason,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile
  ) public {
    _missionByIdByProfile[profileId][missionId].state = DataTypes.State.failed;

    emit Failed(profileId, missionId, msg.sender, reason, block.timestamp);
  }

  function isChallengeAvailable(
    string calldata challenge,
    mapping(bytes32 => uint256) storage _missionIdByChallengeHash
  ) public view {
    if (_missionIdByChallengeHash[keccak256(bytes(challenge))] != 0) revert ChallengeTaken();
  }

  function isOwner(address expectedAddress) internal view {
    if (expectedAddress != msg.sender) revert NotOwner();
  }

  function isSlugAvailable(
    string calldata slug,
    mapping(bytes32 => uint256) storage _missionIdBySlugHash
  ) internal view {
    if (_missionIdBySlugHash[keccak256(bytes(slug))] != 0) revert SlugTaken();
  }

  function startMission(
    uint256 profileId,
    uint256 missionId,
    string calldata slug,
    uint256 minutesToExpire,
    address verifier,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile,
    mapping(bytes32 => uint256) storage _missionIdBySlugHash,
    mapping(uint256 => DataTypes.ProfileStruct) storage _profileById
  ) public {
    _profileById[profileId].missionCount++;
    _missionIdBySlugHash[keccak256(bytes(slug))] = missionId;
    _missionByIdByProfile[profileId][missionId].owner = msg.sender;
    _missionByIdByProfile[profileId][missionId].profileId = profileId;
    _missionByIdByProfile[profileId][missionId].missionId = missionId;
    _missionByIdByProfile[profileId][missionId].verifier = verifier;
    _missionByIdByProfile[profileId][missionId].starttime = KhronusTimeCog.addMinutes(
      block.timestamp,
      0
    );
    uint256 endtime = KhronusTimeCog.addMinutes(block.timestamp, minutesToExpire);
    _missionByIdByProfile[profileId][missionId].endtime = endtime;
    _missionByIdByProfile[profileId][missionId].state = DataTypes.State.nftDeposited;

    emit Started(profileId, missionId, slug, endtime, block.timestamp);
  }

  function validateHandleOrSlug(string calldata handle) internal pure {
    bytes memory byteHandle = bytes(handle);
    if (byteHandle.length == 0 || byteHandle.length > MAX_HANDLE_LENGTH) revert InvalidLength();

    uint256 byteHandleLength = byteHandle.length;
    for (uint256 i = 0; i < byteHandleLength; ) {
      if (
        (byteHandle[i] < '0' ||
          byteHandle[i] > 'z' ||
          (byteHandle[i] > '9' && byteHandle[i] < 'a')) &&
        byteHandle[i] != '.' &&
        byteHandle[i] != '-' &&
        byteHandle[i] != '_'
      ) revert ContainsInvalidChars();
      unchecked {
        ++i;
      }
    }
  }

  function validateSignature(
    bytes32 digest,
    address expectedAddress,
    bytes calldata signature,
    uint256 deadline
  ) public view returns (bool) {
    if (deadline < block.timestamp) revert SignatureExpired();
    address recoveredAddress = ECDSA.recover(digest, signature);
    if (
      expectedAddress != recoveredAddress ||
      !SignatureChecker.isValidSignatureNow(expectedAddress, digest, signature)
    ) revert SignatureInvalid();
    return true;
  }

  function verifyMission(
    uint256 profileId,
    uint256 missionId,
    mapping(uint256 => mapping(uint256 => DataTypes.MissionStruct)) storage _missionByIdByProfile
  ) public {
    _missionByIdByProfile[profileId][missionId].state = DataTypes.State.verified;

    emit Verified(profileId, missionId, msg.sender, block.timestamp);
  }
}

