// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import '@openzeppelin/contracts/access/AccessControl.sol';
import '@openzeppelin/contracts/security/Pausable.sol';
import '@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import '@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721.sol';
import 'hardhat/console.sol';
import {DataTypes} from './libraries/DataTypes.sol';
import {Storage} from './libraries/Storage.sol';
import {CoreLogic} from './libraries/CoreLogic.sol';
import './interfaces/IPomp.sol';
import './interfaces/IMissionNFT.sol';

contract Pomp is Pausable, AccessControl, EIP712, ERC721Holder, Storage, IPomp {
  using Counters for Counters.Counter;

  Counters.Counter private _profileIdCounter;

  address public immutable missionNFT;

  event Aborted(uint256 indexed profileId, uint256 indexed missionId, uint256 timestamp);
  event Completed(
    uint256 indexed profileId,
    uint256 indexed missionId,
    bytes32 challenge,
    uint256 timestamp
  );
  event ProfileCreated(
    uint256 indexed profileId,
    address indexed creator,
    string handle,
    uint256 timestamp
  );
  event Started(
    uint256 indexed profileId,
    uint256 indexed missionId,
    string slug,
    uint256 endtime,
    uint256 timestamp
  );
  event Verified(
    uint256 indexed profileId,
    uint256 indexed missionId,
    address verifier,
    uint256 timestamp
  );
  event Failed(
    uint256 indexed profileId,
    uint256 indexed missionId,
    address verifier,
    string reason,
    uint256 timestamp
  );

  constructor(address _missionNFT, bytes32 _root) EIP712('Pomp', '1') {
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _grantRole(PAUSER_ROLE, msg.sender);
    _merklerootForProfiles = _root;
    missionNFT = _missionNFT;
    _profileIdCounter.increment();
  }

  /// ***********************
  /// *****GOV FUNCTIONS*****
  /// ***********************

  /// @inheritdoc IPomp
  function pause() external onlyRole(PAUSER_ROLE) {
    _pause();
  }

  /// @inheritdoc IPomp
  function unpause() external onlyRole(PAUSER_ROLE) {
    _unpause();
  }

  /// @inheritdoc IPomp
  function setMerklerootForProfiles(bytes32 root)
    external
    whenNotPaused
    onlyRole(DEFAULT_ADMIN_ROLE)
  {
    _merklerootForProfiles = root;
  }

  /// *********************************
  /// *****WRITE FUNCTIONS*****
  /// *********************************

  /// @inheritdoc IPomp
  function abortWithSig(DataTypes.AbortWithSigData calldata vars) external override whenNotPaused {
    address owner = _profileById[vars.profileId].owner;
    CoreLogic.isOwner(owner);
    CoreLogic.validateSignature(
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            ABORT_WITH_SIG_TYPEHASH,
            vars.profileId,
            vars.missionId,
            sigNonces[owner],
            vars.deadline
          )
        )
      ),
      owner,
      vars.signature,
      vars.deadline
    );
    CoreLogic.canAbortOrComplete(vars.profileId, vars.missionId, false, _missionByIdByProfile);

    sigNonces[owner]++;

    ERC721Burnable(missionNFT).burn(vars.missionId);

    CoreLogic.abortMission(vars.profileId, vars.missionId, _missionByIdByProfile);
  }

  /// @inheritdoc IPomp
  function completeWithSig(DataTypes.CompleteWithSigData calldata vars)
    external
    override
    whenNotPaused
  {
    address owner = _profileById[vars.profileId].owner;
    CoreLogic.isOwner(owner);
    CoreLogic.validateSignature(
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            COMPLETE_WITH_SIG_TYPEHASH,
            vars.profileId,
            vars.missionId,
            keccak256(bytes(vars.challenge)),
            sigNonces[owner],
            vars.deadline
          )
        )
      ),
      owner,
      vars.signature,
      vars.deadline
    );
    CoreLogic.canAbortOrComplete(vars.profileId, vars.missionId, true, _missionByIdByProfile);
    CoreLogic.isChallengeAvailable(vars.challenge, _missionIdByChallengeHash);

    sigNonces[owner]++;

    CoreLogic.completeMission(
      vars.profileId,
      vars.missionId,
      vars.challenge,
      _missionByIdByProfile,
      _missionIdByChallengeHash
    );
  }

  /// @inheritdoc IPomp
  function createProfile(string calldata handle, bytes32[] calldata proof)
    external
    override
    whenNotPaused
    returns (uint256)
  {
    CoreLogic.canClaimProfileWithRevert(
      claimed,
      msg.sender,
      proof,
      handle,
      _merklerootForProfiles,
      _profileIdByHandleHash
    );
    uint256 profileId = _profileIdCounter.current();
    _profileIdCounter.increment();

    CoreLogic.createProfile(
      claimed,
      msg.sender,
      handle,
      profileId,
      _profileIdByHandleHash,
      _profileById
    );

    return profileId;
  }

  /// @inheritdoc IPomp
  function failWithSig(DataTypes.FailWithSigData calldata vars) external override whenNotPaused {
    bytes32 hash = keccak256(bytes(vars.challenge));
    CoreLogic.validateSignature(
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            FAIL_WITH_SIG_TYPEHASH,
            vars.profileId,
            hash,
            keccak256(bytes(vars.reason)),
            sigNonces[msg.sender],
            vars.deadline
          )
        )
      ),
      msg.sender,
      vars.signature,
      vars.deadline
    );
    uint256 missionId = CoreLogic.getMissionIdByChallengeHash(hash, _missionIdByChallengeHash);
    CoreLogic.canVerifyOrFail(vars.profileId, missionId, vars.challenge, _missionByIdByProfile);

    sigNonces[msg.sender]++;

    ERC721Burnable(missionNFT).burn(missionId);

    CoreLogic.failMission(vars.profileId, missionId, vars.reason, _missionByIdByProfile);
  }

  /// @inheritdoc IPomp
  function startWithSig(DataTypes.StartWithSigData calldata vars)
    external
    override
    whenNotPaused
    returns (uint256)
  {
    address owner = _profileById[vars.profileId].owner;
    CoreLogic.isOwner(owner);
    CoreLogic.validateSignature(
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            START_WITH_SIG_TYPEHASH,
            vars.profileId,
            keccak256(bytes(vars.slug)),
            keccak256(bytes(vars.contentURI)),
            vars.minutesToExpire,
            vars.verifier,
            sigNonces[owner],
            vars.deadline
          )
        )
      ),
      owner,
      vars.signature,
      vars.deadline
    );
    CoreLogic.validateHandleOrSlug(vars.slug);
    CoreLogic.isSlugAvailable(vars.slug, _missionIdBySlugHash);

    sigNonces[owner]++;

    uint256 tokenId = IMissionNFT(missionNFT).safeMint(address(this), vars.contentURI);

    CoreLogic.startMission(
      vars.profileId,
      tokenId,
      vars.slug,
      vars.minutesToExpire,
      vars.verifier,
      _missionByIdByProfile,
      _missionIdBySlugHash,
      _profileById
    );

    return tokenId;
  }

  /// @inheritdoc IPomp
  function verifyWithSig(DataTypes.VerifyWithSigData calldata vars)
    external
    override
    whenNotPaused
  {
    bytes32 hash = keccak256(bytes(vars.challenge));
    CoreLogic.validateSignature(
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            VERIFY_WITH_SIG_TYPEHASH,
            vars.profileId,
            hash,
            sigNonces[msg.sender],
            vars.deadline
          )
        )
      ),
      msg.sender,
      vars.signature,
      vars.deadline
    );
    uint256 missionId = CoreLogic.getMissionIdByChallengeHash(hash, _missionIdByChallengeHash);
    CoreLogic.canVerifyOrFail(vars.profileId, missionId, vars.challenge, _missionByIdByProfile);

    sigNonces[msg.sender]++;

    IERC721(missionNFT).safeTransferFrom(
      address(this),
      _missionByIdByProfile[vars.profileId][missionId].owner,
      missionId
    );

    CoreLogic.verifyMission(vars.profileId, missionId, _missionByIdByProfile);
  }

  /// ***********************
  /// ***VIEW FUNCTIONS***
  /// ***********************

  /// @inheritdoc IPomp
  function canClaim(
    address claimer,
    string calldata handle,
    bytes32[] calldata proof
  ) external view override returns (bool) {
    return
      CoreLogic.canClaimProfile(
        claimed,
        claimer,
        proof,
        handle,
        _merklerootForProfiles,
        _profileIdByHandleHash
      );
  }

  /// @inheritdoc IPomp
  function getDomainSeparator() external view override returns (bytes32) {
    return _domainSeparatorV4();
  }

  /// @inheritdoc IPomp
  function getHandle(uint256 profileId) external view returns (string memory) {
    return _profileById[profileId].handle;
  }

  /// @inheritdoc IPomp
  function missionCount(uint256 profileId) external view returns (uint256) {
    return _profileById[profileId].missionCount;
  }

  /// @inheritdoc IPomp
  function profileById(uint256 profileId)
    external
    view
    override
    returns (DataTypes.ProfileStruct memory)
  {
    return _profileById[profileId];
  }

  /// @inheritdoc IPomp
  function profileByHandle(string calldata handle)
    external
    view
    override
    returns (DataTypes.ProfileStruct memory)
  {
    return _profileById[_profileIdByHandleHash[keccak256(bytes(handle))]];
  }

  /// @inheritdoc IPomp
  function profileIdByHandle(string calldata handle) external view returns (uint256) {
    bytes32 handleHash = keccak256(bytes(handle));
    return _profileIdByHandleHash[handleHash];
  }

  /// @inheritdoc IPomp
  function missionById(uint256 profileId, uint256 missionId)
    external
    view
    override
    returns (DataTypes.MissionStruct memory)
  {
    return _missionByIdByProfile[profileId][missionId];
  }

  /// @inheritdoc IPomp
  function missionBySlug(uint256 profileId, string calldata slug)
    external
    view
    override
    returns (DataTypes.MissionStruct memory)
  {
    return _missionByIdByProfile[profileId][_missionIdBySlugHash[keccak256(bytes(slug))]];
  }

  // The following functions are overrides required by Solidity.
  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(AccessControl)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }
}

