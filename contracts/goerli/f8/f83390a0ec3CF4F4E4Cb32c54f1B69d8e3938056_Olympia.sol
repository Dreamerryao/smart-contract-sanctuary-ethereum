import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {IUniswapV2Router02} from "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import {IUniswapV2Factory} from "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import {IUniswapV2Pair} from "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "hardhat/console.sol";

contract Olympia is ERC20, Ownable {

    using SafeMath for uint256;

    //Contract functional variables
    uint256 private constant TOTAL_SUPPLY = 1000000000 * 1e18;
    bool private _swapping;
    bool public _tradingActive = false;

    // Limits
    uint256 public _maxTransactionAmount;
    uint256 public _maxWallet;
    mapping(address => bool) public _isExcludedFromLimit;
    bool public _limitsInEffect = true;

    // Anti-bot
    mapping(address => uint256) public _lastTransferTimestamp;
    bool public _transferDelay = false;

    // Rewards
    bool public rewardEnable = false;
    uint public counter = 0;
    uint public goal = 5;
    uint public buySteps = 1;
    uint public sellSteps = 2;
    uint256 public minTransAmountForReward = TOTAL_SUPPLY / 1000 * 2; // .2%
    address public lastHitter;
    address public lastTop1Buyer;
    address public lastTop2Buyer;
    uint public hitterRewardPercent = 100; // 10%
    uint public top1BuyerRewardPercent = 70; // 7%
    uint public top2BuyerRewardPercent = 30; // 3%


    mapping(address => uint256) public buyAmount;
    TopBuyer[20] public topBuyers;

    struct TopBuyer {
        uint balance;
        address addr;
    }


    uint public initPenalty = 8000; // 80%
    uint256 public decreasePerBlock = 10; // .1% per block
    mapping(address => uint256) public taxPenalties;

    //Fees
    address public marketingWallet;

    uint public buyTotalFees;
    uint public buyMarketingFee;
    uint public buyLiquidityFee;
    uint public buyRewardFee;

    uint public sellTotalFees;
    uint public sellMarketingFee;
    uint public sellLiquidityFee;
    uint public sellRewardFee;

    uint256 public tokensForMarketing;
    uint256 public tokensForLiquidity;
    uint256 public tokensForReward;
    mapping(address => bool) private _isExcludedFromFees;
    uint256 public _swapTokensAtAmount;

    //Constants
    uint public ONE_HUNDRED_PERCENT = 10000;

    //Uniswap
    IUniswapV2Router02 private router;
    address public pair;
    address private constant DEAD = address(0xdead);

    constructor() ERC20("Olympia", "O") {
        setupSwapRouter();

        _maxTransactionAmount = TOTAL_SUPPLY / 1000 * 5;
        // .5%
        _maxWallet = _maxTransactionAmount * 4;
        // 2%
        _swapTokensAtAmount = TOTAL_SUPPLY / 100;
        // .1%

        buyMarketingFee = 300;
        buyLiquidityFee = 100;
        buyRewardFee = 300;
        buyTotalFees = buyMarketingFee + buyLiquidityFee + buyRewardFee;

        sellMarketingFee = 300;
        sellLiquidityFee = 100;
        sellRewardFee = 500;
        sellTotalFees = sellMarketingFee + sellLiquidityFee + sellRewardFee;

        marketingWallet = owner();
        // set as marketing wallet

        // exclude from paying fees or having max transaction amount
        excludeFromFees(owner(), true);
        excludeFromFees(address(this), true);
        excludeFromFees(address(0xdead), true);
        excludeFromFees(address(marketingWallet), true);

        excludeFromLimit(owner(), true);
        excludeFromLimit(address(this), true);
        excludeFromLimit(address(0xdead), true);
        excludeFromLimit(address(marketingWallet), true);

        _mint(msg.sender, TOTAL_SUPPLY);
    }

    function setupSwapRouter() private {
        router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        pair = IUniswapV2Factory(router.factory()).getPair(address(this), router.WETH());
        if (pair == address(0)) {
            pair = IUniswapV2Factory(router.factory()).createPair(address(this), router.WETH());
        }
        excludeFromLimit(address(pair), true);
    }

    receive() external payable {}

    function getBuyAmount(address userAddress) public view returns (uint value) {
        return buyAmount[userAddress];
    }

    function getTaxPenalty(address userAddress) public view returns (uint value) {
        uint256 duration = block.number - taxPenalties[userAddress];
        uint256 penaltyTax;
        (, penaltyTax) = initPenalty.trySub(duration.mul(decreasePerBlock));
        return penaltyTax;
    }

    function getTopBuyer(uint index) external view returns (uint, address, uint256) {
        return (index, topBuyers[index - 1].addr, topBuyers[index - 1].balance);
    }

    function getTopBuyers() external view returns (uint[] memory, address[] memory, uint256[] memory) {
        uint[] memory rank = new uint256[](20);
        address[] memory addr = new address[](20);
        uint256[] memory balance = new uint256[](20);
        for (uint i = 0; i < 20; i++) {
            rank[i] = i + 1;
            addr[i] = topBuyers[i].addr;
            balance[i] = topBuyers[i].balance;
        }
        return (rank, addr, balance);
    }

    function activeTrading() external onlyOwner {
        _tradingActive = true;
    }

    function removeLimits() external onlyOwner {
        _limitsInEffect = false;
    }

    function disableTransferDelay() external onlyOwner {
        _transferDelay = false;
    }

    function enableTransferDelay() external onlyOwner {
        _transferDelay = true;
    }

    function enableReward() external onlyOwner {
        rewardEnable = true;
    }

    function disableReward() external onlyOwner {
        rewardEnable = false;
    }

    // change the minimum amount of tokens to sell from fees
    function updateSwapTokensAtAmount(uint256 newAmount)
    external
    onlyOwner
    returns (bool)
    {
        require(
            newAmount >= ((totalSupply() * 1) / 10000) / 1e18,
            "Swap amount cannot be lower than 0.01% total supply."
        );
        require(
            newAmount <= ((totalSupply() * 5) / 1000) / 1e18,
            "Swap amount cannot be higher than 0.5% total supply."
        );
        _swapTokensAtAmount = newAmount * 1e18;
        return true;
    }

    function updateMaxTxnAmount(uint256 newAmount) external onlyOwner {
        require(
            newAmount >= ((totalSupply() * 1) / 1000) / 1e18,
            "Cannot set maxTransactionAmount lower than 0.1%"
        );
        _maxTransactionAmount = newAmount * 1e18;
    }

    function updateMaxWalletAmount(uint256 newNum) external onlyOwner {
        require(
            newNum >= ((totalSupply() * 5) / 1000) / 1e18,
            "Cannot set maxWallet lower than 0.5%"
        );
        _maxWallet = newNum * 1e18;
    }

    function excludeFromLimit(address updAds, bool isEx)
    public
    onlyOwner
    {
        _isExcludedFromLimit[updAds] = isEx;
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function isExcludedFromFees(address account) public view returns (bool) {
        return _isExcludedFromFees[account];
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if (!_tradingActive) {
            require(
                from == owner() || to == owner(),
                "Trading is not active."
            );
        }

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if (_swapping) {
            super._transfer(from, to, amount);
            return;
        }

        if (_limitsInEffect) {
            if (//when buy
                from == pair &&
                !_isExcludedFromLimit[to]
            ) {
                require(
                    amount <= _maxTransactionAmount,
                    "Buy transfer amount exceeds the maxTransactionAmount."
                );
                require(
                    amount + balanceOf(to) <= _maxWallet,
                    "Max wallet exceeded"
                );
                // Delay on buy only
                if (
                    _transferDelay
                ) {
                    require(
                        _lastTransferTimestamp[tx.origin] < block.number,
                        "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed."
                    );
                    _lastTransferTimestamp[tx.origin] = block.number;
                }
            } else if (//when sell
                to == pair &&
                !_isExcludedFromLimit[from]
            ) {
                require(
                    amount <= _maxTransactionAmount,
                    "Sell transfer amount exceeds the maxTransactionAmount."
                );
            } else if (!_isExcludedFromLimit[to]) {// transfer
                require(
                    amount + balanceOf(to) <= _maxWallet,
                    "Max wallet exceeded"
                );
            }
        }

        //Swap back
        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= _swapTokensAtAmount;
        if (
            canSwap &&
            _tradingActive &&
            to == pair && //on sell
            !_isExcludedFromFees[from] &&
            !_isExcludedFromFees[to]
        ) {
            swapBack();
        }

        //Take fee
        bool takeFee = true;
        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }
        uint256 fees = 0;
        if (takeFee) {
            // On sell
            if (to == pair && sellTotalFees > 0) {
                fees = amount.mul(sellTotalFees).div(ONE_HUNDRED_PERCENT);
                tokensForLiquidity += (fees * sellLiquidityFee) / sellTotalFees;
                tokensForReward += (fees * sellRewardFee) / sellTotalFees;
                tokensForMarketing += (fees * sellMarketingFee) / sellTotalFees;
            }
            // On buy
            else if (from == pair && buyTotalFees > 0) {
                fees = amount.mul(buyTotalFees).div(ONE_HUNDRED_PERCENT);
                tokensForLiquidity += (fees * buyLiquidityFee) / buyTotalFees;
                tokensForReward += (fees * buyRewardFee) / buyTotalFees;
                tokensForMarketing += (fees * buyMarketingFee) / buyTotalFees;
            }

            // Penalty tax in all: Buy, Sell, Transfer
            //TODO: exclude buy
            if (taxPenalties[tx.origin] > 0) {
                uint256 duration = block.number - taxPenalties[tx.origin];
                uint256 penaltyTax;
                (, penaltyTax) = initPenalty.trySub(duration.mul(decreasePerBlock));
                if (penaltyTax > 0) {
                    uint penaltyFee = amount.mul(penaltyTax).div(ONE_HUNDRED_PERCENT);
                    tokensForReward += penaltyFee;
                    fees += penaltyFee;
                }
            }

            if (fees > 0) {
                super._transfer(from, address(this), fees);
            }

            amount -= fees;
        }

        // Olympia
        if (rewardEnable && (to == pair || from == pair)) {
            // Update steps
            if (to == pair) {//sell
                (, counter) = counter.trySub(sellSteps);
            } else if (from == pair && amount >= minTransAmountForReward) {//buy
                counter += buySteps;
            }

            // Store individual progress
            if (to == pair) {//sell
                processOnSell(tx.origin);
            } else if (from == pair) {//buy
                processOnBuy(tx.origin, amount);
            } else {// transfer

            }

            // Reward
            if (counter >= goal) {
                reward();
            }
        }

        super._transfer(from, to, amount);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        _approve(address(this), address(router), tokenAmount);

        // make the swap
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(router), tokenAmount);

        // add the liquidity
        router.addLiquidityETH{value : ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            marketingWallet,
            block.timestamp
        );
    }

    function swapBack() private lockTheSwap {
        uint256 totalTokensToSwap = tokensForLiquidity + tokensForMarketing;
        bool success;

        if (totalTokensToSwap == 0) {
            return;
        }

        if (totalTokensToSwap > _swapTokensAtAmount * 20) {
            totalTokensToSwap = _swapTokensAtAmount * 20;
        }

        // Halve the amount of liquidity tokens
        uint256 liquidityTokens = tokensForLiquidity / 2;
        uint256 amountToSwapForETH = totalTokensToSwap.sub(liquidityTokens);

        uint256 initialETHBalance = address(this).balance;

        swapTokensForEth(amountToSwapForETH);

        uint256 ethSwaped = address(this).balance.sub(initialETHBalance);

        uint256 ethForMarketing = ethSwaped.mul(tokensForMarketing).div(totalTokensToSwap);

        uint256 ethForLiquidity = ethSwaped - ethForMarketing;

        tokensForLiquidity = 0;
        tokensForMarketing = 0;

        if (liquidityTokens > 0 && ethForLiquidity > 0) {
            addLiquidity(liquidityTokens, ethForLiquidity);
        }

        (success,) = address(marketingWallet).call{value : address(this).balance}("");
    }

    function processOnBuy(address addr, uint amount) private {
        uint256 currentAmount = buyAmount[addr];
        if (isInTop20(currentAmount)) {
            removeFromTop20(addr);
        }
        currentAmount += amount;
        elaborateTop20(addr, currentAmount);
        buyAmount[addr] = currentAmount;
    }

    function processOnSell(address addr) private {
        uint256 currentAmount = buyAmount[addr];
        if (isInTop20(currentAmount)) {
            removeFromTop20(addr);
        }
        buyAmount[addr] = 0;
    }

    function isInTop20(uint256 value) private returns (bool){
        return (topBuyers[topBuyers.length - 1].balance < value);
    }

    function elaborateTop20(address addr, uint256 value) private {
        if (topBuyers[topBuyers.length - 1].balance >= value) {
            return;
        }

        uint i = 0;
        /** get the index of the current max element **/
        for (i; i < topBuyers.length; i++) {
            if (topBuyers[i].balance < value) {
                console.log(">>>> elaborateTop20 Add address %s at index %s ", addr, i);
                break;
            }
        }
        /** shift the array of position (getting rid of the last element) **/
        for (uint j = topBuyers.length - 1; j > i; j--) {
            topBuyers[j].balance = topBuyers[j - 1].balance;
            topBuyers[j].addr = topBuyers[j - 1].addr;
        }
        /** update the new max element **/
        topBuyers[i].balance = value;
        topBuyers[i].addr = addr;
        getTopBuyerByRank(1);
        getTopBuyerByRank(2);
        getTopBuyerByRank(3);
    }

    function removeFromTop20(address addr) private {
        uint i = 0;
        /** get the index of the current max element **/
        for (i; i < topBuyers.length; i++) {
            if (topBuyers[i].addr == addr) {
                console.log(">>>> removeFromTop20 found address %s at index %s ", addr, i);
                break;
            }
        }
        /** shift the array of position (getting rid of the last element) **/
        for (uint j = i; j < topBuyers.length - 1; j++) {
            topBuyers[j].balance = topBuyers[j + 1].balance;
            topBuyers[j].addr = topBuyers[j + 1].addr;
        }
        /** update the new max element **/
        topBuyers[topBuyers.length - 1].balance = 0;
        topBuyers[topBuyers.length - 1].addr = DEAD;
    }

    function getTopBuyerByRank(uint rank) private returns (address){
        console.log(">>>> getTopBuyerByRank rank %s is %s ", rank, topBuyers[rank - 1].addr);
        return topBuyers[rank - 1].addr;
    }

    function reward() private {
        // Reward for hitter
        address winner = tx.origin;
        uint256 rewardForHitter = tokensForReward.mul(hitterRewardPercent).div(ONE_HUNDRED_PERCENT);
        lastHitter = winner;
        taxPenalties[winner] = block.number;
        counter = 0;
        tokensForReward -= rewardForHitter;
        super._transfer(address(this), winner, rewardForHitter);
        console.log(">>>> Reward winner for address %s : %s ", winner, rewardForHitter);

        //Reward for topBuyers
        address top1 = getTopBuyerByRank(1);
        address top2 = getTopBuyerByRank(2);
        if (top1 != DEAD) {
            uint256 rewardForTop1Buyer = tokensForReward.mul(top1BuyerRewardPercent).div(ONE_HUNDRED_PERCENT);
            lastTop1Buyer = top1;
            taxPenalties[top1] = block.number;
            removeFromTop20(top1);
            tokensForReward -= rewardForTop1Buyer;
            super._transfer(address(this), top1, rewardForTop1Buyer);
            console.log(">>>> Reward top 1 for address %s : %s ", top1, rewardForTop1Buyer);
        }

        if (top2 != DEAD) {
            uint256 rewardForTop2Buyer = tokensForReward.mul(top2BuyerRewardPercent).div(ONE_HUNDRED_PERCENT);
            lastTop2Buyer = top2;
            taxPenalties[top2] = block.number;
            removeFromTop20(top2);
            tokensForReward -= rewardForTop2Buyer;
            super._transfer(address(this), top2, rewardForTop2Buyer);
            console.log(">>>> Reward top 2 for address %s : %s ", top2, rewardForTop2Buyer);
        }

        //        emit Reward(winner, rewardAmount);
    }

    modifier lockTheSwap {
        _swapping = true;
        _;
        _swapping = false;
    }

}

