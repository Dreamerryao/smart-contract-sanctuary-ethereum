pragma solidity 0.8.9;

/******************************************************************************\
/// @title Contract Operation Facet - exclusive features for committer and executor
/// @author Ather Labs
/******************************************************************************/

import {AppStorage} from "../libraries/AppStorage.sol";
import {LibAccessControl} from "../libraries/LibAccessControl.sol";
import {LibBytes} from "../libraries/LibBytes.sol";
import {LibSafeCast} from "../libraries/LibSafeCast.sol";
import {LibOperations} from "../libraries/LibOperations.sol";
import {LibWithdrawBalance} from "../libraries/LibWithdrawBalance.sol";
import {LibTokenManagement} from "../libraries/LibTokenManagement.sol";
import {LibStatusControl} from "../libraries/LibStatusControl.sol";
import {LibStrings} from "../libraries/LibStrings.sol";
import {IOperations} from "../interfaces/IOperations.sol";
import {IERC20} from "../interfaces/IERC20.sol";
import {IERC721} from "../interfaces/IERC721.sol";
import {IERC1155} from "../interfaces/IERC1155.sol";
import {ReentrancyGuard} from "../ReentrancyGuard.sol";
import "hardhat/console.sol";

contract ExecuteFacet is IOperations, ReentrancyGuard {
    /* ------ TYPE & VARIABLES ------ */

    /// @dev
    uint256 internal constant EXECUTE_TIME_INTERVAL = 10 minutes;

    /// @dev ERC20 and Native tokens withdrawals gas limit, used only for complete withdrawals
    uint256 internal constant WITHDRAWAL_GAS_LIMIT = 100000;

    ///@dev A fixed deadline timestamp for ERC1155 minting Tx
    uint32 internal constant DEADLINE_TO_MINT_ERC1155C = 4294967295;

    /* ------ EVENTS ------ */

    /// @notice
    event Executed(bytes32 pendingOperationsHash, uint32 batchId, uint32 depositCounter, uint32 executedCounter);

    /// @notice
    event Withdrawn(bytes32 withdrawKey, uint128 amount);

    /// @notice
    event Minted(bytes32 withdrawKey, uint88 amount);

    /* ------ EXTERNAL FUNTIONS ------ */

    /// @notice
    /*
     * requestType == 0 execute deposit request
     * requestType == 1 execute deposit batch request
     * requestType == 2 execute withdraw native token request
     * requestType == 3 execute withdraw ERC20 token request
     * requestType == 4 execute withdraw ERC721 token request
     * requestType == 5 execute withdraw ERC1155 token request
     * requestType == 6 execute withdraw ERC1155C token request (also support batch transaction)
     * requestType == 7 execute withdraw batch ERC1155 token request
     */
    function executeBatch(bytes[] calldata _data) external nonReentrant {
        LibAccessControl._requireExecutor();
        LibStatusControl._requireOperationsEnable();

        CommittedBatchInfo memory commit = s.latestCommittedBatch;
        // require(commit.timestamp + EXECUTE_TIME_INTERVAL < block.timestamp, "EF: Too early to execute");
        require(_data.length >= commit.numOnchainOperations, "EF:data length shorter than requested");
        require(!s.isExecuted[commit.batchId], "EF: Current batch already executed");

        bytes32 pendingOperationsHash_ = s.domainSeparator;
        uint256 startId = s.currentPendingRequestId;
        uint32 depositCounter;
        uint32 executedCounter;

        for (uint256 i = 0; i < _data.length; ++i) {
            bytes memory pubData = _data[i];
            uint8 requestType = LibBytes.bytesToUInt8(pubData, 0);
            // Case 0: Execute deposit request
            if (requestType == 0) {
                Deposit memory deposit = LibOperations._readDepositRequest(pubData, 1);
                require(
                    s.pendingDepositRecord[startId + depositCounter].hashedRequestData ==
                        keccak256(abi.encode(s.domainSeparator, LibOperations._hashDepositRequest(deposit))),
                    "EF: Invalid deposit request data"
                );
                delete s.pendingDepositRecord[startId + depositCounter];
                ++depositCounter;
                // Case 1: Execute deposit batch request
            } else if (requestType == 1) {
                DepositBatch memory depositBatch = LibOperations._readDepositBatchRequest(pubData, 1);
                require(
                    s.pendingDepositRecord[startId + depositCounter].hashedRequestData ==
                        keccak256(abi.encode(s.domainSeparator, LibOperations._hashDepositBatchRequest(depositBatch))),
                    "EF: Invalid deposit batch request data"
                );
                delete s.pendingDepositRecord[startId + depositCounter];
                ++depositCounter;
                // Case 2: Execute withdraw native token request
            } else if (requestType == 2) {
                Withdraw memory withdraw = LibOperations._readWithdrawRequest(pubData, 1);
                require(withdraw.listingId == 0, "EF: Withdrawn token is not native token");
                _withdrawOrStoreCryptoCurrency(withdraw.to, withdraw.listingId, withdraw.amount);
                // Case 3: Execute withdraw ERC20 token request
            } else if (requestType == 3) {
                Withdraw memory withdraw = LibOperations._readWithdrawRequest(pubData, 1);
                require(s.tokenTypes[withdraw.listingId] == 1, "EF: Withdrawn token is not ERC20 token");
                _withdrawOrStoreCryptoCurrency(withdraw.to, withdraw.listingId, withdraw.amount);
                // Case 4: Execute withdraw ERC721 token request
            } else if (requestType == 4) {
                Withdraw memory withdraw = LibOperations._readWithdrawRequest(pubData, 1);
                require(s.tokenTypes[withdraw.listingId] == 2, "EF: Withdrawn token is not ERC721 token");
                _releaseOrMintERC721(withdraw.to, withdraw.listingId, withdraw.tokenId);
                // Case 5: Execute withdraw ERC1155 token request
            } else if (requestType == 5) {
                Withdraw memory withdraw = LibOperations._readWithdrawRequest(pubData, 1);
                require(s.tokenTypes[withdraw.listingId] == 3, "EF: Withdrawn token is not ERC1155 token");
                _releaseOrMintERC1155(withdraw.to, withdraw.listingId, withdraw.tokenId, withdraw.amount);
                // Case 6: Execute withdraw ERC1155C token request
            } else if (requestType == 6) {
                Withdraw1155C memory withdraw1155C = LibOperations._readWithdraw1155CRequest(pubData, 1);
                require(s.tokenTypes[withdraw1155C.listingId] == 4, "EF: Withdrawn token is not ERC1155C token");
                _mintAndStoreERC1155C(
                    withdraw1155C.executeSig,
                    withdraw1155C.to,
                    DEADLINE_TO_MINT_ERC1155C,
                    withdraw1155C.salt,
                    withdraw1155C.listingId,
                    withdraw1155C.tokenIds,
                    withdraw1155C.amounts
                );
                // Case 7: Execute withdraw batch ERC1155 token request
            } else if (requestType == 7) {
                WithdrawBatch memory withdrawBatch = LibOperations._readWithdrawBatchRequest(pubData, 1);
                require(s.tokenTypes[withdrawBatch.listingId] == 3, "EF:not a ERC1155 token withdrawal");

                uint88[] memory tokenIds = new uint88[](withdrawBatch.arrayLength);
                uint88[] memory amounts = new uint88[](withdrawBatch.arrayLength);

                for (uint8 j = 0; j < withdrawBatch.arrayLength; j++) {
                    tokenIds[j] = withdrawBatch.tokenIds[j];
                    amounts[j] = withdrawBatch.amounts[j];
                    _releaseOrMintERC1155(withdrawBatch.to, withdrawBatch.listingId, tokenIds[j], amounts[j]);
                }
            } else {
                revert("EF: Invalid request type");
            }
            pendingOperationsHash_ = LibOperations._concatHash(pendingOperationsHash_, pubData);
            ++executedCounter;
        }

        require(executedCounter == _data.length, "EF: Invalid execution length");
        require(commit.numOnchainOperations == depositCounter, "EF:invalid number of req");
        require(commit.pendingOperationsHash == pendingOperationsHash_, "EF:invalid execution");
        s.isExecuted[commit.batchId] = true;
        s.currentPendingRequestId += depositCounter;
        s.numberOfPendingRequest -= depositCounter;
        ++s.currentBatchId;
        emit Executed(commit.pendingOperationsHash, commit.batchId, depositCounter, executedCounter);
    }

    /// @notice
    function transferERC20(
        IERC20 _tokenAddress,
        address _to,
        uint88 _amount,
        uint256 _minLiquidity
    ) external returns (uint88) {
        require(msg.sender == address(this), "EF:invalid caller");
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this));
        _tokenAddress.transfer(_to, _amount);
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this));
        require(balanceAfter >= _minLiquidity, "EF:Minimum liquidity limit is reached");
        uint88 balanceDiff = LibSafeCast.toUint88(balanceBefore - balanceAfter);
        require(balanceDiff == _amount, "EF:invalid transfer amount");

        return balanceDiff;
    }

    function transferERC721(
        IERC721 _tokenAddress,
        address _to,
        uint88 _tokenId
    ) external returns (uint88) {
        require(msg.sender == address(this), "EF:invalid caller");
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this));
        _tokenAddress.transferFrom(address(this), _to, _tokenId);
        require(_tokenAddress.ownerOf(_tokenId) == _to, "EF:not found transfered NFT");
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this));
        uint88 balanceDiff = LibSafeCast.toUint88(balanceBefore - balanceAfter);
        require(balanceDiff == 1, "EF:invalid transfer amount");

        return balanceDiff;
    }

    function transferERC1155(
        IERC1155 _tokenAddress,
        address _to,
        uint88 _tokenId,
        uint88 _amount,
        uint256 _minLiquidity
    ) external returns (uint88) {
        require(msg.sender == address(this), "EF:invalid caller");
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this), _tokenId);
        _tokenAddress.safeTransferFrom(address(this), _to, _tokenId, _amount, "");
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this), _tokenId);
        require(balanceAfter >= _minLiquidity, "EF:Minimum liquidity limit is reached");
        uint88 balanceDiff = LibSafeCast.toUint88(balanceBefore - balanceAfter);
        require(balanceDiff == _amount, "EF:invalid transfer amount");

        return balanceDiff;
    }

    /// @notice
    function mintERC721(
        IERC721 _tokenAddress,
        address _recipient,
        uint88 _tokenId
    ) external {
        require(msg.sender == address(this), "EF:invalid caller");
        _tokenAddress.mint(_recipient, _tokenId);
        emit Minted(bytes32(abi.encodePacked(_recipient, s.listingIds[address(_tokenAddress)], _tokenId)), 1);
    }

    /// @notice
    function mintERC1155(
        IERC1155 _tokenAddress,
        address _recipient,
        uint88 _tokenId,
        uint88 _amount
    ) external {
        require(msg.sender == address(this), "EF:invalid caller");
        _tokenAddress.mint(_recipient, _tokenId, _amount);
        emit Minted(bytes32(abi.encodePacked(_recipient, s.listingIds[address(_tokenAddress)], _tokenId)), _amount);
    }

    /* ------ INTERNAL FUNTIONS ------ */
    /// @notice
    function _withdrawOrStoreCryptoCurrency(
        address _recipient,
        uint8 _listingId,
        uint88 _amount
    ) internal {
        bool sent = false;
        if (_listingId == 0) {
            address payable toPayable = payable(_recipient);
            sent = _sendNativeTokenNoRevert(toPayable, _amount);
        } else {
            address tokenAddr = LibTokenManagement._validateLisitingId(_listingId);
            try
                this.transferERC20{gas: WITHDRAWAL_GAS_LIMIT}(
                    IERC20(tokenAddr),
                    _recipient,
                    _amount,
                    s.minLiquidity[_listingId]
                )
            {
                sent = true;
            } catch {
                sent = false;
            }
        }
        if (sent) {
            emit Withdrawn(bytes32(abi.encodePacked(_recipient, _listingId, uint88(0))), _amount);
        } else {
            LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, 0, _amount);
        }
    }

    /// @notice Sends native token
    /// @param _to Address of recipient
    /// @param _amount Amount of tokens to transfer
    /// @return bool flag indicating that transfer is successful
    function _sendNativeTokenNoRevert(address payable _to, uint256 _amount) internal returns (bool) {
        (bool callSuccess, ) = _to.call{gas: WITHDRAWAL_GAS_LIMIT, value: _amount}("");
        return callSuccess;
    }

    /// @notice
    function _releaseOrMintERC721(
        address _recipient,
        uint8 _listingId,
        uint88 _tokenId
    ) internal {
        IERC721 tokenAddr = IERC721(LibTokenManagement._validateLisitingId(_listingId));
        if (tokenAddr.ownerOf(_tokenId) == address(this)) {
            try this.transferERC721{gas: WITHDRAWAL_GAS_LIMIT}(tokenAddr, _recipient, _tokenId) {
                emit Withdrawn(bytes32(abi.encodePacked(_recipient, _listingId, _tokenId)), 1);
            } catch {
                LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, _tokenId, 1);
            }
        } else {
            try this.mintERC721{gas: WITHDRAWAL_GAS_LIMIT}(IERC721(tokenAddr), _recipient, _tokenId) {
                emit Withdrawn(bytes32(abi.encodePacked(_recipient, _listingId, _tokenId)), 1);
            } catch {
                LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, _tokenId, 1);
            }
        }
    }

    /// @notice
    function _releaseOrMintERC1155(
        address _recipient,
        uint8 _listingId,
        uint88 _tokenId,
        uint88 _amount
    ) internal {
        IERC1155 tokenAddr = IERC1155(LibTokenManagement._validateLisitingId(_listingId));
        uint256 balance = tokenAddr.balanceOf(address(this), _tokenId);
        if (balance >= _amount) {
            try
                this.transferERC1155{gas: WITHDRAWAL_GAS_LIMIT}(
                    tokenAddr,
                    _recipient,
                    _tokenId,
                    _amount,
                    s.minLiquidity[_listingId]
                )
            {
                emit Withdrawn(bytes32(abi.encodePacked(_recipient, _listingId, _tokenId)), _amount);
            } catch {
                LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, _tokenId, _amount);
            }
        } else {
            uint88 amount = _amount - LibSafeCast.toUint88(balance);
            try this.mintERC1155{gas: WITHDRAWAL_GAS_LIMIT}(tokenAddr, _recipient, _tokenId, amount) {
                try
                    this.transferERC1155{gas: WITHDRAWAL_GAS_LIMIT}(
                        IERC1155(tokenAddr),
                        _recipient,
                        _tokenId,
                        LibSafeCast.toUint88(balance),
                        s.minLiquidity[_listingId]
                    )
                {
                    emit Withdrawn(bytes32(abi.encodePacked(_recipient, _listingId, _tokenId)), _amount);
                } catch {
                    LibWithdrawBalance._increasePendingBalance(
                        _recipient,
                        _listingId,
                        _tokenId,
                        LibSafeCast.toUint88(balance)
                    );
                }
            } catch {
                LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, _tokenId, _amount);
            }
        }
    }

    /// @notice
    function _mintAndStoreERC1155C(
        bytes memory _executeSig,
        address _recipient,
        uint32 _deadline,
        uint24 _salt,
        uint8 _listingId,
        uint88[] memory _tokenIds,
        uint88[] memory _amounts
    ) internal {
        IERC1155 tokenAddr = IERC1155(LibTokenManagement._validateLisitingId(_listingId));

        uint256[] memory tokenIds = new uint256[](_tokenIds.length);
        uint256[] memory amounts = new uint256[](_tokenIds.length);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            tokenIds[i] = _tokenIds[i];
            amounts[i] = _amounts[i];
            LibWithdrawBalance._increasePendingBalance(_recipient, _listingId, _tokenIds[i], _amounts[i]);
        }
        tokenAddr.mintBatch(_deadline, tokenIds, amounts, LibStrings.toString(_salt), _executeSig);
    }
}

