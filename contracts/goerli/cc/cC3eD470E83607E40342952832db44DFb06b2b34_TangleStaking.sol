pragma solidity ^0.8.7;
import "./abdk/ABDKMath64x64.sol";

/**
 * Tools for calculating rewards.
 * Calculation formula: F=P*(1+i)^n
 */
library Calculator {

    function compoundInterestCalculator(
        uint256 principal,
        uint256 timePassed, // in seconds
        uint256 apy // %
   ) internal pure returns (uint256 amount) {
        int128 secondRate = ABDKMath64x64.divu(apy, 100 * 365 days); // second rate
        int128 sum = ABDKMath64x64.add(ABDKMath64x64.fromInt(1), secondRate);
        int128 pow = ABDKMath64x64.pow(sum, timePassed);
        amount = ABDKMath64x64.mulu(pow, principal) - principal;
    }

}

