pragma solidity ^0.8.7;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./library/Calculator.sol";

library Array {
    function indexOf(uint256[] memory array, uint256 value) internal pure returns (uint256) {
        uint256 index = 0;
        for (uint256 i = 0; i < array.length; i++) {
            if (array[i] == value) {
                index = i;
                break;
            }
        }
        return index;
    }
}

/**
 * shimmer nft staking
 */
contract TangleStaking is Context, Ownable, ReentrancyGuard, Pausable, ERC721Holder {
    using Address for address;
    using SafeMath for uint256;
    using Calculator for uint256;
    using Array for uint256[];
    using SafeERC20 for IERC20;

    /**
     * Emitted when a user store farming rewards(ERC20 token).
     * @param sender User address.
     * @param amount Current store amount.
     * @param timestamp The time when store erc20 rewards.
     */
    event ContractFunded(address indexed sender, uint256 amount, uint256 timestamp);

    /**
     * Emitted when the owner claim remaining farming rewards.
     * @param sender User address.
     * @param amount Claim amount.
     * @param timestamp The time when claim remaining funding.
     */
    event FundingClaimed(address indexed sender, uint256 amount, uint256 timestamp);

    /**
     * Emitted when a new baseApy value is set.
     *
     * @param _nftAddress nft contract address
     * @param tokenId array of token id to set the apy
     * @param apy array of apy to set to the above token ids
     */
    event BaseApySet(address _nftAddress, uint256[] tokenId, uint256[] apy);

    /**
     * Emitted when a new lockPeriodDays value is set.
     * @param value A new lock period days value.
     * @param sender The owner address at the moment of lockPeriodDays changing.
     */
    event LockPeriodSet(uint256 value, address sender);

    /**
     * Emitted when a new base reward principal value is set.
     * @param value A new erc20 token value.
     * @param sender The owner address at the moment of base reward principal changing.
     */
    event BaseRewardPrincipalSet(uint256 value, address sender);

    /**
     * @dev Emitted when a user withdraw interest only.
     * @param sender User address.
     * @param principal The principal of user.
     * @param claimTimestamp Claim timestamp.
     */
    event Claimed(
        address indexed sender,
        address indexed nftAddress,
        uint256[] nftIds,
        uint256 principal,
        uint256 claimTimestamp
    );

    /**
     * Emitted when a user stakes nft token.
     * @param sender User address.
     * @param nftAddress The address of nft.
     * @param nftId The nft id.
     * @param rewardPrincipal Reward principal.
     * @param timestamp The time when staking nft.
     */
    event NftStaked(
        address indexed sender,
        address indexed nftAddress,
        uint256 nftId,
        uint256 rewardPrincipal,
        uint256 timestamp
    );

    /**
     * Emitted when a user unstake nft token.
     * @param sender User address.
     * @param nftAddress The address of nft.
     * @param nftId The nft id.
     * @param rewardAmount The amount of erc20 reward claimed.
     * @param timestamp The time when unstake nft.
     */
    event NftUnstaked(
        address indexed sender,
        address indexed nftAddress,
        uint256 nftId,
        uint256 rewardAmount,
        uint256 timestamp
    );

    /**
     * Emitted when add nft to allow list or remove nft from allow list
     * @param nftAddress The address of nft.
     * @param isAdded true: add, false: remove
     */
    event AllowListSet(address nftAddress, bool isAdded);

    /**
     * Emitted when the total funding is not enough to pay
     * @param user User address.
     * @param nftAddress The address of nft.
     * @param nftId The nft id.
     * @param rewardAmount The amount of erc20 reward unpaid.
     */
    event UnpaidRewards(address indexed user, address indexed nftAddress, uint256 nftId, uint256 rewardAmount);

    /**
     * Emitted when user withdraws a previously unpaid reward
     * @param user User address.
     * @param rewardAmount The amount of reward.
     */
    event WithdrawUnpaidRewards(address indexed user, uint256 rewardAmount);

    // ERC20 about
    // erc20 reward token
    IERC20 public erc20Token;
    // The farming rewards of users(address => total amount)
    mapping(address => uint256) public funding;
    // The total farming rewards for users
    uint256 public totalFunding;
    uint256 public claimDuration;

    // ERC721 about
    // base apy
    mapping(address => mapping(uint256 => uint256)) public baseApy;
    // Lock Period 365 days
    uint256 public lockPeriodDays = 365 days;
    // base reward principal: 1000 * 10**decimals()
    uint256 public baseRewardPrincipal = 1000 * (10**18);
    // struct NftRewardInfo
    struct NftRewardInfo {
        uint256 baseApy;
        uint256 stakeTime;
        uint256 claimedTime;
        uint256 lockPeriodDays;
        uint256 rewardPrincipal;
        uint256 interestClaimed;
    }
    // Mapping (nftAddress => nftId => user address => user reward info)
    mapping(address => mapping(uint256 => mapping(address => NftRewardInfo))) public stakeNftRewardInfo;
    // Mapping (nftAddress => bool) nft address allow list
    mapping(address => bool) public allowList;
    mapping(address => mapping(address => uint256[])) public stakedNftById;
    // Mapping (rate => number of locked nfts) locked nft count per rate
    mapping(uint256 => uint256) public totalLocked;
    // total unpaid rewards
    uint256 public totalUnpaidRewards;
    // the user's unpaid rewards
    mapping(address => uint256) public unpaidRewards;

    /**
     * @dev Initializes the contract by setting a ERC20 tokenAddress.
     */
    constructor(address tokenAddress_, address nftAddress_) {
        require(tokenAddress_.isContract(), "must be contract address");
        // init erc20Token
        erc20Token = IERC20(tokenAddress_);
        // initialize allow list
        setAllowList(nftAddress_);
        claimDuration = 1 days;
    }

    /**
     * @notice reward fund cannot be less than 0
     * @dev set rewards fund
     * @param amount Reward funds that need to be set
     */
    function fundingContract(uint256 amount) external {
        require(amount > 0, "invalid amount");
        funding[_msgSender()] += amount;
        // increase total funding
        totalFunding += amount;
        // transfer to the address of the initiating contract through erc20
        erc20Token.safeTransferFrom(_msgSender(), address(this), amount);
        // send event
        emit ContractFunded(_msgSender(), amount, _now());
    }

    /**
     * Only owner can set the daily rate.
     *
     * Note: Set apy for nft token id with batch action
     *
     * @param _nftAddress nft contract address
     * @param tokenId array of token id to set the apy
     * @param apy array of apy to set to the above token ids
     */
    function setBaseApy(
        address _nftAddress,
        uint256[] memory tokenId,
        uint256[] memory apy
    ) external onlyOwner {
        require(_nftAddress != address(0), "invalid nft");
        require(tokenId.length == apy.length, "length not match");
        for (uint256 i = 0; i < tokenId.length; i++) {
            baseApy[_nftAddress][tokenId[i]] = apy[i];
        }
        emit BaseApySet(_nftAddress, tokenId, apy);
    }

    /**
     * Only owner can set lock Period.
     *
     * Note: If you want to set 365 days, just pass 365 days
     *
     * @param lockDays lock days
     */
    function setLockPeriod(uint256 lockDays) external onlyOwner {
        lockPeriodDays = lockDays;
        // send event
        emit LockPeriodSet(lockPeriodDays, _msgSender());
    }

    /**
     * Only owner can set reward erc20 token number
     *
     * @param rewardPrincipal Base reward principal.
     */
    function setBaseRewardPrincipal(uint256 rewardPrincipal) external onlyOwner {
        baseRewardPrincipal = rewardPrincipal;
        // send event
        emit BaseRewardPrincipalSet(baseRewardPrincipal, _msgSender());
    }

    function setClaimDuration(uint256 duration) external onlyOwner {
        claimDuration = duration;
    }

    function stakeNft(address nftAddress, uint256 tokenId) external whenNotPaused nonReentrant {
        require(nftAddress.isContract(), "must be contract address");
        require(_isExistsAllowList(nftAddress), "nft address not in the allow list");

        _stakeNft(_msgSender(), address(this), nftAddress, tokenId);
    }

    // (nftAddress -> nftId -> user address -> user reward info)
    // mapping(address => mapping(uint256 => mapping(address => NftRewardInfo))) stakeNftRewardInfo;
    function _stakeNft(
        address _from,
        address _to,
        address _nftAddress,
        uint256 _tokenId
    ) internal {
        // check nt owner
        require(_getERC721(_nftAddress).ownerOf(_tokenId) == _from, "not owning item");
        require(baseApy[_nftAddress][_tokenId] > 0, "apy is not set");
        // transfer nft
        _getERC721(_nftAddress).transferFrom(_from, _to, _tokenId);
        totalLocked[baseApy[_nftAddress][_tokenId]] = totalLocked[baseApy[_nftAddress][_tokenId]] + 1;
        stakedNftById[_nftAddress][_from].push(_tokenId);
        // save mapping (nftAddress -> nftId -> user address -> user reward info)
        stakeNftRewardInfo[_nftAddress][_tokenId][_from] = NftRewardInfo(
            baseApy[_nftAddress][_tokenId],
            _now(),
            _now(),
            lockPeriodDays,
            baseRewardPrincipal,
            0
        );
        // send event
        emit NftStaked(_from, _nftAddress, _tokenId, baseRewardPrincipal, _now());
    }

    function unstakeNft(address nftAddress, uint256 tokenId) external whenNotPaused nonReentrant {
        require(nftAddress.isContract(), "must be contract address");
        // get user reward info
        NftRewardInfo memory rewardInfo = stakeNftRewardInfo[nftAddress][tokenId][_msgSender()];
        // check if the user have nft staked
        require(rewardInfo.stakeTime > 0, "no nft staked");
        // check lock period
        require(_checkLockPeriod(rewardInfo), "still within lock period");
        _unstakeNft(nftAddress, tokenId, rewardInfo);
    }

    /**
     * Unstake nft token.
     *
     * @param _nftAddress The address of nft contract.
     * @param _tokenId The nft id.
     */
    function _unstakeNft(
        address _nftAddress,
        uint256 _tokenId,
        NftRewardInfo memory rewardInfo
    ) internal {
        // calculation total interest
        uint256 totalInterest = _calculationInterest(rewardInfo);
        bool isSufficientFunds;
        // transfer erc20 token plus interest
        if (totalInterest > 0) {
            if (totalFunding >= totalInterest) {
                totalFunding -= totalInterest;
                isSufficientFunds = true;
                erc20Token.safeTransfer(_msgSender(), totalInterest);
            } else {
                // when the total funding is not enough to pay interest
                unpaidRewards[_msgSender()] += totalInterest;
                totalUnpaidRewards += totalInterest;
                emit UnpaidRewards(_msgSender(), _nftAddress, _tokenId, totalInterest);
            }
        }
        totalLocked[baseApy[_nftAddress][_tokenId]] = totalLocked[baseApy[_nftAddress][_tokenId]] - 1;

        delete stakeNftRewardInfo[_nftAddress][_tokenId][_msgSender()];

        uint256 index = Array.indexOf(stakedNftById[_nftAddress][_msgSender()], _tokenId);
        uint256 length = stakedNftById[_nftAddress][_msgSender()].length;
        stakedNftById[_nftAddress][_msgSender()][index] = stakedNftById[_nftAddress][_msgSender()][length - 1];
        stakedNftById[_nftAddress][_msgSender()].pop();

        // transfer nft token from current contract
        _getERC721(_nftAddress).safeTransferFrom(address(this), _msgSender(), _tokenId);

        // send event
        emit NftUnstaked(_msgSender(), _nftAddress, _tokenId, isSufficientFunds ? totalInterest : 0, _now());
    }

    /**
     * User withdraw their unpaid rewards.
     */
    function withdrawUnpaidRewards() external whenNotPaused nonReentrant {
        uint256 rewardAmount = unpaidRewards[_msgSender()];
        require(rewardAmount > 0, "no unpaid rewards");
        require(totalFunding >= rewardAmount, "insufficient funds");

        unpaidRewards[_msgSender()] = 0;
        totalUnpaidRewards -= rewardAmount;
        totalFunding -= rewardAmount;
        erc20Token.safeTransfer(_msgSender(), rewardAmount);
        emit WithdrawUnpaidRewards(_msgSender(), rewardAmount);
    }

    // Check if the lockout period has expired
    function _checkLockPeriod(NftRewardInfo memory _ntfRewardInfo) internal view virtual returns (bool) {
        return _now().sub(_ntfRewardInfo.stakeTime) >= _ntfRewardInfo.lockPeriodDays;
    }

    /**
     * Withdraw the interest only of the user, and update interest claimed.
     */
    function _claim(address nftAddress, uint256 tokenId) internal whenNotPaused {
        // check contract address
        require(nftAddress.isContract(), "must be contract address");
        // get user reward info
        NftRewardInfo memory rewardInfo = stakeNftRewardInfo[nftAddress][tokenId][_msgSender()];
        // check if user have stake nft
        require(rewardInfo.stakeTime > 0, "no nft staked");
        // calculate total interest
        uint256 totalInterest = _calculationInterest(rewardInfo);
        require(totalInterest > 0, "no interest to claim");
        // transfer erc20 token interest
        erc20Token.safeTransfer(_msgSender(), totalInterest);

        rewardInfo.interestClaimed += totalInterest;
        rewardInfo.claimedTime = _now();
        stakeNftRewardInfo[nftAddress][tokenId][_msgSender()] = rewardInfo;
        totalFunding -= totalInterest;
    }

    function batchClaim(address nftAddress, uint256[] memory tokenIds) external whenNotPaused nonReentrant {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _claim(nftAddress, tokenIds[i]);
        }
        emit Claimed(_msgSender(), nftAddress, tokenIds, baseRewardPrincipal, _now());
    }

    // calculation interest
    function _calculationInterest(NftRewardInfo memory _rewardInfo) internal view returns (uint256) {
        // time interval seconds
        uint256 timeInterval = _now().sub(_rewardInfo.stakeTime);
        uint256 n = timeInterval / claimDuration;
        if (n < 1) {
            return 0;
        }
        // get total interest
        uint256 totalInterest = Calculator.compoundInterestCalculator(
            _rewardInfo.rewardPrincipal,
            timeInterval,
            _rewardInfo.baseApy
        );
        return totalInterest - _rewardInfo.interestClaimed;
    }

    function getTotalLockedPerRate(uint256 rate) external view returns (uint256) {
        return totalLocked[rate];
    }

    /**
     * Get erc20 token balance by address.
     * @param addr The address of the account that needs to check the balance.
     * @return Return balance of erc20 token.
     */
    function getERC20Balance(address addr) external view returns (uint256) {
        return erc20Token.balanceOf(addr);
    }

    /**
     * Get erc721 token instance by address.
     */
    function _getERC721(address _nftAddress) internal pure returns (IERC721) {
        IERC721 nftContract = IERC721(_nftAddress);
        return nftContract;
    }

    /**
     * Get nft balance by user address and nft id.
     *
     * @param user The address of user.
     * @param nftAddress The address of nft contract.
     */
    function getNftBalance(address user, address nftAddress) external view returns (uint256) {
        return _getERC721(nftAddress).balanceOf(user);
    }

    function getRewardInfo(
        address nftAddress,
        uint256 id,
        address user
    ) external view returns (NftRewardInfo memory) {
        return stakeNftRewardInfo[nftAddress][id][user];
    }

    function getStakedNftById(address nftAddress, address user) external view returns (uint256[] memory) {
        return stakedNftById[nftAddress][user];
    }

    function getClaimableAmount(address nftAddress, uint256[] memory tokenIds) external view returns (uint256) {
        uint256 sum = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            NftRewardInfo memory rewardInfo = stakeNftRewardInfo[nftAddress][tokenIds[i]][_msgSender()];
            sum = sum + _calculationInterest(rewardInfo);
        }
        return sum;
    }

    // withdraw remaining erc20 tokens(only owner)
    function withdraw() external onlyOwner returns (uint256) {
        totalFunding = 0;
        uint256 balance = erc20Token.balanceOf(address(this));
        erc20Token.safeTransfer(_msgSender(), balance);
        emit FundingClaimed(_msgSender(), balance, _now());
        return balance;
    }

    /**
     * Pauses all token stake, unstake.
     *
     * See {Pausable-_pause}.
     *
     * Requirements: the caller must be the owner.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * Unpauses all token stake, unstake.
     *
     * See {Pausable-_unpause}.
     *
     * Requirements: the caller must be the owner.
     */
    function unpause() external onlyOwner {
        _unpause();
    }

    /**
     * @return Returns current timestamp.
     */
    function _now() internal view returns (uint256) {
        return block.timestamp;
    }

    /**
     * set nft allow list
     *
     * @param nftAddress The address of nft contract.
     */
    function setAllowList(address nftAddress) public onlyOwner {
        require(nftAddress.isContract(), "must be contract address");
        require(!_isExistsAllowList(nftAddress), "nft address already exists in allow list");
        allowList[nftAddress] = true;
        emit AllowListSet(nftAddress, true);
    }

    /**
     * Remove nft from allow list
     *
     * * @param nftAddress The address of nft contract.
     */
    function removeFromAllowList(address nftAddress) external onlyOwner {
        require(_isExistsAllowList(nftAddress), "not in allow list");
        allowList[nftAddress] = false;
        emit AllowListSet(nftAddress, false);
    }

    /**
     * Check if nft address already exists allow list.
     *
     * @param nftAddress The address of nft contract.
     */
    function _isExistsAllowList(address nftAddress) internal view returns (bool) {
        return allowList[nftAddress];
    }
}

