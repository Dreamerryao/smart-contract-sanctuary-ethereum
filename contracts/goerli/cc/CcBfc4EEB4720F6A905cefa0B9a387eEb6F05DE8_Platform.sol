pragma solidity ^0.8.7;

contract Interfaces {
    struct Account {
        AccountType accountType;
        address accountAddress;
        uint256 index; // ! index of the account in the corresponding array (parties, donors, services)
        uint256 balance;
    }

    struct Party {
        uint256 partyId;
        string name;
        string motto;
        string description;
        address partyAddress;
    }

    struct Donor {
        uint256 donorId;
        DonorType donorType;
        string name;
        string nationality;
        string description;
    }

    struct Service {
        uint256 serviceId;
        ServiceType serviceType;
        string name;
        string location;
        string description;
    }

    struct Campaign {
        uint256 campaignId;
        uint256 fundTotal;
        bool isActive;
    }

    struct CampaignParty {
        address partyAddress;
        uint256 fundRaised;
    }

    struct Donation {
        address from;
        address to;
        DonorType donorType;
        uint256 amount;
        uint256 index; // ! donation array index
        uint256 campaignIndex;
        uint256 partyIndex;
        string description;
    }

    struct Spending {
        address from;
        address to;
        ServiceType serviceType;
        uint256 amount;
        uint256 serviceIndex;
        string description;
    }

    enum AccountType {
        ADMIN,
        PARTY,
        DONOR,
        USER,
        SERVICES
    }

    enum DonorType {
        INDIVIDUAL,
        NGOS,
        CORPORTATE
    }

    enum ServiceType {
        FOOD,
        CLOTHES,
        MEDICINE,
        EDUCATION,
        PRINTING,
        SOCIAL_MEDIA,
        TRANSPORT,
        MILITARY,
        ELECTRICITY,
        WATER_SUPPLY,
        EMERGENCY_SERVICES,
        OTHER
    }
}

