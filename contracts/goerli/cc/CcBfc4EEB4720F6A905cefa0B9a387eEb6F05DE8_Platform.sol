pragma solidity ^0.8.7;

import "./Interfaces.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

error NotAdmin();
error Registered();
error InvalidCampaign();
error InvalidIndex();
error InvalidParty();
error NotRegistered();
error NotParty();
error InsufficientFunds();

contract Platform is Ownable {
    mapping(address => Interfaces.Account) private accounts;
    Interfaces.Party[] private parties;
    Interfaces.Donor[] private donors;
    Interfaces.Service[] private services;

    Interfaces.Campaign[] private campaigns; // ! list of campaigns
    mapping(uint256 => Interfaces.CampaignParty[])
        private campaignIndexToParties; // ! campaign index => list of CampaignParties
    mapping(address => uint256[]) private partyToCampaignIndexes; // ! party address => list of campaign indexes

    Interfaces.Donation[] private donations; // ! list of donations
    mapping(uint256 => uint256[]) private campaignIndexToDonations; // ! campaign index => list of donation indexes

    mapping(address => Interfaces.Spending[]) private partySpendings; // ! party address => list of donation indexes

    constructor(address admin) payable {
        transferOwnership(admin);
    }

    // ! modifier to check if the account is registered
    modifier verifyRegisration(address _addr) {
        if (accounts[_addr].accountAddress == address(0)) {
            revert NotRegistered();
        }
        _;
    }

    function registerAccount(
        Interfaces.AccountType accountType,
        uint256 enumType,
        string memory str1,
        string memory str2,
        string memory str3
    ) public returns (uint256, address) {
        if (
            accountType == Interfaces.AccountType.ADMIN && msg.sender != owner()
        ) {
            revert NotAdmin();
        }
        if (accounts[msg.sender].accountAddress != address(0)) {
            revert Registered();
        }
        uint256 index;
        if (accountType == Interfaces.AccountType.PARTY) {
            index = parties.length;
            parties.push(Interfaces.Party(index, str1, str2, str3, msg.sender));
        } else if (accountType == Interfaces.AccountType.DONOR) {
            index = donors.length;
            donors.push(
                Interfaces.Donor(
                    index,
                    Interfaces.DonorType(enumType),
                    str1,
                    str2,
                    str3
                )
            );
        } else if (accountType == Interfaces.AccountType.SERVICES) {
            index = services.length;
            services.push(
                Interfaces.Service(
                    index,
                    Interfaces.ServiceType(enumType),
                    str1,
                    str2,
                    str3
                )
            );
        }
        Interfaces.Account memory account = Interfaces.Account(
            accountType,
            msg.sender,
            index,
            0
        );
        accounts[msg.sender] = account;
        return (index, msg.sender);
    }

    function createCampaign(
        address[] memory _parties
    ) public onlyOwner returns (uint256) {
        Interfaces.Campaign memory campaign = Interfaces.Campaign(
            campaigns.length,
            0,
            true
        );
        campaigns.push(campaign);
        uint256 campaignIndex = campaigns.length - 1;
        for (uint256 i = 0; i < _parties.length; i++) {
            Interfaces.CampaignParty memory campaignParty = Interfaces
                .CampaignParty(_parties[i], 0);
            campaignIndexToParties[campaignIndex].push(campaignParty);
            partyToCampaignIndexes[_parties[i]].push(campaignIndex);
        }
        return campaignIndex;
    }

    function addPartyToCampaign(
        uint256 campaignIndex,
        address partyAddress
    ) public onlyOwner {
        if (campaignIndex >= campaigns.length) {
            revert InvalidCampaign();
        }
        Interfaces.CampaignParty memory campaignParty = Interfaces
            .CampaignParty(partyAddress, 0);
        campaignIndexToParties[campaignIndex].push(campaignParty);
        partyToCampaignIndexes[partyAddress].push(campaignIndex);
    }

    function donate(
        address partyAddress,
        uint256 campaignIndex,
        uint256 partyIndex,
        Interfaces.DonorType donorType,
        string memory description
    ) public payable verifyRegisration(msg.sender) {
        if (campaignIndex >= campaigns.length) {
            revert InvalidCampaign();
        }
        if (partyIndex >= campaignIndexToParties[campaignIndex].length) {
            revert InvalidIndex();
        }
        if (
            campaignIndexToParties[campaignIndex][partyIndex].partyAddress !=
            partyAddress
        ) {
            revert InvalidParty();
        }
        uint256 fundRaised = campaignIndexToParties[campaignIndex][partyIndex]
            .fundRaised;
        campaignIndexToParties[campaignIndex][partyIndex].fundRaised =
            fundRaised +
            msg.value;
        campaigns[campaignIndex].fundTotal += msg.value;
        accounts[partyAddress].balance += msg.value;

        uint256 donationIndex = donations.length;
        Interfaces.Donation memory donation = Interfaces.Donation(
            msg.sender,
            partyAddress,
            donorType,
            msg.value,
            donationIndex,
            campaignIndex,
            partyIndex,
            description
        );
        donations.push(donation);
        campaignIndexToDonations[campaignIndex].push(donationIndex);
    }

    // ! getters
    function isRegistered(address accountAddress) public view returns (bool) {
        return accounts[accountAddress].accountAddress != address(0);
    }

    function getAccount(
        address accountAddress
    )
        public
        view
        verifyRegisration(accountAddress)
        returns (Interfaces.Account memory)
    {
        return accounts[accountAddress];
    }

    function getDonor(
        uint256 donorIndex
    ) public view returns (Interfaces.Donor memory) {
        if (donors.length == 0 || donorIndex > donors.length) {
            revert InvalidIndex();
        }
        return donors[donorIndex];
    }

    function getDonorByAddr(
        address donorAddr
    )
        public
        view
        verifyRegisration(donorAddr)
        returns (Interfaces.Donor memory)
    {
        return donors[accounts[donorAddr].index];
    }

    function getParty(
        uint256 partyIndex
    ) public view returns (Interfaces.Party memory) {
        if (parties.length == 0 || partyIndex > parties.length) {
            revert InvalidIndex();
        }
        return parties[partyIndex];
    }

    function getParties() public view returns (Interfaces.Party[] memory) {
        return parties;
    }

    function getPartyByAddr(
        address partyAddr
    )
        public
        view
        verifyRegisration(partyAddr)
        returns (Interfaces.Party memory)
    {
        return parties[accounts[partyAddr].index];
    }

    function getService(
        uint256 serviceIndex
    ) public view returns (Interfaces.Service memory) {
        if (services.length == 0 || serviceIndex > services.length) {
            revert InvalidIndex();
        }
        return services[serviceIndex];
    }

    function getServiceByAddr(
        address serviceAddr
    )
        public
        view
        verifyRegisration(serviceAddr)
        returns (Interfaces.Service memory)
    {
        return services[accounts[serviceAddr].index];
    }

    function getCampaigns() public view returns (Interfaces.Campaign[] memory) {
        return campaigns;
    }

    function getCampaign(
        uint256 campaignIndex
    ) public view returns (Interfaces.Campaign memory) {
        if (campaigns.length == 0 || campaignIndex > campaigns.length) {
            revert InvalidIndex();
        }
        return campaigns[campaignIndex];
    }

    function _getCampaignsByIndexes(
        uint256[] memory _indexes
    ) internal view returns (Interfaces.Campaign[] memory) {
        Interfaces.Campaign[] memory _campaigns = new Interfaces.Campaign[](
            _indexes.length
        );
        for (uint256 i = 0; i < _indexes.length; i++) {
            if (_indexes[i] > campaigns.length) {
                revert InvalidIndex();
            }
            _campaigns[i] = campaigns[_indexes[i]];
        }
        return _campaigns;
    }

    function getCampaignParties(
        uint256 campaignIndex
    ) public view returns (Interfaces.CampaignParty[] memory) {
        if (campaigns.length == 0 || campaignIndex > campaigns.length) {
            revert InvalidIndex();
        }
        return campaignIndexToParties[campaignIndex];
    }

    function getPartyCampaings(
        address partyAddress
    )
        public
        view
        verifyRegisration(partyAddress)
        returns (Interfaces.Campaign[] memory)
    {
        uint256 index = accounts[partyAddress].index;
        if (bytes(parties[index].name).length == 0) {
            revert NotRegistered();
        }
        return _getCampaignsByIndexes(partyToCampaignIndexes[partyAddress]);
    }

    function getDonations() public view returns (Interfaces.Donation[] memory) {
        return donations;
    }

    function _getCampaignDonationsByIndexes(
        uint256[] memory _indexes
    ) internal view returns (Interfaces.Donation[] memory) {
        Interfaces.Donation[] memory _donations = new Interfaces.Donation[](
            _indexes.length
        );
        for (uint256 i = 0; i < _indexes.length; i++) {
            if (_indexes[i] > donations.length) {
                revert InvalidIndex();
            }
            _donations[i] = donations[_indexes[i]];
        }
        return _donations;
    }

    function getCampaignDonations(
        uint256 campaignIndex
    ) public view returns (uint256 total, Interfaces.Donation[] memory) {
        if (campaigns.length == 0 || campaignIndex > campaigns.length) {
            revert InvalidIndex();
        }
        return (
            getCampaignTotalFund(campaignIndex),
            _getCampaignDonationsByIndexes(
                campaignIndexToDonations[campaignIndex]
            )
        );
    }

    function getCampaignTotalFund(
        uint256 campaignIndex
    ) public view returns (uint256) {
        if (campaigns.length == 0 || campaignIndex > campaigns.length) {
            revert InvalidIndex();
        }
        return campaigns[campaignIndex].fundTotal;
    }

    function spendFunds(
        uint256 amount,
        address to,
        Interfaces.ServiceType serviceType,
        uint256 serviceIndex,
        string memory description
    ) public verifyRegisration(msg.sender) {
        Interfaces.Account memory account = accounts[msg.sender];
        if (account.accountType != Interfaces.AccountType.PARTY) {
            revert NotParty();
        }
        if (account.balance < amount) {
            revert InsufficientFunds();
        }
        // check if service is valid
        if (services.length == 0 || serviceIndex > services.length) {
            revert InvalidIndex();
        }
        accounts[msg.sender].balance -= amount;
        accounts[to].balance += amount;
        Interfaces.Spending memory spending = Interfaces.Spending(
            msg.sender,
            to,
            serviceType,
            amount,
            serviceIndex,
            description
        );
        partySpendings[to].push(spending);
    }

    function getServiceSpendings(
        address serviceAddress
    )
        public
        view
        verifyRegisration(serviceAddress)
        returns (Interfaces.Spending[] memory)
    {
        return partySpendings[serviceAddress];
    }
}

