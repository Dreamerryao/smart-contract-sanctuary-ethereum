pragma solidity ^0.8.0;




import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "./FactoryGuard.sol";


import "hardhat/console.sol";

contract OwnableDelegateProxy { }

contract ProxyRegistry {
  mapping(address => OwnableDelegateProxy) public proxies;
}

contract DeezShop is ERC1155, ERC1155Holder, AccessControl, ReentrancyGuard, FactoryGuard {
    bytes32 public constant CONTROLLER_ROLE = keccak256("CONTROLLER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    address proxyRegistryAddress;
    string public name;
    string public symbol;

    IERC721 nutsCollectionContract;

    event EquipEvent(uint16 nutId, uint256 tokenType, uint8 isEquip);

    struct TypeWalletLimit {
        uint8 walletLimit;
        mapping(address => uint8) alreadyMinted;
    }

    struct TokenType {
        string typeName;
        string baseUri;
        bool isNFT;
        bool isRedeemable; 
    }

    struct TokenSaleData {
        bool isEthPayable;
        uint16 currentEthPurchases;
        uint16 maxEthPurchases;
        uint16 maxSupply;    
        uint16 nextTokenId;
        uint48 saleStartTime; 
        uint16 hourlyIncrement;
        uint16 currentRegularPurchases;
        uint16 maxRegularPurchases;
        string paymentMethod;
        uint256 ethPriceInWei;
        uint256 priceInWei;
        bool onlyFactory;
    }

    struct NewTokenParams {
        string _typeName;
        string _baseUri; 
        bool _isNFT; 
        bool _isRedeemable;
    }

    struct NewSaleParams {
        uint16 _maxSupply; 
        uint256 _priceInWei; 
        uint16 _maxRegularPurchases; 
        uint8 _walletLimit; 
        string _paymentMethod; 
        bool _ethPayable; 
        uint16 _maxEthPurchases; 
        uint256 _ethPriceInWei; 
        uint16 _hourlyIncrement;
        bool _onlyFactory;
    }

    struct PaymentMethod {
        IERC20 contractReference;
        uint256 redeemableAmount;
    }

    uint256 public nextTokenTypeId;
    mapping(uint256 => TokenType) public tokenTypes;
    mapping(uint256 => TokenSaleData) public tokenSales;
    mapping(uint256 => TypeWalletLimit) typeWalletLimits;
    // Stores possible payment methods for tokens
    mapping(string => PaymentMethod) public paymentMethods;

    // nested mapping for equipments NutsTokenId => ShopTokenTypeId => EquippedAmount
    mapping(uint16 => mapping(uint128 => uint16)) private equipedItems;
    
    mapping(uint256 => uint256) public tokenToTypeMapping;
    mapping(uint256 => uint16) public tokenToTypeIdMapping;

    uint256[] public allTokenTypes;
    string[] public allPaymentMethods;

    constructor(string memory _name, string memory _symbol, address _proxyRegistryAddress, address _nutsCollectionAddress) ERC1155("DEPLOYMENT_URI") {
        name = _name;
        symbol = _symbol;
        proxyRegistryAddress = _proxyRegistryAddress;
        nextTokenTypeId = 1;
        nutsCollectionContract = IERC721(_nutsCollectionAddress);
        _setupRole(AccessControl.DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(CONTROLLER_ROLE, msg.sender);
    }

    function getEquipedItem(uint16 _nutId, uint128 _typeId) public view returns (uint16) {
        return equipedItems[_nutId][_typeId];
    }

    function getAllTokenTypes() public view returns (uint256[] memory) {
        return allTokenTypes;
    }

    function getBatchTokenTypes(uint256[] memory _tokenIds) public view returns (uint256[] memory) {
        uint256[] memory idTypes = new uint256[](_tokenIds.length);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            idTypes[i] = (tokenToTypeIdMapping[_tokenIds[i]]);
        }

        return idTypes;
    }

    function getAllPaymetnMethods() public view returns (string[] memory) {
        return allPaymentMethods;
    }

    function grantAdminRole(address _to) public {
        grantRole(CONTROLLER_ROLE, _to);
    }

    function equipItem(uint16 _nutsTokenId, uint128 _tokenTypeId) public nonReentrant {
        require(nutsCollectionContract.ownerOf(_nutsTokenId) == msg.sender, "Only owner of token can equip items to nut");
        require(balanceOf(msg.sender, _tokenTypeId) > 0, "You do not own the item you are trying to equip");
        equipedItems[uint16(_nutsTokenId)][uint128(_tokenTypeId)] += 1;
        safeTransferFrom(msg.sender, address(this), _tokenTypeId, 1, "");
        emit EquipEvent(_nutsTokenId, _tokenTypeId, 1);
    }

    function takeItem(uint16 _nutsTokenId, uint128 _tokenTypeId) public nonReentrant {
        require(nutsCollectionContract.ownerOf(_nutsTokenId) == msg.sender, "Only owner of token can take items from nut");
        require(equipedItems[uint16(_nutsTokenId)][uint128(_tokenTypeId)] > 0, "The nut doesn't hold the item right now");
        equipedItems[uint16(_nutsTokenId)][uint128(_tokenTypeId)] -= 1;
        this.safeTransferFrom(address(this), msg.sender, _tokenTypeId, 1, "");
        emit EquipEvent(_nutsTokenId, _tokenTypeId, 0);

    }

    function createTokenType(NewTokenParams calldata params) public onlyRole(CONTROLLER_ROLE) {
        TokenType storage newType = tokenTypes[nextTokenTypeId];
        newType.typeName = params._typeName;
        newType.baseUri = params._baseUri;
        newType.isNFT = params._isNFT;
        newType.isRedeemable = params._isRedeemable;

        if(!params._isNFT) {
            tokenToTypeMapping[nextTokenTypeId] = nextTokenTypeId;
        }

        allTokenTypes.push(nextTokenTypeId);
        nextTokenTypeId += 1;
    }

    function createOrEditTokenSale(uint256 _tokenId, NewSaleParams calldata params) public onlyRole(CONTROLLER_ROLE) {
        TokenSaleData storage newSale = tokenSales[_tokenId];
        newSale.saleStartTime = uint48(block.timestamp);
        newSale.hourlyIncrement = params._hourlyIncrement;
        newSale.maxSupply = params._maxSupply;
        newSale.priceInWei = params._priceInWei;
        newSale.currentRegularPurchases = newSale.currentRegularPurchases;
        newSale.maxRegularPurchases = params._maxRegularPurchases;
        newSale.paymentMethod = params._paymentMethod;
        newSale.isEthPayable = params._ethPayable;
        newSale.maxEthPurchases = params._maxEthPurchases;
        newSale.ethPriceInWei = params._ethPriceInWei;
        newSale.onlyFactory = params._onlyFactory;
        newSale.currentEthPurchases = newSale.currentEthPurchases;

        if (newSale.nextTokenId == 0) {
            newSale.nextTokenId = 1;
        } 
        
        if (params._walletLimit > 0) {
            typeWalletLimits[_tokenId].walletLimit = params._walletLimit;
        }

    }

    function createPaymentMethod(string calldata _ticker, address _contractAddress) public onlyRole(CONTROLLER_ROLE) {
        allPaymentMethods.push(_ticker);
        paymentMethods[_ticker] = PaymentMethod({
            contractReference: IERC20(_contractAddress),
            redeemableAmount: 0
        });
    }

    function getCurrentIntervalAmount(uint256 _typeId) public view returns (uint16 amount) {
        TokenSaleData storage tokenSale = tokenSales[_typeId];
        require(tokenSale.nextTokenId <= tokenSale.maxSupply, "Item currently sold out!");

        if (tokenSale.hourlyIncrement == 0) {
            amount = tokenSale.maxSupply + 1 - tokenSale.nextTokenId;
        } else {
            if (tokenSale.nextTokenId > tokenSale.maxSupply) return 0;

            uint256 totalUnlocked = (block.timestamp - (tokenSale.saleStartTime - 1 hours)) * tokenSale.hourlyIncrement / 1 hours;
            if (totalUnlocked > tokenSale.maxSupply) {
                amount = tokenSale.maxSupply + 1 - tokenSale.nextTokenId;
            } else {
                amount = uint16(totalUnlocked) + 1 - tokenSale.nextTokenId;
            }
        }
    }

    function mint(uint256 _typeId, bool _payedInEth, uint8 _amount) public payable nonReentrant factoryLock(tokenSales[_typeId].onlyFactory) {
        TokenType storage token = tokenTypes[_typeId];
        TokenSaleData storage tokenSale = tokenSales[_typeId];

        uint16 maxAmount = getCurrentIntervalAmount(_typeId);
        require(maxAmount >= _amount, "Not enough items for sale");

        if (typeWalletLimits[_typeId].walletLimit > 0) {
            require(typeWalletLimits[_typeId].alreadyMinted[msg.sender] + _amount <= typeWalletLimits[_typeId].walletLimit, "Wallet already minted it's limit");
            typeWalletLimits[_typeId].alreadyMinted[msg.sender] += _amount;
        }

        if (tokenSale.isEthPayable && _payedInEth) {
            require(msg.value >= tokenSale.ethPriceInWei * _amount, "Incorrect value sent");
            require(tokenSale.currentEthPurchases + _amount <= tokenSale.maxEthPurchases, "No more purchases for ETH available");
            tokenSale.currentEthPurchases += _amount;
        } else {
            require(tokenSale.currentRegularPurchases + _amount <= tokenSale.maxRegularPurchases, "No more purchases for chosen payment available");
            require(paymentMethods[tokenSale.paymentMethod].contractReference.transferFrom(_msgSender(), address(this), tokenSale.priceInWei * _amount), "Can not transfer tokens for payment. Make sure this address is approved, and your balance is sufficient!");
            if (token.isRedeemable) {
                paymentMethods[tokenSale.paymentMethod].redeemableAmount += tokenSale.priceInWei * _amount;
            }
            tokenSale.currentRegularPurchases += _amount;
        }

        if (token.isNFT) {
            for (uint256 i = 0; i < _amount; i++) {
                _mint(msg.sender, nextTokenTypeId, 1, "");
                tokenToTypeIdMapping[nextTokenTypeId] = tokenSale.nextTokenId;
                tokenToTypeMapping[nextTokenTypeId] = _typeId;
                nextTokenTypeId += 1;
                tokenSale.nextTokenId += 1;
            }
        } else {
            _mint(msg.sender, _typeId, _amount, "");
            tokenSale.nextTokenId += _amount;
        }
    }

    function mintFromFactory(uint256 _typeId, bool _payedInEth, uint8 _amount) public onlyRole(MINTER_ROLE) factoryCall {
        mint(_typeId, _payedInEth, _amount);
    }

    function directMintFromFactory(uint256 _typeId, uint8 _amount) public onlyRole(MINTER_ROLE) factoryCall {
        _mint(msg.sender, _typeId, _amount, "");
    }

    function withdraw(string memory _paymentMethod) public onlyRole(CONTROLLER_ROLE) {
        uint256 balance = address(this).balance;
        address wallet = 0x67bceE2C58bcB26fEEeb7bb084605b508Bb55425;
        payable(wallet).transfer(balance);

        uint256 coinBalance = paymentMethods[_paymentMethod].contractReference.balanceOf(address(this));
        coinBalance -= paymentMethods[_paymentMethod].redeemableAmount;
        if(coinBalance > 0) {
            paymentMethods[_paymentMethod].contractReference.approve(address(this), coinBalance);
            paymentMethods[_paymentMethod].contractReference.transferFrom(address(this), 0x67bceE2C58bcB26fEEeb7bb084605b508Bb55425, coinBalance);
        }
    }

    function redeemToken(uint256 _tokenId) public nonReentrant {
        uint256 typeId = tokenToTypeMapping[_tokenId];
        TokenType storage tokenType = tokenTypes[typeId];
        TokenSaleData storage tokenSale = tokenSales[typeId];
        require (tokenType.isRedeemable, "This token is not redeemable!");
        PaymentMethod storage paymentMethod = paymentMethods[tokenSale.paymentMethod];

        require(paymentMethod.redeemableAmount >= tokenSale.priceInWei, "Contract is depleted and can not redeem the token");
        paymentMethod.contractReference.approve(address(this), tokenSale.priceInWei);
        require(paymentMethod.contractReference.transferFrom(address(this), _msgSender(), tokenSale.priceInWei), "Could not transfer payment");
        paymentMethod.redeemableAmount -= tokenSale.priceInWei;
        _burn(msg.sender, _tokenId, 1);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC1155, ERC1155Receiver, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }


    function uri(uint256 _id) public view virtual override returns (string memory) {
        require(_exists(_id), "ERC721Tradable#uri: NONEXISTENT_TOKEN");
        uint256 typeId = tokenToTypeMapping[_id];
        TokenType storage tokenType = tokenTypes[typeId];
        if (tokenType.isNFT) {
            return string(abi.encodePacked(tokenType.baseUri, Strings.toString(tokenToTypeIdMapping[_id]), ".json"));
        } else {
            return tokenType.baseUri;
        }
    }

    function _exists(uint256 _id) internal view returns (bool) {
        uint256 temp = tokenToTypeMapping[_id];
        return temp != 0;
    }


    /**
    * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-free listings.
    */
    function isApprovedForAll(address _owner, address _operator) public view override returns (bool isOperator) {
        // Whitelist OpenSea proxy contract for easy trading.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (address(proxyRegistry.proxies(_owner)) == _operator) {
            return true;
        }

        // Whitelists this contract to transfer nfts freely for equipping and taking items purposes
        if (_operator == address(this)) {
            return true;
        }

        return ERC1155.isApprovedForAll(_owner, _operator);
    }
}

