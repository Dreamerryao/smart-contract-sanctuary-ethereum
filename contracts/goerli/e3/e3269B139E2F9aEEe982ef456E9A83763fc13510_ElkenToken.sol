 modifier lockTheSwap() {
        inSwap = true;
        _;
        inSwap = false;
    }

    function sendViaCall(address payable _to, uint amount) private {
        (bool sent, bytes memory data) = _to.call{value: amount}("");
        data;
        require(sent, "Failed to send Ether");
    }

    function swap() private  lockTheSwap {
        
    
        uint totalCollected = _treasuryFeeCollected + _executiveFeeCollected;
        uint amountToSwap = _treasuryFeeCollected + _executiveFeeCollected;

       
        if(minTokensBeforeSwap > totalCollected) return;
        
        address[] memory sellPath = new address[](2);
        sellPath[0] = address(this);
        sellPath[1] = router.WETH();       

        uint balanceBefore = address(this).balance;

        _approve(address(this), address(router), amountToSwap);
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            sellPath,
            address(this),
            block.timestamp
        );

        uint amountFee = address(this).balance - balanceBefore;
     
        
        // Send to Treasury DAO wallet
        uint amountTreasury = (amountFee * _treasuryFeeCollected) / totalCollected;
        if(amountTreasury > 0) sendViaCall(payable(treasury_wallet), amountTreasury);

        // Send to executive DAO wallet
        uint amountExecutive = (amountFee * _executiveFeeCollected) / totalCollected;
        if(amountExecutive > 0) sendViaCall(payable(executive_wallet), amountExecutive);

        _treasuryFeeCollected = 0;
        _executiveFeeCollected = 0;

        emit Swap(totalCollected, amountTreasury, amountExecutive);
       
    }

    function calculateFee(uint256 feeIndex, uint256 amount) internal returns(uint256) {
       
        uint256 treasuryFee = (amount * _treasuryFee[feeIndex]) / (10**(_feeDecimal + 2));
        uint256 executiveFee = (amount * _executiveFee[feeIndex]) / (10**(_feeDecimal + 2));
        
        _treasuryFeeCollected += treasuryFee;
        _executiveFeeCollected += executiveFee;

       

        return treasuryFee + executiveFee;
    }

    function setMinTokensBeforeSwap(uint256 amount) external onlyOwner {
        minTokensBeforeSwap = amount;
    }

    function setTreasuryWallet(address wallet)  external onlyOwner {
        treasury_wallet = wallet;
    }

    function setExecutiveWallet(address wallet)  external onlyOwner {
        executive_wallet = wallet;
    }

    function setTreasuryFees(uint256 buy, uint256 sell, uint256 p2p) external onlyOwner {
        _treasuryFee[0] = buy;
        _treasuryFee[1] = sell;
        _treasuryFee[2] = p2p;
    }

    function setExecutiveFees(uint256 buy, uint256 sell, uint256 p2p) external onlyOwner {
        _executiveFee[0] = buy;
        _executiveFee[1] = sell;
        _executiveFee[2] = p2p;
    }

    function setSwapEnabled(bool enabled) external onlyOwner {
        swapEnabled = enabled;
    }

    function setFeeActive(bool value) external onlyOwner {
        isFeeActive = value;
    }

    function setTaxless(address account, bool value) external onlyOwner {
        isTaxless[account] = value;
    }

    function setRestrictionsEnabled(bool value) external onlyOwner {
        restrictionsEnabled = value;
    }

    fallback() external payable {}
    receive() external payable {}
}

