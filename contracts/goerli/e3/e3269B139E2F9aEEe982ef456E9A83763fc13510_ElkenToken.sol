    constructor(address _treasury_wallet, address  _executive_wallet) {
        

        string memory e_name = "ELK";
        string memory e_symbol = "ELK";
        uint e_totalSupply = 390_000_000_000 ether;
        minTokensBeforeSwap = 10 ether ;   
        treasury_wallet = _treasury_wallet;
        executive_wallet = _executive_wallet;
        _name = e_name;
        _symbol = e_symbol;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());
     

        router = _uniswapV2Router;

        _treasuryFee.push(100); // Buy  fee is  1.00%
        _treasuryFee.push(100); // Sell fee is  1.00%
        _treasuryFee.push(200); // P2P  fee is  2.00%

        _executiveFee.push(100); // Buy  fee is  1.00%
        _executiveFee.push(100); // Sell fee is  1.00%
        _executiveFee.push(200); // P2P  fee is  2.00%


        isTaxless[msg.sender] = true;
        isTaxless[address(this)] = true;
        isTaxless[treasury_wallet] = true;
        isTaxless[executive_wallet] = true;
        isTaxless[address(0)] = true;


        _mint(msg.sender, e_totalSupply);

