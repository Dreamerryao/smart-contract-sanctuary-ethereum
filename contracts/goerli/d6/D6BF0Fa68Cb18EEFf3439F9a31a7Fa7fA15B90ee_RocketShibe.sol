pragma solidity ^0.8.4;

import "../lib/solmate/src/tokens/ERC721.sol";
import "../lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "../lib/openzeppelin-contracts/contracts/token/ERC1155/IERC1155.sol";
import "../lib/openzeppelin-contracts/contracts/token/ERC1155/IERC1155Receiver.sol";
import "../lib/openzeppelin-contracts/contracts/utils/Strings.sol";
import "../lib/openzeppelin-contracts/contracts/utils/structs/EnumerableSet.sol";
import "../lib/openzeppelin-contracts/contracts/utils/cryptography/MerkleProof.sol";

contract RocketShibe is
    ERC721("RocketShibe", "RSHIBE"),
    IERC1155Receiver,
    Ownable
{
    using Strings for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    uint256 public totalSupply = 10000;
    uint256 public price = 0.01 ether;
    uint256 public count = 0;
    uint256 public baseYield = 1;

    string public baseURI =
        "ipfs://QmeSjSinHpPnmXmspMjwiXyN6zS4E9zccariGR3jxcaWtq/";

    bool public paused = false;

    IERC1155 public opensea;
    // IERC1155(0x495f947276749Ce646f68AC8c248420045cb7b5e);

    // rocketshibes and the erc1155 tokens they hold
    mapping(uint256 => EnumerableSet.UintSet) private rocketToCryptoShibes;
    // totalYield
    mapping(uint256 => uint256) private rocketYield;
    // index 0: og whitelist
    // index 1: regular whitelist
    // index 2,3,4,5: common, uncommon, rare, epic, legendary merkleRoots (based off their tokenIds)
    // possibly could add other opensesa storefront collections (as collaboration?)
    mapping(uint256 => bytes32) private merkleRoots;
    // yields that each rarity will reward when boosted
    mapping(uint256 => uint256) private rootYields;

    constructor(address _opensea) {
        opensea = IERC1155(_opensea);
        // index 0 1 are reserved for og and whitelist mints
        merkleRoots[0] = bytes32(
            0x8d988cf84eeb47126dbb8739faf2da4879f2b5eb3d7121d70ac95390b7e403ed
        );

        // in order common, uncommon, rare, epic, legendary
        merkleRoots[2] = bytes32(
            0xfcfc64fc73701bc4a556443c38b70a78bbeae2428a14303fc47f69cf8a8f72a6
        );
        merkleRoots[3] = bytes32(
            0x09a595ce68369ec57864f8da0686576d8bce3ad937e51d517d673966fce43272
        );
        merkleRoots[4] = bytes32(
            0x64a944cb344d946832c4bbf4d1a0c9cded1c96fcdd13fa1a4d7b3a419d6d62c9
        );
        merkleRoots[5] = bytes32(
            0x3a11a3d298f7f0e0d15fab0e0d2b025714955600d6298585e5a1df4f64e16323
        );
        merkleRoots[6] = bytes32(
            0xa2e242f1acebd31b628b337079cae86e848b3886190b7cf61b1e540c482a44d5
        );

        rootYields[2] = 1;
        rootYields[3] = 2;
        rootYields[4] = 3;
        rootYields[5] = 4;
        rootYields[6] = 5;
    }

    function pauseState(bool _state) public onlyOwner {
        paused = _state;
    }

    function setMerkleRoot(uint256 index, bytes32 root) public onlyOwner {
        merkleRoots[index] = root;
    }

    function setRootYields(uint256 index, uint256 yield) public onlyOwner {
        rootYields[index] = yield;
    }

    function updateBaseURI(string memory _uri) public onlyOwner {
        baseURI = _uri;
    }

    function tokenURI(uint256 id) public view override returns (string memory) {
        require(ownerOf[id] != address(0), "invalid id");

        return string(abi.encodePacked(baseURI, id.toString()));
    }

    function getTokenYield(uint256 tokenId, bytes32[] memory proof)
        public
        view
        returns (uint256)
    {
        bytes32 tmpRoot = MerkleProof.processProof(
            proof,
            keccak256(abi.encodePacked(tokenId))
        );
        for (uint256 i = 2; i < 7; i++) {
            if (merkleRoots[i] == tmpRoot) {
                return rootYields[i];
            }
        }
    }

    function boostRocket(
        uint256 rocketId,
        uint256[] calldata _shibeIds,
        bytes32[][] calldata proof,
        uint256[] calldata _amounts
    ) public {
        require(paused == false);
        require(ownerOf[rocketId] == msg.sender, "not owner");

        for (uint256 i; i < _shibeIds.length; i++) {
            uint256 y = getTokenYield(_shibeIds[i], proof[i]);
            require(y != 0, "not valid");
            require(opensea.balanceOf(msg.sender, _shibeIds[i]) > 0); // for testing this can be many
            rocketToCryptoShibes[rocketId].add(_shibeIds[i]);
            rocketYield[rocketId] += y;
        }

        opensea.safeBatchTransferFrom(
            msg.sender,
            address(this),
            _shibeIds,
            _amounts,
            ""
        );
    }

    function exitRocket(
        uint256 rocketId,
        uint256[] calldata _shibeIds,
        bytes32[][] calldata proof,
        uint256[] calldata _amounts
    ) public {
        require(paused == false);
        require(ownerOf[rocketId] == msg.sender, "not owner");

        for (uint256 i; i < _shibeIds.length; i++) {
            uint256 y = getTokenYield(_shibeIds[i], proof[i]);
            require(y != 0, "not valid");
            require(
                opensea.balanceOf(address(this), _shibeIds[i]) > 0,
                "dont have"
            );
            rocketToCryptoShibes[rocketId].remove(_shibeIds[i]);
            rocketYield[rocketId] -= y;
        }

        opensea.safeBatchTransferFrom(
            address(this),
            msg.sender,
            _shibeIds,
            _amounts,
            ""
        );
    }

    function getYield(uint256 tokenId) public view returns (uint256) {
        return rocketYield[tokenId];
    }

    function ogMint(bytes32[] calldata proof) public {
        require(paused == false, "paused");
        require(count < totalSupply, "oversupply");
        require(
            MerkleProof.verify(
                proof,
                merkleRoots[0],
                keccak256(abi.encodePacked(address(msg.sender)))
            ),
            "merkle problem"
        );

        _mint(msg.sender, count++);
    }

    function whitelistMint(bytes32[] calldata proof) public {
        require(paused == false, "paused");
        require(count < totalSupply, "over supply");
        require(
            MerkleProof.verify(
                proof,
                merkleRoots[1],
                keccak256(abi.encodePacked(msg.sender))
            ),
            "merkle problem"
        );

        _mint(msg.sender, count++);
    }

    function claim() public {
        //merkle here
        _mint(msg.sender, count++);
    }

    function mint() public payable {
        require(paused == false, "paused");
        require(count < totalSupply, "lss");

        // TODO check for reentrancy / if msg.sender is contract?

        _mint(msg.sender, count++);
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external override returns (bytes4) {
        return RocketShibe.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata data
    ) external override returns (bytes4) {
        return RocketShibe.onERC1155BatchReceived.selector;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        pure
        virtual
        override(ERC721, IERC165)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

