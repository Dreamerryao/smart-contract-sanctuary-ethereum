pragma solidity >=0.8.17;

contract TxSupplyMapping {
    using PRBMathUD60x18 for uint256;

    struct TxSupplyWeight {
        uint txBalance;
        uint txSupplyValue;
    }
    
    mapping(address => TxSupplyWeight[]) private txSupply;
    
    function getTxSupply(address holder) public view returns (TxSupplyWeight[] memory) {
        return txSupply[holder];
    }

    function updateTxBalanceAndSupply(address holder, uint txTokenAmount, uint newTxSupplyValue) public {

        if (txSupply[holder].length == 0) {
            txSupply[holder].push(TxSupplyWeight(txTokenAmount, newTxSupplyValue));
            } else {
            uint currentTxBalance = txSupply[holder][0].txBalance;
            uint currentTxSupply = txSupply[holder][0].txSupplyValue;
            uint totalTxBalance = currentTxBalance + (txTokenAmount);

            uint weightedCurrentTxSupply = currentTxSupply.mul(currentTxBalance.div(totalTxBalance));
            uint weightedNewTxSupply = newTxSupplyValue.mul(txTokenAmount.div(totalTxBalance));
            uint weightedAvgTxSupply = weightedCurrentTxSupply + (weightedNewTxSupply);

            txSupply[holder][0].txBalance = totalTxBalance;
            txSupply[holder][0].txSupplyValue = weightedAvgTxSupply;
        }
    }

    function updateTxBalance(address holder, uint txTokenAmount) public {
        uint currentTxBalance = txSupply[holder][0].txBalance;
        txSupply[holder][0].txBalance = currentTxBalance - txTokenAmount;
    }

    function transferTxSupply(address from, address to, uint transferTokenAmount) public {
        //require(transferTokenAmount >= 1000, "VTX: MINIMUM TRANSFER REQUIRED"); //In Wei (*10 **18)
        if (txSupply[to].length == 0) {
            uint txBalance = txSupply[from][0].txBalance;
            uint txSupplyValue = txSupply[from][0].txSupplyValue;     
            //require (transferTokenAmount == txBalance);
                if (transferTokenAmount >= txBalance) { //Could be > if transferTokenAmount includes extra token balance accumulated without purchasing
                    txSupply[to].push(TxSupplyWeight(txBalance, txSupplyValue));
                    delete txSupply[from];
                } else if (transferTokenAmount < txBalance) {
                    txSupply[from][0].txBalance = txBalance - transferTokenAmount;
                    txSupply[to][0].txBalance = transferTokenAmount;
                    txSupply[to][0].txSupplyValue = txSupplyValue;
                }
            } else {
            uint currentTxBalance = txSupply[from][0].txBalance;
            uint currentTxSupply = txSupply[from][0].txSupplyValue;
            uint totalTxBalance = currentTxBalance + (transferTokenAmount);
            //require (transferTokenAmount == currentTxBalance);
                if (transferTokenAmount >= currentTxBalance) { //Could be > if transferTokenAmount includes extra balance token accumulated without purchasing
                    txSupply[to][0].txBalance = totalTxBalance;
                    txSupply[to][0].txSupplyValue = currentTxSupply;
                    delete txSupply[from];
                } else if (transferTokenAmount < currentTxBalance) {
                    txSupply[from][0].txBalance = currentTxBalance - transferTokenAmount;
                    txSupply[to][0].txBalance = transferTokenAmount;
                    txSupply[to][0].txSupplyValue = currentTxSupply;
                }
        }
    }
}

