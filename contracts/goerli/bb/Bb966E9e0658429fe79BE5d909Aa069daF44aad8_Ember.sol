contract Ember is IEmber {
    uint256 lendingId;
    mapping(uint256 => IEmber.LendingRenting) private lendingRenting;
    mapping(address => bool) private multisig;
    mapping(address => bool) private VerifiedEOA;
    mapping(address => string) private EOA_ENCKEY;
    uint256 internal constant HUNDRED = 100 ether;
    uint256 internal constant FEE_PERCENTAGE = 5 ether;
    uint256 internal constant BUY_FEE_PERCENTAGE = 1 ether;
    uint256 internal constant MINUTES_IN_DAY = 1440;
    address constant owner = 0xFcce5CE96D091395bef9f9c19306983e4D812c10;
    address constant admin = 0xCD02feDc82139DfD5F638D3bBB753c109B90b553;
    event lendInfo(
        uint256 _lendingId,
        address nft,
        uint256 tokenid,
        uint256 lendAmount,
        uint256 charges,
        uint256 sellPrice,
        uint256 minDuration,
        uint256 maxDuration,
        address lender,
        address borrower,
        uint256 timestamp,
        address adapter
    );
    event subletInfo(
        uint256 _lendingId,
        uint256 charges,
        uint256 minDuration,
        uint256 maxDuration,
        address lender,
        address borrower,
        uint256 timestamp
    );
    event rentInfo(
        uint256 _lendingId,
        uint256 duration,
        uint256 amount,
        uint256 timestamp
    );
    event directrentInfo(
        uint256 _lendingId,
        uint256 duration,
        uint256 timestamp
    );
    event unlendInfo(uint256 _lendingId, bool isSublet);
    event forceClaimInfo(uint256 _lendingId, address from, bool isSublet);
    event sellInfo(uint256 _lendingId);

    function addsigner(address signer) external {
        //require(msg.sender==admin,"NO");
        multisig[signer] = true;
    }

    function handleLend(lendDetails[] calldata data) external {
        lendDetails memory lendData;
        uint256 _lendingId = lendingId;
        for (uint256 i = 0; i < data.length; i++) {
            Lending memory lend = lendingRenting[_lendingId].lending;
            lendData = data[i];
            lend.lenderAddress = msg.sender;
            lend.adapter = lendData.adapter;

            lend.maxDuration = lendData.maxDuration;
            lend.tokenId = lendData.tokenId;
            lend.nft = lendData.nft;
            lend.lendAmount = lendData.lendAmount;
            _require(lendData.maxDuration != 0, Errors.ZERO_MAX_DURATION);
            if (lendData.borrower != address(0)) {
                lend.borrower = lendData.borrower;
                lend.charges = lendData.charges;
            } else {
                lend.charges = lendData.charges / MINUTES_IN_DAY;
            }
            if (lendData.minDuration > 0) {
                lend.minDuration = lendData.minDuration;
            }

            if (lendData.buyPrice != 0) {
                //nft for sell
                lend.buyPrice = lendData.buyPrice;
                _require(
                    lendData.borrower == address(0),
                    Errors.DIRECT_LEND_RESTRICTED
                );
            }
            lendingRenting[_lendingId].lending = lend;
            safeTransfer(
                lendData.nft,
                msg.sender,
                address(this),
                lendData.tokenId,
                lendData.lendAmount
            );

            emit lendInfo(
                _lendingId,
                lendData.nft,
                lendData.tokenId,
                lend.lendAmount,
                lendData.charges,
                lendData.buyPrice,
                lendData.maxDuration,
                lendData.minDuration,
                msg.sender,
                lendData.borrower,
                block.timestamp,
                lendData.adapter
            );
            _lendingId++;
        }
        lendingId = _lendingId;
    }

    function handleSublet(subLetting[] calldata data) external {
        //break subletting chain==> needs to be done
        subLetting memory lendData;
        uint256 id;
        for (uint256 i = 0; i < data.length; i++) {
            lendData = data[i];
            id = lendData.id;
            subLet memory sublend = lendingRenting[id].sublet;
            Renting memory rent = lendingRenting[id].renting;
            Lending memory lend = lendingRenting[id].lending;
            _require(!rent.sublet, Errors.ALREADY_SUBLENDED);
            _require(
                lend.borrower == address(0),
                Errors.DIRECT_LEND_RESTRICTED
            );
            sublend.lender = msg.sender;
            sublend.maxDuration = lendData.maxDuration;

            if (lendData.borrower != address(0)) {
                sublend.borrower = lendData.borrower;
                sublend.charges = lendData.charges;
            } else {
                sublend.charges = lendData.charges / MINUTES_IN_DAY;
            }
            if (lendData.minDuration > 0) {
                lend.minDuration = lendData.minDuration;
            }
            rent.sublet = true;
            safeTransfer(
                lend.nft,
                msg.sender,
                address(this),
                lend.tokenId,
                lend.lendAmount
            );
            rent.renterAddress = address(this);
            lendingRenting[id].renting = rent;
            lendingRenting[id].sublet = sublend;
            emit subletInfo(
                id,
                lendData.charges,
                lendData.maxDuration,
                lendData.minDuration,
                msg.sender,
                lendData.borrower,
                block.timestamp
            );
        }
    }

    function handleRent(
        IEmber.rentDetials[] calldata data,
        bytes[] calldata signatures
    ) external payable {
        if (!VerifiedEOA[msg.sender]) {
            verifySigner(msg.sender, signatures, ".");
        }

        rentDetials memory rentData;
        uint256 charges;
        uint256 totalCharges;
        uint256 id;

        for (uint256 i; i < data.length; i++) {
            rentData = data[i];
            id = rentData.id;
            Renting memory rent = lendingRenting[id].renting;
            if (rent.sublet) {
                if (getRentedTill(id) < block.timestamp) {
                    //orignal borrower time expires then exexute normal rent
                    delete lendingRenting[id].sublet;
                    console.log(
                        "sublender duration expires executes normal rent"
                    );
                    charges = handleNormalUpfrontRent(
                        id,
                        rentData.rentDuration,
                        true,
                        rentData.charges
                    );
                } else {
                    _require(
                        getSubRentedTill(id) < block.timestamp,
                        Errors.RENT_DURATION_NOT_EXPIRED
                    );
                    console.log("subrenter duration expires new subrenter");
                    charges = handleSubletUpfrontRent(
                        id,
                        rentData.rentDuration,
                        rentData.charges
                    );
                }
            } else if (!rent.sublet) {
                if (rent.renterAddress != address(0)) {
                    _require(
                        getRentedTill(id) < block.timestamp,
                        Errors.RENT_DURATION_NOT_EXPIRED
                    );
                    console.log("normal re-rent");
                    charges = handleNormalUpfrontRent(
                        id,
                        rentData.rentDuration,
                        true,
                        rentData.charges
                    );
                } else {
                    console.log("normal rent");
                    charges = handleNormalUpfrontRent(
                        id,
                        rentData.rentDuration,
                        false,
                        rentData.charges
                    );
                }
            } else {
                revert("invalid-data");
            }
            totalCharges = totalCharges + charges;
            emit rentInfo(
                id,
                rentData.rentDuration,
                rentData.charges,
                block.timestamp
            );
        }
        payable(owner).transfer(totalCharges);
    }

    //rethink
    function handleDirectRent(
        IEmber.directRentDetails[] calldata data,
        bytes[] calldata signatures
    ) external {
        if (!VerifiedEOA[msg.sender]) {
            verifySigner(msg.sender, signatures, ".");
        }
        directRentDetails memory rentData;
        uint256 id;
        for (uint256 i; i < data.length; i++) {
            rentData = data[i];
            id = rentData.id;
            if (lendingRenting[id].lending.borrower != address(0)) {
                console.log("executing normal directlend");
                handleNoramlDirectRent(id, rentData.rentDuration);
            } else if (lendingRenting[id].sublet.borrower != address(0)) {
                handleSubletDirectRent(id, rentData.rentDuration);
            }
            emit directrentInfo(id, rentData.rentDuration, block.timestamp);
        }
    }

    function unLend(uint256[] calldata _lendingId) external {
        uint256 id;
        for (uint256 i = 0; i < _lendingId.length; i++) {
            id = _lendingId[i];
            Renting memory rent = lendingRenting[id].renting;
            Lending memory lend = lendingRenting[id].lending;

            if (lend.lenderAddress == msg.sender) {
                console.log("Unlend:lender");
                if (rent.renterAddress == address(0)) {
                    //in contract but above require check will revert if sublended
                    safeTransfer(
                        lend.nft,
                        address(this),
                        msg.sender,
                        lend.tokenId,
                        lend.lendAmount
                    );
                } else {
                    _require(
                        getRentedTill(id) < block.timestamp,
                        Errors.RENT_DURATION_NOT_EXPIRED
                    );
                    safeTransfer(
                        lend.nft,
                        rent.renterAddress,
                        msg.sender,
                        lend.tokenId,
                        lend.lendAmount
                    );
                }
                delete lendingRenting[id];
                emit unlendInfo(id, false);
            }
            //sublender
            else if (lendingRenting[id].sublet.lender == msg.sender) {
                //sub-renter duration not expired
                //if(getSubRentedTill(id)>0){
                console.log("Unlend:sublender");
                _require(
                    getSubRentedTill(id) < block.timestamp,
                    Errors.RENT_DURATION_NOT_EXPIRED
                );
                //_require(getRentedTill(id)> block.timestamp,Errors.RENT_DURATION_EXPIRED); //normal rent duration expired
                safeTransfer(
                    lend.nft,
                    rent.renterAddress,
                    msg.sender,
                    lend.tokenId,
                    lend.lendAmount
                );
                lendingRenting[id].renting.renterAddress = msg.sender;
                lendingRenting[id].renting.sublet = false;
                delete lendingRenting[id].sublet;
                emit unlendInfo(id, true);
            } else {
                revert("invalid call");
            }
        }
    }

    function forceClaim(uint256[] calldata _lendingId) external {
        //if borrowed & not sublet

        uint256 id;
        for (uint256 i = 0; i < _lendingId.length; i++) {
            id = _lendingId[i];

            Renting memory rent = lendingRenting[id].renting;
            Lending memory lend = lendingRenting[id].lending;

            if (lend.lenderAddress == msg.sender) {
                _require(
                    lend.borrower != address(0),
                    Errors.INVALID_FORCECLAIMER
                );
                safeTransfer(
                    lend.nft,
                    rent.renterAddress,
                    msg.sender,
                    lend.tokenId,
                    lend.lendAmount
                );
                delete lendingRenting[id];
                emit forceClaimInfo(id, rent.renterAddress, false);
            } else if (lendingRenting[id].sublet.lender == msg.sender) {
                //for sublender
                _require(
                    lendingRenting[id].sublet.borrower != address(0),
                    Errors.INVALID_FORCECLAIMER
                );
                safeTransfer(
                    lend.nft,
                    rent.renterAddress,
                    msg.sender,
                    lend.tokenId,
                    lend.lendAmount
                );
                lendingRenting[id].renting.renterAddress = msg.sender;
                delete lendingRenting[id].sublet;
                emit forceClaimInfo(id, rent.renterAddress, true);
            } else {
                revert("In-valid call");
            }
        }
    }

    function buyNft(uint256 id) external payable {
        Renting memory rent = lendingRenting[id].renting;
        Lending memory lend = lendingRenting[id].lending;
        if (lend.buyPrice > 0) {
            if (rent.renterAddress == address(0)) {
                //if not borrowed & no sublet
                safeTransfer(
                    lend.nft,
                    address(this),
                    msg.sender,
                    lend.tokenId,
                    lend.lendAmount
                );
            } else if (rent.renterAddress != address(0)) {
                _require(
                    getRentedTill(id) < block.timestamp,
                    Errors.RENT_DURATION_NOT_EXPIRED
                );
                safeTransfer(
                    lend.nft,
                    rent.renterAddress,
                    msg.sender,
                    lend.tokenId,
                    lend.lendAmount
                );
            } else {
                revert("invalid Lid");
            }
            _require(msg.value == lend.buyPrice, Errors.INVALID_RENT_AMOUNT);
            uint256 charges = (lend.buyPrice * BUY_FEE_PERCENTAGE) / HUNDRED;
            payable(lend.lenderAddress).transfer(msg.value - charges);
            payable(owner).transfer(charges);
            emit sellInfo(id);
            delete lendingRenting[id];
        } else {
            revert("NFT to for Sell");
        }
    }

    function handleNormalUpfrontRent(
        uint256 id,
        uint256 rentDuration,
        bool isBorrowed,
        uint256 rentCharges
    ) internal returns (uint256 charges) {
        Renting memory rent = lendingRenting[id].renting;
        Lending memory lend = lendingRenting[id].lending;

        _require(
            rentDuration <= lend.maxDuration,
            Errors.RENTDURATION_GT_LENDDURATION
        );
        _require(
            rentDuration >= lend.minDuration,
            Errors.RENTDURATION_GT_LENDDURATION
        );
        _require(
            rentCharges / rentDuration == lend.charges,
            Errors.INVALID_RENT_AMOUNT
        );
        _require(lend.borrower == address(0), Errors.SPLIT_SHARE);
        if (isBorrowed) {
            safeTransfer(
                lend.nft,
                rent.renterAddress,
                msg.sender,
                lend.tokenId,
                lend.lendAmount
            );
            rent.sublet = false;
        } else {
            safeTransfer(
                lend.nft,
                address(this),
                msg.sender,
                lend.tokenId,
                lend.lendAmount
            );
        }
        rent.renterAddress = msg.sender;
        rent.rentedTill = block.timestamp + (rentDuration * 60);
        lendingRenting[id].renting = rent;
        charges = (rentCharges * FEE_PERCENTAGE) / HUNDRED;
        payable(lend.lenderAddress).transfer(rentCharges - charges);
        return charges;
    }

    function handleSubletUpfrontRent(
        uint256 id,
        uint256 rentDuration,
        uint256 rentCharges
    ) internal returns (uint256 charges) {
        Renting memory rent = lendingRenting[id].renting;
        Lending memory lend = lendingRenting[id].lending;
        subLet memory sublend = lendingRenting[id].sublet;

        _require(
            rentDuration <= sublend.maxDuration,
            Errors.RENTDURATION_GT_LENDDURATION
        );
        _require(
            rentCharges / rentDuration == sublend.charges,
            Errors.INVALID_RENT_AMOUNT
        );
        _require(sublend.borrower == address(0), Errors.SPLIT_SHARE);
        rent.subRentedTill = block.timestamp + (rentDuration * 60);
        _require(
            rent.subRentedTill < rent.rentedTill,
            Errors.RENTDURATION_GT_LENDDURATION
        );
        safeTransfer(
            lend.nft,
            rent.renterAddress,
            msg.sender,
            lend.tokenId,
            lend.lendAmount
        ); //}
        rent.renterAddress = msg.sender;
        lendingRenting[id].renting = rent;
        charges = (rentCharges * FEE_PERCENTAGE) / HUNDRED;
        payable(lend.lenderAddress).transfer(rentCharges - charges);
        return charges;
    }

    function handleNoramlDirectRent(uint256 id, uint256 rentDuration) internal {
        Renting memory rent = lendingRenting[id].renting;
        Lending memory lend = lendingRenting[id].lending;
        _require(lend.borrower == msg.sender, Errors.INVALID_BORROWER);
        rent.renterAddress = msg.sender;
        rent.rentedTill = block.timestamp + (rentDuration * 60);
        safeTransfer(
            lend.nft,
            address(this),
            msg.sender,
            lend.tokenId,
            lend.lendAmount
        );
        rent.renterAddress = msg.sender;
        lendingRenting[id].renting = rent;
    }

    function handleSubletDirectRent(uint256 id, uint256 rentDuration) internal {
        Renting memory rent = lendingRenting[id].renting;
        Lending memory lend = lendingRenting[id].lending;
        //subLet memory sublend = lendingRenting[id].sublet;
        _require(
            lendingRenting[id].sublet.borrower == msg.sender,
            Errors.INVALID_BORROWER
        );
        rent.subRentedTill = block.timestamp + (rentDuration * 60);
        _require(
            rent.subRentedTill < rent.rentedTill,
            Errors.RENTDURATION_GT_LENDDURATION
        );
        //_require(rentDuration<=sublend.maxDuration,Errors.RENTDURATION_GT_LENDDURATION);
        safeTransfer(
            lend.nft,
            address(this),
            msg.sender,
            lend.tokenId,
            lend.lendAmount
        );
        rent.renterAddress = msg.sender;
        lendingRenting[id].renting = rent;
    }

    function verifySigner(
        address EOA,
        bytes[] calldata signatures,
        string memory encriptedKey
    ) public {
        bytes32 dataHASH1 = keccak256(abi.encodePacked(EOA));
        bytes32 datahash = keccak256(
            abi.encodePacked("\x19Ethereum Signed Message:\n32", dataHASH1)
        );
        address signer;

        for (uint256 i; i < signatures.length; i++) {
            signer = recoverSigner(signatures[i], datahash);
            if (!multisig[signer]) {
                revert("Invalid Signer");
            }
        }
        EOA_ENCKEY[EOA] = encriptedKey;
        VerifiedEOA[EOA] = true;
        //EOA_ENCKEY[EOA]=abi.encodePacked(true,encriptedKey);//what if signature length 0?
    }

    function recoverSigner(
        bytes memory sig,
        bytes32 _hash //signature, hash
    ) internal pure returns (address) {
        (uint8 v, bytes32 r, bytes32 s) = splitSignature(sig);

        return ecrecover(bytes32(_hash), v, r, s);
    }

    function splitSignature(bytes memory sig)
        internal
        pure
        returns (
            uint8 v,
            bytes32 r,
            bytes32 s
        )
    {
        require(sig.length == 65);

        assembly {
            // first 32 bytes, after the length prefix.
            r := mload(add(sig, 32))
            // second 32 bytes.
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 byt (es).
            v := byte(0, mload(add(sig, 96)))
        }

        return (v, r, s);
    }

    function safeTransfer(
        address _nft,
        address _from,
        address _to,
        uint256 _tokenId,
        uint256 _lentAmounts
    ) internal {
        if (is721(_nft)) {
            IERC721(_nft).transferFrom(_from, _to, _tokenId);
        } else if (is1155(_nft)) {
            IERC1155(_nft).safeTransferFrom(
                _from,
                _to,
                _tokenId,
                _lentAmounts,
                ""
            );
        } else {
            revert("unsupported token type");
        }
    }

    function getRentedTill(uint256 _lendingId) public view returns (uint256) {
        return lendingRenting[_lendingId].renting.rentedTill;
    }

    function getSubRentedTill(uint256 _lendingId)
        public
        view
        returns (uint256)
    {
        return lendingRenting[_lendingId].renting.subRentedTill;
    }

    function is721(address _nft) private view returns (bool) {
        return IERC165(_nft).supportsInterface(type(IERC721).interfaceId);
    }

    function checkEOAVerifed(address EOA) external view returns (bool) {
        return VerifiedEOA[EOA];
    }

    function getEPK(address EOA) external view returns (string memory) {
        return EOA_ENCKEY[EOA];
    }

    function getRentCharges(uint256 id) external view returns (uint256) {}

    function is1155(address _nft) public view returns (bool) {
        return IERC165(_nft).supportsInterface(type(IERC1155).interfaceId);
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

