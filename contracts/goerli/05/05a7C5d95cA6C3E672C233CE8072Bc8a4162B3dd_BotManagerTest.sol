    // TODO weiroll safe gaurd for address check
    // TODO check gnosis safe gaurds
    // TODO auth gov, keeper, gaurdian
    // gov - updating stuff, unpause
    // keeper - bot manager Auth
    // gaurdian - emergencyPause

    // safegaurd list added by governance, with timelock

    constructor(address _addressProvider)
        AddressProviderService(_addressProvider)
    {}

    // execute direct - executed by wallet owner
    // execute from bot - executed on behalf of strategy

    // this is a call not a delegatecall, can be called by EOA
    function executeVMTxn(address _wallet, IVMParams.VMParams memory _params)
        public
    {
        IExecutable.Executable memory txn = _encodeVMParamsForBrahVM(_params);
        executeTxn(_wallet, txn);
    }

    function executeTxn(address _wallet, IExecutable.Executable memory _txn)
        public
    {
        address sender = msg.sender;
        IWalletAdapter adapter = findWalletAdapter(_wallet);
        if (!adapter.isAuthorized(_wallet, sender))
            revert UnauthorizedExecuteDirect();
        Executor._executeOnWallet(_wallet, adapter, _txn);
    }

    function executeAutomationViaBot(address _wallet, bytes32 _strategyId)
        public
    {
        if (msg.sender != address(addressProvider.botManager()))
            revert UnauthorizedExecuteBot();
        _executeAutomation(_wallet, _strategyId);
    }

    function _executeAutomation(address _wallet, bytes32 _strategyId) internal {
        // todo add sub active check
        if (!canExecute(_wallet, _strategyId)) revert TriggerFailed();

        IExecutable.Executable memory txn = IStrategy(
            StrategyRegistry(addressProvider.strategyRegistry())
                .strategyAddress(_strategyId)
        ).action(_wallet);

        IWalletAdapter adapter = findWalletAdapter(_wallet);
        (bool success, bytes memory data) = Executor._executeOnWallet(
            _wallet,
            adapter,
            txn
        );
        require(success, string(data));
    }

    function canExecute(address _wallet, bytes32 _strategyId)
        public
        view
        returns (bool)
    {
        if (
            !StrategyRegistry(addressProvider.strategyRegistry())
                .isStrategySupported(_strategyId)
        ) revert StrategyDoesntExist();
        if (
            !SubscriptionRegistry(addressProvider.subscriptionRegistry())
                .isSubActive(_strategyId, _wallet)
        ) revert StrategyUnActive();
        IStrategy strategy = IStrategy(
            StrategyRegistry(addressProvider.strategyRegistry())
                .strategyAddress(_strategyId)
        );

        IExecutable.Executable memory triggerCheck = strategy.trigger(_wallet);

        // Should revert if trigger is not successfull
        (bool success, bytes memory result) = Executor._executeStatic(
            triggerCheck
        );

        if (success) {
            bool triggerSuccess = abi.decode(result, (bool));
            return triggerSuccess;
        }

        return false;
    }

    // finds adapter for wallet if wallet is registered
    function findWalletAdapter(address _wallet)
        public
        view
        returns (IWalletAdapter adapter)
    {
        uint8 walletType = WalletRegistry(addressProvider.walletRegistry())
            .walletType(_wallet);
        adapter = IWalletAdapter(
            WalletAdapterRegistry(addressProvider.walletAdapterRegistry())
                .walletAdapter(walletType)
        );
        if (address(adapter) == address(0)) revert UnsupportedWallet(_wallet);
    }

    // takes weiroll vm params and converts them into a txn which is executable on any wallet
    function _encodeVMParamsForBrahVM(IVMParams.VMParams memory _params)
        internal
        view
        returns (IExecutable.Executable memory txn)
    {
        txn = IExecutable.Executable({
            callType: IExecutable.CallType.DELEGATECALL,
            target: address(addressProvider.brahVM()),
            value: 0,
            data: abi.encodeCall(
                BrahVM(addressProvider.brahVM()).executeWithVM,
                (_params)
            )
        });
    }
}

