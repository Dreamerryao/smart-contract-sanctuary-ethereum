pragma solidity ^0.8.18;

interface IExecutable {
    enum CallType {
        STATICCALL,
        DELEGATECALL,
        CALL
    }

    struct Executable {
        CallType callType;
        address target;
        uint256 value;
        bytes data;
    }
}

pragma solidity ^0.8.18;

import "src/interfaces/IExecutable.sol";

interface IStrategy {
    function strategyName() external view returns (string memory); // Must be unique

    function id() external view returns (bytes32);

    function action(address _wallet)
        external
        view
        returns (IExecutable.Executable memory);

    function trigger(address _wallet)
        external
        view
        returns (IExecutable.Executable memory);
}

pragma solidity ^0.8.18;

interface IVMParams {
    struct VMParams {
        bytes32[] commands;
        bytes[] state;
    }
}

pragma solidity ^0.8.18;

import "./IExecutable.sol";

interface IWalletAdapter is IExecutable {
    function id() external view returns (uint8);

    function formatForWallet(
        address _wallet,
        IExecutable.Executable memory _txn
    ) external view returns (IExecutable.Executable memory);

    function isAuthorized(address _wallet, address _user)
        external
        view
        returns (bool);

    function decodeReturnData(bytes memory data)
        external
        view
        returns (bool success);
}