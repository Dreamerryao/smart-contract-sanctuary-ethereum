import "src/interfaces/IBotManagerTest.sol";
import "src/interfaces/IStrategy.sol";

import "src/core/GelatoManagerTest.sol";

import "src/core/AddressProviderService.sol";

contract BotManagerTest is IBotManagerTest, GelatoManagerTest {
    error UnauthorizedBot();

    address public fundManager;

    bytes32[] public taskIds;

    // uint256 public value;
    mapping(uint i => uint value) public valueMapping;

    constructor(address _ops) GelatoManagerTest(_ops, msg.sender) {
        fundManager = msg.sender;
    }

    function getTasks() public view returns (bytes32[] memory) {
        return taskIds;
    }

    function isBotAuthorized(address bot) public returns (bool) {
        return msg.sender == dedicatedMsgSender; // from Gelato OpsReady
    }

    function depositFunds() external payable {
        onlyFundManager();
        _depositFunds(msg.value, 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE);
    }

    function createTask(uint256 i) external returns (bytes32 taskId) {
        taskId = _initTask(i);

        taskIds.push(taskId);
    }

    function removeTask(bytes32 taskId) external {
        _removeTask(taskId);

        for (uint256 idx; idx < taskIds.length; idx++) {
            if (taskIds[idx] == taskId) delete taskIds[idx];
        }
    }

    function resolver(uint256 i) external view returns (bool, bytes memory) {
        return (true, abi.encodeCall(this.execute, (i)));
    }

    function execute(uint256 i) external {
        if (!isBotAuthorized(msg.sender)) revert UnauthorizedBot();

        valueMapping[i] += 1;
    }

    function onlyFundManager() internal {
        require(msg.sender == fundManager);
    }
}

