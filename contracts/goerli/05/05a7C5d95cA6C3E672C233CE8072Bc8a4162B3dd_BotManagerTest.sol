pragma solidity ^0.8.18;

import "src/interfaces/IExecutable.sol";
import "src/interfaces/IWalletAdapter.sol";

contract Executor {
    error UnsupportedExecution();
    error NonStaticExecution();

    function _executeOnWallet(
        address _wallet,
        IWalletAdapter adapter,
        IExecutable.Executable memory _txn
    ) internal returns (bool, bytes memory) {
        IExecutable.Executable memory formattedTxn = adapter.formatForWallet(
            _wallet,
            _txn
        );
        (bool success, bytes memory result) = _execute(formattedTxn);
        bool txnSuccess = adapter.decodeReturnData(result);
        return (txnSuccess, result);
    }

    function _executeStatic(IExecutable.Executable memory _txn)
        internal
        view
        returns (bool success, bytes memory result)
    {
        IExecutable.CallType callType = _txn.callType;
        if (callType != IExecutable.CallType.STATICCALL)
            revert NonStaticExecution();
        (success, result) = (_txn.target).staticcall(_txn.data);
    }

    function _execute(IExecutable.Executable memory _txn)
        private
        returns (bool success, bytes memory result)
    {
        IExecutable.CallType callType = _txn.callType;

        if (callType == IExecutable.CallType.DELEGATECALL) {
            (success, result) = payable(_txn.target).delegatecall(_txn.data);
        } else if (callType == IExecutable.CallType.CALL) {
            (success, result) = payable(_txn.target).call{value: _txn.value}(
                _txn.data
            );
        } else {
            revert UnsupportedExecution();
        }
    }
}

