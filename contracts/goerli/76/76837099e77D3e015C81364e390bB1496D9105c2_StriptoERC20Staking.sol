// 
interface IStripto {
    function init(bytes memory _data) external;
}

// 
interface IStriptoFactory {
    function addImplementation(address _implementation) external;

    function addManyWhitelist(
        address[] memory accounts,
        uint256[] memory credits
    ) external;

    function addWhitelist(address account, uint256 deployCredit) external;

    function createStripto(
        uint256 _index,
        bytes memory _data
    ) external returns (address stripto);

    function disableWhitelist() external;

    function enableWhitelist() external;

    function fee() external view returns (uint256);

    function implementations(uint256) external view returns (address);

    function implementationsLength() external view returns (uint256);

    function isWhitelistEnabled() external view returns (bool);

    function isWhitelisted(address account) external view returns (uint256);

    function owner() external view returns (address);

    function predictAddress(
        uint256 _index,
        bytes memory _data,
        address _deployer
    ) external view returns (address);

    function removeManyWhitelist(
        address[] memory accounts,
        uint256[] memory credits
    ) external;

    function removeWhitelist(address account, uint256 deployCredit) external;

    function renounceOwnership() external;

    function setFee(uint256 _fee) external;

    function striptos(uint256) external view returns (address);

    function striptosLength() external view returns (uint256);

    function transferOwnership(address newOwner) external;
}

// 
uint256 constant REWARD_DEBT_SCALE = 1e36;

struct UserDeposit {
    uint256 amount;
    uint256 rewardDebt;
    uint64 unlockTime;
    uint64 tier;
}

struct Pool {
    uint64 lockPeriod;
    uint64 multiplier;
    uint64 penalty;
    uint64 lastRewardTime;
    uint256 accPerShare;
    uint256 totalStaked;
    uint256 minStake;
    uint256 maxStake;
}

struct FarmData {
    address stakedToken;
    address rewardToken;
    uint64 startTime;
    uint64 endTime;
    uint64 lastEmissionChange;
    uint64 totalMultipliers;
    uint256 rewardsSinceLastChange;
    uint256 totalRewards;
    uint256 totalRewardsPerSec;
    uint256 totalStaked;
    uint256 totalRewardsReceived;
    Pool[4] pools;
}

struct InitData {
    address stakedToken;
    address rewardToken;
    address owner;
    uint256 initialRewards;
    uint64 startTime;
    uint64 endTime; // in seconds
    uint64[4] lockPeriods;
    uint64[4] multipliers;
    uint64[4] penalties;
    uint256[4] minStakes;
    uint256[4] maxStakes;
}

contract StriptoERC20Staking is IStripto, Ownable {
    using SafeERC20 for IERC20;

    address public immutable factory;

    FarmData farmData;

    function getFarmData() external view returns (FarmData memory) {
        return farmData;
    }

    mapping(address => UserDeposit[]) userDeposits;

    mapping(address => uint256) public userRewardsReceived;
    mapping(address => uint256[4]) userTotalStaked;

    event Deposit(address indexed user, int64 indexed tier, uint256 amount);

    event Withdraw(address indexed user, int64 indexed tier, uint256 amount);

    event EmergencyWithdraw(
        address indexed user,
        int64 indexed tier,
        uint256 amount
    );

    event RewardPaid(address indexed user, uint256 reward);

    constructor(address _factory) {
        factory = _factory;
    }

    modifier onlyBeforeEnd() {
        require(block.timestamp < farmData.endTime, "stake ended");
        _;
    }

    modifier onlyWithFee() {
        require(
            msg.value >= (IStriptoFactory(factory).fee() * 10) / 100,
            "invalid fee"
        );
        address payable feeReceiver = payable(factory);
        feeReceiver.transfer(msg.value);
        _;
    }

    function init(bytes calldata _data) external override {
        require(msg.sender == factory, "StriptoERC20Staking: only factory");

        InitData memory data = abi.decode(_data, (InitData));

        require(data.stakedToken != address(0), "staked token is zero");
        require(data.rewardToken != address(0), "reward token is zero");
        require(data.owner != address(0), "owner is zero");
        require(data.startTime < data.endTime, "endtime is before start time");
        require(data.startTime > block.timestamp, "starttime passed");

        farmData.stakedToken = data.stakedToken;
        farmData.rewardToken = data.rewardToken;

        farmData.startTime = data.startTime;
        farmData.endTime = data.startTime;
        farmData.lastEmissionChange = data.startTime;

        uint256 totalTime = data.endTime - data.startTime;
        farmData.totalRewardsPerSec = data.initialRewards / totalTime;
        require(
            farmData.totalRewardsPerSec > 0,
            "not enough rewards to sustain this farm's lifetime"
        );

        _transferOwnership(data.owner);

        for (uint256 i = 0; i < 4; i++) {
            farmData.pools[i].lockPeriod = data.lockPeriods[i];
            farmData.pools[i].multiplier = data.multipliers[i];
            farmData.pools[i].penalty = data.penalties[i];
            farmData.pools[i].lastRewardTime = data.startTime;
            farmData.totalMultipliers += data.multipliers[i];
            farmData.pools[i].minStake = data.minStakes[i];
            farmData.pools[i].maxStake = data.maxStakes[i];
        }

        require(data.initialRewards > 0, "no rewards");
        _depositRewardToken(data.initialRewards, data.owner);
    }

    function depositRewardToken(
        uint256 _amount
    ) external payable onlyOwner onlyBeforeEnd onlyWithFee {
        _depositRewardToken(_amount, msg.sender);
    }

    function deposit(uint64 _tier, uint256 _amount) external onlyBeforeEnd {
        updatePool(_tier);

        Pool storage pool = farmData.pools[_tier];

        require(_amount >= pool.minStake && _amount > 0, "amount < min stake");

        userTotalStaked[msg.sender][_tier] += _amount;

        if (pool.maxStake > 0)
            require(
                userTotalStaked[msg.sender][_tier] < pool.maxStake,
                "over the allowed max stake in tier"
            );

        uint256 netAmount = _safeTransferFrom(
            farmData.stakedToken,
            msg.sender,
            address(this),
            _amount
        );

        uint256 rewardsDebt = (netAmount * pool.accPerShare) /
            REWARD_DEBT_SCALE;

        userDeposits[msg.sender].push(
            UserDeposit({
                amount: netAmount,
                rewardDebt: rewardsDebt,
                unlockTime: uint64(block.timestamp) + pool.lockPeriod * 1 days,
                tier: _tier
            })
        );

        farmData.totalStaked += netAmount;
        pool.totalStaked += netAmount;

        emit Deposit(msg.sender, int64(_tier), netAmount);
    }

    function withdraw(uint256 _index) external {
        UserDeposit memory userDeposit = userDeposits[msg.sender][_index];

        require(
            userDeposit.unlockTime <= block.timestamp ||
                farmData.endTime <= block.timestamp,
            "Deposit is Locked"
        );

        // remove user deposit from array
        userDeposits[msg.sender][_index] = userDeposits[msg.sender][
            userDeposits[msg.sender].length - 1
        ];
        userDeposits[msg.sender].pop();

        updatePool(userDeposit.tier);

        Pool storage pool = farmData.pools[userDeposit.tier];

        uint256 pendingRewards = (userDeposit.amount * pool.accPerShare) /
            REWARD_DEBT_SCALE -
            userDeposit.rewardDebt;

        pool.totalStaked -= userDeposit.amount;
        farmData.totalStaked -= userDeposit.amount;

        userRewardsReceived[msg.sender] += pendingRewards;

        farmData.totalRewardsReceived += pendingRewards;

        if (pendingRewards > 0) {
            IERC20(farmData.rewardToken).safeTransfer(
                msg.sender,
                pendingRewards
            );
        }
        IERC20(farmData.stakedToken).safeTransfer(
            msg.sender,
            userDeposit.amount
        );

        emit Withdraw(msg.sender, int64(userDeposit.tier), userDeposit.amount);

        emit RewardPaid(msg.sender, pendingRewards);
    }

    function earlyWithdraw(uint256 _index) external {
        UserDeposit memory userDeposit = userDeposits[msg.sender][_index];

        require(
            userDeposit.unlockTime > block.timestamp &&
                farmData.endTime > block.timestamp,
            "Deposit is not Locked"
        );

        // remove user deposit from array
        userDeposits[msg.sender][_index] = userDeposits[msg.sender][
            userDeposits[msg.sender].length - 1
        ];
        userDeposits[msg.sender].pop();

        updatePool(userDeposit.tier);

        Pool storage pool = farmData.pools[userDeposit.tier];

        uint256 pendingRewards = (userDeposit.amount * pool.accPerShare) /
            REWARD_DEBT_SCALE -
            userDeposit.rewardDebt;

        uint256 penalty = (pendingRewards * pool.penalty) / 100;

        uint256 rewards = pendingRewards - penalty;

        pool.totalStaked -= userDeposit.amount;
        farmData.totalStaked -= userDeposit.amount;

        userRewardsReceived[msg.sender] += rewards;

        farmData.totalRewardsReceived += rewards;

        if (rewards > 0) {
            IERC20(farmData.rewardToken).safeTransfer(msg.sender, rewards);
        }

        farmData.totalRewards += penalty;
        uint256 addedTime = penalty / farmData.totalRewardsPerSec;
        farmData.endTime += uint64(addedTime);

        IERC20(farmData.stakedToken).safeTransfer(
            msg.sender,
            userDeposit.amount
        );

        emit EmergencyWithdraw(
            msg.sender,
            int64(userDeposit.tier),
            userDeposit.amount
        );

        emit RewardPaid(msg.sender, rewards);
    }

    function updatePool(uint256 _pid) public {
        Pool storage pool = farmData.pools[_pid];

        uint256 currentTime = block.timestamp < farmData.endTime
            ? block.timestamp
            : farmData.endTime;

        if (currentTime <= pool.lastRewardTime) {
            return;
        }

        uint256 staked = pool.totalStaked;
        if (staked == 0) {
            pool.lastRewardTime = uint64(currentTime);
            return;
        }

        uint256 timeSinceLastReward = currentTime - pool.lastRewardTime;
        uint256 reward = (farmData.totalRewardsPerSec *
            timeSinceLastReward *
            pool.multiplier) / farmData.totalMultipliers;
        pool.accPerShare += (reward * REWARD_DEBT_SCALE) / staked;
        pool.lastRewardTime = uint64(currentTime);
    }

    function updateAllPools() public {
        for (uint256 i = 0; i < 4; i++) {
            updatePool(i);
        }
    }

    function updateMultiplier(
        uint256 _pid,
        uint64 _multiplier
    ) external payable onlyOwner onlyWithFee {
        require(
            farmData.pools[_pid].multiplier != _multiplier && _multiplier > 0,
            "multiplier is the same"
        );

        // calculates all rewards up to this point
        updateAllPools();

        uint64 oldMultiplier = farmData.pools[_pid].multiplier;
        farmData.pools[_pid].multiplier = _multiplier;
        farmData.totalMultipliers -= oldMultiplier;
        farmData.totalMultipliers += _multiplier;
    }

    function updateRewardsPerSeconds(
        uint256 _newRewardsPerSec
    ) external payable onlyOwner onlyBeforeEnd onlyWithFee {
        require(
            _newRewardsPerSec > farmData.totalRewardsPerSec,
            "you can only increase rewards per second"
        );

        updateAllPools();

        uint256 totalRewardsTilNow = _totalRewardsTilNow();

        farmData.lastEmissionChange = uint64(block.timestamp);

        uint256 rewardsDiff = _newRewardsPerSec - farmData.totalRewardsPerSec;

        uint256 extraRewardsNeeded = rewardsDiff *
            (farmData.endTime - block.timestamp);

        uint256 amount = _safeTransferFrom(
            farmData.rewardToken,
            msg.sender,
            address(this),
            extraRewardsNeeded
        );

        require(amount == extraRewardsNeeded, "Tax Detected");

        farmData.totalRewards += amount;

        farmData.totalRewardsPerSec = _newRewardsPerSec;

        farmData.rewardsSinceLastChange = totalRewardsTilNow;
    }

    function _totalRewardsTilNow() internal view returns (uint256) {
        uint256 totalRewardsTilNow = farmData.rewardsSinceLastChange;
        if (block.timestamp > farmData.lastEmissionChange) {
            uint256 timePassed = block.timestamp - farmData.lastEmissionChange;
            totalRewardsTilNow += timePassed * farmData.totalRewardsPerSec;
        }
        return totalRewardsTilNow;
    }

    function _safeTransferFrom(
        address _token,
        address _from,
        address _to,
        uint256 _amount
    ) internal returns (uint256) {
        uint256 balanceBefore = IERC20(_token).balanceOf(_to);
        IERC20(_token).safeTransferFrom(_from, _to, _amount);
        uint256 balanceAfter = IERC20(_token).balanceOf(_to);

        return balanceAfter - balanceBefore;
    }

    function _depositRewardToken(uint256 _amount, address _from) internal {
        uint256 netAmount = _safeTransferFrom(
            farmData.rewardToken,
            _from,
            address(this),
            _amount
        );

        farmData.totalRewards += netAmount;

        // adjust end time
        if (farmData.totalRewardsPerSec > 0) {
            uint256 newTime = netAmount / farmData.totalRewardsPerSec;
            farmData.endTime += uint64(newTime);
        } else farmData.endTime = type(uint64).max;
    }

    function pendingRewardForDeposit(
        address _user,
        uint256 _index
    ) public view returns (uint256 pending) {
        UserDeposit storage userDeposit = userDeposits[_user][_index];

        Pool memory pool = farmData.pools[userDeposit.tier];

        uint256 currentTime = block.timestamp < farmData.endTime
            ? block.timestamp
            : farmData.endTime;

        if (currentTime > pool.lastRewardTime) {
            uint256 staked = pool.totalStaked;
            if (staked > 0) {
                uint256 timeSinceLastReward = currentTime - pool.lastRewardTime;
                uint256 reward = (farmData.totalRewardsPerSec *
                    timeSinceLastReward *
                    pool.multiplier) / farmData.totalMultipliers;
                pool.accPerShare =
                    pool.accPerShare +
                    ((reward * REWARD_DEBT_SCALE) / staked);
            }
        }
        pending =
            (userDeposit.amount * pool.accPerShare) /
            REWARD_DEBT_SCALE -
            userDeposit.rewardDebt;
    }

    function pendingReward(
        address _user
    ) public view returns (uint256 pendingRewards) {
        for (uint256 i = 0; i < userDeposits[_user].length; i++) {
            pendingRewards += pendingRewardForDeposit(_user, i);
        }
    }

    struct ExtendedUserDeposit {
        uint256 amount;
        uint256 rewardDebt;
        uint256 tier;
        uint256 pendingRewards;
        uint256 unlockTime;
    }

    function getUserDeposits(
        address _user
    ) public view returns (ExtendedUserDeposit[] memory) {
        ExtendedUserDeposit[] memory deposits = new ExtendedUserDeposit[](
            userDeposits[_user].length
        );

        for (uint256 i = 0; i < userDeposits[_user].length; i++) {
            deposits[i].amount = userDeposits[_user][i].amount;
            deposits[i].rewardDebt = userDeposits[_user][i].rewardDebt;
            deposits[i].tier = userDeposits[_user][i].tier;
            deposits[i].pendingRewards = pendingRewardForDeposit(_user, i);
            deposits[i].unlockTime = userDeposits[_user][i].unlockTime;
        }

        return deposits;
    }

    struct ExtendedUserData {
        uint256 totalStaked;
        uint256 totalPaidRewards;
        uint256 totalPendingRewards;
        ExtendedUserDeposit[] deposits;
    }

    function getUserData(
        address _user
    ) public view returns (ExtendedUserData memory) {
        ExtendedUserData memory userData;

        for (uint256 i = 0; i < userDeposits[_user].length; i++) {
            userData.totalStaked += userDeposits[_user][i].amount;
            userData.totalPaidRewards += userDeposits[_user][i].rewardDebt;
            userData.totalPendingRewards += pendingRewardForDeposit(_user, i);
        }

        userData.deposits = getUserDeposits(_user);

        return userData;
    }
}