 * @dev over the order in which VRF responses appear on the blockchain, so if
 * @dev your contract could have multiple VRF requests in flight simultaneously,
 * @dev you must ensure that the order in which the VRF responses arrive cannot
 * @dev be used to manipulate your contract's user-significant behavior.
 *
 * @dev Since the block hash of the block which contains the requestRandomness
 * @dev call is mixed into the input to the VRF *last*, a sufficiently powerful
 * @dev miner could, in principle, fork the blockchain to evict the block
 * @dev containing the request, forcing the request to be included in a
 * @dev different block with a different hash, and therefore a different input
 * @dev to the VRF. However, such an attack would incur a substantial economic
 * @dev cost. This cost scales with the number of blocks the VRF oracle waits
 * @dev until it calls responds to a request. It is for this reason that
 * @dev that you can signal to an oracle you'd like them to wait longer before
 * @dev responding to the request (however this is not enforced in the contract
 * @dev and so remains effective only in the case of unmodified oracle software).
 */
abstract contract VRFConsumerBaseV2 {
  error OnlyCoordinatorCanFulfill(address have, address want);
  address private immutable vrfCoordinator;

  /**
   * @param _vrfCoordinator address of VRFCoordinator contract
   */
  constructor(address _vrfCoordinator) {
    vrfCoordinator = _vrfCoordinator;
  }

  /**
   * @notice fulfillRandomness handles the VRF response. Your contract must
   * @notice implement it. See "SECURITY CONSIDERATIONS" above for important
   * @notice principles to keep in mind when implementing your fulfillRandomness
   * @notice method.
   *
   * @dev VRFConsumerBaseV2 expects its subcontracts to have a method with this
   * @dev signature, and will call it once it has verified the proof
   * @dev associated with the randomness. (It is triggered via a call to
   * @dev rawFulfillRandomness, below.)
   *
   * @param requestId The Id initially returned by requestRandomness
   * @param randomWords the VRF output expanded to the requested number of words
   */
  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal virtual;

  // rawFulfillRandomness is called by VRFCoordinator when it receives a valid VRF
  // proof. rawFulfillRandomness then calls fulfillRandomness, after validating
  // the origin of the call
  function rawFulfillRandomWords(uint256 requestId, uint256[] memory randomWords) external {
    if (msg.sender != vrfCoordinator) {
      revert OnlyCoordinatorCanFulfill(msg.sender, vrfCoordinator);
    }
    fulfillRandomWords(requestId, randomWords);
  }
}


pragma solidity ^0.8.7;

error SaleInactive();
error SoldOut();
error InvalidPrice();
error WithdrawFailed();
error InvalidQuantity();
error InvalidProof();
error InvalidBatchMint();

contract RTTT is ERC721, Ownable, ERC2981, VRFConsumerBaseV2, OperatorFilterer {

    using Strings for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private currentTokenId;

    uint256 public immutable supply = 10000;
    uint256 public constant _price = 0.5 ether;
    uint public endDay;
    string public baseExtension = ".json";
    string public _baseTokenURI;

    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId;
    address vrfCoordinator = 0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D;
    bytes32 s_keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
    uint32 callbackGasLimit = 2500000;
    uint16 requestConfirmations = 3;
    uint32 numWords = 100;
    address admin;
    
    event eFulFillRandomWords(uint256 indexed requestId);
    event eliminateFunctionCalled(uint256 indexed reqeustId);
    struct Ticket {
        uint level;
        string comment;
        bool claimed;
    }
    mapping (uint => Ticket) public _tickets;
    mapping (uint => uint) private _forEliminate;
    uint public currentLevel = 1;
    uint public _eliminated = 0;
    uint public _forEliminatedCount = 0; 

    /**
     * @notice Constructor inherits VRFConsumerBaseV2
     *
     * @dev NETWORK: Goerli
     *
     * @param subscriptionId subscription id that this consumer contract can use
     */

    constructor(
        string memory _baseUri,
        uint96 _royaltyAmount,
        uint64 subscriptionId
    ) ERC721("Rise to the top", "RTTT") VRFConsumerBaseV2(vrfCoordinator) OperatorFilterer(address(0), false) {
        _baseTokenURI = _baseUri;
        _setDefaultRoyalty(0x20F11547e74A1Fb8bf75A6E65AF65E4c1e153Ed9, _royaltyAmount);
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_subscriptionId = subscriptionId;
        endDay = (block.timestamp + 10 days);
    }

    function mintToken(string memory _comment) payable external {
        uint256 newItemId = currentTokenId.current();
        require(_eliminated == 0, "Elimination has been started");
        require(newItemId <= supply, "Purchase would exceed max supply of tokens");
        require(_price <= msg.value, "Ether value sent is not correct");

        _safeMint(msg.sender, newItemId);
        _tickets[newItemId].comment = _comment;
        _tickets[newItemId].level = 6;
        _tickets[newItemId].claimed = false;
        _forEliminate[newItemId] = newItemId;
        currentTokenId.increment();
    }

    function promoMint(string memory _comment) payable external {
        uint256 newItemId = currentTokenId.current();
        require(msg.sender == admin, "This function is for only the admin.");
        require(newItemId <= supply, "Purchase would exceed max supply of tokens");
        require(_price <= msg.value, "Ether value sent is not correct");

        _safeMint(msg.sender, newItemId);
        _tickets[newItemId].comment = _comment;
        _tickets[newItemId].level = 6;
        _tickets[newItemId].claimed = false;
        _forEliminate[newItemId] = newItemId;
        currentTokenId.increment();
    }

    /**
     * @notice Callback function used by VRF Coordinator to return the random number to this contract.
     *
     * @dev Some action on the contract state should be taken here, like storing the result.
     * @dev WARNING: take care to avoid having multiple VRF requests in flight if their order of arrival would result
     * in contract states with different outcomes. Otherwise miners or the VRF operator would could take advantage
     * by controlling the order.
     * @dev The VRF Coordinator will only send this function verified responses, and the parent VRFConsumerBaseV2
     * contract ensures that this method only receives randomness from the designated VRFCoordinator.
     *
     * @param requestId uint256
     * @param randomWords  uint256[] The random result returned by the oracle.
     */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        uint256 tsupply = currentTokenId.current();
        for(uint i = 0; i < randomWords.length; i ++) {
            uint256 randomId = (randomWords[i] % (tsupply - _forEliminatedCount));
            uint index = _forEliminate[randomId];
            if(currentLevel == 6 || currentLevel == 7)
                _tickets[index].level = 5;
            else if(currentLevel == 8 || currentLevel == 9)
                _tickets[index].level = 4;
            else if (currentLevel == 10 || currentLevel == 11) 
                _tickets[index].level = 3;
            else if (currentLevel == 12) 
                _tickets[index].level = 2;
            else _tickets[index].level = 7;
            _forEliminate[randomId] = _forEliminate[tsupply -1 - _forEliminatedCount];
            _forEliminate[tsupply - _forEliminatedCount] = index;
            _forEliminatedCount ++;
        }
        if(_forEliminatedCount == tsupply / 5) currentLevel = 2;
        if(_forEliminatedCount == tsupply * 2 / 5) currentLevel = 3;
        if(_forEliminatedCount == tsupply * 3 / 5) currentLevel = 4;
        if(_forEliminatedCount == tsupply * 4 / 5) currentLevel = 5;
        if(_forEliminatedCount == tsupply * 8887 / 10000) currentLevel = 6;
        if(_forEliminatedCount == tsupply * 9387 / 10000) currentLevel = 7;
        if(_forEliminatedCount == tsupply * 9887 / 10000) currentLevel = 8;
        if(_forEliminatedCount == tsupply * 9937 / 10000) currentLevel = 9;
        if(_forEliminatedCount == tsupply * 9987 / 10000) currentLevel = 10;
        if(_forEliminatedCount == tsupply * 9992 / 10000) currentLevel = 11;
        if(_forEliminatedCount == tsupply * 9997 / 10000) currentLevel = 12;
        if(_forEliminatedCount == tsupply * 9999 / 10000) currentLevel = 13;
        emit eFulFillRandomWords(requestId);
    }

    function eliminate() public onlyOwner returns (uint256 requestId) {
        uint current = 0;
        uint256 tsupply = currentTokenId.current();
        if(currentLevel == 1 || currentLevel == 2 || currentLevel == 3 || currentLevel == 4) {
            current = tsupply * currentLevel / 5;
        } else if(currentLevel == 5) {
            current = tsupply * 8887 / 10000;
        }  else if (currentLevel == 6) {
            current = tsupply * 9387 / 10000;
        } else if (currentLevel == 7) {
            current = tsupply * 9887 / 10000;
        } else if (currentLevel == 8) {
            current = tsupply * 9937 / 10000;
        } else if (currentLevel == 9) {
            current = tsupply * 9987 / 10000;
        } else if (currentLevel == 10) {
            current = tsupply * 9992 / 10000;
        } else if (currentLevel == 11) {
            current = tsupply * 9997 / 10000;
        } else if (currentLevel == 12) {
            current = tsupply * 9999 / 10000;
        } else if (currentLevel == 13) {
            uint index = _forEliminate[0];
            _tickets[index].level = 1;
            current = _eliminated;
        }

        while(current != _eliminated) {
            if(current - _eliminated > numWords) {
                requestId = COORDINATOR.requestRandomWords(s_keyHash, s_subscriptionId, requestConfirmations, callbackGasLimit, numWords);
                _eliminated += numWords;
            }
            else {
                requestId = COORDINATOR.requestRandomWords(s_keyHash, s_subscriptionId, requestConfirmations, callbackGasLimit, uint32( current - _eliminated));
                _eliminated += current - _eliminated;  
            }
        }
        emit eliminateFunctionCalled(requestId);
    }

    function getLevels(uint tokenId) public view returns (uint) {
        return _tickets[tokenId].level;
    }
    
     function getForElimate(uint index) public view returns (uint) {
        return _forEliminate[index];
    }

    function claimRewards(uint index) public {
        require(_tickets[index].claimed == false, "This ticket has claimed his reward.");
        require(_tickets[index].level != 0, "This ticket has been eliminated.");
        require(ownerOf(index) == msg.sender, "You are not the owner of this ticket.");
        uint256 balance = address(this).balance;
        uint256 tsupply = currentTokenId.current();
        if(_tickets[index].level == 1) {
            payable(msg.sender).transfer(balance * 20 / 100);
        } else if (_tickets[index].level == 2) {
            payable(msg.sender).transfer( balance * 30 / (100 * tsupply * 2 / 10000)  );
        } else if (_tickets[index].level == 3) {
            payable(msg.sender).transfer( balance * 10 / (100 * tsupply / 1000)  );
        } else if (_tickets[index].level == 4) {
            payable(msg.sender).transfer( balance * 10 / (100 * tsupply / 100)  );
        } else if (_tickets[index].level == 5) {
            payable(msg.sender).transfer( balance * 20 / (100 * tsupply / 10)  );
        }
        _tickets[index].claimed = true;
    }

    function withdrawAdmin() public {
        require(msg.sender == admin, "You are not the admin.");
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance / 10);
    }

    function transfer(uint256 amount, address to) public onlyOwner {
        require(block.timestamp > endDay, "This lottery is not finished.");
        payable(to).transfer(amount);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        return string(abi.encodePacked(_baseURI(), _tickets[tokenId].level, "/", (tokenId).toString(), baseExtension));
    }
    
    function setBaseURI(string memory baseURI) external onlyOwner {
        _baseTokenURI = baseURI;
    }

    function setRoyaltyInfo(address receiver, uint96 feeBasisPoints)
        external
        onlyOwner
    {
        _setDefaultRoyalty(receiver, feeBasisPoints);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(address operator, uint256 tokenId) public override onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

}