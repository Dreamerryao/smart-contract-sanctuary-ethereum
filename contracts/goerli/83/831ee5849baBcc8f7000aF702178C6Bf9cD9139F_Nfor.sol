     *  /^OrgRoleControl: account (0x[0-9a-f]{40}) is missing role (0x[0-9a-f]{64})$/
     */
    modifier onlyOrgRole(string memory org, string memory role) {
        checkOrgRole(org, role, msg.sender);
        _;
    }

    function setOrgPauseUnpause(string calldata org, bool mode) public authorize("toManage") {
        orgPause[org] = mode;
    }

    /**
     * @dev Org getter.
     *
     * Receives a string and returns the instrument in bytes32
     */
    function getOrg(string calldata org) public view returns (bytes32) {
        return orgs[org];
    }

    /**
     * @dev Returns `true` if `account` has been granted `role`.
     *
     * Receives a Org, Role and Account and returns the associated memberhip
     */
    function hasOrgRole(
        string memory org,
        string memory role,
        address account
    ) public view existentOrg(org) returns (bool) {
        uint256 id = idFromOrgRole(org, role);
        if (orgPause[org] == false) {
            if (
                balanceOf(account, id) > 0 &&
                keccak256(abi.encodePacked(cred[id].org)) == keccak256(abi.encodePacked(org)) &&
                keccak256(abi.encodePacked(cred[id].role)) == keccak256(abi.encodePacked(role))
            ) {
                return true;
            }
        }
        revert("Nfor: Not Allowed");
    }

    /**
     * @dev Revert with a standard message if `account` is missing `role`.
     *
     * The format of the revert reason is given by the following regular expression:
     *
     *  /^OrgRoleControl: account (0x[0-9a-f]{40}) is missing role (0x[0-9a-f]{64})$/
     */
    function checkOrgRole(
        string memory org,
        string memory role,
        address account
    ) public view returns (bool) {
        if (hasOrgRole(org, role, account)) {
            return true;
        }
        revert(
            string(
                abi.encodePacked(
                    "Nfor: account ",
                    Strings.toHexString(uint160(account), 20),
                    " missing role ",
                    role,
                    " at ",
                    org
                )
            )
        );
    }

    /**
     * @dev Grants `role` to `account`.
     *
     * If `account` had not been already granted `role`, emits a {RoleGranted}
     * event.
     *
     * Requirements:
     *
     * - the caller must have ``role``'s admin role.
     */
    function createOrg(string memory org) public newOrg(org) {
        require(bytes(org).length > 0 && bytes(org).length < 30, "Nfor: Inv org length");
        orgs[org] = keccak256(abi.encodePacked(org));
        _mint(org, "ORG_ADMIN_ROLE", msg.sender);
    }

    function _mint(
        string memory org,
        string memory role,
        address grantee
    ) internal {
        require(bytes(role).length > 0 && bytes(role).length < 30, "Nfor: Inv role length");
        uint256 id = idFromOrgRole(org, role);
        require(balanceOf(grantee, id) == 0, "Nfor: Already granted");
        cred[id].org = org;
        cred[id].role = role;
        ERC1155._mint(grantee, id, 1, "");
    }

    /**
     * @dev Grants `role` to `account`.
     *
     * If `account` had not been already granted `role`, emits a {RoleGranted}
     * event.
     *
     * Requirements:
     *
     * - the caller must have ``role``'s admin role at the specific organization.
     */
    function grantOrgRole(
        string memory org,
        string memory role,
        address account
    ) public existentOrg(org) onlyOrgRole(org, "ORG_ADMIN_ROLE") {
        _mint(org, role, account);
    }

    /**
     * @dev Revokes `role` from `account`.
     *
     * If `account` had been granted `role`, emits a {RoleRevoked} event.
     *
     * Requirements:
     *
     * - the caller must have ``role``'s admin role at the specific organization.
     */

    function revokeOrgRole(
        string memory org,
        string memory role,
        address account
    ) public existentOrg(org) onlyOrgRole(org, "ORG_ADMIN_ROLE") {
        require(hasOrgRole(org, role, account), "Nfor: Not granted");
        uint256 id = idFromOrgRole(org, role);
        ERC1155._burn(account, id, balanceOf(msg.sender, id));
    }

    /**
     * @dev Revokes `role` from the calling account.
     *
     * Roles are often managed via {grantRole} and {revokeRole}: this function's
     * purpose is to provide a mechanism for accounts to lose their privileges
     * if they are compromised (such as when a trusted device is misplaced).
     *
     * If the calling account had been revoked `role`, emits a {RoleRevoked}
     * event.
     *
     * Requirements:
     *
     * - the caller must be `account`.
     */
    function renounceOrgRole(string memory org, string memory role) public existentOrg(org) {
        require(hasOrgRole(org, role, msg.sender), "Nfor: Not granted");
        uint256 id = idFromOrgRole(org, role);
        ERC1155._burn(msg.sender, id, balanceOf(msg.sender, id));
    }

    /**
     * @dev See {IERC1155-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public pure override {
        revert("Nfor: Not transferable");
    }

    /**
     * @dev See {IERC1155-safeBatchTransferFrom}.
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public pure override {
        revert("Nfor: Not transferable");
    }

    function _auth(
        string memory org,
        string[POLICY_ROLES] memory _policyRoles,
        address account
    ) internal view returns (bool result) {
        for (uint256 i = 0; i < _policyRoles.length; i++) {
            if ((hasOrgRole(org, _policyRoles[i], account) && bytes(_policyRoles[i]).length != 0)) {
                return result = true;
            }
        }
        revert(
            string(
                abi.encodePacked(
                    "Nfor: ",
                    Strings.toHexString(uint160(account), 20),
                    " missing policy at ",
                    org
                )
            )
        );
    }

    function authenticate(string memory _org, string memory _authPolicy) public view {
        _auth(_org, authPolicy[_org][_authPolicy], msg.sender);
    }

    function setPolicyRoles(
        string memory org,
        string memory _policyName,
        string[POLICY_ROLES] memory _policyRoles
    ) public onlyOrgRole(org, "ORG_ADMIN_ROLE") {
        emit setPolicy(org, _policyName, _policyRoles);
        for (uint256 i = 0; i < POLICY_ROLES; i++) {
            if (i <= _policyRoles.length) {
                require(bytes(_policyRoles[i]).length != 0, "Reserved role");
                authPolicy[org][_policyName][i] = _policyRoles[i];
            }
        }
    }

    function getPolicyRoles(string memory org, string memory _policyName)
        public
        view
        onlyOrgRole(org, "ORG_ADMIN_ROLE")
        returns (string[POLICY_ROLES] memory)
    {
        return (authPolicy[org][_policyName]);
    }

    function withdraw(address _tokenContract) public authorize("toWhithdraw") {
        payable(msg.sender).transfer(address(this).balance);
        IERC20(_tokenContract).transfer(
            msg.sender,
            IERC20(_tokenContract).balanceOf(address(this))
        );
    }
