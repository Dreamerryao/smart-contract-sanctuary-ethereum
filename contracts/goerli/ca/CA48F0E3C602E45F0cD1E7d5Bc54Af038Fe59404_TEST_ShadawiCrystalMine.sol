    function newTimeWindow(uint _windowDays, uint _tokenID, uint _timeLimit, uint _start) internal view returns(uint[2] memory){
        uint _windowLimit = (_windowDays * 86400);
        uint _max = (_windowLimit - _timeLimit);
        

        uint result = suedoRand(totalDays(_start, _windowLimit), _tokenID, _max); //number will last the whole window period
        result = _midigate(result, _max);
        return [result, result + _timeLimit];
    }


    /* returns if time now is between desired window */
    function _status(uint256 _tokenDays, uint256 _tokenID, uint _start, uint _timeLimit) internal view returns(uint){
        uint c = timeInside(_tokenDays, _start);
