    function elapsed(uint _start) internal view returns(uint){
        return block.timestamp - _start;
    }

    function totalDays(uint _start, uint _reqDays) internal view returns(uint){
        return (elapsed(_start)/_reqDays); //returns Math floor
    }

    function suedoRand(uint first, uint second, uint _limit) internal view returns(uint){
        return uint(keccak256(abi.encodePacked(first, msg.sender, second))) % _limit; 
    }
    
    function _midigate(uint n, uint nMax) internal view returns(uint){
        uint dice = suedoRand(n, nMax, 8); 
        uint percent = nMax/100;

        if(n > (percent * 50) && dice >= 6){
            return suedoRand(nMax, dice , (percent * 20));
        }

        return n;
    }


    //Get time inside a window.
    function timeInside(uint _windowDays, uint _start) internal view returns(uint){  
        uint _el = elapsed(_start);
        uint _window = (86400 * _windowDays);
        uint _WithoutToday = (_el / _window) * _window;
        uint result = _el - _WithoutToday;

    return result;
    }


