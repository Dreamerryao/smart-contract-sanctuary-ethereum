pragma solidity ^0.8.7;

//Author: @kyrers
contract Deployer {
    bytes contractBytecode = "";
    
    function deploy(bytes32 salt) public {
        bytes memory bytecode = contractBytecode;
        address addr;

        assembly {
            addr := create2(0, add(bytecode, 0x20), mload(bytecode), salt)
        }
    }
}