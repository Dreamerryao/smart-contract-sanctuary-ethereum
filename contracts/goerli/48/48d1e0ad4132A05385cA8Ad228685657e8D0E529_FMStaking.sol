pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./timelib.sol";
import "hardhat/console.sol";

contract FMStaking {
    address owner;
    IERC20 FMToken;
    uint256 public currentStakeID;
    uint256 public totalStakedFMTokenAmount;
    uint256 public stakeTerm;
    uint256 public percentageReturn;


    struct stake {
        bool settled;
        address stakeOwner;
        uint256 stakeID;
        uint256 stakeAmount;
        uint256 startOfTerm;
        uint256 endOfTerm;
        uint256 settlementAmount;
        uint256 stakeReturns;
    }

    mapping(uint256 => stake) public stakeByID;
    mapping(address => uint256[]) public stakeByOwnerAddress;

    event STAKEADDED (uint256 stakeID, uint256 stakeAmount, address stakeOwner);
    event STAKECLAIMED (uint256 stakeID,uint256 settlementAmount,uint256 stakeReturns,address stakeOwner);

    constructor(IERC20 _token) {
        FMToken = _token;
        owner = msg.sender;
    }

    modifier onlyOwner() {
        require(msg.sender ==owner,"FmStaking; Not an owner");
        _;
    }

    function addTerm(uint256 _term) external onlyOwner {
        stakeTerm = _term;
    }

    function addStakePercentage(uint256 _percentage) external onlyOwner {
        percentageReturn = _percentage;
    }

    function addStake(uint256 amount) external {
        require(amount <= FMToken.balanceOf(msg.sender),"FMStaking: Buy more FMTokens");
        currentStakeID += 1;
        bool success = FMToken.transferFrom(msg.sender,address(this),amount);
        require(success == true,"FMStaking:Transfer was not successful");
        stakeByID[currentStakeID].stakeOwner = msg.sender;
        stakeByID[currentStakeID].stakeID = currentStakeID;
        stakeByID[currentStakeID].stakeAmount = amount;
        stakeByID[currentStakeID].startOfTerm = block.timestamp;
        uint256 end = BokkyPooBahsDateTimeLibrary.addSeconds(stakeByID[currentStakeID].startOfTerm,stakeTerm);
        stakeByID[currentStakeID].endOfTerm = end;
        totalStakedFMTokenAmount += amount;
        emit STAKEADDED(currentStakeID,amount,stakeByID[currentStakeID].stakeOwner);
    }

    function claimMyStake(uint256 stakeID) external {
        require(stakeByID[stakeID].stakeOwner == msg.sender,"FMStaking: Not stakeOwner");
        require(stakeByID[stakeID].settled == false,"FMStaking: Already settled");
        require(stakeByID[stakeID].endOfTerm < block.timestamp,"FMStaking:Can't claim now");
        stakeByID[stakeID].stakeReturns = (stakeByID[stakeID].stakeAmount * percentageReturn) / 100;
        console.log(stakeByID[stakeID].stakeReturns);
        stakeByID[stakeID].settlementAmount = stakeByID[stakeID].stakeAmount + stakeByID[stakeID].stakeReturns;
        console.log(stakeByID[stakeID].settlementAmount);
        bool success = FMToken.transfer(msg.sender,stakeByID[stakeID].settlementAmount);
        require(success == true,"FMStaking:Transfer was not successful");
        totalStakedFMTokenAmount -= stakeByID[stakeID].stakeAmount;
        stakeByID[stakeID].settled = true;
        emit STAKECLAIMED(stakeID,stakeByID[stakeID].settlementAmount,stakeByID[stakeID].stakeReturns,stakeByID[stakeID].stakeOwner);
    }

    
}

