pragma solidity ^0.8.4;
import "../lib/contracts/contracts/tunnel/FxBaseRootTunnel.sol";
import "../lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "../lib/solmate/src/tokens/ERC721.sol";
import "./RocketShibe.sol";

contract Wormhole is FxBaseRootTunnel, Ownable {
    RocketShibe public rocketShibe;
    bool public paused = true;

    mapping(address => uint256) public stakeInfo;

    constructor(
        address checkpointManager,
        address fxRoot,
        address _rocketShibe
    ) FxBaseRootTunnel(checkpointManager, fxRoot) {
        rocketShibe = RocketShibe(_rocketShibe);
    }

    function pauseState(bool state) public onlyOwner {
        paused = state;
    }

    // Custom yields for now to test tunnel
    function stakeRocket(uint256 tokenId) external {
        require(paused == false, "paused");

        rocketShibe.transferFrom(msg.sender, address(this), tokenId);

        uint256 yield = rocketShibe.getYield(tokenId) != 0
            ? rocketShibe.getYield(tokenId)
            : 0;

        _sendMessageToChild(abi.encode(msg.sender, tokenId, yield, true));
    }

    function unstakeRocket(uint256 tokenId) external {
        require(paused == false, "paused");

        rocketShibe.transferFrom(address(this), msg.sender, tokenId);

        // pack these bytes?
        _sendMessageToChild(abi.encode(msg.sender, tokenId, 0, false));
    }

    function _processMessageFromChild(bytes memory message) internal override {
        // only sending messages one way
    }
}

