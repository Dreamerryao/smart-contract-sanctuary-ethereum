import "openzeppelin-contracts/contracts/access/Ownable.sol";
import "forge-std/console.sol";

interface ITokenLauncher {
    function afterERC20TokenTransfer(address from, address to, uint256 amount) external;
}

contract CustomERC20 is ERC20, Ownable {
    address public tokenLauncherAddress;
    ITokenLauncher public immutable tokenLauncher;

    constructor(string memory name, string memory symbol) ERC20(name, symbol) {
        tokenLauncherAddress = msg.sender;
        console.log(tokenLauncherAddress);
        tokenLauncher = ITokenLauncher(msg.sender);
    }

    function mint(address account, uint256 amount) public onlyOwner {
        _mint(account, amount);
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        tokenLauncher.afterERC20TokenTransfer(from, to, amount);
    }
}

