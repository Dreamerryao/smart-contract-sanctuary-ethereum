import './ERC20.sol';
import "forge-std/console.sol";

interface IWETH9 {
    function deposit() external payable;
    function withdraw(uint256 wad) external;
}

contract TokenLauncher is IERC721Receiver, Ownable {
    using ECDSA for bytes32;

    uint24 public constant poolFee = 10000;
    address public constant wethAddr = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
    uint256 public constant tokenMaxSupply = 1000000000000000000000000;
    uint256 public constant poolSupply = 500000000000000000000000;
    uint256 public constant subjectAllocation = 10000000000000000000000;
    uint256 public constant minimumDonation = 1000000000000;
    uint256 public constant tokenSupplyAllocatedForClaiming = tokenMaxSupply - poolSupply - subjectAllocation;

    // Address for factory obtained from documentation
    address public constant factoryAddr = 0x1F98431c8aD98523631AE4a59f267346ea31F984;
    INonfungiblePositionManager public immutable nonfungiblePositionManager = INonfungiblePositionManager(0xC36442b4a4522E871399CD717aBDD847Ab11FE88);

    event DonationReceived(string twitterId, address indexed donor, uint256 value);

    event LaunchEventEnded(
        string twitterId,
        address subject,
        uint256 totalDonations
    );

    event PoolPositionCreated(
        string twitterId,
        address subject,
        uint256 tokenId,
        uint256 amount0,
        uint256 amount1
    );

    struct Launch {
        address token;
        uint256 totalDonations;
        mapping(address => uint256) donations; // track donations per address
        mapping(address => bool) claimed; // track donations per address
        uint256 startTime;
        uint256 endTime;
        address subject;
        bool isPoolCreated;
        uint256 claimedTokens;
        address poolAddress;
        uint256 positionTokenId;
    }

    struct Deposit {
        address owner;
        uint128 liquidity;
        address token0;
        address token1;
    }

    event Transfer(address indexed from, address indexed to, address indexed tokenAddress, uint256 value);

    event TokenInitialized(string twitterId, address subjectAddress, address tokenAddress);

    mapping(uint256 => Deposit) public deposits;

    mapping(string => Launch) public launches;

    mapping(address => bool) public launchedTokenAddresses;

    /* We are having the first donator/contributor initialize the
       token and launch
    */
    function donateAndInitialize(string memory twitterId, string memory name, string memory symbol, address subject, uint256 endTime, bytes calldata signature) public payable {
        require(msg.value > minimumDonation, "Donation must be greater than minumum donation");
        bytes32 hash = keccak256(abi.encodePacked(twitterId, name, symbol, subject, endTime, owner()));
        bytes32 messageHash = hash.toEthSignedMessageHash();
        address signer = messageHash.recover(signature);

        // require(signer == owner(), "Not signed by owner.");

        require(endTime > block.timestamp, "End time must be in the future");

        Launch storage launch = launches[twitterId];
        // There are different way to check whether a mapping value is set. This is just one way.
        require(!(launch.endTime > 0), "Launch already exists for this id");

        // Initializing token
        CustomERC20 token = new CustomERC20(name, symbol);
        emit TokenInitialized(twitterId, subject, address(token));
        launchedTokenAddresses[address(token)] = true;
        // Setting launch data
        launch.token = address(token);
        launch.startTime = block.timestamp;
        launch.endTime = endTime;
        launch.subject = subject;
        launch.isPoolCreated = false;

        // Handle donation once token is initialized
        launch.donations[msg.sender] += msg.value; // track donations per address
        launch.totalDonations += msg.value;
        emit DonationReceived(twitterId, msg.sender, msg.value);
    }

    function donate(string memory twitterId, address donor) public payable {
        require(msg.value > minimumDonation, "Donation must be greater than minumum donation");
        Launch storage launch = launches[twitterId];
        require(launch.endTime != 0, "Launch does not exist");
        require(block.timestamp < launch.endTime, "Event has ended");

        launch.donations[donor] += msg.value; // track donations per address
        launch.totalDonations += msg.value;
        // associate donation with donor address
        // for simplicity, we just emit an event here
        emit DonationReceived(twitterId, donor, msg.value);
    }

    function expectedClaimAmount(string memory twitterId, address donor) public view returns (uint) {
        Launch storage launch = launches[twitterId];
        return (launch.donations[donor]* tokenSupplyAllocatedForClaiming) / launch.totalDonations;
    }

    function claim(string memory twitterId, address recipient) public {
        Launch storage launch = launches[twitterId];
        require(block.timestamp >= launch.endTime, "Event has not ended");
        require(launch.donations[recipient] > 0, "No donation found for the recipient");

        // ensure that the recipient can only claim once
        require(!launch.claimed[recipient], "Donor has already claimed tokens");

        // Perform pool initialization and subject allocation
        if (!launch.isPoolCreated) {
            endLaunchEvent(twitterId);
            mintSubjectAllocation(twitterId);
        }

        require(launch.claimedTokens <= tokenSupplyAllocatedForClaiming - subjectAllocation, "All tokens claimed");

        uint256 amount = expectedClaimAmount(twitterId, recipient);
        launch.claimedTokens += amount;
        CustomERC20 token = CustomERC20(launch.token);
        token.mint(recipient, amount);

        // mark recipient as claimed
        launch.claimed[recipient] = true;
    }

    function endLaunchEvent(string memory _twitterId) internal {
        Launch storage launch = launches[_twitterId];

        launch.isPoolCreated = true;

        address poolAddress = createUniswapV3Pool(_twitterId);
        console.log("Pooladdress: ", poolAddress);
        launch.poolAddress = poolAddress;
        createPoolPosition(_twitterId);

        emit LaunchEventEnded(_twitterId, launch.subject, launch.totalDonations);
    }

    // This is the same as claiming the tokens from the subject
    function mintSubjectAllocation(string memory twitterId) internal {
        Launch storage launch = launches[twitterId];

        launch.claimedTokens += subjectAllocation;
        // Assumes that 1 million tokens will be minted
        CustomERC20 token = CustomERC20(launch.token);
        token.mint(launch.subject, subjectAllocation);
    }

    function createUniswapV3Pool(string memory twitterId) internal returns (address){
        Launch storage launch = launches[twitterId];

        address token0 = wethAddr;
        address token1 = launch.token;

        address poolAddress = IUniswapV3Factory(factoryAddr).createPool(token0, token1, poolFee);
        /*
            sqrtPriceX96 = sqrt(price) * 2^96
            price = y/x (e.g 500,000 / 10 = 50,0000 or 500,000/100 = 5,000)
        */
        uint256 price = FixedPointMathLib.divWadDown((launch.totalDonations * 9800 / 10000), poolSupply);
        console.log("Calculated price: ", price);

        uint160 sqrtPriceX96 = uint160(FixedPointMathLib.sqrt(price) * (2**96) / FixedPointMathLib.sqrt(10**18));
        console.log("Calculated sqrtPriceX96: ", sqrtPriceX96);

        IUniswapV3Pool(poolAddress).initialize(sqrtPriceX96);
        return poolAddress;
    }

    function _createDeposit(address owner, uint256 tokenId) internal {
        (, , address token0, address token1, , , , uint128 liquidity, , , , ) =
            nonfungiblePositionManager.positions(tokenId);

        // set the owner and data for position
        deposits[tokenId] = Deposit({owner: owner, liquidity: liquidity, token0: token0, token1: token1});
    }

    function createPoolPosition(string memory twitterId) internal {
        Launch storage launch = launches[twitterId];

        // uint256 amount0ToMint = poolSupply;
        CustomERC20 token = CustomERC20(launch.token);
        token.mint(address(this), poolSupply);
        // 98% being sent to pool. performing this using basis points
        uint256 amount1ToMint = launch.totalDonations * 9800 / 10000;

        IWETH9 weth = IWETH9(wethAddr);
        weth.deposit{value: amount1ToMint}();

        // Approve the position manager
        TransferHelper.safeApprove(launch.token, address(nonfungiblePositionManager), poolSupply);
        TransferHelper.safeApprove(wethAddr, address(nonfungiblePositionManager), amount1ToMint);

        int24 tickSpacing = IUniswapV3Pool(launch.poolAddress).tickSpacing();

        int24 nearestMinTick = nearestUsableTick(TickMath.MIN_TICK, tickSpacing);
        // console.log("Nearest Usable Tick Min: ");
        // console.logInt(nearestMinTick);
        int24 nearestMaxTick = nearestUsableTick(TickMath.MAX_TICK, tickSpacing);
        // console.log("Nearest Usable Tick Max: ");
        // console.logInt(nearestMaxTick);

        PoolAddress.PoolKey memory poolKey = PoolAddress.getPoolKey(launch.token, wethAddr, poolFee);

        // Note: we do not care about slippage since we can assume that we are the first position creators
        INonfungiblePositionManager.MintParams memory params =
            INonfungiblePositionManager.MintParams({
                token0: poolKey.token0,
                token1: poolKey.token1,
                fee: poolKey.fee,
                tickLower: nearestMinTick,
                tickUpper: nearestMaxTick,
                amount0Desired: poolSupply,
                amount1Desired: amount1ToMint,
                amount0Min: 0,
                amount1Min: 0,
                recipient: address(this),
                deadline: block.timestamp
            });

        uint256 tokenId;
        uint128 liquidity;
        uint256 amount0;
        uint256 amount1;
        // Note that the pool must already exist
        (tokenId, liquidity, amount0, amount1) = nonfungiblePositionManager.mint(params);
        // console.log("Position minted successfully: (tokenId)", tokenId);
        console.log("Total Donations: ", (launch.totalDonations / 1 ether));
        console.log("Following uints are in WEI");
        console.log("Position Liquidity: ", liquidity);
        console.log("Amount 0(Amount of Launched Token Used in Pool): ", amount0);
        console.log("Amount 1(Amount of WETH used in Pool): ", amount1);
        launch.positionTokenId = tokenId;

        // Create a deposit
        _createDeposit(address(this), tokenId);

        // Log the pool position creation
        emit PoolPositionCreated(twitterId, launch.subject, tokenId, amount0, amount1);
    }

    function claimPoolFees(string memory twitterId) public {
        Launch storage launch = launches[twitterId];
        require(block.timestamp >= launch.endTime, "Event has not ended");
        require(launch.isPoolCreated, "Liquidity Pool must exist");
        require(msg.sender == launch.subject, "Fees are only claimable by subject");

        // set amount0Max and amount1Max to uint256.max to collect all fees
        // alternatively can set recipient to msg.sender and avoid another transaction in `sendToOwner`
        INonfungiblePositionManager.CollectParams memory params =
            INonfungiblePositionManager.CollectParams({
                tokenId: launch.positionTokenId,
                recipient: address(this),
                amount0Max: 0,
                amount1Max: type(uint128).max
            });

        (uint amount0, uint amount1 )= nonfungiblePositionManager.collect(params);
        console.log("Amount 0 Collected: ", amount0);
        console.log("Amount 1 Collected: ", amount1);
        if (amount1 > 0) {
            _sendFeesToSubject(launch.subject, amount1);
        }
    }

    function _sendFeesToSubject(
        address subject,
        uint amount
    ) internal {
        IWETH9 weth = IWETH9(wethAddr);
        weth.withdraw(amount);
        // send half of fees to subject
        payable(subject).transfer(amount/2);
        // address token1 = deposits[tokenId].token1;
        // TransferHelper.safeTransfer(token1, subject, amount / 2);
    }

    // Implementing `onERC721Received` so this contract can receive custody of erc721 tokens
    function onERC721Received(
        address operator,
        address,
        uint256 tokenId,
        bytes calldata
    ) external override returns (bytes4) {
        // get position information

        _createDeposit(operator, tokenId);

        return this.onERC721Received.selector;
    }

    function divRound(int128 x, int128 y)
        internal
        pure
        returns (int128 result)
    {
        int128 quot = ABDKMath64x64.div(x, y);
        result = quot >> 64;

        // Check if remainder is greater than 0.5
        if (quot % 2**64 >= 0x8000000000000000) {
            result += 1;
        }
    }

    function nearestUsableTick(int24 tick_, int24 tickSpacing)
        internal
        pure
        returns (int24 result)
    {
        result =
            int24(divRound(int128(tick_), int128(tickSpacing))) *
            int24(tickSpacing);

        if (result < TickMath.MIN_TICK) {
            result += int24(tickSpacing);
        } else if (result > TickMath.MAX_TICK) {
            result -= int24(tickSpacing);
        }
    }

    function withdrawEther(address payable to, uint256 amount) public onlyOwner {
        to.transfer(amount);
    }

    receive() external payable {}

    function afterERC20TokenTransfer(
        address from,
        address to,
        uint256 amount
    ) public {
        require(launchedTokenAddresses[msg.sender], "Only callable by ERC20 token created by this contract.");
        emit Transfer(from, to, msg.sender, amount);
    }

}

