pragma solidity =0.8.10;

// import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC20Metadata.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC1155.sol";
import "./interface/IPriceOracle.sol";
import "./TransferHelpers.sol";
import "hardhat/console.sol";
import "./common.sol";

contract Offer is ERC721Holder, ERC1155Holder, TransferHelpers {
    using SafeERC20 for IERC20;

    // the status of the offer or counter offers.
    enum OfferStatus {
        UNDEFINED,
        ACTIVE,
        LOAN,
        CLOSED
    }

    // emit when a new counter offer is created.
    struct CounterOfferInfo {
        Token[] tokens;
        GeneralInfo general;
        address creator;
        uint256 createTimestamp;
        uint256 lastEditTimestamp;
        OfferStatus status;
    }

    // original offer creator.
    address public borrower;

    // the EOA who provide the loan token.
    address public lender;

    // offer factory, the only entrypoint for creating a new offer.
    address public factory;

    // the all in one price provider.
    IPriceOracle public priceOracle;

    // the id of the accepted counteroffer, 0 by default.
    uint8 public acceptedId;

    // the latest counter offer id, 0 by default.
    uint8 public latestId;

    // initialize function flag, can be only set once.
    bool public alreadyInit;

    // the timestamp when the interest calculation begins and ends.
    uint256 public interestStartTime;
    uint256 public interestEndTime;

    // the flag to indicate if this offer can be claim be the lender.
    bool private _canBeSeized;

    // the flag to prevent the lender from claiming collaterals twice.
    bool private _alreadySeized;

    // the first offer(id = 0) is the origin offer created by the borrower
    // otherwise would be the counter offers created by the lender
    mapping(uint8 => CounterOfferInfo) private _counterOfferList;

    uint256 public constant MAXALLOWED = 10000;
    uint256 public constant SECOND_PER_YEAR = 31536000;
    uint256 public constant DECIMAL_18 = 18;

    /*//////////////////////////////////////////////////////////////
                                 EVENTS
    //////////////////////////////////////////////////////////////*/

    event CreateCounterOffer(
        uint8 _offerId,
        Token[] _tokens,
        GeneralInfo _general,
        address _borrower,
        address _creator,
        address _lastCaller,
        uint256 _createTimestamp,
        uint256 _lastEditTimestamp,
        uint8 _status
    );

    event EditCounterOffer(
        uint8 _offerId,
        Token[] _tokens,
        GeneralInfo _general,
        address _caller
    );

    event AcceptCounterOffer(uint8 _offerId, address _lender);

    event StatusChanged(uint8 _offerId, address _caller, uint8 _status);

    modifier onlyBorrower() {
        require(msg.sender == borrower, "Bisaar: only borrower!");
        _;
    }

    modifier onlyNotBorrower() {
        require(msg.sender != borrower, "Bisaar: only lender!");
        _;
    }

    modifier onlyValidInfo(GeneralInfo calldata _general) {
        require(_general.ltv < MAXALLOWED, "Bisaar: invalid interest ltv!");
        require(
            _general.interestRate < MAXALLOWED,
            "Bisaar: invalid interest rate!"
        );
        require(
            _general.collateralThreshold < MAXALLOWED,
            "Bisaar: invalid collateralThreshold!"
        );
        require(
            _general.expireTimestamp > block.timestamp,
            "Bisaar: expireTimestamp must gt block.timestamp!"
        );
        require(
            _general.repaymentTimestamp > _general.expireTimestamp,
            "Bisaar: repaymentTimestamp must gt expireTimestamp!"
        );
        _;
    }

    receive() external payable {}

    /**
    @notice used for clone, set the borrower and factory 
    @param _borrower offer creator
    @param _factory offer factory
    @param _priceOracle price oracle
    */
    function initialize(
        address _borrower,
        address _factory,
        IPriceOracle _priceOracle
    ) external {
        require(!alreadyInit, "Bisaar: offer already initialized!");
        borrower = _borrower;
        factory = _factory;
        priceOracle = _priceOracle;
        alreadyInit = true;
    }

    /**
    @notice create offer can only be called by offer factory
    * Status changed from UNDEFINED to ACTIVE
    @param _tokens tokens that user selected
    @param _general general info
    */
    function createOffer(
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) external {
        require(msg.sender == factory, "Bisaar: only factory allowed!");
        require(
            _getStatus(0) == OfferStatus.UNDEFINED,
            "Bisaar: offer already created!"
        );

        _createCounterOffer(borrower, _tokens, _general);
    }

    /**
    @notice _tokens the entire token infos, include the original offer info
    * Status changed from ACTIVE to ACTIVE (unchanged)
    @param _general the entire general info
    */
    function createCounterOffer(
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) external onlyNotBorrower {
        CounterOfferInfo memory offer = _counterOfferList[acceptedId];
        require(
            offer.status == OfferStatus.ACTIVE,
            "Bisaar: offer not active!"
        );

        // actually if the tokens in not consistent with the original offer, this will be an invalid offer
        // can't be avoid this from contract side properly, will set limitation from the Frontend side
        require(
            _tokens.length == offer.tokens.length,
            "Bisaar: invalid tokens length"
        );

        latestId++;
        _createCounterOffer(msg.sender, _tokens, _general);
    }

    /**
    @notice can be only called by the offer creator, used for both offer and counter offer
    * Status changed from ACTIVE to ACTIVE (unchanged)
    @param _offerId counter offer id, 0: edit offer 1: counter offer
    @param _tokens tokens that prices has been changed
    @param _general general info that has been changed
     */
    function editCounterOffer(
        uint8 _offerId,
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) external {
        _editCounterOffer(_offerId, _tokens, _general);
    }

    /**
    @notice this called by the borrower when the orginal offer is expired and whants to extend it.
    @param _tokens token info
    @param _general general info
    */
    function reCreateOffer(
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) external {
        //1. make sure it's expired
        CounterOfferInfo memory offer = _counterOfferList[0];

        require(
            block.timestamp > offer.general.expireTimestamp,
            "Bisaar: not expired!"
        );

        //2. checking parameters
        _editCounterOffer(0, _tokens, _general);
    }

    /**
    @notice can be only called by the offer owner, used for both offer and counter offer
    * Status changed from ACTIVE to CLOSED
    * send all collateral tokens back to the owner
    @param _offerId counter offer id
    */
    function removeCounterOffer(uint8 _offerId) external {
        if (_offerId == 0) {
            // if its the original offer
            require(
                _getStatus(acceptedId) == OfferStatus.ACTIVE,
                "Bisaar: forbidden to remove when offer is not active!"
            );
        } else {
            // if its the counter offer
            require(
                _getStatus(_offerId) == OfferStatus.ACTIVE,
                "Bisaar: offer not active!"
            );
        }

        // update status
        _counterOfferList[_offerId].status = OfferStatus.CLOSED;

        CounterOfferInfo memory offer = _counterOfferList[_offerId];
        require(offer.creator == msg.sender, "Bisaar: permission denied!");

        if (_offerId == 0) {
            // send collateral tokens back to borrower
            _batchTransferOut(address(this), offer.creator, offer.tokens);
        } else {
            // if this is a counter offer, send owned loan tokens back to the lender
            IERC20(offer.general.loanToken).safeTransfer(
                offer.creator,
                offer.general.loanAmount
            );
        }
        emit StatusChanged(_offerId, msg.sender, uint8(offer.status));
    }

    /** 
    @notice called by Lender, need the lender to approve loan token first
    * Status changed from ACTIVE to LOAN
    *  transfer loan token to offer contract
    *  send all collateral tokens to Lender
    */
    function acceptOffer() external onlyNotBorrower {
        _acceptCounterOffer(0, msg.sender);
    }

    /**
    @notice called by borrower, need the lender approve and transfer loan token first
    * Status changed from ACTIVE to LOAN
    @param _offerId accepted counter id
    */
    function acceptCounterOffer(uint8 _offerId) external onlyBorrower {
        require(_offerId != 0, "Bisaar: not allowed!");
        _acceptCounterOffer(_offerId, address(this));
    }

    /**
    @notice called by borrower to repay the loan token, 
    * Status changed from LOAN to CLOSED
    * the collateral tokens will sent back automatically
    */
    function repayLoan() external onlyBorrower {
        CounterOfferInfo storage offer = _counterOfferList[acceptedId];
        require(
            offer.status == OfferStatus.LOAN,
            "Bisaar: not available to repay!"
        );

        require(
            block.timestamp < offer.general.repaymentTimestamp,
            "Bisaar: exceed repayment date!"
        );
        interestEndTime = block.timestamp;

        //1. calculate the interest
        uint256 interestAmt = Offer.getInterestAmt();

        //2. send collateral tokens back to the borrower
        _batchTransferOut(address(this), borrower, offer.tokens);

        //3. send loan tokens and interest to the lender
        IERC20(offer.general.loanToken).safeTransferFrom(
            borrower,
            lender,
            offer.general.loanAmount + interestAmt
        );

        //4. change status from LOAN to CLOSED
        offer.status = OfferStatus.CLOSED;
        emit StatusChanged(acceptedId, msg.sender, uint8(offer.status));
    }

    /**
    @notice could be called by anyone, will set the status of the accepted offer to LIQUIDATED
    */
    function liquidateLoan() external {
        require(isLiquidatable(), "Bisaar: not available to liquidate!");
        interestEndTime = block.timestamp;

        CounterOfferInfo storage offer = _counterOfferList[acceptedId];
        offer.status = OfferStatus.CLOSED;
        _canBeSeized = true;
        emit StatusChanged(acceptedId, msg.sender, uint8(offer.status));
    }

    /**
    @notice avaliable for the lender to called by the followoing conditions:
    * has been liquidated by bot service
    * loan not reapid and the expire date was hit
    */
    function seizeCollateralTokens() external {
        CounterOfferInfo memory offer = _counterOfferList[acceptedId];

        //1. only the lender can make this call
        require(lender == msg.sender, "Bisaar: permission denied!");
        require(isSeizeable(), "Bisaar: not avaliable to seize");

        //2. send all collateral tokens to lender
        _batchTransferOut(address(this), lender, offer.tokens);

        _alreadySeized = true;

        emit StatusChanged(acceptedId, msg.sender, uint8(offer.status));
    }

    /**
    @notice get counterOffer details by a specific id
    @param _offerId offer id
    */
    function getCounterOfferInfoDetails(uint8 _offerId)
        external
        view
        returns (CounterOfferInfo memory _info)
    {
        return _counterOfferList[_offerId];
    }

    function isSeizeable() public view returns (bool) {
        return _canBeSeized && !_alreadySeized;
    }

    /**
    @notice get estimated amounts of loan interest
     */
    function getInterestAmt() public view returns (uint256 interestAmt) {
        CounterOfferInfo storage offer = _counterOfferList[acceptedId];
        if (interestStartTime == 0) {
            return 0;
        }

        // calculate seconds has passed
        uint256 finalTime = interestEndTime == 0
            ? block.timestamp
            : interestEndTime;

        uint256 secondsPassed = finalTime - interestStartTime;

        // loanAmt = x, interestRate = 10% apy
        // secondPerYear = 31536000 (365 * 24 * 3600)
        // interestAmt = (interestRate * x * secondsPassed) / secondPerYear

        console.log("block.timestamp:", block.timestamp);
        console.log("interestStartTime:", interestStartTime);
        console.log("secondsPassed:", secondsPassed);
        console.log("offer.general.interestRate:", offer.general.interestRate);
        console.log("offer.general.loanAmount:", offer.general.loanAmount);

        interestAmt =
            (((offer.general.interestRate * offer.general.loanAmount) / 10000) *
                secondsPassed) /
            SECOND_PER_YEAR; // 1000 represents for 10%
    }

    /**
    @notice called by bot to check if this offer could be liquidated
    @return true: can be liquidated, false: no need to call liquidateLoan
     */
    function isLiquidatable() public view returns (bool) {
        // 1. check timestamp to the expire date
        CounterOfferInfo memory acceptedOffer = _counterOfferList[acceptedId];

        // only offers that have been accepted and expired can be liquidated.
        if (acceptedOffer.status != OfferStatus.LOAN) {
            console.log("Offer.isLiquidatable: still ACTIVE!");
            return false;
        }

        if (block.timestamp > acceptedOffer.general.repaymentTimestamp) {
            return true;
        }

        console.log("Offer.isLiquidatable: not expired!");
        return _isBorrowTooMuch(acceptedId);
    }

    /**
    @notice
    @param _creator borrower or lender that create this offer
    @param _tokens  the entire token infos, include the original offer info
    @param _general the entire general info
    */
    function _createCounterOffer(
        address _creator,
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) internal onlyValidInfo(_general) {
        // prepare a new counter offer
        CounterOfferInfo storage newCounterOffer = _counterOfferList[latestId];
        _makeOfferInternal(newCounterOffer, _tokens, _general, _creator);

        // make sure the value of the collateral tokens worth the value of the loan tokens
        require(
            !_isBorrowTooMuch(latestId),
            "Bisaar: borrow too much loan tokens!"
        );

        if (0 != latestId) {
            // latestId != 0 means this is a counter offer, need to deposit loan token by force(required)
            IERC20(_general.loanToken).safeTransferFrom(
                _creator,
                address(this),
                _general.loanAmount
            );
        }

        emit CreateCounterOffer(
            latestId,
            _tokens,
            _general,
            borrower,
            _creator,
            _creator,
            newCounterOffer.createTimestamp,
            newCounterOffer.lastEditTimestamp,
            uint8(newCounterOffer.status)
        );
    }

    /**
    @notice called by Borrower, need the lender approve and transfer loan token first
    @param _offerId accepted counter id
    */
    function _acceptCounterOffer(uint8 _offerId, address _loanTokenProvider)
        internal
    {
        require(
            _getStatus(_offerId) == OfferStatus.ACTIVE &&
                _getStatus(acceptedId) == OfferStatus.ACTIVE,
            "Bisaar: not active!"
        );

        acceptedId = _offerId;
        CounterOfferInfo storage accepted = _counterOfferList[acceptedId];

        require(
            accepted.general.expireTimestamp > block.timestamp,
            "Bisaar: offer expired!"
        );

        // make sure the value of the collateral tokens worth the value of the loan tokens
        require(
            !_isBorrowTooMuch(acceptedId),
            "Bisaar: borrow too much loan tokens!"
        );

        // called by acceptCounterOffer(), this branch means token already transfered into address(this).
        if (_loanTokenProvider == address(this)) {
            IERC20(accepted.general.loanToken).safeTransfer(
                borrower,
                accepted.general.loanAmount
            );
            lender = accepted.creator;
        } else {
            // called by acceptOffer(), transfer from lender to borrower directly.
            IERC20(accepted.general.loanToken).safeTransferFrom(
                _loanTokenProvider,
                borrower,
                accepted.general.loanAmount
            );
            lender = _loanTokenProvider; // msg.sender
        }

        accepted.status = OfferStatus.LOAN;
        interestStartTime = block.timestamp;

        emit AcceptCounterOffer(acceptedId, lender);
        emit StatusChanged(acceptedId, msg.sender, uint8(accepted.status));
    }

    /**
    @notice can be only called by the offer creator, used for both offer and counter offer
    * 1. if not accepted(acceptId = 0), every one can edit owned counteroffer
    * 2. if nobody accepted this, and the expired date was hit, the owner can change this to active(extend it)
    * Status changed from ACTIVE to ACTIVE (unchanged)
    @param _offerId counter offer id, 0: edit offer 1: counter offer
    @param _tokens tokens that prices has been changed
    @param _general general info that has been changed
     */
    function _editCounterOffer(
        uint8 _offerId,
        Token[] calldata _tokens,
        GeneralInfo calldata _general
    ) internal onlyValidInfo(_general) {
        require(
            _getStatus(acceptedId) == OfferStatus.ACTIVE,
            "Bisaar: offer not active!"
        );
        CounterOfferInfo storage counterOffer = _counterOfferList[_offerId];
        require(
            counterOffer.creator == msg.sender,
            "Bisaar: permission denied!"
        );

        /* update those NFTs or general tokens' prices without oracle */
        for (uint8 i = 0; i < counterOffer.tokens.length; i++) {
            for (uint8 j = 0; j < _tokens.length; j++) {
                if (counterOffer.tokens[i].tokenAddr == _tokens[j].tokenAddr) {
                    counterOffer.tokens[i].tokenPrice = _tokens[j].tokenPrice;
                }
            }
        }

        /* loan amount, interest, collateral threshold, duration */
        counterOffer.general.loanAmount = _general.loanAmount;
        counterOffer.general.interestRate = _general.interestRate;
        counterOffer.general.collateralThreshold = _general.collateralThreshold;
        counterOffer.general.repaymentTimestamp = _general.repaymentTimestamp;
        counterOffer.general.expireTimestamp = _general.expireTimestamp;

        counterOffer.lastEditTimestamp = block.timestamp;

        // make sure the value of the collateral tokens worth the value of the loan tokens
        require(
            !_isBorrowTooMuch(latestId),
            "Bisaar: borrow too much loan tokens!"
        );

        emit EditCounterOffer(
            _offerId,
            _tokens,
            _general,
            counterOffer.creator
        );
    }

    function _makeOfferInternal(
        CounterOfferInfo storage newOffer,
        Token[] calldata _tokens,
        GeneralInfo calldata _general,
        address _provider
    ) internal {
        for (uint8 i = 0; i < _tokens.length; i++) {
            newOffer.tokens.push(
                Token(
                    _tokens[i].tokenType,
                    _tokens[i].tokenAddr,
                    _tokens[i].tokenAmounts,
                    _tokens[i].tokenId,
                    _tokens[i].tokenPrice
                )
            );
        }

        newOffer.general = GeneralInfo(
            _general.loanToken,
            _general.ltv,
            _general.featuredFlag,
            _general.loanAmount,
            _general.interestRate,
            _general.collateralThreshold,
            _general.repaymentTimestamp,
            _general.expireTimestamp
        );

        newOffer.status = OfferStatus.ACTIVE;
        newOffer.creator = _provider;
        newOffer.createTimestamp = block.timestamp;
        newOffer.lastEditTimestamp = newOffer.createTimestamp;
    }

    function _isBorrowTooMuch(uint8 _offerId) internal view returns (bool) {
        CounterOfferInfo memory currentOffer = _counterOfferList[_offerId];
        uint256 sumValueCollateral;

        // 2. compare the value of collateral tokens to the value of loan Tokens
        for (uint8 i = 0; i < currentOffer.tokens.length; i++) {
            Token memory t = currentOffer.tokens[i];
            console.log("=====================================> i: ", i);
            console.log("\tt.tokenAmounts = ", t.tokenAmounts);
            console.log("\tt.tokenType = ", uint8(t.tokenType));
            console.log("\tt.tokenAddr = ", t.tokenAddr);

            if (t.tokenType == TokenType.NATIVE) {
                sumValueCollateral =
                    sumValueCollateral +
                    (t.tokenAmounts * 1 * 10**DECIMAL_18);
            } else if (t.tokenType == TokenType.ERC20) {
                uint8 decimals = IERC20Metadata(t.tokenAddr).decimals();
                if (decimals <= 18) {
                    // deal with decimals, make value comparison at the scale of 18
                    uint256 amt = t.tokenAmounts * 10**(DECIMAL_18 - decimals);

                    // get token price from oracle or from user input (in ETH)
                    (uint256 price, ) = IPriceOracle(priceOracle)
                        .getTokenEthPrice(t.tokenAddr); // in wei
                    if (price == 0) {
                        price = t.tokenPrice; // in wei
                        console.log("\tuser input price:", price);
                    }

                    // sum up
                    sumValueCollateral = sumValueCollateral + amt * price;
                    console.log(
                        "\terc20 current amt * price:",
                        amt * price,
                        "sumValueCollateral:",
                        sumValueCollateral
                    );
                }
                // we take tokens whese decimals' gt 18 as invalid tokens, will skip
            } else {
                console.log("type -> ERC1155 or ERC721");
                // for NFT721 or NFT1155
                sumValueCollateral =
                    sumValueCollateral +
                    (t.tokenAmounts * t.tokenPrice * 10**DECIMAL_18);

                console.log("\tt.tokenPrice:", t.tokenPrice);
                console.log(
                    "\tnft current amt * price:",
                    t.tokenAmounts * t.tokenPrice * 10**DECIMAL_18,
                    "sumValueCollateral:",
                    sumValueCollateral
                );
            }
        }

        console.log("************* get loan token info ******************");
        // do the comparation
        address lTokenAddr = currentOffer.general.loanToken;
        (uint256 _lTokenPriceInEth, ) = IPriceOracle(priceOracle)
            .getTokenEthPrice(lTokenAddr);

        uint8 lTokenDecimal = IERC20Metadata(lTokenAddr).decimals();
        uint256 lTokenAmt = currentOffer.general.loanAmount *
            10**(18 - lTokenDecimal);

        uint256 shrinkCollateralValue = (sumValueCollateral *
            currentOffer.general.collateralThreshold) / 10000; //8000 represents 80%

        console.log("\tlTokenPriceInEth:", _lTokenPriceInEth);
        console.log("\tlTokenDecimal:", lTokenDecimal);
        console.log("\tlTokenAmt:", lTokenAmt);
        console.log("\tshrinkCollateralValue:", shrinkCollateralValue);
        console.log(
            "\tlTokenAmt * _lTokenPriceInEth:",
            lTokenAmt * _lTokenPriceInEth
        );
        console.log(
            "\twill get liquidated? ",
            shrinkCollateralValue < lTokenAmt * _lTokenPriceInEth
        );

        return shrinkCollateralValue < lTokenAmt * _lTokenPriceInEth;
    }

    function _getStatus(uint8 _offerId)
        internal
        view
        returns (OfferStatus status)
    {
        return _counterOfferList[_offerId].status;
    }
}

