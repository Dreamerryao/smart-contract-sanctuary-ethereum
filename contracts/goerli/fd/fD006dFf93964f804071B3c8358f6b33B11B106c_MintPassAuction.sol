      // 1. First ensure that the current time in which this function is called is indeed PAST the Auction's 
      // official End Time:
      // require(block.timestamp >= auctionEndTime, "'endTheAuction()' --> Auction not yet ended!");
      
      // 2. Also make sure this function is being called WHILE the Auction IS STILL GOING ON. Cause if the Auction isn't
      //    still going - and has already ended, we shouldn't be allowed to call this function again:
      require(auctionIsRunning == true, "'endTheAuction()' has already been called!");
      console.log(" =>Got through the SECOND 'require', meaning 'auctionIsRunning' == is still TRUE");

      // If we got this far... we can indeed go on and END the Auction:
      auctionEndTime = block.timestamp;
      auctionIsRunning = false;
      auctionWasEnded = true;

      console.log(" =>Set the value of 'auctionIsRunning' to 'false'. Let's verify:\n >'auctionIsRunning' = %s", auctionIsRunning);

      emit AuctionEnded(auctionEndTime);
 
   }


  

