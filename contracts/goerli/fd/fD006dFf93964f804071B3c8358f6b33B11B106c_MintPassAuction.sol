   // function withdrawLosingBid(bytes32 leafedAddress, bytes32 rootHash, bytes32[] calldata theMerkleProof) public payable whenNotPaused returns(bool)
   function bid(bytes32 leafedAddress, bytes32 rootHash, bytes32[] calldata theMerkleProof) public payable whenNotPaused returns(bool) {
   // function bid() public whenNotPaused payable {
      console.log("\n>Smart-Contract 'bid()' --> New Bid has arrived!");
      console.log(">Amount of new Bid = %s", msg.value);
      console.log(">Current 'bidCount' = %s ", bidCount);

      // 1. Make sure the Auction is still running:
      require(auctionIsRunning == true, "bid() ERROR: Auction already Ended!");
      // require(block.timestamp <= auctionEndTime, "'bid()' --> Auction already Ended!");

      // 2. Make sure the Address attempting to be is on the Allowlist.
      //    Do this by checking the MerkleProof provided by the Bidder:
      console.log("\n\n ===>NOW gonna run the 'verify()' function' from OpenZeppelin's 'MerkleTree.sol' file...");
      // REMINDER (from "MerkleTree.sol"):
      // function verify(bytes32[] memory proof, bytes32 root, bytes32 leaf) internal pure returns (bool) { ... } 
      require(MerkleProof.verify(theMerkleProof, rootHash, leafedAddress), "'bid()' --> ERROR - BAD MERKLE PROOF!!!");
     
      // 3. Make sure the account attempting to Bid hasn't bid already:
      Bid memory potentialBid = AddressesAndBidsDictionary[msg.sender];
      require(potentialBid.bidAmount == 0, "bid() ERROR: THIS ACCOUNT HAS ALREADY BID!");

      // 3. Make sure the FUNDS sent are enough to cover the minimum Bid amount:
      require(msg.value >= minimumBidAmount, "bid() ERROR! This Bid is too small!");

      // 4. Create a new Bid Struct-Object:
      Bid memory newBid = Bid(bidCount, msg.sender, msg.value, block.timestamp); 
      AddressesAndBidsDictionary[msg.sender] = newBid;
      bidStructsArray.push(newBid);
      console.log("\n>PUSHED new bid into 'bidStructsArray'");

      bidCount += 1;
      console.log(">...just iterated my 'bidCount' variable, so the *NEXT* Bid's ID # will be: %s ", bidCount);
      
      // minimumBidAmount = msg.value + minBidIncrement;
      // console.log(">'minimumBidAmount's new value = %s ", minimumBidAmount);

      emit NewBidArrived(msg.sender, msg.value);
   }





   // Allows a Bidder to INCREASE their already-existing Bid by adding more ETH to it: 
   function bidUp() public payable whenNotPaused {
      console.log("\n>Smart-Contract 'bidUp()' - incoming bidIncrease Amount = %s", msg.value);
      
      // 1. Make sure the Auction is still running:
      require(auctionIsRunning == true, "bidUp() ERROR: Auction already Ended!");
      // require(block.timestamp <= auctionEndTime, "'bidUp()' --> Auction already Ended!");
      
      // 2. Is there already an existing Bid from this ETH-Address??? 
      //    Let's try retrieving it and see what's up:
      Bid memory existingBid = AddressesAndBidsDictionary[msg.sender];
      console.log(">'existingBid's 'bidAmount' value = %s ", existingBid.bidAmount);
      require(existingBid.bidAmount > 0, "bidUp() ERROR!  There's no Bid for this ETH Address!");

      // 3. Make sure the bid was raised by more than just 0.01 ETH...
      require(msg.value >= minBidIncrement, "bidUp() ERROR! Bid-increase too low");

      // 4. Add to it the new bid-increase-amount:
      existingBid.bidAmount += msg.value;

      // 5. And finally make sure this newly updated Bid is also updated in the 'AddressesAndBidsDictionary':
      AddressesAndBidsDictionary[msg.sender] = existingBid;

      // 6. Also update the Bid in the 'bidStructsArray':
      // uint existingBidIndex = existingBid.bidIndex;
      bidStructsArray[existingBid.bidIndex] = existingBid;

      // 6.5  LOG-OUT for verification:
      console.log(">After updating, 'updatedBid' 'bidAmount' is: %s ", existingBid.bidAmount);

      // 7. Announce the "BidIncrease" EVENT:
      emit BidIncreased(msg.sender, msg.value, block.timestamp);
   }





