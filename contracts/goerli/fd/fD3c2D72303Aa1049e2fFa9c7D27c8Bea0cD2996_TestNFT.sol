pragma solidity 0.8.9;

import "./Utils.sol";

/// @title UUID Conversion Library
/// @author Myna Swap, Inc. & 0x113f
/// @notice This contract should be deployed only once on the 'HOME' Blockchain
contract UUID {
    uint8[256] hexTable;
    uint8[256] shl4Table;
    uint8[8] positions = [0, 4, 9, 14, 19, 24, 28, 32];

    constructor() {
        uint8 i;
        
        while(i <= 255) {
            if(i >= 48 && i <= 57) hexTable[i] = i - 48;
            else if(i >= 97 && i <= 102) hexTable[i] = i - 87;
            else if(i >= 65 && i <= 70) hexTable[i] = i - 55;
            else if(i >= 65 && i <= 70) hexTable[i] = i - 55;
            else hexTable[i] = 255;
            shl4Table[i] = i << 4;

            if (i == 255) break;
            i++;
        }
    }
    
    // @notice Parses a UUID string into a bytes16 format
    // @param uuid The UUID string to parse
    // @return bytes16 The converted UUID to bytes16
    function toBytes16(string memory uuid) internal view returns (bytes16) {
        bytes memory uuidBytes = bytes(uuid);

        if(uuidBytes.length != 36) 
            revert InvalidUUIDLength(uuid);

        if (uuidBytes[8] != 0x2D || uuidBytes[13] != 0x2D || uuidBytes[18] != 0x2D || uuidBytes[23] != 0x2D)
             revert InvalidHypenPosition(uuid);

        bytes memory buffer = new bytes(16);
        uint8 j;

        unchecked {
            do {
                uint8 i = positions[j];
                uint8 h1 = hexTable[uint8(uuidBytes[i])];
                uint8 h2 = hexTable[uint8(uuidBytes[i+1])];
                uint8 h3 = hexTable[uint8(uuidBytes[i+2])];
                uint8 h4 = hexTable[uint8(uuidBytes[i+3])];

                if (h1 == 0xFF || h2 == 0xFF || h3 == 0xFF || h4 == 0xFF) revert InvalidHexCharacter(h1, h2, h3, 4);

                buffer[j*2] = bytes1(shl4Table[h1]) | bytes1(h2);
                buffer[j*2+1] = bytes1(shl4Table[h3]) | bytes1(h4);
            } while(++j < 8);
        }

        bytes16 result;
        assembly {
            result := mload(add(buffer, 32))
        }
        
        return result;
    }

    // @notice Packs a UUID string and an unpacked ID into a bytes32 format
    // @param variantId The UUID string to parse
    // @param unpackedId The unpacked ID to pack
    // @return bytes32 The converted UUID and unpacked ID packed together in a bytes32 format
    function buildTokenId(string memory variantId, uint128 unpackedId) public view returns (bytes32) {
        bytes memory unpackedIdBytes = abi.encodePacked(unpackedId);
        bytes memory uuid = abi.encodePacked(toBytes16(variantId));
        bytes memory concat = bytes.concat(uuid, unpackedIdBytes);

        bytes32 result;
        assembly {
            result := mload(add(concat, 32))
        }

        return result;
    }
}

