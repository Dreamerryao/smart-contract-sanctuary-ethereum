pragma solidity 0.8.9;

import "solmate/tokens/ERC721.sol";
import "./TestNFTConstants.sol";
import "../constants/UUID.sol";

contract TestNFT is ERC721, UUID {
    /**
     * @notice Last minted token ID
     */
    uint128 public lastTokenId;

    /**
     * @notice Token Test NFT Constants
     */
    TestNFTConstants public testNFTConstants;

    constructor() ERC721("Test", "TEST") {
        testNFTConstants = new TestNFTConstants();
    }

    function mintAll() public {
        uint256 i;
        uint256 length = testNFTConstants.tokenIdsLength();
        uint256 recipientIndex = 0;

        unchecked {
             do { 
                _mint(testNFTConstants.getRecipientAtIndex(recipientIndex), testNFTConstants.getTokenAtIndex(i));

                recipientIndex = (recipientIndex + 1) % testNFTConstants.recipientsLength();
            } while(++i < length);
        }
    }

    function mint(string calldata variantId, address to) public {
        bytes32 result = buildTokenId(variantId, lastTokenId);

        _mint(to, uint256(result));
        lastTokenId++;
    }

    function tokenURI(uint256 tokenId) public pure override returns (string memory) {
        return "https://test.com";
    }
}

