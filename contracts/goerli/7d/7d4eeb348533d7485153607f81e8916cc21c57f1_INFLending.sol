pragma solidity ^0.8.0;
import "./interfaces/ILendingPool.sol";
import "@rari-capital/solmate/src/tokens/ERC20.sol";
import "@rari-capital/solmate/src/utils/SafeTransferLib.sol";
import "@boringcrypto/boring-solidity/contracts/libraries/BoringRebase.sol";
import "@boringcrypto/boring-solidity/contracts/BoringOwnable.sol";

interface IOracle {
    function getPrice(address preIPOToken) external view returns (uint256 price);
}

contract LendingPool is ILendingPool, ERC20, BoringOwnable {
    using SafeTransferLib for ERC20;
    using RebaseLibrary for Rebase;

    // optimization of totalSupply by using it as such and combining it with totalLentOut
    Rebase public totalAsset;

    uint256 totalLentOut;

    // combination with balanceOf possible
    mapping (address => uint256) public totalPaidOut;

    mapping (address => TokenConditions) public loanConditions;

    ERC20 public immutable USDC;

    IOracle public oracle;

    uint256 private constant BPS = 10_000;

    constructor (ERC20 _USDC) ERC20("INF LENDING POOL PRIME", "INF-LEND-PRIME", 18) {
        USDC = _USDC;
    }

    // regulates access to correct deployment
    function approveLending (address to) external {
        USDC.safeApprove(to, type(uint256).max);
    }

    function setMaxAllocation (address token, uint80 limit) external{
        loanConditions[token].limit = limit;
    }

    function setOracle(IOracle oracle_) external onlyOwner {
        oracle = oracle_;
    }

    function accountProfits() public returns (uint256 amount) {
        amount = USDC.balanceOf(address(this)) + totalLentOut - uint256(totalAsset.elastic);
        totalAsset.addElastic(amount);
    }

    function deposit(address to, uint256 share) external returns (uint256 amount) {
        accountProfits();
        // Should there be a deposit limit?

        Rebase memory total = totalAsset;

        amount = total.toElastic(share, true);

        _mint(to, share);

        totalAsset = total.add(amount, share);

        USDC.safeTransferFrom(msg.sender, address(this), amount);
    }

    function withdraw(address to, uint256 share) external returns (uint256 amount) {
        Rebase memory total = totalAsset;

        amount = total.toElastic(share, false);

        uint256 ratio = totalLentOut * 1e18 / total.elastic;

        // alternative is bank run scenario
        require(total.toElastic(balanceOf[msg.sender],false) * ratio / 1e18 >= amount+ totalPaidOut[msg.sender], "NOT LIQUID");

        _burn(msg.sender, amount);

        totalAsset = total.sub(amount, share);

        totalPaidOut[msg.sender] += amount;

        USDC.safeTransfer(to, amount);
    }

    function willLend(uint256 tokenId, LoanParams memory params)
        external
        view
        override
        returns (bool) {

            TokenConditions memory conditions = loanConditions[params.preIPOToken];
            
            require(
                params.duration == conditions.duration 
                && params.interestPerYear == conditions.interestPerYear 
                && params.underlyingPerYear == conditions.underlyingPerYear 
                && params.borrowOpeningFee == conditions.borrowOpeningFee
                );
            

            return true;
        }

    function lendingConditions(uint32 duration, uint104 underlying, address preIPOToken)
        external
        view
        override
        returns (bool, LoanParams memory params) {
            return (false, LoanParams(0,0,0,0,0,address(0), 0));
        }

    function rescueToken(ERC20 token, address to, uint256 amount) onlyOwner external {
        token.safeTransfer(to, amount);
    }

}
