pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@boringcrypto/boring-solidity/contracts/Domain.sol";
import "@boringcrypto/boring-solidity/contracts/BoringOwnable.sol";
import "@rari-capital/solmate/src/tokens/ERC721.sol";
import "./interfaces/ILendingPool.sol";
import "hardhat/console.sol";

interface IERC20Burnable is IERC20 {
    function burn(uint256 amount) external;
}

contract INFLending is ERC721, Domain, BoringOwnable {
    using SafeERC20 for IERC20Burnable;

    event LogRepay(address indexed sender, uint256 id, uint256 amountRepaid);
    event LogSetExecutorOrManager(address indexed executor, address indexed manager, bool setExecutor, bool setManager);
    event LogLiquidated(address indexed sender);

    struct PaymentData {
        uint32 beginTimeOrExpiry;
        uint32 duration;
        uint32 lastRepaid;
        uint16 interestPerYear;
        uint16 underlyingPerYear;
        uint8 state;
        uint16 borrowOpeningFee;
        uint104 underlying;
        address borrower;
        uint96 loanAmount;
    }

    struct SignatureParams {
        uint256 deadline;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    struct LoanDetails {
        IERC20Burnable preIPOToken;
        uint80 amountRepaid;
        uint16 underlyingPaidOutPercentage;
    }

    uint256 public totalSupply;

    uint8 private constant INITIALIZED = 1;
    uint8 private constant BORROW_OPEN = 2;
    uint8 private constant LIQUIDATED = 3;

    uint256 constant private SECONDS_PER_YEAR = 31_536_000;

    // if a different stable coin would be used the uint sizes should be reconsidered
    IERC20Burnable public immutable USDC;

    uint256 private constant BPS = 10_000;
    uint256 private constant PROFIT_SHARE= 2_000; // 20%

    mapping(uint256 => PaymentData) public idMapping;
    mapping(uint256 => LoanDetails) public loanDetails;

    mapping(address => bool) public isManager;
    mapping(address => bool) public isExecutor;
    mapping(address => uint256) public nonces;

    mapping(address => uint256) public getPrice;

    address public treasury;

    bytes32 constant SIGNATURE_HASH = keccak256("Lend(address contract,uint256 id,address preIPOToken,uint32 duration,uint16 interestPerYear,uint16 underlyingPerYear,uint16 borrowOpeningFee,uint96 loanAmount,uint256 nonce,uint256 deadline)");

    modifier onlyManager {
        require(isManager[msg.sender]);
        _;
    }

    modifier onlyExecutor {
        require(isExecutor[msg.sender]);
        _;
    }

    constructor (IERC20Burnable USDC_, address _treasury) ERC721("Infinity Lending Platform", "INF-LEND"){
        USDC = USDC_;
        treasury = _treasury;
    }

    function tokenURI(uint256 id) public view override returns (string memory) {
        return "EMPTY";
    }

    function setExecutorOrManager(address executor, address manager, bool setExecutor, bool setManager) external onlyOwner {
        isExecutor[executor] = setExecutor;
        isManager[manager] = setManager;
        emit LogSetExecutorOrManager(executor, manager, setExecutor, setManager);
    }

    // should we keep an oracle or fix the valuation at loan creation
    function setOraclePrice(address preIPOToken, uint256 price) external onlyManager {
        getPrice[preIPOToken] = price;
    }

    function createLoanObject(uint32 expiry, uint32 duration, uint104 underlying, address borrower, IERC20Burnable preIPOToken) external onlyManager returns (uint256 tokenId) {
        tokenId = totalSupply++;
        preIPOToken.safeTransferFrom(msg.sender, address(this), underlying);
        idMapping[tokenId] = PaymentData(expiry, duration, 0, 0, 0, INITIALIZED, 0, underlying, borrower, 0);
        loanDetails[tokenId] = LoanDetails(preIPOToken, 0, 0);
        _mint(borrower, tokenId);
    }

    function calculateUnderlyingToBeRedeemed (PaymentData memory data, LoanDetails memory details) internal view returns (uint256 outstandingUnderlying) {
        uint256 delta = block.timestamp - uint256(data.beginTimeOrExpiry);
        //                          100          * 5184000 *    50               /    31_536_000    - 0
        outstandingUnderlying = data.underlying * delta * data.underlyingPerYear / SECONDS_PER_YEAR - uint256(data.underlying) * uint256(details.underlyingPaidOutPercentage);
    }

    function calculateOutstandingInterest (PaymentData memory data, LoanDetails memory details) internal view returns (uint256 outstandingInterest) {
        uint256 delta = block.timestamp - uint256(data.beginTimeOrExpiry);
        console.log(data.loanAmount, delta, data.interestPerYear, 'dadada111');
        console.log(uint256(details.amountRepaid), 'dadada222');
        outstandingInterest = data.loanAmount * delta * data.interestPerYear / SECONDS_PER_YEAR / BPS - uint256(details.amountRepaid);
        console.log('outstandingInterest: ', outstandingInterest);
    }

    function calculateCurrentlyOutstanding(uint256 id) external view returns (uint256 outstanding) {
        /**
         uint32 beginTimeOrExpiry;
         uint32 duration;
         uint32 lastRepaid;
         uint16 interestPerYear;
         uint16 underlyingPerYear;
         uint8 state;
         uint16 borrowOpeningFee;
         uint104 underlying;
         address borrower;
         uint96 loanAmount;
         */
        PaymentData memory data = idMapping[id];
        /**
         IERC20Burnable preIPOToken;
         uint80 amountRepaid;
         uint16 underlyingPaidOutPercentage;
         */
        LoanDetails memory details = loanDetails[id];
        //                            1655802567       +   63072000
        uint256 repayRequired = data.beginTimeOrExpiry + data.duration > block.timestamp ? data.loanAmount : 0;
        outstanding = repayRequired + calculateOutstandingInterest(data, details);
        console.log('repayRequired, outstanding', repayRequired, outstanding);
    }

    function calculateRequiredAmountToBePaid(PaymentData memory data, LoanDetails memory details) internal view returns (uint256 amountOutstanding) {
        uint256 repayRequired = data.beginTimeOrExpiry + data.duration + 30 days > block.timestamp ? data.loanAmount : 0;
        amountOutstanding = repayRequired + calculateOutstandingInterest(data, details);
    }

    function borrow(uint256 id, IERC20Burnable preIPOToken, uint32 duration, uint16 interestPerYear, uint16 underlyingPerYear, uint16 borrowOpeningFee, uint96 loanAmount, address lender, SignatureParams calldata signature) external {
        PaymentData memory data = idMapping[id];

        require(msg.sender == data.borrower && block.timestamp < data.beginTimeOrExpiry);
        if (signature.v == 0 && signature.r == bytes32(0) && signature.s == bytes32(0)) {
            ILendingPool.LoanParams memory params = ILendingPool.LoanParams(duration, interestPerYear, underlyingPerYear, borrowOpeningFee, data.underlying, address(preIPOToken), loanAmount);
            require(ILendingPool(lender).willLend(id, params), "LENDINGPOOL NO FRIEND");
        } else {
            require(block.timestamp <= signature.deadline, "SIGNATURE EXPIRED");
            uint256 nonce = nonces[lender]++;
            bytes32 dataHash = keccak256(
                abi.encode(
                    SIGNATURE_HASH,
                    address(this),
                    id,
                    preIPOToken,
                    duration,
                    interestPerYear,
                    underlyingPerYear,
                    borrowOpeningFee,
                    loanAmount,
                    nonce,
                    signature.deadline
                )
            );
            require(ecrecover(_getDigest(dataHash), signature.v, signature.r, signature.s) == lender, "SIGNATURE INVALID");
        }
        require(data.state == INITIALIZED, "LOAN EXISTS");

        LoanDetails memory details = loanDetails[id];
        require(details.preIPOToken == preIPOToken);

        data.duration = duration;
        data.interestPerYear = interestPerYear;
        data.underlyingPerYear = underlyingPerYear;
        data.borrowOpeningFee = borrowOpeningFee;
        data.loanAmount = loanAmount;
        data.beginTimeOrExpiry = uint32(block.timestamp);

        data.state = BORROW_OPEN;

        idMapping[id] = data;
        // TODO: fee take for treasury
        console.log('Transfer: ', loanAmount, borrowOpeningFee, BPS);
        USDC.safeTransferFrom(lender, data.borrower, loanAmount - loanAmount * borrowOpeningFee / BPS);
        transferFrom(_ownerOf[id], lender, id);
    }

    function _repay(address sender, uint256 id, uint256 amountToRepay) internal  returns (PaymentData memory data, LoanDetails memory details){
        data = idMapping[id];
        details = loanDetails[id];
        require(data.state == BORROW_OPEN, "Borrow needs to exist");

        address nftOwner = _ownerOf[id];
        console.log(sender, nftOwner,'sender, nftOwner');
        USDC.safeTransferFrom(sender, nftOwner, amountToRepay);
        details.amountRepaid = details.amountRepaid + uint80(amountToRepay);

        uint256 redeemed = calculateUnderlyingToBeRedeemed(data, details);
        // TODO: give percentage to treasury
        details.preIPOToken.transfer(nftOwner, redeemed);
        details.underlyingPaidOutPercentage = details.underlyingPaidOutPercentage + uint16(uint128(redeemed) * BPS / data.underlying);

        // extra protection of borrower
        require(block.timestamp > uint256(data.beginTimeOrExpiry) + uint256(data.duration) || details.amountRepaid <= data.loanAmount * data.duration * data.interestPerYear / BPS);

        loanDetails[id] = details;

        emit LogRepay(sender, id, amountToRepay);
    }

    function repay(uint256 id, uint256 amountToRepay) external {
        _repay(msg.sender, id, amountToRepay);
    }

    // is direct debit functionality wanted?
    // if so who should be allowed to trigger it
    function repayFrom(uint256 id) external onlyExecutor {
        PaymentData memory data = idMapping[id];
        LoanDetails memory details = loanDetails[id];
        uint256 interest = calculateOutstandingInterest(data, details);
        _repay(data.borrower, id, interest);
    }

    function repayAndBurn(uint256 id, uint256 amountToRepay) external {
        (PaymentData memory data, LoanDetails memory details) = _repay(msg.sender, id, amountToRepay);
        require(details.amountRepaid >= data.loanAmount * data.duration * data.interestPerYear / BPS + data.loanAmount);
        //              110
        delete idMapping[id];
        delete loanDetails[id];
        _burn(id);

        uint256 underlyingPaidOut = uint256(data.underlying) * uint256(details.underlyingPaidOutPercentage) / BPS;
        details.preIPOToken.burn(data.underlying - underlyingPaidOut);
    }      

    function liquidate(uint256 id) external {
        PaymentData memory data = idMapping[id];
        LoanDetails memory details = loanDetails[id];
        console.log(60 days, data.interestPerYear, BPS, 'test');
        uint256 twoMonthsInterest = 60 days * data.interestPerYear / BPS;
        require(calculateRequiredAmountToBePaid(data, details) > twoMonthsInterest);
        idMapping[id].state = LIQUIDATED;

        address nftOwner = _ownerOf[id];
        
        delete idMapping[id];
        delete loanDetails[id];
        _burn(id);

        uint256 underlyingPaidOut = uint256(data.underlying) * uint256(details.underlyingPaidOutPercentage) / BPS;

        details.preIPOToken.transfer(nftOwner, data.underlying - underlyingPaidOut);
    }
}

