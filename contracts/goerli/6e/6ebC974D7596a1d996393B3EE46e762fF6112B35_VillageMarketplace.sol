pragma solidity ^0.8.9;




contract PlaykenToken is ERC20Burnable {
  using SafeMath for uint256;
  using Math for uint256;

  uint256 public basePercent = 100;

  constructor(uint256 initialSupply) ERC20("PlaykenToken", "PT") {
    _mint(msg.sender, initialSupply);
  }

  function transfer(address to, uint256 amount) public override returns (bool) {
    address owner = msg.sender;
    require(amount <= balanceOf(owner));
    require(to != address(0));

    uint256 tokensToBurn = _findFivePercent(amount);
    uint256 tokensToTransfer = amount.sub(tokensToBurn);

    burnFrom(owner, tokensToBurn);
    _transfer(owner, to, tokensToTransfer);
    return true;
  }

  function _findFivePercent(uint256 value) public view returns (uint256)  {
    uint256 roundValue = value.ceilDiv(basePercent);
    uint256 onePercent = roundValue.mul(basePercent).div(100);
    return onePercent.mul(5);
  }
}

