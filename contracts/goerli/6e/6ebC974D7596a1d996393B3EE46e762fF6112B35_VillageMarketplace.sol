pragma solidity ^0.8.9;



contract VillageWearableNFT is ERC721URIStorage {
  using Counters for Counters.Counter;
  Counters.Counter private _tokenIdCounter;

  address private _marketplaceContractAddress;
  mapping(uint256 => address) private _tokenIdToCreator;

  mapping(uint256 => WearableData) private _tokenIdToWearableData;

  struct WearableData {
    uint256 level;
    string wearableType;
  }

  constructor(address marketplaceContractAddress) ERC721 ("VillageWearableNFT", "VWNFT") {
    _marketplaceContractAddress = marketplaceContractAddress;
  }

  function mintNFT(string memory tokenUri, string memory tokenWearableType) public {
    _mintWithLevel(msg.sender, tokenUri, 1, tokenWearableType);
    setApprovalForAll(_marketplaceContractAddress, true);
  }

  function getTokenCreatorById(uint256 tokenId) public view returns (address) {
    return _tokenIdToCreator[tokenId];
  }

  function level(uint256 id) public view returns (uint256) {
    return _tokenIdToWearableData[id].level;
  }

  function wearableType(uint256 id) public view returns (string memory) {
    return _tokenIdToWearableData[id].wearableType;
  }

  function fuse(address account, uint256 tokenId1, uint256 tokenId2) public returns (uint256) {
    require(ownerOf(tokenId1) == account, "Fuse: you're not the token owner.");
    require(ownerOf(tokenId2) == account, "Fuse: you're not the token owner.");
    require(tokenId1 != tokenId2, "Fuse: can't fuse same token.");

    uint256 token1Level = level(tokenId1);
    uint256 token2Level = level(tokenId2);
    require(token1Level == token2Level, "Fuse: incorrect levels for base NFTs.");

    string memory token1Type = _tokenIdToWearableData[tokenId1].wearableType;
    string memory token2Type = _tokenIdToWearableData[tokenId2].wearableType;
    require(_isEqualString(token1Type, token2Type), "Fuse: different types for base NFTs");

    uint256 inheritTokenId = _randomNumber(100) < 50 ? tokenId1 : tokenId2;
    string memory targetTokenUri = tokenURI(inheritTokenId);
    uint256 targetLevel = level(inheritTokenId) + 1;
    string memory targetWearableType = _tokenIdToWearableData[inheritTokenId].wearableType;

    _burn(tokenId1);
    _burn(tokenId2);
    _mintWithLevel(account, targetTokenUri, targetLevel, targetWearableType);

    return _tokenIdCounter.current() - 1;
  }

  function _mintWithLevel(address account, string memory tokenUri, uint256 tokenLevel, string memory tokenWearableType) private {
    uint256 newItemId = _tokenIdCounter.current();

    _safeMint(account, newItemId);
    _setTokenURI(newItemId, tokenUri);

    _tokenIdToCreator[newItemId] = account;
    _tokenIdToWearableData[newItemId] = WearableData(tokenLevel, tokenWearableType);

    _tokenIdCounter.increment();
  }

  uint _randNonce = 0;

  function _randomNumber(uint modulus) private returns(uint) {
    _randNonce++;
    return uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, _randNonce))) % modulus;
  }
  function _isEqualString(string memory str1, string memory str2) public pure returns (bool) {
    return (keccak256(abi.encodePacked((str1))) == keccak256(abi.encodePacked((str2))));
  }
}

