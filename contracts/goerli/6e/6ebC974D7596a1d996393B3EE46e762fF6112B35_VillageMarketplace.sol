pragma solidity ^0.8.9;






contract VillageMarketplace is ReentrancyGuard {
  using Counters for Counters.Counter;

  Counters.Counter private _itemIds;
  Counters.Counter private _listedItems;

  address payable private _owner;
  uint256 private _listingFee = 0.00045 ether;

  mapping(uint256 => MarketItem) private _itemIdToMarketItem;

  struct MarketItem {
    uint256 itemId;
    address nftContractAddress;
    uint256 tokenId;
    address payable creator;
    address payable seller;
    address payable owner;
    uint256 price;
  }

  PlaykenToken public playkenToken;

  constructor(address tokenContractAddress) {
    _owner = payable(msg.sender);
    playkenToken = PlaykenToken(tokenContractAddress);
  }

  function getListingFee() public view returns (uint256) {
    return _listingFee;
  }

  function listItemForSale(address nftContractAddress, uint256 nftTokenId, uint256 price) public payable nonReentrant returns (uint256) {
    require(price > 0, "ListItemForSale: price must be at least 1 wei");
    require(msg.value == _listingFee, "ListItemForSale: price must be equal to listing price");

    MarketItem memory targetMarketItem = _getExistedMarketItem(nftTokenId);
    uint256 itemId;
    bool isMarketItemExisted = _exists(targetMarketItem);

    if (isMarketItemExisted) {
      itemId = targetMarketItem.itemId;
      targetMarketItem.seller = payable(msg.sender);
      targetMarketItem.owner = payable(address(0));
      targetMarketItem.price = price;
      _itemIdToMarketItem[itemId] = targetMarketItem;
    } else {
      itemId = _itemIds.current();
      address creator = VillageWearableNFT(nftContractAddress).getTokenCreatorById(nftTokenId);

      _itemIdToMarketItem[itemId] = MarketItem(
        itemId,
        nftContractAddress,
        nftTokenId,
        payable(creator),
        payable(msg.sender),
        payable(address(0)),
        price
      );

      _itemIds.increment();
    }

    _listedItems.increment();
    IERC721(nftContractAddress).transferFrom(msg.sender, address(this), nftTokenId);
    return itemId;
  }

  function delistItemFromSale(address nftContractAddress, uint256 marketItemId) public payable nonReentrant {
    uint256 nftTokenId = _itemIdToMarketItem[marketItemId].tokenId;

    require(nftTokenId >= 0, "DelistItemFromSale: market item should exist");
    require(_itemIdToMarketItem[marketItemId].seller == msg.sender, "DelistItemFromSale: you are not the seller");

    IERC721(nftContractAddress).transferFrom(address(this), msg.sender, nftTokenId);

    _itemIdToMarketItem[marketItemId].owner = payable(msg.sender);
    _listedItems.decrement();
  }

  function buyItem(address nftContractAddress, uint256 marketItemId) public payable nonReentrant {
    address payable currentUser = payable(msg.sender);
    uint256 currentPrice = msg.value;
    uint256 price = _itemIdToMarketItem[marketItemId].price;
    require(currentPrice == price, "BuyItem: please submit the asking price in order to continue");
    require(currentPrice <= playkenToken.balanceOf(currentUser), "BuyItem: not enough tokens");

    uint256 nftTokenId = _itemIdToMarketItem[marketItemId].tokenId;
    _itemIdToMarketItem[marketItemId].owner = currentUser;

    playkenToken.transferFrom(currentUser, _itemIdToMarketItem[marketItemId].seller, currentPrice);
    playkenToken.transferFrom(currentUser, _owner, _listingFee);

    IERC721(nftContractAddress).transferFrom(address(this), msg.sender, nftTokenId);

    _listedItems.decrement();
  }

  function getAllItems() public view returns (MarketItem[] memory) {
    uint256 totalItemsCount = _itemIds.current();
    uint256 existingItemIdCount = 0;

    for (uint256 i = 0; i < totalItemsCount; i++) {
      MarketItem storage item = _itemIdToMarketItem[i];
      if (_exists(item)) {
        existingItemIdCount += 1;
      }
    }

    uint256 currentIndex = 0;
    MarketItem[] memory items = new MarketItem[](existingItemIdCount);

    for (uint256 i = 0; i < totalItemsCount; i++) {
        MarketItem storage item = _itemIdToMarketItem[i];
        if (_exists(item)) {
          items[currentIndex] = item;
          currentIndex += 1;
        }
    }
    return items;
  }

  function listedItemsCount() public view returns (uint) {
    return _listedItems.current();
  }

  function getAllOnSaleItems() public view returns (MarketItem[] memory) {
    uint allItemsCounts = _itemIds.current();
    uint currentIndex = 0;
    MarketItem[] memory items = new MarketItem[](listedItemsCount());

    for (uint i = 0; i < allItemsCounts; i++) {
      MarketItem storage item = _itemIdToMarketItem[i];

      if (_isItemOnSale(item)) {
        items[currentIndex] = item;
        currentIndex += 1;
      }
    }

    return items;
  }

  function getOwnedItems() public view returns (MarketItem[] memory) {
    uint256 totalItemsCount = _itemIds.current();
    uint256 ownedItemsCount = 0;


    for (uint256 i = 0; i < totalItemsCount; i++) {
        MarketItem storage item = _itemIdToMarketItem[i];
        address itemOwner = item.owner;
        if (itemOwner != msg.sender) continue;
        ownedItemsCount += 1;
    }

    uint256 currentIndex = 0;
    MarketItem[] memory items = new MarketItem[](ownedItemsCount);

    for (uint256 i = 0; i < totalItemsCount; i++) {
        MarketItem storage item = _itemIdToMarketItem[i];
        address itemOwner = item.owner;
        if (itemOwner != msg.sender) continue;
        items[currentIndex] = item;
        currentIndex += 1;
    }
    return items;
  }

  function getItem(uint256 itemId) public view returns(MarketItem memory) {
    MarketItem memory targetItem = _itemIdToMarketItem[itemId];
    require(_exists(targetItem), "GetItem: Item not found");
    return targetItem;
  }

  /**
    1. Check if token1 or token 2 is on sale
    2. Check token1's level == token2's are from same NFT contract
    3. Fuse: Burn token 1 and token 2,  get newly minted token3
    4. Remove token1 and toekn2 in _itemIdToMarketItem
    5. Add newly minted token3 to marketplace
  */
  function fuseItem(uint256 itemId1, uint256 itemId2) public nonReentrant {
    MarketItem memory item1 = _itemIdToMarketItem[itemId1];
    MarketItem memory item2 = _itemIdToMarketItem[itemId2];
    require(_isItemOnSale(item1) == false, "FuseItem: not allowed during onsale.");
    require(_isItemOnSale(item2) == false, "FuseItem: not allowed during onsale.");
    require(item1.nftContractAddress == item2.nftContractAddress, "FuseItem: incorrect NFT contract.");

    uint256 newTokenId = VillageWearableNFT(item1.nftContractAddress).fuse(msg.sender, item1.tokenId, item2.tokenId);

    delete _itemIdToMarketItem[itemId1];
    delete _itemIdToMarketItem[itemId2];
    _addItemToMarketplace(item1.nftContractAddress, newTokenId);
  }

  function _getExistedMarketItem(uint256 nftTokenId) private view returns (MarketItem memory) {
    uint allItemsCount = _itemIds.current();

    MarketItem memory existedMarketItem;

    for (uint i = 0; i < allItemsCount; i++) {
      MarketItem storage item = _itemIdToMarketItem[i];
      if (item.tokenId == nftTokenId) {
        existedMarketItem = item;
        break;
      }
    }
    return existedMarketItem;
  }

  function _isItemOnSale(MarketItem memory item) private pure returns (bool) {
    return item.owner == address(0);
  }

  function _addItemToMarketplace(address nftContractAddress, uint256 nftTokenId) private {
    MarketItem memory existedMarketItem = _getExistedMarketItem(nftTokenId);
    uint256 itemId;
    bool isMarketItemExisted = existedMarketItem.creator != address(0);
    if (isMarketItemExisted) return;

    itemId = _itemIds.current();
    address creator = VillageWearableNFT(nftContractAddress).getTokenCreatorById(nftTokenId);
    _itemIdToMarketItem[itemId] = MarketItem(
      itemId,
      nftContractAddress,
      nftTokenId,
      payable(creator),
      payable(address(0)),
      payable(msg.sender),
      uint256(0)
    );
    _itemIds.increment();
  }

  function _exists(MarketItem memory marketItem) private pure returns (bool) {
    return marketItem.creator != address(0);
  }
}