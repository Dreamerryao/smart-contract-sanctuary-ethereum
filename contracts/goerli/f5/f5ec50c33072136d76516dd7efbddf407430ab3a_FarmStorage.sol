pragma solidity ^0.8.0;

// Import this file to use console.log
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract FarmStorage is AccessControl {
  bytes32 public constant dataAdd_ROLE = keccak256("dataAdd_ROLE");

  constructor() {
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _grantRole(dataAdd_ROLE, msg.sender);
  }

  uint256 farmerIdCounter = 100; // collector id would be in 2000 series
  uint256 colCenIdCounter = 200; // transporter id would be in 3000 series (collection plus transport id)
  uint256 packagingIdCounter = 300; // packaging id would be in 4000 series
  uint256 processingIdCounter = 400; // processing id would be in 5000 series
  uint256 warehouseIdCounter = 500; // warehouse id would be in 6000 series
  uint256 retailerIdCounter = 600; // retailer id would be in 7000 series

  uint256 productIdCounter = 700; // product id would be in 7000 series

  // Procurement

  struct farmerSt {
    string nameOfFarmer;
    string nameOfCollector;
    uint256 timeOfCollection;
    uint256 quantity;
    uint256 fatPer;
    uint256 SNF;
    uint256 cattelType; // 1- cow, 2 - buffalo
  }

  struct colCenSt {
    string vehicleNumber;
    string colCenName;
    string nameOfDriver;
    string routeName;
    uint256 quantity;
    uint256 temperature;
    uint256 timeOfTrans;
  }

  mapping(string => mapping(uint256 => farmerSt)) dateWiseInfoFarmer;
  mapping(string => mapping(uint256 => mapping(uint256 => colCenSt))) dateWiseInfoColCen;

  string[] farmerArray;
  string[] colCenArray;

  function getAllfarmer() public view returns (string[] memory) {
    return farmerArray;
  }

  function getAllColCen() public view returns (string[] memory) {
    return colCenArray;
  }

  mapping(string => bool) farmerIdCheck;
  mapping(string => uint256) farmerId;
  mapping(uint256 => string) farmerNameFrId;
  mapping(string => bool) colCenIdCheck;
  mapping(string => uint256) colCenId;
  mapping(uint256 => string) colCenNameFrId;

  function getFarmerName(uint256 _Id) public view returns (string memory) {
    return farmerNameFrId[_Id];
  }

  function getColCenName(uint256 _Id) public view returns (string memory) {
    return colCenNameFrId[_Id];
  }

  function generateFarmerId(string memory _nameF) public returns(uint256){
    bool idGeneration = farmerIdCheck[_nameF];
    if (!idGeneration) {
      farmerIdCheck[_nameF] = true;
      farmerId[_nameF] = farmerIdCounter;
      farmerNameFrId[farmerIdCounter] = _nameF;
      farmerArray.push(_nameF);
      farmerIdCounter++;
    }
    return farmerId[_nameF];
  }

  function generateColCenId(string memory _nameCen) public returns(uint256){
    bool idGeneration = colCenIdCheck[_nameCen];
    if (!idGeneration) {
      colCenIdCheck[_nameCen] = true;
      colCenId[_nameCen] = colCenIdCounter;
      colCenNameFrId[colCenIdCounter] = _nameCen;
      colCenArray.push(_nameCen);
      colCenIdCounter++;
    }
    return  colCenId[_nameCen];
  }

  function getFarmerId(string memory _registeredName)
    public
    view
    returns (uint256)
  {
    return farmerId[_registeredName];
  }

  function getColCenId(string memory _registeredName)
    public
    view
    returns (uint256)
  {
    return colCenId[_registeredName];
  }

  function addFarmerDetails(
    string memory _nameC,
    uint256 _farmerId,
    uint256 _cattelType,
    uint256 _quantity,
    string memory _date,
    uint256 _timeC,
    uint256 _fat,
    uint256 _SNF
  ) public {
    require(_farmerId >= 100, "not a farmer id or generate one");
    require(_farmerId < 200, "not a farmer id or generate one");
    dateWiseInfoFarmer[_date][_farmerId].nameOfCollector = _nameC;
    dateWiseInfoFarmer[_date][_farmerId].nameOfFarmer = farmerNameFrId[_farmerId];
    dateWiseInfoFarmer[_date][_farmerId].timeOfCollection = _timeC;
    dateWiseInfoFarmer[_date][_farmerId].quantity = _quantity;
    dateWiseInfoFarmer[_date][_farmerId].cattelType = _cattelType;
    dateWiseInfoFarmer[_date][_farmerId].fatPer = _fat;
    dateWiseInfoFarmer[_date][_farmerId].SNF = _SNF;
  }

  function addColCenDetails(
    string memory _numV,
    uint256 _colCenId,
    uint256 _quantity,
    string memory _routeName,
    string memory _driverName,
    string memory _date,
    uint256 _timeT,
    uint256 _temp,
    uint256 entNo
  ) public {
    require(_colCenId >= 200, "not a ColCen id or generate one");
    require(_colCenId < 300, "not a ColCen id or generate one");
    dateWiseInfoColCen[_date][_colCenId][entNo].vehicleNumber = _numV;
    dateWiseInfoColCen[_date][_colCenId][entNo].colCenName = colCenNameFrId[_colCenId];
    dateWiseInfoColCen[_date][_colCenId][entNo].nameOfDriver = _driverName;
    dateWiseInfoColCen[_date][_colCenId][entNo].routeName = _routeName;
    dateWiseInfoColCen[_date][_colCenId][entNo].quantity = _quantity;
    dateWiseInfoColCen[_date][_colCenId][entNo].temperature = _temp;
    dateWiseInfoColCen[_date][_colCenId][entNo].timeOfTrans = _timeT;
  }

  function getFarmerDetails(string memory _date, uint256 _farmerId)
    public
    view
    returns (
      string memory,
      string memory,
      uint256,
      uint256,
      uint256,
      uint256,
      uint256
    )
  {
    farmerSt memory collect = dateWiseInfoFarmer[_date][_farmerId];
    return (
      collect.nameOfCollector,
      collect.nameOfFarmer,
      collect.timeOfCollection,
      collect.quantity,
      collect.fatPer,
      collect.SNF,
      collect.cattelType
    );
  }

  function getColCenDetails(
    string memory _date,
    uint256 _colCenId,
    uint256 _entNo
  )
    public
    view
    returns (
      string memory,
      string memory,
      string memory,
      string memory,
      uint256,
      uint256,
      uint256
    )
  {
    colCenSt memory trans = dateWiseInfoColCen[_date][_colCenId][_entNo];
    return (
      trans.vehicleNumber,
      trans.colCenName,
      trans.nameOfDriver,
      trans.routeName,
      trans.quantity,
      trans.temperature,
      trans.timeOfTrans
    );
  }

  //Packaging and Processing

  struct packagingSt {
    string productName;
    string packageCenName;
    string PMTNo;
    string timeOfPacking;
    string dateOfPacking;
    string useBy;
  }

  struct processingSt {
    string receivedBy;
    string processCenName;
    string quantity;
    string time;
    uint256 fat;
    string SNF;
    string MBRT;
    string acidity;
    uint256 accOrRej;
    string RMTPMTNo;
    string PastuTime;
  }

  mapping(string => mapping(uint256 => mapping(uint256 => processingSt))) dateWiseInfoProcessing;
  mapping(string => mapping(uint256 => mapping(uint256 => packagingSt))) dateWiseInfoPackaging;

  mapping(string => bool) processCenterIdCheck;
  mapping(string => uint256) processCenterId;
  mapping(uint256 => string) processCenterNameFrId;
  mapping(string => bool) productIdCheck;
  mapping(string => uint256) productId;
  mapping(uint256 => string) productNameFrId;

  function getProcessCenName(uint256 _Id) public view returns (string memory) {
    return processCenterNameFrId[_Id];
  }

  function getProductName(uint256 _Id) public view returns (string memory) {
    return productNameFrId[_Id];
  }

  function generateProcessCenId(string memory _namePro) public returns(uint256){
    bool idGeneration = processCenterIdCheck[_namePro];
    if (!idGeneration) {
      processCenterIdCheck[_namePro] = true;
      processCenterId[_namePro] = processingIdCounter;
      processCenterNameFrId[processingIdCounter] = _namePro;
      processCenArray.push(_namePro);
      processingIdCounter++;
    }
    return processCenterId[_namePro];
  }

  function generateProductId(string memory _nameProd) public returns(uint256){
    bool idGeneration = productIdCheck[_nameProd];
    if (!idGeneration) {
      productIdCheck[_nameProd] = true;
      productId[_nameProd] = productIdCounter;
      productNameFrId[productIdCounter] = _nameProd;
      productArray.push(_nameProd);
      productIdCounter++;
    }
    return productId[_nameProd];
  }

  function getAllProcessCen() public view returns (string[] memory) {
    return processCenArray;
  }

  function getAllProduct() public view returns (string[] memory) {
    return productArray;
  }

  string[] processCenArray;
  string[] productArray;

  function addProcessingDetails(
    string memory _receivedBy,
    string memory _quantity,
    string memory _time,
    uint256 _fat,
    uint256 _accOrRej,
    string[3] memory _SNFMBRTacidity,
    string[2] memory _RmtPmtNopastuTime,
    uint256 _processCenterId,
    string memory _date,
    uint256 _productId
  ) public {
    dateWiseInfoProcessing[_date][_processCenterId][_productId].receivedBy = _receivedBy;
    dateWiseInfoProcessing[_date][_processCenterId][_productId].quantity = _quantity;
    dateWiseInfoProcessing[_date][_processCenterId][_productId].time = _time;
    dateWiseInfoProcessing[_date][_processCenterId][_productId].fat = _fat;
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .SNF = _SNFMBRTacidity[0];
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .MBRT = _SNFMBRTacidity[1];
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .acidity = _SNFMBRTacidity[2];
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .accOrRej = _accOrRej;
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .RMTPMTNo = _RmtPmtNopastuTime[0];
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .PastuTime = _RmtPmtNopastuTime[1];
    dateWiseInfoProcessing[_date][_processCenterId][_productId]
      .processCenName = processCenterNameFrId[_processCenterId];
  }

  function addPackagingDetails(
    string memory _PMTNo,
    string memory _timeOfPacking,
    string memory _dateOfPacking,
    string memory _useBy,
    string memory _date,
    uint256 _ProductId,
    uint256 _packagingCenterId // is same as proces center id
  ) public {
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId].productName = productNameFrId[_ProductId];
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId].packageCenName = processCenterNameFrId[_packagingCenterId];
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId].PMTNo = _PMTNo;
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId]
      .timeOfPacking = _timeOfPacking;
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId]
      .dateOfPacking = _dateOfPacking;
    dateWiseInfoPackaging[_date][_packagingCenterId][_ProductId].useBy = _useBy;
  }

  function getProcesingDetails(
    string memory _date,
    uint256 _processCenterId,
    uint256 _productId  // Id for milk, as we will get milk in processing center
  )
    public
    view
    returns (
      string memory,
      string memory,
      string memory,
      uint256,
      string memory,
      string memory,
      string memory,
      uint256,
      string memory,
      string memory,
      string memory
    )
  {
    processingSt memory collect = dateWiseInfoProcessing[_date][
      _processCenterId
    ][_productId];
    return (
      collect.processCenName,
      collect.receivedBy,
      collect.quantity,
      collect.fat,
      collect.SNF,
      collect.MBRT,
      collect.acidity,
      collect.accOrRej,
      collect.RMTPMTNo,
      collect.PastuTime,
      collect.time
    );
  }

  function getPackagingDetails(
    string memory _date,
    uint256 _packageCenterId,
    uint256 _productId
  )
    public
    view
    returns (
      string memory,
      string memory,
      string memory,
      string memory,
      string memory,
      string memory
    )
  {
    packagingSt memory collect = dateWiseInfoPackaging[_date][_packageCenterId][
      _productId
    ];
    return (
      collect.packageCenName,
      collect.productName,
      collect.PMTNo,
      collect.timeOfPacking,
      collect.dateOfPacking,
      collect.useBy
    );
  }

  // warehouse and retailer

  struct wareHouseSt {
    string location;
    string productName;
    string timeOfArrival;
    string qualtity;
    string batchId;
    string usedBy;
  }

  struct retailerSt {
    string nameOfRetailer;
    string productName;
    string batchId;
    string deliveryDate;
    string deliveryTime;
    string quantity;
  }

  mapping(string => mapping(uint256 => mapping(uint256 => wareHouseSt))) dateWiseInfoWarehouse;
  mapping(string => mapping(uint256 => mapping(uint256 => retailerSt))) dateWiseInfoRetailers;

  mapping(string => bool) wareHouseIdCheck;
  mapping(string => uint256) wareHouseId;
  mapping(uint256 => string) wareHouseNameFrId;
  mapping(string => bool) retailerIdCheck;
  mapping(string => uint256) retailerId;
  mapping(uint256 => string) retailerNameFrId;

  function getWareHouseName(uint256 _Id) public view returns (string memory) {
    return wareHouseNameFrId[_Id];
  }

  function getRetailerName(uint256 _Id) public view returns (string memory) {
    return retailerNameFrId[_Id];
  }

  function generateWarehouseId(string memory _nameWarehouse) public returns(uint256){
    bool idGeneration = wareHouseIdCheck[_nameWarehouse];
    if (!idGeneration) {
      wareHouseIdCheck[_nameWarehouse] = true;
      wareHouseId[_nameWarehouse] = warehouseIdCounter;
      wareHouseNameFrId[warehouseIdCounter] = _nameWarehouse;
      wareHouseArray.push(_nameWarehouse);
      warehouseIdCounter++;
    }
    return wareHouseId[_nameWarehouse];
  }

  function generateRetailerId(string memory _nameRetailer) public returns(uint256){
    bool idGeneration = retailerIdCheck[_nameRetailer];
    if (!idGeneration) {
      retailerIdCheck[_nameRetailer] = true;
      retailerId[_nameRetailer] = retailerIdCounter;
      retailerNameFrId[retailerIdCounter] = _nameRetailer;
      retailerArray.push(_nameRetailer);
      retailerIdCounter++;
    }
    return retailerId[_nameRetailer];
  }

  function getAllWareHouse() public view returns (string[] memory) {
    return wareHouseArray;
  }

  function getAllRetailer() public view returns (string[] memory) {
    return retailerArray;
  }

  string[] wareHouseArray;
  string[] retailerArray;

  function addWarehouseDetails(
    string memory _timeOfArrival,
    string memory _qualtity,
    string memory _batchId,
    string memory _usedBy,
    uint256 _wareHouseId,
    string memory _date,
    uint256 _productId
  ) public {
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId].location = wareHouseNameFrId[_wareHouseId];
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId]
      .productName = productNameFrId[_productId];
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId]
      .timeOfArrival = _timeOfArrival;
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId].qualtity = _qualtity;
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId].batchId = _batchId;
    dateWiseInfoWarehouse[_date][_wareHouseId][_productId].usedBy = _usedBy;
  }

  function addRetailerDetails(
    string memory _batchId,
    string memory _deliveryDate,
    string memory _deliveryTime,
    string memory _quantity,
    string memory _date,
    uint256 _productId,
    uint256 _retailerId
  ) public {
    dateWiseInfoRetailers[_date][_retailerId][_productId]
      .nameOfRetailer = retailerNameFrId[_retailerId];
    dateWiseInfoRetailers[_date][_retailerId][_productId]
      .productName = productNameFrId[_productId];
    dateWiseInfoRetailers[_date][_retailerId][_productId].batchId = _batchId;
    dateWiseInfoRetailers[_date][_retailerId][_productId]
      .deliveryDate = _deliveryDate;
    dateWiseInfoRetailers[_date][_retailerId][_productId]
      .deliveryTime = _deliveryTime;
    dateWiseInfoRetailers[_date][_retailerId][_productId].quantity = _quantity;
  }

  function getWarehouseDetails(
    string memory _date,
    uint256 _wareHouseId,
    uint256 _productId
  )
    public
    view
    returns (
      string memory,
      string memory,
      string memory,
      string memory,
      string memory,
      string memory
    )
  {
    wareHouseSt memory collect = dateWiseInfoWarehouse[_date][_wareHouseId][
      _productId
    ];
    return (
      collect.location,
      collect.productName,
      collect.timeOfArrival,
      collect.qualtity,
      collect.batchId,
      collect.usedBy
    );
  }

  function getRetailersDetails(
    string memory _date,
    uint256 _retailerId,
    uint256 _productId
  )
    public
    view
    returns (
      string memory,
      string memory,
      string memory,
      string memory,
      string memory,
      string memory
    )
  {
    retailerSt memory collect = dateWiseInfoRetailers[_date][_retailerId][
      _productId
    ];
    return (
      collect.nameOfRetailer,
      collect.productName,
      collect.batchId,
      collect.deliveryDate,
      collect.deliveryTime,
      collect.quantity
    );
  }
}

