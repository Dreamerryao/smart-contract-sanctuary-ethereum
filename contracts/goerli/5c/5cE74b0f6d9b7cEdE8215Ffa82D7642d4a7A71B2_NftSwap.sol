import '@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/security/Pausable.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import '@openzeppelin/contracts/utils/introspection/ERC165Checker.sol';

import 'hardhat/console.sol'; /** To do: remove */

/**
 * @title NftSwap
 * @dev To do: applying proxy pattern
 */
contract NftSwap is ERC721Holder, Pausable, Ownable {
    /** [contract address]: allowed */
    mapping(address => bool) private _allowedContracts;

    /** [contract address][merged token id]: art token id + offset */
    uint256 constant tokenIdOffset = 1;
    mapping(address => mapping(uint256 => uint256)) private _relations;

    event AllowedContractAdded(address indexed contractAddress);
    event AllowedContractDeleted(address indexed contractAddress);

    event Swap(
        address indexed contractAddress,
        uint256 indexed artId,
        uint256 indexed mergedId
    );

    modifier onlyAllowedContract(address contractAddress) {
        _checkAllowedContract(contractAddress);
        _;
    }

    modifier onlyRelated(address contractAddress, uint256 mergedId) {
        _checkRelation(contractAddress, mergedId);
        _;
    }

    function addAllowedContract(address contractAddress) external onlyOwner {
        require(
            !_isAllowedContract(contractAddress),
            'contract already allowed'
        );
        require(
            ERC165Checker.supportsInterface(
                contractAddress,
                type(IERC721).interfaceId
            ),
            'not supported contract'
        );
        _addAllowedContract(contractAddress);
    }

    function deleteAllowedContract(address contractAddress)
        external
        onlyOwner
        onlyAllowedContract(contractAddress)
    {
        _deleteAllowedContract(contractAddress);
    }

    function isAllowedContract(address contractAddress)
        external
        view
        returns (bool)
    {
        return _isAllowedContract(contractAddress);
    }

    function _addAllowedContract(address contractAddress) internal {
        _allowedContracts[contractAddress] = true;
        emit AllowedContractAdded(contractAddress);
    }

    function _deleteAllowedContract(address contractAddress) internal {
        delete _allowedContracts[contractAddress];
        emit AllowedContractDeleted(contractAddress);
    }

    function _checkAllowedContract(address contractAddress) internal view {
        require(_isAllowedContract(contractAddress), 'not allowed address');
    }

    function _isAllowedContract(address contractAddress)
        internal
        view
        returns (bool)
    {
        return _allowedContracts[contractAddress];
    }

    function swap(address contractAddress, uint256 mergedId)
        external
        onlyAllowedContract(contractAddress)
        onlyRelated(contractAddress, mergedId)
        whenNotPaused
    {
        _swap(contractAddress, mergedId);
    }

    function addRelations(
        address contractAddress,
        uint256[] calldata artIds,
        uint256[] calldata mergedIds
    ) external onlyOwner onlyAllowedContract(contractAddress) {
        require(
            artIds.length > 0 && artIds.length == mergedIds.length,
            'invalid list size'
        );
        for (uint256 i = 0; i < mergedIds.length; i++) {
            require(
                !_hasRelation(contractAddress, mergedIds[i]),
                'duplicated id'
            );
            require(mergedIds[i] != artIds[i], 'invalid id');
            _addRelation(contractAddress, artIds[i], mergedIds[i]);
        }
    }

    function deleteRelation(address contractAddress, uint256 mergedId)
        external
        onlyOwner
        onlyRelated(contractAddress, mergedId)
    {
        _deleteRelation(contractAddress, mergedId);
    }

    function getRelation(address contractAddress, uint256 mergedId)
        external
        view
        onlyAllowedContract(contractAddress)
        onlyRelated(contractAddress, mergedId)
        returns (uint256)
    {
        return _getRelation(contractAddress, mergedId);
    }

    function safeTransfer(
        address contractAddress,
        address to,
        uint256 tokenId
    ) external onlyOwner {
        _safeTransferFrom(IERC721(contractAddress), to, tokenId);
    }

    function _swap(address contractAddress, uint256 mergedId) internal {
        uint256 artId = _getRelation(contractAddress, mergedId);

        /** delete relations */
        _deleteRelation(contractAddress, mergedId);

        /** below is a call to another contact */
        IERC721 erc721 = IERC721(contractAddress);

        /** merged nft operator check */
        _checkApprovedOrOwner(erc721, _msgSender(), mergedId);

        /** art nft operator check */
        _checkApprovedOrOwner(erc721, address(this), artId);

        /** swap */
        _safeTransferFrom(erc721, address(this), mergedId);
        _safeTransferFrom(erc721, _msgSender(), artId);

        emit Swap(contractAddress, artId, mergedId);
    }

    function _addRelation(
        address contractAddress,
        uint256 artId,
        uint256 mergedId
    ) internal {
        _relations[contractAddress][mergedId] = artId + tokenIdOffset;
    }

    function _deleteRelation(address contractAddress, uint256 mergedId)
        internal
    {
        delete _relations[contractAddress][mergedId];
    }

    function _checkRelation(address contractAddress, uint256 mergedId)
        internal
        view
    {
        require(_hasRelation(contractAddress, mergedId), 'no relation');
    }

    function _hasRelation(address contractAddress, uint256 mergedId)
        internal
        view
        returns (bool)
    {
        return _relations[contractAddress][mergedId] != 0;
    }

    function _getRelation(address contractAddress, uint256 mergedId)
        internal
        view
        returns (uint256)
    {
        return _relations[contractAddress][mergedId] - tokenIdOffset;
    }

    /** ERC721 functions */
    function _safeTransferFrom(
        IERC721 erc721,
        address to,
        uint256 tokenId
    ) internal {
        _checkApprovedOrOwner(erc721, address(this), tokenId);
        erc721.safeTransferFrom(erc721.ownerOf(tokenId), to, tokenId);
    }

    function _checkApprovedOrOwner(
        IERC721 erc721,
        address spender,
        uint256 tokenId
    ) internal view {
        require(
            _isApprovedOrOwner(erc721, spender, tokenId),
            'invalid permission'
        );
    }

    function _isApprovedOrOwner(
        IERC721 erc721,
        address spender,
        uint256 tokenId
    ) internal view returns (bool) {
        address owner = erc721.ownerOf(tokenId);
        return (spender == owner ||
            erc721.isApprovedForAll(owner, spender) ||
            erc721.getApproved(tokenId) == spender);
    }

    /** Pausable functions */
    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
}

