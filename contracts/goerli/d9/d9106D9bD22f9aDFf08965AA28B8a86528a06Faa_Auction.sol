    event AuctionCreated(
        uint256 indexed itemId,
        uint256 duration,
        address indexed seller,
        uint256 reservePrice,
        address paymentToken
    );

    event AuctionBid(
        uint256 indexed itemId,
        address indexed bidder,
        uint256 price,
        bool firstBid,
        bool extended
    );

    event AuctionDurationExtended(
        uint256 indexed itemId,
        uint256 duration
    );

    event AuctionEnded(
        uint256 indexed itemId,
        address seller,
        address winner,
        uint256 endingPrice
    );

    event AuctionCanceled(
        uint256 indexed itemId,
        address seller
    );

    event ReservePriceChanged(
        uint256 indexed itemId,
        uint256 reservePrice,
        address paymentToken
    );

    event DurationChanged(
        uint256 indexed itemId,
        uint256 duration
    );

    function newSale(
        address tokenContract,
        uint256 tokenId,
        uint256 duration,
        uint256 reservePrice,
        address paymentToken
    ) external;

    function bid(uint256 itemId, uint256 amount) external payable;

    function endSale(uint256 itemId) external;
}

