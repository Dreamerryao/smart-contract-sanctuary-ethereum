pragma solidity ^0.8.4;

import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./interfaces/IAuction.sol";
import "./Market.sol";
import "./utils/IWETH.sol";

import "hardhat/console.sol";

contract Auction is IAuction, Market {
    using SafeMath for uint256;
    using Address for address;

    struct SaleDetail {
        uint256 itemId;
        uint256 duration;
        uint256 firstBidTime;
        uint256 reservePrice;
        uint256 highestBet;     // current highest bidding price
        address highestBidder;  // current highest bidder
    }

    uint8 public minBidChgPercent = 5; // minimum percentage change of next bid

    uint256 public timeBuffer = 15 * 60;  // 15 minutes extension if a bid made in the last 15 minutes

    address public wethAddress;  // WETH address for payment

    mapping(uint256 => SaleDetail) public saleDetails;  // Recording auction details

    constructor(address registry, address weth) Market(registry) {
        wethAddress = weth;
    }

    /**
        Create a new auction item
     */
    function newSale(
        address tokenContract,
        uint256 tokenId,
        uint256 duration,
        uint256 reservePrice,
        address paymentToken
    ) external {

        uint256 itemId = IRegistry(registry).createMarketItem(
            tokenContract, tokenId, msg.sender, paymentToken, uint16(SaleType.ENGLISH_AUCTION));
        
        newSaleDetail(itemId, duration, reservePrice);

        // Transfer the token to the market contract during the auction
        try IERC721(tokenContract).transferFrom(msg.sender, address(this), tokenId) {
            emit AuctionCreated(
                itemId,
                duration,
                msg.sender,
                reservePrice,
                paymentToken 
            );
        } catch Error(string memory _err) {
            clearItem(itemId);
            delete saleDetails[itemId];
            emit Failure(_err);
            revert("token transfer failed");
        }
    }

    /**
        Bid to an item
     */
    function bid(uint256 itemId, uint256 amount) external payable whenNotPaused {
        MarketItem memory item = getMarketItem(itemId);

        // Check the item is auction sale
        require(item.saleType == uint(SaleType.ENGLISH_AUCTION), "not an english auction item");

        // Do not allow bidding his own item
        require(item.seller != msg.sender, "can't bid to your own item");

        SaleDetail storage detail = saleDetails[itemId];

        // Check if it is first bid(the first bid starts the auction) or the auction is not expired
        require(detail.firstBidTime == 0 || block.timestamp < (detail.firstBidTime + detail.duration),
            "auction expired"
        );

        // Check if the bid is equal or above the reserve price
        require(amount >= detail.reservePrice, "bid amount too low");

        // Check if bidding price is greater than the previous bid plus minimum change
        require(amount > detail.highestBet + (detail.highestBet * minBidChgPercent / 100),
            "need more than minimum bid increase");

        address payable lastBidder = payable(detail.highestBidder);
        
        if(detail.firstBidTime == 0) {
            detail.firstBidTime = block.timestamp;
        } else if(lastBidder != address(0)) {
            sendOutgoingPayment(lastBidder, detail.highestBet, item.paymentToken);
        }

        // receive payment from the outbidder
        receiveIncomingPayment(amount, item.paymentToken);

        detail.highestBet = amount;
        detail.highestBidder = msg.sender;

        bool extended = false;

        // Disable this feature for now
        // bid is made within 15 minutes before the auction ends, extend the duration
        // if(detail.firstBidTime + detail.duration - block.timestamp < timeBuffer) {
        //     uint256 oldDuration = detail.duration;
        //     detail.duration = oldDuration.add(timeBuffer.sub(detail.firstBidTime.add(oldDuration).sub(block.timestamp)));
        //     extended = true;
        // }

        emit AuctionBid(itemId, msg.sender, amount, lastBidder == address(0), extended);
    }

    /**
        End an auction
     */
    function endSale(uint256 itemId) external {
        MarketItem memory item = getMarketItem(itemId);
        require(item.saleType == uint(SaleType.ENGLISH_AUCTION), "not an auction item");

        SaleDetail memory detail = saleDetails[itemId];

        require(detail.firstBidTime != 0, "auction not started");
        require(block.timestamp > (detail.firstBidTime + detail.duration),
            "auction not finished");

        // exchange token and payment
        try IERC721(item.tokenContract).safeTransferFrom(
            address(this),
            detail.highestBidder,
            item.tokenId
        ) {
            uint256 marketFeePermille = IRegistry(registry).getMarketFee();
            uint256 saleFee = detail.highestBet * marketFeePermille / 1000;
            uint256 payout = detail.highestBet - saleFee;
            sendOutgoingPayment(item.seller, payout, item.paymentToken);

            emit AuctionEnded(
                itemId,
                item.seller,
                detail.highestBidder,
                detail.highestBet
            );

            clearItem(itemId);
            delete saleDetails[itemId];
        } catch {
            // transfer failed, return the bidding price
            sendOutgoingPayment(detail.highestBidder, detail.highestBet, item.paymentToken);
            cancelSale(itemId);
            return;
        }
    }

    /**
        Cancel an auction
     */
    function cancelSale(uint256 itemId) public {
        MarketItem memory item = getMarketItem(itemId);

        // item should be on auction sale
        require(item.saleType == uint(SaleType.ENGLISH_AUCTION),"not an auction item");

        // only seller or owner can cancel the auction
        require(msg.sender == item.seller || msg.sender == owner(), "only seller or owner can cancel");

        SaleDetail memory detail = saleDetails[itemId];
        require(detail.firstBidTime == 0, "can't cancel started auction");

        // return the item to the seller
        IERC721(item.tokenContract).safeTransferFrom(address(this), item.seller, item.tokenId);

        emit AuctionCanceled(
            itemId,
            item.seller
        );

        clearItem(itemId);
        delete saleDetails[itemId];
    }

    /**
        Change reserved price for an item
     */
    function changeReservePrice(uint256 itemId, uint256 newPrice) external {
        MarketItem memory item = getMarketItem(itemId);
        // Only seller can change the reserve price
        require(item.seller == msg.sender, "not the seller of the item");

        SaleDetail storage detail = saleDetails[itemId];
        // Once bidding started, the reserve price can't be changed
        require(detail.firstBidTime == 0, "auction started. can't change");

        detail.reservePrice = newPrice;

        emit ReservePriceChanged(
            itemId,
            newPrice,
            item.paymentToken
        );
    }

    /**
        Change auction duration of the item
     */
    function changeAuctionDuration(uint256 itemId, uint256 newDuration) external {
        MarketItem memory item = getMarketItem(itemId);
        // Only seller can change the duration
        require(item.seller == msg.sender, "not the seller of the item");

        SaleDetail storage detail = saleDetails[itemId];
        // Once bidding started, the duration can't be changed
        require(detail.firstBidTime == 0, "auction started. can't change");

        detail.duration = newDuration;

        emit DurationChanged(
            itemId,
            newDuration
        );
    }

    /**
        Receive incoming payment during auction
     */
    function receiveIncomingPayment(uint256 amount, address paymentToken) internal {
        // Payment token is ETH, msg.value should match with the price
        if (paymentToken == address(0)) {     
            require(msg.value == amount, "value not match with amount");
            IWETH(wethAddress).deposit{value: amount}();
        } else {
            IERC20 token = IERC20(paymentToken);
            uint256 beforeBalance = token.balanceOf(address(this));
            require(token.transferFrom(msg.sender, address(this), amount), "payment failed");
            uint256 afterBalance = token.balanceOf(address(this));

            require(beforeBalance + amount == afterBalance, "bid didn't match");
        }
    }

    /**
        Send out payment if there is outbid
     */
    function sendOutgoingPayment(
        address to,
        uint256 amount,
        address paymentToken
    ) internal {
        // if bid token is ETH, return it.
        if (paymentToken == address(0)) {
            IWETH(wethAddress).withdraw(amount);
            (bool success, ) = to.call{value: amount}(new bytes(0));
            // if ETH transfer failed, rewrap ETH and try to send WETH
            if (!success) {
                IWETH(wethAddress).deposit{value: amount}();
                IERC20(wethAddress).transfer(to, amount);
            }
        } else {
            // if payment is ERC20 token
            require(IERC20(paymentToken).transfer(to, amount), "Outgoing payment failed");
        }
    }

    /**
        Create a new auction detail
     */
    function newSaleDetail(
        uint256 itemId, 
        uint256 duration, 
        uint256 reservePrice
    ) private {

        saleDetails[itemId] = SaleDetail(
            itemId,
            duration, 
            0,
            reservePrice,
            0,
            address(0)
        );
    }

    /**
        Set the minimum increase percentage for the next bid
        (this is only for the owner of the contract)
     */
    function setMinBidChgPercent(uint8 minBidPercentage) external onlyOwner {
        minBidChgPercent = minBidPercentage;
    }
}

