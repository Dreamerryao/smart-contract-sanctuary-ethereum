pragma solidity ^0.8.14;

import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";

interface ICabinet {
    function create(
        address initialOwner,
        uint256 totalTokenSupply,
        string calldata tokenUri,
        bytes calldata data
    ) external returns (uint256);
}

interface IBookOfLore {
    function addLoreWithScribe(
        address tokenContract,
        uint256 tokenId,
        uint256 parentLoreId,
        bool nsfw,
        string memory loreMetadataURI
    ) external;
}

contract EnchantedMirror {
    event VictimTrapped(uint256 victim, uint256 miscreant);
    event VictimSaved(uint256 victim, uint256 savior);
    event MirrorBroken(uint256);
    event ShardFound(string code);

    address wizardsAddress;
    address poniesAddress;
    address soulsAddress;
    address warriorsAddress;
    address cabinetAddress;
    address bookOfLoreAddress;
    address managerAddress;
    uint256 private constant MAX_INT = 2**256 - 1;
    uint256 public victimId = MAX_INT;
    address[] private shardHolders;
    address public victimAddress;

    uint256 public claimPrice = 0.02 ether;
    uint256 public TOTAL_SHARDS = 256;
    /**
     * Keeps track of number of breaking - repairing cycles
     */
    uint256 private counter = 0;
    uint256 lastProgress = block.timestamp;
    bool public mirrorBroken = false;
    bytes32 public merkleRoot;
    mapping(uint256 => string) public tokenURIs;
    mapping(uint256 => mapping(string => bool)) public codesUsed;
    uint8 shardsFound = 0;

    constructor(
        address _cabinetAddress,
        address _wizardsAddress,
        address _soulsAddress,
        address _warriorsAddress,
        address _poniesAddress,
        address _bookOfLoreAddress,
        uint256 _totalShards
    ) {
        cabinetAddress = _cabinetAddress;
        wizardsAddress = _wizardsAddress;
        soulsAddress = _soulsAddress;
        warriorsAddress = _warriorsAddress;
        poniesAddress = _poniesAddress;
        bookOfLoreAddress = _bookOfLoreAddress;
        TOTAL_SHARDS = _totalShards;
    }

    /**
     * The manager is the owner of the Mirror token, making the game
     * contract a tradable asset.
     */
    modifier onlyManager() {
        require(
            IERC1155(cabinetAddress).balanceOf(msg.sender, 0) == 1,
            "You are not the owner of the Enchanted Mirror"
        );
        if (managerAddress != msg.sender) {
            managerAddress = msg.sender;
        }
        _;
    }

    /**
     * breaking the mirror starts the game. Can only break a non-broken mirror,
     * a wizard has to be trapped inside, and only a wizard, warrior, soul
     * or pony can break the mirror.
     */
    function breakMirror(uint256 breakerId, address breakerAddress) external {
        require(!mirrorBroken, "mirror is already broken");
        require(
            victimId != MAX_INT,
            "The mirror without a victim trapped is just an illusion"
        );
        string memory assetType = _getAssetType(breakerAddress);
        require(
            keccak256(abi.encodePacked(assetType)) !=
                keccak256(abi.encodePacked("invalid")) &&
                IERC721(breakerAddress).ownerOf(breakerId) == msg.sender,
            "Only creatures of the Runiverse can break the mirror"
        );
        mirrorBroken = true;

        _writeLore(
            breakerId,
            breakerAddress,
            string.concat(
                "@",
                assetType,
                Strings.toString(breakerId),
                " has broken the mirror where ",
                "@",
                assetType,
                Strings.toString(victimId),
                " is trapped, its shards are now scattered across the Runiverse"
            )
        );
        emit MirrorBroken(breakerId);
    }

    /**
     * A broken mirror can only be repaired by a wizard or a soul.
     * The repairer has to collect all shards to repair the mirror.
     * Repairing the mirror frees the trapped victim, and the caller
     * becomes its owner, and the owner of the mirror. The shards
     * are returned to the contract.
     */
    function repairMirror(uint256 repairerId, address repairerAddress) public {
        require(mirrorBroken, "The mirror is not broken");
        address _wizardsAddress = wizardsAddress;
        bool isWizardOrSoul = repairerAddress == _wizardsAddress ||
            repairerAddress == soulsAddress;
        require(
            isWizardOrSoul &&
                IERC721(repairerAddress).ownerOf(repairerId) == msg.sender,
            "Only magicians can fix the mirror"
        );
        uint256 mirrorShardBalance = IERC1155(cabinetAddress).balanceOf(
            address(this),
            1
        );
        uint256 repairerShardBalance = IERC1155(cabinetAddress).balanceOf(
            msg.sender,
            1
        );
        require(
            repairerShardBalance + mirrorShardBalance == TOTAL_SHARDS,
            "You need all the remaining shards to repair the mirror"
        );

        // if any funds are left, transfer to the current manager
        (bool sent, ) = payable(managerAddress).call{
            value: address(this).balance
        }("");
        require(sent, "Failure, ETH not sent");

        // shards go back to the contract
        IERC1155(cabinetAddress).safeTransferFrom(
            msg.sender,
            address(this),
            1,
            repairerShardBalance,
            ""
        );
        // the mirror goes to the wizard who repaired it
        IERC1155(cabinetAddress).safeTransferFrom(
            managerAddress,
            msg.sender,
            0,
            1,
            ""
        );
        IERC721(_wizardsAddress).safeTransferFrom(
            address(this),
            msg.sender,
            victimId
        );

        _writeLore(
            repairerId,
            repairerAddress,
            string.concat(
                "@",
                _getAssetType(repairerAddress),
                Strings.toString(repairerId),
                " has found all the shards of the [Enchanted Mirror](https://cabinetofcuriosities.xyz/mirror), and performed a repair incantation. ",
                "@",
                _getAssetType(victimAddress),
                Strings.toString(victimId),
                " is free again!"
            )
        );

        _writeLore(
            victimId,
            victimAddress,
            string.concat(
                "@",
                _getAssetType(victimAddress),
                Strings.toString(victimId),
                " has been freed form the [Enchanted Mirror](https://cabinetofcuriosities.xyz/mirror) by ",
                "@",
                _getAssetType(repairerAddress),
                Strings.toString(repairerId),
                "!"
            )
        );

        emit VictimSaved(victimId, repairerId);
        mirrorBroken = false;
        victimId = MAX_INT;
        counter++;
    }

    function _writeLore(
        uint256 tokenId,
        address tokenContract,
        string memory text
    ) internal {
        if (bookOfLoreAddress != address(0)) {
            IBookOfLore(bookOfLoreAddress).addLoreWithScribe(
                tokenContract,
                tokenId,
                0,
                false,
                text
            );
        }
    }

    /**
     * Claim shards by finding the codes which scattered across
     * the runiverse. Only broken mirrors have shards, and each
     * code can be used once.
     */
    function claimShard(
        string calldata code,
        bytes32[] calldata proof,
        address founderAddress,
        uint256 founderId
    ) external payable {
        require(mirrorBroken, "The mirror is not broken");
        require(
            _verify(_leaf(code), proof),
            "Don't try to temper with magic you don't fully understand"
        );
        string memory assetType = _getAssetType(founderAddress);
        require(
            keccak256(abi.encodePacked(assetType)) !=
                keccak256(abi.encodePacked("invalid")) &&
                IERC721(founderAddress).ownerOf(founderId) == msg.sender,
            "Only creatures of the Runiverse can find the shards"
        );
        require(
            codesUsed[counter][code] == false,
            "This shard of the mirror has been already found"
        );
        require(msg.value >= claimPrice, "Ether value sent is not sufficient");
        codesUsed[counter][code] = true;
        IERC1155(cabinetAddress).safeTransferFrom(
            address(this),
            msg.sender,
            1,
            1,
            ""
        );

        shardsFound++;
        _writeLore(
            founderId,
            founderAddress,
            string.concat(
                "@",
                _getAssetType(founderAddress),
                Strings.toString(founderId),
                " has found a shard of the [Enchanted Mirror](https://cabinetofcuriosities.xyz/mirror). Now there are ",
                Strings.toString(TOTAL_SHARDS - shardsFound),
                " pieces left missing to save ",
                "@",
                _getAssetType(victimAddress),
                Strings.toString(victimId),
                "!"
            )
        );
        emit ShardFound(code);
    }

    /**
     * A wizard or a soul can use the mirror to trap a victim, which can be a wizard
     * or a warrior. The mirror may be broken, in which case all shards have to be
     * found to repair it and free the trapped victim.
     */
    function trap(
        uint256 _victimId,
        address _victimAddress,
        uint256 _culpritId,
        address _culpritAddress
    ) external onlyManager {
        address _wizardsAddress = wizardsAddress;

        bool _isVictimWizard = _victimAddress == _wizardsAddress;
        bool _isCulpritWizard = _culpritAddress == _wizardsAddress;

        require(
            (_isVictimWizard || _victimAddress == warriorsAddress) &&
                IERC721(_victimAddress).ownerOf(_victimId) == msg.sender,
            "Can only trap a wizard or a warrior"
        );
        require(
            (_isCulpritWizard || _culpritAddress == soulsAddress) &&
                IERC721(_culpritAddress).ownerOf(_culpritId) == msg.sender,
            "Only a wizard or a soul can use the mirror"
        );

        require(victimId == MAX_INT, "There's a victim already trapped");
        IERC721(wizardsAddress).transferFrom(
            msg.sender,
            address(this),
            _victimId
        );
        victimAddress = _victimAddress;
        victimId = _victimId;
        string memory lore = string.concat(
            "@",
            _getAssetType(_culpritAddress),
            Strings.toString(_culpritId),
            " has trapped ",
            "@",
            _getAssetType(_victimAddress),
            Strings.toString(_victimId),
            " in the [Enchanted Mirror](https://cabinetofcuriosities.xyz/mirror)."
        );
        _writeLore(_victimId, _victimAddress, lore);
        _writeLore(_culpritId, _culpritAddress, lore);
        emit VictimTrapped(_victimId, _culpritId);
    }

    function updateOwners(address to) external {
        shardHolders.push(to);
    }

    function getOwners() external view returns (address[] memory) {
        return shardHolders;
    }

    function progressGame() external {
        require(mirrorBroken, "Mirror is not broken");
        require(shardsFound == TOTAL_SHARDS, "Not all shards were found");
        require(
            block.timestamp - lastProgress > 3 days,
            "There was already progress in the last 3 days"
        );

        uint256[] memory ids = new uint256[](1);
        ids[0] = 1;
        uint256[] memory balances = IERC1155(cabinetAddress).balanceOfBatch(
            shardHolders,
            ids
        );
        for (uint256 i = 0; i < shardHolders.length; ++i) {
            if (balances[i] > 0) {
                IERC1155(cabinetAddress).safeTransferFrom(
                    shardHolders[i],
                    address(this),
                    1,
                    1,
                    ""
                );
            }
        }

        lastProgress = block.timestamp;
    }

    function _getAssetType(address contractAddress)
        internal
        view
        returns (string memory)
    {
        return
            contractAddress == wizardsAddress
                ? "wizard"
                : contractAddress == warriorsAddress
                ? "warrior"
                : contractAddress == soulsAddress
                ? "soul"
                : contractAddress == poniesAddress
                ? "pony"
                : "invalid";
    }

    function _leaf(string calldata code) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked(code));
    }

    function _verify(bytes32 leaf, bytes32[] memory proof)
        internal
        view
        returns (bool)
    {
        return MerkleProof.verify(proof, merkleRoot, leaf);
    }

    function setMerkleRoot(bytes32 _merkleRoot) external onlyManager {
        merkleRoot = _merkleRoot;
    }

    function setClaimPrice(uint256 _claimPrice) external onlyManager {
        require(!mirrorBroken, "Cannot set price when mirror is broken");
        claimPrice = _claimPrice;
    }

    function withdraw() external onlyManager {
        (bool sent, ) = payable(msg.sender).call{value: address(this).balance}(
            ""
        );
        require(sent, "Failure, ETH not sent");
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

