pragma solidity ^0.8.15;

import "sgn-v2-contracts/contracts/message/libraries/MessageSenderLib.sol";
import "sgn-v2-contracts/contracts/message/interfaces/IMessageBus.sol";
import "sgn-v2-contracts/contracts/message/interfaces/IMessageReceiverApp.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

import "hardhat/console.sol";

uint8 constant MESSAGE_KIND_ROUNDTRIP = 1;
uint8 constant MESSAGE_KIND_REWARD = 2;

abstract contract IMCeler is IMessageReceiverApp {
    using SafeERC20 for IERC20;

    struct OutgoingMessageWithTransfer {
        address token;
        uint256 amount;
        bytes message;
        uint64 nonce;
    }

    struct IncomingMessageWithTransfer {
        address token;
        uint256 amount;
        bytes message;
        address sender;
    }

    address public peerAddress;
    uint64 public peerChainId;

    uint32 public maxSlippage = 1000000;
    address public messageBus;
    uint64 public celerSendMessageWithTransferNonce;

    mapping (bytes32 => bool) public seenRoundtripId;

    bytes[] public incomingMessageQueue;
    bytes[] public outgoingMessageQueue;
    OutgoingMessageWithTransfer[] public outgoingMessageWithTransferQueue;
    IncomingMessageWithTransfer[] public incomingMessageWithTransferQueue;

    event OutgoingMessageQueued(bytes32 roundtripId);
    event OutgoingMessageSent(bytes32 roundtripId);

    event OutgoingMessageWithTransferQueued(bytes32 roundtripId);
    event OutgoingMessageWithTransferSent(bytes32 roundtripId);

    event OutgoingMessageWithTransferRefund(bytes32 roundtripId, address token, uint256 tokenAmount);
    event OutgoingMessageWithTransferFallback(bytes32 roundtripId, address token, uint256 tokenAmount);

    event IncomingMessageQueued(bytes32 roundtripId);
    event IncomingMessageWithTransferQueued(bytes32 roundtripId);

    // IMCeler inherited classes are responsible for firing events indicating successful processing of incoming messages

    constructor(address _messageBus) {
        messageBus = _messageBus;
    }

    modifier onlyMessageBus() {
        require(msg.sender == messageBus, "MESSAGEBUS");
        _;
    }

    modifier onlyFromPeer(address _peerAddress, uint64 _peerChainId) {
        require(peerAddress == _peerAddress && peerChainId == _peerChainId, "PEER");
        _;
    }

    modifier onlyUnique(bytes memory message) {
        bytes32 roundtripId = abi.decode(message, (bytes32));

        if (seenRoundtripId[roundtripId]) {
            revert("UNIQUE");
        }

        seenRoundtripId[roundtripId] = true;

        _;
    }

    function onMessage(bytes memory message) internal virtual;
    function onMessageWithTransfer(bytes memory message, address token, uint256 amount) internal virtual;

    function executeMessageWithTransfer(
        address sender,
        address token,
        uint256 amount,
        uint64 srcChainId,
        bytes calldata incomingMessage,
        address executor // solhint-disable-line no-unused-vars
    )
        external
        payable
        override
        onlyMessageBus
        onlyFromPeer(sender, srcChainId)
        onlyUnique(incomingMessage)
        returns (ExecutionStatus)
    {
        require(IERC20(token).balanceOf(address(this)) >= amount, "INSUFFICIENT_BRIDGE");

        incomingMessageWithTransferQueue.push(IncomingMessageWithTransfer({
            sender: sender,
            token: token,
            amount: amount,
            message: incomingMessage
        }));

        bytes32 roundtripId = abi.decode(incomingMessage, (bytes32));
        emit IncomingMessageWithTransferQueued(roundtripId);

        return ExecutionStatus.Success;
    }

    function executeMessageWithTransferRefund(
        address token,
        uint256 amount,
        bytes calldata incomingMessage,
        address executor // solhint-disable-line no-unused-vars
    )
        external
        payable
        override
        onlyMessageBus
        onlyUnique(incomingMessage)
        returns (ExecutionStatus)
    {
        bytes32 roundtripId = abi.decode(incomingMessage, (bytes32));
        emit OutgoingMessageWithTransferRefund(roundtripId, token, amount);

        return ExecutionStatus.Success;
    }

    function executeMessageWithTransferFallback(
        address sender,
        address token,
        uint256 amount,
        uint64 srcChainId,
        bytes calldata incomingMessage,
        address executor // solhint-disable-line no-unused-vars
    )
        external
        payable
        override
        onlyMessageBus
        onlyFromPeer(sender, srcChainId)
        onlyUnique(incomingMessage)
        returns (ExecutionStatus)
    {
        bytes32 roundtripId = abi.decode(incomingMessage, (bytes32));
        emit OutgoingMessageWithTransferFallback(roundtripId, token, amount);

        return ExecutionStatus.Success;
    }

    function executeMessage(
        address sender,
        uint64 srcChainId,
        bytes calldata incomingMessage,
        address executor // solhint-disable-line no-unused-vars
    )
        external
        payable
        override
        onlyMessageBus
        onlyFromPeer(sender, srcChainId)
        onlyUnique(incomingMessage)
        returns (ExecutionStatus)
    {
        incomingMessageQueue.push(incomingMessage);

        bytes32 roundtripId = abi.decode(incomingMessage, (bytes32));
        emit IncomingMessageQueued(roundtripId);

        return ExecutionStatus.Success;
    }

    function queueSendMessage(bytes memory message) internal {
        outgoingMessageQueue.push(message);

        bytes32 roundtripId = abi.decode(message, (bytes32));
        emit OutgoingMessageQueued(roundtripId);
    }

    function queueSendMessageWithTransfer(address token, uint256 amount, bytes memory message) internal {
        outgoingMessageWithTransferQueue.push(OutgoingMessageWithTransfer({
            token: token,
            amount: amount,
            message: message,
            nonce: celerSendMessageWithTransferNonce
        }));

        celerSendMessageWithTransferNonce++;

        bytes32 roundtripId = abi.decode(message, (bytes32));
        emit OutgoingMessageWithTransferQueued(roundtripId);
    }

    function tossIncomingMessageQueue() public {
        require(incomingMessageQueue.length > 0, "EMPTY");

        for (uint i=0; i<incomingMessageQueue.length; i++) {
            onMessage(incomingMessageQueue[i]);
        }

        delete incomingMessageQueue;
    }

    function tossIncomingMessageWithTransferQueue() public {
        require(incomingMessageWithTransferQueue.length > 0, "EMPTY");

        for (uint i=0; i<incomingMessageWithTransferQueue.length; i++) {
            onMessageWithTransfer(incomingMessageWithTransferQueue[i].message, incomingMessageWithTransferQueue[i].token, incomingMessageWithTransferQueue[i].amount);
        }

        delete incomingMessageWithTransferQueue;
    }

    function tossOutgoingMessageQueue() public payable {
        require(outgoingMessageQueue.length > 0, "EMPTY");

        uint256 totalFeeUsed = 0;
        for (uint i=0; i<outgoingMessageQueue.length; i++) {
            uint256 fee = IMessageBus(messageBus).calcFee(outgoingMessageQueue[i]);
            require(address(this).balance >= fee, "CELER_FEE");

            totalFeeUsed += fee;

            MessageSenderLib.sendMessage(peerAddress, peerChainId, outgoingMessageQueue[i], messageBus, fee);

            bytes32 roundtripId = abi.decode(outgoingMessageQueue[i], (bytes32));
            emit OutgoingMessageSent(roundtripId);
        }

        delete outgoingMessageQueue;

        payable(msg.sender).transfer(msg.value - totalFeeUsed);
    }

    function tossOutgoingMessageWithTransferQueue() public payable {
        require(outgoingMessageWithTransferQueue.length > 0, "EMPTY");

        uint256 totalFeeUsed = 0;
        for (uint i=0; i<outgoingMessageWithTransferQueue.length; i++) {
            uint256 fee = IMessageBus(messageBus).calcFee(outgoingMessageWithTransferQueue[i].message);
            require(address(this).balance >= fee, "CELER_FEE");

            totalFeeUsed += fee;

            MessageSenderLib.sendMessageWithTransfer(
                peerAddress,
                outgoingMessageWithTransferQueue[i].token,
                outgoingMessageWithTransferQueue[i].amount,
                peerChainId,
                outgoingMessageWithTransferQueue[i].nonce,
                maxSlippage,
                outgoingMessageWithTransferQueue[i].message,
                MsgDataTypes.BridgeSendType.Liquidity,
                messageBus,
                fee
            );

            bytes32 roundtripId = abi.decode(outgoingMessageWithTransferQueue[i].message, (bytes32));
            emit OutgoingMessageWithTransferSent(roundtripId);
        }

        delete outgoingMessageWithTransferQueue;

        payable(msg.sender).transfer(msg.value - totalFeeUsed);
    }

    function executeMessageWithTransferForce(address token, uint256 amount, bytes calldata message) public {
        onMessageWithTransfer(message, token, amount);
    }

    function executeMessageForce(bytes calldata message) public {
        onMessage(message);
    }

    function clearExecuteMessageQueue() public {
        delete incomingMessageQueue;
    }

    function clearSendMessageQueue() public {
        delete outgoingMessageQueue;
    }

    function clearSendMessageWithTransferQueue() public {
        delete outgoingMessageWithTransferQueue;
    }

    function clearExecuteMessageWithTransferQueue() public {
        delete incomingMessageWithTransferQueue;
    }

    function setMessageBus(address _messageBus) public {
        messageBus = _messageBus;
    }

    function setPeer(address _peerAddress, uint64 _peerChainId) public {
        peerAddress = _peerAddress;
        peerChainId = _peerChainId;
    }

    function setMaxSlippage(uint32 _maxSlippage) public {
