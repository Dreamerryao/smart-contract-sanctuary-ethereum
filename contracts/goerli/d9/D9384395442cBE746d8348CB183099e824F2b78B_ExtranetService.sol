pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

import "./ExtranetToken.sol";
import "./CollectTokens.sol";
import "./Shutdown.sol";
import "./IMCeler.sol";
import "./interfaces/IQueueEntry.sol";
import "./Locking.sol";

import "hardhat/console.sol";

contract ExtranetService is IMCeler, CollectTokens, Shutdown {
    using SafeERC20 for IERC20;

    bool public isInvestPaused;
    bool public isInvestQueuePaused;
    bool public isWithdrawPaused;
    bool public isWithdrawQueuePaused;

    IERC20 public quoteToken;
    ExtranetToken public immutable extranetToken;

    uint16 public maxQueueLength = 10;

    uint256 public investMinAmount;
    uint256 public investQueueMinAmount;
    uint256 public withdrawMinAmount;

    QueueEntry[] public investQueue;
    QueueEntry[] public withdrawQueue;

    uint256 private roundtripNonce = 1;
    bytes32 public withdrawRoundtripId;
    bytes32 public investRoundtripId;

    Locking public locking;
    uint256 public minLockedPower;
    uint256 public minLockedPowerPerQuoteTokenAmount;

    event InvestQueued(bytes32 roundtripId, address sender, uint256 amount);
    event InvestSent(bytes32 roundtripId, uint256 amount);
    event InvestFinished(bytes32 roundtripId);

    event WithdrawQueued(bytes32 roundtripId, address sender, uint256 amount);
    event WithdrawSent(bytes32 roundtripId, uint256 amount);
    event WithdrawFinished(bytes32 roundtripId);

    event RewardReceived(bytes32 rewardMessageId, uint256 amount);

    constructor(
        address _quoteToken,
        address _extranetToken,
        address _messageBus
    )
    IMCeler(_messageBus)
    {
        quoteToken = IERC20(_quoteToken);
        extranetToken = ExtranetToken(_extranetToken);
        generateInvestRoundtripId();
        generateWithdrawRoundtripId();
    }

    modifier whenNotPaused(bool whatExactly) {
        require(!whatExactly, "PAUSED");
        _;
    }

    modifier hasMinLockedPower(uint256 quoteTokenAmount) {
        if (address(locking) == address(0) || minLockedPower == 0) {
            _;
            return;
        }

        uint256 lockedPower = locking.powerBy(msg.sender);
        uint256 requiredLockedPower = minLockedPower * quoteTokenAmount / minLockedPowerPerQuoteTokenAmount;
        require(lockedPower >= requiredLockedPower, "MIN_LOCKED");

        _;
    }

    function invest(uint256 amount)
        public
        whenNotPaused(isInvestPaused)
        hasMinLockedPower(amount)
    {
        require(investQueue.length < maxQueueLength, "QUEUE");
        require(amount >= investMinAmount && amount > 0, "MIN_AMOUNT");

        quoteToken.safeTransferFrom(msg.sender, address(this), amount);

        investQueue.push(QueueEntry({
            account: msg.sender,
            amount: amount
        }));

        emit InvestQueued(investRoundtripId, msg.sender, amount);
    }

    function withdraw(uint256 amount)
        public
        whenNotPaused(isWithdrawPaused)
    {
        require(withdrawQueue.length < maxQueueLength, "QUEUE");
        require(amount > 0 && (amount >= withdrawMinAmount || extranetToken.balanceOf(msg.sender) == amount), "MIN_AMOUNT");

        extranetToken.burnFrom(msg.sender, amount);

        withdrawQueue.push(QueueEntry({
            account: msg.sender,
            amount: amount
        }));

        emit WithdrawQueued(withdrawRoundtripId, msg.sender, amount);
    }

    function tossInvestQueue() public whenNotPaused(isInvestQueuePaused) {
        require(investQueue.length > 0, "EMPTY");

        uint256 investQueueTotalAmount = _calculateQueueTotalAmount(investQueue);

        require(investQueueTotalAmount >= investQueueMinAmount, "MIN_AMOUNT");

        bytes memory message = abi.encode(investRoundtripId, MESSAGE_KIND_ROUNDTRIP, investQueueTotalAmount, investQueue);
        queueSendMessageWithTransfer(address(quoteToken), investQueueTotalAmount, message);

        emit InvestSent(investRoundtripId, investQueueTotalAmount);

        delete investQueue;

        generateInvestRoundtripId();
    }

    function tossWithdrawQueue() public whenNotPaused(isWithdrawQueuePaused) {
        require(withdrawQueue.length > 0, "EMPTY");

        uint256 withdrawQueueTotalAmount = _calculateQueueTotalAmount(withdrawQueue);

        bytes memory message = abi.encode(withdrawRoundtripId, MESSAGE_KIND_ROUNDTRIP, withdrawQueueTotalAmount, withdrawQueue);
        queueSendMessage(message);

        emit WithdrawSent(withdrawRoundtripId, withdrawQueueTotalAmount);

        delete withdrawQueue;

        generateWithdrawRoundtripId();
    }

    function onMessage(bytes memory message) override internal {
        (, uint8 messageKind) = abi.decode(message, (bytes32, uint8));

        if (messageKind == MESSAGE_KIND_ROUNDTRIP) {
            onMessageRoundtrip(message);

        } else if (messageKind == MESSAGE_KIND_REWARD) {
            onMessageReward(message);

        } else {
            revert("MESSAGE_KIND");
        }
    }

    function onMessageReward(bytes memory message) internal {
        (bytes32 rewardMessageId, , uint256 rewardAmount) = abi.decode(message, (bytes32, uint8, uint256));

        extranetToken.onReward(rewardAmount);

        emit RewardReceived(rewardMessageId, rewardAmount);
    }

    function onMessageRoundtrip(bytes memory message) internal {
        (bytes32 incomingRoundtripId, , QueueEntry[] memory queue) = abi.decode(message, (bytes32, uint8, QueueEntry[]));

        for (uint i=0; i<queue.length; i++) {
            QueueEntry memory entry = queue[i];
            extranetToken.mintTo(entry.account, entry.amount);
        }

        emit InvestFinished(incomingRoundtripId);
    }

    function onMessageWithTransfer(bytes memory message, address token, uint256 amount) override internal {
        require(token == address(quoteToken), "QUOTE");

        (, uint8 messageKind) = abi.decode(message, (bytes32, uint8));
        require(messageKind == 1, "MESSAGE_KIND");

        (bytes32 incomingRoundtripId, , uint256 quoteTokenAmount, QueueEntry[] memory incomingQueue) = abi.decode(message, (bytes32, uint8, uint256, QueueEntry[]));

        for (uint256 i=0; i<incomingQueue.length; i++) {
            QueueEntry memory entry = incomingQueue[i];
            quoteToken.safeTransfer(entry.account, entry.amount * amount / quoteTokenAmount);
        }

        emit WithdrawFinished(incomingRoundtripId);
    }

    function setLimits(uint16 _maxQueueLength, uint256 _investMinAmount, uint256 _investQueueMinAmount, uint256 _withdrawMinAmount) public {
        maxQueueLength = _maxQueueLength;
        investMinAmount = _investMinAmount;
        investQueueMinAmount = _investQueueMinAmount;
        withdrawMinAmount = _withdrawMinAmount;
    }

    function setPauses(
        bool _isInvestPaused,
        bool _isExecuteInvestPaused,
        bool _isWithdrawPaused,
        bool _isExecuteWithdrawPaused
    ) public {
        isInvestPaused = _isInvestPaused;
        isInvestQueuePaused = _isExecuteInvestPaused;
        isWithdrawPaused = _isWithdrawPaused;
        isWithdrawQueuePaused = _isExecuteWithdrawPaused;
    }

    function setMinLockedPower(uint256 _minLockedPower, uint256 _minLockedPowerPerQuoteTokenAmount) public {
        require(address(locking) != address(0), "LOCKING_CONTRACT");
        minLockedPower = _minLockedPower;
        minLockedPowerPerQuoteTokenAmount = _minLockedPowerPerQuoteTokenAmount;
    }

    function setLockingContractAndMinLockedPower(address _locking, uint256 _minLockedPower, uint256 _minLockedPowerPerQuoteTokenAmount) public {
        locking = Locking(_locking);
        minLockedPower = _minLockedPower;
        minLockedPowerPerQuoteTokenAmount = _minLockedPowerPerQuoteTokenAmount;
    }

    function shutdown() public override {
        selfdestruct(payable(msg.sender));
    }

    function generateInvestRoundtripId() internal {
        investRoundtripId = keccak256(abi.encode(1, block.timestamp, block.number, address(this), block.chainid, roundtripNonce)); // solhint-disable-line not-rely-on-time
        roundtripNonce++;
    }

    function generateWithdrawRoundtripId() internal {
        withdrawRoundtripId = keccak256(abi.encode(2, block.timestamp, block.number, address(this), block.chainid, roundtripNonce)); // solhint-disable-line not-rely-on-time
        roundtripNonce++;
    }

    function _calculateQueueTotalAmount(QueueEntry[] memory queue) internal pure returns (uint256 total) {
        total = 0;
        for (uint i=0; i<queue.length; i++) {
            total += queue[i].amount;
        }
    }
}

