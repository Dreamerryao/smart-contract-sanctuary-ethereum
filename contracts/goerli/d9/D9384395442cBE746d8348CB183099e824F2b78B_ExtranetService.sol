pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

uint256 constant ACC_INCENT_PRECISION = 1e12;

contract Locking {
    using SafeERC20 for IERC20;

    struct UnlockData {
        uint256 amount;
        uint256 unlockAfter;
    }

    mapping (address => uint256) public lockedBy;

    mapping (address => UnlockData[]) public unlockData;

    uint32 public immutable durationSeconds;
    IERC20 public immutable token;

    uint256 public immutable powerPrecision;

    constructor(address _token, uint32 _durationSeconds, uint256 _powerPrecision) {
        durationSeconds = _durationSeconds;
        token = IERC20(_token);
        powerPrecision = _powerPrecision;
    }

    function lock(uint256 amount) public {
        require(amount > 0, "zero");

        unlockData[msg.sender].push(UnlockData({
            amount: amount,
            unlockAfter: block.timestamp + durationSeconds
        }));

        lockedBy[msg.sender] += amount;

        token.safeTransferFrom(msg.sender, address(this), amount);
    }

    function unlockable(address account) public view returns (uint256) {
        UnlockData[] storage userUnlockData = unlockData[account];

        uint256 amount = 0;

        for (uint i = 0; i < userUnlockData.length; i++) {
            UnlockData memory entry = userUnlockData[i];

            if (block.timestamp >= entry.unlockAfter) {
                amount += entry.amount;
            }
        }

        return amount;
    }

    function powerBy(address account) public view returns (uint256) {
        return lockedBy[account] * durationSeconds / powerPrecision;
    }

    function unlock(uint256 amount) public {
        require(amount > 0, "AMOUNT_ZERO");

        require(lockedBy[msg.sender] >= amount, "INSUFFICIENT_AMOUNT");

        UnlockData[] storage userUnlockData = unlockData[msg.sender];

        uint256 unlockableAmount = 0;

        uint i = userUnlockData.length;
        while (i > 0) {
            i--;

            UnlockData memory entry = userUnlockData[i];

            if (block.timestamp < entry.unlockAfter) {
                continue;
            }

            unlockableAmount += entry.amount;

            if (i == userUnlockData.length-1) {
                userUnlockData.pop();

            } else {
                userUnlockData[i] = userUnlockData[userUnlockData.length-1];
                userUnlockData.pop();
            }
        }

        require(unlockableAmount >= amount, "LOCKED_AMOUNT");

        if (unlockableAmount > amount) {
            uint256 relockAmount = unlockableAmount - amount;

            unlockData[msg.sender].push(UnlockData({
                amount: relockAmount,
                unlockAfter: 0
            }));
        }

        lockedBy[msg.sender] -= amount;

        token.safeTransfer(msg.sender, amount);
    }
}

