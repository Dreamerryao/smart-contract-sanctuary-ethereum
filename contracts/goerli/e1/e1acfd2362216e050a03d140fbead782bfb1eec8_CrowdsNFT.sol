    pragma solidity ^0.8.7;
    
    contract CrowdsNFT is ERC721A, Ownable {
    using Strings for uint256;


  string private uriPrefix  ="https://api.crowds.app/getNFTMetadata/crowds/";
  
 

  
    uint public presaleCost = 0.00015 ether;
    uint public publicsaleCost = 0.2 ether;
   

  uint16 public constant maxSupply = 5000;
  uint8 public maxMintAmountPerTx = 50;
                                                             
 
  bool public paused = true;
    bool public presalePaused  = true; 
  
  

  constructor() ERC721A("Crowds NFT", "CRWDS") {
  }

  
 
  function mint(uint16 _mintAmount) external payable  {
     uint16 totalSupply = uint16(totalSupply());
    require(totalSupply + _mintAmount <= maxSupply, "Exceeds max supply.");
    require(_mintAmount <= maxMintAmountPerTx, "Exceeds max  nft per transaction.");

       require(!paused, "Sale has not started yet");
   
       require(msg.value >= publicsaleCost * _mintAmount, "Insufficient funds!");
   

    _safeMint(msg.sender , _mintAmount);
     
     delete totalSupply;
     delete _mintAmount;
  }
  function presaleMint(uint16 _mintAmount) external payable  {
     uint16 totalSupply = uint16(totalSupply());
   
    require(_mintAmount <= maxMintAmountPerTx, "Exceeds max  nft per transaction.");

       require(!presalePaused, "Sale has not started yet");
   
       require(msg.value >= presaleCost * _mintAmount, "Insufficient funds!");
   

    _safeMint(msg.sender , _mintAmount);
    if(totalSupply + _mintAmount >= 500)
    {
        presalePaused = true;
    }
     
     delete totalSupply;
     delete _mintAmount;
  }
  
  function Reserve(uint16 _mintAmount, address _receiver) external onlyOwner {
     uint16 totalSupply = uint16(totalSupply());
    require(totalSupply + _mintAmount <= maxSupply, "Exceeds max supply.");
     _safeMint(_receiver , _mintAmount);
     delete _mintAmount;
     delete _receiver;
     delete totalSupply;
  }

 


   
  function tokenURI(uint256 _tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(
      _exists(_tokenId),
      "ERC721Metadata: URI query for nonexistent token"
    );
    
  

    

    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, _tokenId.toString()))
        : "";
  }
 
  
 



  function setUriPrefix(string memory _uriPrefix) external onlyOwner {
    uriPrefix = _uriPrefix;
  }



  function setPaused() external onlyOwner {
    paused = !paused;
   
  }

   function setPreSalePaused() external onlyOwner {
    presalePaused = !presalePaused;
   
  }

  function setpreSaleCost(uint _cost) external onlyOwner{
      presaleCost = _cost;

  }
  
   function setpublicSaleCost(uint _cost) external onlyOwner{
      publicsaleCost = _cost;

  }



  function setMaxMintAmountPerTx(uint8 _maxtx) external onlyOwner{
      maxMintAmountPerTx = _maxtx;

  }

 

  function withdraw() external onlyOwner {
  uint _balance = address(this).balance;
     payable(msg.sender).transfer(_balance ); 
       
  }


  function _baseURI() internal view  override returns (string memory) {
    return uriPrefix;
  }
}