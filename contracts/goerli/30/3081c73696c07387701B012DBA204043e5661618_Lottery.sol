// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "hardhat/console.sol";


contract Lottery is Ownable, VRFConsumerBase{
    using SafeMath for uint256;
    // Random number generation with ChainLink VRF
    bytes32 internal keyHash;
    uint256 internal fee;
    uint256 public randomResult;
    bool public lotteryIsActive;

    struct entryStruct{
        uint256 sentEtherValue;
        uint guessNumber;
        bool withdrawed;
    }
    mapping(address => entryStruct) private entries; // Lottery ticket buyers information
    address[] private entryAddresses; // Lottery ticket buyers addresses
    address[] private winnerAddresses; // Lottery winners in the current round
    uint private winningNumber; // Random generated number with ChainLink VRF
    uint256 private contractBalance; // Contract balance
    uint256 private totalBalance; // Total balance which can be distributed among the winners
    uint256 private winningBalance; // Sum of ethers of those who won the current round
    uint256 private winningPercentage; // The percentage of the contract balance which can be distributed among the winners
    uint256 private ownerPercentage; // The percentage of the contract balance which can be sent to the owner

    constructor() VRFConsumerBase(
        0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D, // VRF Coordinator
        0x326C977E6efc84E512bB9C30f76E30c160eD06FB  // LINK Token
    )
    {
        keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
        fee = 1 * 10 ** 18;
        lotteryIsActive = true;
        winningPercentage = 70;
        ownerPercentage = 100 - winningPercentage;
    }

    /** 
    * Requests randomness from a user-provided seed
    */
    function getRandomNumber() public returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) > fee, "Not enough LINK - fill contract with faucet");
        return requestRandomness(keyHash, fee);
    }

    /**
    * Callback function used by VRF Coordinator
    */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        randomResult = randomness;
    }

    /**
    * Pause sale if active, make active if paused
    */
    function flipLotteryState() external onlyOwner {
        lotteryIsActive = !lotteryIsActive;
    }

    /**
    * Source: https://muens.io/how-to-calculate-percentages-in-solidity
    */
    function calculatePercentage(uint256 amount, uint256 bps) public pure returns (uint256) {
        require((amount.mul(bps)) >= 10_000);
        return amount.mul(bps).div(10_000);
    }

    /**
    * Setter function of the winning amount of the contract balance.
    */
    function setWinningPercentage(uint _percentage) external onlyOwner {
        winningPercentage = _percentage;
        ownerPercentage = 100 - winningPercentage;
    }

    /**
    * Lottery status query
    */
    function getLotteryStatus() public view returns(bool){
        return lotteryIsActive;
    }

    /**
    * Get lottery ticket buyers amount
    */
    function getLotteryTicketCount() public view returns(uint count) {
        return entryAddresses.length;
    }

    /**
    * Get guessed number based on the queried address
    */
    function getEntryGuessedNumber(address _address) public view returns(uint){
        return entries[_address].guessNumber;
    }

    /**
    * Check if address withdrawed already
    */
    function getEntryWithdrawInfo(address _address) public view returns(bool){
        return entries[_address].withdrawed;
    }

    /**
    * Returns true if the wallet is in the entries map
    */
    function isInTheEntries(address _address) public view returns(bool) {
        if (entries[_address].sentEtherValue > 0){
            return true;
        }
        return false;
    }

    /**
    * Delete entry element from mapping
    */
    function removeAddress(address _address) private {
        delete entries[_address];
    }

    /**
    * Lottery ticket buy function
    */
    function buyEntry(uint number) external payable {
        // CHECK
        require(lotteryIsActive, "Lottery is not active!");
        require(1 <= number && number <= 20, "Guessed number is not correct! Please give an integer number between 1 and 20!");
        require(0 < msg.value, "Ether value sent must be bigger than 0!");
        require(!isInTheEntries(msg.sender), "Address already bought an entry!");
        // EFFECT
        // ACTION
        entryAddresses.push(msg.sender);
        entries[msg.sender] = (entryStruct({
            sentEtherValue: msg.value,
            guessNumber: number,
            withdrawed: false
        }));
    }

    /**
    * Finalize function based on percentages and winners
    */
    function finalize() public onlyOwner {
        // CHECK
        contractBalance = address(this).balance;
        require(contractBalance != 0, "Insufficent contract balance, cannot withdraw anything!");
        // EFFECT
        lotteryIsActive = false;
        uint lotteryCount = getLotteryTicketCount();
        // ACTION
        winningNumber = (uint(getRandomNumber()) % 20) + 1;
        totalBalance = calculatePercentage(contractBalance, winningPercentage.mul(100));
        uint256 ownerBalance = calculatePercentage(contractBalance, ownerPercentage.mul(100));
        _withdraw(msg.sender, ownerBalance);
        // winning distribution
        for(uint i = 0; i < lotteryCount; i++){
            address entryAddr = entryAddresses[i];
            uint guessedNumber = getEntryGuessedNumber(entryAddr);
            if (isInTheEntries(entryAddr) && guessedNumber == winningNumber){
                winnerAddresses.push(entryAddr);
                winningBalance += calculatePercentage(entries[entryAddr].sentEtherValue, winningPercentage.mul(100));
            }
        }
        if (winnerAddresses.length == 0){
            // NO WINNER CASE
            for(uint i = 0; i < lotteryCount; i++){
                address entryAddr = entryAddresses[i];
                if (!entries[entryAddr].withdrawed){
                    entries[entryAddr].withdrawed = true;
                    uint256 entryValue = calculatePercentage(entries[entryAddr].sentEtherValue, winningPercentage.mul(100)); // wei, based on winning percentage
                    _withdraw(entryAddr, entryValue);
                }
            }
        }else{
            if (winnerAddresses.length == 1){
                // ONE WINNER CASE
                address entryAddr = winnerAddresses[0];
                if (!entries[entryAddr].withdrawed){
                    entries[entryAddr].withdrawed = true;
                    _withdraw(entryAddr, totalBalance);
                }
            } else {
                for(uint i = 0; i < winnerAddresses.length; i++){
                    // MULTIPLE WINNER CASE
                    address entryAddr = winnerAddresses[i];
                    if (!entries[entryAddr].withdrawed){
                        entries[entryAddr].withdrawed = true;
                        uint256 entryValue = calculatePercentage(entries[entryAddr].sentEtherValue, winningPercentage.mul(100)); // wei, based on winning percentage
                        uint256 overallPercentage = entryValue.mul(10_000).div(winningBalance); // get percentage for the total amount
                        _withdraw(entryAddr, calculatePercentage(totalBalance, overallPercentage));
                    }
                }
            }
        }
        _reStart();
    }

    /**
    * Lottery restart function
    */
    function _reStart() private {
        // CHECK
        require(!lotteryIsActive, "Lottery is active! Failed to restart the lottery!");
        // EFFECT
        lotteryIsActive = true;
        winningBalance = 0;
        totalBalance = 0;
        winningNumber = 0;
        uint lotteryCount = getLotteryTicketCount();
        // ACTION
        for(uint i = 0; i < lotteryCount; i++){
            address entryAddr = entryAddresses[i];
            if (isInTheEntries(entryAddr)){
                removeAddress(entryAddr);
            }
        }
        delete entryAddresses;
        delete winnerAddresses;
    }

    /**
    * Private withdrawal function
    */  
    function _withdraw(address _address, uint256 _amount) private {
        (bool success, ) = _address.call{ value: _amount }("");
        require(success, "Failed to widthdraw Ether");
    }
}

