interface IWETHGateway {
    function depositETH(
        address lendingPool,
        address onBehalfOf,
        uint16 referralCode
    ) external payable;

    function withdrawETH(
        address lendingPool,
        uint256 amount,
        address onBehalfOf
    ) external;
}

error EtherTransferFailed();
error NeedsMoreThanZero();
error ZeroAddress();

contract StakingEthersContract is Ownable, ReentrancyGuard {

    string public constant name = "Staking ethers smart contract";

        //IWETHGateway interface for the Goerli testnet
   // IWETHGateway public iWethGateway = IWETHGateway(0xd5B55D3Ed89FDa19124ceB5baB620328287b915d);
    IWETHGateway immutable iWethGateway;

    //Lending Pool address for the Aave (v3) lending pool on Goerli testnet
    /// Retrieve LendingPool address
  //  LendingPoolAddressesProvider provider = LendingPoolAddressesProvider(address(0xc4dCB5126a3AfEd129BC3668Ea19285A9f56D15D)); // mainnet address, for other addresses: https://docs.aave.com/developers/deployed-contracts/deployed-contract-instances 
  //  LendingPool lendingPool = LendingPool(provider.getLendingPool());
    address immutable lendingPoolAddress; // = 0x368EedF3f56ad10b9bC57eed4Dac65B26Bb667f6;
    //Contract Address for the aWeth tokens generated after depositing ETH to keep track of the amount deposited in lending pool
    // address public constant aWethAddress = 0x27B4692C93959048833f40702b22FE3578E77759;
    address immutable aWethAddress;

    // time constant in seconds, lock period
    uint256 immutable lockPeriod; 

    // current number of participants
    // za sada nepotrebno
    uint256 private stakersCount;
    // current amount of deposited balance
    uint256 private totalStakedBalance;
    // token reward pool of native ETH
    // generated from early withdrawal
    // penalties
    // uint256 private totalRewardSupply;
    uint private constant granularPrecision = 1e12;
    // Early withdrawal penalties when there are
    // no other users left in the pool to be distributed.
    // It's allocated to the contract owner!
    uint256 private unallocatedRewards;

    // info about deposit between two withdrawals
    struct stakeInfo {        
        uint256 depositTime; // uint64 
        // uint256 amount;         
        uint256 curStakedAmount;  
        uint256 curWithdrawalLength;     
    }

    mapping(address => stakeInfo[]) private stakers;

    struct withdrawInfo {  
        // address account;      
        uint256 withdrawTime;        
        uint256 penaltyAmount; 
        uint256 curStakedBalance;   
        uint256 curRewardFactor;   
    }

    // time ordered withdrawal informations
    mapping(uint => withdrawInfo) private withdrawals;
    uint256 private withdrawalsLength;

    modifier moreThanZero(uint256 amount) {
        if (amount == 0) {
            revert NeedsMoreThanZero();
        }
        _;
    }

    modifier isZeroAddress(address _address) {
        if (_address == address(0)) {
            revert ZeroAddress();
        }
        _;
    }

    event Staked(address indexed from, uint256 amount, uint256 time);
    event Unstaked(address indexed from, uint256 amount, uint256 inRewardPool, uint256 time);
    event OwnerIncomeWithdrawn(address indexed from, uint256 amount, uint256 time);
    event OwnerGotRewards(address indexed from, address indexed owner, uint256 amount, uint256 time);
    event OwnerRewardsWithdrawn(address indexed from, uint256 amount, uint256 time);
    

    constructor(uint256 _lockPeriod, address _wETHGateway, address _aWETH, address _lendingPoolAddress) 
        moreThanZero(_lockPeriod) 
        isZeroAddress(_wETHGateway)
        isZeroAddress(_aWETH)
        isZeroAddress(_lendingPoolAddress)
    {
        lockPeriod = _lockPeriod;

        iWethGateway = IWETHGateway(_wETHGateway);
        aWethAddress = _aWETH;
        lendingPoolAddress = _lendingPoolAddress;
    }    

    // Override Ownable renounceOwnership function
    function renounceOwnership() public view override onlyOwner {
        require(false, "Can't renounce contract ownership.");
    }

    // mozda zabraniti owner-u da deposit i ucestvuje u staking
    // there are no constraints for minimal deposits in ETH
    function depositETH() public payable moreThanZero(msg.value) nonReentrant {
        // assert(stakers[msg.sender].amount + msg.value >= stakers[msg.sender].amount);
        
        uint t_now = block.timestamp;
        // update the participant staking info

        // number of user deposits
        uint numOfDeposits = stakers[msg.sender].length;
        // previous staked amount in the pool
        uint prevAmount = 0;
        uint prevWithdrawalLen = 0;
        if(numOfDeposits > 0){
            // get the user amount of deposits until now
            prevAmount = stakers[msg.sender][numOfDeposits-1].curStakedAmount;
            prevWithdrawalLen = stakers[msg.sender][numOfDeposits-1].curWithdrawalLength;
        }

        // ako je od prethodnog deposita bilo withdrawals prosiri array
        // ako nije samo update uradi 
        if(prevWithdrawalLen != withdrawalsLength || numOfDeposits == 0) {
            // expand stake info array
            stakers[msg.sender].push(stakeInfo(t_now, prevAmount+msg.value, withdrawalsLength));
        } else {
            // update time used for lock period
            stakers[msg.sender][numOfDeposits-1].depositTime = t_now;
            // cumulated amount is stored
            stakers[msg.sender][numOfDeposits-1].curStakedAmount = prevAmount+msg.value;
       //   stakers[msg.sender][numOfDeposits-1].curWithdrawalLength = withdrawalsLength;
        }

        // update the staking global info
        totalStakedBalance += msg.value;
        if(numOfDeposits == 0) {
            // new user
            stakersCount++;
        }

        iWethGateway.depositETH{value: msg.value}(lendingPoolAddress, address(this), 0);
       
       // iWethGateway.depositETH(lendingPoolAddress, address(this), 0);

        emit Staked(msg.sender, msg.value, t_now);
    }    

    function withdrawETH() external nonReentrant {
        // number of user deposits
        uint numOfDeposits = stakers[msg.sender].length;
        uint t_now = block.timestamp;

        require(numOfDeposits != 0, "You are not participant!");
               
        uint lastDepositIndex = numOfDeposits-1;
        uint256 t_diff = t_now - stakers[msg.sender][lastDepositIndex].depositTime;
    // int t_diff = min((t_now - stakers[msg.sender].depositTime), T);
        // sta ako je t_now < depositTime  ?!
        // pogledaj sta se desava sa node-ovima i bliskim transakcijama

   // uint private constant granularPrecision = 1;
    // granulacija je na 1%, a kolektovani 'dust' ce biti rasporedjen na reward za ostale usere u staking pool-u
        
        uint256 stakedAmount = stakers[msg.sender][lastDepositIndex].curStakedAmount;

        withdrawDepositsFromAAVE(stakedAmount);

        uint256 amountToWithdraw = stakedAmount;
        uint256 amountToRewardPool = 0;

        if(t_diff < lockPeriod){
            // there are early withdrawal penalties.
            // % of half total staked amount

            // ako nije ispunjen uslov, korisnik nece uspeti da povuce vise od pola deposit amount-a
            // mozda ovaj uslov staviti prilikom depositovanja
            // sta ako je t_diff == 0
            assert(stakedAmount*t_diff>=2*lockPeriod);
            // require((_amount/100)*100==_amount, 'too small');

            uint256 amountUnlocked = stakedAmount * t_diff / (2*lockPeriod);
            // half amount is unlocked anyway + time based unlocked amount 
            amountToWithdraw =stakedAmount/2 + amountUnlocked;
            // rest for the reward pool
            amountToRewardPool = stakedAmount - amountToWithdraw;
        }

        uint reward = calculateReward();
        amountToWithdraw += reward;

        // update the staking global info
        stakersCount--;
        totalStakedBalance -= stakedAmount;

        // update the participant staking info
        uint newRewardFactor = 0;
        if(totalStakedBalance==0){
            // there are no users left in the staking pool,
            // so no users for reward distribution.
            // If any, then allocate amount for the contract owner!
            unallocatedRewards += amountToRewardPool;
            emit OwnerGotRewards(msg.sender, owner(), unallocatedRewards, t_now);
            // reset factor 
            // newRewardFactor = 0
        }else{
            newRewardFactor = calculateRewardFactor(amountToRewardPool, totalStakedBalance);
        }

        // ako se odlucis za array ovo ne moras da upises, ionako brises kompletan withdrawals array

        // update withdrawal info array
        withdrawals[withdrawalsLength] = withdrawInfo(t_now, amountToRewardPool, totalStakedBalance, newRewardFactor);
        withdrawalsLength++;
     
        // free up storage space to refund
        delete stakers[msg.sender];
        if(stakersCount==0){
            // free withdrawal info array
          //  delete withdrawals;
        }        

        // stavi i gas cost zbog attack-a cross contract
        (bool success,) = msg.sender.call{value:amountToWithdraw}("");
     //   require(success,"Transfer failed!");
        if (!success) {
            revert EtherTransferFailed(); 
        }  

        emit Unstaked(msg.sender, amountToWithdraw, amountToRewardPool, t_now);
    }

    function calculateReward() internal view returns(uint256){
        // 'Dust' or remainders could exist on the contract balance 
        // from the calculations on the Reward pool supply.
        // There are not tracked, but could be distributed to the last
        // user in the pool or even contract owner.
        // razmisli ako je ostalo dust-a a poslednji si u pool-u, da pokupis       
        
        if(withdrawalsLength==0){
            //nema withdrawal pa nema ni reward-a
            return 0;
        }

        // number of user deposits
        uint numOfDeposits = stakers[msg.sender].length;
        uint rewardAmount = 0;
        uint256 stakedAmount = 0;
        uint256 prevWithdrawalsLen = 0;
        uint256 nextWithdrawalsLen = 0;
        uint256 prevRewardFactor = 0; 
        uint256 nextRewardFactor = 0;
        
        for(uint i = 0; i < numOfDeposits; i++){
            // uzmi info o deposit-u    
            stakedAmount = stakers[msg.sender][i].curStakedAmount;
            prevWithdrawalsLen = stakers[msg.sender][i].curWithdrawalLength; 
            
            // kada je deposit-ovao nije prethodno bilo withdrawals
            prevRewardFactor = 0; 
            // kada je deposit-ovao bilo je prethodnog withdrawal-a
            if(prevWithdrawalsLen != 0){
                prevRewardFactor = withdrawals[prevWithdrawalsLen-1].curRewardFactor;
            }

            nextRewardFactor = 0;
            nextWithdrawalsLen = 0;
            // nacin da nadjem sledeci withdrawal record bez iteriranja po array-u
            if(i+1<numOfDeposits){
                // postoji sledeci deposit od ovog korisnika tako da se pozicioniram na
                // withdrawal record pre sledeceg deposit-a
                nextWithdrawalsLen = stakers[msg.sender][i+1].curWithdrawalLength;
            }else{
                // ne postoji sledeci deposit tako da uzimam poslednji withdrawal record
                nextWithdrawalsLen = withdrawalsLength;
            }
        
            // ako posle deposit-a nije bilo withdrawals, onda nema ni rewards
            if(prevWithdrawalsLen==nextWithdrawalsLen){
                continue;
            }
            
            nextRewardFactor = withdrawals[nextWithdrawalsLen-1].curRewardFactor;
            // if(prevRewardFactor==nextRewardFactor) continue; // no rewards to collect

            rewardAmount += (nextRewardFactor-prevRewardFactor)*stakedAmount/granularPrecision;
        }

        
        return rewardAmount;
    }

    function calculateRewardFactor(uint rewards, uint balance) internal view returns(uint256) {
        // trazis prvi prethodni withdraw record
        uint256 lastRewardFactor = 0; 

        if(withdrawalsLength != 0){
            //nema withdrawal pa nema ni reward-a
            lastRewardFactor = withdrawals[withdrawalsLength-1].curRewardFactor;
        }

        if(rewards==0){
            return lastRewardFactor;
        }
 
        return lastRewardFactor+rewards*granularPrecision/balance;
    }

    function withdrawDepositsFromAAVE(uint256 amount) internal {
        //require(getContractAWETHBalance() >= amount, "Insufficent account balance!");

        IERC20(aWethAddress).approve(address(iWethGateway), amount);
        //Withdraw lended funds via the Weth Gateway
        iWethGateway.withdrawETH(lendingPoolAddress, amount, address(this));
    }

    // To check the balance of aWeth tokens for this contract address
    function getContractAWETHBalance() internal view returns(uint) {
        return IERC20(aWethAddress).balanceOf(address(this));
    }

   // contract owner is able to remove the passive income from the lending pool.
    function withdrawOwnerRemainingProfit() 
        external
        onlyOwner
        nonReentrant
    {
        uint t_now = block.timestamp;
        // get balance in the Aave v3 lending pool
        uint aWETHBalance = getContractAWETHBalance();
        require(aWETHBalance>totalStakedBalance, "Currently there are no passive income!");
        
        uint income = aWETHBalance-totalStakedBalance;
        withdrawDepositsFromAAVE(income);

        uint _income = income;
        // reset
        income = 0;

        (bool success,) = msg.sender.call{value:_income}("");
     //   require(success,"Transfer failed!");
        if (!success) {
            revert EtherTransferFailed(); 
        }  

        emit OwnerIncomeWithdrawn(msg.sender, _income, t_now);
    }

   // contract  is able to remove the unallocated rewards generated by early withdrawal penalties.
   // case when user is alone in the pool and do withrawal before lock period has expired.
    function withdrawOwnerUnallocatedRewards() 
        external
        onlyOwner
        nonReentrant
       // moreThanZero(unallocatedRewards)
    {
        require(unallocatedRewards > 0, "There are no unallocated rewards at the moment!");

        uint256 _unallocatedRewards = unallocatedRewards;
        // reset state variable
        unallocatedRewards = 0;

        (bool success,) = msg.sender.call{value:_unallocatedRewards}("");
     //   require(success,"Transfer failed!");
        if (!success) {
            revert EtherTransferFailed(); 
        }  

        emit OwnerRewardsWithdrawn(msg.sender, _unallocatedRewards, block.timestamp);
    }

    receive() external payable {
       // Receive function is needed because withdrawETH is sending funds 
       // to the contract without call data
       if(msg.sender != address(iWethGateway)){
            depositETH();
       }       
    } 
    fallback() external payable {
        revert('Fallback not allowed');
    }

    function getCurrentNumberOfUsers() external view returns(uint256) {
        return stakersCount;
    }

    function getCurrentStakedBalance() external view returns(uint256) {
        return totalStakedBalance;
    }

    function getOwnersUnallocatedBalance() external view returns(uint256) {
        return unallocatedRewards;
    }

    function getUserStakedAmount() external view returns(uint256) {
        // number of user deposits
        uint numOfDeposits = stakers[msg.sender].length;
        require(numOfDeposits != 0, "You are not participant!");

        // previous staked amount in the pool
        uint amount = 0;
        if(numOfDeposits > 0){
            // get the user amount of deposits until now
            amount = stakers[msg.sender][numOfDeposits-1].curStakedAmount;
        }

        return amount;
    }

    function getUserStakeInfo() external view returns(stakeInfo[] memory) {
        // number of user deposits
        uint numOfDeposits = stakers[msg.sender].length;
        require(numOfDeposits != 0, "You are not participant!");

        return stakers[msg.sender];
    }
}

