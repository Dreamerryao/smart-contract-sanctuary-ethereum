contract StakedNFTs is Owned, NFTReceiver {
    using EnumerableMap for EnumerableMap.AddressToUintMap;

    event DepositBayc(address indexed user, uint256 tokenId, uint256 amount, uint96 nftFee);
    event WithdrawBayc(address indexed user, uint256 tokenId, uint256 amount);
    event StakeBayc(address indexed user, uint256 tokenId, uint256 amount);
    event UnstakeBayc(address indexed user, uint256 tokenId, uint256 amount);
    event DepositMayc(address indexed user, uint256 tokenId, uint256 amount, uint96 nftFee);
    event WithdrawMayc(address indexed user, uint256 tokenId, uint256 amount);
    event StakeMayc(address indexed user, uint256 tokenId, uint256 amount);
    event UnstakeMayc(address indexed user, uint256 tokenId, uint256 amount);
    event ClaimBayc(uint256 tokenId, uint256 amount);
    event ClaimMayc(uint256 tokenId, uint256 amount);

    struct Deposit {
        address owner;
        uint96 nftFee; // % of apecoin earned going to the NFT owner (0-10000 where 10000 is 100%)
    }

    IStaking public immutable staking;
    IERC20 public immutable apecoin;
    IERC721 public immutable bayc;
    IERC721 public immutable mayc;
    IERC4626 public immutable stakedApecoin;

    uint256 public constant FEE = 690; // 6.9% fee
    uint256 public constant UNIT = 1e18;
    uint256 public maxBaycApe = 10094000000000000000000;
    uint256 public maxMaycApe = 2042000000000000000000;

    mapping(uint256 => Deposit) public baycDeposits;
    mapping( uint256 => EnumerableMap.AddressToUintMap) internal baycStakers;
    mapping(uint256 => Deposit) public maycDeposits;
    mapping(uint256 => EnumerableMap.AddressToUintMap) internal maycStakers;

    constructor(address _apecoin, address _stakedApecoin, address _bayc, address _mayc, address _staking) Owned(msg.sender) {
        apecoin = IERC20(_apecoin);
        bayc = IERC721(_bayc);
        mayc = IERC721(_mayc);
        stakedApecoin = IERC4626(_stakedApecoin);
        staking = IStaking(_staking);
        apecoin.approve(_staking, type(uint256).max);
        apecoin.approve(_stakedApecoin, type(uint256).max);
    }

    /*//////////////////////////////////////////////////////////////
                        DEPOSIT/WITHDRAWAL LOGIC
    //////////////////////////////////////////////////////////////*/

    /// @notice deposit BAYC NFTs into the contract
    /// @param _nfts the NFTs to deposit and the amount of ApeCoin to stake
    /// @param nftFees the % of ApeCoin earned going to the NFT owner
    function depositBayc(IStaking.SingleNft[] memory _nfts, uint96 nftFees) public {
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            bayc.transferFrom(msg.sender, address(this), tokenId);
            apecoin.transferFrom(msg.sender, address(this), _nfts[x].amount);
            baycDeposits[tokenId] = Deposit(msg.sender, nftFees);
            baycStakers[tokenId].set(msg.sender, _nfts[x].amount);
            emit DepositBayc(msg.sender, tokenId, _nfts[x].amount, nftFees);
        }
        staking.depositBAYC(_nfts);
    }

    /// @notice deposit MAYC NFTs into the contract
    /// @param _nfts the NFTs to deposit and the amount of ApeCoin to stake
    /// @param nftFees the % of ApeCoin earned going to the NFT owner
    function depositMayc(IStaking.SingleNft[] memory _nfts, uint96 nftFees) public {
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            mayc.transferFrom(msg.sender, address(this), tokenId);
            apecoin.transferFrom(msg.sender, address(this), _nfts[x].amount);
            maycDeposits[tokenId] = Deposit(msg.sender, nftFees);
            maycStakers[tokenId].set(msg.sender, _nfts[x].amount);
            emit DepositMayc(msg.sender, tokenId, _nfts[x].amount, nftFees);
        }
        staking.depositMAYC(_nfts);
    }

    /// @notice withdraw BAYC NFTs from the contract
    /// @dev this will kick out all 3rd party stakers as well
    /// @param _nfts the NFTs to withdraw
    function withdrawBayc(IStaking.SingleNft[] memory _nfts) public {
        staking.withdrawBAYC(_nfts, address(this));
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            (uint256 staked, ) = staking.nftPosition(1, tokenId);
            require(staked == 0, "NOT_FULLY_WITHDRAWN");
            // only owner can call this
            require(baycDeposits[tokenId].owner == msg.sender, "NOT_OWNER");
            while(baycStakers[tokenId].length() > 0) {
                (address staker, uint256 amount) = baycStakers[tokenId].at(0);
                apecoin.transfer(staker, amount);
                baycStakers[tokenId].remove(staker);
            }
            bayc.transferFrom(address(this), msg.sender, tokenId);
            emit WithdrawBayc(msg.sender, tokenId, _nfts[x].amount);
            // now lets zero out the staked position
            delete baycDeposits[tokenId];
        }
    }

    /// @notice withdraw MAYC NFTs from the contract
    /// @dev this will kick out all 3rd party stakers as well
    /// @param _nfts the NFTs to withdraw
    function withdrawMayc(IStaking.SingleNft[] memory _nfts) public {
        staking.withdrawMAYC(_nfts, address(this));
        for(uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            (uint256 staked, ) = staking.nftPosition(2, tokenId);
            require(staked == 0, "NOT_FULLY_WITHDRAWN");
            // only owner can call this
            require(maycDeposits[tokenId].owner == msg.sender, "NOT_OWNER");
            while(maycStakers[tokenId].length() > 0) {
                (address staker, uint256 amount) = maycStakers[tokenId].at(0);
                apecoin.transfer(staker, amount);
                maycStakers[tokenId].remove(staker);
            }
            mayc.transferFrom(address(this), msg.sender, tokenId);
            emit WithdrawMayc(msg.sender, tokenId, _nfts[x].amount);
            // now lets zero out the staked position
            delete maycDeposits[tokenId];
        }
    }

    /*//////////////////////////////////////////////////////////////
                            STAKE APE LOGIC
    //////////////////////////////////////////////////////////////*/

    /// @notice Stake APE on another users BAYC
    /// @param _nfts The NFTs to stake on and the amount to stake
    function stakeBayc(IStaking.SingleNft[] memory _nfts) public {
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            // get how much we have staked
            (uint256 staked,) = staking.nftPosition(1, tokenId);
            // get how much we want to borrow
            uint256 amount = _nfts[x].amount;
            // we can't borrow more than the max stake amount
            require(staked + amount <= maxBaycApe, "MAX_DEBT");
            // set how much user is staking
            baycStakers[tokenId].set(msg.sender, amount);
            // take the apecoins
            apecoin.transferFrom(msg.sender, address(this), _nfts[x].amount);
            emit StakeBayc(msg.sender, tokenId, amount);
        }
        // now lets stake them in the BAYC pool
        staking.depositBAYC(_nfts);
    }

    /// @notice Stake APE on another users MAYC
    /// @param _nfts The NFTs to stake on and the amount to stake
    function stakeMayc(IStaking.SingleNft[] memory _nfts) public {
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            // get how much we have staked
            (uint256 staked,) = staking.nftPosition(2, tokenId);
            // get how much we want to borrow
            uint256 amount = _nfts[x].amount;
            // we can't borrow more than the max stake amount
            require(staked + amount <= maxMaycApe, "MAX_DEBT");
            // set how much user is staking
            maycStakers[tokenId].set(msg.sender, amount);
            // take the apecoins
            apecoin.transferFrom(msg.sender, address(this), _nfts[x].amount);
            emit StakeMayc(msg.sender, tokenId, amount);
        }
        // now lets stake them in the MAYC pool
        staking.depositMAYC(_nfts);
    }

    /// @notice Unstake APE on another users BAYC
    /// @param _nfts The NFTs to unstake on and the amount to unstake
    function unstakeBayc(IStaking.SingleNft[] memory _nfts) public {
        // first lets withdraw the APE from the BAYC pool
        staking.withdrawBAYC(_nfts, msg.sender);
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            // get how much we want to remove
            uint256 amount = _nfts[x].amount;
            // get how much we have staked
            uint256 staked = baycStakers[tokenId].get(msg.sender);
            uint256 newStake = staked - amount;
            // we can't remove more than we have staked
            require(newStake >= 0, "TOO_MUCH");
            if (newStake == 0) {
                baycStakers[tokenId].remove(msg.sender);
            } else {
                baycStakers[tokenId].set(msg.sender, newStake);
            }
            emit UnstakeBayc(msg.sender, tokenId, amount);
        }
    }

    /// @notice Unstake APE on another users MAYC
    /// @param _nfts The NFTs to unstake on and the amount to unstake
    function unstakeMayc(IStaking.SingleNft[] memory _nfts) public {
        // first lets withdraw the APE from the MAYC pool
        staking.withdrawMAYC(_nfts, msg.sender);
        for (uint256 x = 0; x < _nfts.length; x++) {
            uint256 tokenId = _nfts[x].tokenId;
            // get how much we want to remove
            uint256 amount = _nfts[x].amount;
            // get how much we have staked
            uint256 staked = maycStakers[tokenId].get(msg.sender);
            uint256 newStake = staked - amount;
            // we can't remove more than we have staked
            require(newStake >= 0, "TOO_MUCH");
            if (newStake == 0) {
                maycStakers[tokenId].remove(msg.sender);
            } else {
                maycStakers[tokenId].set(msg.sender, newStake);
            }
            emit UnstakeMayc(msg.sender, tokenId, amount);
        }
    }

    /*//////////////////////////////////////////////////////////////
                        CLAIM & RESTAKE LOGIC
    //////////////////////////////////////////////////////////////*/

    /// @notice Claim and restake ApeCoin
    /// @dev we will max out our available ApeCoin balance and pay debts
    /// if we are already maxed out we deposit into StakedApecoin
    /// @param _nfts array of NFTs to claim
    function claimBayc(uint256[] memory _nfts) public {
        uint256[] memory nft = new uint256[](1);
        for (uint256 x = 0; x < _nfts.length; x++) {
            nft[0] = _nfts[x];
            // claim tokens
            staking.claimSelfBAYC(nft);
            // get how much we have staked
            (uint256 staked,) = staking.nftPosition(1, nft[0]);
            // get how much we have claimed
            uint256 claimed = apecoin.balanceOf(address(this));
            uint256 fee = claimed * 690 / 10000;
            // transfer the fee to the contract owner
            apecoin.transfer(owner, fee);
            claimed -= fee;
            emit ClaimBayc(nft[0], claimed);
            // deposit into staked apecoin
            claimed = stakedApecoin.deposit(claimed, address(this));
            // transfer the fee to the NFT owner
            fee = claimed * baycDeposits[nft[0]].nftFee / 10000;
            stakedApecoin.transfer(baycDeposits[nft[0]].owner, fee);
            claimed -= fee;
            // loop through stakers and give them their tokens
            for (uint256 y = 0; y < baycStakers[nft[0]].length(); y++) {
                (address staker, uint256 amount) = baycStakers[nft[0]].at(y);
                uint256 share = claimed * amount / staked;
                stakedApecoin.transfer(staker, share);
            }
        }
    }

    function claimMayc(uint256[] memory _nfts) public {
        uint256[] memory nft = new uint256[](1);
        for (uint256 x = 0; x < _nfts.length; x++) {
            nft[0] = _nfts[x];
            // claim tokens
            staking.claimSelfMAYC(nft);
            // get how much we have staked
            (uint256 staked,) = staking.nftPosition(2, nft[0]);
            // get how much we have claimed
            uint256 claimed = apecoin.balanceOf(address(this));
            uint256 fee = claimed * 690 / 10000;
            // transfer the fee to the contract owner
            apecoin.transfer(owner, fee);
            claimed -= fee;
            emit ClaimMayc(nft[0], claimed);
            // deposit into staked apecoin
            claimed = stakedApecoin.deposit(claimed, address(this));
            // transfer the fee to the NFT owner
            fee = claimed * maycDeposits[nft[0]].nftFee / 10000;
            stakedApecoin.transfer(maycDeposits[nft[0]].owner, fee);
            claimed -= fee;
            // loop through stakers and give them their tokens
            for (uint256 y = 0; y < maycStakers[nft[0]].length(); y++) {
                (address staker, uint256 amount) = maycStakers[nft[0]].at(y);
                uint256 share = claimed * amount / staked;
                stakedApecoin.transfer(staker, share);
            }
        }
    }

}