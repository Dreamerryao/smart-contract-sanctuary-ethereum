import "forge-std/console.sol";
import {INameWrapper}                 from "ens-contracts/wrapper/INameWrapper.sol";
import {ISubnameWrapper}              from "contracts/interfaces/ISubnameWrapper.sol";
import {ENS}                          from "ens-contracts/registry/ENS.sol";
import {IAggregatorInterface}         from "contracts/interfaces/IAggregatorInterface.sol";
import {Balances}                     from "./Balances.sol";
import {IERC165}                      from "openzeppelin-contracts/contracts/utils/introspection/IERC165.sol";
import {ERC20Recoverable}             from "ens-contracts/utils/ERC20Recoverable.sol";
import {IFixedPriceRenewalController} from "contracts/interfaces/IFixedPriceRenewalController.sol";
import {BaseRenewalController}        from "./BaseRenewalController.sol";

contract FixedPriceRenewalController is 
    BaseRenewalController,
    IFixedPriceRenewalController
    {

    uint256 public constant SECONDS_PER_YEAR = 31536000;

    // Chainlink oracle address
    IAggregatorInterface public usdOracle;

    uint256 public dollarsPerSecond;

    constructor(  
        ENS _ens,
        INameWrapper _nameWrapper,
        ISubnameWrapper _subnameWrapper,
        IAggregatorInterface _usdOracle,
        uint16 _dollarsPerYear
    ) BaseRenewalController(
        _ens,
        _nameWrapper,
        _subnameWrapper
    ) {

        // Set the oracle address.
        usdOracle        = _usdOracle;
        dollarsPerSecond = _dollarsPerYear / SECONDS_PER_YEAR * 1e18;
    }


    function updateUsdOracle(
        IAggregatorInterface _usdOracle
    ) external
    {
        usdOracle = _usdOracle;
    }

    function updatePrice(
        uint16 _dollarsPerYear
    ) external
    {
        dollarsPerSecond = _dollarsPerYear / SECONDS_PER_YEAR * 1e18;
    }


    /**
     * @notice Gets the total cost of rent in wei, from the defined dollarsPerSecond value, i.e. USD, and duration.
     * @param name The name in DNS format, e.g. vault.vitalik.eth
     * @param duration The amount of time the name will be rented for/extended in years. 
     * @return The rent price for the duration in Wei. 
     */

    function rentPrice(bytes memory name, uint256 duration)
        public
        view
        override (BaseRenewalController)
        returns (uint256)
    {

        // Convert the unit price from USD to Wei.
        return usdToWei(dollarsPerSecond * duration);
    }

    /**
    * @param interfaceId The interface identifier, as specified in ERC-165.
    * @return `true` if the contract implements `interfaceID`
    */ 

    function supportsInterface(bytes4 interfaceId)
        public
        pure
        override(BaseRenewalController)
        returns (bool)
    {
        return
            interfaceId == type(IFixedPriceRenewalController).interfaceId ||
            super.supportsInterface(interfaceId);
    }


    /**
    * @dev Converts USD to Wei. 
    * @param amount The amount of USD to be converted to Wei.
    * @return The amount of Wei.
    */
    function usdToWei(uint256 amount) internal view returns (uint256) {

        // Get the price of ETH in USD (with 8 digits of precision) from the oracle.
        uint256 ethPrice = uint256(usdOracle.latestAnswer());

        // Convert the amount of USD (with 18 digits of precision) to Wei. 
        return (amount * 1e8) / ethPrice;
    }
}

