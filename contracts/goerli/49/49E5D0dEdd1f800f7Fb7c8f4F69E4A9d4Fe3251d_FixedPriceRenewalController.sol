import "forge-std/console.sol";
import {INameWrapper}                  from "ens-contracts/wrapper/INameWrapper.sol";
import {StringUtils}                   from "ens-contracts/ethregistrar/StringUtils.sol";
import {ISubnameWrapper}               from "contracts/interfaces/ISubnameWrapper.sol";
import {ENS}                           from "ens-contracts/registry/ENS.sol";
import {IAggregatorInterface}          from "contracts/interfaces/IAggregatorInterface.sol";
import {Balances}                      from "./Balances.sol";
import {BytesUtilsSub}                 from "./BytesUtilsSub.sol";
import {IERC165}                       from "openzeppelin-contracts/contracts/utils/introspection/IERC165.sol";
import {ERC20Recoverable}              from "ens-contracts/utils/ERC20Recoverable.sol";
import {IRenewalController}            from "contracts/interfaces/IRenewalController.sol";
import {IRenewalControllerUpgradeable} from "contracts/interfaces/IRenewalControllerUpgradeable.sol";

error UnauthorizedParent_RC();
error InsufficientValue_RC();
error UnauthorizedAddress_RC(bytes32 node);

abstract contract BaseRenewalController is 
    Balances, 
    ERC20Recoverable,
    IERC165,
    IRenewalController,
    IRenewalControllerUpgradeable 
    {

    using StringUtils for *;
    using BytesUtilsSub for bytes;

    uint256 public nameWrapperVersion;
    uint256 public subnameWrapperVersion;

    // The NameWrapper and SubnameWrapper can be upgraded. 
    INameWrapper[] public nameWrappers = new INameWrapper[](1);
    ISubnameWrapper[] public subnameWrappers = new ISubnameWrapper[](1);
    ENS public immutable ens;

    constructor(
        ENS _ens,
        INameWrapper _nameWrapper,
        ISubnameWrapper _subnameWrapper
    ) {

        ens = _ens;
        nameWrappers[0] = _nameWrapper;

        // The contract that wraps the subnames.
        subnameWrappers[0] = _subnameWrapper;
    }


    function rentPrice(bytes memory name, uint256 duration)
        public
        view
        override (IRenewalController, Balances)
        virtual
        returns (uint256);

    /**
     * @notice If the NameWrapper smart contract is upgraded, add the new version.
     * @param _nameWrapper The next version of the NameWrapper
     */
    function addNextNameWrapperVersion(
        INameWrapper _nameWrapper
    ) public onlyOwner {

        nameWrappers.push(_nameWrapper);

        emit NameWrapperVersionUpdated();
    }

    /**
     * @notice If the NameWrapper smart contract is upgraded, add the new version.
     * @param _subnameWrapper The next version of the NameWrapper
     */
    function addNextSubnameWrapperVersion(
        ISubnameWrapper _subnameWrapper
    ) public onlyOwner {

        subnameWrappers.push(_subnameWrapper);
        
        emit SubnameWrapperVersionUpdated();
    }

    // This struct is being used to solve a stack too deep error.
    struct ExpiryMemoryStorage{
        uint64 expiry;
        uint64 parentExpiry;
        uint64 nodeExpiry;
    }

    /**
    * @dev Function to renew a name for a specified duration. 
    * @param name The name to be renewed in DNS format.
    * @param duration The duration for which the name should be renewed in years.
    */

    function renew(bytes calldata name, address referrer, uint256 duration)
        external
        payable
    {        

        // Renew with the latest version of the NameWrapper and SubnameWrapper contracts.
        renewWithVersions(
            nameWrappers.length-1,    
            subnameWrappers.length-1,
            name,
            referrer,
            duration);
    }

    /**
    * @dev Function to renew a name for a specified duration. 
    * @param nameWrapperVersion The version of the NameWrapper.
    * @param subnameWrapperVersion The version of the SubnameWrapper. 
    * @param name The name to be renewed in DNS format.
    * @param duration The duration for which the name should be renewed in years.
    */

    function renewWithVersions(
        uint256 nameWrapperVersion,
        uint256 subnameWrapperVersion, 
        bytes memory name, 
        address referrer, 
        uint256 duration
        )
        public
        payable
    {        
        (bytes32 labelhash, uint256 offset) = name.readLabel(0);
        bytes32 parentNode = name.namehash(offset);
        bytes32 node = _makeNode(parentNode, labelhash);

        // Get the owners of the name and the parent name.
        address nodeOwner = subnameWrappers[subnameWrapperVersion].ownerOf(uint256(node));
        address parentOwner = subnameWrappers[subnameWrapperVersion].ownerOf(uint256(parentNode));

        // Check to make sure the caller (msg.sender) is authorised to renew the name.
        if( msg.sender != nodeOwner && !subnameWrappers[subnameWrapperVersion].isApprovedForAll(nodeOwner, msg.sender)){
            revert UnauthorizedAddress_RC(node);
        }

        ExpiryMemoryStorage memory m;

        // Get the previous expiry. 
        (,, m.nodeExpiry) = nameWrappers[nameWrapperVersion].getData(uint256(node));

        // Check to see if the duration is too long and
        // if it is set the duration.
        (,, m.parentExpiry) = nameWrappers[nameWrapperVersion].getData(uint256(parentNode));
        if (m.nodeExpiry + duration > m.parentExpiry) {
            duration = m.parentExpiry - m.nodeExpiry;
        }

        // Set the expiry
        m.expiry =  uint64(m.nodeExpiry + duration);

        // Get the price for the duration.
        uint256 price = rentPrice(name, duration);
        if (msg.value < price) {
            revert InsufficientValue_RC();
        }

        // Calculate the amount to be given to the referrer.
        uint256 referrerAmount = price * referrerCuts[referrer] / 10000;

        // Calculate the amount to be given to the owner of the contract. 
        // We don't need a balance for the owner of the contract because the owner
        // can withdraw any funds in the contract minus total balances. 
        uint256 ownerAmount = price * ownerCut / 10000;

        // Increase the referrer's balance
        balances[referrer] += referrerAmount;

        // Increase the owner of the parent name's balance minus the
        // referrer amount and the owner amount.
        balances[parentOwner] += price - referrerAmount - ownerAmount;

        // Increase the total balances
        totalBalance += price - ownerAmount;

        subnameWrappers[subnameWrapperVersion].extendExpiry(
            parentNode,
            labelhash,
            m.expiry
        );

        emit NameRenewed(name, price, m.expiry);

        // If the caller paid too much refund the amount overpaid. 
        if (msg.value > price) {
            payable(msg.sender).transfer(msg.value - price);
        }

    }


    /**
    * @param interfaceId The interface identifier, as specified in ERC-165.
    * @return `true` if the contract implements `interfaceID`
    */ 

    function supportsInterface(bytes4 interfaceId)
        public
        virtual
        pure
        returns (bool)
    {
        return
            interfaceId == type(IERC165).interfaceId ||
            interfaceId == type(IRenewalControllerUpgradeable).interfaceId ||
            interfaceId == type(IRenewalController).interfaceId;
    }


    /**
     * @dev Returns the node for a namehash and labelhash.
     * @param parentNode The namehash of the parent node.
     * @param labelhash The labelhash of the label.
     * @return The node for the namehash and labelhash.
     */
    function _makeNode(bytes32 parentNode, bytes32 labelhash)
        private
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(parentNode, labelhash));
    }
}

