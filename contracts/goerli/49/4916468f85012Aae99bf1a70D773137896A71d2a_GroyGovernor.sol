// TODO: Investigate ReentrancyGuard
//       https://github.com/OpenZeppelin/openzeppelin-contracts/blob/master/contracts/security/ReentrancyGuard.sol
contract GroyGovernor is Ownable {

    /////// Governor

    // TODO: consider if this should be the default, yet each initiative stores their voting address token?
    address public groyContractAddress;

    bool private creationEnabled = true;
    
    function enableCreation(bool value) public onlyOwner {
        creationEnabled = value;
    }

    /// @notice All voting must be stopped before the address can be changed, or some active votes would lose tokens 
    function updateGroyTokenContract(address _tokenAddress) public onlyOwner {
        require(!creationEnabled, "Disable creation first");
        require(_initiativesFinished(), "Initiatives must finish first");
        groyContractAddress = _tokenAddress;
    }

    function _initiativesFinished() private view returns(bool) {
        for (uint i = 0; i <= initiativeCount; i++) {
            Initiative memory initiative = initiatives[i];
            if (initiative.endBlock >= block.number) {
                return false;
            }
        }
        return true;
    }

    /////// Initiatives

    // TODO: consider reducing size for more efficient storage
    //       consider using bitmask + bitmap instead of bools, those take up 8-bits
    /*
    function getBoolean(uint256 _packedBools, uint256 _boolNumber)
        public view returns(bool)
    {
        uint256 flag = (_packedBools >> _boolNumber) & uint256(1);
        return (flag == 1 ? true : false);
    }
    function setBoolean(
        uint256 _packedBools,
        uint256 _boolNumber,
        bool _value
    ) public view returns(uint256) {
        if (_value)
            return _packedBools | uint256(1) << _boolNumber;
        else
            return _packedBools & ~(uint256(1) << _boolNumber);
    }
    */
    struct Initiative { // Similar to a proposal
        // TODO: Allow to be changed until started
        // TODO: cannot pick start date in past
        uint startBlock;
        // TODO: Allow to be changed until locked
        // TODO: cannot pick lock date before start or after end data
        uint lockBlock;
        // TODO: Allow to be changed until ended
        // TODO: cannot pick end before lock or start
        uint endBlock;
        bool allowLateDeposit;
        bool allowEarlyWithdrawal;
    }

    uint public initiativeCount;

    /// @dev Storage of Initiative ID to Initiative objects (structs)
    mapping (uint => Initiative) public initiatives;

    // TODO: See if events can be used to replace these...
    mapping (uint => address[]) public initiativeToVoters; 
    mapping (address => uint[]) public voterToInitiatives;

    function getInitiativesFromVoter(address _account) external view returns (uint[] memory) {
        return voterToInitiatives[_account];
    }

    function getVotersFromInitiative(uint _initiativeId) external view returns (address[] memory) {
        return initiativeToVoters[_initiativeId];
    }

    function allowLateDeposit(uint _initiativeId, bool allow) external 
        onlyOwner
        initiativeExists(_initiativeId, initiativeCount)
    {
        Initiative storage initiative = initiatives[_initiativeId];
        initiative.allowLateDeposit = allow;
    }

    function allowEarlyWithdrawal(uint _initiativeId, bool allow) external 
        onlyOwner
        initiativeExists(_initiativeId, initiativeCount)
    {
        Initiative storage initiative = initiatives[_initiativeId];
        initiative.allowEarlyWithdrawal = allow;
    }

    // TODO: Should we allow anyone to create an initiative (i.e. Initiatives would have Owners)?
    function createInitiative(uint _startBlock, uint _lockBlock, uint _endBlock) public onlyOwner {
        require(creationEnabled, "Creation is disabled");
        require(_startBlock <= _lockBlock, "Start block > lock block");
        require(_lockBlock <= _endBlock, "Lock block > end block");
        uint id = initiativeCount;
        Initiative storage initiative = initiatives[initiativeCount++];
        initiative.startBlock = _startBlock;
        initiative.lockBlock = _lockBlock;
        initiative.endBlock = _endBlock;
        initiative.allowLateDeposit = false; // default
        initiative.allowEarlyWithdrawal = false; // default
        emit InitiativeCreated(
            msg.sender,
            id,
            initiative.startBlock,
            initiative.lockBlock,
            initiative.endBlock,
            initiative.allowLateDeposit,
            initiative.allowEarlyWithdrawal
        );
    }

    event InitiativeCreated(
        address indexed initiator, uint indexed initiativeId,
        uint startBlock, uint lockBlock, uint endBlock,
        bool allowLateDeposit, bool allowEarlyWithdrawal
    );

    modifier initiativeExists(uint _initiativeId, uint _initiativeCount) {
        require(_initiativeId < _initiativeCount, "Initiative does not exist");
        _;
    }

    /////// Options

    // TODO: Add bitmask and bitmap for data compression? 
    event IncreaseVote(
        address indexed voter, 
        uint indexed initiativeId,
        uint votes
    );

    event DecreaseVote(
        address indexed voter,
        uint indexed initiativeId,
        uint votes
    );

    struct Option {
        uint totalVotes;
        string description;
    }

    uint public optionCount;

    /// @dev Storage of Option ID to Initiative objects (structs)
    mapping (uint => Option) public options;

    /// @dev Storage of Initiative ID to Voter ID/Address to the receipt object for their vote
    mapping (uint => mapping(address => uint)) public currentVotes; // current user votes
    // mapping (uint => mapping (address => Receipt)) public receipts;

    // TODO: See if events can be used to replace these...
    //       Use events to track the count of votes at given blocks
    mapping (uint => uint) public optionToInitiativeId;
    mapping (uint => uint[]) public initiativeToOptions;

    function getOptionsFromInitiative(uint _initiativeId) external view returns (uint[] memory) {
        return initiativeToOptions[_initiativeId];
    }

    function getInitiativeFromOption(uint _optionId) external view returns (uint) {
        return optionToInitiativeId[_optionId];
    }

    // TODO: Implement the 'winning' vote algorithm and use the vote counts over blocks to decide, see checkpoints in GovernorBravo

    // TODO: Add reward system, remember remaining reward is distributed to the remaining participants (when withdrawal early is set)

    function createOption(string memory _description, uint _initiativeId) public 
        onlyOwner 
        initiativeExists(_initiativeId, initiativeCount)
    {
        require(creationEnabled, "Creation is disabled");
        uint id = optionCount;
        Option memory option = options[optionCount++];
        // option.currentVotes = ...mapping...
        option.description = _description;
        option.totalVotes = 0; // TODO: Not needed, check if uses extra Gas or is optimized away
        optionToInitiativeId[id] = _initiativeId;
        initiativeToOptions[_initiativeId].push(id);
        emit OptionCreated(msg.sender, id, _description);
    }

    event OptionCreated(address indexed initiator, uint indexed optionId, string description);

    modifier optionExists(uint _optionId, uint _optionCount) {
        require(_optionId < _optionCount, "Option does not exist");
        _;
    }

    ///// Governor

    constructor(address _tokenAddress) {
        groyContractAddress = _tokenAddress;
    }

    function decreaseVote(uint _optionId, uint _amount) public
        optionExists(_optionId, optionCount)
    {
        uint initiativeId = optionToInitiativeId[_optionId];
        Initiative memory initiative = initiatives[initiativeId];
        _requireVoteIsActive(initiative, initiative.allowEarlyWithdrawal);
        mapping(address => uint) storage votesOnOptionId = currentVotes[_optionId];
        require(votesOnOptionId[msg.sender] >= _amount, "Vote removal amount too large");
        Option storage option = options[_optionId];

        IERC20(groyContractAddress).transfer(
            msg.sender,
            _amount
            );

        option.totalVotes -= _amount; // TODO: Examine cost benefits of forcing the client to calculate this value?
        votesOnOptionId[msg.sender] -= _amount; // <== clients could use this value for each 
        emit DecreaseVote(msg.sender, _optionId, _amount);
    }

    function increaseVote(uint _optionId, uint _amount) public
        optionExists(_optionId, optionCount)
    {
        _registerVoter(_optionId);
        uint initiativeId = optionToInitiativeId[_optionId];
        Initiative memory initiative = initiatives[initiativeId];
        _requireVoteIsActive(initiative, initiative.allowLateDeposit);
        mapping(address => uint) storage votesOnOptionId = currentVotes[_optionId];
        Option storage option = options[_optionId];

        uint senderBalance = IERC20(groyContractAddress).balanceOf(msg.sender);
        // console.log("AMOUNT: %s", _amount);
        require(senderBalance >= _amount, "Vote amount exceeds balance");

        uint currentAllowance = IERC20(groyContractAddress).allowance(msg.sender, address(this));
        // console.log("CURRENT_ALLOWANCE: %s", currentAllowance);
        require(currentAllowance >= _amount, "Vote amount exceeds allowance");

        IERC20(groyContractAddress).transferFrom(
            msg.sender,
            address(this),
            _amount
            );
        
        option.totalVotes += _amount;
        votesOnOptionId[msg.sender] += _amount;
        emit IncreaseVote(msg.sender, _optionId, _amount);
    }

    function _requireVoteIsActive(Initiative memory initiative, bool disableLock) private view {
        require(block.number <= initiative.endBlock, "Vote has ended");
        require(disableLock || (block.number <= initiative.lockBlock), "Vote is locked");
        require(block.number >= initiative.startBlock, "Vote has not yet started");
    }

    function _registerVoter(uint _optionId) private {
        uint initiativeId = optionToInitiativeId[_optionId];
        address[] storage voters = initiativeToVoters[initiativeId];
        if (!_arrayContainsAddress(voters, msg.sender)) {
            voters.push(msg.sender);
        }
        uint[] storage voterInitiatives = voterToInitiatives[msg.sender];
        if (!_arrayContainsInt(voterInitiatives, initiativeId)) {
            voterInitiatives.push(initiativeId);
        }
    }

    function _arrayContainsAddress(address[] memory _array, address _value) internal pure returns(bool) {
        for (uint i=0; i < _array.length; i++) {
            if (_value == _array[i]) {
                return true;
            }
        }
        return false;
    }

    function _arrayContainsInt(uint[] memory _array, uint _value) internal pure returns(bool) {
        for (uint i=0; i < _array.length; i++) {
            if (_value == _array[i]) {
                return true;
            }
        }
        return false;
    }
}

