// : MIT
contract Playground is IVRFConsumer, Initializable, AccessControlUpgradeable {
    event MemberMonth(address indexed user, uint256 num, uint256 timestamp);

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    struct ProductInfo {
        bool suspended; // suspended= true ,box can't be opened
        uint256 boxNum;
        uint256 opened;
        uint256 openTime;
        uint256 tokenId;
    }

    uint16 public curProductId;

    mapping(uint8 => address) public tokenAddress;

    mapping(uint256 => uint8) public boxConfLimited;
    mapping(uint256 => uint8) boxConfMemberMonth;

    mapping(uint16 => ProductInfo) public productBoxInfo;

    mapping(uint256 => bool) public pendingReq;

    mapping(address => mapping(uint256 => uint8)) public opened;

    mapping(uint256 => uint16[]) public tokenId2Product;

    // (pid | index-8 (0...)) =>  [0] = num|(tokenIndex-8)              / [1...]=  (leftNum | class-8)
    mapping(uint24 => uint256[]) public type1INfo;
    // (pid | index-8 (0/1)) =>   [0] =  thresholdClass-8|(tokenIndex-8)   / [1...]= (leftNum | class-8)
    mapping(uint24 => uint256[]) public type2INfo;

    // vrf
    address public VRFProxyAddr;
    uint256 public VRFReqNonce;
    mapping(uint256 => address) public reqSender;
    uint256 public startVRFIndex;
    uint256 public endVRFIndex;
    mapping(uint256 => uint256) public reqMap;
    mapping(uint256 => uint256) public reqRandom;

    function initialize(address _VRFProxyAddr) public initializer {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(OPERATOR_ROLE, _msgSender());
        VRFProxyAddr = _VRFProxyAddr;

        boxConfMemberMonth[4] = 1;
        boxConfMemberMonth[5] = 3;
        boxConfMemberMonth[6] = 3;
        boxConfMemberMonth[7] = 3;
        boxConfMemberMonth[8] = 6;
        boxConfMemberMonth[9] = 12;
        boxConfMemberMonth[10] = 24;

        boxConfLimited[7] = 1;
    }

    struct BoxInfoBindView {
        uint256 tokenId;
        uint256 boxNum;
        uint256 openTime;
    }

    struct Type1Conf {
        uint8 tokenAddrIndex;
        uint8 num;
        uint256[] numPerClass;
    }

    struct Type2Conf {
        uint8 thresholdClass;
        uint8 tokenAddrIndex1;
        uint256[] numPerClass1;
        uint8 tokenAddrIndex2;
        uint256[] numPerClass2;
    }

    function issueAndBindBox(
        BoxInfoBindView calldata boxInfoView,
        Type1Conf[] calldata type1Confs,
        Type2Conf[] calldata type2Confs
    ) public onlyRole(OPERATOR_ROLE) {
        curProductId++;
        productBoxInfo[curProductId] = ProductInfo({
            suspended: false,
            boxNum: boxInfoView.boxNum,
            opened: 0,
            openTime: boxInfoView.openTime,
            tokenId: boxInfoView.tokenId
        });
        tokenId2Product[boxInfoView.tokenId].push(curProductId);
        for (uint8 i = 0; i < type1Confs.length; i++) {
            Type1Conf calldata _conf = type1Confs[i];
            uint24 _key = (uint24(curProductId) << 8) | i;
            uint256 data0 = (uint256(_conf.num) << 8) | _conf.tokenAddrIndex;
            type1INfo[_key].push(data0);
            for (uint8 j = 0; j < _conf.numPerClass.length; j++) {
                if (_conf.numPerClass[j] == 0) {
                    continue;
                }
                uint8 class = (j + 1);
                type1INfo[_key].push(((_conf.numPerClass[j]) << 8) | class);
                INFT(tokenAddress[_conf.tokenAddrIndex]).issueNFT(
                    curProductId,
                    class,
                    _conf.numPerClass[j]
                );
            }
        }
        if (type2Confs.length == 0) {
            return;
        }
        Type2Conf calldata conf = type2Confs[0];
        uint24 _key1 = (uint24(curProductId) << 8) | 0;
        type2INfo[_key1].push(
            (uint256(conf.thresholdClass) << 8) | conf.tokenAddrIndex1
        );
        for (uint8 j = 0; j < conf.numPerClass1.length; j++) {
            if (conf.numPerClass1[j] == 0) {
                continue;
            }
            uint8 class = (j + 1);
            type2INfo[_key1].push(((conf.numPerClass1[j]) << 8) | class);
            INFT(tokenAddress[conf.tokenAddrIndex1]).issueNFT(
                curProductId,
                class,
                conf.numPerClass1[j]
            );
        }
        uint24 _key2 = (uint24(curProductId) << 8) | 1;
        type2INfo[_key2].push(
            (uint256(conf.thresholdClass) << 8) | conf.tokenAddrIndex2
        );
        for (uint8 j = 0; j < conf.numPerClass2.length; j++) {
            if (conf.numPerClass2[j] == 0) {
                continue;
            }
            uint8 class = (j + 1);
            type2INfo[_key2].push(((conf.numPerClass2[j]) << 8) | class);
            INFT(tokenAddress[conf.tokenAddrIndex2]).issueNFT(
                curProductId,
                class,
                conf.numPerClass2[j]
            );
        }
    }

    function setOpenTime(uint16 _productId, uint256 _openTime)
        public
        onlyRole(OPERATOR_ROLE)
    {
        productBoxInfo[_productId].openTime = _openTime;
    }

    function setStatus(uint16 _productId, bool _suspended)
        public
        onlyRole(OPERATOR_ROLE)
    {
        productBoxInfo[_productId].suspended = _suspended;
    }

    function setAddress(
        uint8[] memory tokenAddrIndex,
        address[] memory addresses
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(tokenAddrIndex.length == addresses.length, "param err");
        for (uint256 i = 0; i < tokenAddrIndex.length; i++) {
            tokenAddress[tokenAddrIndex[i]] = addresses[i];
        }
    }

    function setVRFAddress(address _VRFProxyAddr)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        VRFProxyAddr = _VRFProxyAddr;
    }

    // todo
    function openBoxVRF(uint256 tokenId) public {
        if (boxConfLimited[tokenId] > 0) {
            require(
                opened[msg.sender][tokenId] < boxConfLimited[tokenId],
                "open limited"
            );
            opened[msg.sender][tokenId]++;
        }
        IERC1155Burnable(tokenAddress[0]).burn(msg.sender, tokenId, 1);
        VRFReqNonce++;
        uint256 reqId = (VRFReqNonce << 128) | tokenId;
        reqSender[reqId] = msg.sender;
        IVRFProxy(VRFProxyAddr).vfrReq(reqId, 1);
        if (boxConfMemberMonth[tokenId] > 0) {
            emit MemberMonth(
                msg.sender,
                boxConfMemberMonth[tokenId],
                block.timestamp
            );
        }
    }

    function handleVRF(uint256 reqId, uint256[] memory randomWords)
        public
        override
    {
        require(msg.sender == VRFProxyAddr, "send err");
        address user = reqSender[reqId];
        require(user != address(0), "resp err");
        reqMap[endVRFIndex++] = reqId;
        reqRandom[reqId] = randomWords[0];
    }

    function task() public {
        require(startVRFIndex < endVRFIndex, "last");
        uint256 reqId = reqMap[startVRFIndex];
        address user = reqSender[reqId];
        uint256 tokenId = uint128(reqId);
        (, uint16 _productId) = getCurProInfo(tokenId);
        uint256 random = reqRandom[reqId];
        productBoxInfo[_productId].opened++;
        handleNFT2(_productId, random, user);
        handleNFT1(_productId, random, user);
        delete reqMap[startVRFIndex];
        delete reqRandom[reqId];
        delete reqSender[reqId];
        startVRFIndex++;
    }

    function handleNFT2(
        uint16 _productId,
        uint256 random,
        address user
    ) internal {
        uint8 _random = uint8(random % 2);
        uint24 key1 = (uint24(_productId) << 8) | (_random % 2);
        if (type2INfo[key1].length == 0) {
            // no conf
            return;
        }
        uint8 _thresholdClass = uint8(type2INfo[key1][0] >> 8);
        uint256[] memory confs1 = type2INfo[key1];
        uint8 _class1;
        if (confs1.length > 1) {
            uint256 _leftInfoKey1 = (random % (confs1.length - 1)) + 1;
            uint256 info = confs1[_leftInfoKey1];
            _class1 = uint8(info);
            uint8 _tokenAddrIndex1 = uint8(type2INfo[key1][0]);
            uint256 leftNum1 = info >> 8;
            INFT(tokenAddress[_tokenAddrIndex1]).mintNFT(
                _productId,
                user,
                _class1
            );
            if (leftNum1 > 1) {
                uint256 _leftNum1 = ((leftNum1 - 1) << 8) | _class1;
                type2INfo[key1][_leftInfoKey1] = _leftNum1;
            } else {
                if (confs1.length > 2) {
                    type2INfo[key1][_leftInfoKey1] = type2INfo[key1][
                        confs1.length - 1
                    ];
                }
                type2INfo[key1].pop();
            }
        }
        uint24 key2 = (uint24(_productId) << 8) | ((_random + 1) % 2);
        uint256[] memory confs2 = type2INfo[key2];
        if (confs2.length > 1) {
            bool isMeet = _class1 >= _thresholdClass;
            uint16 __productId = _productId;
            address _user = user;
            uint256 info2;
            uint8 _class2;
            uint256 _leftInfoKey2;
            for (uint256 i = 1; i < confs2.length; i++) {
                uint256 _info = confs2[i];
                uint8 __class = uint8(_info);
                if (_class2 == 0) {
                    _class2 = __class;
                    info2 = _info;
                    _leftInfoKey2 = i;
                    continue;
                }
                if (isMeet && __class < _class2) {
                    _class2 = __class;
                    info2 = _info;
                    _leftInfoKey2 = i;
                } else if (!isMeet && __class > _class2) {
                    _class2 = __class;
                    info2 = _info;
                    _leftInfoKey2 = i;
                }
            }
            uint8 _tokenAddrIndex2 = uint8(type2INfo[key2][0]);
            INFT(tokenAddress[_tokenAddrIndex2]).mintNFT(
                __productId,
                _user,
                _class2
            );
            uint256 leftNum2 = info2 >> 8;
            if (leftNum2 > 1) {
                uint256 _leftNum2 = ((leftNum2 - 1) << 8) | _class2;
                type2INfo[key2][_leftInfoKey2] = _leftNum2;
            } else {
                if (confs2.length > 2) {
                    type2INfo[key2][_leftInfoKey2] = type2INfo[key2][
                        confs2.length - 1
                    ];
                }
                type2INfo[key2].pop();
            }
        }
    }

    function handleNFT1(
        uint16 _productId,
        uint256 random,
        address user
    ) internal {
        for (uint8 i = 0; i < 30; i++) {
            uint24 _key = (uint24(_productId) << 8) | i;
            uint256 leftLen = type1INfo[_key].length;
            if (leftLen == 1) {
                continue;
            } else if (leftLen == 0) {
                break;
            }
            uint256 conf = type1INfo[_key][0];
            uint8 _tokenAddrIndex = uint8(conf);
            uint256 num = conf >> 8;
            uint16 __productId = _productId;
            address _user = user;
            uint24 __key = _key;
            for (uint256 j = 0; j < num; j++) {
                leftLen = type1INfo[__key].length;
                uint256 diff = getRandom(random, i * 30 + j);
                uint256 _leftInfoKey = (diff % (leftLen - 1)) + 1;
                uint256 leftInfo = type1INfo[__key][_leftInfoKey];
                uint8 _class = uint8(leftInfo);
                uint256 leftNum = leftInfo >> 8;
                INFT(tokenAddress[_tokenAddrIndex]).mintNFT(
                    __productId,
                    _user,
                    _class
                );
                if (leftNum > 1) {
                    uint256 _leftNum = ((leftNum - 1) << 8) | _class;
                    type1INfo[__key][_leftInfoKey] = _leftNum;
                } else {
                    if (leftLen > 2) {
                        type1INfo[__key][_leftInfoKey] = type1INfo[__key][
                            leftLen - 1
                        ];
                    }
                    type1INfo[__key].pop();
                }
            }
        }
    }

    function getRandom(uint256 _random, uint256 nonce)
        internal
        pure
        returns (uint256)
    {
        return uint256(keccak256(abi.encodePacked(_random, nonce)));
    }

    function getCurProInfo(uint256 tokenId)
        public
        view
        returns (ProductInfo memory _info, uint16 productId)
    {
        uint16[] memory proIds = tokenId2Product[tokenId];
        require(proIds.length > 0, "tokenId err");
        for (uint256 i = 0; i < proIds.length; i++) {
            uint16 _productId = proIds[i];
            ProductInfo memory info = productBoxInfo[_productId];
            if (
                info.boxNum > info.opened &&
                !info.suspended 
            ) {
                _info = info;
                productId = _productId;
                break;
            }
        }
        require(productId > 0, "tokenId err");
    }

    function openBox(uint256 tokenId) public {
        (, uint16 _productId) = getCurProInfo(tokenId);
        if (boxConfLimited[tokenId] > 0) {
            require(
                opened[msg.sender][tokenId] < boxConfLimited[tokenId],
                "open limited"
            );
            opened[msg.sender][tokenId]++;
        }
        IERC1155Burnable(tokenAddress[0]).burn(msg.sender, tokenId, 1);

        if (boxConfMemberMonth[tokenId] > 0) {
            emit MemberMonth(
                msg.sender,
                boxConfMemberMonth[tokenId],
                block.timestamp
            );
        }

        uint256 random = getRandom(block.number, gasleft());
        productBoxInfo[_productId].opened++;
        handleNFT2(_productId, random, msg.sender);
        handleNFT1(_productId, random, msg.sender);
    }

    function openBoxBatch(uint256 num, uint256 tokenId) public {
        for (uint256 i = 0; i < num; i++) {
            openBox(tokenId);
        }
    }

    function setTokenId2Product(uint256 tokenId, uint16[] memory pids)
        public
        onlyRole(OPERATOR_ROLE)
    {
        tokenId2Product[tokenId] = pids;
    }
}