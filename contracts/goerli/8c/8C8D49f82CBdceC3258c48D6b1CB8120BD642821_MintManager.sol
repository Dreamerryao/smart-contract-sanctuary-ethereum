pragma solidity ^0.8.13;

import "./TheVillage.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./NFTStaking.sol";


contract MintManager is Ownable {

    enum MintPhase{ PAUSED, WHITELIST, PUBLIC, PHASE3, PHASE4, PHASE5}
    MintPhase phase;

    // Mint phases
    uint256 public whitelistMintPhase;
    uint256 public publicMintPhase;
    uint256 public phase3Mint;
    uint256 public phase4Mint;
    uint256 public phase5Mint;

    // Token prices
    uint256 public whitelistMintPrice = 0.008 ether;
    uint256 public publicMintPrice = 0.012 ether;
    uint256 public phase3MintPrice = 40;
    uint256 public phase4MintPrice = 80;
    uint256 public phase5MintPrice = 110;

    // Mint limits
    mapping(address => uint256) public whitelistMintLimits;

    EMRD token;
    TheVillage nft;
    NFTStaking staking;

    constructor(EMRD _token, TheVillage _nft, NFTStaking _staking, uint256 _whitelistMintPhase, uint256 _publicMintPhase, uint256 _phase3Mint, uint256 _phase4Mint, uint256 _phase5Mint) {
        nft = _nft;
        token = _token;
        staking = _staking;
        whitelistMintPhase = _whitelistMintPhase;
        publicMintPhase = _publicMintPhase;
        phase3Mint = _phase3Mint;
        phase4Mint = _phase4Mint;
        phase5Mint = _phase5Mint;
        phase = MintPhase.PAUSED;
    }

    function mintWhitelist(uint256 quantity) external payable {
        uint phaseThreshold = whitelistMintPhase;

        require(quantity > 0 && quantity <= 5, "Quantity must not be 0 and less than 6");
        // Check if address is whitelisted
        require(nft.isWhitelisted(msg.sender), "Address must be whitelisted to mint tokens");
        // Check if in whitelist mint phase
        require(phase == MintPhase.WHITELIST, "Not in public whitelist phase");
        // Check if address has reached mint limit
        require(whitelistMintLimits[msg.sender] + quantity <= 5, "Whitelist mint limit reached for this address");
        // Check if correct ether value is sent
        require(msg.value == whitelistMintPrice * quantity, "Incorrect ether value sent");

        // Increment the mint limit for the address
        whitelistMintLimits[msg.sender] += quantity;

        nft._mint(msg.sender, quantity);

        if (nft.getNumTokenMinted() == phaseThreshold) {
            nft.setPause(true);
            phase = MintPhase.PUBLIC;
        }
    }

    function mintPublic(uint quantity) external payable {
        uint previousThreshold = whitelistMintPhase;
        uint phaseThreshold = previousThreshold + publicMintPhase;

        require(quantity > 0 && quantity <= 5, "Quantity must not be 0 and less than 6");
        // Check if in public mint phase
        require(phase == MintPhase.PUBLIC, "Not in public mint phase");

        // Check if correct ether value is sent
        require(msg.value == publicMintPrice * quantity, "Incorrect ether value sent");

        nft._mint(msg.sender, quantity);

        if (nft.getNumTokenMinted() == phaseThreshold) {
            nft.setPause(true);
            phase = MintPhase.PHASE3;
        }
    }

    function mintPhase3WithEmerald(uint quantity) external payable {
        uint previousThreshold = whitelistMintPhase + publicMintPhase;
        uint phaseThreshold = previousThreshold + phase3Mint;

        require(quantity > 0, "Quantity must not be 0");
        // Check if in mint phase 3
        require(phase == MintPhase.PHASE3, "Not in mint phase 3");

        // Check if sufficient $EMERALD balance
        require(token.balanceOf(msg.sender) >= phase3MintPrice * quantity, "Insufficient $EMERALD balance");

        // Check if correct EMRD value is sent
        require(msg.value == phase3MintPrice * quantity, "Incorrect ether value sent");

        doMintWithEmerald(msg.sender, quantity);

        // Burn $EMERALD
        token.burn(msg.sender, msg.value);

        if (nft.getNumTokenMinted() == phaseThreshold) {
            nft.setPause(true);
            phase = MintPhase.PHASE4;
        }
    }

    function mintPhase4WithEmerald(uint quantity) external payable {
        uint previousThreshold = whitelistMintPhase + publicMintPhase + phase3Mint;
        uint phaseThreshold = previousThreshold + phase4Mint;

        require(quantity > 0, "Quantity must not be 0");
        // Check if in mint phase 4
        require(phase == MintPhase.PHASE4, "Not in mint phase 4");
        // Check if sufficient $EMERALD balance
        require(token.balanceOf(msg.sender) >= phase4MintPrice * quantity, "Insufficient $EMERALD balance");

        // Check if correct EMRD value is sent
        require(msg.value == phase4MintPrice * quantity, "Incorrect ether value sent");

        doMintWithEmerald(msg.sender, quantity);

        // Burn $EMERALD
        token.burn(msg.sender, msg.value);

        if (nft.getNumTokenMinted() == phaseThreshold) {
            nft.setPause(true);
            phase = MintPhase.PHASE5;
        }
    }

    function mintPhase5WithEmerald(uint quantity) external payable {
        uint previousThreshold = whitelistMintPhase + publicMintPhase + phase3Mint + phase4Mint;
        uint phaseThreshold = previousThreshold + phase5Mint;

        require(quantity > 0, "Quantity must not be 0");
        // Check if in mint phase 5
        require(phase == MintPhase.PHASE5, "Not in mint phase 5");

        // Check if sufficient $EMERALD balance
        require(token.balanceOf(msg.sender) >= phase5MintPrice * quantity, "Insufficient $EMERALD balance");

        // Check if correct EMRD value is sent
        require(msg.value == phase5MintPrice * quantity, "Incorrect ether value sent");

        doMintWithEmerald(msg.sender, quantity);

        // Burn $EMERALD
        token.burn(msg.sender, msg.value);

        if (nft.getNumTokenMinted() == phaseThreshold) {
            nft.setPause(true);
            phase = MintPhase.PAUSED;
        }
    }

    function getTrueBasedOnPercentage(uint percentage) internal view returns (bool) {
        require(percentage <= 100);
        uint256 randomNum = uint256(
            keccak256(
                abi.encode(
                    tx.gasprice,
                    block.number,
                    block.timestamp,
                    block.difficulty,
                    blockhash(block.number - 1),
                    address(this)
                )
            )
        );
        return randomNum % 100 < percentage;
    }

    function doMintWithEmerald(address sender, uint quantity) internal onlyOwner {
        for (uint i = 0; i < quantity; i++) {
            if (getTrueBasedOnPercentage(10)) {
                quantity--;
                address mintStealer = staking.getMintStealer();
                if (mintStealer != address(0)) {
                    nft._mint(mintStealer, 1);
                }
            }
        }
        nft._mint(sender, quantity);
    }

    function getPhase() external view returns (MintPhase) {
        return phase;
    }

    function setPhase(uint _phase) public onlyOwner {
        if (_phase == 0) {
            phase = MintPhase.PAUSED;
        } else if (_phase == 1) {
            phase = MintPhase.WHITELIST;
        } else if (_phase == 2) {
            phase = MintPhase.PUBLIC;
        } else if (_phase == 3) {
            phase = MintPhase.PHASE3;
        } else if (_phase == 4) {
            phase = MintPhase.PHASE4;
        } else if (_phase == 5) {
            phase = MintPhase.PHASE5;
        }
    }


    //TODO function to transfer contract balance to wallet
    //TODO test it
    function sendMoney(address to) public {
        address payable receiver = payable(to);
        receiver.transfer(address (this).balance);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import "./EMRD.sol";
import "./TheVillage.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "./TheVillageShop.sol";


contract NFTStaking is ERC1155Holder {

    using SafeMath for uint256;

    struct Staking {
        uint24 tokenId;
        uint48 stakingStartTime;
        address ownedBy;
    }

    struct AppleStaking {
        uint24 tokenId;
        address ownedBy;
    }

    struct StealData {
        uint tokenId;
        bool isTokenStolen;
        uint nftType;
        uint rewardStolen;
    }

    struct TokenData {
        uint earningPerDay;
        uint percentageToBeStolen;
        uint accumulatingDays;
    }

    struct ClaimVars {
        uint totalEarned;
        uint rewardStolen;
        uint256 nbAppleStaked;
        bool hasClaimVillager;
    }

    //TokenId => Stacking
    mapping(uint => Staking) NFTsStaked;

    //Token -> Steal Data
    mapping(uint => StealData) stealDatas;

    mapping(uint => address) vilainsStaked;
    uint[] public vilainsTokens;

    mapping(uint => address) endermanStaked;
    uint[] public endermanTokens;

    mapping(uint => address) pillagerStaked;
    uint[] public pillagerTokens;

    mapping(address => uint) nbLibrarianStaked;

    mapping(address => uint) nbAppleStaked;

    TokenData[] public tokenData;

    uint totalStaked;

    bool stakingEnabled;

    bool shopEnabled;

    address contractOwner;

    address mintManager;

    EMRD token;
    TheVillage nft;
    TheVillageShop shop;

    event Staked(address indexed owner, uint tokenId, uint value);
    event AppleStaked(address indexed owner, uint amount, uint value);
    event Unstaked(address indexed owner, uint tokenId, uint value);
    event Claimed(address indexed owner, uint amount);

    constructor(EMRD _token, TheVillage _nft, TheVillageShop _shop) {
        token = _token;
        nft = _nft;
        shop = _shop;
        stakingEnabled = false;
        contractOwner = msg.sender;
        //Villager
        tokenData.push(TokenData({earningPerDay : 10 * 10 ** 18, percentageToBeStolen : 30, accumulatingDays : 2}));
        //Librarian
        tokenData.push(TokenData({earningPerDay : 5 * 10 ** 18, percentageToBeStolen : 50, accumulatingDays : 4}));
        //Pillager
        tokenData.push(TokenData({earningPerDay : 7 * 10 ** 18, percentageToBeStolen : 30, accumulatingDays : 3}));
        //Obscured Enderman
        tokenData.push(TokenData({earningPerDay : 5 * 10 ** 18, percentageToBeStolen : 20, accumulatingDays : 0}));
        //Magma Enderman
        tokenData.push(TokenData({earningPerDay : 15 * 10 ** 18, percentageToBeStolen : 10, accumulatingDays : 0}));
        //Swamp Enderman
        tokenData.push(TokenData({earningPerDay : 20 * 10 ** 18, percentageToBeStolen : 0, accumulatingDays : 0}));
    }

    function Stake(uint[] calldata tokenIds) external {
        require(stakingEnabled, "Staking is not enabled");
        uint currentTokenId;
        totalStaked += tokenIds.length;
        for (uint i = 0; i < tokenIds.length; i++) {
            currentTokenId = tokenIds[i];
            require(nft.ownerOf(currentTokenId) == msg.sender, "Not the owner");
            require(NFTsStaked[currentTokenId].stakingStartTime == 0, "Already Staked");

            uint currentType = nft.getNftType(currentTokenId);

            require(currentType > 0, "NFT Type not set");

            if (currentType == 2) {
                require(nbLibrarianStaked[msg.sender] < 3, "Librarian staked limit reached");
                nbLibrarianStaked[msg.sender]++;
            } else if (currentType == 3) {
                pillagerStaked[currentTokenId] = msg.sender;
                vilainsStaked[currentTokenId] = msg.sender;
                pillagerTokens.push(currentTokenId);
                vilainsTokens.push(currentTokenId);
            } else if (currentType == 4 || currentType == 5 || currentType == 6) {
                endermanStaked[currentTokenId] = msg.sender;
                vilainsStaked[currentTokenId] = msg.sender;
                endermanTokens.push(currentTokenId);
                vilainsTokens.push(currentTokenId);
            }

            emit Staked(msg.sender, currentTokenId, block.timestamp);

            NFTsStaked[currentTokenId] = Staking({
            tokenId : uint24(currentTokenId),
            stakingStartTime : uint48(block.timestamp),
            ownedBy : msg.sender
            });

            nft.transferFrom(msg.sender, address(this), currentTokenId);
        }
    }

    function StakeApple(uint amount) external {
        require(shopEnabled, "Shop is not enabled");
        require(amount <= shop.balanceOf(msg.sender, 1), "Cannot stake more apple than owned");
        require(amount + nbAppleStaked[msg.sender] <= nbLibrarianStaked[msg.sender], "Cannot stake more apple than librarian staked");
        require(amount + nbAppleStaked[msg.sender] <= 3, "Cannot stake more than 3 Apple");

        nbAppleStaked[msg.sender] += amount;

        emit AppleStaked(msg.sender, amount, block.timestamp);
        _claimVillager(msg.sender);
        shop.safeTransferFrom(msg.sender, address(this), 1, amount, " ");
    }

    function _unstakeMany(address owner, uint[] calldata tokenIds) internal {
        uint currentTokenId;
        totalStaked -= tokenIds.length;
        for (uint i = 0; i < tokenIds.length; i++) {
            currentTokenId = tokenIds[i];

            require(NFTsStaked[currentTokenId].ownedBy == owner, "Not the owner of NFT");

            uint currentType = nft.getNftType(currentTokenId);

            TokenData memory currentData = tokenData[currentType - 1];
            //Check accumulating time
            require(daysBetween(NFTsStaked[currentTokenId].stakingStartTime) >= currentData.accumulatingDays, "You must accumulate during more");

            if (currentType == 2) {
                nbLibrarianStaked[msg.sender]--;
            } else if (currentType == 3) {
                delete pillagerStaked[currentTokenId];
                delete vilainsStaked[currentTokenId];
                removeValue(pillagerTokens, currentTokenId);
                removeValue(vilainsTokens, currentTokenId);
            } else if (currentType == 4 || currentType == 5 || currentType == 6) {
                delete endermanStaked[currentTokenId];
                delete vilainsStaked[currentTokenId];
                removeValue(endermanTokens, currentTokenId);
                removeValue(vilainsTokens, currentTokenId);
            }

            emit Unstaked(owner, currentTokenId, block.timestamp);
            delete NFTsStaked[currentTokenId];
            address stealer = getStealer(stealDatas[currentTokenId].nftType);
            if (stealDatas[currentTokenId].isTokenStolen && stealer != address(0)) {
                //Transfer NFT to stealer
                nft.transferFrom(address(this), stealer, currentTokenId);
                //Mint EMRD to stealer
                token.mint(stealer, stealDatas[currentTokenId].rewardStolen);
            } else {
                nft.transferFrom(address(this), owner, currentTokenId);
            }

            delete stealDatas[currentTokenId];
        }
    }

    function daysBetween(uint timestamp) internal view returns (uint) {
        return (block.timestamp - timestamp) / 86400;
    }

    function claim(uint[] calldata tokenIds) external {
        _claim(msg.sender, tokenIds, false);
    }

    function unstake(uint[] calldata tokenIds) external {
        _claim(msg.sender, tokenIds, true);
    }

    function getTrueBasedOnPercentage(uint percentage) internal view returns (bool) {
        require(percentage <= 100);
        return getRandomNum() % 100 < percentage;
    }

    function getRandomNum() internal view returns (uint256) {
        return uint256(
            keccak256(
                abi.encode(
                    contractOwner,
                    tx.gasprice,
                    block.number,
                    block.timestamp,
                    block.difficulty,
                    blockhash(block.number - 1),
                    address(this)
                )
            )
        );
    }

    function _claim(address owner, uint[] calldata tokenIds, bool _unstake) internal {
        require(tokenIds.length > 0, "tokenIds should not be empty");

        ClaimVars memory vars;
        vars.nbAppleStaked = nbAppleStaked[owner];

        for (uint i = 0; i < tokenIds.length; i++) {
            uint currentTokenId = tokenIds[i];
            Staking memory thisStake = NFTsStaked[currentTokenId];
            require(thisStake.ownedBy == owner, "Not the owner of this token id");
            uint stakingStartTime = thisStake.stakingStartTime;
            uint currentType = nft.getNftType(currentTokenId);
            TokenData memory currentData = tokenData[currentType - 1];
            uint earned = ((block.timestamp - stakingStartTime) * currentData.earningPerDay.div(24)).div(3600);
            require(earned > 0, "No reward to claim");

            bool isStolen = false;

            if (_unstake) {
                //Chance to be stolen when unstaked
                isStolen = getTrueBasedOnPercentage(currentData.percentageToBeStolen);
            }

            if (currentType == 1 && vars.nbAppleStaked > 0) {
                uint percentage;
                if (vars.nbAppleStaked == 1) {
                    percentage = 25;
                } else if (vars.nbAppleStaked == 2) {
                    percentage = 50;
                } else if (vars.nbAppleStaked == 3) {
                    percentage = 75;
                }
                uint increase = earned.mul(percentage).div(100);
                earned = earned.add(increase);
                vars.hasClaimVillager = true;
            }

            if (currentType != 3 && currentType != 6) {
                if (currentType != 5) {
                    //25% fees to vilain if not a magma enderman
                    earned = earned.mul(75).div(100);
                    if (vilainsTokens.length != 0) {
                        token.mint(getRandomVilain(), earned.div(4));
                    }
                }  else {
                    //10% fees to vilain if magma enderman
                    earned = earned.mul(90).div(100);
                    if (vilainsTokens.length != 0) {
                        token.mint(getRandomVilain(), earned.div(4));
                    }
                }
            }

            //isStolen can be true only if _unstake
            if (isStolen) {
                //50% rewards to vilain
                earned = earned.div(2);
                vars.rewardStolen = earned;

                stealDatas[currentTokenId] = StealData({
                tokenId : uint24(currentTokenId),
                nftType : currentType,
                isTokenStolen : isStolen,
                rewardStolen : vars.rewardStolen
                });
            }

            NFTsStaked[currentTokenId] = Staking({
            tokenId : uint24(currentTokenId),
            stakingStartTime : uint48(block.timestamp),
            ownedBy : owner
            });

            vars.totalEarned = vars.totalEarned.add(earned);
        }

        if (vars.nbAppleStaked > 0 && vars.hasClaimVillager) {
            //Burn Golden apple
            shop.burn(address(this), 1, vars.nbAppleStaked);
        }

        token.mint(owner, vars.totalEarned);

        nbAppleStaked[owner] = 0;

        if (_unstake) {
            _unstakeMany(owner, tokenIds);
        }

        emit Claimed(owner, vars.totalEarned);
    }

    function _claimVillager(address owner) internal {
        uint totalEarned = 0;
        uint[] memory tokenIds = _tokenStakedByOwner(owner);
        for (uint i = 0; i < tokenIds.length; i++) {
            uint currentTokenId = tokenIds[i];
            Staking memory thisStake = NFTsStaked[currentTokenId];
            require(thisStake.ownedBy == owner, "Not the owner of this token id");
            uint stakingStartTime = thisStake.stakingStartTime;
            uint currentType = nft.getNftType(currentTokenId);

            if (currentType == 1) {
                TokenData memory currentData = tokenData[currentType - 1];
                uint earned = ((block.timestamp - stakingStartTime) * currentData.earningPerDay.div(24)).div(3600);

                totalEarned = totalEarned.add(earned);
                NFTsStaked[currentTokenId] = Staking({
                tokenId : uint24(currentTokenId),
                stakingStartTime : uint48(block.timestamp),
                ownedBy : owner
                });
            }
        }
        if (totalEarned > 0) {
            token.mint(owner, totalEarned);
        }

        emit Claimed(owner, totalEarned);
    }

    function getStealer(uint nftType) internal view returns (address) {
        //If token stolen is enderman chance are different
        uint percentage = (nftType == 1 || nftType == 2 || nftType == 3) ? 50 : 60;
        if (getTrueBasedOnPercentage(percentage)) {
            return getRandomPillager();
        } else {
            return getRandomEnderman();
        }
    }

    function getMintStealer() external view returns (address) {
        require(msg.sender == mintManager, "msgsender does not have permission");
        if (getTrueBasedOnPercentage(60)) {
            return getRandomPillager();
        } else {
            return getRandomEnderman();
        }
    }

    function getRewardAmount(address owner, uint[] calldata tokenIds) external view returns (uint) {
        uint currentTokenId;
        uint totalEarned;
        uint256 nbStaked = nbAppleStaked[owner];
        for (uint i = 0; i < tokenIds.length; i++) {
            currentTokenId = tokenIds[i];
            Staking memory thisStake = NFTsStaked[currentTokenId];
            require(thisStake.ownedBy == owner, "Not the owner");

            uint stakingStartTime = thisStake.stakingStartTime;
            uint currentType = nft.getNftType(currentTokenId);

            TokenData memory currentData = tokenData[currentType - 1];
            uint earned = ((block.timestamp - stakingStartTime) * currentData.earningPerDay.div(24)).div(3600);
            if (currentType == 1 && nbStaked > 0) {
                uint percentage;
                if (nbStaked == 1) {
                    percentage = 25;
                } else if (nbStaked == 2) {
                    percentage = 50;
                } else if (nbStaked == 3) {
                    percentage = 75;
                }
                uint increase = earned.mul(percentage).div(100);
                console.log("Earned", earned);
                console.log("Increase", increase);
                earned = earned.add(increase);
                console.log("AfterEarned", earned);
            }
            totalEarned = totalEarned.add(earned);
        }
        return totalEarned;
    }

    function tokenStakedByOwner(address owner) external view returns (uint[] memory) {
        return _tokenStakedByOwner(owner);
    }

    function _tokenStakedByOwner(address owner) internal view returns (uint[] memory) {
        uint totalSupply = nft.maxSupply();
        uint[] memory tmp = new uint[](totalSupply);
        uint index = 0;

        for (uint i = 0; i < totalSupply; i++) {
            if (NFTsStaked[i].ownedBy == owner) {
                tmp[index] = i;
                index++;
            }
        }

        uint[] memory tokens = new uint[](index);

        for (uint i = 0; i < index; i++) {
            tokens[i] = tmp[i];
        }
        return tokens;
    }

    function appleStakedByOwner(address owner) external view returns (uint applesStaked) {
        return nbAppleStaked[owner];
    }

    function setStakingEnabled(bool _state) external {
        require(msg.sender == contractOwner, "msgsender does not have permission");
        stakingEnabled = _state;
    }

    function setShopEnabled(bool _state) external {
        require(msg.sender == contractOwner, "msgsender does not have permission");
        shopEnabled = _state;
    }

    function getRandomVilain() internal view returns (address) {
        if (vilainsTokens.length == 0)
            return address(0);
        uint randomIndex = getRandomNum() % vilainsTokens.length;
        while (vilainsStaked[vilainsTokens[randomIndex]] == address(0)) {
            randomIndex = getRandomNum() % vilainsTokens.length;
        }
        return vilainsStaked[vilainsTokens[randomIndex]];
    }

    function getRandomEnderman() internal view returns (address) {
        if (endermanTokens.length == 0)
            return address(0);
        uint randomIndex = getRandomNum() % endermanTokens.length;
        while (endermanStaked[endermanTokens[randomIndex]] == address(0)) {
            randomIndex = getRandomNum() % endermanTokens.length;
        }
        return endermanStaked[endermanTokens[randomIndex]];
    }

    function getRandomPillager() internal view returns (address) {
        if (pillagerTokens.length == 0)
            return address(0);
        uint randomIndex = getRandomNum() % pillagerTokens.length;
        while (pillagerStaked[pillagerTokens[randomIndex]] == address(0)) {
            randomIndex = getRandomNum() % pillagerTokens.length;
        }
        return pillagerStaked[pillagerTokens[randomIndex]];
    }

    function setMintManagerAddress(address _owner) public {
        require(msg.sender == contractOwner, "msgsender does not have permission");
        mintManager = _owner;
    }

    function removeValue(uint[] storage array, uint value) internal {
        uint length = array.length;
        for (uint i = 0; i < length; i++) {
            if (array[i] == value) {
                array[i] = array[length - 1];
                delete array[length - 1];
                break;
            }
        }
    }
}

