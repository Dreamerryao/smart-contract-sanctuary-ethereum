    address GGContract;
    address WETHContract;
    function setWETHContract(address _addy) public onlyOwner {
        WETHContract = _addy;
    }

    function setGGContract(address _contract) public onlyOwner {
        GGContract = _contract;
    }

    function setCharContract(address _charContract) public onlyOwner {
        charContract = _charContract;
    }

    function setGameCoverContract(address _gameCoverContract) public onlyOwner {
        gameCoverContract = _gameCoverContract;
    }

    function newCharacter() public {
        require(msg.sender == charContract || msg.sender == owner, "Invalid Caller");
        idToAmountAvailable[0] += 200;
        idToAmountAvailable[1] += 150;
        idToAmountAvailable[2] += 100;
        idToAmountAvailable[3] += 80;
        idToAmountAvailable[4] += 60;
        idToAmountAvailable[5] += 40;
        idToAmountAvailable[6] += 30;
        idToAmountAvailable[7] += 30;
        idToAmountAvailable[8] += 25;
        idToAmountAvailable[9] += 5;
        idToAmountAvailable[10] += 1;
    }

    function newGameCover() public {
        require(msg.sender == gameCoverContract || msg.sender == owner, "Invalid Caller");
        idToAmountAvailable[0] += 200;
        idToAmountAvailable[1] += 150;
        idToAmountAvailable[2] += 100;
        idToAmountAvailable[3] += 80;
        idToAmountAvailable[4] += 60;
        idToAmountAvailable[5] += 40;
        idToAmountAvailable[6] += 30;
        idToAmountAvailable[7] += 30;
        idToAmountAvailable[8] += 25;
        idToAmountAvailable[9] += 5;
        idToAmountAvailable[10] += 1;
    }

    function mintPotion(uint256 _id, address _to, uint8 _amount) public {
        if (_id == 10 || _id == 5 || _id == 500) {
            require(isTeam(msg.sender), "Not available to public");
        }
        require(_id < 11, "Invalid potion");
        require(idToAmountAvailable[_id]>0, "Max Supply Reached");
        ERC20(GGContract).transferFrom(_to, address(this), idToPriceGG[_id]);
        for(uint8 i = 0 ; i < _amount; i++){
        _mint(msg.sender,_id,1,"");
        idToAmountAvailable[_id] -= 1;
        }
    }

    function initialAirdropPotion(uint256 _id, address[] memory holders) public {
        require(isTeam(msg.sender));
        //newCharacter();
        for (uint i=0; i < holders.length; i++) {
            _mint(holders[i], _id, 1, "");
            idToAmountAvailable[_id] -= 1;
        }
    }

    modifier onlyCharacterContract {
        require(msg.sender == charContract, "Invalid Caller");
        _;
    }
    
    function burnPotion(address user, uint256 _id) public {
        _burn(user,_id,1);
        _mint(user, EMPTY,1,"");
    } 

    function burnEmptyBottle(address user, uint256 _id) public {
        _burn(user ,_id, 1);
    }  

    function setPrice(uint256 _id, uint256 _newP) public {
        require(isTeam(msg.sender));
        idToPriceGG[_id] = _newP;
    } 
    
    function uri(uint256 _id) override public view returns (string memory) {
        return string(abi.encodePacked(
            baseURI,
            Strings.toString(_id),
            ".json")
        );
    }

    function isTeam(address[] calldata _users) public onlyOwner {
      delete teamAddresses;
      teamAddresses = _users;
    }

    function isTeam(address _user) public view returns (bool) {
      for (uint i = 0; i < teamAddresses.length; i++) {
        if (teamAddresses[i] == _user) {
            return true;
        }
      }
      return false;
    }


    function extractWETH() external onlyOwner {
      ERC20(WETHContract).transfer(msg.sender, ERC20(WETHContract).balanceOf(address(this)));
    }

    function extractGG() external onlyOwner {
      ERC20(GGContract).transfer(msg.sender, ERC20(WETHContract).balanceOf(address(this)));
    }

}