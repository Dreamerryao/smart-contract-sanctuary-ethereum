contract Now {
    uint256 m;

    function x(uint248 n, uint8 z) external {
        if (z != 0) {
            m = n;
        }
    }
    
    function a(uint256 n1, uint256[] memory n2) external {
        if (n2[0] != 0) {
            m = n1 + n2[0];
        }
    }
}