// File: default_workspace/TokenGrowth.sol


pragma solidity >=0.8.7 <0.9.0;





contract TokenGrowth is ReentrancyGuard{

    address public constant primaryToken = address(0xc48b2A0b1d7582b244fbb3A0DD13e06d4044d774);

    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;
    address public constant deadAddress = address(0xdead);

    mapping(address=>bool) public _approvedAddress;
    mapping(address => uint256) _indexMapping;
    mapping(address => uint256) _LPWarningCount;
    address[] public approvedLPAddress;
    uint256 public length;
    uint256 index;
    uint256 constant fee = 0.005 ether;

    constructor(){
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).getPair(primaryToken, _uniswapV2Router.WETH());
        uniswapV2Router = _uniswapV2Router;
        _setLPAddress(address(0xDaBC82501330EC330Eb1daD8A8277d242985d04A), true); 
        _setLPAddress(address(0x3b0B45274FF3F7aDB8a5b36E12F4ea705E34Ad96), true); 

    }

    function _checkLPCreditablility(address _LP)internal{
        uint256 minium = IERC20(primaryToken).balanceOf(uniswapV2Pair)/200;
        if(minium > IERC20(primaryToken).balanceOf(_LP)){
            _LPWarningCount[_LP]++;
        }
        if(_LPWarningCount[_LP] > 5){
            _setLPAddress(_LP, false);
        }

    }

    function addLPAddress(address _LP)external payable nonReentrant {

        require(!_approvedAddress[_LP], "Already Approved");
        require(msg.value >= 0.005 ether, "Not enough funding...");
        IUniswapV2Pair pair = IUniswapV2Pair(_LP);
        IUniswapV2Factory facts = IUniswapV2Factory(uniswapV2Router.factory());
        address token0 = pair.token0();
        address token1 = pair.token1();
        address testing = facts.getPair(token0, token1);
        require(_LP == testing && testing != address(0), "LP Address Not in uniswap");
        require(token0 == primaryToken || token1 == primaryToken, "Not Token LP");
        require(IERC20(primaryToken).balanceOf(_LP) >= IERC20(primaryToken).balanceOf(uniswapV2Pair)/100, "Not enough Liquidity");
        _addLiquidity(token0, token1, msg.value);
        _sync(_LP, token0);
        _sync(_LP, token1);

        _setLPAddress(_LP, true);

    }

    function _sync(address _LP, address token)internal{
        IUniswapV2Pair pair = IUniswapV2Pair(_LP);
        IERC20(token).transfer(_LP, IERC20(token).balanceOf(address(this)));
        pair.sync();
    }

    function _addLiquidity(address token0, address token1, uint256 eth) internal {
        uint256 amount = eth/2;
        uint256 amount0 = _buyTokens(token0, amount);
        uint256 amount1 = _buyTokens(token1, amount);
        _resetApproval(token0);
        _resetApproval(token1);
        uniswapV2Router.addLiquidity(token0, token1, amount0, amount1, 0, 0, deadAddress, block.timestamp);
    }

    function _resetApproval(address token)internal{
        IERC20(token).approve(address(uniswapV2Router), type(uint256).max);
        //IERC20(token).increaseAllowance(address(uniswapV2Router), type(uint256).max);
    }

    function _buyTokens(address token, uint256 value)internal returns(uint256){

        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = token;
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: value}(0, path, address(this), block.timestamp);
        return IERC20(token).balanceOf(address(this));
    }

    function _setLPAddress(address _LP, bool truth)internal{
        
        if(truth){
            _indexMapping[_LP] = length;
            approvedLPAddress.push(_LP);
            length++;
        }else{
            uint i = _indexMapping[_LP];
            require(_LP == approvedLPAddress[i], "Addresses Don't match");
            approvedLPAddress[i] = approvedLPAddress[length-1];
            _indexMapping[approvedLPAddress[i]] = i;
            length--;
            index %= length;
        }

        _approvedAddress[_LP] = truth;

    }

    function grow()external nonReentrant{
        if(address(this).balance >= 0.005 ether){_grow();}
    }

    function _grow()internal{
        _checkLPCreditablility(approvedLPAddress[index]);
        _buyTokens(primaryToken, 0.005 ether);
        _sync(approvedLPAddress[index], primaryToken);
        index = (index+1)%length;
    }

    function swapTokenForEth(address token) external nonReentrant{

       address LP = IUniswapV2Factory(uniswapV2Router.factory()).getPair(primaryToken, uniswapV2Router.WETH());
       require(LP != address(0), "No eth LP found");
        uint256 tokenAmount = IERC20(token).balanceOf(address(this));
        address[] memory path = new address[](2);
        path[0] = token;
        path[1] = uniswapV2Router.WETH();
        _resetApproval(token);
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
        if(address(this).balance >= 0.005 ether){_grow();}
    }
    
    receive()external payable{}
}