pragma solidity 0.8.14;
import "hardhat/console.sol";
import "./interfaces/INFTAuction.sol";
import "./interfaces/ILiqdLending.sol";
import "./interfaces/ILiqdLendingCore.sol";
import "./interfaces/IInstantLiquidityVault.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./libraries/DataTypes.sol";

contract InstantLiquidityVault is
    IInstantLiquidityVault,
    Ownable,
    ReentrancyGuard,
    AccessControl
{
    using SafeERC20 for IERC20;

    bytes4 internal constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 internal constant INTERFACE_ID_ERC1155 = 0xd9b67a26;
    bytes32 public constant ADMIN_ROLE = keccak256("ADMI_ROLE");

    /// @notice emitted when LiqidLending contract is set
    event UsdbLendingSet(address usdblending, bool status, uint256 timestamp);
    event PoolCreated(
        uint256 indexed pid,
        string indexed name,
        address collection,
        address currency,
        uint64 apr,
        uint64 duration,
        uint64 fee,
        uint256 floorPrice
    );
    event PoolUpdated(
        uint256 indexed pid,
        uint64 _apr,
        uint64 duration,
        uint64 fee
    );
    event FloorPriceUpdated(uint256 _pid, uint256 _floorPrice);
    event Deposited(uint256 indexed pid, address indexed user, uint256 amount);
    event Borrowed(
        address indexed _usdblending,
        uint256 indexed borrowId,
        address indexed user,
        uint256 amount,
        uint256 tokenId
    );
    event Withdrawn(uint256 indexed pid, address indexed user, uint256 amount);
    event Repaid(address indexed _usdblending, uint256 indexed loanId);
    event Liquidated(address indexed _usdblending, uint256 indexed borrowedId);
    event CreatedAuctionExternal(
        address indexed _usdblending,
        uint256 _loanId,
        address _marketplace
    );
    event ListingDurationUpdated(uint64 duration);
    event SoldWithAuction(address _usdblending, uint256 _loanId);
    event MigratedFrom(
        address _poolFrom,
        address _owner,
        uint256 _pid,
        uint256 _amount
    );
    event MigratedTo(
        address _poolFrom,
        address _to,
        uint256 _pid,
        uint256 _amount
    );
    event WithdrawnBenefit(
        address _currency,
        uint256 _amount,
        address _beneficiary
    );
    enum Status {
        NONE,
        LISTED,
        SOLD
    }
    struct PoolInfo {
        string name;
        address collection;
        address currency;
        uint64 apr;
        uint64 duration;
        uint64 fee;
        uint256 floorPrice;
    }
    struct DepositInfo {
        address user;
        uint256 amount;
    }
    struct BorrowInfo {
        DepositInfo[] deposits;
        uint256 pid;
        uint256 listingId;
        Status status;
    }

    uint256 public pid = 0;
    uint256 public borrowedId = 0;
    uint64 public listingDuration = 3 days;

    /// @notice usdlendingcontract => available
    mapping(address => bool) public usdbLendingContracts;
    mapping(uint256 => uint256) public lastBorrowedIndex;
    mapping(uint256 => PoolInfo) public pools;
    mapping(uint256 => BorrowInfo) public borrows;
    mapping(uint256 => uint256) public benefit;
    mapping(uint256 => DepositInfo[]) public poolDeposits;
    mapping(uint256 => uint256) public poolAmount;
    mapping(uint256 => mapping(address => uint256)) availAmount; // pid=>user=>availableAmount
    mapping(address => uint256) auctionBenefit;

    address public auction;
    address public usdblending;

    modifier validUsdbLending(address _usdblending) {
        require(usdbLendingContracts[_usdblending], "INV_USDBLENDING");
        _;
    }

    constructor(address _auction) {
        require(_auction != address(0), "INVALID_AUCTION");
        auction = _auction;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /// @param _usdblending address of new usdblending
    /// @param _status returns if available
    function setUsdbLending(address _usdblending, bool _status)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(_usdblending != address(0), "INV_USDBLENDING");

        emit UsdbLendingSet(_usdblending, _status, block.timestamp);

        usdbLendingContracts[_usdblending] = _status;
    }

    function createPool(
        string memory _name,
        address _collection,
        address _currency,
        uint64 _apr,
        uint64 _duration,
        uint64 _fee,
        uint256 _floorPrice
    ) external onlyRole(ADMIN_ROLE) {
        require(bytes(_name).length != 0, "INVALID_NAME");
        require(_collection != address(0), "INVALID_COLLECTION");
        require(_currency != address(0), "INVALID_CURRENCY");
        require(_duration != 0, "INVALID_DURATION");
        require(_floorPrice != 0, "INVALID_PRICE");
        require(
            IERC165(_collection).supportsInterface(INTERFACE_ID_ERC1155) ||
                IERC165(_collection).supportsInterface(INTERFACE_ID_ERC721),
            "INVALID_COLLECTION"
        );

        uint256 _pid = _getNextPID();
        _incrementPID();

        PoolInfo memory newPool = PoolInfo({
            name: _name,
            collection: _collection,
            currency: _currency,
            apr: _apr,
            duration: _duration,
            fee: _fee,
            floorPrice: _floorPrice
        });

        pools[_pid] = newPool;

        emit PoolCreated(
            _pid,
            _name,
            _collection,
            _currency,
            _apr,
            _duration,
            _fee,
            _floorPrice
        );
    }

    function updatePool(
        uint256 _pid,
        uint64 _apr,
        uint64 _duration,
        uint64 _fee
    ) external onlyRole(ADMIN_ROLE) {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_duration != 0, "INVALID_DURATION");

        PoolInfo storage pool = pools[_pid];
        pool.apr = _apr;
        pool.duration = _duration;
        pool.fee = _fee;

        emit PoolUpdated(_pid, _apr, _duration, _fee);
    }

    function updateFloorPrice(uint256 _pid, uint256 _floorPrice)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_floorPrice != 0, "INVALID_FloorPrice");

        PoolInfo storage pool = pools[_pid];
        pool.floorPrice = _floorPrice;

        emit FloorPriceUpdated(_pid, _floorPrice);
    }

    function updateListingDuration(uint64 _listingDuration)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(_listingDuration != 0, "INVALID_DURATION");

        listingDuration = _listingDuration;
        emit ListingDurationUpdated(listingDuration);
    }

    function withdraw(uint256 _pid, uint256 _amount) public nonReentrant {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_amount != 0 && _amount <= poolAmount[_pid], "INVALID_AMOUNT");

        uint256 availableAmount = availAmount[_pid][msg.sender];
        require(_amount <= availableAmount, "AMOUNT_TOO_LARGE");

        poolAmount[_pid] -= _amount;
        availAmount[_pid][msg.sender] -= _amount;

        IERC20(pools[_pid].currency).safeTransfer(msg.sender, _amount);

        emit Withdrawn(_pid, msg.sender, _amount);
    }

    function deposit(uint256 _pid, uint256 _amount) external nonReentrant {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_amount != 0, "INVALID_AMOUNT");

        DepositInfo memory _deposit = DepositInfo({
            user: msg.sender,
            amount: _amount
        });

        IERC20(pools[_pid].currency).safeTransferFrom(
            msg.sender,
            address(this),
            _amount
        );

        poolAmount[_pid] += _amount;
        availAmount[_pid][msg.sender] += _amount;

        poolDeposits[_pid].push(_deposit);

        emit Deposited(_pid, msg.sender, _amount);
    }

    function borrow(
        address _usdblending,
        uint256 _pid,
        uint256 _amount,
        uint256 _tokenId
    )
        external
        nonReentrant
        validUsdbLending(_usdblending)
        returns (uint256 loanId)
    {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_amount <= poolAmount[_pid], "INVALID_AMOUNT");
        require(
            _amount <= pools[_pid].floorPrice / 2,
            "AMOUNT_REACHED_TO_BORROW_LIMIT"
        );

        uint256 sum;
        uint256 borrowedIndex;

        DepositInfo[] memory deposits = poolDeposits[_pid];
        for (uint256 i = lastBorrowedIndex[_pid]; i < deposits.length; i++) {
            sum += deposits[i].amount;
            if (sum >= _amount) {
                borrowedIndex = i;
                break;
            }
        }
        BorrowInfo storage borrowInfo = borrows[loanId];
        borrowInfo.pid = _pid;

        if (borrowedIndex > lastBorrowedIndex[_pid]) {
            for (uint256 i = lastBorrowedIndex[_pid]; i < borrowedIndex; i++) {
                borrowInfo.deposits.push(deposits[i]);

                // Update Borrowed Total Amounts
                availAmount[_pid][deposits[i].user] -= deposits[i].amount;
            }
            borrowInfo.deposits.push(
                DepositInfo({
                    amount: deposits[borrowedIndex].amount - (sum - _amount),
                    user: deposits[borrowedIndex].user
                })
            );
        } else {
            borrowInfo.deposits.push(
                DepositInfo({
                    amount: _amount,
                    user: deposits[borrowedIndex].user
                })
            );
        }

        deposits[borrowedIndex].amount = sum - _amount;

        lastBorrowedIndex[_pid] = borrowedIndex;

        if (
            IERC20(pools[_pid].currency).allowance(
                address(this),
                _usdblending
            ) < _amount
        ) {
            IERC20(pools[_pid].currency).approve(_usdblending, 2**256 - 1);
        }

        loanId = _createLoan(_usdblending, _pid, _amount, _tokenId);

        emit Borrowed(_usdblending, loanId, msg.sender, _amount, _tokenId);
    }

    function repay(address _usdblending, uint256 _loanId)
        external
        nonReentrant
        validUsdbLending(_usdblending)
    {
        ILiqdLendingCore.Loan memory loan = ILiqdLendingCore(_usdblending)
            .getLoan(_loanId);

        require(loan.lender == address(this), "INVALID_LENDER");
        BorrowInfo memory borrowedInfo = borrows[_loanId];
        require(
            loan.status == ILiqdLendingCore.Status.REPAID ||
                INFTAuction(auction).isNFTSold(borrowedInfo.listingId) ||
                borrowedInfo.status == Status.SOLD,
            "INVALID_LOAN"
        );

        DepositInfo[] storage poolDeposit = poolDeposits[borrowedInfo.pid];

        for (uint256 i = 0; i < borrowedInfo.deposits.length; i++) {
            borrowedInfo.deposits[i].amount =
                (borrowedInfo.deposits[i].amount * loan.amountDue) /
                loan.loanAmount;

            poolDeposit.push(borrowedInfo.deposits[i]);
            // Update Deposit Info
            availAmount[borrowedInfo.pid][
                borrowedInfo.deposits[i].user
            ] += borrowedInfo.deposits[i].amount;
        }

        delete borrows[_loanId];
        emit Repaid(_usdblending, _loanId);
    }

    function liquidate(address _usdblending, uint256 _loanId)
        external
        nonReentrant
        validUsdbLending(_usdblending)
    {
        ILiqdLendingCore.Loan memory loan = ILiqdLendingCore(_usdblending)
            .getLoan(_loanId);

        require(loan.lender == address(this), "INVALID_LENDER");
        require(
            loan.status == ILiqdLendingCore.Status.LIQUIDATED,
            "INVALID_LOAN"
        );

        BorrowInfo memory borrowedInfo = borrows[_loanId];
        require(borrowedInfo.status != Status.LISTED, "ALREADY_LISTED");

        address _nftToken = loan.nftAddress;
        if (IERC165(_nftToken).supportsInterface(INTERFACE_ID_ERC1155)) {
            IERC1155(_nftToken).setApprovalForAll(auction, true);
        } else {
            IERC721(_nftToken).approve(auction, loan.nftTokenId);
        }

        uint256 _listingId = INFTAuction(auction).createAuction(
            _nftToken,
            loan.currency,
            address(this),
            loan.nftTokenId,
            loan.amountDue,
            listingDuration,
            uint64(block.timestamp)
        );

        borrows[_loanId].status = Status.LISTED;
        borrows[_loanId].listingId = _listingId;

        emit Liquidated(_usdblending, _loanId);
    }

    function soldWithAuction(
        address _usdblending,
        uint256 _loanId,
        uint256 _amount
    ) external nonReentrant onlyRole(ADMIN_ROLE) {
        ILiqdLendingCore.Loan memory loan = ILiqdLendingCore(_usdblending)
            .getLoan(_loanId);
        require(_amount >= loan.amountDue, "AMOUNT_TOO_SMALL");
        require(loan.lender == address(this), "INVALID_LENDER");

        if (_amount != loan.amountDue) {
            auctionBenefit[loan.currency] += _amount - loan.amountDue;
        }

        borrows[_loanId].status = Status.SOLD;

        emit SoldWithAuction(_usdblending, _loanId);
    }

    /// @param _currency address of currency
    /// @param _amount amount to withdraw
    function withdrawBenefit(address _currency, uint256 _amount)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(auctionBenefit[_currency] >= _amount, "AMOUNT_TOO_LARGE");

        auctionBenefit[_currency] = 0;

        if (_currency == address(0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE)) {
            (bool toSuccess, ) = msg.sender.call{value: _amount}("");
            require(toSuccess, "TX_FAILED");
        } else {
            IERC20(_currency).safeTransfer(msg.sender, _amount);
        }
        emit WithdrawnBenefit(_currency, _amount, msg.sender);
    }

    function createAuctionExternal(
        address _usdblending,
        uint256 _loanId,
        address _marketplace
    )
        external
        nonReentrant
        onlyRole(ADMIN_ROLE)
        validUsdbLending(_usdblending)
    {
        ILiqdLendingCore.Loan memory loan = ILiqdLendingCore(_usdblending)
            .getLoan(_loanId);

        require(loan.lender == address(this), "INVALID_LENDER");
        BorrowInfo memory borrowedInfo = borrows[_loanId];
        require(
            borrowedInfo.status == Status.LISTED &&
                INFTAuction(auction).isNFTCancelled(borrowedInfo.listingId),
            "INVALID_LOAN"
        );

        address _nftToken = loan.nftAddress;
        if (IERC165(_nftToken).supportsInterface(INTERFACE_ID_ERC1155)) {
            IERC1155(_nftToken).setApprovalForAll(_marketplace, true);
        } else {
            IERC721(_nftToken).approve(_marketplace, loan.nftTokenId);
        }
        emit CreatedAuctionExternal(_usdblending, _loanId, _marketplace);
    }

    function migrateTo(
        address _poolFrom,
        uint256 _pid,
        uint256 _amount
    ) external nonReentrant {
        withdraw(_pid, _amount);
        IInstantLiquidityVault(_poolFrom).migrateFrom(
            _poolFrom,
            msg.sender,
            _pid,
            _amount
        );

        emit MigratedTo(_poolFrom, msg.sender, _pid, _amount);
    }

    function migrateFrom(
        address _poolFrom,
        address _owner,
        uint256 _pid,
        uint256 _amount
    ) external nonReentrant {
        require(_pid != 0 && _pid <= pid, "INVALID_PID");
        require(_amount != 0, "INVALID_AMOUNT");

        DepositInfo memory _deposit = DepositInfo({
            user: _owner,
            amount: _amount
        });

        IERC20(pools[_pid].currency).safeTransferFrom(
            _owner,
            address(this),
            _amount
        );

        poolAmount[_pid] += _amount;
        availAmount[_pid][_owner] += _amount;

        poolDeposits[_pid].push(_deposit);

        emit MigratedFrom(_poolFrom, _owner, _pid, _amount);
    }

    function _getNextPID() private view returns (uint256) {
        return pid + 1;
    }

    function _incrementPID() private {
        pid++;
    }

    function _createLoan(
        address _usdblending,
        uint256 _pid,
        uint256 _amount,
        uint256 _tokenId
    ) internal returns (uint256 loanId) {
        address _nftAddress = pools[_pid].collection;
        uint256 _duration = pools[_pid].duration;
        address _currency = pools[_pid].currency;
        uint8 _nftTokenType;

        if (IERC165(_nftAddress).supportsInterface(INTERFACE_ID_ERC1155)) {
            _nftTokenType = 1;
        }

        poolAmount[_pid] -= _amount;

        loanId = ILiqdLending(_usdblending).createLoanExternal(
            DataTypes.LoanPayload({
                lender: address(this),
                borrower: msg.sender,
                nftAddress: _nftAddress,
                currency: _currency,
                nftTokenId: _tokenId,
                duration: _duration,
                expiration: block.timestamp + _duration,
                loanAmount: _amount,
                apr: pools[_pid].apr,
                nftTokenType: _nftTokenType
            })
        );
    }

    function getBorrowDeposits(uint256 borrowId)
        external
        view
        returns (DepositInfo[] memory)
    {
        return borrows[borrowId].deposits;
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public pure returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public pure returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

