import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

/// @author Tomas C. Imach
/// @title Staking contract for Exactly Solidity Challenge
contract ETHPool is Pausable, AccessControl {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    Counters.Counter public totalStakes;
    Counters.Counter public totalRewards;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant TEAM_ROLE = keccak256("TEAM_ROLE");
    
    struct StakeInfo {
        address stakerAddress;
        uint256 stakeAmount;
        uint256 blockNumber;
    }

    struct RewardInfo {
        uint256 reward;
        uint256 totalStaked;
        uint256 blockNumber;
    }

    mapping (uint256 => StakeInfo) public stakesInfo;
    mapping (uint256 => RewardInfo) public rewardsInfo;

    bool internal locked;

    modifier noReentrant() {
        require(!locked, "No re-entrancy");
        locked = true;
        _;
        locked = false;
    }

    event Stake(address indexed stakerAddress, uint256 stakeAmount, uint256 blockNumber);
    event Unstake(address indexed stakerAddress, uint256 stakeAmount, uint256 rewardAmount);
    event DepositReward(uint256 rewardAmount, uint256 totalStaked, uint256 blockNumber);

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(TEAM_ROLE, msg.sender);
    }

    function depositReward() external payable onlyRole(TEAM_ROLE) {
        uint256 _reward = msg.value;
        require(_reward > 0, "ETHPool: reward amount < 0");

        uint256 _totalStakedAmount = totalStakedAmount();

        // create new reward
        uint256 _newRewardId = totalRewards.current();
        rewardsInfo[_newRewardId] = RewardInfo({
            reward: _reward,
            totalStaked: _totalStakedAmount,
            blockNumber: block.number
        });

        totalRewards.increment();

        emit DepositReward(_reward, _totalStakedAmount, block.number);
    }

    function stake() external payable {
        uint256 _amount = msg.value;
        require(_amount > 0, "ETHPool: stake amount < 0");

        // create new stake
        uint256 _newStakeId = totalStakes.current();
        stakesInfo[_newStakeId] = StakeInfo({
            stakerAddress: msg.sender,
            stakeAmount: _amount,
            blockNumber: block.number
        });

        totalStakes.increment();

        emit Stake(msg.sender, _amount, block.number);
    }

    function unstake() external payable noReentrant() {
        uint256[] memory _stakeIdsOfAddress = stakeIdsOfAddress(msg.sender);

        uint256 _rewardAmount = getTotalRewards(msg.sender);

        uint256 _totalStakedAmount = 0;
        for (uint256 i = 0; i < _stakeIdsOfAddress.length; i++) {
            _totalStakedAmount = stakesInfo[_stakeIdsOfAddress[i]].stakeAmount;
            delete stakesInfo[_stakeIdsOfAddress[i]]; // gas saving
        }
        require(_totalStakedAmount > 0, "ETHPool: nothing staked");

        bool isSent = payable(msg.sender).send(_totalStakedAmount.add(_rewardAmount));
        require(isSent, "ETHPool: Failed to send ETH");

        emit Unstake(msg.sender, _totalStakedAmount, _rewardAmount);
    }

    function stakeIdsOfAddress(address _address) public view returns(uint256[] memory) {
        uint256 _numberOfStakesId = 0;
        for (uint256 i = 0; i < totalStakes.current(); i++) {
            if (stakesInfo[i].stakerAddress == _address) {
                _numberOfStakesId++;
            }
        }
        require(_numberOfStakesId > 0, "ETHPool: no stake IDs for this address");

        uint256[] memory _stakeIdsOfAddress = new uint256[](_numberOfStakesId);
        uint256 j = 0;
        uint256 k = 0;
        while (k < _numberOfStakesId) {
            if (stakesInfo[j].stakerAddress == _address) {
                _stakeIdsOfAddress[k] = j;
                k++;
            }
            j++;
        }
        return _stakeIdsOfAddress;
    }

    function totalStakedAmount() public view returns(uint256) {
        uint256 _totalStaked = 0;
        for (uint256 i = 0; i < totalStakes.current(); i++) {
            _totalStaked += stakesInfo[i].stakeAmount;
        }
        return _totalStaked;
    }

    function getTotalStakedByAddress(address _address) public view returns(uint256) {
        uint256[] memory _stakeIdsOfAddress = stakeIdsOfAddress(_address);
        uint256 _totalStakedAmount = 0;
        for (uint256 i = 0; i < _stakeIdsOfAddress.length; i++) {
            _totalStakedAmount = stakesInfo[_stakeIdsOfAddress[i]].stakeAmount;
        }
        return _totalStakedAmount;
    }
    
    function getTotalRewards(address _address) public view returns(uint256) {
        uint256[] memory _stakeIdsOfAddress = stakeIdsOfAddress(_address);
        uint256 _totalRewards = 0;
        for (uint256 i = 0; i < _stakeIdsOfAddress.length; i++) {
            _totalRewards += _getRewardPerStakeId(_stakeIdsOfAddress[i]);
        }
        return _totalRewards;
    }

    function _getRewardPerStakeId(uint256 _stakeId) private view returns(uint256) {
        StakeInfo memory _stakeInfo = stakesInfo[_stakeId];
        uint256 _reward = 0;

        for (uint256 i = 0; i < totalRewards.current(); i++) {
            RewardInfo memory _rewardInfo = rewardsInfo[i];
            if (_rewardInfo.blockNumber > _stakeInfo.blockNumber) {
                _reward += _stakeInfo.stakeAmount.mul(_rewardInfo.reward).div(_rewardInfo.totalStaked);
            }
        }

        return _reward;
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }
}

