import "./RPSCHECKS721.sol";
import "./interfaces/IRPSChecks.sol";
import "./libs/RPSArt.sol";
import "./libs/RPSMetadata.sol"; 
import "./libs/Utils.sol";
import "erc721psi/contracts/extension/ERC721PsiBurnable.sol";

import "./utils/OwnableAdmin.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "solidity-bits/contracts/BitMaps.sol";

import "hardhat/console.sol";

contract RPSChecks is
    ERC721PsiBurnable,
    IRPSChecks,
    ReentrancyGuard,
    OwnableAdmin
{
    // TODO: code to allow for all for opensea
    // registry code
    // mint for team
    using BitMaps for BitMaps.BitMap;

    bytes32 private constant _BOARDUPDATED_EVENT_SIGNATURE =
        0xb62a6c4840cdab9c0eddf936be3b4bd2cc5122ee0ac6a6317e48724e62d5c6bd;

    BitMaps.BitMap private _metaDataBatchHead;

    mapping(uint256 => uint256) private batchSeed;

    mapping(uint256 => uint256) public merged;

    uint256 public dailyThrows = 0;

    constructor() ERC721Psi("Rock, Paper, Scissors, Check", "RPSCHECKS") {}

    function mint(address to, uint256 quantity) external payable nonReentrant {
        // TODO: need checks

        uint256 nextTokenId = _nextTokenId();
        uint256 random = uint(
            keccak256(
                abi.encodePacked(
                    block.timestamp,
                    msg.sender,
                    nextTokenId,
                    quantity
                )
            )
        );

        batchSeed[nextTokenId] = random;
        _metaDataBatchHead.set(nextTokenId);

        _safeMint(to, quantity);
    }

    function seed(uint256 tokenId) private view returns (uint256) {
        if (!_exists(tokenId)) revert TokenDoesNotExist();

        uint256 tokenIdMetaDataBatchHead = _metaDataBatchHead.scanForward(
            tokenId
        );

        unchecked {
            uint256 _batchSeed = batchSeed[tokenIdMetaDataBatchHead];
            return uint256(keccak256(abi.encode(_batchSeed, tokenId)));
        }
    }

    function tokenBoard(uint256 tokenId) external view returns (uint256) {
        uint256 rawBoard = fromMergedOrSeed(tokenId);
        uint256 board = 0;

        unchecked {
            for (uint i = 0; i < 80; i++) {
                uint256 mask = 3 << (2 * i);
                uint256 slotValue = (rawBoard & mask) >> (2 * i);
                uint256 value = (slotValue % 3) + 1;

                board |= value << (2 * i);
            }
        }

        return board;
    }

    function dailyThrow(uint256 dailySeed, uint8 day) public onlyOwnerAdmin {
        // TODO: update with assembly so cheaper possibly
        uint thrown = (uint(
            keccak256(abi.encodePacked(block.timestamp, dailySeed))
        ) % 3) + 1;

        dailyThrows |= thrown << (2 * (day - 1));

        // bytes memory baseRandom = abi.encodePacked(block.timestamp, dailySeed);

        // assembly {
        //     let rand := keccak256(baseRandom, 64)
        //     let thrown := add(mod(rand, 3), 1)
            
        //     let daily := sload(dailyThrows.slot)
        //     let updated := or(daily, shl(mul(2, sub(day, 1)),thrown))
        //     sstore(dailyThrows.slot, updated)
        // }
    }

    function getDailyThrow(uint256 day) public view returns (uint256) {
        uint256 mask = 3 << (2 * (day - 1));
        uint256 extracted = dailyThrows & mask;
        return extracted >> (2 * (day - 1));
    }

    function fromMergedOrSeed(uint256 tokenId) private view returns (uint256) {
        uint256 fromMerged = merged[tokenId];
        return fromMerged != 0 ? fromMerged : seed(tokenId);
    }

    function merge(uint256 keepId, uint256 mergeId) public {
        if (!_exists(keepId)) revert TokenDoesNotExist();
        if (!_exists(mergeId)) revert TokenDoesNotExist();
        if (ownerOf(keepId) != msg.sender) revert YouAreNotTheOwner();
        if (ownerOf(mergeId) != msg.sender) revert YouAreNotTheOwner();

        uint256 keepBoard = fromMergedOrSeed(keepId);
        uint256 mergeBoard = fromMergedOrSeed(mergeId);
        uint256 dailyBoard = dailyThrows;

        assembly {
            function houseThrow(b, day) -> result {
                let mask := shl(mul(2, day), 3)
                let slot := and(b, mask)
                result := shr(mul(2, day), slot)
            }

            function defThrow(b, day) -> result {
                let base := houseThrow(b, day)
                result := add(mod(base, 3), 1)
            }

            let wTable := 9000463377192598
            let board := 0

            for {
                let i := 0
            } lt(i, 80) {
                i := add(i, 1)
            } {
                let hThrow := houseThrow(dailyBoard, i)

                switch hThrow
                case 0 {
                    let slotSize := mul(2, i)
                    let remainingValue := shl(slotSize, shr(slotSize, keepBoard))
                    board := or(board, remainingValue)
                    break
                }
                default {
                    let kThrow := defThrow(keepBoard, i)
                    let mThrow := defThrow(mergeBoard, i)
                    // position = ((x - 1) * 18) + ((y - 1) * 6) + ((z - 1) * 2)
                    let position := add(
                        add(mul(sub(kThrow, 1), 18), mul(sub(mThrow, 1), 6)),
                        mul(sub(hThrow, 1), 2)
                    )
                    let raw := and(wTable, shl(position, 3))
                    let result := shr(position, raw)

                    board := or(board, shl(mul(2, i), sub(result, 1)))
                    continue
                }
            }

            mstore(0, keepId)
            mstore(32, merged.slot)
            let hash := keccak256(0, 64)
            sstore(hash, board)

            log3(
                0, // Start of data (0, since no data).
                0, // End of data (0, since no data).
                _BOARDUPDATED_EVENT_SIGNATURE, // Signature.
                board,
                keepId
            )
        }

        _burn(mergeId);
    }
  
    function tokenURI(
        uint256 tokenId
    ) public view override returns (string memory) {
        // _requireMinted(tokenId);

        uint256 _dailyThrows = dailyThrows;
        uint256 _defenderThrows = fromMergedOrSeed(tokenId);
        uint256 board = 0;
        unchecked {
            for (uint i = 0; i < 80; i++) {
                uint house = Utils.extractThrow(_dailyThrows, i);

                if (house == 0) {
                    board += (_defenderThrows >> (3 * i)) << (3 * i);
                    break;
                }

                uint defender = Utils.extractThrow(_defenderThrows, i);
                uint winner = Utils.extractWinner(defender, house);

                board += winner << (3 * i);
            }
        }

        return RPSMetadata.tokenURI(tokenId, board);
    }

    function totalBurned() public view returns (uint256) {
        return _burned();
    }
}

