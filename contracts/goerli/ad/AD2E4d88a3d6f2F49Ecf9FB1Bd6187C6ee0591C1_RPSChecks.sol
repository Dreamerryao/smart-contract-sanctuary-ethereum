library Utils {
    function extractThrow(
        uint256 board,
        uint256 day
    ) internal pure returns (uint256 result) {
        // uint mask = 7 << (3 * (day - 1));
        // uint slot = dna & mask;
        // return slot >> (3 * (day - 1));
        // TODO: this doesn't work because if merged it will be greater than 3


        // uint256 seedBit = (board & (1 << 255)) >> 255;


        assembly {
            let mask := shl(mul(2, day), 3)
            let slot := and(board, mask)
            let value := shr(mul(2, day), slot)
            let p := mload(0x40)
            // if eq(seedBit, 1) {
                mstore(p, add(mod(value, 3), 1))
            // }
            // if eq(seedBit, 0) {
            //     mstore(p, value)
            // }
            result := mload(p)
        }
    }

    function extractHouseThrow(
        uint256 board,
        uint256 day
    ) internal pure returns (uint256 result) {
        // uint mask = 7 << (3 * (day - 1));
        // uint slot = dna & mask;
        // return slot >> (3 * (day - 1));
        // TODO: this doesn't work because if merged it will be greater than 3


        // uint256 seedBit = (board & (1 << 255)) >> 255;


        assembly {
            let mask := shl(mul(2, day), 3)
            let slot := and(board, mask)
            let value := shr(mul(2, day), slot)
            let p := mload(0x40)
            // if eq(seedBit, 1) {
                mstore(p, value)
            // }
            // if eq(seedBit, 0) {
            //     mstore(p, value)
            // }
            result := mload(p)
        }
    }

    function extractWinner(
        uint defenderThrow,
        uint houseThrow
    ) internal pure returns (uint256 convertedResult) {
        // 0 = tie, so convert to 5
        // 1 = loss, so convert to 4
        // 2 = win, so convert to 6
        // uint result = (3 - defenderThrow + houseThrow) % 3;
        // return result == 2 ? 6 : result == 1 ? 4 : 5;

        assembly {
            let p := mload(0x40)

            switch defenderThrow
            case 4 {
                mstore(p, 4)
                convertedResult := mload(p)
            }
            case 5 {
                mstore(p, 5)
                convertedResult := mload(p)
            }
            case 6 {
                mstore(p, 6)
                convertedResult := mload(p)
            }
            default {
                let result := mod(add(sub(3, defenderThrow), houseThrow), 3)
                switch result
                case 2 {
                    mstore(p, 6)
                    convertedResult := mload(p)
                }
                case 1 {
                    mstore(p, 4)
                    convertedResult := mload(p)
                }
                case 0 {
                    mstore(p, 5)
                    convertedResult := mload(p)
                }
            }
        }
    }
}

