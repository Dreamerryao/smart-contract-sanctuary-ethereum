    function generateBoard(uint256 board) private pure returns (bytes memory) {
        bytes memory userThrows;

        for (uint256 i = 1; i < 81; i++) {
            uint userThrow = extractThrow(board, i - 1);
            string memory dailyResult;
            string memory color;

            (dailyResult, color) = extractDailyResult(userThrow);

            uint rowNumber = Math.ceilDiv(i, 8);
            uint columnNumber = i - (rowNumber - 1) * 8;

            string memory x = Strings.toString((196 + (columnNumber - 1) * 36));
            string memory y = Strings.toString((160 + (rowNumber - 1) * 36));

             userThrows = abi.encodePacked(userThrows, abi.encodePacked(
                '<g transform="translate(', x, ', ', y, ') scale(1)">',
                    '<use href="', dailyResult,'" fill="', color, '"></use>',
                '</g>'
            ));
        }

        return userThrows;
    }

    function extractDailyResult(uint256 userThrow) private pure returns (string memory dailyResult, string memory color) {
        if (userThrow == 1) {
            dailyResult = '#rock';
            color = '#fff';
        } else if (userThrow == 2) {
            dailyResult = '#paper';
            color = '#fff';
        } else if (userThrow == 3) {
            dailyResult = '#scissors';
            color = '#fff';
        } else if (userThrow == 4) {
            dailyResult = '#check';
            color = '#FF3131';
        } else if (userThrow == 5) {
            dailyResult = '#check';
            color = '#FFF01F';
        } else if (userThrow == 6) {
            dailyResult = '#check';
            color = '#39ff14';
        } else {
            revert();
        }
    }

    function extractThrow(uint256 board, uint256 slot) private pure returns (uint256) {
        uint mask = 7 << (3 * slot);
        uint positionedThrow = board & mask;
        return positionedThrow >> (3 * slot);
    }

