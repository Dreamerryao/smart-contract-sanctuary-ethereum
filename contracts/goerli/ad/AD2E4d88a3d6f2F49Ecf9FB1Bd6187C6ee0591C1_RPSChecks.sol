pragma solidity ^0.8.17;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "./Utils.sol";

import "./RPSArt.sol";

library RPSMetadata {
    function tokenURI(
        uint256 tokenId,
        uint256 board
    ) internal pure returns (string memory) {
        bytes memory svg = RPSArt.generateSVG(board);

        bytes memory metadata = abi.encodePacked(
            "{",
            '"name": "Board #',
            Strings.toString(tokenId),
            '",',
            '"description": "This artwork may or may not be notable.",',
            '"image": ',
            '"data:image/svg+xml;base64,',
            Base64.encode(svg),
            '",',
            '"attributes": [',
            attributes(board),
            "]",
            "}"
        );

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(metadata)
                )
            );
    }

    function attributes(uint256 board) internal pure returns (bytes memory) {
        bytes memory attrs;

        for (uint i = 1; i < 81; i++) {
            uint userThrow = Utils.extractThrow(board, i);
            string memory trailing = i == 80 ? "" : ",";
            attrs = abi.encodePacked(
                attrs,
                abi.encodePacked(
                    trait(Strings.toString(i), throwName(userThrow)),
                    trailing
                )
            );
        }

        return attrs;
    }

    function throwName(
        uint256 userThrow
    ) internal pure returns (string memory) {
        if (userThrow == 1) {
            return "Rock";
        } else if (userThrow == 2) {
            return "Paper";
        } else if (userThrow == 3) {
            return "Scissors";
        } else if (userThrow == 4) {
            return "Loss";
        } else if (userThrow == 5) {
            return "Tie";
        } else {
            return "Win";
        }
    }

    function trait(
        string memory traitNumber,
        string memory traitValue
    ) internal pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "{",
                    '"trait_type": "Day ',
                    traitNumber,
                    '",'
                    '"value": "',
                    traitValue,
                    '"'
                    "}"
                )
            );
    }
}

