/// @title Matching Criteria
/// @author 0xEND
/// @notice Common validation methods for base orders.
abstract contract MatchingCriteria {
    /// ~~~ Errors ~~~
    /// @notice Raised when an invalid signature is provided for an off chain order
    error InvalidSignatureError(
        bytes32 hash,
        address orderCreator,
        uint8 v,
        bytes32 r,
        bytes32 s,
        bytes32 domainSeparator
    );
    /// @notice Raised when the maker and taker orders don't have matching collections
    /// @param makerCollection Address defined for the collection in the maker order
    /// @param takerCollection Address defined for the collection in the taker order
    error NonMatchingCollectionError(
        address makerCollection,
        address takerCollection
    );
    /// @notice Raised when an order is trying to be executed outside of the valid window
    /// @param startTime Start time of the maker order
    /// @param endTime Endtime of the maker order
    /// @param currentTime Current timestamp
    error OutOfTimeWindowError(
        uint256 startTime,
        uint256 endTime,
        uint256 currentTime
    );

    /// @notice Common checks on base orders to be a valid match. Throws if invalid.
    /// @param baseMakerOrder Maker Order to be checked
    function _validateBaseOrder(
        OffChainBaseOrders.BaseMakerOrder calldata baseMakerOrder
    ) internal view {
        /// Checks the current block timestamp is within the valid time window
        if (
            (baseMakerOrder.startTime > block.timestamp) ||
            baseMakerOrder.endTime < block.timestamp
        ) {
            revert OutOfTimeWindowError(
                baseMakerOrder.startTime,
                baseMakerOrder.endTime,
                block.timestamp
            );
        }
    }

    /// @notice Validates signature for a given off chain order. Throws if invalid.
    /// @param hash Hash of the offchain order
    /// @param orderCreator Actor who submitted the order
    /// @param v 27/28 depending on parity of r
    /// @param r standard ECDSA
    /// @param s standard ECDSA
    function _validateSignatures(
        bytes32 hash,
        address orderCreator,
        uint8 v,
        bytes32 r,
        bytes32 s,
        bytes32 domainSeparator
    ) internal view {
        bool isValid;
        bytes32 digest = keccak256(
            abi.encodePacked("\x19\x01", domainSeparator, hash)
        );
        /// If the actor that submitted the order is a contract, use IERC1271 to valdate it
        /// Else use EIP 712
        if (Address.isContract(orderCreator)) {
            isValid =
                IERC1271(orderCreator).isValidSignature(
                    digest,
                    abi.encodePacked(r, s, v)
                ) ==
                0x1626ba7e;
        } else {
            isValid = ECDSA.recover(digest, v, r, s) == orderCreator;
        }
        if (!isValid) {
            revert InvalidSignatureError(
                hash,
                orderCreator,
                v,
                r,
                s,
                domainSeparator
            );
        }
    }
}

