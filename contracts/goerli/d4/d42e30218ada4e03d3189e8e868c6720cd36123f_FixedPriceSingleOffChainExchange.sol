    // keccak256("OneOfManyMakerOrder(BaseMakerOrder baseOrder,uint256 price,uint256[] tokenIds)BaseMakerOrder(address orderCreator,address collection,address currency,bool isOrderAsk,uint256 creatorOrderId,uint256 startTime,uint256 endTime,uint256 sourcerId)")
