/// @title FixedPriceSingleOffChainExchange
/// @author 0xEND
/// @notice Mechanism to match orders that correspond to a fixed price on a single item. Maker order listed off chain
contract FixedPriceSingleOffChainExchange is
    BaseOffChainExchange,
    IFixedPriceSingleOffChainExchange,
    PayoutHelper,
    ReentrancyGuard
{
    using FixedPriceOffChainOrders for FixedPriceOffChainOrders.SingleMakerOrder;
    using FixedPriceOffChainOrders for FixedPriceOffChainOrders.CollectionMakerOrder;
    using FixedPriceOffChainOrders for FixedPriceOffChainOrders.PrivateSingleMakerOrder;
    using FixedPriceOffChainOrders for FixedPriceOffChainOrders.OneOfManyMakerOrder;

    /// ~~ Variables ~~
    /// @notice Used in compliance with EIP712
    bytes32 public immutable DOMAIN_SEPARATOR;

    /// ~~ Errors ~~
    /// @notice Raised on private orders when called by wrong taker
    /// @param taker Taker that invalidly tried to fill the order
    error TakerNotAllowedError(address taker);

    /// @notice Raised when the tokenId provided by the taker is not one of the many in the maker
    /// @param tokenId Token Id provided by the taker
    error InvalidOneOfManyError(uint256 tokenId);

    /// ~~ Events ~~

    /// @notice Emitted when an order is filled
    /// @param isOrderAsk True if the order is an ask (NFT being offered)
    /// @param withFees No fees for private orders
    /// @param orderCreator Address of the actor creating the maker order
    /// @param collection Address of the ERC721
    /// @param tokenId Id of the ERC721 being offered
    /// @param currency Currency in which the price is listed
    /// @param creatorOrderId Unique Id for orders created by orderCreator
    /// @param makerSourcerId Maker Sourcer Id
    /// @param takerSourcerId Taker Sourcer Id
    /// @param price Price set for the order
    event OrderFilled(
        bool isOrderAsk,
        bool withFees,
        address orderCreator,
        address collection,
        uint256 tokenId,
        address currency,
        uint256 creatorOrderId,
        uint256 makerSourcerId,
        uint256 takerSourcerId,
        uint256 price
    );

    /// @notice Emitted when an actor cancels all its orders
    /// @param collection Address of the ERC721
    /// @param tokenId Id of the ERC721 being offered
    /// @param owner Original NFT owner
    /// @param recipient New NFT owner
    event ERC721Transfered(
        address collection,
        uint256 tokenId,
        address owner,
        address recipient
    );

    /// @notice Constructor
    /// @param royaltyManagerAddress Address of the Royalty Manager contract (i.e. RoyaltyRegistry)
    /// @param feeManagerAddress Address of the FeeManager contract
    /// @param stakedAddress Address Staking Contract
    /// @param wethAddress Address of WETH
    constructor(
        address royaltyManagerAddress,
        address feeManagerAddress,
        address stakedAddress,
        address wethAddress
    )
        PayoutHelper(
            royaltyManagerAddress,
            feeManagerAddress,
            stakedAddress,
            wethAddress
        )
    {
        DOMAIN_SEPARATOR = keccak256(
            abi.encode(
                0x8b73c3c69bb8fe3d512ecc4cf759cc79239f7b179b0ffacaa9a75d522b39400f, // keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"),
                0x4e285a523fa0f1baf403e0f34940e4a0fd434343b9721f27f42d125db734f2f9, // keccak256(bytes("PIXEL::FixedPriceSingleExchangeOffChain")),
                0xc89efdaa54c0f20c7adf612882df0950f5a951637e0307cdcb4c672f298b8bc6, // keccak256(bytes("1")),
                block.chainid,
                address(this)
            )
        );
    }

    function fillAsk(
        FixedPriceOffChainOrders.SingleMakerOrder calldata makerOrder,
        uint256 takerSourcerId,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        _fillAsk(
            makerOrder.baseOrder,
            makerOrder.tokenId,
            makerOrder.price,
            makerOrder.hash(),
            takerSourcerId,
            true,
            vrs
        );
    }

    function fillBid(
        FixedPriceOffChainOrders.SingleMakerOrder calldata makerOrder,
        uint256 takerSourcerId,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        _fillBid(
            makerOrder.baseOrder,
            makerOrder.tokenId,
            makerOrder.price,
            makerOrder.hash(),
            takerSourcerId,
            true,
            vrs
        );
    }

    function fillMultipleOrders(
        FixedPriceOffChainOrders.SingleMakerOrder[] calldata makerOrders,
        uint256 takerSourcerId,
        OffChainBaseOrders.VRS[] calldata vrs
    ) external payable override {
        uint256 total = makerOrders.length;
        for (uint256 i = 0; i < total; ) {
            if (makerOrders[i].baseOrder.isOrderAsk) {
                _fillAsk(
                    makerOrders[i].baseOrder,
                    makerOrders[i].tokenId,
                    makerOrders[i].price,
                    makerOrders[i].hash(),
                    takerSourcerId,
                    true,
                    vrs[i]
                );
            } else {
                _fillBid(
                    makerOrders[i].baseOrder,
                    makerOrders[i].tokenId,
                    makerOrders[i].price,
                    makerOrders[i].hash(),
                    takerSourcerId,
                    true,
                    vrs[i]
                );
            }
            unchecked {
                ++i;
            }
        }
    }

    function fillPrivateAsk(
        FixedPriceOffChainOrders.PrivateSingleMakerOrder calldata makerOrder,
        uint256 takerSourcerId,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        if (msg.sender != makerOrder.taker) {
            revert TakerNotAllowedError(msg.sender);
        }
        _fillAsk(
            makerOrder.order.baseOrder,
            makerOrder.order.tokenId,
            makerOrder.order.price,
            makerOrder.hash(),
            takerSourcerId,
            false,
            vrs
        );
    }

    function fillOneOfManyAsk(
        FixedPriceOffChainOrders.OneOfManyMakerOrder calldata makerOrder,
        FixedPriceOffChainOrders.OneOfManyTakerOrder calldata takerOrder,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        bool found = false;
        uint256 total = makerOrder.tokenIds.length;
        for (uint256 i = 0; i < total && !found; ) {
            found = makerOrder.tokenIds[i] == takerOrder.tokenId;
            unchecked {
                ++i;
            }
        }
        if (!found) {
            revert InvalidOneOfManyError(takerOrder.tokenId);
        }
        _fillAsk(
            makerOrder.baseOrder,
            takerOrder.tokenId,
            makerOrder.price,
            makerOrder.hash(),
            takerOrder.takerSourcerId,
            true,
            vrs
        );
    }

    function fillOneOfManyBid(
        FixedPriceOffChainOrders.OneOfManyMakerOrder calldata makerOrder,
        FixedPriceOffChainOrders.OneOfManyTakerOrder calldata takerOrder,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        bool found = false;
        uint256 total = makerOrder.tokenIds.length;
        for (uint256 i = 0; i < total && !found; ) {
            found = makerOrder.tokenIds[i] == takerOrder.tokenId;
            unchecked {
                ++i;
            }
        }
        if (!found) {
            revert InvalidOneOfManyError(takerOrder.tokenId);
        }
        _fillBid(
            makerOrder.baseOrder,
            takerOrder.tokenId,
            makerOrder.price,
            makerOrder.hash(),
            takerOrder.takerSourcerId,
            true,
            vrs
        );
    }

    function fillCollectionBid(
        FixedPriceOffChainOrders.CollectionMakerOrder calldata makerOrder,
        FixedPriceOffChainOrders.CollectionTakerOrder calldata takerOrder,
        OffChainBaseOrders.VRS calldata vrs
    ) external payable override {
        _fillBid(
            makerOrder.baseOrder,
            takerOrder.tokenId,
            makerOrder.price,
            makerOrder.hash(),
            takerOrder.takerSourcerId,
            true,
            vrs
        );
    }

    function _fillAsk(
        OffChainBaseOrders.BaseMakerOrder calldata baseMakerOrder,
        uint256 tokenId,
        uint256 price,
        bytes32 orderHash,
        uint256 takerSourcerId,
        bool withFees,
        OffChainBaseOrders.VRS calldata vrs
    ) private nonReentrant {
        _validateOrder(
            baseMakerOrder,
            orderHash,
            DOMAIN_SEPARATOR,
            vrs.v,
            vrs.r,
            vrs.s
        );
        isOrderCancelledOrFilled[baseMakerOrder.orderCreator][
            baseMakerOrder.creatorOrderId
        ] = true;
        if (withFees) {
            _feeManager.countMakerOrder(baseMakerOrder.sourcerId, price);
            _feeManager.countTakerOrder(takerSourcerId, price);
        }
        _handleFungiblePayment(
            PaymentData(
                baseMakerOrder.sourcerId,
                takerSourcerId,
                baseMakerOrder.collection,
                tokenId,
                baseMakerOrder.currency,
                price,
                msg.sender,
                baseMakerOrder.orderCreator,
                withFees
            )
        );
        emit OrderFilled(
            true,
            withFees,
            baseMakerOrder.orderCreator,
            baseMakerOrder.collection,
            tokenId,
            baseMakerOrder.currency,
            baseMakerOrder.creatorOrderId,
            baseMakerOrder.sourcerId,
            takerSourcerId,
            price
        );
        _handleERC721Transfer(
            baseMakerOrder.collection,
            tokenId,
            baseMakerOrder.orderCreator,
            msg.sender
        );
        emit ERC721Transfered(
            baseMakerOrder.collection,
            tokenId,
            baseMakerOrder.orderCreator,
            msg.sender
        );
    }

    function _fillBid(
        OffChainBaseOrders.BaseMakerOrder calldata baseMakerOrder,
        uint256 tokenId,
        uint256 price,
        bytes32 orderHash,
        uint256 takerSourcerId,
        bool withFees,
        OffChainBaseOrders.VRS calldata vrs
    ) private nonReentrant {
        _validateOrder(
            baseMakerOrder,
            orderHash,
            DOMAIN_SEPARATOR,
            vrs.v,
            vrs.r,
            vrs.s
        );
        isOrderCancelledOrFilled[baseMakerOrder.orderCreator][
            baseMakerOrder.creatorOrderId
        ] = true;
        if (withFees) {
            _feeManager.countMakerOrder(baseMakerOrder.sourcerId, price);
            _feeManager.countTakerOrder(takerSourcerId, price);
        }
        _handleFungiblePayment(
            PaymentData(
                baseMakerOrder.sourcerId,
                takerSourcerId,
                baseMakerOrder.collection,
                tokenId,
                baseMakerOrder.currency,
                price,
                baseMakerOrder.orderCreator,
                msg.sender,
                withFees
            )
        );
        emit OrderFilled(
            false,
            withFees,
            baseMakerOrder.orderCreator,
            baseMakerOrder.collection,
            tokenId,
            baseMakerOrder.currency,
            baseMakerOrder.creatorOrderId,
            baseMakerOrder.sourcerId,
            takerSourcerId,
            price
        );
        _handleERC721Transfer(
            baseMakerOrder.collection,
            tokenId,
            msg.sender,
            baseMakerOrder.orderCreator
        );
        emit ERC721Transfered(
            baseMakerOrder.collection,
            tokenId,
            msg.sender,
            baseMakerOrder.orderCreator
        );

        _feeManager.countMakerOrder(baseMakerOrder.sourcerId, price);
        _feeManager.countTakerOrder(takerSourcerId, price);
    }
}

