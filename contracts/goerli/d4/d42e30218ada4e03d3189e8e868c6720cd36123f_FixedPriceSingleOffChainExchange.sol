pragma solidity ^0.8.13;

import "../../../interfaces/exchanges/mechanisms/IMechanism.sol";
import "../mixins/MatchingCriteria.sol";
import "./OffChainBaseOrders.sol";

abstract contract BaseOffChainExchange is IMechanism, MatchingCriteria {
    /// ~~ Events ~~

    /// @notice Emitted when a single order is cancelled.
    /// @param sender The address of the canceling actor
    /// @param orderId the Id of the cancelled order
    event CancelledFixedPriceSingleOffChainOrder(
        address sender,
        uint256 orderId
    );

    /// @notice Emitted when an actor cancels multiple orders
    /// @param sender The address of the canceling actor
    /// @param orderIds Array of orderIds to be cancelled
    event CancelledFixedPriceOffChainOrders(address sender, uint256[] orderIds);

    /// @notice Emitted when an actor cancels all its orders
    /// @param sender The address of the canceling actor
    /// @param minOrderId All orders below this ID are considered inactive
    event CancelledAllOrders(address sender, uint256 minOrderId);

    /// ~~ Errors ~~

    /// @notice Raised when trying to fill a filled or cancelled order.
    error NotActiveOrderError(address makerAddress, uint256 orderId);

    /// @notice Raised when cancel orders is called with an empty array
    error EmptyOrdersArgumentError();

    /// @notice Raised when trying to set a min order id lower than the current one
    error CancellingMinOrderIdLowerThanPreviousError();

    /// ~~ Variables ~~

    /// @notice Used for validating off chain maker orders / canceling all
    mapping(address => uint256) public userMinOrderId;

    /// @notice Used for validate off chain maker orders / canceling one
    mapping(address => mapping(uint256 => bool))
        public isOrderCancelledOrFilled;

    function cancelOrder(uint256 orderId) external override {
        isOrderCancelledOrFilled[msg.sender][orderId] = true;
        emit CancelledFixedPriceSingleOffChainOrder(msg.sender, orderId);
    }

    function cancelOrders(uint256[] calldata orderIds) external override {
        if (orderIds.length == 0) {
            revert EmptyOrdersArgumentError();
        }
        for (uint256 i = 0; i < orderIds.length; ) {
            isOrderCancelledOrFilled[msg.sender][orderIds[i]] = true;
            unchecked {
                ++i;
            }
        }
        emit CancelledFixedPriceOffChainOrders(msg.sender, orderIds);
    }

    function cancelAllOrders(uint256 minOrderId) external override {
        if (minOrderId < userMinOrderId[msg.sender]) {
            revert CancellingMinOrderIdLowerThanPreviousError();
        }
        userMinOrderId[msg.sender] = minOrderId;
        emit CancelledAllOrders(msg.sender, minOrderId);
    }

    /// @notice Helper to validate valid orders
    /// @param orderHash Full order hash
    /// @param domainSeparator Exchange domain separator
    /// @param v 27/28 depending on parity of r
    /// @param r standard ECDSA
    /// @param s standard ECDSA
    function _validateOrder(
        OffChainBaseOrders.BaseMakerOrder calldata baseMakerOrder,
        bytes32 orderHash,
        bytes32 domainSeparator,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal view {
        // Common checks for base orders, across all mechanisms.
        _validateBaseOrder(baseMakerOrder);

        _validateSignatures(
            orderHash,
            baseMakerOrder.orderCreator,
            v,
            r,
            s,
            domainSeparator
        );

        // For an order to be active it might be higher than the minOrderId for a given creator
        // and it must not be flagged as cancelled or filled.
        if (
            (userMinOrderId[baseMakerOrder.orderCreator] >
                baseMakerOrder.creatorOrderId) ||
            (
                isOrderCancelledOrFilled[baseMakerOrder.orderCreator][
                    baseMakerOrder.creatorOrderId
                ]
            )
        ) {
            revert NotActiveOrderError(
                baseMakerOrder.orderCreator,
                baseMakerOrder.creatorOrderId
            );
        }
    }
}

