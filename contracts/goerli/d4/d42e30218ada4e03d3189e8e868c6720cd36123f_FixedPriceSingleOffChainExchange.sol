pragma solidity ^0.8.13;

import "@forge-std/console.sol";
import "@solmate/tokens/ERC20.sol";
import "@solmate/tokens/ERC721.sol";
import "@solmate/utils/SafeTransferLib.sol";
import "@royalty-registry/IRoyaltyEngineV1.sol";
import "../../../interfaces/IWETH.sol";
import "../../../interfaces/token/IStaked.sol";
import "../../fees/FeeManager.sol";
import "./CurrencyManager.sol";

/// @title Payout Helper
/// @author 0xEND
/// @notice Common methods to validate and transfer ETH/ERC20s/ERC721s. Inherited by all mechanisms.
abstract contract PayoutHelper is CurrencyManager {
    using SafeTransferLib for IWETH;
    using SafeTransferLib for ERC20;
    using SafeTransferLib for address;

    /// ~~~ Variables ~~~
    /// @notice Fee Manager - calculates fees' split for sourcers/protocol
    FeeManager internal _feeManager;
    /// @notice Registry for royalty (see: https://royaltyregistry.xyz/)
    IRoyaltyEngineV1 private _royaltyManager;
    /// @notice Wrapped ETH contract
    IWETH private _WETH;
    /// @notice Staked contract
    IStaked _staked;

    /// ~~~ Structs ~~~
    /// @notice Helper struct to move necessary payment data around
    /// @param makerSourcerId Sourcer ID for the storefront that sourced liquidity
    /// @param takerSourcerId Sourcer ID for the storefront that brought the active order
    /// @param collection Address of the ERC721 contract
    /// @param tokenId NFT id
    /// @param currency Address of the fungible token (ETH/ERC20). address(0) iff ETH
    /// @param amount Quantity of fungible tokens (i.e. price)
    /// @param buyerAddress Address of the new NFT owner
    /// @param sellerAddress Address of the original NFT owner
    /// @param withFees Whether this txn pays fees
    struct PaymentData {
        uint256 makerSourcerId;
        uint256 takerSourcerId;
        address collection;
        uint256 tokenId;
        address currency;
        uint256 amount;
        address buyerAddress;
        address sellerAddress;
        bool withFees;
    }

    /// ~~~ Events ~~~
    /// @notice Emmited when there's a fee payment
    /// @param collection Address of the ERC721 contract
    /// @param tokenId  NFT id
    /// @param currency Address of the fungible token (ETH/ERC20) in which the fee is paid. address(0) iff ETH
    /// @param fees Fees involved in the txn
    event FeePayment(
        address collection,
        uint256 tokenId,
        address currency,
        IFeeManager.Fees fees
    );
    /// @notice Emmited when there's a royalty payment
    /// @param royaltyRecipient Address of the royalty recipient
    /// @param collection Address of the ERC721 contract
    /// @param tokenId  NFT id
    /// @param currency Address of the fungible token (ETH/ERC20) in which the fee is paid. address(0) iff ETH
    /// @param royalty Amount of tokens being transfered
    event RoyaltyPayment(
        address royaltyRecipient,
        address collection,
        uint256 tokenId,
        address currency,
        uint256 royalty
    );

    /// ~~~ Custom Errors ~~~

    /// @notice Raised when there's not enough tokens for fees
    error InsufficientAmountForFeesError();
    /// @notice Raised when there's not enough tokens for royalties
    error InsufficientAmountForRoyaltiesError();
    /// @notice Raised when the msg contains more ETH than necessary
    /// @param sent Amount of ETH sent (msg.value)
    /// @param expected Amount expected given the order
    error TooMuchEthSentError(uint256 sent, uint256 expected);
    /// @notice Raised when the msg contains less ETH than necessary
    /// @param sent Amount of ETH sent (msg.value)
    /// @param expected Amount expected given the order
    error NotEnoughEthSentError(uint256 sent, uint256 expected);

    /// @notice Constructor
    /// @param royaltyManagerAddress Address of the Royalty Manager contract (i.e. RoyaltyRegistry)
    /// @param feeManagerAddress Address of the FeeManager contract
    /// @param stakedAddress Address of the staking contract
    /// @param wethAddress Address of WETH
    constructor(
        address royaltyManagerAddress,
        address feeManagerAddress,
        address stakedAddress,
        address wethAddress
    ) {
        _royaltyManager = IRoyaltyEngineV1(royaltyManagerAddress);
        _feeManager = FeeManager(feeManagerAddress);
        _staked = IStaked(stakedAddress);
        _WETH = IWETH(wethAddress);
    }

    /// @notice ERC 721 transfers
    /// @param collection Address of the ERC721 contract
    /// @param tokenId  NFT id
    /// @param sellerAddress Address of the original NFT owner
    /// @param buyerAddress Address of the new NFT owner
    function _handleERC721Transfer(
        address collection,
        uint256 tokenId,
        address sellerAddress,
        address buyerAddress
    ) internal {
        ERC721(collection).safeTransferFrom(
            sellerAddress,
            buyerAddress,
            tokenId
        );
    }

    /// @notice ERC 20 transfers
    /// @param paymentData Necessary data to transfer fees/royalties/seller<>buyer payment
    function _handleFungiblePayment(PaymentData memory paymentData) internal {
        uint256 sellerAmount = paymentData.amount;

        bool isCurrencyEth = _isEthAddress(paymentData.currency);

        if (isCurrencyEth) {
            if (paymentData.amount < msg.value) {
                revert TooMuchEthSentError(msg.value, paymentData.amount);
            } else if (paymentData.amount > msg.value) {
                revert NotEnoughEthSentError(msg.value, paymentData.amount);
            }
        } else if (!approvedCurrencies[paymentData.currency]) {
            revert CurrencyNotApprovedError(paymentData.currency);
        }

        // Send protocol + sourcers fees
        if (paymentData.withFees) {
            sellerAmount = _handleFeePayments(paymentData, sellerAmount);
        }

        // Send royalties
        sellerAmount = _handleRoyalties(paymentData, sellerAmount);

        // Seller <> Buyer
        _handlePayment(paymentData, paymentData.sellerAddress, sellerAmount);
    }

    /// @notice Handle Royalties
    /// @param paymentData Necessary data to transfer royalties
    /// @param currentAmount Amount still available, use to check if there are enough tokens
    /// @return Amount still available
    function _handleRoyalties(
        PaymentData memory paymentData,
        uint256 currentAmount
    ) private returns (uint256) {
        (
            address payable[] memory royaltyRecipients,
            uint256[] memory royaltyAmounts
        ) = _royaltyManager.getRoyalty(
                paymentData.collection,
                paymentData.tokenId,
                paymentData.amount
            );

        uint256 totalRoyaltyRecipients = royaltyRecipients.length;
        uint256 royaltyAmount;
        address royaltyRecipient;
        for (uint256 i; i < totalRoyaltyRecipients; ) {
            royaltyRecipient = royaltyRecipients[i];
            royaltyAmount = royaltyAmounts[i];
            if (currentAmount < royaltyAmount) {
                revert InsufficientAmountForRoyaltiesError();
            }
            _handlePayment(paymentData, royaltyRecipient, royaltyAmount);
            emit RoyaltyPayment(
                royaltyRecipient,
                paymentData.collection,
                paymentData.tokenId,
                paymentData.currency,
                royaltyAmount
            );
            unchecked {
                currentAmount -= royaltyAmount;
                ++i;
            }
        }
        return currentAmount;
    }

    /// @notice Handle fees
    /// @param paymentData Necessary data to transfer fees
    /// @param currentAmount Amount still available, use to check if there are enough tokens
    /// @return Amount still available
    function _handleFeePayments(
        PaymentData memory paymentData,
        uint256 currentAmount
    ) private returns (uint256) {
        uint256 makerSourcerId = paymentData.makerSourcerId;
        uint256 takerSourcerId = paymentData.takerSourcerId;
        FeeManager.Fees memory fees = _feeManager.getFees(
            makerSourcerId,
            takerSourcerId,
            paymentData.amount
        );
        address currency = paymentData.currency;
        uint256 totalFees = _staked.addFeesReceived(currency, fees);

        _handlePayment(paymentData, address(_staked), totalFees);

        emit FeePayment(
            paymentData.collection,
            paymentData.tokenId,
            currency,
            fees
        );

        return currentAmount - totalFees;
    }

    /// @notice Helper to route either to the ERC20/WETH contract
    /// @param paymentData Necessary data to execute the transfer
    /// @param recipient Can be the recipient of fees/royalties/seller
    /// @param amount Amount to be transferred
    function _handlePayment(
        PaymentData memory paymentData,
        address recipient,
        uint256 amount
    ) private {
        if (_isEthAddress(paymentData.currency)) {
            bool success;

            assembly {
                success := call(gas(), recipient, amount, 0, 0, 0, 0)
            }
            if (!success) {
                _WETH.deposit{value: amount}();
                _WETH.safeTransfer(recipient, amount);
            }
        } else {
            ERC20(paymentData.currency).safeTransferFrom(
                paymentData.buyerAddress,
                recipient,
                amount
            );
        }
    }

    function _isEthAddress(address currency) private pure returns (bool) {
        return currency == address(0);
    }
}

