pragma solidity ^0.8.0;




contract LPTokenSwap {
    address public constant ROUTER_ADDRESS =
        0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    address public constant LP_TOKEN_ADDRESS =
        0xDCee90E63B7EF390fb97b773b72c21b6C88C160C;
    address public constant TOKEN1_ADDRESS =
        0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
    address public constant TOKEN2_ADDRESS =
        0x11538DBf7c46D2DD02767e3A695ac9B9519C507C;

    IUniswapV2Router02 public router;
    IUniswapV2Pair public pair;
    IERC20 public token1;
    IERC20 public token2;

    constructor() {
        router = IUniswapV2Router02(ROUTER_ADDRESS);
        pair = IUniswapV2Pair(LP_TOKEN_ADDRESS);
        token1 = IERC20(TOKEN1_ADDRESS);
        token2 = IERC20(TOKEN2_ADDRESS);
    }

    function swapLPForTokens(
        uint256 amountLP,
        uint256 minAmount1,
        uint256 minAmount2,
        address recipient1,
        address recipient2
    ) external {
        // Get the current reserves of the two tokens in the LP pair
        (uint256 reserve1, uint256 reserve2, ) = pair.getReserves();

        // Calculate the amount of each token that will be received
        uint256 amount1 = ((amountLP * reserve1) / pair.totalSupply()) * 10**18;
        uint256 amount2 = ((amountLP * reserve2) / pair.totalSupply()) * 10**9;

        // Swap the LP token for the two tokens
        pair.approve(address(router), amountLP);
        address[] memory path = new address[](2);
        path[0] = LP_TOKEN_ADDRESS;
        path[1] = TOKEN1_ADDRESS;
        router.swapExactTokensForTokens(
            amountLP,
            minAmount1,
            path,
            recipient1,
            block.timestamp
        );
        path[1] = TOKEN2_ADDRESS;
        router.swapExactTokensForTokens(
            amountLP,
            minAmount2,
            path,
            recipient2,
            block.timestamp
        );

        // Send the appropriate amounts of each token to the respective recipients
        token1.transfer(recipient1, amount1);
        token2.transfer(recipient2, amount2);
    }
}