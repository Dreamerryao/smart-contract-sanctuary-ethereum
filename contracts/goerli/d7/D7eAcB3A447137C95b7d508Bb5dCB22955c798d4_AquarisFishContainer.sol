pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "hardhat/console.sol";

contract AquarisFishContainer is Ownable, ERC1155Supply, ERC1155URIStorage {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address private _signerAccount;

    mapping(bytes32 => bool) private _hashMap;

    uint256 public constant FISH = 0;
    uint256 public constant PROMO_FISH = 1;

    uint256 public minPercent = 1;
    uint256 public maxPercent = 3;
    uint256 public tokenStep = 10000;

    string public constant name = "AquarisFishContainer";
    string public constant symbol = "AQSFish";

    bool public stakingIsOver;

    uint256 public constant limitMintNFTTotal = 2000;

    IERC20 public aqsErc20;
    IERC20 public stableErc20;

    constructor() ERC1155("") {
        
    }

    function setURI(uint256 tokenId, string memory tokenURI) public onlyOwner {
        _setURI(tokenId, tokenURI);
    }

    function setToken(address erc20Address) public onlyOwner {
        aqsErc20 = IERC20(erc20Address);
    }

    function setStableToken(address erc20Address) public onlyOwner {
        stableErc20 = IERC20(erc20Address);
    }

    function uri(uint256 tokenId)
        public
        view
        override(ERC1155, ERC1155URIStorage)
        returns (string memory)
    {
        return ERC1155URIStorage.uri(tokenId);
    }
    
    function totalSupply() external view returns (uint256 supply) {
        uint256 _totalSupply;
        for (uint256 id = 0; id <= 1; ) {
            _totalSupply += totalSupply(id);
            unchecked {
                ++id;
            }
            return _totalSupply;
        }
    }

    function mint(
        address account,
        uint256 amount
    ) public {
        //_verify(hash, v, r, s);
        require(totalSupply(0).add(amount) <= limitMintNFTTotal, "AquarisFishContainer: NFT mint limit is exceeded");

        uint256 allowance = stableErc20.allowance(msg.sender, address(this));
        require(allowance >= amount.mul(100).mul(10**18),  "AquarisFishContainer: check the token allowance");
        stableErc20.transferFrom(msg.sender, address(this), amount.mul(100).mul(10**18));

        _mint(account, 0, amount, "0x");
    }

    function withdrawRewards() public view returns (uint256){
        //require(stakingIsOver, "AquarisFishContainer: staking is not over.");
        require(balanceOf(msg.sender, 0) > 0, "AquarisFishContainer: You do not have NFT to get the rewards.");
        
        uint256 rewardPercent = _calculateRewardsPercent(msg.sender);

        if (rewardPercent == maxPercent.mul(100000)) {
            // TODO calculate boost percentage
            return rewardPercent.add(_calculateBoost(msg.sender));
        }

        return rewardPercent;
    }

    // ---- utilities ----

    function _verify(
        bytes32 _hash,
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) internal {
        require(
            _hashMap[_hash] == false,
            "AquarisFishContainer: hash code have been used"
        );
        _hashMap[_hash] = true;

        address _signer = ecrecover(_hash, _v, _r, _s);
        
        require (_signer == _signerAccount, "AquarisFishContainer: invalid call signature");
    }

    function _calculateRewardsPercent(address account) internal view returns (uint256) {
        if (balanceOf(account, 0) == 1) { 
            return minPercent.mul(100000);
        }
        else if (balanceOf(account, 0) == 50) {
            return maxPercent.mul(100000);
        } else {
            return ((balanceOf(account, 0).sub(1)).mul(((maxPercent.sub(minPercent)).mul(100000)).div(49))).add(100000);
        }
    }

    function _calculateBoost(address account) internal view returns (uint256) {
        uint256 accountBalance = aqsErc20.balanceOf(account).div(10**18);
        if (accountBalance < tokenStep) {
            return 0; // back minBoostPercent
        }

        if (accountBalance > tokenStep.mul(10)) {
            return 100000; // back maxBoostPercent (10000 = 0.1%, then 100000 = 1%)
        }

        return accountBalance.div(tokenStep).mul(10000); // back boostPercent
    }

    // ---- only owner ----

    function setSignerAccount(address signerAccount) public onlyOwner {
        _signerAccount = signerAccount;
    }

    function mintForDrop(
        address account,
        uint256 amount
    ) public onlyOwner {
        _mint(account, 1, amount, "0x");
    }

    function burn(
        uint256 id,
        uint256 amount
    ) external onlyOwner {
        _burn(msg.sender, id, amount);
    }

    function setTokenStep(uint256 step) external onlyOwner {
        tokenStep = step;
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        ERC1155Supply._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
