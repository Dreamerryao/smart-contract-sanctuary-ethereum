/**
    @title A contract for Bus Taxing System
    @author iamenochlee
    @notice This smart contract eradicates the 
    need for tax collectors which could lead to traffic on a travel highway, 
    it calculates tax base on giving details, taking into count neccesary scenerios where change might be needed. 
    @dev A smart contract solution to bus passenger vehicle taxing system.
 */

contract BusTaxing is PassengerVehichleTaxing, KeeperCompatibleInterface {
    /// @notice this abstract contract implement
    ///PassengerVehichleTaxing contract also adds a
    ///function that runs evry 24 hours to implement the tax
    /// @dev Using PassengerVehichleTaxing contract and also
    /// creating some new functions to tax the drivers

    uint256 private s_minBusCapacity = 12;
    address internal owner;
    uint256 public immutable INTERVAL;
    uint256 private lastTimeStamp;
    bool private isCalculated = false;

    constructor(
        address priceFeed,
        uint256 interval
    ) PassengerVehichleTaxing(priceFeed, s_minBusCapacity, msg.sender) {
        owner = msg.sender;
        INTERVAL = interval;
        lastTimeStamp = block.timestamp;
    }

    //for testing purpose
    function tax() private {
        status = Status.CALCULATING;
        handleTax();
        handleDefaulters();
        emit TaxCalculated();
        status = Status.OPEN;
        /// @dev ensuring that the caller is a registered driver
        // for (uint256 i = 0; i < s_drivers.length; i++) {
        //     /// @dev resetting the count for updating working status to 0
        //     s_drivers[i].isWorking = true;
        // }
        lastTimeStamp = block.timestamp;
    }

    //Called by Chainlink Keepers to check if work needs to be done
    function checkUpkeep(
        bytes calldata /*checkData */
    ) external returns (bool upkeepNeeded, bytes memory upKeepData) {
        require(isCalculated == false, "isCalculated not false");
        upkeepNeeded = (block.timestamp - lastTimeStamp) > INTERVAL;
        if (upkeepNeeded == true) {
            isCalculated = true;
        }
        return (upkeepNeeded, upKeepData);
    }

    function getCalculated() public view returns (bool) {
        return isCalculated;
    }

    //Called by Chainlink Keepers to handle work
    function performUpkeep(bytes calldata) external {
        require(isCalculated == true, "cant perform");
        tax();
        lastTimeStamp = block.timestamp;
        isCalculated = false;
        emit TaxCalculated();
    }
}

