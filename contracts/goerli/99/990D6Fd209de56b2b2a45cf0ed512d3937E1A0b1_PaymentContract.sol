import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

/**
 * @title PaymentContract
 * @author https://github.com/deftfitf
 * @notice
 * This contract acts as a back-end contract
 * to build an open payment system that can be verified by a third party*
 *
 * At least three actors operate this contract.
 * 1. The operator who manages the payment system
 * 2. Store owners who want to provide payments using the payment system
 * 3. End users who want to make payments using the payment system
 *
 */
contract PaymentContract is AccessControl, Initializable {
    using Math for uint;

    enum StoreStatus {UnVerified, Verified, Frozen}
    struct Store {
        uint storeId;
        string name;
        address storeOwner;
        uint deposit;
        StoreStatus status;
        uint paymentCount;
        uint lastPaidTime;
    }

    struct Payment {
        uint storeId;
        uint paymentId;
        uint value;
        uint expireAt;
        bool paid;
    }

    event StoreRegistered(uint storeId, address storeOwner, bytes32 signature);
    event StoreVerified(uint storeId);
    event StoreRejected(uint storeId);
    event StoreFrozen(uint storeId);
    event PaidUsageFee(uint storeId);
    event CreatedPayment(uint storeId, uint paymentId);
    event PaidPayment(uint storeId, uint paymentId);

    uint public constant PAYMENT_SERVICE_FEE_CYCLE_SEC = 30 * 24 * 60 * 60;
    uint public constant PAYMENT_EXPIRE_TTL_UPPER_LIMIT = 24 * 60 * 60;
    uint public constant MONTHLY_PAYMENT_UPPER_LIMIT = 50;
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    uint public contractDeposit;
    uint public monthlyCharge;
    mapping(uint => Store) public storeIdToStore;
    mapping(bytes32 => Payment) public paymentKeyToPayment;

    bool private testFlag;
    /**
     * @dev Modifier to indicate a function that can only be called for testing
     */
    modifier onlyTest() {
        require(testFlag, "This function can only be called for testing.");
        _;
    }


    function initialize(uint _monthlyCharge, bool _testFlag) public initializer {
        _setRoleAdmin(OPERATOR_ROLE, ADMIN_ROLE);
        _setupRole(ADMIN_ROLE, msg.sender);
        monthlyCharge = _monthlyCharge;
        testFlag = _testFlag;
    }

    /**
     * @return get key to determine store's payment
     */
    function getPaymentKey(uint _storeId, uint _paymentId) internal pure returns (bytes32) {
        return keccak256(abi.encode(_storeId, _paymentId));
    }

    /**
     * @notice
     * Users who want to register a store can make a link
     * by depositing a monthly usage fee of at least one month
     * and submitting a signature to a pre-issued one-time token.
     *
     * @param _storeId Pre-assigned storeId
     * @param _name The name of the store available for your own management
     * @param _signature A signature for the one-time token associated with the pre-assigned storeId.
     */
    function registerStore(uint _storeId, string calldata _name, bytes32 _signature) public payable {
        require(msg.value >= monthlyCharge, "A deposit of at least one month's usage fee is required to start using the service.");
        require(storeIdToStore[_storeId].storeId == 0, "The requested storeId has already been registered, so you have to try another storeId.");

        storeIdToStore[_storeId] = Store(
            _storeId, _name, msg.sender, msg.value,
            StoreStatus.UnVerified, 0, 0
        );

        emit StoreRegistered(_storeId, msg.sender, _signature);
    }

    /**
     * @notice Reject stores that could not verify the correct signature.
     * @param _storeIds Specify all the storeIds to reject.
     * @dev Expect operations from backend applications with OPERATOR_ROLE.
     */
    function rejectStore(uint[] memory _storeIds) public onlyRole(OPERATOR_ROLE) {
        for (uint i = 0; i < _storeIds.length; i++) {
            uint storeId = _storeIds[i];
            Store memory store = storeIdToStore[storeId];
            if (store.storeId == 0) {
                console.log("The storeId=%d is not registered, so this is skipped.", storeId);
                continue;
            }
            if (store.status != StoreStatus.UnVerified) {
                console.log("You cannot reject a store that is not unapproved.");
                continue;
            }

            // This will return the deposited amount after deducting the gas fee.
            uint rest = store.deposit - (tx.gasprice).min(store.deposit);
            if (rest > 0) {
                payable(store.storeOwner).transfer(rest);
            }

            delete storeIdToStore[storeId];
            emit StoreRejected(storeId);
        }
    }

    /**
     * @notice Finds information for all specified stores.
     * @param _storeIds Specify all the storeIds to find.
     */
    function findStoresBy(uint[] calldata _storeIds) public view returns (
        uint[] memory storeIds,
        string[] memory names,
        address[] memory storeOwners,
        uint[] memory deposits,
        StoreStatus[] memory statuses,
        uint[] memory paymentCounts,
        uint[] memory lastPaidTimes
    ) {
        storeIds = new uint[](_storeIds.length);
        names = new string[](_storeIds.length);
        storeOwners = new address[](_storeIds.length);
        deposits = new uint[](_storeIds.length);
        statuses = new StoreStatus[](_storeIds.length);
        paymentCounts = new uint[](_storeIds.length);
        lastPaidTimes = new uint[](_storeIds.length);

        for (uint i = 0; i < _storeIds.length; i++) {
            Store memory store = storeIdToStore[_storeIds[i]];
            storeIds[i] = store.storeId;
            names[i] = store.name;
            storeOwners[i] = store.storeOwner;
            deposits[i] = store.deposit;
            statuses[i] = store.status;
            paymentCounts[i] = store.paymentCount;
            lastPaidTimes[i] = store.lastPaidTime;
        }
    }

    /**
     * @notice Finds information for all specified payments.
     * @dev
     * Call each array with a combination of storeId and paymentId that uniquely identifies the target payment.
     *
     * example:
     *   If you want to retrieve Payment(storeId=1, paymentId=5), Payment(storeId=3, paymentId=9),
     *   you have to request with _storeIds=[1, 3], _paymentIds=[5, 9].
     */
    function findPaymentsBy(uint[] calldata _storeIds, uint[] calldata _paymentIds) public view returns (
        uint[] memory storeIds,
        uint[] memory paymentIds,
        uint[] memory paymentValues,
        uint[] memory expireAts,
        bool[] memory paids
    ) {
        storeIds = new uint[](_storeIds.length);
        paymentIds = new uint[](_storeIds.length);
        paymentValues = new uint[](_storeIds.length);
        expireAts = new uint[](_storeIds.length);
        paids = new bool[](_storeIds.length);

        for (uint i = 0; i < _storeIds.length; i++) {
            bytes32 paymentKey = getPaymentKey(_storeIds[i], _paymentIds[i]);
            Payment memory payment = paymentKeyToPayment[paymentKey];
            storeIds[i] = payment.storeId;
            paymentIds[i] = payment.paymentId;
            paymentValues[i] = payment.value;
            expireAts[i] = payment.expireAt;
            paids[i] = payment.paid;
        }
    }

    /**
     * @notice The store that can be verified as having the correct signature is transitioned to the verified status.
     * @param _storeIds Specify all the storeIds to verify.
     * @dev Expect operations from backend applications with OPERATOR_ROLE.
     */
    function verifyStore(uint[] calldata _storeIds) public onlyRole(OPERATOR_ROLE) {
        for (uint i = 0; i < _storeIds.length; i++) {
            uint storeId = _storeIds[i];
            Store storage store = storeIdToStore[storeId];
            if (store.storeId == 0) {
                console.log("The storeId=%d is not registered, so this is skipped.", storeId);
                continue;
            }
            if (store.status != StoreStatus.UnVerified) {
                console.log("You cannot approve a store that is not unapproved.");
                continue;
            }

            // If the balance is insufficient, it will be rejected.
            if (store.deposit < monthlyCharge) {
                uint256[] memory rejects = new uint256[](1);
                rejects[0] = storeId;
                rejectStore(rejects);
                return;
            }

            // Withdraw the usage fee for the first month when the store is approved.
            store.deposit -= monthlyCharge;
            contractDeposit += monthlyCharge;
            store.status = StoreStatus.Verified;
            store.lastPaidTime = block.timestamp;
            emit StoreVerified(storeId);
            emit PaidUsageFee(storeId);
        }
    }

    /**
     * @notice You can withdraw any amount you like from your deposit balance.
     * @param _storeId ID of the store you want to withdraw
     * @param _value Amount you want to withdraw
     */
    function withdraw(uint _storeId, uint _value) public returns (uint withdrawn) {
        Store storage store = storeIdToStore[_storeId];
        require(
            store.storeOwner == msg.sender,
            "Only the owner of the store can withdraw."
        );

        withdrawn = (store.deposit).min(_value);
        console.log(
            "%d has been withdrawn from the balance=%d of storeId=%d.",
            withdrawn, store.deposit, _storeId
        );

        store.deposit -= withdrawn;
        payable(store.storeOwner).transfer(withdrawn);
    }

    /**
     * @notice Make an additional deposit
     */
    function deposit(uint _storeId) public payable {
        Store storage store = storeIdToStore[_storeId];
        require(
            store.storeOwner == msg.sender,
            "Only the owner of the store can deposit."
        );

        store.deposit += msg.value;
    }

    /**
      * @notice Unlock the number of additional uses
      */
    function unlockUsage(uint _storeId) public {
        Store storage store = storeIdToStore[_storeId];
        require(
            store.storeOwner == msg.sender,
            "Only the owner of the store can unlock."
        );

        _payServiceFee(_storeId, store);
    }

    /**
     * @notice Create a new payment. The end-user can make a payment for this.
     * @param _storeId Payee storeId
     * @param _paymentId PaymentId
     * @param _value Requested payment amount
     * @dev
     * Expect operations from backend applications with OPERATOR_ROLE.
     */
    function createPayment(uint _storeId, uint _paymentId, uint _value, uint _ttl) public onlyRole(OPERATOR_ROLE) {
        Store storage store = storeIdToStore[_storeId];
        require(
            store.storeId != 0 && store.status == StoreStatus.Verified,
            "The store must exist and be verified."
        );
        require(
            _paymentId > 0,
            "The payment id must be greater than 0"
        );
        require(
            store.paymentCount < MONTHLY_PAYMENT_UPPER_LIMIT,
            "You have exceeded your monthly payment limit. An additional deposit is required for additional use."
        );
        require(
            _ttl <= PAYMENT_EXPIRE_TTL_UPPER_LIMIT,
            "The payment ttl you specified must be within 24 hours."
        );

        bytes32 paymentKey = getPaymentKey(_storeId, _paymentId);
        require(
            paymentKeyToPayment[paymentKey].storeId == 0 &&
            paymentKeyToPayment[paymentKey].paymentId == 0,
            "The payment specified by the key mustn't exists"
        );

        paymentKeyToPayment[paymentKey] = Payment(
            _storeId, _paymentId, _value, block.timestamp + _ttl, false
        );
        store.paymentCount++;
        emit CreatedPayment(_storeId, _paymentId);
    }

    /**
     * @notice
     * Make a payment for the payment that has been created.
     * Please send more than the amount required by the payment amount.
     * If you pay more than the requested amount, its change will be returned.
     * @param _storeId Payee storeId
     * @param _paymentId PaymentId
     */
    function doPayment(uint _storeId, uint _paymentId) public payable {
        bytes32 key = getPaymentKey(_storeId, _paymentId);
        Payment storage payment = paymentKeyToPayment[key];
        require(
            payment.storeId != 0 && payment.paymentId != 0 && payment.value >= msg.value,
            "The payment you specified by store_id and payment_id doesn't exist or your `msg.value` doesn't meet the payment value needed."
        );
        require(payment.expireAt > block.timestamp, "The payment is already expired");
        require(!payment.paid, "This payment has already been paid.");

        Store storage store = storeIdToStore[_storeId];
        require(
            store.storeId != 0,
            "The store linked to the payment must exists"
        );

        store.deposit += payment.value;
        payment.paid = true;
        if (msg.value > payment.value) {
            uint change = msg.value - payment.value;
            payable(msg.sender).transfer(change);
        }

        emit PaidPayment(_storeId, _paymentId);
    }

    /**
     * @notice Triggers the operator to withdraw the monthly fee.
     * @param _storeIds Specifies a list of storeIds that trigger the withdrawal.
     * @dev
     * Expect operations from backend applications with OPERATOR_ROLE.
     *
     * This call is valid only for stores that have already passed one month
     * since the last payment and are Verified.
     *
     * Therefore, it is recommended to get and call the list of storeIds
     * for which the withdrawal can be executed by the query in advance to save the gas bill.
     */
    function payServiceFee(uint[] calldata _storeIds) public onlyRole(OPERATOR_ROLE) {
        for (uint i = 0; i < _storeIds.length; i++) {
            uint storeId = _storeIds[i];
            Store storage store = storeIdToStore[storeId];
            if (store.storeId == 0) {
                console.log("The storeId=%d is not registered, so this is skipped.", storeId);
                continue;
            }
            if (store.status != StoreStatus.Verified) {
                console.log("The storeId=%d is not verifed, so this is skipped.", storeId);
                continue;
            }

            // This guarantee that you will only be deducted once a month.
            if (block.timestamp - store.lastPaidTime < PAYMENT_SERVICE_FEE_CYCLE_SEC) {
                continue;
            }

            _payServiceFee(storeId, store);
        }
    }

    function _payServiceFee(uint storeId, Store memory store) internal {
        console.log(
            "One month has passed since the last payment of the service fee for storeId=%d, so the fee=%d will be deducted.",
            storeId, monthlyCharge
        );

        // If the deposit is less than the monthly fee, we will freeze it. If it is frozen, you will not be able to make new payments.
        if (store.deposit < monthlyCharge) {
            console.log(
                "The storeId=%d deposit is below the monthly fee, so we will freeze the store.",
                storeId
            );

            store.status = StoreStatus.Frozen;
            emit StoreFrozen(storeId);
            return;
        }

        store.deposit -= monthlyCharge;
        contractDeposit += monthlyCharge;
        store.lastPaidTime = block.timestamp;
        store.status = StoreStatus.Verified;
        store.paymentCount = 0;
        emit PaidUsageFee(storeId);
    }

    /**
     * @dev When testing, change some states of difficult-to-control values only in the test contract.
     */
    function modifyStoreState(uint storeId, uint paymentCount, uint lastPaidTime) external onlyTest {
        Store storage store = storeIdToStore[storeId];
        store.paymentCount = paymentCount;
        store.lastPaidTime = lastPaidTime;
    }

}

