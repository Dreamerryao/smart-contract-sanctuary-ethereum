pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";

import "./CyanVaultV1.sol";
import "./IFactory.sol";
import "hardhat/console.sol";

contract CyanPaymentPlanV2 is
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable
{
    using ECDSAUpgradeable for bytes32;

    enum PaymentPlanStatus {
        BNPL_CREATED,
        BNPL_FUNDED,
        BNPL_ACTIVE,
        BNPL_DEFAULTED,
        BNPL_REJECTED,
        BNPL_COMPLETED,
        BNPL_LIQUIDATED,
        PAWN_ACTIVE,
        PAWN_DEFAULTED,
        PAWN_COMPLETED
    }
    struct Item {
        address contractAddress;
        uint256 tokenId;
        uint256 tokenAmount;
    }
    struct Plan {
        uint256 amount;
        uint256 downPaymentAmount;
        uint256 interestRate;
        uint256 serviceFeeRate;
        uint256 term;
        uint8 totalNumberOfPayments;
        uint8 counterPaidPayments;
    }
    // TODO: update order of the variables for gas optimization
    struct PaymentPlan {
        Item item;
        Plan plan;
        uint256 createdDate;
        address cyanVaultAddress;
        address createdUserAddress;
        address cyanWalletAddress;
        PaymentPlanStatus status;
    }

    bytes32 public constant CYAN_ROLE = keccak256("CYAN_ROLE");
    uint256 private claimableServiceFee;
    address private cyanSigner;
    mapping(address => address) public collectionToVault;
    mapping(uint256 => PaymentPlan) public paymentPlan;
    address private lockERC721Module;
    address private walletFactory;

    function initialize(
        address _cyanSigner,
        address _cyanSuperAdmin,
        address _lockERC721Module,
        address _walletFactory
    ) external initializer {
        require(
            _cyanSigner != address(0),
            "Cyan signer address cannot be zero"
        );
        require(
            _cyanSuperAdmin != address(0),
            "Cyan super admin address cannot be zero"
        );

        claimableServiceFee = 0;
        cyanSigner = _cyanSigner;
        lockERC721Module = _lockERC721Module;
        walletFactory = _walletFactory;
        _setupRole(DEFAULT_ADMIN_ROLE, _cyanSuperAdmin);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function updateCollectionToVault(
        address[] calldata collections,
        address[] calldata vaults
    ) external {
        require(
            collections.length == vaults.length,
            "Input array sizes are not equal"
        );
        for (uint256 i; i < collections.length; ++i) {
            collectionToVault[collections[i]] = vaults[i];
        }
    }

    /**
     * @notice Create BNPL payment plan
    
     */
    function createBNPLPaymentPlan(
        Item calldata item,
        Plan calldata plan,
        uint256 planId,
        uint256 signedBlockNum,
        bytes memory signature
    ) external payable nonReentrant {
        // TODO: Add verify signature
        require(
            signedBlockNum <= block.number,
            "Signed block number must be older"
        );
        require(signedBlockNum + 50 >= block.number, "Signature expired");
        require(
            plan.serviceFeeRate <= 300,
            "Service fee rate must be less than 3 percent"
        );
        require(
            paymentPlan[planId].plan.totalNumberOfPayments == 0,
            "Payment plan already exists"
        );
        require(plan.amount > 0, "Price of token is non-positive");
        require(plan.interestRate > 0, "Interest rate is non-positive");
        require(msg.value > 0, "Downpayment amount is non-positive");
        require(plan.term > 0, "Term is non-positive");
        require(
            plan.totalNumberOfPayments > 0,
            "Total number of payments is non-positive"
        );
        require(
            collectionToVault[item.contractAddress] != address(0),
            "Collection is not supported"
        );
        // Factory factory = Factory(walletFactory);
        // address cyanWalletAddress = factory.deployWallet(msg.sender);
        address cyanWalletAddress = msg.sender;

        paymentPlan[planId] = PaymentPlan(
            item,
            plan,
            block.timestamp, // uint256 createdDate
            collectionToVault[item.contractAddress],
            msg.sender,
            cyanWalletAddress,
            PaymentPlanStatus.BNPL_CREATED // status
        );
    }

    /**
     * @notice Lending ETH from Vault for BNPL payment plan
     * @param planId Payment plan ID
     */
    function fundBNPL(uint256 planId)
        external
        nonReentrant
        onlyRole(CYAN_ROLE)
    {
        require(
            paymentPlan[planId].plan.counterPaidPayments == 1,
            "Only downpayment must be paid"
        );
        require(
            paymentPlan[planId].status == PaymentPlanStatus.BNPL_CREATED,
            "BNPL payment plan must be at CREATED stage"
        );

        paymentPlan[planId].status = PaymentPlanStatus.BNPL_FUNDED;

        address _cyanVaultAddress = paymentPlan[planId].cyanVaultAddress;
        CyanVaultV1 _cyanVaultV1 = CyanVaultV1(payable(_cyanVaultAddress));
        _cyanVaultV1.lend(msg.sender, paymentPlan[planId].plan.amount);
    }

    /**
     * @notice Activating a BNPL payment plan
     * @param planId Payment Plan ID
     */
    function activateBNPL(uint256 planId)
        external
        nonReentrant
        onlyRole(CYAN_ROLE)
    {
        require(
            paymentPlan[planId].plan.counterPaidPayments == 1,
            "Only downpayment must be paid"
        );
        require(
            paymentPlan[planId].status == PaymentPlanStatus.BNPL_FUNDED,
            "BNPL payment plan must be at FUNDED stage"
        );

        paymentPlan[planId].status = PaymentPlanStatus.BNPL_ACTIVE;

        ERC721Upgradeable erc721Contract = ERC721Upgradeable(
            paymentPlan[planId].item.contractAddress
        );
        erc721Contract.safeTransferFrom(
            msg.sender,
            paymentPlan[planId].cyanWalletAddress,
            paymentPlan[planId].item.tokenId
        );
        (bool success, bytes memory result) = address(
            paymentPlan[planId].cyanWalletAddress
        ).call(
                abi.encodeWithSignature(
                    "executeModule(address,bytes)",
                    lockERC721Module,
                    abi.encodeWithSignature(
                        "setLockedERC721Token(address,uint256,bool)",
                        paymentPlan[planId].item.contractAddress,
                        paymentPlan[planId].item.tokenId,
                        true
                    )
                )
            );
        require(success, "Locking not successful");

        uint256 singleServiceFee = (paymentPlan[planId].plan.amount *
            paymentPlan[planId].plan.serviceFeeRate) / 40000;

        claimableServiceFee += singleServiceFee;

        address _cyanVaultAddress = paymentPlan[planId].cyanVaultAddress;
        transferEarnedAmountToCyanVault(
            _cyanVaultAddress,
            paymentPlan[planId].plan.downPaymentAmount - singleServiceFee,
            0
        );
    }

    /**
     * @notice Reject the payment plan
     * @param planId Payment plan ID
     */
    function rejectBNPLPaymentPlan(uint256 planId)
        external
        nonReentrant
        onlyRole(CYAN_ROLE)
    {
        require(
            paymentPlan[planId].plan.counterPaidPayments == 1,
            "Payment done other than downpayment for this plan"
        );
        require(
            paymentPlan[planId].status == PaymentPlanStatus.BNPL_CREATED,
            "BNPL payment plan must be at CREATED stage"
        );

        // Returning downpayment to created user address
        payable(paymentPlan[planId].createdUserAddress).transfer(
            paymentPlan[planId].plan.downPaymentAmount
        );
        paymentPlan[planId].status = PaymentPlanStatus.BNPL_REJECTED;
    }

    /**
     * @notice Reject the payment plan after FUNDED
     * @param planId Payment Plan ID
     */
    function rejectBNPLPaymentPlanAfterFunded(uint256 planId)
        external
        payable
        nonReentrant
        onlyRole(CYAN_ROLE)
    {
        require(
            paymentPlan[planId].plan.counterPaidPayments == 1,
            "Payment done other than downpayment for this plan"
        );
        require(
            paymentPlan[planId].status == PaymentPlanStatus.BNPL_FUNDED,
            "BNPL payment plan must be at FUNDED stage"
        );
        require(
            paymentPlan[planId].plan.amount == msg.value,
            "Wrong fund return amount"
        );
        // Returning downpayment to created user address
        payable(paymentPlan[planId].createdUserAddress).transfer(
            paymentPlan[planId].plan.downPaymentAmount
        );
        paymentPlan[planId].status = PaymentPlanStatus.BNPL_REJECTED;

        // Returning funded amount back to Vault
        address _cyanVaultAddress = paymentPlan[planId].cyanVaultAddress;
        transferEarnedAmountToCyanVault(_cyanVaultAddress, msg.value, 0);
    }

    /**
     * @notice Liquidate defaulted payment plan
     * @param planId Payment Plan ID
     * @param estimatedValue Estimated value of defaulted assets
     */
    function liquidate(uint256 planId, uint256 estimatedValue)
        external
        nonReentrant
        onlyRole(CYAN_ROLE)
    {
        require(
            paymentPlan[planId].plan.totalNumberOfPayments >
                paymentPlan[planId].plan.counterPaidPayments,
            "Total payment done"
        );

        (
            uint256 payAmountForCollateral,
            uint256 payAmountForInterest,
            uint256 payAmountForService,
            uint256 currentPayment,
            uint256 dueDate
        ) = getNextBNPLPayment(planId);

        // TODO: calculate unpaid amount
        uint256 unpaidAmount = payAmountForCollateral;

        require(dueDate < block.timestamp, "Next payment is still due");
        require(unpaidAmount > 0, "Unpaid is non-positive");

        // Should integrate with Cyan wallet
        ERC721Upgradeable erc721Contract = ERC721Upgradeable(
            paymentPlan[planId].item.contractAddress
        );
        erc721Contract.safeTransferFrom(
            paymentPlan[planId].cyanWalletAddress,
            paymentPlan[planId].cyanVaultAddress,
            paymentPlan[planId].item.tokenId
        );
        paymentPlan[planId].status = PaymentPlanStatus.BNPL_LIQUIDATED;

        address _cyanVaultAddress = paymentPlan[planId].cyanVaultAddress;
        require(_cyanVaultAddress != address(0), "Cyan vault has zero address");
        CyanVaultV1 _cyanVaultV1 = CyanVaultV1(payable(_cyanVaultAddress));
        _cyanVaultV1.nftDefaulted(unpaidAmount, estimatedValue);
    }

    /**
     * @notice Make a payment for the payment plan
     * @param planId Payment Plan ID
     */
    function pay(uint256 planId) external payable nonReentrant {
        (
            uint256 payAmountForCollateral,
            uint256 payAmountForInterest,
            uint256 payAmountForService,
            uint256 currentPayment,
            uint256 dueDate
        ) = getNextBNPLPayment(planId);

        require(currentPayment == msg.value, "Wrong payment amount");
        require(dueDate >= block.timestamp, "Payment due date is passed");

        paymentPlan[planId].plan.counterPaidPayments++;
        claimableServiceFee += payAmountForService;

        address _cyanVaultAddress = paymentPlan[planId].cyanVaultAddress;
        transferEarnedAmountToCyanVault(
            _cyanVaultAddress,
            payAmountForCollateral,
            payAmountForInterest
        );
        if (
            paymentPlan[planId].plan.counterPaidPayments ==
            paymentPlan[planId].plan.totalNumberOfPayments
        ) {
            // UNLOCK the token here
            paymentPlan[planId].status = PaymentPlanStatus.BNPL_COMPLETED;
            (bool success, bytes memory result) = address(
                paymentPlan[planId].cyanWalletAddress
            ).call(
                    abi.encodeWithSignature(
                        "executeModule(address,bytes)",
                        lockERC721Module,
                        abi.encodeWithSignature(
                            "setLockedERC721Token(address,uint256,bool)",
                            paymentPlan[planId].item.contractAddress,
                            paymentPlan[planId].item.tokenId,
                            false
                        )
                    )
                );
        } else {
            // paid event
        }
    }

    /**
     * @notice Transfer earned amount to Cyan Vault
     * @param cyanVaultAddress Original price of the token
     * @param paidTokenPayment Paid token payment
     * @param paidInterestFee Paid interest fee
     */
    function transferEarnedAmountToCyanVault(
        address cyanVaultAddress,
        uint256 paidTokenPayment,
        uint256 paidInterestFee
    ) private {
        require(cyanVaultAddress != address(0), "Cyan vault has zero address");
        CyanVaultV1 _cyanVaultV1 = CyanVaultV1(payable(cyanVaultAddress));
        _cyanVaultV1.earn{value: paidTokenPayment + paidInterestFee}(
            paidTokenPayment,
            paidInterestFee
        );
    }

    /**
     * @notice Return expected BNPL payment plan for given price and interest rate
     * @param amount Price of the asset(s)
     * @param downPaymentAmount amount that user is paying upfront
     * @param interestRate Interest rate
     * @param serviceFeeRate service fee rate
     * @param numOfPayment Number of payments including downpayment
     */
    function getExpectedBNPLPlan(
        uint256 amount,
        uint256 downPaymentAmount,
        uint256 interestRate,
        uint256 serviceFeeRate,
        uint8 numOfPayment
    )
        external
        pure
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        // Total service fee, Note that service fee rate is x100
        uint256 totalServiceFee = (amount * serviceFeeRate) / 10000;

        // Remaining payment amount is considered sum of down payment + single service fee
        uint256 remainingPaymentAmount = (amount +
            totalServiceFee /
            numOfPayment) - downPaymentAmount;

        // Calculating interest fee, Note that interest fee rate is x100
        uint256 totalInterestFee = (remainingPaymentAmount * interestRate) /
            10000;

        // Total purchasing amount
        uint256 totalPayment = (amount + totalInterestFee) + totalServiceFee;

        // Subsequent payment amount after down payment, Note that exact amount of the last payment might differ due to floating point
        uint256 singlePayment = (totalPayment - downPaymentAmount) /
            (numOfPayment - 1);

        return (
            amount,
            totalInterestFee,
            totalServiceFee,
            singlePayment,
            totalPayment
        );
    }

    /**
     * @notice Return very next paymet of the BNPL plan
     * @param planId Payment Plan ID
     */
    function getNextBNPLPayment(uint256 planId)
        public
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        require(
            paymentPlan[planId].plan.totalNumberOfPayments != 0,
            "No payment plan found"
        );
        // Total service fee, Note that service fee rate is x100
        uint256 totalServiceFee = (paymentPlan[planId].plan.amount *
            paymentPlan[planId].plan.serviceFeeRate) / 10000;

        // Remaining payment amount is considered sum of down payment + single service fee
        uint256 remainingPaymentAmount = paymentPlan[planId].plan.amount -
            paymentPlan[planId].plan.downPaymentAmount +
            totalServiceFee /
            paymentPlan[planId].plan.totalNumberOfPayments;

        // Calculating interest fee, Note that interest fee rate is x100
        uint256 totalInterestFee = (remainingPaymentAmount *
            paymentPlan[planId].plan.interestRate) / 10000;

        uint256 singleServiceFee = totalServiceFee /
            paymentPlan[planId].plan.totalNumberOfPayments;
        uint256 singleInterestFee = totalInterestFee /
            (paymentPlan[planId].plan.totalNumberOfPayments - 1);
        uint256 singleLoanPayment = remainingPaymentAmount /
            (paymentPlan[planId].plan.totalNumberOfPayments - 1);
        uint256 dueDate = paymentPlan[planId].createdDate +
            paymentPlan[planId].plan.counterPaidPayments *
            paymentPlan[planId].plan.term;

        return (
            singleLoanPayment,
            singleInterestFee,
            singleServiceFee,
            singleServiceFee + singleInterestFee + singleLoanPayment,
            dueDate
        );
    }

    /**
     * @notice Check if payment plan is pending
     * @param planId Payment Plan ID
     * @return PaymentPlanStatus
     */
    function getBNPLPlanStatus(uint256 planId)
        external
        view
        returns (PaymentPlanStatus)
    {
        if (paymentPlan[planId].status != PaymentPlanStatus.BNPL_ACTIVE) {
            return paymentPlan[planId].status;
        }
        (, , , , uint256 dueDate) = getNextBNPLPayment(planId);
        bool isDefaulted = block.timestamp > dueDate;

        if (isDefaulted) {
            return PaymentPlanStatus.BNPL_DEFAULTED;
        }
        return paymentPlan[planId].status;
    }

    /**
     * @notice Getting claimable service fee amount
     */
    function getClaimableServiceFee()
        external
        view
        onlyRole(CYAN_ROLE)
        returns (uint256)
    {
        return claimableServiceFee;
    }

    /**
     * @notice Claiming collected service fee amount
     */
    function claimServiceFee()
        external
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        payable(msg.sender).transfer(claimableServiceFee);
        claimableServiceFee = 0;
    }

    function updateWalletFactoryAddress(address factory) public {
        walletFactory = factory;
    }

    function updateLockERC721Address(address lock) public {
        lockERC721Module = lock;
    }
}

