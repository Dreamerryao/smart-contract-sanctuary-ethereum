        exchanges.push( Exchange({ location: Location({q: 2, r: -1, s: -1}), x: Currency.COPPER, y: Currency.SILVER, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: 1, r: -2, s: 1}), x: Currency.COPPER, y: Currency.DIAMOND, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: -3, r: 0, s: 3}), x: Currency.SILVER, y: Currency.USD, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: -2, r: 1, s: 1}), x: Currency.SILVER, y: Currency.COPPER, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: -1, r: -1, s: 2}), x: Currency.SILVER, y: Currency.DIAMOND, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: 0, r: 3, s: -3}), x: Currency.DIAMOND, y: Currency.USD, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: -1, r: 2, s: -1}), x: Currency.DIAMOND, y: Currency.COPPER, port: ExchangePort(address(0)) }) );
        exchanges.push( Exchange({ location: Location({q: 2, r: 0, s: -2}), x: Currency.DIAMOND, y: Currency.SILVER, port: ExchangePort(address(0)) }) );
        // forgefmt: disable-end
        vrgdaFactory();
    }

    // Join a created but still pending game
    function join(bytes32 color, bytes32 initials) public gameIs(State.PENDING) {
        // when tenth player asks to join, playerCount will be 9
        require(playerCount < MAX_PLAYERS);
        require(players[msg.sender].id == 0);
        Player storage player = players[msg.sender];
        // first player has id = 1; tenth has id = 10
        player.id = ++playerCount;
        player.oldLocation = Location(0, 0, 0);
        player.color = color;
        player.initials = initials;
        for (uint256 i = 0; i <= uint256(type(Currency).max); i++) {
            currencies[Currency(i)].asset.mint(msg.sender, currencies[Currency(i)].startingBalance);
        }
        // maintain array of players since mapping does not have a way to iterate over all keys
        playerList.push(player);
    }

    function startGame() public onlyOwner gameIs(State.PENDING) {
        // todo: other init logic?
        gameState = State.STARTED;
        // start the game clock for all exchanges
        for (uint256 i = 0; i < exchanges.length; i++) {
            Exchange storage exchange = exchanges[i];
            exchange.port.setStartTime(block.timestamp);
        }
    }

    function move(Location[] calldata directionVectors) external isPlayer gameIs(State.STARTED) {
        finishMove(players[msg.sender]);

        require(players[msg.sender].isMoving == false);
        Location memory playerLocation = players[msg.sender].oldLocation;

        for (uint256 i = 0; i < directionVectors.length; i++) {
            Location memory directionVector = directionVectors[i];

            // make sure the direction vector is one of the 6 possible directions
            require(
                (directionVector.q == 1 && directionVector.r == -1 && directionVector.s == 0)
                    || (directionVector.q == 1 && directionVector.r == 0 && directionVector.s == -1)
                    || (directionVector.q == 0 && directionVector.r == 1 && directionVector.s == -1)
                    || (directionVector.q == -1 && directionVector.r == 1 && directionVector.s == 0)
                    || (directionVector.q == -1 && directionVector.r == 0 && directionVector.s == 1)
                    || (directionVector.q == 0 && directionVector.r == -1 && directionVector.s == 1)
            );

            playerLocation = Location(
                playerLocation.q + directionVector.q,
                playerLocation.r + directionVector.r,
                playerLocation.s + directionVector.s
            );

            // make sure the player is not moving outside the grid
            require(
                playerLocation.q >= -GRID_RADIUS && playerLocation.q <= GRID_RADIUS && playerLocation.r >= -GRID_RADIUS
                    && playerLocation.r <= GRID_RADIUS && playerLocation.s >= -GRID_RADIUS
                    && playerLocation.s <= GRID_RADIUS
            );
        }
        players[msg.sender].isMoving = true;
        players[msg.sender].moveStartTime = block.timestamp;
        players[msg.sender].delay = directionVectors.length * TILE_WALK_TIME;
        players[msg.sender].newLocation = playerLocation;
    }

    function finishMove(Player storage _player) internal gameIs(State.STARTED) {
        if ((_player.isMoving) && (block.timestamp >= _player.moveStartTime + _player.delay)) {
            _player.isMoving = false;
            _player.oldLocation = _player.newLocation;
        }
    }

    function trade(bytes memory data, uint256 exchangeIdx) external gameIs(State.STARTED) {
        Exchange memory exchange = exchanges[exchangeIdx];
        Location memory playerLocation = getPlayerLocation(msg.sender);
        require(
            (exchange.location.q == playerLocation.q) && (exchange.location.r == playerLocation.r)
                && (exchange.location.s == playerLocation.s)
        );
        (bool success, bytes memory result) = address(exchange.port).call(data);
        if (!success) {
            // Next 5 lines from https://ethereum.stackexchange.com/a/83577
            if (result.length < 68) revert();
            assembly {
                result := add(result, 0x04)
            }
            revert(abi.decode(result, (string)));
        }
    }

    function getPlayerBalance(address who, Currency cur) public view returns (uint256) {
        return currencies[cur].asset.balanceOf(who);
    }

    function getPlayerLocation(address who) public view returns (Location memory) {
        if ((players[who].isMoving) && (block.timestamp > players[who].moveStartTime + players[who].delay)) {
            return players[who].newLocation;
        } else {
            return players[who].oldLocation;
        }
    }

    function getPlayer(address who) external view returns (Player memory) {
        return players[who];
    }

    function getPlayerList() external view returns (Player[] memory) {
        return playerList;
    }

    function getExchange(uint256 idx) public view returns (Exchange memory) {
        return exchanges[idx];
    }

    function getExchangeList() public view returns (Exchange[] memory) {
        return exchanges;
    }
}