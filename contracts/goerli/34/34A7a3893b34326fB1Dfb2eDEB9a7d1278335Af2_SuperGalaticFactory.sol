pragma solidity 0.8.7;

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol';
import './../interfaces/ISuperGalatic.sol';
import './../interfaces/ISuperGalaticFactory.sol';
import './../Errors.sol';
import 'hardhat/console.sol';

contract SuperGalaticFactory is Ownable, ReentrancyGuard, ISuperGalaticFactory {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    mapping(uint256 => address) public nftContracts;
    mapping(address => bool) public isSuperGalatics;
    address public plasmaAddr;
    uint256 public plasmaAmountPerNFT;
    /**
     * @notice event when pool is created
     * @param nftIndex Index of the nft
     * @param nftContract address of the nft
     */
    event CreateSuperGalatic(uint256 indexed nftIndex, address indexed nftContract);

    /**
     *@notice constructor of factory. create super galatic NFT using beacon proxy
     *@param _admin this address is set as a owner of this contract
     *@param _beacon beacon proxy address for super galatic NFTs
     */
    constructor(
        address _admin,
        address _beacon,
        address _plasmaAddr,
        uint256 _plasmaAmountPerNFT
    ) {
        require(_admin != address(0), Errors.SHOULD_BE_NON_ZERO);
        Ownable.transferOwnership(_admin);
        plasmaAddr = _plasmaAddr;
        plasmaAmountPerNFT = _plasmaAmountPerNFT;
        for (uint256 index = 0; index < 10; index++) {
            _createSuperGalaticNFTs(_admin, _beacon, index);
        }
    }

    /**
     *@notice update plasma address
     *@param _plasma address of new plasma contract
     */
    function updatePlasmaAddr(address _plasma) external onlyOwner {
        plasmaAddr = _plasma;
    }

    /**
     *@notice update the plasma amount per nft
     *@param _plasmaAmount nft amount corresponding to category Ids
     */
    function updatePlasmaAmountPerNFT(uint256 _plasmaAmount) external onlyOwner {
        plasmaAmountPerNFT = _plasmaAmount;
    }

    /**
     *@notice update body part of nft
     *@param nftContract gensis nft contract address
     *@param nftId nft id which need to be updated
     *@param bodyType body type which need to be updated
     */
    function updateNFTBodypart(
        address nftContract,
        uint256 nftId,
        uint256 bodyType
    ) external {
        require(isSuperGalatics[nftContract], Errors.ONLY_SUPERGALATIC_CONTRACT);
        require(_owns(nftContract, msg.sender, nftId), Errors.ONLY_OWNER);
        ISuperGalatic(nftContract).updateBody(bodyType, nftId);
    }

    /**
     *@notice mint nft from gensis nft smart contract
     *@param categoryIds gensis nft contracts array
     *@param amounts nft amount corresponding to category Ids
     */
    function mintBatchSuperGalatic(uint256[] calldata categoryIds, uint256[] calldata amounts) external nonReentrant {
        require(categoryIds.length == amounts.length, Errors.SHOULD_SAME);
        for (uint256 i = 0; i < categoryIds.length; i++) {
            require(amounts[i] < 6, Errors.SHOULD_BE_LESS_THAN_FIVE);
            for (uint256 j = 0; j < amounts[i]; j++) {
                _mintSuperGalatic(categoryIds[i]);
            }
        }
    }

    /**
     *@notice mint nft from gensis nft smart contract
     *@param _categoryId gensis nft contract index
     */
    function mintSuperGalatic(uint256 _categoryId) external nonReentrant {
        _mintSuperGalatic(_categoryId);
    }

    /**
     *@notice interface function. check whether the given contract address is superGalatic NFT contract or not
     *@param _nftAddress this address is set as a owner of this contract
     */
    function isSuperGalaticNFTContract(address _nftAddress) external view override returns (bool) {
        return isSuperGalatics[_nftAddress];
    }

    /**
     * @notice Withdraw UFO tokens available in case of any emergency
     * @param recipient Address to receive the emergency deposit
     */
    function withdrawPlasma(address recipient) external onlyOwner {
        uint256 plasmaBalance = IERC20Upgradeable(plasmaAddr).balanceOf(address(this));
        IERC20Upgradeable(plasmaAddr).safeTransfer(recipient, plasmaBalance);
    }

    /**
     *@notice mint nft from gensis nft smart contract
     *@param _categoryId gensis nft contract index
     */
    function _mintSuperGalatic(uint256 _categoryId) private {
        address nftContract = nftContracts[_categoryId];
        require(nftContract != address(0), Errors.SHOULD_BE_NON_ZERO);
        IERC20Upgradeable crypto = IERC20Upgradeable(plasmaAddr);
        crypto.safeTransferFrom(msg.sender, address(this), plasmaAmountPerNFT);
        ISuperGalatic(nftContract).createGenesis(msg.sender);
    }

    /**
     *@notice create and deploy gensis contract to blockchain
     *@param _admin this address is set as a owner of this contract
     *@param _beacon beacon proxy address for super galatic NFTs
     *@param _index genesis NFT index
     */
    function _createSuperGalaticNFTs(
        address _admin,
        address _beacon,
        uint256 _index
    ) private {
        bytes memory empty;
        address _superGalatic = address(new BeaconProxy(_beacon, empty));
        ISuperGalatic(_superGalatic).initialize(_admin, address(this), _index);
        nftContracts[_index] = _superGalatic;
        isSuperGalatics[_superGalatic] = true;
        emit CreateSuperGalatic(_index, _superGalatic);
    }

    /// @dev Returns true if the claimant owns the token.
    /// @param _nftAddress - The address of the NFT.
    /// @param _claimant - Address claiming to own the token.
    /// @param _tokenId - ID of token whose ownership to verify.
    function _owns(
        address _nftAddress,
        address _claimant,
        uint256 _tokenId
    ) internal view returns (bool) {
        IERC721Upgradeable _nftContract = IERC721Upgradeable(_nftAddress);
        return (_nftContract.ownerOf(_tokenId) == _claimant);
    }
}

