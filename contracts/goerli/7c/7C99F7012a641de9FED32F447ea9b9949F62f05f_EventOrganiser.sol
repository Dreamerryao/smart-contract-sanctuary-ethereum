pragma solidity 0.8.8;

//Imports
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "hardhat/console.sol";
//libraries
import "./Weiconvertor.sol";

contract EventOrganiser is KeeperCompatible {
  using Weiconvertor for uint256;
  using console for uint256;

  // DataTypes

  enum eventStatus {
    OPEN,
    ENDED
  }

  // Let define our event by creating a structure

  struct Event {
    uint256 eventId;
    address organizer;
    string eventName;
    uint256 date;
    uint256 ticketPrice;
    uint256 ticketCount;
    uint256 ticketRemain;
    eventStatus status;
  }

  // Event

  event CreateEvent(
    uint256 indexed eventId,
    address organizer,
    string eventName,
    uint256 indexed date,
    uint256 indexed ticketPrice,
    uint256 ticketCount
  );

  event OutdatedEventsDetected(
    Event[] indexed outdatedEvents
  );

  //Sate Variables

  // In here we will assign ids to our events creating key value pairs of ids and events
  mapping(uint256 => Event) private idToEvents;

  uint256 private id;

  mapping(address => mapping(uint256 => uint256)) public attendeeToEvent;

  // Create a variable that stores the aggregator contract
  AggregatorV3Interface priceFeed;

  Event[] private s_events;
  Event[] private s_outdatedEvents;
  uint256[] private s_outdatedIndexes;
  uint256 private totalPayment;

  // errors

  error Event_Date_Has_Already_Passed();
  error No_Tickets_For_Sale();
  error Event_Doesnt_Exist();
  error Not_Enough_Ticket_Remains();
  error Event_Already_Completed();
  error Not_Enough_Amount();
  error YouDontHaveEnoughtTickets();
  error UpKeepNotNeeded();
  error Event_Is_Closed();

  // Modifiers

  /* */

  // Constructor

  constructor(address aggregatorContractAddress) {
    priceFeed = AggregatorV3Interface(aggregatorContractAddress);
  }

  // Create a function that lets only the organizer create the event

  function createEvent(
    string calldata _eventName,
    uint256 _date,
    uint256 _ticketPrice,
    uint256 _ticketCount
  ) public {
    // Checking if the date given organiser for hosting his event is greater than the time he's creating the event on

    /* Assuming they give us the date in seconds so we compare it to the seconds when they are creating to seconds as to when this
            transactions containing block is going to be mined */

    if (_date < block.timestamp) {
      revert Event_Date_Has_Already_Passed();
    }

    // Making sure event there are tickets to be bought
    if (_ticketCount <= 0) {
      revert No_Tickets_For_Sale();
    }

    Event memory env = Event({
      eventId: id,
      organizer: msg.sender,
      eventName: _eventName,
      date: _date,
      ticketPrice: _ticketPrice * 1e18,
      ticketCount: _ticketCount,
      ticketRemain: _ticketCount,
      status: eventStatus.OPEN
    });

    // Assign id and storing the event object created and its id as a key value pair in my mapping structure
    idToEvents[id] = env;

    s_events.push(env);

    emit CreateEvent(id, msg.sender, _eventName, _date, _ticketPrice * 1e18, _ticketCount);
    // Adding one so the next event gets stored with a unique id
    id++;
  }

  function buyTickets(uint256 _eventId, uint256 _quantity) public payable {
    /** If the event doesn't exist then there will be no value to that id key given */

    if (idToEvents[_eventId].date == 0) {
      revert Event_Doesnt_Exist();
    }

    // Checking to see if there are enough tickets to buy

    if (_quantity > idToEvents[_eventId].ticketRemain) {
      revert Not_Enough_Ticket_Remains();
    }

    // Checking to see if the event they are trying to buy tickets is still open and its date of hosting is still to come
    if (block.timestamp > idToEvents[_eventId].date) {
      revert Event_Already_Completed();
    }

    // Checking if the state is till open
    if (idToEvents[_eventId].status == eventStatus.ENDED) {
      revert Event_Is_Closed();
    }

    uint256 ticketPrice = idToEvents[_eventId].ticketPrice;
    totalPayment = ticketPrice * _quantity;

    if (msg.value.weiConvertor(priceFeed) < totalPayment) {
      revert Not_Enough_Amount();
    }

    // Updating the ticket remaining

    idToEvents[_eventId].ticketRemain -= _quantity;

    for (uint256 i = 0; i < s_events.length; i++) {
      if (s_events[i].eventId == _eventId) {
        s_events[i].ticketRemain -= _quantity;
      }
    }

    // Recording the transaction in our mapping

    uint256 previousOwnedTickets = attendeeToEvent[msg.sender][_eventId];

    // If they don't own any tickets previously we will just set the quantity they put in as the value for the key
    // But if they do then then we will take there previously owned tickets and add them recnetly purchased tickets and store them
    //along with there event id

    if (previousOwnedTickets == 0) {
      attendeeToEvent[msg.sender][_eventId] = _quantity;
    } else {
      attendeeToEvent[msg.sender][_eventId] = previousOwnedTickets + _quantity;
    }
  }

  function transferTicket(
    address transferTo,
    uint256 _eventId,
    uint256 numberOfTicketsTransferred
  ) public {
    /** If the event doesn't exist then there will be no value to that id key given */

    if (idToEvents[_eventId].date == 0) {
      revert Event_Doesnt_Exist();
    }

    // Checking to see if the event they are trying to buy tickets is still open and its date of hosting is still to come
    if (block.timestamp > idToEvents[_eventId].date) {
      revert Event_Already_Completed();
    }

    // Checking if the state is till open
    if (idToEvents[_eventId].status == eventStatus.ENDED) {
      revert Event_Is_Closed();
    }

    if (attendeeToEvent[msg.sender][_eventId] < numberOfTicketsTransferred) {
      revert YouDontHaveEnoughtTickets();
    }

    uint256 amountOfTicketsOwned = attendeeToEvent[msg.sender][_eventId];
    attendeeToEvent[msg.sender][_eventId] = amountOfTicketsOwned - numberOfTicketsTransferred;

    uint256 ticketsOwned = attendeeToEvent[transferTo][_eventId];
    attendeeToEvent[transferTo][_eventId] = ticketsOwned + numberOfTicketsTransferred;
  }

  // Automation

  function checkUpkeep(
    bytes memory /* checkData*/
  )
    public
    returns (
      bool UpkeepNeeded,
      bytes memory /* performData */
    )
  {
    for (uint256 index = 0; index < s_events.length; index++) {
      if (block.timestamp > s_events[index].date && s_events[index].status == eventStatus.OPEN) {
        s_outdatedEvents.push(s_events[index]);
        // Im storing the indexes in an array of those elements in an array
        s_outdatedIndexes.push(index);
      }
    }
    uint256 length = s_outdatedEvents.length;
    UpkeepNeeded = (length != 0);
  }

  function performUpkeep(
    bytes calldata /* performData */
  ) external override {
    (bool UpkeepNeeded, ) = checkUpkeep("");
    if (!UpkeepNeeded) {
      revert UpKeepNotNeeded();
    } else {
      emit OutdatedEventsDetected(s_outdatedEvents);
    }
    for (uint256 index = 0; index < s_outdatedEvents.length; index++) {
      uint256 eventId = s_outdatedEvents[index].eventId;
      idToEvents[eventId].status = eventStatus.ENDED;
    }
    for (uint256 index = 0; index < s_outdatedIndexes.length; index++) {
      uint256 indexOfOutDatedElement = s_outdatedIndexes[index];
      s_events[indexOfOutDatedElement].status = eventStatus.ENDED;
    }
    for (uint256 i = 0; i < s_outdatedEvents.length; i++) {
      s_outdatedEvents.pop();
    }   
    }

  // View/Pure functions

  function getEventByIndex(uint256 index) public view returns (Event memory) {
    return s_events[index];
  }

  function getOutdatedEvents(uint256 index) public view returns (Event memory) {
    return s_outdatedEvents[index];
  }

  function getIndexOfOutdatedEvents(uint256 index) public view returns(uint256){
    return s_outdatedIndexes[index];
  }

  function getAttendeeInfo(address attendee, uint256 eventId) public view returns (uint256) {
    return attendeeToEvent[attendee][eventId];
  }

  function getPriceFeed() public view returns (AggregatorV3Interface) {
    return priceFeed;
  }

  function getAllEvents() public view returns (Event[] memory) {
    return s_events;
  }

  function getEventById(uint256 id) public view returns (Event memory) {
    return idToEvents[id];
  }

  function getTotalPayment() public view returns (uint256) {
    return totalPayment;
  }
}

