// SPDX-License-Identifier: MIT
pragma solidity 0.8.8;

import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

library Weiconvertor {
    function priceFeed(AggregatorV3Interface aggregatorContract) internal view returns (uint256) {
        (, int256 price, , , ) = aggregatorContract.latestRoundData();
        int256 ethtoUSD = price / 1e8;
        return uint256(ethtoUSD);
    }

    function weiConvertor(uint256 amountpaid, AggregatorV3Interface aggregatorContract)
        internal
        view
        returns (uint256)
    {
        uint256 amountPaid = amountpaid;
        uint256 ethInUsd = priceFeed(aggregatorContract);
        uint256 weiToUsd = amountPaid * ethInUsd;
        return weiToUsd;
    }
}

