pragma solidity ^0.8.4;

import {ABDKMath64x64 as ABDK} from "../math/abdk.sol";

import "hardhat/console.sol";

struct HistoryArray {
    int[] values;
    uint[] timestamps;
    uint index;
    int32 normalPeriod;
}

library HistoryArrayLib {
    function init(
        HistoryArray storage self,
        int[] memory values,
        uint32[] memory timestamps
    ) internal {
        self.values = values;
        self.timestamps = timestamps;
        self.index = 0;
    }

    function add(
        HistoryArray storage self,
        int value,
        uint32 timestamp
    ) internal {
        self.values[self.index] = value;
        self.timestamps[self.index] = timestamp;
        self.index = (self.index + 1) % self.values.length;
    }

    function getLast(HistoryArray storage self) internal view returns (int) {
        return self.values[self.index];
    }

    function getHead(HistoryArray storage self) internal view returns (int) {
        return
            self.values[
                (self.values.length + self.index - 1) % self.values.length
            ];
    }

    function getHeadVelocity(HistoryArray storage self)
        internal
        view
        returns (int)
    {
        if (self.values.length > 1) {
            int s0 = self.values[
                (self.values.length + self.index - 2) % self.values.length
            ];
            int t0 = int(
                self.timestamps[
                    (self.values.length + self.index - 2) % self.values.length
                ]
            );

            int s1 = self.values[
                (self.values.length + self.index - 1) % self.values.length
            ];
            int t1 = int(
                self.timestamps[
                    (self.values.length + self.index - 1) % self.values.length
                ]
            );

            return (s1 - s0) / (t1 - t0);
        } else {
            return 0;
        }
    }

    function getHeadVelAccNorm(
        HistoryArray storage self,
        int maxShock,
        int maxSurge
    )
        internal
        view
        returns (
            int128 velNorm,
            int128 accNorm,
            int128 shock,
            int128 surge
        )
    {
        if (self.values.length > 2) {
            (int s1, int128 v1, int128 v2, int128 acc) = _getReal(self);
            (velNorm, accNorm) = _getNormal(s1, v1, v2, acc, self.normalPeriod);
            if (v2 < 0) {
                shock =
                    ABDK.fromInt(1) +
                    ABDK.div(ABDK.mul(ABDK.fromInt(maxShock), 7), v2);
            } else {
                shock = ABDK.fromInt(1);
            }
            if (acc < 0) {
                surge =
                    ABDK.fromInt(1) +
                    ABDK.div(ABDK.mul(ABDK.fromInt(maxSurge), 7), acc);
            } else {
                surge = ABDK.fromInt(1);
            }
        } else {
            return (0, 0, 0, 0);
        }
    }

    function _getReal(HistoryArray storage self)
        internal
        view
        returns (
            int s1,
            int128 v1,
            int128 v2,
            int128 acc
        )
    {
        uint length = self.values.length;
        int s0 = self.values[(2 * length + self.index - 3) % length];
        int t0 = int(self.timestamps[(2 * length + self.index - 3) % length]);

        s1 = self.values[(length + self.index - 2) % length];
        int t1 = int(self.timestamps[(length + self.index - 2) % length]);

        int s2 = self.values[(length + self.index - 1) % length];
        int t2 = int(self.timestamps[(length + self.index - 1) % length]);

        v1 = ABDK.divi(s1 - s0, t1 - t0);
        v2 = ABDK.divi(s2 - s1, t2 - t1);

        acc = ABDK.div(ABDK.sub(v2, v1), ABDK.fromInt(t2 - t1));
    }

    function _getNormal(
        int s1,
        int128 v1,
        int128 v2,
        int128 acc,
        int32 normalPeriod
    ) internal pure returns (int128 velNorm, int128 accNorm) {
        if (v2 > 0) {
            velNorm = ABDK.div(
                ABDK.mul(v2, ABDK.fromInt(normalPeriod)),
                ABDK.fromInt(s1)
            );
            velNorm = ABDK.sub(ABDK.fromInt(1), velNorm);
            if (velNorm < 0) {
                velNorm = 0;
            }
        } else if (v2 < 0) {
            velNorm = ABDK.div(
                ABDK.mul(-v2, ABDK.fromInt(normalPeriod)),
                ABDK.fromInt(s1)
            );
            velNorm = ABDK.fromInt(1) + ABDK.mul(velNorm, ABDK.fromInt(7));
            if (ABDK.toInt(velNorm) >= 8) {
                velNorm = ABDK.fromInt(8);
            }
        } else {
            velNorm = ABDK.fromInt(1);
        }

        /*
        velocity is calculate on avg 
        0 means -100% change
        1 means 0% change
        8 mean +100% change
        Acc ~ 1 
        Price go up quickly - > acc 
      */
        if (acc > 0) {
            accNorm = ABDK.div(ABDK.mul(acc, ABDK.fromInt(normalPeriod)), v1);
            accNorm = ABDK.sub(ABDK.fromInt(1), accNorm);
            if (accNorm < 0) {
                accNorm = 0;
            }
        } else if (acc < 0) {
            accNorm = ABDK.div(ABDK.mul(-acc, ABDK.fromInt(normalPeriod)), v1);
            accNorm = ABDK.fromInt(1) + ABDK.mul(accNorm, ABDK.fromInt(7));
            if (ABDK.toInt(accNorm) >= 8) {
                accNorm = ABDK.fromInt(8);
            }
        } else {
            accNorm = ABDK.fromInt(1);
        }
    }

    function getAll(HistoryArray storage self)
        internal
        view
        returns (int[] memory result)
    {
        result = new int[](self.values.length);
        for (uint i = 0; i < self.values.length; ++i) {
            result[i] = self.values[(self.index + i) % self.values.length];
        }
    }

    function getAllVelocity(HistoryArray storage self)
        internal
        view
        returns (int128[] memory result)
    {
        if (self.values.length > 1) {
            result = new int128[](self.values.length - 1);

            for (uint i = 1; i < self.values.length; ++i) {
                int v0 = self.values[(self.index + i - 1) % self.values.length];
                int t0 = int(
                    self.timestamps[(self.index + i - 1) % self.values.length]
                );

                int v1 = self.values[(self.index + i) % self.values.length];
                int t1 = int(
                    self.timestamps[(self.index + i) % self.values.length]
                );

                result[i - 1] = ABDK.divi(v1 - v0, t1 - t0);
            }
        } else {
            result = new int128[](0);
        }
    }

    function getAllAcc(HistoryArray storage self)
        internal
        view
        returns (int128[] memory result)
    {
        if (self.values.length > 2) {
            int128[] memory velocity = getAllVelocity(self);
            result = new int128[](self.values.length - 2);

            for (uint i = 1; i < self.values.length - 1; ++i) {
                int t0 = int(
                    self.timestamps[(self.index + i) % self.values.length]
                );
                int t1 = int(
                    self.timestamps[(self.index + i + 1) % self.values.length]
                );

                result[i - 1] = ABDK.div(
                    velocity[i] - velocity[i - 1],
                    ABDK.fromInt(t1 - t0)
                );
            }
        } else {
            result = new int128[](0);
        }
    }
}

