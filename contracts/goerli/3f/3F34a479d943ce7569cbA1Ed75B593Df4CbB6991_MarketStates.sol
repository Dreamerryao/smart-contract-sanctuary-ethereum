pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

import "../access/BACUpgradable.sol";

import "../interfaces/IProtocolConfig.sol";
import "../interfaces/IMarketStates.sol";
import "../interfaces/IMarket.sol";
import "../interfaces/IRiskCalculation.sol";

import {ABDKMath64x64 as ABDK} from "../math/abdk.sol";
import "../math/HistoryArray.sol";

import "hardhat/console.sol";

// @title Its a smart contract that gets prices from Chainlink oracle and stores them
//        and some calculated indexes day by day for future risk and position calculations.
contract MarketStates is IMarketStates, Initializable, BACUpgradable {
    using HistoryArrayLib for HistoryArray;

    /// @notice protocol configuration
    IProtocolConfig public config;

    /// @notice raw price history for tokens
    mapping(address => HistoryArray) priceHistoryRaw;

    /// @notice avrg price history for tokens
    mapping(address => HistoryArray) priceHistory;

    /// @notice store information day by day (all history or )
    // key - token address
    // value - mapping where key - index of day since the day contract was deployed and value - struct with data
    mapping(address => MarketState) state;

    /// @notice contract start timestamp
    uint start;

    function initialize(address _config) public initializer {
        start = block.timestamp;
        config = IProtocolConfig(_config);

        _setGlobalAccessController(config.getGAC());

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function initializeHistory(
        address token,
        HistoryArray calldata _priceHistoryRaw,
        HistoryArray calldata _priceHistory
    ) external onlyGovernance {
        priceHistoryRaw[token] = _priceHistoryRaw;
        priceHistory[token] = _priceHistory;
    }

    function lastPAP(address token) public view override returns (int128) {
        return state[token].PAP;
    }

    /// @notice get HistoryData structure for current day index
    function getCurrentState(address token)
        public
        view
        override
        returns (MarketState memory data)
    {
        data = state[token];
    }

    /// @notice get the current index
    function getCurrentIndex() public view returns (uint index) {
        return (block.timestamp - start) / 1 days;
    }

    /// @notice get the date index from given timestamp
    function getDateIndex(uint timestamp) public view returns (uint index) {
        return (timestamp - start) / 1 days;
    }

    /// @notice get weighted average price
    function getWeightedAvgPrice(address token)
        public
        view
        override
        returns (int128)
    {
        return ABDK.toInt128(priceHistory[token].getHead(), 8);
    }

    function getPriceVelAcc(address token)
        public
        view
        returns (
            int128 vel,
            int128 acc,
            int128 shock,
            int128 surge
        )
    {
        MarketConfig memory merketConfig = config.getConfig(token);
        return
            priceHistory[token].getHeadVelAccNorm(
                merketConfig.Vel_Max,
                merketConfig.Acc_Max
            );
    }

    /// @notice generate new market state
    function generateState(address token)
        public
        view
        returns (MarketState memory _state)
    {
        // get weighted avarage price
        int128 wprice = getWeightedAvgPrice(token);
        (int128 vel, int128 acc, int128 shock, int128 surge) = getPriceVelAcc(
            token
        );

        _state = MarketState(wprice, vel, acc, shock, surge, 0, 0);
    }

    /// @notice write current data to storage
    function updateStates(address[] memory tokens) external override {
        for (uint i = 0; i < tokens.length; i++) {
            state[tokens[i]] = generateState(tokens[i]);

            IMarket market = IMarket(config.getMarket(tokens[i]));
            IRiskCalculation calc = IRiskCalculation(market.getRiskCalc());
            state[tokens[i]].PAP = calc.PAP(tokens[i]);
            state[tokens[i]].EPS = calc.epsilon(tokens[i]);
            console.log("EPS: %s", uint(int(state[tokens[i]].EPS)));
        }
    }

    /// @notice update price
    function updatePrices(address[] memory tokens) external override {
        for (uint i = 0; i < tokens.length; i++) {
            updateMarketPrice(tokens[i]);
        }
    }

    /// @notice add new price to circle buffer
    function updateMarketPrice(address token) public {
        (int price, uint updatedAt) = IMarket(config.getMarket(token)).price();
        int lastRaw = priceHistoryRaw[token].getLast();
        priceHistoryRaw[token].add(price, uint32(updatedAt));

        int headAvrg = priceHistory[token].getHead();

        int length = int(priceHistoryRaw[token].values.length);
        int newPriceAvrg = headAvrg + price / length - lastRaw / length;

        priceHistory[token].add(newPriceAvrg, uint32(updatedAt));
    }

    function getPriceAverHistoryVelocity(address token)
        public
        view
        returns (int128[] memory)
    {
        return priceHistory[token].getAllVelocity();
    }

    function getPriceAverHistory(address token)
        public
        view
        returns (int[] memory)
    {
        return priceHistory[token].getAll();
    }

    /// @notice min of two numbers 64.64
    function min(int128 x, int128 y) public pure returns (int128) {
        return x > y ? y : x;
    }
}

