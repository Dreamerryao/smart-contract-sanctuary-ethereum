pragma solidity 0.8.17;
import "./AdvertiserProxy.sol";

import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./AdvertiserProxy.sol";

contract ProxyFactory is Ownable {
    mapping(uint32 => address) public proxies;
    address public beacon;
    uint32 public counter;
    address public workerAddress;
    address public keeperAddress;

    constructor(address _beacon, address _admin, address _workerAddress, address _keeperAddress) {
        beacon = _beacon;
        /// @dev transfering ownership to multisig admin upon init
        _transferOwnership(_admin);
        workerAddress = _workerAddress;
        keeperAddress = _keeperAddress;
    }

    event WorkerAddressUpdated(address indexed oldAddress, address newAddress);
    event KeeperAddressUpdated(address indexed oldAddress, address newAddress);
    event DeployedProxy(
        address indexed proxyAddress,
        address admin,
        address advertiserAddress,
        string name,
        address workerAddress,
        address keeperAddress
    );

    modifier onlyOwnerAndWorker() {
        if (msg.sender != owner() && msg.sender != workerAddress) {
            revert Unauthorized();
        }
        _;
    }

    function setWorkerAddress(address _newAddress) external onlyOwner {
        /// @dev allowing admin to set address(0) in case we want to not allow any worker address to update
        emit WorkerAddressUpdated(workerAddress, _newAddress);
        workerAddress = _newAddress;
    }

    function setKeeperAddress(address _newAddress) external onlyOwner {
        /// @dev allowing admin to set address(0) in case we want to not allow any keeper address to update
        emit KeeperAddressUpdated(keeperAddress, _newAddress);
        keeperAddress = _newAddress;
    }

    function deployProxy(
        address _advertiserAddress,
        string memory _advertiserName,
        address[] calldata _paymentTypes
    ) external onlyOwnerAndWorker {
        BeaconProxy proxy = new BeaconProxy(
            address(beacon),
            abi.encodeWithSelector(
                AdvertiserProxy(address(0)).initialize.selector,
                owner(), /// @dev admin
                _advertiserAddress,
                _advertiserName, /// @dev advertister name
                workerAddress,
                keeperAddress,
                _paymentTypes
            )
        );
        emit DeployedProxy(address(proxy), owner(), _advertiserAddress, _advertiserName, workerAddress, keeperAddress);

        counter += 1;
        proxies[counter] = address(proxy);
    }
}

