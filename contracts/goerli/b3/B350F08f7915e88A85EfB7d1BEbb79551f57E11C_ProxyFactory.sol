error ImproperCampaignState();

contract AdvertiserProxy is Initializable, OwnableUpgradeable, AutomationCompatibleInterface {
    address public advertiserAddress;
    address public workerAddress;
    string public advertiserName;

    /// @notice for chainlink automation contract
    address public keeperRegistryAddress;

    mapping(uint32 => Campaign) public campaigns;

    /// @notice we use this for ids & increment. Can also be tracked total # of campaigns / referrers created
    uint32 public campaignCounter;

    struct Campaign {
        uint256 totalBudget;
        CampaignStatus status;
        address paymentType;
        uint256 totalEarned;
        mapping(address => ReferrerDetails) referrers;
    }

    enum CampaignStatus {
        NONE,
        CREATED,
        ACTIVE,
        PAUSED,
        COMPLETED,
        CANCELLED
    }

    struct ReferrerDetails {
        ReferrerStatus status;
        uint256 earned;
        uint256 withdrawn;
    }

    struct ReferrerUpdates {
        address addr;
        uint256 earned;
    }

    /// @dev this struct is used to update both campaigns and referrers
    struct UpdateDetails {
        uint32 campaignId;
        ReferrerUpdates[] referrers;
    }

    enum ReferrerStatus {
        NONE,
        ACTIVE,
        PAUSED
    }

    mapping(address => bool) public paymentTypes;

    function initialize(
        address _admin,
        address _advertiserAddress,
        string calldata _advertiserName,
        address _workerAddress,
        address _keeperRegistryAddress,
        address[] calldata _paymentTypes
    ) public initializer {
        __Ownable_init();

        /// @dev transfering ownership to multisig admin upon init
        _transferOwnership(_admin);

        if (bytes(_advertiserName).length > 10) {
            revert NameTooLong();
        }

        /// @dev add initial data
        advertiserAddress = _advertiserAddress;
        advertiserName = _advertiserName;
        workerAddress = _workerAddress;
        keeperRegistryAddress = _keeperRegistryAddress;

        /// @dev this will be used for native currency such as Eth or Matic
        paymentTypes[address(0)] = true;

        /// @dev we don't expect to have more than 5-10 payment options
        for (uint16 i; i < _paymentTypes.length; i++) {
            paymentTypes[_paymentTypes[i]] = true;
        }
    }

    /// @notice Events

    event KeeperRegistryAddressUpdated(address indexed oldAddress, address newAddress);
    event AdvertiserAddressUpdated(address indexed oldAddress, address newAddress);
    event WorkerAddressUpdated(address indexed oldAddress, address newAddress);
    event CampaignCreated(uint32 indexed id, address paymentType);
    event ReferrerAdded(uint32 indexed campaignId, address addr);
    event ReferrerStatusUpdated(uint32 indexed campaignId, address indexed referrerAddress, ReferrerStatus status);
    event CampaignFunded(uint32 indexed campaignId, uint256 updatedBudget);
    event UpdateCompleted(bytes performData); /// @dev we want to keep track of what data was updated each time
    event CampaignStatusUpdated(uint32 indexed campaignId, CampaignStatus oldState, CampaignStatus newState);
    event WithdrawSuccess(
        uint32 indexed campaignId,
        address indexed referrerAddress,
        address paymentType,
        uint256 amount
    );
    event PaymentAdded(address addr);
    event BalanceWithdrawn(uint32 campaignId, uint256 withdrawAmount, uint256 newTotalBalance);

    /// @notice Modifiers

    modifier onlyOwnerAndWorker() {
        if (msg.sender != owner() && msg.sender != workerAddress) {
            revert Unauthorized();
        }
        _;
    }

    modifier onlyAdvertiser() {
        if (msg.sender != advertiserAddress) {
            revert Unauthorized();
        }
        _;
    }

    modifier onlyKeeper() {
        if (msg.sender != keeperRegistryAddress) {
            revert Unauthorized();
        }
        _;
    }

    modifier onlyInProgressCampaigns(uint32 _campaignId) {
        CampaignStatus campaignStatus = campaigns[_campaignId].status;
        if (campaignStatus == CampaignStatus.ACTIVE || campaignStatus == CampaignStatus.CREATED) {
            _;
        } else {
            revert ImproperCampaignState();
        }
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /// @notice in case worker address needs to be updated
    function setWorkerAddress(address _newAddress) external virtual onlyOwner {
        /// @dev allowing admin to set address(0) in case we want to not allow any worker address to update
        emit WorkerAddressUpdated(workerAddress, _newAddress);
        workerAddress = _newAddress;
    }

    /// @notice in case chainlink keeper address needs to be updated
    function setKeeperRegistryAddress(address _newAddress) external virtual onlyOwner {
        /// @dev allowing admin to set address(0) in case we want to not allow any keeper registry address to update

        emit KeeperRegistryAddressUpdated(keeperRegistryAddress, _newAddress);
        keeperRegistryAddress = _newAddress;
    }

    /// @notice in case advertiser address needs to be updated
    function setAdvertiserAddress(address _newAddress) external virtual onlyOwner {
        if (_newAddress == address(0) || _newAddress == advertiserAddress) {
            revert BadAddress();
        }
        emit AdvertiserAddressUpdated(advertiserAddress, _newAddress);

        advertiserAddress = _newAddress;
    }

    /// @notice create campaign
    function createCampaign(address _paymentType) external virtual onlyOwnerAndWorker {
        /// @dev this can be thought of as campaign id
        campaignCounter += 1;

        /// @dev address(0) is used for native currency such as Eth or Matic
        campaigns[campaignCounter].paymentType = _paymentType;

        /// @dev default values
        campaigns[campaignCounter].status = CampaignStatus.CREATED;

        emit CampaignCreated(campaignCounter, _paymentType);
    }

    /// @notice fund campaign with either erc20 or native currency. if native, we will receive via `msg.value`. if erc20, we use `_amount`
    function fundCampaign(
        uint32 _campaignId,
        uint256 _amount
    ) external payable virtual onlyAdvertiser onlyInProgressCampaigns(_campaignId) {
        address paymentType = campaigns[_campaignId].paymentType;

        /// @dev address(0) is the native crypto like Eth, Matic, etc.
        if (paymentType == address(0)) {
            if (msg.value == 0) {
                revert BadPayment();
            }

            /// @dev this allows advertiser to add to budget multiple times
            campaigns[_campaignId].totalBudget += msg.value;

            emit CampaignFunded(_campaignId, campaigns[_campaignId].totalBudget);

            /// @dev this is for ERC20 payment types
        } else {
            if (_amount == 0) {
                revert BadPayment();
            }
            /// @dev this allows advertiser to add to budget multiple times
            campaigns[_campaignId].totalBudget += _amount;
            bool success = IERC20(paymentType).transferFrom(msg.sender, address(this), _amount);
            require(success, "ERC20_TRANSFER_FAILED");
        }

        emit CampaignFunded(_campaignId, campaigns[_campaignId].totalBudget);
    }

    function addReferrer(
        uint32 _campaignId,
        address _address
    ) external virtual onlyOwnerAndWorker onlyInProgressCampaigns(_campaignId) {
        ReferrerDetails storage referrer = campaigns[_campaignId].referrers[_address];

        if (_address == address(0) || _address == advertiserAddress || !(referrer.status == ReferrerStatus.NONE)) {
            revert BadAddress();
        }

        referrer.status = ReferrerStatus.ACTIVE;

        emit ReferrerAdded(_campaignId, _address);
    }

    function getReferrer(uint32 _campaignId, address _referrerAddress) public view returns (ReferrerDetails memory) {
        return campaigns[_campaignId].referrers[_referrerAddress];
    }

    function withdrawReferrerEarnings(uint32 _campaignId, uint256 _amount) external virtual {
        address paymentType = campaigns[_campaignId].paymentType;
        CampaignStatus status = campaigns[_campaignId].status;

        ReferrerDetails storage referrer = campaigns[_campaignId].referrers[msg.sender];
        if (!(referrer.status == ReferrerStatus.ACTIVE) || _amount == 0) {
            revert BadRequest();
        }

        /// @dev can only withdraw if campaign is active/completed
        if (
            campaigns[_campaignId].status == CampaignStatus.PAUSED
        ) /// @dev can only withdraw if campaign is active/completed/cancelled
        {
            revert Unauthorized();
        }

        uint256 referrerAvailableBalance = referrer.earned - referrer.withdrawn;

        /// @dev cannot withdraw more than difference between earned & already withdrawn
        /// @dev also, in `peformUpkeep` we ensure totalEarned cannot exceed totalBalance so we don't have to worry about withdrawing more than total balance in a campaign here
        if (_amount > referrerAvailableBalance) {
            revert IncorrectAmount();
        }
        // update state before sending money
        referrer.withdrawn += _amount;

        /// @dev address(0) is the native crypto like Eth, Matic, etc.
        if (paymentType == address(0)) {
            (bool success, ) = msg.sender.call{ value: _amount }("");
            require(success, "TRANSFER_FAILED");

            /// @dev this is for ERC20 payment types
        } else {
            bool success = IERC20(paymentType).transfer(address(msg.sender), _amount);
            require(success, "TRANSFER_FAILED");
        }

        emit WithdrawSuccess(_campaignId, msg.sender, paymentType, _amount);
    }

    function setReferrerStatus(
        uint32 _campaignId,
        address _referrerAddress,
        ReferrerStatus _status
    ) external virtual onlyOwnerAndWorker {
        ReferrerDetails memory referrer = getReferrer(_campaignId, _referrerAddress);
        if (referrer.status == _status) {
            revert BadRequest();
        }

        campaigns[_campaignId].referrers[_referrerAddress].status = _status;
        emit ReferrerStatusUpdated(_campaignId, _referrerAddress, _status);
    }

    /// @dev method here in case we use chainlink keepers
    function checkUpkeep(
        bytes calldata /* checkData */
    ) external view virtual override onlyKeeper returns (bool upkeepNeeded, bytes memory /* performData */) {
        upkeepNeeded = true;
    }

    /// @notice this to update both campaigns and referrers
    /// @dev since we are updating only 2 numbers for both campaigns and referrers, we can reuse the same UpdateDetails struct
    function performUpkeep(bytes calldata performData) external virtual override onlyKeeper {
        UpdateDetails[] memory updateArray = abi.decode(performData, (UpdateDetails[]));

        for (uint32 i = 0; i < updateArray.length; ) {
            UpdateDetails memory updateObj = updateArray[i];

            Campaign storage campaign = campaigns[updateObj.campaignId];

            if (campaign.status == CampaignStatus.NONE || campaign.status == CampaignStatus.CREATED) {
                revert ImproperCampaignState();
            }

            ReferrerUpdates[] memory referrers = updateArray[i].referrers;

            for (uint32 k = 0; k < referrers.length; ) {
                ReferrerUpdates memory referrerUpdate = referrers[k];
                ReferrerDetails storage referrer = campaigns[updateObj.campaignId].referrers[referrerUpdate.addr];

                if (referrer.status == ReferrerStatus.NONE) {
                    revert ReferrerNonExistent();
                }

                /// @dev you cannot make earned less than already withdrawn
                if (referrer.withdrawn > referrerUpdate.earned) {
                    revert IncorrectAmount();
                }

                /// @dev we are adding the difference to totalEarned. it should never be negative.
                campaign.totalEarned = campaign.totalEarned - referrer.earned + referrerUpdate.earned;

                /// @dev preventing over-attribution
                if (campaign.totalEarned > campaign.totalBudget) {
                    revert OverAttributed();
                }

                /// @dev for update referrer
                referrer.earned = referrerUpdate.earned;

                unchecked {
                    k++;
                }
            }

            /// @dev for gas saving since we know that there is no way array will be bigger than uint32
            unchecked {
                i++;
            }
