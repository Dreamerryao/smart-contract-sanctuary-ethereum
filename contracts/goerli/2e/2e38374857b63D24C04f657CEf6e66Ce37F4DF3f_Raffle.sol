pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "hardhat/console.sol";

error Raffle__IncorrectAmountSent();
error Raffle__RaffleNotOpen();
error Raffle__TransferFailed();
error Raffle__EntrantMustNotBeAContract();

// enums
enum RaffleStatus {
    Open,
    Drawing,
    Closed
}

// structs
struct RaffleDetails {
    RaffleStatus status;
    uint256 startTime;
    uint256 endTime;
    uint256 ticketPrice;
    uint256 totalTickets;
    uint256 totalTicketsSold;
    uint256 numberOfWinners;
    uint256 entrantTickets;
}

struct RaffleEntry {
    address payable entrant; // player address
    uint256 lastTicketNumber; // last ticket number boaught in the raffle
}

struct RequestStatus {
    bool fulfilled; // whether the request has been successfully fulfilled
    bool exists; // whether a requestId exists
    uint256 randomWord;
}

struct Winner {
    uint256 ticketNumber;
    uint256 entrantNumber;
    RaffleEntry raffleEntry;
}

contract Raffle is Ownable, VRFConsumerBaseV2 {
    // constants
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint16 private constant NUM_WORDS = 1;
    
    // immutable
    VRFCoordinatorV2Interface public immutable i_vrfCoordinator;
    uint256 private immutable i_startTime;
    uint256 private immutable i_endTime;
    uint256 private immutable i_ticketPrice;
    uint256 private immutable i_totalTickets;
    uint256 private immutable i_numberOfWinners;
    
    // storage
    RaffleStatus private s_raffleStatus;
    RaffleEntry[] public s_raffleEntries;
    uint256 private s_lastTicketNumber;
    mapping(address => uint256) private s_entrantTotalTickets;
    Winner public s_winner;
    mapping(uint256 => RequestStatus) public s_requests;
    uint256 public s_lastRequestId;
    
    // events
    event RaffleTicketsBoaught(uint256 totalTickets, uint256 lastTicketNumber, address indexed player);
    event RaffleMoneyCollected(address indexed owner, uint256 balance);
    event RequestSent(uint256 requestId, uint32 numWords);
    event RequestFulfilled(uint256 requestId, uint256[] randomWords);
    event WinnerDrawn(Winner indexed winner);
    
    constructor(
        address _vrfCoordinatorV2,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _ticketPrice,
        uint256 _totalTickets,
        uint256 _numberOfWinners
    ) VRFConsumerBaseV2(_vrfCoordinatorV2) {
        i_vrfCoordinator = VRFCoordinatorV2Interface(
            _vrfCoordinatorV2
        );
        
        i_startTime = _startTime;
        i_endTime = _endTime;
        i_ticketPrice = _ticketPrice;
        i_totalTickets = _totalTickets;
        i_numberOfWinners = _numberOfWinners;
    }
    
    function enterRaffle(
        uint256 _numberOfTickets
    ) 
        external payable
    {
        if (Address.isContract(msg.sender)) {
            revert Raffle__EntrantMustNotBeAContract();
        }
        
        if (s_raffleStatus != RaffleStatus.Open) {
            revert Raffle__RaffleNotOpen();
        }
        
        // require(msg.value >= i_entranceFee, "Not enough money sent!");
        if (msg.value != (i_ticketPrice * _numberOfTickets)) {
            revert Raffle__IncorrectAmountSent();
        }
        
        RaffleEntry memory raffleEntry = RaffleEntry({
            entrant: payable(msg.sender),
            lastTicketNumber: s_lastTicketNumber + _numberOfTickets
        });
        
        s_lastTicketNumber = raffleEntry.lastTicketNumber;
        s_raffleEntries.push(raffleEntry);
        s_entrantTotalTickets[msg.sender] += _numberOfTickets;
        
        console.log(
            "Transferring %s from %s, total tickets boaught: %s",
            msg.value,
            msg.sender,
            raffleEntry.lastTicketNumber
        );

        emit RaffleTicketsBoaught(_numberOfTickets, raffleEntry.lastTicketNumber, msg.sender);
    }
    
    function drawWinner(
        uint64 _subscriptionId,
        bytes32 _gasLane,
        uint32 _gasLimit
    )
        external
        onlyOwner
        returns (uint256)
    {
//        require(s_raffleStatus == RaffleStatus.Open, "request not found");
        
        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            _gasLane,
            _subscriptionId,
            REQUEST_CONFIRMATIONS,
            _gasLimit,
            NUM_WORDS
        );
        
        s_requests[requestId] = RequestStatus({
            exists: true,
            fulfilled: false,
            randomWord: 0
        });
        
        s_lastRequestId = requestId;
        
        emit RequestSent(requestId, NUM_WORDS);
        
        s_raffleStatus = RaffleStatus.Drawing;
        
        return requestId;
    }
    
    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    )
        internal override
    {
        require(s_requests[_requestId].exists, "request not found");
        
        console.log(
            "Fulfill random words: %s",
            _requestId
        );
        
        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWord = _randomWords[0];
        
        emit RequestFulfilled(_requestId, _randomWords);

        uint256 randomNumber = (_randomWords[0] % s_lastTicketNumber) + 1;
        uint256 position = findUpperBound(s_raffleEntries, randomNumber);
        
        RaffleEntry memory winningEntry = s_raffleEntries[position];
        
        Winner memory winner = Winner({
            ticketNumber: randomNumber,
            entrantNumber: position,
            raffleEntry: winningEntry
        });
        
        (bool success, ) = winningEntry.entrant.call{value: address(this).balance}("");
        if (!success) {
            revert Raffle__TransferFailed();
        }
        
        s_winner = winner;
        s_raffleStatus = RaffleStatus.Closed;
        
        emit WinnerDrawn(winner);
    }
    
    /**
     * @dev Searches a sorted `array` and returns the first index that contains
     * a value greater or equal to `element`. If no such index exists (i.e. all
     * values in the array are strictly less than `element`), the array length is
     * returned. Time complexity O(log n).
     *
     * `array` is expected to be sorted in ascending order, and to contain no
     * repeated elements.
     */
    function findUpperBound(
        RaffleEntry[] storage _array,
        uint256 _element
    ) 
        internal view
        returns (uint256)
    {
        if (_array.length == 0) {
            return 0;
        }

        uint256 low = 0;
        uint256 high = _array.length;

        while (low < high) {
            uint256 mid = Math.average(low, high);
    
            // Note that mid will always be strictly less than high (i.e. it will be a valid array index)
            // because Math.average rounds down (it does integer division with truncation).
            if (_array[mid].lastTicketNumber > _element) {
                high = mid;
            } else {
                low = mid + 1;
            }
        }
    
        // At this point `low` is the exclusive upper bound. We will return the inclusive upper bound.
        if (low > 0 && _array[low - 1].lastTicketNumber == _element) {
            return low - 1;
        } else {
            return low;
        }
    }
    
    function collect() 
        external
        onlyOwner
        returns (bool)
    {
        uint256 balance = address(this).balance;
        (bool sent,) = owner().call{value: balance}("");
        
        emit RaffleMoneyCollected(msg.sender, balance);
        
        return sent;
    }
    
    /**
     *  get the details of the raffle
     */
    function getDetails()
        external view
        returns (RaffleDetails memory)
    {
        return RaffleDetails({
            status: s_raffleStatus,
            startTime: i_startTime,
            endTime: i_endTime,
            ticketPrice: i_ticketPrice,
            totalTickets: i_totalTickets,
            totalTicketsSold: s_lastTicketNumber,
            numberOfWinners: i_numberOfWinners,
            entrantTickets: s_entrantTotalTickets[msg.sender]
        });
    }
    
    function isOwner()
        external view
        returns (bool)
    {
        return owner() == msg.sender;
    }
}

