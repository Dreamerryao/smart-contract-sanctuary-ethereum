contract NFTStaking is Ownable, IERC721Receiver {
    using SafeERC20 for IERC20; // Wrappers around ERC20 operations that throw on failure
    uint256 public totalStaked;

    // struct to store a stake's token, owner, and earning values
    struct Stake {
        uint24 tokenId;
        uint256 timestamp;
        address owner;
        uint256 rewards;
    }

    struct CarbonVault {
        uint256 carbonToken;
        uint256 amount;
    }

    uint256 private constant TOKEN_SHARE_PRECISION = 1e12;
    uint256 private rewardTokensPerBlock = 10000;

    event NFTStaked(address owner, uint256 tokenId, uint256 value);
    event NFTUnstaked(address owner, uint256 tokenId, uint256 value);
    event Claimed(address owner, uint256 amount);

    // reference to the Block NFT contract
    GreenTreeCollection nft;
    GTTRewards token;
    GreenTreeRanking ranking;

    // maps tokenId to stake
    mapping(uint256 => Stake) public vault;
    // pourcent fees on claim to buy back carbon
    uint256 private constant FEES_ON_CLAIM = 500; // divide by 100 cause no float in solidity 500BasePoint

    // array of carbon vault
    mapping(uint256 => CarbonVault) public carbonVault;

    constructor(
        GreenTreeCollection _nft,
        GTTRewards _token,
        GreenTreeRanking _ranking
    ) {
        nft = _nft;
        token = _token;
        ranking = _ranking;
        carbonVault[1] = CarbonVault({carbonToken: 1, amount: 0});
    }

    function setRankingContract(GreenTreeRanking _ranking) external {
        ranking = _ranking;
    }

    function getRankingContract() external view returns (GreenTreeRanking) {
        return ranking;
    }

    function setTokenIdCarbon(uint256 tokenId, uint256 amount) external {
        ranking.updateCarbonTons(tokenId, amount);
    }

    function getTokenIdRank(uint256 tokenId) external view returns (uint256) {
        return ranking.getTreeRank(tokenId);
    }

    function getTotalCarbon() external view returns (uint256) {
        return ranking.getTotalCarbonStaked();
    }

    function stake(uint256[] calldata tokenIds) external {
        uint256 tokenId;
        totalStaked += tokenIds.length;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            require(nft.ownerOf(tokenId) == msg.sender, "not your token");
            require(vault[tokenId].tokenId == 0, "already staked");

            nft.transferFrom(msg.sender, address(this), tokenId);
            emit NFTStaked(msg.sender, tokenId, block.timestamp);

            uint256 carbonTon = nft.carbonTons(tokenId);
            ranking.addTree(tokenId, carbonTon); // replace 0 with the right amount from nft collection

            vault[tokenId] = Stake({
                owner: msg.sender,
                tokenId: uint24(tokenId),
                timestamp: uint48(block.timestamp),
                rewards: 0
            });
        }
    }

    function _unstakeMany(address account, uint256[] calldata tokenIds)
        internal
    {
        uint256 tokenId;
        totalStaked -= tokenIds.length;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            Stake memory staked = vault[tokenId];
            require(staked.owner == msg.sender, "not an owner");

            delete vault[tokenId];
            emit NFTUnstaked(account, tokenId, block.timestamp);
            nft.transferFrom(address(this), account, tokenId);
            ranking.removeTree(tokenId);
        }
    }

    function claim(uint256[] calldata tokenIds) external {
        _claim(msg.sender, tokenIds, false);
    }

    function claimForAddress(address account, uint256[] calldata tokenIds)
        external
    {
        _claim(account, tokenIds, false);
    }

    function unstake(uint256[] calldata tokenIds) external {
        _claim(msg.sender, tokenIds, true);
    }

    function _claim(
        address account,
        uint256[] calldata tokenIds,
        bool _unstake
    ) internal {
        uint256 tokenId;
        uint256 earned = 0;
        uint256 totalFees = 0;

        updateTokensRewards();

        for (uint256 i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            earned = earned + vault[tokenId].rewards;

            uint256 fees = (vault[tokenId].rewards * FEES_ON_CLAIM) / 10000; // fees = 0 car 5/100 = 0 en solidity
            totalFees = totalFees + fees;

            vault[tokenId].rewards = 0;

            nft.addCarbonTon(tokenId, fees);
            ranking.updateCarbonTons(tokenId, fees);
        }

        if (earned > 0) {
            earned = earned / 10;

            token.mint(account, earned);
        }

        // Add SWAP here
        earned = earned - totalFees;
        // Increase Global Carbon Vault with totalFees
        carbonVault[1].amount = carbonVault[1].amount + totalFees;

        if (_unstake) {
            _unstakeMany(account, tokenIds);
        }
        emit Claimed(account, earned);
    }

    function updateTokensRewards() private {
        for (uint256 i = 1; i <= totalStaked; i++) {
            Stake storage staked = vault[i];
            if (staked.tokenId == 0) return;
            uint256 tokenShare = ((ranking.getTokenCarbon(staked.tokenId) +
                1 *
                TOKEN_SHARE_PRECISION) /
                totalStaked +
                1); // PB: Au début aucun token n'a cumulé de carbon donc 0 et jamais de rewards
            uint256 timeSinceLastReward = block.timestamp - staked.timestamp;
            uint256 rewards = ((timeSinceLastReward *
                rewardTokensPerBlock *
                tokenShare) / TOKEN_SHARE_PRECISION) / 1 days;
            console.log("TokenShare = %s", tokenShare / TOKEN_SHARE_PRECISION); // Token Share OK
            console.log("TokenRewards = %s", rewards); // Rewards OK
            staked.timestamp = block.timestamp;
            staked.rewards = staked.rewards + rewards;
            console.log(
                "[NFT Contract] - Update(%s) : %s",
                staked.tokenId,
                staked.rewards
            );
        }
    }

    function pourcentOfTotal(uint256 tokenId) external view returns (uint256) {
        uint256 totalCarbon = ranking.getTotalCarbonStaked();
        uint256 tokenCarbon = ranking.getTokenCarbon(tokenId); // Attention tokenId owner = msg.sender ?
        return (tokenCarbon / totalCarbon) * 100;
    }

    // PROBLEM on Goerli execution reverted
    function earningInfo(uint256[] calldata tokenIds)
        external
        view
        returns (uint256)
    {
        uint256 totalScore = 0;
        uint256 earned = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            Stake memory staked = vault[tokenId];
            uint256 stakedAt = staked.timestamp;
            uint256 totalCarbon = ranking.getTotalCarbonStaked();
            console.log("[Contract] - Total Carbon: ", totalCarbon);
            uint256 tokenCarbon = ranking.getTokenCarbon(tokenId); // Attention tokenId owner = msg.sender ?
            console.log("[Contract] - Token Carbon: ", tokenCarbon);
            uint256 pourcentOfTotal = (tokenCarbon * 100) / totalCarbon;
            console.log("[Contract] - PourcentOfTotal: ", pourcentOfTotal);

            // rewardPerSecond = 10000 / 24*60*60
            // duration = now - stakedAt
            // ratePerDay = (carbonShare/100) * totalPerDay
            // earned += duration * ratePerDay
            uint256 ratePerDay = (pourcentOfTotal / 100) * 10000 ether;
            console.log("[Contract - RatePerDay: ", ratePerDay);

            earned += ratePerDay * ((block.timestamp - stakedAt) / 1 days);
            console.log("[Contract] - Earned: ", uint256(earned));
        }

        return earned;
    }

    // should never be used inside of transaction because of gas fee
    function balanceOf(address account) public view returns (uint256) {
        uint256 balance = 0;
        uint256 supply = nft.totalSupply();
        for (uint256 i = 1; i <= supply; i++) {
            if (vault[i].owner == account) {
                balance += 1;
            }
        }
        return balance;
    }

    // should never be used inside of transaction because of gas fee
    function tokensOfOwner(address account)
        public
        view
        returns (uint256[] memory ownerTokens)
    {
        uint256 supply = nft.totalSupply();
        uint256[] memory tmp = new uint256[](supply);

        uint256 index = 0;
        for (uint256 tokenId = 1; tokenId <= supply; tokenId++) {
            if (vault[tokenId].owner == account) {
                tmp[index] = vault[tokenId].tokenId;
                index += 1;
            }
        }

        uint256[] memory tokens = new uint256[](index);
        for (uint256 i = 0; i < index; i++) {
            tokens[i] = tmp[i];
        }

        return tokens;
    }

    function onERC721Received(
        address,
        address from,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        require(from == address(0x0), "Cannot send nfts to Vault directly");
        return IERC721Receiver.onERC721Received.selector;
    }
}

