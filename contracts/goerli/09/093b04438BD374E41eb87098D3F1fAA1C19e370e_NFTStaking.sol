pragma solidity ^0.8.9;

import "hardhat/console.sol";

contract GreenTreeRanking {
    struct Tree {
        uint256 tokenId;
        uint256 carbonTons;
    }

    mapping(uint256 => Tree) public trees;
    mapping(uint256 => uint256) _nextTrees;
    uint256 public listSize;
    uint256 constant MAX_INT = 2**256 - 1;
    uint256 constant GUARD = MAX_INT;

    constructor() {
        _nextTrees[GUARD] = GUARD;
    }

    function _verifyIndex(
        uint256 prevTree_,
        uint256 carbonTons_,
        uint256 nextTree_
    ) internal view returns (bool) {
        return
            (prevTree_ == GUARD ||
                trees[prevTree_].carbonTons >= carbonTons_) &&
            (nextTree_ == GUARD || carbonTons_ > trees[nextTree_].carbonTons);
    }

    function _findIndex(uint256 carbonTons_) internal view returns (uint256) {
        uint256 candidateAddress = GUARD;
        while (true) {
            if (
                _verifyIndex(
                    candidateAddress,
                    carbonTons_,
                    _nextTrees[candidateAddress]
                )
            ) return candidateAddress;
            candidateAddress = _nextTrees[candidateAddress];
        }
    }

    function _isPrevTree(uint256 tokenId, uint256 prevTree_)
        internal
        view
        returns (bool)
    {
        return _nextTrees[prevTree_] == tokenId;
    }

    function _findPrevTree(uint256 tokenId) internal view returns (uint256) {
        uint256 currentAddress = GUARD;
        while (_nextTrees[currentAddress] != GUARD) {
            if (_isPrevTree(tokenId, currentAddress)) return currentAddress;
            currentAddress = _nextTrees[currentAddress];
        }
        return 0;
    }

    function addTree(uint256 tokenId, uint256 carbonTons_) public {
        require(_nextTrees[tokenId] == 0);
        uint256 index = _findIndex(carbonTons_);
        console.log(_nextTrees[0]);
        trees[tokenId].carbonTons = carbonTons_;
        _nextTrees[tokenId] = _nextTrees[index];
        _nextTrees[index] = tokenId;
        listSize++;
    }

    function updateCarbonTons(uint256 tokenId, uint256 carbonTons_) public {
        require(_nextTrees[tokenId] != 0);
        uint256 prevTree = _findPrevTree(tokenId);
        uint256 nextTree = _nextTrees[tokenId];
        uint256 increasedCarbonTons = trees[tokenId].carbonTons + carbonTons_;
        if (_verifyIndex(prevTree, carbonTons_, nextTree)) {
            trees[tokenId].carbonTons = increasedCarbonTons;
        } else {
            removeTree(tokenId);
            addTree(tokenId, increasedCarbonTons);
        }
    }

    function increaseCarbonTons(uint256 tokenId, uint256 carbonTons_) public {
        updateCarbonTons(tokenId, carbonTons_);
    }

    function removeTree(uint256 tokenId) public {
        require(_nextTrees[tokenId] != 0);
        uint256 prevTree = _findPrevTree(tokenId);
        _nextTrees[prevTree] = _nextTrees[tokenId];
        delete _nextTrees[tokenId];
        listSize--;
    }

    function getTop(uint256 k_) public view returns (uint256[] memory) {
        require(k_ <= listSize);
        uint256[] memory treesList = new uint256[](k_);
        uint256 currentAddress = _nextTrees[GUARD];
        for (uint256 i = 0; i < k_; i++) {
            treesList[i] = currentAddress;
            currentAddress = _nextTrees[currentAddress];
        }
        return treesList;
    }

    function getTreeRank(uint256 tokenId) public view returns (uint256) {
        require(_nextTrees[tokenId] != 0);
        uint256 currentAddress = _nextTrees[GUARD];
        uint256 rank = 1;

        if (currentAddress == tokenId) return rank;

        while (currentAddress != tokenId) {
            rank++;
            currentAddress = _nextTrees[currentAddress];
        }

        return rank;
    }

    function getTokenCarbon(uint256 tokenId) external view returns (uint256) {
        uint256 tokenCarbon = trees[tokenId].carbonTons;

        console.log(
            "[Ranking Contract] - tokenId(%s) carbon: %s",
            tokenId,
            tokenCarbon
        );

        return tokenCarbon;
    }

    function getTotalCarbonStaked() external view returns (uint256) {
        uint256 totalCarbon = 0;
        uint256 currentAddress = _nextTrees[GUARD];

        while (currentAddress != GUARD) {
            totalCarbon += trees[currentAddress].carbonTons;
            currentAddress = _nextTrees[currentAddress];
        }

        return totalCarbon;
    }
}

