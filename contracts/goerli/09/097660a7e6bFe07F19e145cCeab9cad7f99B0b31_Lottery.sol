pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/PullPayment.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

error InvalidState();
error InvalidVrfRequestResponse();
error InvalidTicket();
error PlayerAlreadyEntered();
error MaximumPlayersReached();
error FailedPayout(address address_);
error InvalidAddress(address address_);

/**
 * @title The RandomNumberConsumerV2 contract
 * @notice A contract that gets random values from Chainlink VRF V2
 */
contract Lottery is
  Ownable,
  Pausable,
  ReentrancyGuard,
  PullPayment,
  VRFConsumerBaseV2,
  KeeperCompatibleInterface
{
  enum LotteryState {
    Opened,
    Draw,
    ReadyForPayouts,
    Closed
  }
  VRFCoordinatorV2Interface public immutable vrfCoordinatorV2Interface;
  LotteryState public lotteryState = LotteryState.Closed;

  uint16 public immutable vrfRequestConfirmations = 3;
  uint16 public maxPlayers;
  uint32 public immutable vrfCoordinatorCallbackGasLimit;
  uint32 public immutable vrfRequestRandomNumbersCount = 1;
  uint64 public vrfSubscriptionId;
  uint256 public maxDuration;
  uint256 public vrfRequestId;
  uint256 public ticketPrice;
  uint256 public entryFee;
  uint256 public jackpot;
  uint256 public winnerCut;
  uint256 public vrfRandomNumber;
  uint256 private startedAt;
  address payable public winner;
  address payable public entryFeeDestination;
  address payable public charityFeeDestination;
  address payable[] public players;
  bytes32 public immutable vrfKeyHash;
  mapping(address => bool) public playerHasEntered;

  event ReturnedRandomness(uint256[] randomWords);
  event LotteryOpened(
    uint256 ticketPrice,
    uint256 entryFee,
    uint256 winnerCut,
    uint16 maxPlayers,
    address entryFeeDestination,
    address charityFeeDestination
  );

  /**
   * @notice Constructor inherits VRFConsumerBaseV2
   *
   * @param vrfSubscriptionId_ - the subscription ID that this contract uses for funding requests
   * @param vrfCoordinatorAddress_ - coordinator, check https://docs.chain.link/docs/vrf-contracts/#configurations
   * @param vrfKeyHash_ - the gas lane to use, which specifies the maximum gas price to bump to
   */
  constructor(
    uint64 vrfSubscriptionId_,
    address vrfCoordinatorAddress_,
    uint32 vrfCoordinatorCallbackGasLimit_,
    bytes32 vrfKeyHash_
  ) VRFConsumerBaseV2(vrfCoordinatorAddress_) {
    vrfCoordinatorV2Interface = VRFCoordinatorV2Interface(vrfCoordinatorAddress_);
    vrfKeyHash = vrfKeyHash_;
    vrfSubscriptionId = vrfSubscriptionId_;
    vrfCoordinatorCallbackGasLimit = vrfCoordinatorCallbackGasLimit_;
  }

  function close() external nonReentrant whenNotPaused onlyOwner {
    if (lotteryState == LotteryState.Closed) revert InvalidState();
    lotteryState = LotteryState.Closed;
  }

  function open(
    uint256 ticketPrice_,
    uint256 entryFee_,
    uint256 winnerCut_,
    uint16 maxPlayers_,
    uint256 maxDuration_,
    address entryFeeDestination_,
    address charityFeeDestination_
  ) external nonReentrant whenNotPaused onlyOwner {
    if (lotteryState != LotteryState.Closed) revert InvalidState();
    if (entryFeeDestination_ == address(0)) revert InvalidAddress(entryFeeDestination_);
    if (charityFeeDestination_ == address(0)) revert InvalidAddress(entryFeeDestination_);

    lotteryState = LotteryState.Opened;
    ticketPrice = ticketPrice_;
    entryFee = entryFee_;
    entryFeeDestination = payable(entryFeeDestination_);
    charityFeeDestination = payable(charityFeeDestination_);
    winnerCut = winnerCut_;
    maxPlayers = maxPlayers_;
    maxDuration = maxDuration_;
    startedAt = block.timestamp;
    players = new address payable[](0);

    emit LotteryOpened(
      ticketPrice,
      entryFee,
      winnerCut,
      maxPlayers,
      entryFeeDestination,
      charityFeeDestination
    );
  }

  receive() external payable nonReentrant whenNotPaused {
    if (msg.sender == address(0)) revert InvalidAddress(msg.sender);
    if (lotteryState != LotteryState.Opened) revert InvalidState();
    if (msg.value != ticketPrice) revert InvalidTicket();
    if (playerHasEntered[msg.sender]) revert PlayerAlreadyEntered();
    if (players.length + 1 > maxPlayers) revert MaximumPlayersReached();

    players.push(payable(msg.sender));
    playerHasEntered[msg.sender] = true;
    jackpot += msg.value - entryFee;
  }

  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    if (lotteryState != LotteryState.Draw) revert InvalidState();
    if (vrfRequestId != requestId) revert InvalidVrfRequestResponse();
    if (randomWords.length < 1) revert InvalidVrfRequestResponse();

    vrfRandomNumber = randomWords[0];
    console.log("Received random number: %s", vrfRandomNumber);
    winner = players[vrfRandomNumber % players.length];

    console.log("Winner is: %s", winner);
    lotteryState = LotteryState.ReadyForPayouts;
  }

  function unsafeInc(uint256 index) private pure returns (uint256) {
    unchecked {
      return index + 1;
    }
  }

  function _isReadyForDraw() private view returns (bool) {
    bool isOpen = (lotteryState == LotteryState.Opened);
    bool areEnoughPlayers = (players.length >= 1);
    bool maxPlayersReached = (players.length == maxPlayers);

   return (isOpen && (maxPlayersReached || areEnoughPlayers));
  }

  function _isReadyForPayouts() private view returns (bool) {
   return (lotteryState == LotteryState.ReadyForPayouts);
  }

  function draw() external nonReentrant whenNotPaused {
    if (!(_isReadyForDraw())) revert InvalidState();
    lotteryState = LotteryState.Draw;
    vrfRequestId = vrfCoordinatorV2Interface.requestRandomWords(
      vrfKeyHash,
      vrfSubscriptionId,
      vrfRequestConfirmations,
      vrfCoordinatorCallbackGasLimit,
      vrfRequestRandomNumbersCount
    );
  }

  function checkUpkeep(bytes calldata /* checkData */)
    external
    view
    override
    returns (bool upkeepNeeded, bytes memory /* performData */)
  {
    upkeepNeeded = _isReadyForDraw() || _isReadyForPayouts();
  }

  function payouts() external nonReentrant whenNotPaused {
    if (!(_isReadyForPayouts())) revert InvalidState();
    lotteryState = LotteryState.Closed;

    address payable[] memory players_ = players;
    uint winnerPayout = jackpot * winnerCut / 100;
    playerHasEntered[winner] = false;
    console.log("Winner payout: %s", winnerPayout);

    uint leftOverPayout = (jackpot - winnerPayout) / players_.length;
    console.log("Left over payout: %s", leftOverPayout);

    for (uint index=0; index < players_.length; index=unsafeInc(index)) {
      if (players_[index] == winner) continue;
      console.log("Sending left over payout to %s: %s", players_[index], leftOverPayout);
      playerHasEntered[players_[index]] = false;
      _asyncTransfer(players_[index], leftOverPayout);
    }
    
    _asyncTransfer(winner, winnerPayout);
  }

  function performUpkeep(bytes calldata /* performData */) external override nonReentrant whenNotPaused {
    if (!(_isReadyForDraw() || _isReadyForPayouts())) revert InvalidState();

    if (_isReadyForDraw()) this.draw();
    if (_isReadyForPayouts()) this.payouts();

  }
}

