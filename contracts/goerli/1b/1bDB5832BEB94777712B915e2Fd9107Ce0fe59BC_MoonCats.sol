pragma solidity ^0.8.16;

// To test in testnets
contract MoonCats {
    address maker;
    constructor() {
        maker = msg.sender;
    }

    function ownerOf(uint256 tokenId) external view returns (address) {
        return tokenId % 5 == 0 ? maker : address(this);
    }

}