contract DevPoCTicket is PoCTicket {
    constructor(
        address admin,
        string memory baseTokenURI,
        PROOFTokens memory tokens,
        IDelegationRegistry delegationRegistry,
        address payable primarySalesReceiver,
        address bnSigner
    )
        PoCTicket(
            admin,
            PoCTicket.ERC721MetadataSetup({
                name: "DevPoCTicket",
                symbol: "DevPoCT",
                baseTokenURI: baseTokenURI
            }),
            tokens,
            delegationRegistry,
            primarySalesReceiver,
            bnSigner
        )
    {
        _grantRole(DEFAULT_STEERING_ROLE, address(this));
    }

    function devmint(uint256 num) public {
        devmint(msg.sender, num);
    }

    function devmint(address to, uint256 num) public {
        _mintPurchase(to, num);
    }

    function devSetStage(Stage stage_) public {
        stage = stage_;
    }

    function devSetPrices(
        uint128 reducedTicketPrice_,
        uint128 fullTicketPrice_,
        uint128 upgradePrice_
    ) public {
        this.setPurchasePrices(reducedTicketPrice_, fullTicketPrice_);
        this.setUpgradePrice(upgradePrice_);
    }
}

