        return IERC20(_tokenAddress).balanceOf(address(this));
    }

    function withdrawToken(address _tokenAddress, uint256 _amount) external onlyOwner{
        IERC20(_tokenAddress).transfer(msg.sender, _amount);
    }

    function changeUpkeepAddress(address _chainlinkUpkeepAddress) external onlyOwner{
        chainlinkUpkeepAddress = _chainlinkUpkeepAddress;
    }
    
    // Neeed to implement transfer and call to fund the upkeep with LINK
    function createUpkeep(string memory _cronString) public{
        address _target = address(this);
        // bytes memory functionToCall = "0x37ec1881";
        bytes4 encodedSignature = bytes4(keccak256("sendSalary(string)"));
        bytes memory functionToCall = bytes(abi.encodePacked(encodedSignature, _cronString));
        // bytes memory encodedCron = cronFactory.encodeCronString(_cronString);
        bytes memory job = cronFactory.encodeCronJob(_target, functionToCall,  _cronString);
        uint256 maxJobs = cronFactory.s_maxJobs();
        address delegateAddress = cronFactory.cronDelegateAddress();
        address newCronUpkeep = address(new CronUpkeep(msg.sender, delegateAddress, maxJobs, job));
        allUpkeeps.push(newCronUpkeep);
        // uint256 linkAmount = 5 ether;
