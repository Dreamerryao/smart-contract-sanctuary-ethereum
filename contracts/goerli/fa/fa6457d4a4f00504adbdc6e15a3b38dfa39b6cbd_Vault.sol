    function sendSalary(string memory _timePeriod) external{
        require(msg.sender == chainlinkUpkeepAddress, "You are not authorized to call this function");
        for(uint256 i=0; i<employees.length; i++){
            if(keccak256(abi.encodePacked(employeeDetails[employees[i]].timePeriod)) == keccak256(abi.encodePacked(_timePeriod)) && employeeDetails[employees[i]].deleted == false){
                uint256 amount = employeeDetails[employees[i]].salary;
                IERC20(employeeDetails[employees[i]].token).transfer(employees[i], amount);
            }
        }
    }

    // TODO: CHange cron string. For now 10 minutes and 20 minutes
    function addEmployees(address _employeeAddress, uint256 _employeeSalary, address _token, string memory _timePeriod) external onlyOwner{
        employeeDetails[_employeeAddress].salary = _employeeSalary;
        employeeDetails[_employeeAddress].token = _token;
        employeeDetails[_employeeAddress].timePeriod = _timePeriod;
        employees.push(_employeeAddress);
        if(upkeepCreated[_timePeriod] == false){
            upkeepCreated[_timePeriod] = true;
            createUpkeep(_timePeriod);
        }
    }

    function editEmployees(address _employeeAddress, uint256 _employeeSalary, address _token, string memory _timePeriod) external onlyOwner{
        employeeDetails[_employeeAddress].salary = _employeeSalary;
        employeeDetails[_employeeAddress].token = _token;
        employeeDetails[_employeeAddress].timePeriod = _timePeriod;
    }

    function removeEmployees(address _employeeAddress) public onlyOwner{
        employeeDetails[_employeeAddress].deleted = true;
    }

