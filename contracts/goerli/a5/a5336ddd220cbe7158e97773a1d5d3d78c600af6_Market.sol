pragma solidity ^0.8.4;

import "./NFT.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";

error NotMarketOwner();

contract CloneFactory {
    address public nftImplementation;
    address public marketOwner;

    constructor(address _marketOwner) {
        nftImplementation = address(new NFT());
        marketOwner = _marketOwner;
    }

    modifier onlyMarketOwner() {
        if (msg.sender != marketOwner) {
            revert NotMarketOwner();
        }
        _;
    }

    event NftContractCreated(
        address cloneContractAddress,
        address nftImplementation,
        address minter
    );

    //@notice clone and initialize a token contract.
    //@param tokenURI- the token uri collecting the metadata.
    //@param royaltiesReceipientAddress- the address that receives the token royalties.
    //@param minter- address of the caller initializing the contract.
    //@param market- address of the market contract.
    //@param edition- the amount of tokens of token type id to be minted.
    //@param royaltyBPS- the amount of royalties in basis points.
    function create(
        string memory tokenURI,
        address payable royaltiesReceipientAddress,
        address payable minter,
        address market,
        uint256 edition,
        uint96 royaltyBPS
    ) external returns(address) {
        address clone;

        clone = Clones.clone(nftImplementation);

        NFT(clone).initialize(
            tokenURI,
            market,
            address(this),
            minter,
            royaltiesReceipientAddress,
            edition,
            royaltyBPS
        );

        emit NftContractCreated(clone, nftImplementation, minter);

        return clone;
    }

    //@notice the contract's owner can set a new market owner
    function setNewMarketOwner(address _newAddress)
        external
        payable
        onlyMarketOwner
    {
        marketOwner = payable(_newAddress);
    }

    //@notice the contract's owner can set a new nft implementation contract
    function setNftImplementation(address _newAddress)
        external
        onlyMarketOwner
    {
        nftImplementation = _newAddress;
    }
}

