pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./@rarible/impl/RoyaltiesV2Impl.sol";
import "./@rarible/royalties/contracts/LibPart.sol";
import "./@rarible/royalties/contracts/LibRoyaltiesV2.sol";

contract NFT is
    Initializable,
    ERC1155Upgradeable,
    OwnableUpgradeable,
    RoyaltiesV2Impl
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenIds;

    address _market;
    address _cloneFactory;
    address _minter;
    bool public paused;
    bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;

    modifier auth() {
        require(
            _msgSender() == _market ||
            _msgSender() == _cloneFactory ||
            _msgSender() == _minter,
            "You are not allowed to call this function"
        );
        _;
    }

    // @notice returns the remaining number of tokens of tokenId that operator will be allowed to spend on behalf of owner through transferToken.
    mapping(address => mapping(uint256 => uint256)) public allowance;
    // @dev map a token ID to a URI
    mapping(uint256 => string) private _uris;

    //@notice initialize the contract and call createToken function.
    //@param tokenURI- the token uri collecting the metadata.
    //@param market- address of the market contract.
    //@param cloneFactory- address of the factory contract.
    //@param minter- address of the caller initializing the contract.
    //@param royaltiesReceipientAddress- the address that receives the token royalties.
    //@param edition- the amount of tokens of token type id to be minted.
    //@param royaltyBPS- the amount of royalties in basis points.
    function initialize(
        string memory tokenURI,
        address market,
        address cloneFactory,
        address minter,
        address payable royaltiesReceipientAddress,
        uint256 edtion,
        uint96 royaltyBPS
    ) public virtual initializer {
        _market = market;
        _cloneFactory = cloneFactory;
        _minter = minter;
        paused = false;

        __ERC1155_init(tokenURI);
        createToken(tokenURI, royaltiesReceipientAddress, edtion, royaltyBPS);
    }

    //@notice mint a token and set royalties
    //@param tokenURI- the token uri collecting the metadata.
    //@param royaltiesReceipientAddress- the address that receives the token royalties.
    //@param royaltyBPS- the amount of royalties in basis points.
    function createToken(
        string memory _tokenURI,
        address payable royaltiesReceipientAddress,
        uint256 edition,
        uint96 royaltyBPS
    ) public auth returns (uint256) {
        require(!paused, "the contract is paused");
        require(
            royaltyBPS <= 5000,
            "Royalties may not exceed 5000 basis points (i.e, 50%)"
        );

        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();

        _mint(_minter, newTokenId, edition, "");
        _uris[newTokenId] = _tokenURI;

        /* ==== SET RARIBLE PROTOCOL ROYALTIES ==== */
        if (royaltyBPS > 0) {
            LibPart.Part[] memory _royalties = new LibPart.Part[](1);
            _royalties[0].value = royaltyBPS;
            _royalties[0].account = royaltiesReceipientAddress;
            _saveRoyalties(newTokenId, _royalties);
        }

        return newTokenId;
    }

    //@notice returns the URI for token type id.
    function uri(uint256 tokenId) public view override returns (string memory) {
        return (_uris[tokenId]);
    }

    //@notice Sets amount as the allowance of operator over the caller’s tokens.
    function setApprove(
        address operator,
        uint256 tokenId,
        uint256 amount
    ) external auth {
        allowance[operator][tokenId] = amount;
    }

    //@notice Transfers amount tokens of token type id from `from` to `to`.
    function transferToken(
        address from,
        address to,
        uint256 tokenId,
        uint256 amount
    ) external {
        require(
            allowance[_msgSender()][tokenId] >= amount || _msgSender() == from,
            "You are not allowed to transfer token"
        );

        require(
            balanceOf(from, tokenId) >= amount == true,
            "Insufficient tokenId balance or not approved for transferToken"
        );

        _safeTransferFrom(from, to, tokenId, amount, "");
    }

    //@notice query if the contract implement an interface
    //@return true if the contract implements interfaceId
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155Upgradeable)
        returns (bool)
    {
        if (interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES) {
            return true;
        }
        if (interfaceId == _INTERFACE_ID_ERC2981) {
            return true;
        }
        return super.supportsInterface(interfaceId);
    }

    /* === EIP-2981 ROYALTY STANDARD === */
    //@notice get how much royalty is owed and to whom
    //@return receiver - address of who should be sent the royalty payment
    //@return royaltyAmount - the royalty payment for _salePrice
    function royaltyInfo(uint256 tokenId, uint256 salePrice)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        LibPart.Part[] memory _royalties = royalties[tokenId];
        if (_royalties.length > 0) {
            return (
                _royalties[0].account,
                (salePrice * _royalties[0].value) / 10000
            );
        }
        return (address(0), 0);
    }

    //@notice the contract's owner can pause the contract.
    function pause(bool state) external onlyOwner {
        paused = state;
    }
}

