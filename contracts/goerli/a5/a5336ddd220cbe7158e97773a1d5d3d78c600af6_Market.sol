pragma solidity ^0.8.4;

import "./NFT.sol";
import "./CloneFactory.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@original-works/contracts-smart-wallet/contracts/Whitelist.sol";

error MustNotExceedRemainingQuantity(
    uint256 quantity,
    uint256 remainingQuantity
);
error BuyerIsSeller(address msgSender, address seller);
error IsNotMinter(address msgSender, address minter);
error IsNotSeller(address msgSender, address seller);
error SalePriceMustBeAboveZero();
error EditionMustBeAboveZero();
error InsufficientFundsToPay();
error CallerDoesNotOwnTokens();
error ContractIsPaused();
error NoProceeds();
error NotListed();
error NoFeesForSmartWallet();

contract Market is
    Initializable,
    OwnableUpgradeable,
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable
{
    using Counters for Counters.Counter;

    address payable public marketOwner;
    address public cloneFactoryAddress;

    //@dev track the number of collections created.
    Counters.Counter private _collectionIds;

    uint256 mintingFee;
    uint256 listingFee;
    uint256 sellingFee;

    bool public paused;

    function initialize(address _cloneFactoryAddress, address _marketOwner)
        public
        initializer
    {
        __Ownable_init();
        __UUPSUpgradeable_init();

        marketOwner = payable(_marketOwner);
        cloneFactoryAddress = _cloneFactoryAddress;
        paused = false;
        mintingFee = 1 ether;
        listingFee = 1 ether;
        sellingFee = 150;
    }

    struct Token {
        uint256 collectionId;
        uint256 tokenId;
        address collectionContract;
        address minter;
        string tokenURI;
        uint256 edition;
        address payable royaltiesRecipientAddress;
        uint256 royaltyBPS;
        bool forSale;
    }

    struct TokenListed {
        address payable seller;
        uint256 collectionId;
        uint256 tokenId;
        uint256 salePrice;
        uint256 remainingQuantity;
    }

    //@dev map Token with collectionContract and tokenId.
    mapping(address => mapping(uint256 => Token)) public token;

    //@dev map TokenListed with collectionContract and tokenId.
    mapping(address => mapping(uint256 => TokenListed)) public tokenListed;

    //@dev map seller’s address and the amount they’ve earned in sales.
    mapping(address => uint256) private proceeds;

    //@dev address of the smart wallet whitelist contract.
    address smartWalletWhitelist;

    event TokenCreated(
        uint256 collectionId,
        uint256 tokenId,
        address collectionContract,
        address minter,
        string tokenURI,
        uint256 edition,
        address payable royaltiesRecipientAddress,
        uint256 royaltyBPS
    );

    event TokenOnSale(
        address seller,
        address collectionContract,
        uint256 collectionId,
        uint256 tokenId,
        uint256 salePrice,
        uint256 remainingQuantity
    );

    event TokenUnlisted(
        address initated_by,
        address collectionContract,
        uint256 collectionId,
        uint256 tokenId
    );

    event TokenPurchased(
        address buyer,
        address collectionContract,
        uint256 collectionId,
        uint256 tokenId,
        uint256 quantity,
        uint256 value,
        uint256 remainingQuantity
    );

    modifier ifPaused() {
        if (paused) {
            revert ContractIsPaused();
        }
        _;
    }

    //@notice Create a collection and initialize it by minting a token to this collection.
    //@param tokenURI- the token uri collecting the metadata.
    //@param royaltiesRecipientAddress - the address that receives the token royalties.
    //@param edition - amount tokens of token type id
    //@param royaltyBPS- amount of royalties in basis points.
    function create(
        string memory tokenURI,
        address payable royaltiesRecipientAddress,
        uint256 edition,
        uint96 royaltyBPS
    ) external payable nonReentrant ifPaused {
        if (edition <= 0) {
            revert EditionMustBeAboveZero();
        }

        _collectFee(mintingFee);

        _collectionIds.increment();
        uint256 collectionId = _collectionIds.current();

        CloneFactory cloneFactory = CloneFactory(cloneFactoryAddress);

        address tokenAddress = cloneFactory.create(
            tokenURI,
            royaltiesRecipientAddress,
            payable(_msgSender()),
            address(this),
            edition,
            royaltyBPS
        );

        token[tokenAddress][1] = Token(
            collectionId,
            1,
            tokenAddress,
            _msgSender(),
            tokenURI,
            edition,
            payable(royaltiesRecipientAddress),
            royaltyBPS,
            false
        );

        emit TokenCreated(
            collectionId,
            1,
            tokenAddress,
            _msgSender(),
            tokenURI,
            edition,
            royaltiesRecipientAddress,
            royaltyBPS
        );
    }

    //@notice - mint a token from a collection you have already created.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param tokenURI- the token uri collecting the metadata.
    //@param royaltiesRecipientAddress- the address that receives the token royalties.
    //@param edition - amount tokens of token type id
    //@param royaltyBPS- amount of royalties in basis points.
    function mint(
        address collectionContract,
        string memory tokenURI,
        address payable royaltiesRecipientAddress,
        uint256 edition,
        uint96 royaltyBPS
    ) external payable nonReentrant ifPaused {
        Token memory _token = token[collectionContract][1];

        if (_msgSender() != _token.minter) {
            revert IsNotMinter(_msgSender(), _token.minter);
        }

        if (edition <= 0) {
            revert EditionMustBeAboveZero();
        }

        _collectFee(mintingFee);

        uint256 newTokenId = NFT(collectionContract).createToken(
            tokenURI,
            royaltiesRecipientAddress,
            edition,
            royaltyBPS
        );

        token[collectionContract][newTokenId] = Token(
            _token.collectionId,
            newTokenId,
            collectionContract,
            _msgSender(),
            tokenURI,
            edition,
            payable(royaltiesRecipientAddress),
            royaltyBPS,
            false
        );

        emit TokenCreated(
            _token.collectionId,
            newTokenId,
            collectionContract,
            _msgSender(),
            tokenURI,
            edition,
            royaltiesRecipientAddress,
            royaltyBPS
        );
    }

    //@notice list a token on the marketplace.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param tokenId - Id of the  collection's child token
    //@param salePrice - selling price defined by the seller.
    function listToken(
        address collectionContract,
        uint256 tokenId,
        uint256 salePrice
    ) external payable ifPaused {
        Token memory _token = token[collectionContract][tokenId];

        if (_msgSender() != _token.minter) {
            revert IsNotMinter(_msgSender(), _token.minter);
        }

        if (salePrice <= 0) {
            revert SalePriceMustBeAboveZero();
        }

        if (getBalanceOf(collectionContract, tokenId, _msgSender()) < 1) {
            revert CallerDoesNotOwnTokens();
        }

        _collectFee(mintingFee);

        uint256 amountToList = getBalanceOf(
            collectionContract,
            tokenId,
            _msgSender()
        );

        uint256 previousAllowance = NFT(collectionContract).allowance(
            address(this),
            tokenId
        );

        NFT(collectionContract).setApprove(
            address(this),
            tokenId,
            previousAllowance + amountToList
        );

        NFT(collectionContract).transferToken(
            _msgSender(),
            marketOwner,
            tokenId,
            amountToList
        );

        token[collectionContract][tokenId].forSale = true;

        tokenListed[collectionContract][tokenId] = TokenListed(
            payable(_msgSender()),
            _token.collectionId,
            tokenId,
            salePrice,
            amountToList
        );

        emit TokenOnSale(
            _msgSender(),
            collectionContract,
            _token.collectionId,
            tokenId,
            salePrice,
            amountToList
        );
    }

    //@notice unlist a token on the marketplace.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param tokenId - Id of the  collection's child token
    function unlistToken(address collectionContract, uint256 tokenId)
        external
        ifPaused
    {
        TokenListed memory _tokenListed = tokenListed[collectionContract][
            tokenId
        ];

        if (_msgSender() != _tokenListed.seller) {
            revert IsNotSeller(_msgSender(), _tokenListed.seller);
        }

        NFT(collectionContract).transferToken(
            marketOwner,
            _msgSender(),
            tokenId,
            _tokenListed.remainingQuantity
        );

        uint256 previousAllowance = NFT(collectionContract).allowance(
            address(this),
            tokenId
        );

        NFT(collectionContract).setApprove(
            address(this),
            tokenId,
            previousAllowance - _tokenListed.remainingQuantity
        );

        token[collectionContract][tokenId].forSale = false;

        emit TokenUnlisted(
            _msgSender(),
            collectionContract,
            _tokenListed.collectionId,
            tokenId
        );
    }

    //@notice purchase a token.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param buyer - Address of the buyer.
    //@param tokenId - Id of the  collection's child token
    //@param quantity - the quantity of tokens the buyer wishes to purchase.
    function purchaseToken(
        address collectionContract,
        address buyer,
        uint256 tokenId,
        uint256 quantity
    ) external payable nonReentrant ifPaused {
        Token memory _token = token[collectionContract][tokenId];
        TokenListed memory _tokenListed = tokenListed[collectionContract][
            tokenId
        ];

        if (_token.forSale == false) {
            revert NotListed();
        }

        if (msg.value != (_tokenListed.salePrice * quantity)) {
            revert InsufficientFundsToPay();
        }

        if (buyer == _tokenListed.seller) {
            revert BuyerIsSeller(buyer, _tokenListed.seller);
        }

        if (quantity > _tokenListed.remainingQuantity) {
            revert MustNotExceedRemainingQuantity(
                quantity,
                _tokenListed.remainingQuantity
            );
        }

        uint256 marketCut = ((_tokenListed.salePrice * quantity * sellingFee) /
            10000);
        uint256 sellerPrice = ((_tokenListed.salePrice * quantity) - marketCut);

        proceeds[marketOwner] += marketCut;
        proceeds[_tokenListed.seller] += sellerPrice;

        NFT(collectionContract).transferToken(
            marketOwner,
            buyer,
            tokenId,
            quantity
        );

        uint256 remainingQuantity = _tokenListed.remainingQuantity - quantity;
        tokenListed[collectionContract][tokenId]
            .remainingQuantity = remainingQuantity;

        NFT(collectionContract).setApprove(
            address(this),
            tokenId,
            remainingQuantity
        );

        if (remainingQuantity == 0) {
            token[collectionContract][tokenId].forSale = false;
        }

        emit TokenPurchased(
            buyer,
            collectionContract,
            _token.collectionId,
            tokenId,
            quantity,
            msg.value,
            remainingQuantity
        );
    }

    //@notice withdraw the balance of an address.
    function withdrawProceeds(address payable _address) external ifPaused {
        uint256 _proceeds = proceeds[_address];
        if (_proceeds <= 0) {
            revert NoProceeds();
        }
        proceeds[_address] = 0;

        (bool success, ) = payable(_address).call{value: _proceeds}("");
        require(success, "Transfer failed");
    }

    //@notice get the selling price of a specific token.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param tokenId - Id of the  collection's child token
    function getSalePrice(address collectionContract, uint256 tokenId)
        external
        view
        returns (uint256)
    {
        TokenListed memory _tokenListed = tokenListed[collectionContract][
            tokenId
        ];
        return _tokenListed.salePrice;
    }

    //@notice Query the minting fee
    function getMintingFee() external view returns (uint256) {
        return mintingFee;
    }

    //@notice Query the listing fee
    function getListingFee() external view returns (uint256) {
        return listingFee;
    }

    //@notice Query the listing fee
    function getSellingFee() external view returns (uint256) {
        return sellingFee;
    }

    //@notice get the quantity of tokens of token type id owned by an address.
    //@param collectionContract - Contract address of the token's parent collection.
    //@param tokenId- Id of the  collection's child token.
    //@param _address - owner’s account.
    function getBalanceOf(
        address collectionContract,
        uint256 tokenId,
        address _address
    ) public view ifPaused returns (uint256) {
        return NFT(collectionContract).balanceOf(_address, tokenId);
    }

    //@notice get the amount earned by a seller.
    //@param seller - seller’s address.
    function getProceeds(address seller) external view returns (uint256) {
        return proceeds[seller];
    }

    /* Only Owner */

    //@notice the contract's owner can pause the contract
    function pause(bool _state) external onlyOwner {
        paused = _state;
    }

    //@notice the contract's owner can set a new market owner
    function setNewMarketOwner(address _newAddress) external payable onlyOwner {
        marketOwner = payable(_newAddress);
    }

    //@notice the contract's owner can set a new clone factory address
    function setNewCloneFactory(address _newAddress) external onlyOwner {
        cloneFactoryAddress = _newAddress;
    }

    //@notice the contract's owner can change the minting fee
    function setMintingFee(uint256 _value) external onlyOwner {
        mintingFee = _value;
    }

    //@notice the contract's owner can change the listing fee
    function setListingFee(uint256 _value) external onlyOwner {
        listingFee = _value;
    }

    //@notice the contract's owner can change the selling fee
    function setSellingFee(uint256 _value) external onlyOwner {
        sellingFee = _value;
    }

    function setSmartWalletWhitelist(address _address) external onlyOwner {
        smartWalletWhitelist = _address;
    }

    //@notice collect fees the caller is not a smart wallet
    function _collectFee(uint256 fee) internal {
        bool isSmartWallet = IWhitelist(smartWalletWhitelist).isWhitelisted(
            _msgSender()
        );
        if (isSmartWallet) {
            if (msg.value > 0) {
                revert NoFeesForSmartWallet();
            }
            return;
        }

        if (msg.value < fee) {
            revert InsufficientFundsToPay();
        }

        payable(marketOwner).transfer(fee);
    }

    //@notice authorize the owner to upgrade the contract
    function _authorizeUpgrade(address) internal override onlyOwner {}
}

