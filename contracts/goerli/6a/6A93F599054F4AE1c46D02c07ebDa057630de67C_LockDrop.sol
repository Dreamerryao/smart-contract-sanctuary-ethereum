 * to the chosen locking schedule.
 LockDrop 合约通过可验证的收据证明锁定存款人的资金，
 该收据证明可用于在单独的、兼容的区块链上兑换代币。令牌被锁定指定的时间长度。代币的铸造速度根据所选的锁定时间表而增长。
 */

contract DSMath {
    function add(uint x, uint y) internal pure returns (uint z) {
        require((z = x + y) >= x, "ds-math-add-overflow");
    }
    function sub(uint x, uint y) internal pure returns (uint z) {
        require((z = x - y) <= x, "ds-math-sub-underflow");
    }
    function mul(uint x, uint y) internal pure returns (uint z) {
        require(y == 0 || (z = x * y) / y == x, "ds-math-mul-overflow");
    }

    function min(uint x, uint y) internal pure returns (uint z) {
        return x <= y ? x : y;
    }
    function max(uint x, uint y) internal pure returns (uint z) {
        return x >= y ? x : y;
    }
    function imin(int x, int y) internal pure returns (int z) {
        return x <= y ? x : y;
    }
    function imax(int x, int y) internal pure returns (int z) {
        return x >= y ? x : y;
    }
}


interface IERC20 {
    function transfer(address to, uint256 value) external returns (bool);

    function approve(address spender, uint256 value) external returns (bool);

    function transferFrom(address from, address to, uint256 value) external returns (bool);

    function totalSupply() external view returns (uint256);

    function balanceOf(address who) external view returns (uint256);

    function allowance(address owner, address spender) external view returns (uint256);

    event Transfer(address indexed from, address indexed to, uint256 value);

    event Approval(address indexed owner, address indexed spender, uint256 value);
}


contract LockDrop is DSMath {

    address public rewardToken;
    address public usdt;

    uint constant THREE_MONTHS = 91;
    uint constant SIX_MONTHS = 182;

    uint public tokenCapacity;
    uint public tokenPrice;
    uint public ending;

    struct Lock {
        uint amount;
        uint numOfTokens;
        uint lockEnding;
    }
    //    mapping (address => mapping (address => bool)) private _operatorApprovals;

    mapping ( uint => mapping (address => Lock[]) )  locks;

    event Deposit(address indexed sender, uint numOfTokens, uint lockIndex);
    event Unlock(address indexed sender, uint lockIndex);
    event Withdraw(address indexed sender, uint value);

    modifier hasNotEnded() {
        require(block.timestamp <= ending, "lock-drop-ended");
        _;
    }

    modifier hasEnded() {
        console.log("withdraw , time ",block.timestamp);
    require(block.timestamp  > ending, "lock-drop-still-active");
        _;
    }

    constructor(uint _lockJoinInDays, address _usdt)  {
        ending = add(block.timestamp , mul(1 days, _lockJoinInDays));
        usdt = _usdt;
        //        前端需要approve 相应数量的 _tokenCapacity 的 token
    }
    function initial( uint _tokenCapacity,address _token)public hasNotEnded{
        tokenCapacity = _tokenCapacity;
        rewardToken = _token;
        IERC20(_token).transferFrom(msg.sender, address(this), _tokenCapacity);
    }

    /**
     * @dev        Lock function for participating in the lock drop
     * @param      _lengthInDays  Number of days chosen for locking ether
     参与锁投的锁函数 @param _lengthInDays 选择锁定以太币的天数
     */
    //  总锁仓没有过期
    function lockERC20(uint _lengthInDays,uint _amount) payable public hasNotEnded {
        require(_amount >0 ,"invalid-value");
        require(tokenCapacity > 0, "no-more-tokens-available");
        require(_lengthInDays ==THREE_MONTHS || _lengthInDays ==  SIX_MONTHS ,"invalid-lengtInDays");
        console.log("i am in lockerc20");
        // 兑换比例
        uint numOfTokens =mul(_amount,uint(2));

        require(numOfTokens <= tokenCapacity, "amount-exceeds-available-tokens");
        IERC20(usdt).transferFrom(msg.sender, address(this), _amount);

        tokenCapacity = sub(tokenCapacity, numOfTokens);
        //        创建具有支付金额和指定时间长度的存款。锁定结束确定为锁定期结束后指定的时间长度，以天为单位。
        Lock memory l = Lock({
        amount: _amount,// usdt
        numOfTokens: numOfTokens,//chatCoin
        lockEnding: add(ending, mul(_lengthInDays, 1 days))
        });
        locks[_lengthInDays][msg.sender].push(l);
        // Emit Deposit event
        emit Deposit(msg.sender, numOfTokens, locks[_lengthInDays][msg.sender].length - uint(1));
    }


    /**
     * @dev        Unlock function reverts the lock before the lock starts
     * @param      _lockIndex  The deposit index to unlock
     解锁函数在锁开始之前恢复锁 @param _lockIndex 要解锁的存款索引  hasNotEnded =>  require(now <= ending, "lock-drop-ended");
     回退函数
     */
    function unlock(uint _lockIndex,uint _lengthInDays) public hasNotEnded {
        require(locks[_lengthInDays][msg.sender].length > _lockIndex, "lock-not-found");
        // Don't spend users gas if deposit is already unlocked 如果存款已经解锁，不要花费用户 gas
        require(locks[_lengthInDays][msg.sender][_lockIndex].amount > 0, "deposit-already-unlocked");

        // Save amount to memory and delete the lock 节省内存并删除锁
        Lock memory l = locks[_lengthInDays][msg.sender][_lockIndex];
        delete locks[_lengthInDays][msg.sender][_lockIndex];

        // Make the tokens available for other buyers 使代币可供其他买家使用
        tokenCapacity = add(tokenCapacity, l.numOfTokens);
        console.log("l.amount",l.amount);
        // Send the funds back to owner
        IERC20(usdt).transfer(msg.sender,  l.amount);

        // Emit Unlock event
        emit Unlock(msg.sender, _lockIndex);
    }

    /**
     * @dev        Withdraw function should withdraw all valid ether after lock 取款功能应该在锁定后取回所有有效的以太币
     */
    function withdraw(uint _lengthInDays) public hasEnded {
        require(locks[_lengthInDays][msg.sender].length > 0, "no-locks-found");
        uint amount = 0;
        uint rewardAmount = 0 ;
        // Iterate over all locks
        for (uint i = 0; i < locks[_lengthInDays][msg.sender].length; i++) {
            Lock memory curr = locks[_lengthInDays][msg.sender][i];
            //如果 ending 已过且 lockEnding 有效，则聚合存款
            // Aggregate deposit if ending has passed and lockEnding is valid
            if (block.timestamp  >= curr.lockEnding && curr.lockEnding != 0) {
                amount = add(amount, curr.amount);
                rewardAmount = add(rewardAmount,curr.numOfTokens);
                delete locks[_lengthInDays][msg.sender][i];
            }
        }
        // 如果没有锁定数量，我们不想花费用户 gas
        // We dont want to spend users gas if there is no amount locked
        require(amount > 0, "no-locked-amount-found");
        require(rewardAmount > 0, "no-locked-amount-found");
        IERC20(usdt).transfer(msg.sender,  amount);
        IERC20(rewardToken).transfer(msg.sender,  rewardAmount);
        emit Withdraw(msg.sender, amount);
    }

    function calculateEffectiveAmount(uint _value, uint _length) public pure returns (uint _effectiveValue) {
        require(_length >= THREE_MONTHS, "invalid-lock-duration");

        if (_length < SIX_MONTHS) {
            return _value;
        }

        // maximum period is 2 years (4 * SIX_MONTHS)
        uint period = min(4, _length / SIX_MONTHS);
        uint bonus = mul(_value, mul(period, 3.75 ether)) / 100 ether;
        uint effectiveValue = add(_value, bonus);
        return effectiveValue;
    }

    function getTotalLocks(address _user,uint _lengthInDays) public view returns (uint _length) {
        return locks[_lengthInDays][_user].length;
    }

    function getLockAt(address _user, uint _index,uint _lengthInDays) public view returns (uint amount, uint numOfTokens, uint lockEnding) {
        console.log("getLockAt",block.timestamp);
        return (locks[_lengthInDays][_user][_index].amount, locks[_lengthInDays][_user][_index].numOfTokens, locks[_lengthInDays][_user][_index].lockEnding);
    }
}