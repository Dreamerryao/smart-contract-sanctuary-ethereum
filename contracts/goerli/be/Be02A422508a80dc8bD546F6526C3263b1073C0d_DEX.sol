/**
 * @title DEX Template
 * @author stevepham.eth and m00npapi.eth
 * @notice Empty DEX.sol that just outlines what features could be part of the challenge (up to you!)
 * @dev We want to create an automatic market where our contract will hold reserves of both ETH and ðŸŽˆ Balloons. These reserves will provide liquidity that allows anyone to swap between the assets.
 * NOTE: functions outlined here are what work with the front end of this branch/repo. Also return variable names that may need to be specified exactly may be referenced (if you are confused, see solutions folder in this repo and/or cross reference with front-end code).
 */
contract DEX {
    uint256 public totalLiquidity;
    mapping(address => uint256) public liquidity;

    // using SafeMath for uint256; //outlines use of SafeMath for uint256 variables
    IERC20 token; //instantiates the imported contract

    /* ========== EVENTS ========== */

    /**
     * @notice Emitted when ethToToken() swap transacted
     */
    event EthToTokenSwap(address sender, string text, uint256 ethAmount, uint256 tokenAmount);

    /**
     * @notice Emitted when tokenToEth() swap transacted
     */
    event TokenToEthSwap(address sender, string text, uint256 ethAmount, uint256 tokenAmount);

    /**
     * @notice Emitted when liquidity provided to DEX and mints LPTs.
     */
    event LiquidityProvided(address provider, uint256 liquidityRelativeAmount, uint256 ethAmount, uint256 tokenAmount);

    /**
     * @notice Emitted when liquidity removed from DEX and decreases LPT count within DEX.
     */
    event LiquidityRemoved(address provider, uint256 liquidityRelativeAmount, uint256 ethAmount, uint256 tokenAmount);

    /* ========== CONSTRUCTOR ========== */

    constructor(address token_addr) {
        token = IERC20(token_addr); //specifies the token address that will hook into the interface and be used through the variable 'token'
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    /**
     * @notice initializes amount of tokens that will be transferred to the DEX itself 
     * from the erc20 contract mintee (and only them based on how Balloons.sol is written). 
     * Loads contract up with both ETH and Balloons.
     * @param tokens amount to be transferred to DEX
     * @return totalLiquidity is the number of LPTs minting as a result of deposits made to DEX contract
     * NOTE: since ratio is 1:1, this is fine to initialize the totalLiquidity (wrt to balloons) 
     * as equal to eth balance of contract.
     */
    function init(uint256 tokens) public payable returns (uint256) {
        console.log("init");
        require(totalLiquidity == 0, "DEX already initialized");
        totalLiquidity = address(this).balance;
        liquidity[msg.sender] = totalLiquidity;
        bool tranferSucceeded = token.transferFrom(msg.sender, address(this), tokens);
        require(tranferSucceeded, "DEX: init - transfer did not transact");
        return totalLiquidity;
    }

    /**
     * @notice returns yOutput, or yDelta for xInput (or xDelta)
     * @dev Follow along with the [original tutorial](https://medium.com/@austin_48503/%EF%B8%8F-minimum-viable-exchange-d84f30bd0c90) Price section for an understanding of the DEX's pricing model and for a price function to add to your contract. You may need to update the Solidity syntax (e.g. use + instead of .add, * instead of .mul, etc). Deploy when you are done.
     */
    function price(
        uint256 xInput,
        uint256 xReserves,
        uint256 yReserves
    ) public pure returns (uint256 yOutput) {
        uint256 inputAmountWithFee = xInput * 997;
        uint256 numerator = inputAmountWithFee * yReserves;
        uint256 denominator = (xReserves * 1000) + inputAmountWithFee;
        return numerator / denominator;
    }

    /**
     * @notice returns liquidity for a user. Note this is not needed typically due to the `liquidity()` mapping variable being public and having a getter as a result. This is left though as it is used within the front end code (App.jsx).
     * if you are using a mapping liquidity, then you can use `return liquidity[lp]` to get the liquidity for a user.
     *
     */
    function getLiquidity(address lp) public view returns (uint256) {
        return liquidity[lp];
    }

    /**
     * @notice sends Ether to DEX in exchange for $BAL
     */
    function ethToToken() public payable returns (uint256 tokenOutput) {
        require(msg.value > 0, "Send some Ether");
        // Determine amount of tokens that the given amount of Ether is worth
        uint256 ethLiqInitial = address(this).balance - msg.value;
        uint256 balLiqInitial = totalLiquidity;
        uint256 balAmount = price(msg.value, ethLiqInitial, balLiqInitial);
        // transfer token from DEX address to msg.sender
        bool transferSucceeded = token.transfer(msg.sender, balAmount);
        require(transferSucceeded, "Transfer of BAL failed");
        // also remove tthe amount of token's from totalLiquidity
        totalLiquidity -= balAmount;

        emit EthToTokenSwap(msg.sender, "Eth to Balloons", msg.value, balAmount);
        
        return balAmount;
    }

    /**
     * @notice sends $BAL tokens to DEX in exchange for Ether
     */
    function tokenToEth(uint256 tokenInput) public returns (uint256 ethOutput) {
        require(tokenInput > 0, "Send some BAL");
        // Determine amount of Ether that the incoming amount of tokens is worth
        uint256 ethLiqInitial = address(this).balance;
        uint256 balLiqInitial = totalLiquidity;
        uint256 ethAmount = price(tokenInput, balLiqInitial, ethLiqInitial);
        bool balTransferSucceeded = token.transferFrom(msg.sender, address(this), tokenInput);
        // Increate totalLiquidity with amount of tokens received
        totalLiquidity += tokenInput;
        require(balTransferSucceeded, "Transfer of BAL failed");
        (bool ethTransferSucceeded, ) = msg.sender.call{ value: ethAmount }("");
        require(ethTransferSucceeded, "Transfer of ETH failed");
        emit TokenToEthSwap(msg.sender, "Balloons to ETH", ethAmount, tokenInput);
        return ethAmount;
    }

    /**
     * @notice allows deposits of $BAL and $ETH to liquidity pool
     * NOTE: parameter is the msg.value sent with this function call. That amount is used to determine the amount of $BAL needed as well and taken from the depositor.
     * NOTE: user has to make sure to give DEX approval to spend their tokens on their behalf by calling approve function prior to this function call.
     * NOTE: Equal parts of both assets will be removed from the user's wallet with respect to the price outlined by the AMM.
     */
    function deposit() public payable returns (uint256 tokensDeposited) {
        require(msg.value > 0, "Send some ETH");
        uint256 ethReserve = address(this).balance - msg.value;
        uint256 tokenReserve = token.balanceOf(address(this));
        uint256 tokenDepositAmount = msg.value * ( tokenReserve / ethReserve ) + 1;

        bool tokensTransferred = token.transferFrom(msg.sender, address(this), tokenDepositAmount);
        require(tokensTransferred, "Token transfer failed");

        // Determine how much relative liquidity was minted
        uint256 relativeLiquidityMinted = msg.value * ( totalLiquidity / ethReserve );
        liquidity[msg.sender] +=  relativeLiquidityMinted;
        totalLiquidity += relativeLiquidityMinted;

        emit LiquidityProvided(msg.sender, relativeLiquidityMinted, msg.value, tokenDepositAmount);
        return tokenDepositAmount;
    }

    /**
     * @notice allows withdrawal of $BAL and $ETH from liquidity pool
     * NOTE: with this current code, the msg caller could end up getting very little back if the liquidity is super low in the pool. I guess they could see that with the UI.
     */
    function withdraw(uint256 amount) public returns (uint256 eth_amount, uint256 token_amount) {
        // amount is the amount of relative liquidity that the address tries to withdraw
        require(liquidity[msg.sender] >= amount, "You don't have that much liquidity in the pool");
        uint256 ethReserve = address(this).balance;
        uint256 tokenReserve = token.balanceOf(address(this));
        
        uint256 ethAmount = amount * ethReserve / totalLiquidity;
        uint256 tokenAmount = amount * tokenReserve / totalLiquidity;
        
        liquidity[msg.sender] -= amount;
        totalLiquidity -= amount;

        (bool ethSent, ) = msg.sender.call{ value: ethAmount }("");
        require(ethSent, "ETH transfer failed");
        bool tokenSent = token.transfer(msg.sender, tokenAmount);
        require(tokenSent, "Token transfer failed");
        emit LiquidityRemoved(msg.sender, amount, ethAmount, tokenAmount);
        return (ethAmount, tokenAmount);
    }
}

