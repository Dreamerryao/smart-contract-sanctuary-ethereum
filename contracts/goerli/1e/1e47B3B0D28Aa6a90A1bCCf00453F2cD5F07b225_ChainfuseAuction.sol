    function mintUnsafe(uint _registrationId, uint _amount, uint256 _price) payable public {
        // require(_mintedNfts[_registrationId][tokenId].minted != true, "NFT already minted with this ID!");
        Registration memory registration = _registrations[_registrationId];

        console.log(_amount, _price, _amount * _price, msg.value);

        require((registration.supply - _amount) >= 0, "NFTs ran out");
        require(_amount >= 0, "Must be positive number");
        require((_amount * _price) == msg.value, "The prices do not match");
        
        uint256 addAmount =  _mintedNfts[_registrationId][msg.sender].amount + _amount;

        _setTokenUri(_registrationId, registration.uri);
        
        _mint(msg.sender, _registrationId, _amount, "");
        
        _registrations[_registrationId].supply = registration.supply - _amount;
        
        _mintedNfts[_registrationId][msg.sender] = MintedStatus(msg.sender, _registrationId, addAmount);
        _mintedNfts[_registrationId][msg.sender].amount = addAmount;

        emit Minted(msg.sender, _amount);
    }

    function mint(
        // Voucher verification required.
        // Combine both of these to get signer confirmation address as verification
        bytes memory _signature,
        bytes32 _messageHash,

        // Data to process
        // (IDEA: Any way parterships can work the same way?)
        address _beneficiary, // Gets the beneficiary for the proceeds
        uint256 _price, // Gets price of the NFT
        uint _registrationId,

        // Potential
        // Lock period: Until when will this NFT be mintable?
        // Decline formula: Devalue price of the NFT over time
        uint256 _amount
    ) payable public {
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(_messageHash);

        Registration memory registration = _registrations[_registrationId];
        
        // Make sure the owner's voucher is verified, and based if that voucher is valid with all of its parameters, proceed.
        require(recoverSigner(ethSignedMessageHash, _signature) == _registrations[_registrationId].owner, "Signer is not the owner of the signature");
        require((_amount * _price) == msg.value, "The prices do not match");
        require((registration.supply - _amount) >= 0, "NFTs ran out");
        require(_amount >= 0, "Must be positive number");

        payable(address(_beneficiary)).transfer(msg.value);

        uint256 addAmount = _mintedNfts[_registrationId][msg.sender].amount + _amount;

        _setTokenUri(_registrationId, registration.uri);
        
        _mint(msg.sender, _registrationId, _amount, "");
        
        _registrations[_registrationId].supply = registration.supply - _amount;
        
        _mintedNfts[_registrationId][msg.sender] = MintedStatus(msg.sender, _registrationId, addAmount);
        _mintedNfts[_registrationId][msg.sender].amount = addAmount;

        emit Minted(msg.sender, _amount);
    }

