pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./BasicToken.sol";

contract Auction is ReentrancyGuard, ERC1155Holder  {
    struct AuctionResponse {
        AuctionItem auctionItem;
        AuctionData auctionData;
    }

    struct AuctionItem {
        uint itemId;
        address nftContract; // The 1155 Token
        uint256 registrationId; // Registration ID of the collection
        uint256 chainTokenId; // Token id coming from chainfuse front end
        uint256 tokenId; // TokenId in the 1155 Token
        address payable owner; // Whoever put it on sale
        address payable seller; // Whoever put it on sale
    }

    struct AuctionData {
        uint256 price; // Price of NFT
        uint256 startBlock;
        uint256 endTime;
        bool onAuction; // Is NFT On Auction (and currently stored in this contract)
        bool canceled;
        bool auctionIsSettled;
        bool ownerHasWithdrawn;
        uint256 totalBids;
        address highestBidder;
        uint256 highestBindingBid;
    }

    struct Bidder {
        address bidder;
        uint256 bid;
        bool withdrew;
    }

    using Counters for Counters.Counter;
    Counters.Counter private _itemIds;
    Counters.Counter private _itemsOnAuction;

    Counters.Counter private _auctionBiddersCounter;

    uint256 public bidIncrement = 0.1 ether;

    mapping (address => mapping (uint256 => AuctionItem)) private addressToAuctionTokenIdToOwnership;
    mapping (address => mapping (uint256 => AuctionData)) private addressToAuctionTokenIdToData;

    mapping (address => mapping (uint256 => mapping (address => Bidder))) private fundsByBidder;
    mapping (address => mapping (uint256 => address[])) private bidderMapping;

    mapping (uint256 => uint256[]) private itemsOnAuction;

    event LogBid(
        address bidder,
        uint256 bid,
        address highestBidder,
        uint256 highestBid,
        uint256 highestBindingBid
    );

    event LogWithdrawal(
        address withdrawer,
        address withdrawalAccount,
        uint256 amount
    );

    event LogCanceled();

    function createAuctionItem(
        address nftContract,
        uint256 chainTokenId,
        uint256 tokenId,
        uint256 price,
        uint256 registrationId, // Can one be pre-made for off-chain 1155's?
        uint256 endTime
    ) public payable nonReentrant {
        require(endTime > block.timestamp, "End time must be in the future.");
        require(price > 0, "Price must be at least 1 wei");
        // require((msg.value == price), "Price must be equal to listing price");

        uint256 itemId = _itemIds.current();

        cashOutAllPreviousBidders(nftContract, chainTokenId);

        addressToAuctionTokenIdToOwnership[nftContract][chainTokenId] = AuctionItem(
            itemId,
            nftContract,
            registrationId,
            chainTokenId,
            tokenId,
            payable(tx.origin),
            payable(tx.origin)
        );

        itemsOnAuction[registrationId].push(chainTokenId);

        addressToAuctionTokenIdToData[nftContract][chainTokenId] = AuctionData(
            price,
            block.timestamp,
            endTime,
            true,
            false,
            false,
            false,
            0,
            address(0),
            0
        );

        IERC1155(nftContract).safeTransferFrom(tx.origin, address(this), chainTokenId, 1, "");

        _itemsOnAuction.increment();
        _itemIds.increment();
    }

    function fetchAuctionItems(address nftContract, uint256 registrationId) view public returns (AuctionResponse[] memory) {
        AuctionResponse[] memory auctionItems = new AuctionResponse[](itemsOnAuction[registrationId].length);
    
        for (uint i = 0; i < itemsOnAuction[registrationId].length; i++) {
            AuctionData memory marketData = addressToAuctionTokenIdToData[nftContract][itemsOnAuction[registrationId][i]];
            AuctionItem memory auctionData = addressToAuctionTokenIdToOwnership[nftContract][itemsOnAuction[registrationId][i]];
            auctionItems[i] = AuctionResponse(auctionData, marketData);
        }

        return auctionItems;
    }

    function getBlockNumber() public view returns (uint256 blockNumber) {
        return block.timestamp;
    }

    function getHighestBid(address nftContract, uint256 chainTokenId) public view returns (uint256) {
        AuctionData memory auctionData = addressToAuctionTokenIdToData[nftContract][chainTokenId];
        return fundsByBidder[nftContract][chainTokenId][auctionData.highestBidder].bid;
    }

    function getHighestBidder(address nftContract, uint256 chainTokenId) public view returns (address) {
        AuctionData memory auctionData = addressToAuctionTokenIdToData[nftContract][chainTokenId];
        return auctionData.highestBidder;
    }

    function placeBid(address nftContract, uint256 chainTokenId)
        public
        payable
    {
        AuctionData memory auctionData = addressToAuctionTokenIdToData[nftContract][chainTokenId];
        AuctionItem memory onwership = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId];

        require((tx.origin != onwership.owner), "The owner cannot call this function.");
        require((!auctionData.canceled), "This function can only be called when the auction is not cancelled.");
        require((block.timestamp > auctionData.startBlock), "This function can only be called when the current block is greater than the start block.");
        require((block.timestamp < auctionData.endTime), "This function can only be called before the auction ends.");

        require(msg.value != 0, "The bid cannot be zero.");
        require(msg.value > auctionData.price, "The bid must be higher than the starting price!");

        // calculate the user's total bid based on the current amount they've sent to the contract
        // plus whatever has been sent with this transaction
        uint256 newBid = fundsByBidder[nftContract][chainTokenId][tx.origin].bid + msg.value;

        // if the user isn't even willing to overbid the highest binding bid, there's nothing for us
        // to do except revert the transaction.
        require(newBid > auctionData.highestBindingBid, "The bid cannot be less than the current highest bid.");

        // grab the previous highest bid (before updating fundsByBidder, in case tx.origin is the
        // highestBidder and is just increasing their maximum bid).
        uint256 highestBid = fundsByBidder[nftContract][chainTokenId][auctionData.highestBidder].bid;

        // If the funds from this particular bidder is 0, create a new mapping
        if (fundsByBidder[nftContract][chainTokenId][tx.origin].bid == 0) {
            bidderMapping[nftContract][chainTokenId].push(tx.origin);
        }

        fundsByBidder[nftContract][chainTokenId][tx.origin] = Bidder(tx.origin, newBid, false);

        if (newBid <= highestBid) {
            // if the user has overbid the highestBindingBid but not the highestBid, we simply
            // increase the highestBindingBid and leave highestBidder alone.

            // note that this case is impossible if tx.origin == highestBidder because you can never
            // bid less ETH than you've already bid.

            addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBindingBid = min(newBid + bidIncrement, highestBid);
        } else {
            // if tx.origin is already the highest bidder, they must simply be wanting to raise
            // their maximum bid, in which case we shouldn't increase the highestBindingBid.

            // if the user is NOT highestBidder, and has overbid highestBid completely, we set them
            // as the new highestBidder and recalculate highestBindingBid.

            if (tx.origin != addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBidder) {
                addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBidder = tx.origin;
                addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBindingBid = min(newBid, highestBid + bidIncrement);
            }

            highestBid = newBid;
        }

        // If bid is ending in 5 minutes...
        if (block.timestamp > addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime - 5 minutes) {
            // Add extra 5 minutes to the bid
            addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime = addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime + 5 seconds;
        }

        emit LogBid(
            tx.origin,
            newBid,
            addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBidder,
            highestBid,
            addressToAuctionTokenIdToData[nftContract][chainTokenId].highestBindingBid
        );
    }

    function totalBidders(address nftContract, uint256 chainTokenId) public view returns (uint256 total) {
        return bidderMapping[nftContract][chainTokenId].length;
    }

    function pendingWithdrawalAmount(address nftContract, uint256 chainTokenId) public view returns (uint256 total) {
        uint256 totalPending = 0;

        for (uint256 i = 0; i < bidderMapping[nftContract][chainTokenId].length; i++) {
            address currentBidder = bidderMapping[nftContract][chainTokenId][i];
            totalPending = totalPending + fundsByBidder[nftContract][chainTokenId][currentBidder].bid;
        }

        return totalPending;
    }

    function cashOutAllPreviousBidders(address nftContract, uint256 chainTokenId) public {
        AuctionItem memory ownership = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId];

        if (bidderMapping[nftContract][chainTokenId].length == 0) {
            return;
        }

        for (uint256 i = 0; i < bidderMapping[nftContract][chainTokenId].length; i++) {
            address currentBidder = bidderMapping[nftContract][chainTokenId][i];
            address payable withdrawer = payable(currentBidder);

            if (fundsByBidder[nftContract][chainTokenId][currentBidder].bid > 0) {
                withdrawer.transfer(fundsByBidder[nftContract][chainTokenId][currentBidder].bid);
                fundsByBidder[nftContract][chainTokenId][currentBidder].withdrew = true;
                fundsByBidder[nftContract][chainTokenId][currentBidder].bid -= fundsByBidder[nftContract][chainTokenId][currentBidder].bid;
            }
        }

        delete itemsOnAuction[ownership.registrationId];
        delete bidderMapping[nftContract][chainTokenId];
    }

    function min(uint256 a, uint256 b) private pure returns (uint256) {
        if (a < b) return a;
        return b;
    }

    function cancelAuction(address nftContract, uint256 chainTokenId)
        public
        onlyOwner(nftContract, chainTokenId)
        onlyBeforeEnd(nftContract, chainTokenId)
        onlyNotCanceled(nftContract, chainTokenId)
    {
        AuctionItem memory ownership = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId];

        addressToAuctionTokenIdToData[nftContract][chainTokenId].canceled = true;
        addressToAuctionTokenIdToData[nftContract][chainTokenId].onAuction = false;

        for (uint i = 0; i < itemsOnAuction[ownership.registrationId].length; i++) {
            if (itemsOnAuction[ownership.registrationId][i] == chainTokenId) {
                itemsOnAuction[ownership.registrationId][i] = itemsOnAuction[ownership.registrationId][itemsOnAuction[ownership.registrationId].length-1];
                itemsOnAuction[ownership.registrationId].pop();
            }
        }

        IERC1155(nftContract).safeTransferFrom(address(this), tx.origin, chainTokenId, 1, "");
        cashOutAllPreviousBidders(nftContract, chainTokenId);

        delete itemsOnAuction[ownership.registrationId];
        delete bidderMapping[nftContract][chainTokenId];

        emit LogCanceled();
    }

    function isAuctionWinner(address nftContract, uint256 chainTokenId) public view returns (bool isWinner) {
        address payable withdrawer = payable(tx.origin);

        // Get the auction object created for the tokenId
        AuctionData memory auctionData = addressToAuctionTokenIdToData[nftContract][chainTokenId];

        if (auctionData.canceled) {
            return false;
        } else {
            if (withdrawer == auctionData.highestBidder) {
                return true;
            } else {
                return false;
            }
        }
    }

    function withdraw(address nftContract, uint256 chainTokenId)
        public
        payable
        onlyEndedOrCanceled(nftContract, chainTokenId)
    {
        address payable withdrawer = payable(tx.origin);

        address withdrawalAccount;
        uint256 withdrawalAmount;

        // Get the auction object created for the tokenId
        AuctionItem memory auctionOwnership = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId];
        AuctionData memory auctionData = addressToAuctionTokenIdToData[nftContract][chainTokenId];

        bool canceled = auctionData.canceled;

        if (canceled) {
            // if the auction was canceled, everyone should simply be allowed to withdraw their funds
            withdrawalAccount = withdrawer;
            withdrawalAmount = fundsByBidder[nftContract][chainTokenId][withdrawalAccount].bid;
        } else {
            // the auction finished without being canceled

            if (withdrawer == auctionOwnership.owner) {
                // the auction's owner should be allowed to withdraw the highestBindingBid
                withdrawalAccount = auctionData.highestBidder;
                withdrawalAmount = auctionData.highestBindingBid;
                addressToAuctionTokenIdToData[nftContract][chainTokenId].ownerHasWithdrawn = true;
            } else if (withdrawer == auctionData.highestBidder) {
                // the highest bidder should only be allowed to withdraw the difference between their
                // highest bid and the highestBindingBid
                withdrawalAccount = auctionData.highestBidder;

                if (auctionData.ownerHasWithdrawn) {
                    withdrawalAmount = fundsByBidder[nftContract][chainTokenId][auctionData.highestBidder].bid;
                } else {
                    withdrawalAmount =
                        fundsByBidder[nftContract][chainTokenId][auctionData.highestBidder].bid -
                        auctionData.highestBindingBid;
                }
            } else {
                // anyone who participated but did not win the auction should be allowed to withdraw
                // the full amount of their funds
                withdrawalAccount = withdrawer;
                withdrawalAmount = fundsByBidder[nftContract][chainTokenId][withdrawalAccount].bid;
            }

            if (withdrawer == auctionData.highestBidder) {
                IERC1155(nftContract).safeTransferFrom(address(this), withdrawer, chainTokenId, 1, "");

                if (withdrawalAmount != 0) {
                    withdrawer.transfer(withdrawalAmount);
                    fundsByBidder[nftContract][chainTokenId][withdrawalAccount].withdrew = true;
                }
            } else if (withdrawer == auctionOwnership.owner) {            
                if (bidderMapping[nftContract][chainTokenId].length != 0) {
                    withdrawer.transfer(withdrawalAmount);
                }
            } else {
                require((withdrawalAmount != 0), "Withdraw amount cannot be zero.");
                withdrawer.transfer(withdrawalAmount);
                fundsByBidder[nftContract][chainTokenId][withdrawalAccount].withdrew = true;
            }

            fundsByBidder[nftContract][chainTokenId][withdrawalAccount].bid -= withdrawalAmount;

            emit LogWithdrawal(withdrawer, withdrawalAccount, withdrawalAmount);
        }
    }

    function getFundsByBidder(address nftContract, uint256 chainTokenId) public view returns (Bidder memory bidder) {
        return fundsByBidder[nftContract][chainTokenId][msg.sender];
    }

    function getAuction(address nftContract, uint256 chainTokenId) public view returns (AuctionData memory item) {
        AuctionData memory auctionItem = addressToAuctionTokenIdToData[nftContract][chainTokenId];
        return auctionItem;
    }

    function isOnAuction(address nftContract, uint256 chainTokenId) public view returns (bool onAuction) {
        return addressToAuctionTokenIdToData[nftContract][chainTokenId].onAuction;
    }

    function auctionHasEnded(address nftContract, uint256 chainTokenId) public view returns (bool ended) {
        uint256 endTime = addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime;
        bool canceled = addressToAuctionTokenIdToData[nftContract][chainTokenId].canceled;
        if ((block.timestamp >= endTime || canceled)) {
            return true;
        } else {
            return false;
        }
    }

    function returnBidders(address nftContract, uint256 chainTokenId) view public returns (Bidder[] memory) {
        address[] memory bidders = bidderMapping[nftContract][chainTokenId];
        Bidder[] memory biddersAlloc = new Bidder[](bidders.length);
    
        for (uint i = 0; i < bidders.length; i++) {
            biddersAlloc[i] = fundsByBidder[nftContract][chainTokenId][bidders[i]];
        }

        return biddersAlloc;
    }

    modifier onlyOwner(address nftContract, uint256 chainTokenId) {
        address owner = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId].owner;
        require((tx.origin == owner), "Only owner can call this function.");
        _;
    }

    modifier onlyNotOwner(address nftContract, uint256 chainTokenId) {
        address owner = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId].owner;
        require((tx.origin != owner), "The owner cannot call this function.");
        _;
    }

    modifier onlyAfterStart(address nftContract, uint256 chainTokenId) {
        uint256 startBlock = addressToAuctionTokenIdToData[nftContract][chainTokenId].startBlock;
        require(
            (block.timestamp > startBlock),
            "This function can only be called when the current block is greater than the start block."
        );
        _;
    }

    modifier onlyIfAbleToBid(address nftContract, uint256 chainTokenId) {
        AuctionData memory auction = addressToAuctionTokenIdToData[nftContract][chainTokenId];
        AuctionItem memory onwership = addressToAuctionTokenIdToOwnership[nftContract][chainTokenId];

        uint256 startBlock = auction.startBlock;
        uint256 endTime = auction.endTime;
        bool isCanceled = auction.canceled;
        address owner = onwership.owner;

        require((tx.origin != owner), "The owner cannot call this function.");
        require((!isCanceled), "This function can only be called when the auction is not cancelled.");
        require((block.timestamp > startBlock), "This function can only be called when the current block is greater than the start block.");
        require((block.timestamp < endTime), "This function can only be called before the auction ends.");
        _;
    }

    modifier onlyBeforeEnd(address nftContract, uint256 chainTokenId) {
        uint256 endTime = addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime;
        require(
            (block.timestamp < endTime),
            "This function can only be called before the auction ends."
        );
        _;
    }

    modifier onlyNotCanceled(address nftContract, uint256 chainTokenId) {
        bool isCanceled = addressToAuctionTokenIdToData[nftContract][chainTokenId].canceled;
        require(
            (!isCanceled),
            "This function can only be called when the auction is not cancelled."
        );
        _;
    }

    modifier onlyEndedOrCanceled(address nftContract, uint256 chainTokenId) {
        uint256 endTime = addressToAuctionTokenIdToData[nftContract][chainTokenId].endTime;
        bool isCanceled = addressToAuctionTokenIdToData[nftContract][chainTokenId].canceled;

        bool isDone;

        if ((block.timestamp >= endTime || isCanceled)) {
            isDone = true;
        } else {
            isDone = false;
        }

        require(
            (isDone == true),
            "This function can only be called after the auction has ended or is canceled."
        );
        _;
    }
}

