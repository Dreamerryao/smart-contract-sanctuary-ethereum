pragma solidity ^0.8.2;

import "./ethX.sol";
import "./SSV/ISSVNetwork.sol";
import "./ethDeposit.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "hardhat/console.sol";

contract StaderSSVManager is Initializable, OwnableUpgradeable {
    ETHX public ethX;
    uint256 public validatorCount;
    uint256 public registeredValidatorCount;
    IDepositContract public ethValidatorDeposit;
    ISSVNetwork public ssvNetwork;
    IERC20 public ssvToken;
    uint256 public minDeposit;
    uint256 public maxDeposit;
    uint256 public totalTVL;
    uint256 public constant decimals = 10**18;

    /// @notice event emitted after call function stake
    event Staked(address indexed to, uint256 EthReceived, uint256 ethXMinted);

    event addedToValidatorRegistry(
        bytes indexed pubKey,
        uint256 validatorCount
    );

    event registeredValidatortoSSVNetwork(
        bytes indexed pubKey,
        uint256 registerCount
    );

    event depositToDepositContract(bytes indexed pubKey);

    struct Validators {
        bytes pubKey;
        bytes[] publicShares;
        bytes[] encyptedShares;
        uint256[] operatorIDs;
    }

    mapping(uint256 => Validators) public validatorsRegistry;

    function initialize(
        ETHX _ethX,
        ISSVNetwork _ssvNetwork,
        IDepositContract _ethValidatorDeposit,
        IERC20 _ssvToken
    ) external initializer {
        __Ownable_init_unchained();
        ethX = _ethX;
        ssvNetwork = _ssvNetwork;
        ethValidatorDeposit = _ethValidatorDeposit;
        ssvToken = _ssvToken;
        validatorCount = 0;
        registeredValidatorCount = 0;
        minDeposit = 10;
        maxDeposit = 10 * 10**18;
        totalTVL = 0;
    }

    function stake() external payable {
        uint256 amount = msg.value;
        require(
            amount >= minDeposit && amount <= maxDeposit,
            "Deposit amount must be within valid range"
        );

        uint256 exchangeRate = getExchangeRate();

        uint256 amountToSend = (amount * decimals) / exchangeRate;

        ethX.mint(msg.sender, amountToSend);

        emit Staked(msg.sender, amount, amountToSend);
    }

    function addToValidatorRegistry(
        bytes memory _pubKey,
        bytes[] memory _publicShares,
        bytes[] memory _encyptedShares,
        uint256[] memory _operatorIDs
    ) public onlyOwner {
        validatorCount++;
        Validators storage _validatorRegistry = validatorsRegistry[
            validatorCount
        ];
        _validatorRegistry.pubKey = _pubKey;
        _validatorRegistry.publicShares = _publicShares;
        _validatorRegistry.encyptedShares = _encyptedShares;
        _validatorRegistry.operatorIDs = _operatorIDs;

        emit addedToValidatorRegistry(_pubKey, validatorCount);
    }

    function registerValidatortoSSVNetwork(uint256 tokenFees)
        external
        onlyOwner
    {
        require(
            registeredValidatorCount + 1 <= validatorCount,
            "registered Validator can not be more than the validator count"
        );
        registeredValidatorCount++;
        
        ssvToken.approve(address(ssvNetwork), tokenFees);

        ssvNetwork.registerValidator(
            validatorsRegistry[registeredValidatorCount].pubKey,
            validatorsRegistry[registeredValidatorCount].operatorIDs,
            validatorsRegistry[registeredValidatorCount].publicShares,
            validatorsRegistry[registeredValidatorCount].encyptedShares,
            tokenFees
        );

        emit registeredValidatortoSSVNetwork(
            validatorsRegistry[registeredValidatorCount].pubKey,
            registeredValidatorCount
        );
    }

    function depositEthtoDepositContract(
        bytes calldata pubKey,
        bytes calldata withdrawal_credentials,
        bytes calldata signature,
        bytes32 deposit_data_root
    ) external onlyOwner {
        require(
            address(this).balance >= 32 ether,
            "balance should be atleast 32 Eth"
        );
        ethValidatorDeposit.deposit(
            pubKey,
            withdrawal_credentials,
            signature,
            deposit_data_root
        );

        emit depositToDepositContract(pubKey);
    }

    function getExchangeRate() public view returns (uint256) {
        uint256 exchangeRate = 1 * decimals;
        uint256 tvl = getTVL();
        uint256 totalSupply = ethX.totalSupply();
        if (totalSupply == 0 || tvl == 0) {
            return exchangeRate;
        } else {
            exchangeRate = (tvl * decimals) / totalSupply;
        }
        return exchangeRate;
    }

    function getTVL() internal view returns (uint256) {
        return totalTVL;
    }

    function setTVL(uint256 _totalTVL) external onlyOwner {
        totalTVL = _totalTVL;
    }

    function updateMinDeposit(uint256 _minDeposit) external onlyOwner {
        require(_minDeposit > 0, "minimum deposit should be greater than 0");
        minDeposit = _minDeposit;
    }

    function updateMaxDeposit(uint256 _maxDeposit) external onlyOwner {
        require(
            _maxDeposit > minDeposit,
            "maximum deposit should be greater than minimum deposit"
        );
        maxDeposit = _maxDeposit;
    }

    function updateEthXAddress(ETHX _ethX)
        external
        checkZeroAddress(address(_ethX))
        onlyOwner
    {
        ethX = _ethX;
    }

    function updatessvNetworkAddress(ISSVNetwork _ssvNetwork)
        external
        checkZeroAddress(address(_ssvNetwork))
        onlyOwner
    {
        ssvNetwork = _ssvNetwork;
    }

    modifier checkZeroAddress(address _address) {
        require(_address != address(0), "Address cannot be zero");
        _;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.2;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

/**
 * @title ethX Contract
 * @author Stader Labs
 * @notice The ERC20 contract for the ethX token
 */
contract ETHX is ERC20, ERC20Burnable, AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor() ERC20("ETHX", "ETHX") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
    }

    /**
     * @notice Mints ethX when called by an authorized caller
     * @param to the account to mint to
     * @param amount the amount of ethX to mint
     */
    function mint(address to, uint256 amount) public onlyRole(MINTER_ROLE) {
        _mint(to, amount);
    }

    /**
     * @notice Burns ethX when called by an authorized caller
     * @param account the account to burn from
     * @param amount the amount of ethX to burn
     */
    function burnFrom(address account, uint256 amount)
        public
        override
        onlyRole(MINTER_ROLE)
    {
        _burn(account, amount);
    }

    function setMinterRole(address _minterRole) external onlyRole(MINTER_ROLE) {
        _grantRole(MINTER_ROLE, _minterRole);
        approve(_minterRole, type(uint256).max);
    }
}

