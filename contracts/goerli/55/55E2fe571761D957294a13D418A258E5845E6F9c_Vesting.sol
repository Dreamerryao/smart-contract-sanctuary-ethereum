import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777.sol";
import "@openzeppelin/contracts/utils/introspection/IERC1820Registry.sol";
import "hardhat/console.sol";

// Errors
error NoTokensToClaim();
error BeneficiaryAddressMustNotBeZero();
error MustBeM2Token();

contract Vesting is ReentrancyGuard {
    uint256 private immutable start;
    uint256 public sharesDenominator;

    mapping(address => uint256) private beneficiaryShares;
    mapping(address => uint256) private beneficiaryClaimedAmount;

    IERC777 public immutable token;

    struct VestingCheckpoint {
        uint256 intervalInSeconds;
        uint256 cumulativeVestedAmount;
    }

    VestingCheckpoint[] vestingSchedule;

    // Events
    event Claimed(address indexed beneficiary, uint256 amount);

    // keccak256('ERC777TokensRecipient')
    bytes32 private constant TOKENS_RECIPIENT_INTERFACE_HASH =
        0xb281fc8c12954d22544db45de3159a39272895b169a852b314f9cc762e44c53b;
    // Shared registry address
    IERC1820Registry private registry =
        IERC1820Registry(0x1820a4B7618BdE71Dce8cdc73aAB6C95905faD24);

    // example inputs to constructor:
    // tokenAddress               :       0x1234...                               (address of token)
    // _start                     :       12345678                                (unix timestamp for when vesting schedule starts)
    // beneficiaries              :       [address1, address2]                    (array of addresses)
    // _beneficiaryShares         :       [20,50]                                 (relative weights of how many tokens each address can claim)
    // vestingCheckpoints         :       [
    //                                         [0,0],
    //                                         [3600*24*30*18,10000],
    //                                         [3600*24*30*24,15000],
    //                                         [3600*24*30*27,23000],
    //                                         [3600*24*30*30,40000],
    //                                         [3600*24*30*48,75000]
    //                                     ]                                      (an array of structs defining the vesting schedule.
    //                                                                             First element in individual struct is the time checkpoint in seconds
    //                                                                             Second element in individual struct is the cumulative tokens released
    //                                                                             Note that the first {0,0} struct is needed)
    //
    //                                                                            E.g. 15000 tokens released after 24 months. address1 can claim 20/70 of this)

    constructor(
        address tokenAddress,
        uint256 _start,
        address[] memory beneficiaries,
        uint256[] memory _beneficiaryShares,
        VestingCheckpoint[] memory vestingCheckpoints
    ) {
        // Set ERC1820 regsitry
        registry.setInterfaceImplementer(
            address(this),
            TOKENS_RECIPIENT_INTERFACE_HASH,
            address(this)
        );

        token = IERC777(tokenAddress);
        start = block.timestamp + _start;

        for (uint256 i = 0; i < beneficiaries.length; i++) {
            if (beneficiaries[i] == address(0)) {
                revert BeneficiaryAddressMustNotBeZero();
            }
            beneficiaryShares[beneficiaries[i]] = _beneficiaryShares[i];
            sharesDenominator += _beneficiaryShares[i];
        }

        for (uint256 i = 0; i < vestingCheckpoints.length; i++) {
            vestingSchedule.push(vestingCheckpoints[i]);
        }
    }

    /**
     * @dev Claims all vested tokens for msg.sender
     */
    function claim() external {
        _claim(msg.sender);
    }

    /**
     * @dev Claims all vested tokens for a specified account
     */
    function claimFor(address beneficiary) external {
        _claim(beneficiary);
    }

    /**
     * @dev Fetches the amount of claimable tokens for a specified account
     */
    function claimable(address beneficiary) public view returns (uint256) {
        return (((_totalVested(block.timestamp) *
            beneficiaryShares[beneficiary]) / sharesDenominator) -
            beneficiaryClaimedAmount[beneficiary]);
    }

    function _claim(address beneficiary) internal nonReentrant {
        uint256 amount = claimable(beneficiary);

        if (amount <= 0) {
            revert NoTokensToClaim();
        }

        beneficiaryClaimedAmount[beneficiary] += amount;
        token.send(beneficiary, amount, "");
        emit Claimed(beneficiary, amount);
    }

    /**
     * @dev Defines the vesting schedule for the contract. This is the cumulative vested amount for all beneficiaries, regardless of how many have already been claimed.
     */
    function _totalVested(uint256 currentTimestamp)
        internal
        view
        returns (uint256)
    {
        uint256 seconds_elapsed = currentTimestamp - start;

        for (uint256 i = vestingSchedule.length - 1; i > 0; --i) {
            if (seconds_elapsed >= vestingSchedule[i].intervalInSeconds) {
                return vestingSchedule[i].cumulativeVestedAmount;
            }
        }

        return 0;
    }

    /**
     * @dev Transfer hook for ERC777 tokens. Does not need implementation
     */
    function tokensReceived(
        address,
        address,
        address,
        uint256,
        bytes calldata,
        bytes calldata
    ) external view {
        if (msg.sender != address(token)) {
            revert MustBeM2Token();
        }
    }
}

