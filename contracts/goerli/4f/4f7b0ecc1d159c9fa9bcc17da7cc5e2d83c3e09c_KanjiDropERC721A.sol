        uint256 totalPrice = _quantityToClaim * _pricePerToken;

        if (msg.value != totalPrice) {
            revert IncorrectMsgValue({expected: totalPrice});
        }

        address[] memory recipients = primarySaleRecipients();

        for (uint256 i = 0; i < recipients.length; i++) {
            uint256 payout = ((totalPrice) * primarySaleRecipientBps(recipients[i])) / MAX_BPS;
