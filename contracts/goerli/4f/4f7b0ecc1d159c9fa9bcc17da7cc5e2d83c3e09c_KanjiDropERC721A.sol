                quantity: _quantity,
                nextTokenIdToClaim: _nextTokenId(),
                nextTokenIdToMint: nextTokenIdToLazyMint
            });
        }
        if (maxSupply != 0 && _nextTokenId() + _quantity > maxSupply) {
            revert ExceedMaxSupplyError({quantity: _quantity, maxSupply: maxSupply, nextTokenIdToClaim: _nextTokenId()});
        }
        if (maxWalletClaimCount != 0 && walletClaimCount[_claimer] + _quantity > maxWalletClaimCount) {
            revert ExceedClaimLimitForWalletError({
                quantity: _quantity,
