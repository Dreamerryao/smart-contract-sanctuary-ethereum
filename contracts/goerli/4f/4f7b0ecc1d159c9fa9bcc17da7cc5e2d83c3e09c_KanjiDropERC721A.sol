import "./lib/CurrencyTransferLib.sol";

//  ==========  Features    ==========

import "./extension/RoyaltySplitter.sol";
import "./extension/PrimarySale.sol";
import "./extension/DelayedReveal.sol";
import "./extension/LazyMint.sol";
import "./extension/Drop.sol";
import "./extension/WalletClaimCounter.sol";
import "./extension/Oneshot.sol";

contract KanjiDropERC721A is
    WalletClaimCounter,
    RoyaltySplitter,
    PrimarySale,
    Ownable,
    DelayedReveal,
    LazyMint,
    Drop,
    ReentrancyGuard,
    Pausable,
    Oneshot,
    ERC721A
{
    using Strings for uint256;

    /*///////////////////////////////////////////////////////////////
                                Errors
    //////////////////////////////////////////////////////////////*/

    error NotEnoughMintedTokensError(uint256 quantity, uint256 nextTokenIdToClaim, uint256 nextTokenIdToMint);

    error ExceedMaxSupplyError(uint256 quantity, uint256 maxSupply, uint256 nextTokenIdToClaim);

    error ExceedClaimLimitForWalletError(
        uint256 quantity, uint256 walletClaimCountClaimer, address claimer, uint256 maxWalletClaimCount
    );

    error PausedError();

    error IncorrectMsgValue(uint256 expected);
    /*///////////////////////////////////////////////////////////////
                                Events
    //////////////////////////////////////////////////////////////*/

    event MaxSupplyUpdated(uint256 maxSupply);

    /*///////////////////////////////////////////////////////////////
                            State variables
    //////////////////////////////////////////////////////////////*/

    /// @dev Max bps in the thirdweb system.
    uint256 private constant MAX_BPS = 10_000;

    /// @dev Global max total supply of NFTs.
    uint256 public immutable maxSupply;

    /*///////////////////////////////////////////////////////////////
                    Constructor + initializer logic
    //////////////////////////////////////////////////////////////*/

    /// @notice Creates a new `KanjiDropERC721A` contract.
    /// @param _name The name of the NFT.
    /// @param _symbol The symbol of the NFT.
    /// @param _maxSupply The maximum supply of the collection
    /// @param _metadata Contract-level metadata
    /// @param _saleRecipients Recipients of the sale
    /// @param _royaltyBps The ERC2981 royalty cut
    /// @param _royaltyRecipients The ERC2981 royalty recipients
    constructor(
        string memory _name,
        string memory _symbol,
        uint256 _maxSupply,
        Metadata memory _metadata,
        PrimarySaleRecipient[] memory _saleRecipients,
        uint16 _royaltyBps,
        RoyaltyRecipient[] memory _royaltyRecipients
    )
        PrimarySale(_saleRecipients)
        RoyaltySplitter(_royaltyRecipients, _metadata, _royaltyBps)
        ERC721A(_name, _symbol)
        ReentrancyGuard()
        Ownable()
        Pausable()
    {
        maxSupply = _maxSupply;
    }

    /*///////////////////////////////////////////////////////////////
                        ERC 165 / 721 / 2981 logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Returns the URI for a given tokenId.

    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        if (isEncrypted) {
            return flaskURI;
        } else {
            return string(abi.encodePacked(baseURI, _tokenId.toString(), ".json"));
        }
    }

    /// @dev See ERC 165
    function supportsInterface(bytes4 interfaceId) public view virtual override (ERC721A, IERC165) returns (bool) {
        return super.supportsInterface(interfaceId) || type(IERC2981).interfaceId == interfaceId;
    }

    /*///////////////////////////////////////////////////////////////
                    Lazy minting + delayed-reveal logic
    //////////////////////////////////////////////////////////////*/

    function lazyMint(uint256 _amount, string calldata _baseURI, string calldata _flaskURI) public onlyOwner oneShot {
        if (_amount == 0) revert AmountCantBeZeroError();
        if (bytes(_baseURI).length == 0) {
            revert BaseURIRequiredError();
        }
        baseURI = _baseURI;

        if (bytes(_flaskURI).length != 0) {
            isEncrypted = true;
            flaskURI = _flaskURI;
        }

        super.lazyMint(_amount);
    }

    /// @dev Lets an owner reveal the URI for a batch of 'delayed-reveal' NFTs.
    function reveal(bytes calldata _key) external onlyOwner oneShot returns (string memory revealedURI) {
        if (!isEncrypted) revert NothingToReveal();
        revealedURI = string(encryptDecrypt(bytes(baseURI), _key));
        baseURI = revealedURI;
        isEncrypted = false;

        emit URIRevealed(revealedURI);
    }

    /// @notice owner mint a reserve of tokens to specific address
    /// @param receiver receiver of mint
    /// @param quantity quantity gonna be mint
    function mintOwner(address receiver, uint256 quantity) external onlyOwner {
        if (_nextTokenId() + quantity > maxSupply) revert ExceedMaxSupplyError(quantity, maxSupply, _nextTokenId());
        super.lazyMint(quantity);
        _safeMint(receiver, quantity);
    }

    /// @notice owner mint a one token for every address in the list
    /// Be aware of the order, as the tokenID depends on it
    /// @param recipients receiver of mint
    function batchMintOwner(address[] memory recipients) external onlyOwner {
        if (_nextTokenId() + recipients.length > maxSupply) {
            revert ExceedMaxSupplyError(recipients.length, maxSupply, _nextTokenId());
        }
        super.lazyMint(recipients.length);
        for (uint256 i = 0; i < recipients.length; i++) {
            _safeMint(recipients[i], 1);
        }
    }

