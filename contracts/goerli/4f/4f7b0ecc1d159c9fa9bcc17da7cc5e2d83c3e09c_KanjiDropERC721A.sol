pragma solidity ^0.8.16;

import "./interface/IContractMetadata.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

abstract contract ContractMetadata is IContractMetadata {
    Metadata private metadata;
    uint16 internal royaltyBps;
    address private royaltyReciever_;
    bool private initialized;

    error InvalidRoyaltyBpsError(uint16 royaltyBps);
    error AlreadyInitializedError();

    function _ContractMetadata_initialize(address _royaltyReciever, Metadata memory _metadata, uint16 _royaltyBps)
        internal
    {
        if (initialized) revert AlreadyInitializedError();
        if (_royaltyBps > 10000) {
            revert InvalidRoyaltyBpsError(_royaltyBps);
        }
        metadata = _metadata;
        royaltyBps = _royaltyBps;
        royaltyReciever_ = _royaltyReciever;
        initialized = true;
    }

    function contractURI() public view override returns (string memory) {
        bytes memory data = abi.encodePacked(
            "{",
            '"name": "',
            metadata.name,
            '",',
            '"description": "',
            metadata.description,
            '",',
            '"image": "',
            metadata.imageURI,
            '",',
            '"seller_fee_basis_points": "',
            Strings.toString(uint256(royaltyBps)),
            '",',
            '"fee_recipient": "',
            Strings.toHexString(royaltyReciever()),
            '"',
            "}"
        );
        return string(abi.encodePacked("data:application/json;base64,", Base64.encode(data)));
    }

    function royaltyReciever() internal view returns (address) {
        if (royaltyReciever_ == address(0)) return address(this);
        return royaltyReciever_;
    }
}

