        walletClaimCount[_msgSender()] += _quantityBeingClaimed;
        _safeMint(_to, _quantityBeingClaimed);
    }

    /*///////////////////////////////////////////////////////////////
                        Miscellaneous
    //////////////////////////////////////////////////////////////*/

    /**
     * Returns the total amount of tokens minted in the contract.
