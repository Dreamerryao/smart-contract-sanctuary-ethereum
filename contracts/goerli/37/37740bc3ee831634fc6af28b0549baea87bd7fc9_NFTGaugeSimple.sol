// ====================================================================
// =========================== NFTGauge ==========================
// ====================================================================
// Farming contract that accounts for NFTs
// Only one possible reward token here (usually IQ), to cut gas costs
// Also, because of the nonfungible nature, and to reduce gas, unlocked staking was removed
// You can lock for as short as 1 day now, which is de-facto an unlocked stake

// Everipedia: https://github.com/EveripediaNetwork
// Frax Finance: https://github.com/FraxFinance

// Primary Author(s)
// Travis Moore: https://github.com/FortisFortuna
// Cesar Rodriguez: https://github.com/kesar

// Reviewer(s) / Contributor(s)
// Jason Huan: https://github.com/jasonhuan
// Sam Kazemian: https://github.com/samkazemian

// Originally inspired by Synthetix.io, but heavily modified by the Frax & Everipedia team
// (Locked, hiIQ, and NFTs portions are new)
// https://raw.githubusercontent.com/Synthetixio/synthetix/develop/contracts/StakingRewards.sol

import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import {Owned} from "@rari-capital/solmate/src/auth/Owned.sol";
import {IGaugeController} from "./IGaugeController.sol";
import {TransferHelper} from "../Utils/TransferHelper.sol";
import {console} from "../../lib/forge-std/src/console.sol";

interface GaugeERC20 {
    function balanceOf(address account) external view returns (uint256);
}

interface GaugeERC721 {
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;
}

interface IGaugeRewardsDistributor {
    function distributeReward(address gauge_address) external returns (uint256 weeks_elapsed, uint256 reward_tally);
}

contract NFTGaugeSimple is Owned {
    using SafeMath for uint256;

    /// -----------------------------------------------------------------------
    /// Errors
    /// -----------------------------------------------------------------------

    error Not_Owner_Or_Curator();
    error Invalid_Data_Length();
    error Mult_Must_Be_Higher_Than_Multiplier_Precision();
    error Lock_Time_Max_Multiplier_Must_Be_Higher_Than_1();
    error Lock_Time_Min_Must_Be_Higher_Than_1();
    error Withdrawals_Paused();
    error Staking_Paused();
    error Rewards_Paused();
    error Minimum_Stake_Time_Not_Met();
    error Trying_To_Stake_For_Too_Long();
    error Token_Id_Not_Found();
    error Period_Has_Not_Expired_Yet();
    error Not_Enough_IQ_Available();
    error Stake_Still_Locked();

    /// -----------------------------------------------------------------------
    /// Events
    /// -----------------------------------------------------------------------

    event LockNFT(address indexed user, uint256 token_id, uint256 secs, address source_address);
    event WithdrawLocked(address indexed user, uint256 token_id, address destination_address);
    event RewardPaid(address indexed user, uint256 farm_reward, address token_address, address destination_address);
    event RecoveredGaugeERC20(address token, uint256 amount);
    event RecoveredERC721(address token, uint256 token_id);
    event RewardsPeriodRenewed(address token);
    event LockedNFTMaxMultiplierUpdated(uint256 multiplier);
    event LockedNFTTimeForMaxMultiplier(uint256 secs);
    event LockedNFTMinTime(uint256 secs);

    /// -----------------------------------------------------------------------
    /// Storage variables
    /// -----------------------------------------------------------------------

    IGaugeController public gaugeController;
    IGaugeRewardsDistributor public rewardsDistributor;
    // Admin addresses
    address public curator;
    // Reward and period related
    uint256 private periodFinish;
    uint256 private lastUpdateTime;
    uint256 private rewardRateManual;
    // Lock time and multiplier settings
    uint256 public lock_max_multiplier = 3e18; // E18. 1x = 1e18
    uint256 public lock_time_for_max_multiplier = 1095 days; // 3 years
    uint256 public lock_time_min = 1 days;
    // Rewards tracking
    uint256 private rewardPerTokenStored0;
    mapping(address => uint256) private userRewardPerTokenPaid;
    mapping(address => uint256) private rewards0;
    uint256 private last_gauge_relative_weight;
    uint256 private last_gauge_time_total;
    // Balance, and weight tracking
    uint256 private totalCombinedMultipliers;
    uint256 public _totalNftsLocked;
    mapping(address => uint256) private combinedMultipliers;
    mapping(address => LockedNFT[]) private lockedNFTs;
    // Admin booleans for emergencies, migrations, and overrides
    bool public stakesUnlocked; // Release locked stakes in case of system migration or emergency
    bool public stakingPaused;
    bool public withdrawalsPaused;
    bool public rewardsCollectionPaused;
    // Struct for the stake
    struct LockedNFT {
        uint256 token_id;
        uint256 start_timestamp;
        uint256 ending_timestamp;
        uint256 lock_multiplier; // 6 decimals of precision. 1x = 1000000
    }

    /// -----------------------------------------------------------------------
    /// Immutable parameters
    /// -----------------------------------------------------------------------

    // Instances
    address public immutable rewardsToken;
    address public immutable nftContract;

    uint256 private constant MULTIPLIER_PRECISION = 1e18;
    uint256 public constant rewardsDuration = 7 days;

    /// -----------------------------------------------------------------------
    /// Modifiers
    /// -----------------------------------------------------------------------

    modifier onlyByOwnerOrCurator() {
        if (msg.sender != owner && msg.sender != curator) {
            revert Not_Owner_Or_Curator();
        }
        _;
    }

    modifier updateRewardAndBalance(address account, bool sync_too) {
        _updateRewardAndBalance(account, sync_too);
        _;
    }

    /// -----------------------------------------------------------------------
    /// Constructor
    /// -----------------------------------------------------------------------

    constructor(
        address _owner,
        address _nft_address,
        address _rewards_distributor_address,
        address _rewards_token_address,
        address _gauge_controller_address
    ) Owned(_owner) {
        rewardsDistributor = IGaugeRewardsDistributor(_rewards_distributor_address);
        rewardRateManual = 0;
        gaugeController = IGaugeController(_gauge_controller_address);

        // Initialize
        lastUpdateTime = block.timestamp;
        periodFinish = block.timestamp.add(rewardsDuration);
        rewardsToken = _rewards_token_address;
        nftContract = _nft_address;
    }

    /// -----------------------------------------------------------------------
    /// External functionsperiodFinish
    /// -----------------------------------------------------------------------

    // Two different getReward functions are needed because of delegateCall and msg.sender issues (important for migration)
    function getReward() external returns (uint256) {
        if (rewardsCollectionPaused) {
            revert Rewards_Paused();
        }

        console.log("BLOCK TIMESTAMP", block.timestamp);
        console.log("PERIOD FINISH", periodFinish);
        
        return _getReward(msg.sender, msg.sender);
    }

    // Two different withdrawLocked functions are needed because of delegateCall and msg.sender issues (important for migration)
    function withdrawLocked(uint256 token_id) external {
        if (withdrawalsPaused) {
            revert Withdrawals_Paused();
        }

        _withdrawLocked(msg.sender, msg.sender, token_id);
    }

    // only owner

    function setMultipliers(uint256 _lock_max_multiplier) external onlyOwner {
        if (_lock_max_multiplier < MULTIPLIER_PRECISION) {
            revert Mult_Must_Be_Higher_Than_Multiplier_Precision();
        }

        lock_max_multiplier = _lock_max_multiplier;

        emit LockedNFTMaxMultiplierUpdated(lock_max_multiplier);
    }

    function setLockedNFTTimeForMinAndMaxMultiplier(uint256 _lock_time_for_max_multiplier, uint256 _lock_time_min)
        external
        onlyOwner
    {
        if (_lock_time_for_max_multiplier < 1) {
            revert Lock_Time_Max_Multiplier_Must_Be_Higher_Than_1();
        }

        if (_lock_time_min < 1) {
            revert Lock_Time_Min_Must_Be_Higher_Than_1();
        }

        lock_time_for_max_multiplier = _lock_time_for_max_multiplier;
        lock_time_min = _lock_time_min;

        emit LockedNFTTimeForMaxMultiplier(lock_time_for_max_multiplier);
        emit LockedNFTMinTime(_lock_time_min);
    }

    function unlockStakes() external onlyOwner {
        stakesUnlocked = !stakesUnlocked;
    }

    function setManualRewardRate(uint256 _reward_rate_manual, bool sync_too) external onlyOwner {
        rewardRateManual = _reward_rate_manual;

        if (sync_too) {
            sync();
        }
    }

    function setCurator(address _new_curator) external onlyOwner {
        curator = _new_curator;
    }

    function setGaugeRelatedAddrs(address _gauge_controller_address, address _rewards_distributor_address)
        external
        onlyOwner
    {
        gaugeController = IGaugeController(_gauge_controller_address);
        rewardsDistributor = IGaugeRewardsDistributor(_rewards_distributor_address);
    }

    function setPauses(
        bool _stakingPaused,
        bool _withdrawalsPaused,
        bool _rewardsCollectionPaused
    ) external onlyByOwnerOrCurator {
        stakingPaused = _stakingPaused;
        withdrawalsPaused = _withdrawalsPaused;
        rewardsCollectionPaused = _rewardsCollectionPaused;
    }

    // Added to support recovering LP Rewards and other mistaken tokens from other systems to be distributed to holders
    function recoverGaugeERC20(address tokenAddress, uint256 tokenAmount) external onlyOwner {
        TransferHelper.safeTransfer(tokenAddress, owner, tokenAmount);
        emit RecoveredGaugeERC20(tokenAddress, tokenAmount);
    }

    // Added to support recovering NFTs and other mistaken tokens from other systems to be distributed to holders
    function recoverERC721(
        address tokenAddress,
        uint256 token_id,
        address destination
    ) external onlyOwner {
        GaugeERC721(tokenAddress).safeTransferFrom(address(this), destination, token_id);
        emit RecoveredERC721(tokenAddress, token_id);
    }

    /// -----------------------------------------------------------------------
    /// Public functions
    /// -----------------------------------------------------------------------

    function onERC721Received(
        address,
        address from,
        uint256 tokenId,
        bytes memory data
    ) public returns (bytes4) {
        if (msg.sender == nftContract) {
            uint256 secs = bytesToUint(data);
            _stakeLocked(from, from, tokenId, secs, block.timestamp);
        }
        return this.onERC721Received.selector;
    }

    function sync_gauge_weight(bool force_update) public {
        if (force_update || (block.timestamp > last_gauge_time_total)) {
            // Update the gauge_relative_weight
            last_gauge_relative_weight = gaugeController.gauge_relative_weight_write(address(this), block.timestamp);
            last_gauge_time_total = gaugeController.time_total();
        }
    }

    function sync() public {
        sync_gauge_weight(false);

        if (block.timestamp > periodFinish) {
            retroCatchUp();
        } else {
            uint256 rewardPerToken_0 = rewardPerToken();
            rewardPerTokenStored0 = rewardPerToken_0;
            lastUpdateTime = lastTimeRewardApplicable();
        }
    }

    /// -----------------------------------------------------------------------
    /// Internal functions
    /// -----------------------------------------------------------------------

    function _updateRewardAndBalance(address account, bool sync_too) internal {
        // Need to retro-adjust some things if the period hasn't been renewed, then start a new one
        if (sync_too) {
            sync();
        }

        if (account != address(0)) {
            (uint256 old_combined_multiplier, uint256 combined_multiplier) = calculateCombinedMultiplier(account);

            uint256 earned0 = earned(account);
            rewards0[account] = earned0;
            userRewardPerTokenPaid[account] = rewardPerTokenStored0;

            if (combined_multiplier >= old_combined_multiplier) {
                uint256 multipliers_diff = combined_multiplier.sub(old_combined_multiplier);
                totalCombinedMultipliers = totalCombinedMultipliers.add(multipliers_diff);
                combinedMultipliers[account] = old_combined_multiplier.add(multipliers_diff);
                // console.log("========================================");
                // console.log("ACCOUNT", account);
                // console.log("COMBINED MULTIPLIER", combined_multiplier);
                // console.log("OLD COMBINED MULTIPLIER", old_combined_multiplier);
                // console.log("MULTIPLIERS DIFF", multipliers_diff);
                // console.log("ACCOUNT MULTIPLIER", combinedMultipliers[account]);
                // console.log("========================================");
            } else {
                uint256 multipliers_diff = old_combined_multiplier.sub(combined_multiplier);
                totalCombinedMultipliers = totalCombinedMultipliers.sub(multipliers_diff);
                combinedMultipliers[account] = old_combined_multiplier.sub(multipliers_diff);
            }
        }
    }

    function _stakeLocked(
        address stakerAddress,
        address source_address,
        uint256 token_id,
        uint256 secs,
        uint256 start_timestamp
    ) internal updateRewardAndBalance(stakerAddress, true) {
        if (stakingPaused) {
            revert Staking_Paused();
        }

        if (secs < lock_time_min) {
            revert Minimum_Stake_Time_Not_Met();
        }

        if (secs > lock_time_for_max_multiplier) {
            revert Trying_To_Stake_For_Too_Long();
        }

        uint256 lock_multiplier = lockMultiplier(secs);
        // console.log("SECS", secs);

        lockedNFTs[stakerAddress].push(
            LockedNFT(token_id, start_timestamp, start_timestamp.add(secs), lock_multiplier)
        );

        // Update totals
        _totalNftsLocked++;

        // Need to call again to make sure everything is correct
        _updateRewardAndBalance(stakerAddress, false);

        emit LockNFT(stakerAddress, token_id, secs, source_address);
    }

    // No withdrawer == msg.sender check needed since this is only internally callable and the checks are done in the wrapper
    // functions like migrator_withdraw_locked() and withdrawLocked()
    function _withdrawLocked(
        address stakerAddress,
        address destination_address,
        uint256 token_id
    ) internal {
        // Collect rewards first and then update the balances
        _getReward(stakerAddress, destination_address);

        LockedNFT memory thisNFT;
        uint256 theArrayIndex;
        uint256 lockedNFTsLength = lockedNFTs[stakerAddress].length;
        for (uint256 i = 0; i < lockedNFTsLength; i++) {
            if (token_id == lockedNFTs[stakerAddress][i].token_id) {
                thisNFT = lockedNFTs[stakerAddress][i];
                theArrayIndex = i;
                break;
            }
        }
        if (thisNFT.token_id != token_id) {
            revert Token_Id_Not_Found();
        }

        if (block.timestamp < thisNFT.ending_timestamp && stakesUnlocked == false) {
            revert Stake_Still_Locked();
        }

        _totalNftsLocked--;

        // Remove the stake from the array
        uint256 stakerIdx = lockedNFTs[stakerAddress].length - 1;
        lockedNFTs[stakerAddress][theArrayIndex] = lockedNFTs[stakerAddress][stakerIdx];
        lockedNFTs[stakerAddress].pop();

        // Need to call again to make sure everything is correct
        _updateRewardAndBalance(stakerAddress, false);

        // Give the tokens to the destination_address
        GaugeERC721(nftContract).safeTransferFrom(address(this), destination_address, token_id);

        emit WithdrawLocked(stakerAddress, token_id, destination_address);
    }

    // No withdrawer == msg.sender check needed since this is only internally callable
    // This distinction is important for the migrator
    function _getReward(address rewardee, address destination_address)
        internal
        updateRewardAndBalance(rewardee, true)
        returns (uint256 reward_0)
    {
        reward_0 = rewards0[rewardee];
        console.log("REWARD 0", reward_0);
        if (reward_0 > 0) {
            rewards0[rewardee] = 0;
            TransferHelper.safeTransfer(rewardsToken, destination_address, reward_0);
            emit RewardPaid(rewardee, reward_0, rewardsToken, destination_address);
        }
    }

    // If the period expired, renew it
    function retroCatchUp() internal {
        // Failsafe check
        if (block.timestamp <= periodFinish) {
            revert Period_Has_Not_Expired_Yet();
        }

        rewardsDistributor.distributeReward(address(this));

        // Ensure the provided reward amount is not more than the balance in the contract.
        // This keeps the reward rate in the right range, preventing overflows due to
        // very high values of rewardRate in the earned and rewardsPerToken functions;
        // Reward + leftover must be less than 2^256 / 10^18 to avoid overflow.
        uint256 numPeriodsElapsed = uint256(block.timestamp.sub(periodFinish)) / rewardsDuration;
        // Floor division to the nearest period
        uint256 balance0 = GaugeERC20(rewardsToken).balanceOf(address(this));
        if (rewardRate0().mul(rewardsDuration).mul(numPeriodsElapsed + 1) > balance0) {
            revert Not_Enough_IQ_Available();
        }

        periodFinish = periodFinish.add((numPeriodsElapsed.add(1)).mul(rewardsDuration));
        uint256 rewardPerToken_0 = rewardPerToken();
        rewardPerTokenStored0 = rewardPerToken_0;
        lastUpdateTime = lastTimeRewardApplicable();

        emit RewardsPeriodRenewed(nftContract);
    }

    function lastTimeRewardApplicable() internal view returns (uint256) {
        return min(block.timestamp, periodFinish);
    }

    function rewardPerToken() internal view returns (uint256) {
        // console.log("||||||||");
        // console.log("NFTS LOCKED", _totalNftsLocked);
        // console.log("TOTAL MULTIPLIERS", totalCombinedMultipliers);
        if (_totalNftsLocked == 0 || totalCombinedMultipliers == 0) {
            return rewardPerTokenStored0;
        } else {
            // console.log("LT RA", lastTimeRewardApplicable);
            // console.log("BLOCK TIMESTAMP", block.timestamp);
            // console.log("PERIOD FINISH", periodFinish);
            return (
                rewardPerTokenStored0.add(
                    lastTimeRewardApplicable().sub(lastUpdateTime).mul(rewardRate0()).mul(MULTIPLIER_PRECISION).div(
                        totalCombinedMultipliers
                    ) // has 1e18 already
                )
            );
        }
    }

    function bytesToUint(bytes memory data) internal pure returns (uint256) {
        if (data.length > 32) {
            revert Invalid_Data_Length();
        }

        uint256 result = 0;
        for (uint256 index = 0; index < data.length; index++) {
            result = result << 8;
            result += uint8(data[index]);
        }
        return result;
    }

    /// -----------------------------------------------------------------------
    /// Getter/Views functions
    /// -----------------------------------------------------------------------

    function totalNFTsLocked() external view returns (uint256) {
        return _totalNftsLocked;
    }

    function lockedNFTsOf(address account) external view returns (LockedNFT[] memory) {
        return lockedNFTs[account];
    }

    function getRewardForDuration() external view returns (uint256) {
        return rewardRate0().mul(rewardsDuration);
    }

    function calculateCombinedMultiplier(address account)
        public
        view
        returns (uint256 old_combined_multiplier, uint256 combined_multiplier)
    {
        // Get the old combined weight
        old_combined_multiplier = combinedMultipliers[account];

        combined_multiplier = 0;
        for (uint256 i = 0; i < lockedNFTs[account].length; i++) {
            LockedNFT memory thisNFT = lockedNFTs[account][i];
            uint256 lock_multiplier = thisNFT.lock_multiplier;

            // If the lock period is over, drop the lock multiplier down to 1x for the weight calculations
            // if (thisNFT.ending_timestamp <= block.timestamp) {
            //     lock_multiplier = MULTIPLIER_PRECISION;
            // }
            // console.log("!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!");
            // console.log("ACCOUNT", account);
            // console.log("COMBINED MULTIPLIER BEFORE", combined_multiplier / 1e18);

            combined_multiplier = combined_multiplier.add(lock_multiplier);
            // console.log("COMBINED MULTIPLIER AFTER", combined_multiplier / 1e18);
            // console.log("!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!");
        }
    }

    function lockMultiplier(uint256 secs) public view returns (uint256) {
        uint256 lock_multiplier = uint256(MULTIPLIER_PRECISION).add(
            secs.mul(lock_max_multiplier.sub(MULTIPLIER_PRECISION)).div(lock_time_for_max_multiplier)
        );


        if (lock_multiplier > lock_max_multiplier) lock_multiplier = lock_max_multiplier;
        return lock_multiplier;
    }

    function userStakedNFTs(address account) public view returns (uint256) {
        return lockedNFTs[account].length;
    }

    function earned(address account) public view returns (uint256) {

        uint256 _rewardPerToken = rewardPerToken();

        // console.log("USER REWARD PER TOKEN", userRewardPerTokenPaid[account] / 1e18);
        // console.log("REWARDS", rewards0[account]);
        // console.log("REWARD PT", _rewardPerToken / 1e18);

        // console.log("###########################");
        // console.log("PERIOD FINISH", periodFinish);
        // console.log("###########################");

        return (
            combinedMultipliers[account].mul(_rewardPerToken.sub(userRewardPerTokenPaid[account])).div(1e18).add(
                rewards0[account]
            )
        );
    }

    function rewardRate0() public view returns (uint256 rwdRate) {
        if (address(gaugeController) != address(0)) {
            rwdRate = (gaugeController.global_emission_rate()).mul(last_gauge_relative_weight).div(1e18);
        } else {
            rwdRate = rewardRateManual;
        }
    }

    function min(uint256 a, uint256 b) internal pure returns (uint256) {
        return a < b ? a : b;
    }
}

