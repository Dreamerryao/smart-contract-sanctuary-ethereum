/// @title Tick
/// @notice Contains functions for managing tick processes and relevant calculations
library Tick {
    using TickBitmap for mapping(int16 => uint);
    using LowGasSafeMath for int;
    using SafeCast for int;

    // info stored for each initialized individual tick
    struct Info {
        // the total position liquidity that references this tick
        uint128 liquidityGross;
        // amount of net liquidity added (subtracted) when tick is crossed from left to right (right to left),
        int128 liquidityNet;
        // fee growth accumulated after the tick went in range (useful when calculating fee accumulating for all tick above the current tick)
        int feeGrowthInside;
        // fee growth accumulated before the tick went in rangen(useful when calculating fee accumulating for all tick below the current tick)
        int feeGrowthOutside;
        // true iff the tick is initialized, i.e. the value is exactly equivalent to the expression liquidityGross != 0
        // these 8 bits are set to prevent fresh sstores when crossing newly initialized ticks
        bool initialized;
    }

    /// @notice Derives max liquidity per tick from given tick spacing
    /// @dev Executed within the pool constructor
    /// @param tickSpacing The amount of required tick separation, realized in multiples of `tickSpacing`
    ///     e.g., a tickSpacing of 3 requires ticks to be initialized every 3rd tick i.e., ..., -6, -3, 0, 3, 6, ...
    /// @return The max liquidity per tick
    function tickSpacingToMaxLiquidityPerTick(int24 tickSpacing)
        internal
        pure
        returns (uint128)
    {
        int24 minTick = (TickMath.MIN_TICK / tickSpacing) * tickSpacing;
        int24 maxTick = (TickMath.MAX_TICK / tickSpacing) * tickSpacing;
        uint24 numTicks = uint24((maxTick - minTick) / tickSpacing) + 1;
        return type(uint128).max / numTicks;
    }

    struct CurrentFees {
        int feeGrowthInside;
        int feeGrowthOutside;
    }
    struct FeeGrowthInsideParams {
        int pnl;
        int24 tick;
        int24 tickSpacing;
        int24 tickLower;
        int24 tickUpper;
        uint128 liquidity;
    }

    /// @notice Retrieves fee growth data
    /// @param self The mapping containing all tick information for initialized ticks
    /// @param tickBitmap The mapping for initialised ticks
    /// @param params The parameters for calculating fee growth
    /// @return feeGrowthInside The all-time fee growth, inside the position's tick boundaries
    function getFeeGrowthInside(
        mapping(int24 => Tick.Info) storage self,
        mapping(int16 => uint) storage tickBitmap,
        FeeGrowthInsideParams memory params
    ) internal view returns (int feeGrowthInside) {
        uint24 tickDelta = uint24(params.tickUpper - params.tickLower);
        while (params.tickLower < params.tickUpper) {
            int24 tickNext = tickBitmap.nextInitializedTick(
                params.tickLower,
                params.tickSpacing,
                false
            );
            if (tickNext < params.tick) {
                feeGrowthInside +=
                    ((tickNext - params.tickLower) *
                        int128(params.liquidity) *
                        (params.pnl - self[tickNext].feeGrowthOutside)) /
                    (tickDelta * PRECISION);
            } else {
                // the tick delta is fully below the active tick, formula will be PnL - tickNext.feeGrowthOutside
                feeGrowthInside +=
                    ((tickNext - params.tickLower) *
                        int128(params.liquidity) *
                        self[tickNext].feeGrowthInside) /
                    (tickDelta * PRECISION);
            }
            params.tickLower = tickNext;
        }
    }

    struct UpdateParams {
        int24 tick;
        int24 currentTick;
        int24 tickSpacing;
        int128 liquidityDelta;
        bool upper;
        uint128 maxLiquidity;
        int pnl;
    }

    /// @notice Updates a tick and returns true if the tick was flipped from initialized to uninitialized, or vice versa
    /// @param self The mapping containing all tick information for initialized ticks
    /// @param tickBitmap the mapping for initialised ticks
    /// @param params The parameters for updating a tick
    /// @return flipped Whether the tick was flipped from initialized to uninitialized, or vice versa
    function update(
        mapping(int24 => Tick.Info) storage self,
        mapping(int16 => uint) storage tickBitmap,
        UpdateParams memory params
    ) internal returns (bool flipped) {
        Tick.Info storage info = self[params.tick];

        uint128 liquidityGrossBefore = info.liquidityGross;
        uint128 liquidityGrossAfter = LiquidityMath.addDelta(
            liquidityGrossBefore,
            params.liquidityDelta
        );

        require(liquidityGrossAfter <= params.maxLiquidity, 'LO');

        flipped = (liquidityGrossAfter == 0) != (liquidityGrossBefore == 0);

        if (liquidityGrossBefore == 0) {
            if (params.pnl != 0) {
                int24 nextTick = tickBitmap.nextInitializedTick(
                    params.tick,
                    params.tickSpacing,
                    false
                );
                // might need to get the previous tick and assign the feeGrowthOutside to the newly initialized tick
                if (params.tick > params.currentTick) {
                    info.feeGrowthInside = self[nextTick].feeGrowthInside;
                } else {
                    if (nextTick > params.currentTick) {
                        info.feeGrowthOutside =
                            params.pnl -
                            self[nextTick].feeGrowthInside;
                    } else {
                        info.feeGrowthOutside = self[nextTick].feeGrowthOutside;
                    }
                }
            }
            info.initialized = true;
        }

        info.liquidityGross = liquidityGrossAfter;

        // when the lower (upper) tick is crossed left to right (right to left), liquidity must be added (removed)
        info.liquidityNet = params.upper
            ? int(info.liquidityNet).sub(params.liquidityDelta).toInt128()
            : int(info.liquidityNet).add(params.liquidityDelta).toInt128();
    }

    /// @notice Clears tick data
    /// @param self The mapping containing all initialized tick information for initialized ticks
    /// @param tick The tick that will be cleared
    function clear(mapping(int24 => Tick.Info) storage self, int24 tick)
        internal
    {
        delete self[tick];
    }

    /// @notice Transitions to next tick as needed by price movement
    /// @param self The mapping containing all tick information for initialized ticks
    /// @param tick The destination tick of the transition
    /// @param feeGrowthGlobal The all-time global fee growth, per unit of liquidity
    /// @param zeroForOne The direction of the transition
    /// @return liquidityNet The amount of liquidity added (subtracted) when tick is crossed from left to right (right to left)
    function cross(
        mapping(int24 => Tick.Info) storage self,
        int24 tick,
        int feeGrowthGlobal,
        bool zeroForOne
    ) internal returns (int128 liquidityNet) {
        Tick.Info storage info = self[tick];
        if (zeroForOne) {
            info.feeGrowthInside = feeGrowthGlobal - info.feeGrowthOutside;
        } else {
            info.feeGrowthOutside = feeGrowthGlobal - info.feeGrowthInside;
        }
        liquidityNet = info.liquidityNet;
    }
}

