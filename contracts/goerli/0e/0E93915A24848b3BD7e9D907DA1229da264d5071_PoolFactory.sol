    mapping(address => Trade.Info) public trades;

    /// @dev Mutually exclusive reentrancy protection into the pool to/from a method. This method also prevents entrance
    /// to a function before the pool is initialized. The reentrancy guard is required throughout the contract because
    /// we use balance checks to determine the payment status of interactions such as mint, swap and flash.
    modifier lock() {
        require(slot0.unlocked, 'LOK');
        slot0.unlocked = false;
        _;
        slot0.unlocked = true;
    }

    constructor() {}

    function initialize(
        int24 tickSpacing_,
        uint24 fee_,
        address collateral_,
        address oracle_
    ) external {
        require(slot0.unlocked == false, 'initialized');

        leftMostInitializedTick = TickMath.MIN_TICK + 1;
        rightMostInitializedTick = TickMath.MAX_TICK - 1;

        tickSpacing = tickSpacing_;
        fee = fee_;
        collateral = collateral_;
        oracle = IOracle(oracle_);
        maxLiquidityPerTick = Tick.tickSpacingToMaxLiquidityPerTick(
            tickSpacing_
        );

        //@todo see if we should rather check the block, as timestamp can easily be manipulated
        lastUpdate = block.timestamp;
        lastPrice = oracle.getPrice().price;

        slot0.unlocked = true;
    }

    /// @dev Common checks for valid tick inputs.
    function checkTicks(int24 tickLower, int24 tickUpper) private pure {
        require(tickLower < tickUpper, 'TLU');
        require(tickLower >= TickMath.MIN_TICK, 'TLM');
        require(tickUpper <= TickMath.MAX_TICK, 'TUM');
    }

    /// @dev Returns the block timestamp truncated to 32 bits, i.e. mod 2**32. This method is overridden in tests.
    function _blockTimestamp() internal view virtual returns (uint32) {
        return uint32(block.timestamp); // truncation is desired
    }

    struct ModifyPositionParams {
        // the address that owns the position
        address owner;
        // the lower and upper tick of the position
        int24 tickLower;
        int24 tickUpper;
        // any change in liquidityPerTick
        int128 liquidityDelta;
    }

    /// @dev noDelegateCall is applied indirectly via _modifyPosition
    function mint(
        address recipient,
        int24 tickLower,
        int24 tickUpper,
        uint128 amount
    ) external override lock {
        require(amount > 0);
        TransferHelper.safeTransferFrom(
            collateral,
            msg.sender,
            address(this),
            amount
        );

        // mark a checkpoint and aggregate the funding rate
        _update(0);

        (, int amount0Int, ) = _modifyPosition(
            ModifyPositionParams({
                owner: recipient,
                tickLower: tickLower,
                tickUpper: tickUpper,
                liquidityDelta: int(amount).toInt128()
            })
        );

        if (tickLower < leftMostInitializedTick) {
            leftMostInitializedTick = tickLower;
        }
        if (tickUpper > rightMostInitializedTick) {
            rightMostInitializedTick = tickUpper;
        }

        // we swap no matter what in case of rebalancing
        // use TICK.MAX for frLImit atm, later on we will store the leftmost tick where liquidity has been deposited
        _swap(-amount0Int, leftMostInitializedTick.iToFR(), false);

        emit Mint(msg.sender, recipient, tickLower, tickUpper, amount);
    }

    /// @inheritdoc IUniswapV3PoolActions
    function collect(
        address recipient,
        int24 tickLower,
        int24 tickUpper,
        uint128 amountRequested
    ) external override lock returns (uint128 amount) {
        // we don't need to checkTicks here, because invalid positions will never have non-zero tokensOwed{0,1}
        Position.Info storage position = positions.get(
            msg.sender,
            tickLower,
            tickUpper
        );

        amount = int128(amountRequested) > position.tokensOwed
            ? position.tokensOwed > 0 ? uint128(position.tokensOwed) : 0
            : amountRequested;

        if (amount > 0) {
            position.tokensOwed -= int128(amount);
            TransferHelper.safeTransfer(
                collateral,
                recipient,
                uint(position.tokensOwed) + uint(amount)
            );
        }

        emit Collect(msg.sender, recipient, tickLower, tickUpper, amount);
    }

    /// @inheritdoc IUniswapV3PoolActions
    /// @dev noDelegateCall is applied indirectly via _modifyPosition
    function burn(
        address owner,
        int24 tickLower,
        int24 tickUpper,
        uint128 amount
    ) external override lock returns (uint amount0, uint amount1) {
        // mark a checkpoint and aggregate the funding rate
        _update(0);
        (
            Position.Info storage position,
            int amount0Int,
            int amount1Int
        ) = _modifyPosition(
                ModifyPositionParams({
                    owner: owner,
                    tickLower: tickLower,
                    tickUpper: tickUpper,
                    liquidityDelta: -int(amount).toInt128()
                })
            );

        amount0 = uint(-amount0Int);
        amount1 = uint(-amount1Int);

        // we swap no matter what, because we might need to rebalance anyway
        _swap(-amount0Int, rightMostInitializedTick, false);

        position.tokensOwed = position.tokensOwed + int128(amount);

        emit Burn(owner, tickLower, tickUpper, amount);
    }

    function getPositionPnL(
        int24 tickLower,
        int24 tickUpper,
        address owner
    ) external view returns (int) {
        Position.Info memory position = positions.get(
            owner,
            tickLower,
            tickUpper
        );
        int fgi = ticks.getFeeGrowthInside(
            tickBitmap,
            Tick.FeeGrowthInsideParams({
                pnl: pnl,
                tick: slot0.tick,
                tickSpacing: tickSpacing,
                tickLower: tickLower,
                tickUpper: tickUpper,
                liquidity: position.liquidity
            })
        );
        return fgi - position.feeGrowthInsideLast;
    }

    function enter(
        uint amount,
        int160 frLimit,
        address recipient
    ) external returns (uint liquidityMoved, uint feeAmount) {
        TransferHelper.safeTransferFrom(
            collateral,
            msg.sender,
            address(this),
            amount
        );

        // mark a checkpoint in fr accounting
        _update(0);

        (liquidityMoved, feeAmount) = _swap(int(amount), frLimit, true);
        // after the swap, we expect totalLiquidities to be above 0
        require(totalLiquidities != 0, 'EP');

        trades[recipient] = Trade.Info({
            amount: liquidityMoved - feeAmount,
            shares: ((liquidityMoved - feeAmount) * traderLiquidities) /
                totalLiquidities,
            leverage: 1 // leverage is not enabled at the moment
        });
    }

    function exit(uint shares, address recipient) external {
        Trade.Info storage trade = trades[msg.sender];

        require(shares <= trade.shares, 'not enough shares');

        uint sharesValue = (shares * totalLiquidities) / traderLiquidities;

        int traderPnl = int(
            sharesValue - ((shares * trade.amount) / trade.shares)
        );

        trade.amount -= (shares * trade.amount) / trade.shares;

        // add the pnl to the pool
        _update(traderPnl);

        _swap(int(-sharesValue), leftMostInitializedTick.iToFR(), true);

        TransferHelper.safeTransfer(collateral, recipient, sharesValue);
    }

    function updatePrice() external returns (int liquidityToMove) {
        PriceData memory p = oracle.getPrice();
        require(lastPrice != p.price, 'PC');
        liquidityToMove = int(
            (int(totalLiquidities) * int(p.price - lastPrice)) / int(p.price)
        );

        // mark a checkpoint before moving the liquidities around
        _update(0);

        _swap(
            liquidityToMove,
            liquidityToMove < 0
                ? int160((TickMath.MIN_TICK + 1).iToFR())
                : int160((TickMath.MAX_TICK - 1).iToFR()),
            false
        );

        lastPrice = p.price;
    }

    /// @dev Effect some changes to a position
    /// @param params the position details and the change to the position's liquidityPerTick to effect
    /// @return position a storage pointer referencing the position with the given owner and tick range
    /// @return amount0 the amount of token0 owed to the pool, negative if the pool should pay the recipient
    /// @return amount1 the amount of token1 owed to the pool, negative if the pool should pay the recipient
    function _modifyPosition(ModifyPositionParams memory params)
        private
        returns (
            Position.Info storage position,
            int amount0,
            int amount1
        )
    {
        checkTicks(params.tickLower, params.tickUpper);

        uint24 tickDelta = uint24(params.tickUpper) - uint24(params.tickLower);

        // tickBitmap.flipTick(params.tickLower, tickSpacing);
        // tickBitmap.flipTick(params.tickUpper, tickSpacing);

        int _pnl = pnl; // SLOAD for gas optimization
        Slot0 memory _slot0 = slot0; // SLOAD for gas optimization

        position = positions.get(
            params.owner,
            params.tickLower,
            params.tickUpper
        );

        if (params.liquidityDelta != 0) {
            bool flipped = ticks.update(
                tickBitmap,
                Tick.UpdateParams({
                    tick: params.tickLower,
                    currentTick: _slot0.tick,
                    tickSpacing: tickSpacing,
                    liquidityDelta: params.liquidityDelta / tickDelta,
                    upper: false,
                    maxLiquidity: maxLiquidityPerTick,
                    pnl: _pnl
                })
            );
            if (flipped) {
                tickBitmap.flipTick(params.tickLower, tickSpacing);
            }
            flipped = ticks.update(
                tickBitmap,
                Tick.UpdateParams({
                    tick: params.tickUpper,
                    currentTick: _slot0.tick,
                    tickSpacing: tickSpacing,
                    liquidityDelta: params.liquidityDelta / tickDelta,
                    upper: true,
                    maxLiquidity: maxLiquidityPerTick,
                    pnl: _pnl
                })
            );
            if (flipped) {
                tickBitmap.flipTick(params.tickUpper, tickSpacing);
            }
        }

        int feeGrowthInside = ticks.getFeeGrowthInside(
            tickBitmap,
            Tick.FeeGrowthInsideParams({
                pnl: _pnl,
                tick: _slot0.tick,
                tickSpacing: tickSpacing,
                tickLower: params.tickLower,
                tickUpper: params.tickUpper,
                liquidity: position.liquidity
            })
        );

        int128 liquidityDeltaPerTick;

        (amount0, amount1, liquidityDeltaPerTick) = TickMath.getAmounts(
            params.tickLower,
            params.tickUpper,
            params.liquidityDelta,
            _slot0.fr
        );

        if (params.liquidityDelta != 0) {
            if (
                _slot0.tick > params.tickLower && _slot0.tick < params.tickUpper
            ) {
                // current tick is inside the passed range
                uint128 liquidityBefore = liquidityPerTick; // SLOAD for gas optimization
                liquidityPerTick = LiquidityMath.addDelta(
                    liquidityBefore,
                    liquidityDeltaPerTick
                );
            }
        }

        position.update(params.liquidityDelta, feeGrowthInside);
    }

    /// @notice update the global fee accounting for this pool
    function _update(int traderPnl) internal {
        uint timeElapsed = block.timestamp - lastUpdate;

        if (timeElapsed == 0 || totalLiquidities == 0) return;

        int accFR = (slot0.fr * int(timeElapsed) * int(totalLiquidities))
            .iFromFR() / (int(FR_TIME_BASIS) * FinMath.iBPS);
        pnlSnapshot += accFR + traderPnl;

        pnl += (pnlSnapshot * PRECISION) / int(totalLiquidities);

        lastUpdate = block.timestamp;
    }

    /// @notice move lquidity accross the fr range
    /// @param amount the amount of liquidityPerTick to move, zeroForOne if negative
    /// @param frLimit the max fr to move to
    /// @param fees whether or not to pay fees
    function _swap(
        int amount,
        int160 frLimit,
        bool fees
    ) internal returns (uint liquidityMoved, uint feeAmount) {
        Slot0 memory slot0Start = slot0;
        int amountToSwap = amount - pnlSnapshot;
        bool zeroForOne = amountToSwap < 0 ? true : false;

        SwapMath.SwapCache memory cache = SwapMath.SwapCache({
            amountSpecified: uint(zeroForOne ? -amountToSwap : amountToSwap),
            liquidityStart: liquidityPerTick,
            feeProtocol: 0 // @todo enable it eventually
        });

        SwapMath.SwapState memory state = SwapMath.SwapState({
            amountSpecifiedRemaining: uint(
                zeroForOne ? -amountToSwap : amountToSwap
            ),
            fr: slot0Start.fr,
            tick: slot0Start.tick,
            traderLiquidities: traderLiquidities,
            totalLiquidities: totalLiquidities,
            pnl: pnl,
            protocolFee: 0,
            liquidityPerTick: liquidityPerTick,
            feeAmount: 0
        });

        if (zeroForOne) {
            frLimit = (frLimit < leftMostInitializedTick.iToFR() ||
                frLimit > slot0Start.fr)
                ? leftMostInitializedTick.iToFR()
                : frLimit;
        } else {
            frLimit = (frLimit > rightMostInitializedTick.iToFR() ||
                frLimit < slot0Start.fr)
                ? rightMostInitializedTick.iToFR()
                : frLimit;
        }
        SwapMath.swap(
            state,
            SwapMath.SwapParams({
                frLimit: frLimit,
                tickSpacing: tickSpacing,
                fee: fees ? fee : 0,
                pnlSnapshotPerLiquidity: totalLiquidities > 0
                    ? (pnlSnapshot * PRECISION) / int(totalLiquidities)
                    : pnlSnapshot,
                zeroForOne: zeroForOne
            }),
            ticks,
            tickBitmap
        );

        // update tick and write an oracle entry if the tick change
        if (state.tick != slot0Start.tick) {
            (slot0.fr, slot0.tick) = (state.fr, state.tick);
        } else {
            // otherwise just update the price
            slot0.fr = state.fr;
        }

        // update liquidityPerTick if it changed
        if (cache.liquidityStart != state.liquidityPerTick)
            liquidityPerTick = state.liquidityPerTick;

        traderLiquidities = state.traderLiquidities;
        totalLiquidities = state.totalLiquidities;

        liquidityMoved = cache.amountSpecified - state.amountSpecifiedRemaining;
        feeAmount = state.feeAmount;
        pnl = state.pnl;

        pnlSnapshot = 0;

        emit Swap(liquidityMoved, state.fr, state.liquidityPerTick, state.tick);
        slot0.unlocked = true;
    }

