import './FullMath.sol';
import './SqrtPriceMath.sol';
import './TickMath.sol';
import './Tick.sol';
import './Constants.sol';

/// @title Computes the result of a swap within ticks
/// @notice Contains methods for computing the result of a swap within a single tick price range, i.e., a single tick.
library SwapMath {
    using TickMath for int24;
    using TickMath for uint160;
    using TickMath for uint;
    using Tick for mapping(int24 => Tick.Info);
    using TickBitmap for mapping(int16 => uint);

    struct SwapCache {
        uint amountSpecified;
        // the protocol fee for the input token
        uint8 feeProtocol;
        // liquidityPerTick at the beginning of the swap
        uint128 liquidityStart;
    }

    // the top level state of the swap, the results of which are recorded in storage at the end
    struct SwapState {
        // the amount remaining to be swapped in/out of the input/output asset
        uint amountSpecifiedRemaining;
        // current sqrt(price)
        int160 fr;
        // the tick associated with the current price
        int24 tick;
        // the current total trader liquidities
        uint traderLiquidities;
        // the current total liquidities actives
        uint totalLiquidities;
        // the current pnl
        int pnl;
        // amount of input token paid as protocol fee
        uint128 protocolFee;
        // the current liquidityPerTick in range
        uint128 liquidityPerTick;
        // fee taken for this swap as swap fees
        uint feeAmount;
    }

    struct StepComputations {
        // the price at the beginning of the step
        int160 frStart;
        // the next tick to swap to from the current tick in the swap direction
        int24 tickNext;
        // whether tickNext is initialized or not
        bool initialized;
        // how much is being swapped in in this step
        uint liquidityMoved;
        // how much fee is being paid in
        uint feeAmount;
    }

    struct SwapParams {
        int160 frLimit;
        int24 tickSpacing;
        uint24 fee;
        int pnlSnapshotPerLiquidity;
        bool zeroForOne;
    }

    function swap(
        SwapState memory state,
        SwapParams memory params,
        mapping(int24 => Tick.Info) storage ticks,
        mapping(int16 => uint) storage tickBitmap
    ) internal {
        if (params.fee != 0) {
            state.amountSpecifiedRemaining = FullMath.mulDiv(
                state.amountSpecifiedRemaining,
                1e4 - params.fee,
                1e4
            );
        }

        // continue swapping as long as we haven't used the entire input/output and haven't reached the price limit
        while (
            state.amountSpecifiedRemaining != 0 && state.fr != params.frLimit
        ) {
            StepComputations memory step;

            step.frStart = state.fr;
            // @todo we can most probably further optimize here by using nextInitializedTick
            (step.tickNext) = tickBitmap.nextInitializedTick(
                state.tick, // @todo debug that issue when going backward in the ticks,
                params.tickSpacing,
                params.zeroForOne
            );

            // accumulate pnl and fr since the last swap
            if (params.pnlSnapshotPerLiquidity != 0) {
                // find the previous tick
                int24 previousTick = tickBitmap.nextInitializedTick(
                    state.tick, // @todo debug that issue when going backward in the ticks,
                    params.tickSpacing,
                    !params.zeroForOne
                );
                // increase the feeGrowthInside to the next initialised tick, pro rata to the filling of the current range
                if (params.zeroForOne) {
                    ticks[previousTick].feeGrowthInside += ((params
                        .pnlSnapshotPerLiquidity *
                        (state.fr - step.tickNext.iToFR())) /
                        (previousTick - step.tickNext).iToFR());
                } else {
                    ticks[step.tickNext].feeGrowthInside +=
                        (params.pnlSnapshotPerLiquidity *
                            (state.fr - previousTick.iToFR())) /
                        (step.tickNext - previousTick).iToFR();
                }
                params.pnlSnapshotPerLiquidity = 0;
            }

            // ensure that we do not overshoot the min/max tick, as the tick bitmap is not aware of these bounds
            if (step.tickNext < TickMath.MIN_TICK) {
                step.tickNext = TickMath.MIN_TICK;
            } else if (step.tickNext > TickMath.MAX_TICK) {
                step.tickNext = TickMath.MAX_TICK;
            }

            int160 frNext = int160(step.tickNext.iToFR());
            // compute values to swap to the target tick, price limit, or point where input/output amount is exhausted
            (state.fr, step.liquidityMoved, step.feeAmount) = computeSwapStep(
                state,
                (
                    !params.zeroForOne
                        ? frNext > params.frLimit
                        : frNext < params.frLimit
                )
                    ? params.frLimit
                    : int160(step.tickNext.iToFR()),
                params.fee
            );
            state = computeLiquidityStep(state, step, params.zeroForOne, true);
            // assign the swapfees to the feeGrowthInside of the next initialised tick, pro rata to the filling of the current range
            if (step.feeAmount > 0) {
                // find the previous tick, you might be able to optimize here, by using the previous tickNext
                int24 previousTick = tickBitmap.nextInitializedTick(
                    state.tick, // @todo debug that issue when going backward in the ticks,
                    params.tickSpacing,
                    !params.zeroForOne
                );
                if (params.zeroForOne) {
                    ticks[previousTick].feeGrowthInside +=
                        (int(step.feeAmount) *
                            PRECISION *
                            (previousTick.iToFR() - state.fr)) /
                        ((previousTick - step.tickNext).iToFR() *
                            (int(state.totalLiquidities) +
                                int(step.liquidityMoved)));
                } else {
                    ticks[step.tickNext].feeGrowthInside +=
                        (int(step.feeAmount) *
                            PRECISION *
                            (state.fr - previousTick.iToFR())) /
                        ((step.tickNext - previousTick).iToFR() *
                            int(state.totalLiquidities));
                }
            }

            state = computeCrossTickSwap(state, step, ticks, params.zeroForOne);
        }
    }

    /// @notice Computes the result of swapping some amount in, or amount out, given the parameters of the swap
    /// @dev The fee, plus the amount in, will never exceed the amount remaining if the swap's `amountSpecified` is positive
    /// @param state The state
    /// @param frTarget The FR that cannot be exceeded, from which the direction of the swap is inferred
    /// @param feePips The fee taken from the input amount, expressed in hundredths of a bip
    /// @return frAfter The price after swapping the amount in/out, not to exceed the price target
    /// @return liquidityMoved The amount of liquidity swapped
    /// @return feeAmount The amount of input that will be taken as a fee
    function computeSwapStep(
        SwapState memory state,
        int160 frTarget,
        uint24 feePips
    )
        internal
        view
        returns (
            int160 frAfter,
            uint liquidityMoved,
            uint feeAmount
        )
    {
        // we move from 0 to 1, or from 1 to 0
        bool zeroForOne = state.fr >= frTarget;

        uint160 deltaFr = zeroForOne
            ? uint160(state.fr - frTarget)
            : uint160(frTarget - state.fr);

        liquidityMoved = (state.liquidityPerTick * deltaFr).fromFR();

        // @todo see if we can optimize
        feeAmount = FullMath.mulDiv(liquidityMoved, feePips, 1e4);
        // if liquidityRemaining is higher than liquidity moved ratio after is equal to the next tick
        if (state.amountSpecifiedRemaining >= liquidityMoved) {
            frAfter = frTarget;
        }
        // if liquidity remaining is lower that liquidity moved, we compute the price after the swap
        else {
            frAfter = zeroForOne
                ? state.fr -
                    int160(
                        (state.amountSpecifiedRemaining +
                            state.feeAmount +
                            feeAmount).toFR() / state.liquidityPerTick
                    )
                : state.fr +
                    int160(
                        state.amountSpecifiedRemaining.toFR() /
                            state.liquidityPerTick
                    );
        }
    }

    // function computeFeeStep()

    /// @dev Decrease amount specified remaining and aggregate trader and total liquidities
    function computeLiquidityStep(
        SwapState memory state,
        StepComputations memory step,
        bool zeroForOne,
        bool fees
    ) internal returns (SwapState memory) {
        if (state.liquidityPerTick > 0) {
            if (zeroForOne) {
                // cap and avoid overflow
                if (step.liquidityMoved > state.totalLiquidities) {
                    state.totalLiquidities = 0;
                } else {
                    state.totalLiquidities -= step.liquidityMoved;
                }
            } else {
                state.totalLiquidities += step.liquidityMoved - step.feeAmount;
            }
            // take the swap fees, distributed accross all active liquidities
            if (fees) {
                state.feeAmount += step.feeAmount;
                if (zeroForOne) {
                    if (step.liquidityMoved > state.traderLiquidities) {
                        state.traderLiquidities = 0;
                    } else {
                        state.traderLiquidities -=
                            step.liquidityMoved -
                            step.feeAmount;
                    }
                    if (step.liquidityMoved > 0 && step.feeAmount > 0) {
                        if (
                            step.liquidityMoved > state.amountSpecifiedRemaining
                        ) {
                            state.pnl +=
                                int(
                                    int(step.feeAmount) *
                                        int(state.amountSpecifiedRemaining) *
                                        PRECISION
                                ) /
                                int(
                                    (state.totalLiquidities +
                                        step.liquidityMoved) *
                                        step.liquidityMoved
                                );
                        } else {
                            state.pnl +=
                                int(int(step.feeAmount) * PRECISION) /
                                int(
                                    state.totalLiquidities + step.liquidityMoved
                                );
                        }
                    }
                } else {
                    state.traderLiquidities += step.liquidityMoved;
                    if (state.totalLiquidities > 0 && step.feeAmount > 0) {
                        if (
                            step.liquidityMoved > state.amountSpecifiedRemaining
                        ) {
                            state.pnl +=
                                int(
                                    int(step.feeAmount) *
                                        int(state.amountSpecifiedRemaining) *
                                        PRECISION
                                ) /
                                int(
                                    state.totalLiquidities * step.liquidityMoved
                                );
                        } else {
                            state.pnl +=
                                int(int(step.feeAmount) * PRECISION) /
                                int(state.totalLiquidities);
                        }
                    }
                }
            }
            if (step.liquidityMoved >= state.amountSpecifiedRemaining) {
                state.amountSpecifiedRemaining = 0;
            } else {
                state.amountSpecifiedRemaining -= (step.liquidityMoved);
            }
        }
        return state;
    }

    function computeCrossTickSwap(
        SwapState memory state,
        StepComputations memory step,
        mapping(int24 => Tick.Info) storage ticks,
        bool zeroForOne
    ) internal returns (SwapState memory) {
        // shift tick if we reached the next price
        if (state.fr == step.tickNext.iToFR()) {
            // @todo clarify the need for decreasing ticknext if we go left
            state.tick = zeroForOne ? step.tickNext - 1 : step.tickNext;

            // if the tick is initialized, run the tick transition
            // if (step.initialized) {
            int128 liquidityNet = ticks.cross(
                step.tickNext,
                state.pnl,
                zeroForOne
            );

            if (zeroForOne) {
                // if we're moving leftward, we interpret liquidityNet as the opposite sign
                // safe because liquidityNet cannot be type(int128).min
                liquidityNet = -liquidityNet;
            }
            state.liquidityPerTick = LiquidityMath.addDelta(
                state.liquidityPerTick,
                liquidityNet
            );
            // }
        } else if (state.fr != step.frStart) {
            // recompute unless we're on a lower tick boundary (i.e. already transitioned ticks), and haven't moved
            state.tick = TickMath.getTickAtFR(state.fr);
        }
        return state;
    }
}

