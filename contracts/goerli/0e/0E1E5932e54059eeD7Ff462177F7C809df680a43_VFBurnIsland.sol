pragma solidity ^0.8.4;

import "./VFAccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

contract VFBurnIsland is Context, ReentrancyGuard {
    using ECDSA for bytes32;

    //Address of burn signer
    address private _burnSigner;

    //Contract for function access control
    VFAccessControl private _controlContract;

    //Mapping of executed transactions
    mapping(bytes32 => bool) private executed;

    //Address to burn tokens to
    address public constant BURN_ADDRESS =
        0x000000000000000000000000000000000000dEaD;

    error OnlyTokenOwnerCanCallThisFunction();
    error ContractDoesNotSupportRequiredInterface();
    error TransactionAlreadyExecuted();
    error InvalidBurnSignature();

    /**
     * @dev Initializes the contract by setting a `signer`, `controlContractAddress`,
     * and `tokenContractAddress` to the sales contract.
     */
    constructor(address signer, address controlContractAddress) {
        _burnSigner = signer;
        _controlContract = VFAccessControl(controlContractAddress);
    }

    modifier onlyRole(bytes32 role) {
        _controlContract.checkRole(role, _msgSender());
        _;
    }

    modifier onlyTokenOwner(address from) {
        if (from != _msgSender()) {
            revert OnlyTokenOwnerCanCallThisFunction();
        }
        _;
    }

    /**
     * @dev Update the burn signer address with `signer`
     *
     * Requirements:
     *
     * - the caller must be an admin role
     */
    function setBurnSigner(
        address signer
    ) external onlyRole(_controlContract.getAdminRole()) {
        _burnSigner = signer;
    }

    /**
     * @dev Update the access control contract
     *
     * Requirements:
     *
     * - the caller must be an admin role
     * - `controlContractAddress` must support the IVFAccesControl interface
     */
    function setControlContract(
        address controlContractAddress
    ) external onlyRole(_controlContract.getAdminRole()) {
        if (
            !IERC165(controlContractAddress).supportsInterface(
                type(IVFAccessControl).interfaceId
            )
        ) {
            revert ContractDoesNotSupportRequiredInterface();
        }
        _controlContract = VFAccessControl(controlContractAddress);
    }

    /**
     * @dev Burn tokens on contract
     *
     * Requirements:
     *
     * - `contractAddress` must support the IVFToken interface
     * - the caller must be a burner role
     */
    function burnTokens(
        address contractAddress,
        uint256[] calldata tokenIds
    ) external onlyRole(_controlContract.getBurnerRole()) nonReentrant {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721(contractAddress).transferFrom(
                address(this),
                BURN_ADDRESS,
                tokenIds[i]
            );
        }
    }

    /**
     * @dev Validate a burn id is signed by the burn signer address
     */
    function _isValidBurnSignature(
        bytes32 txHash,
        bytes calldata signature
    ) internal view returns (bool isValid) {
        address signer = txHash.toEthSignedMessageHash().recover(signature);
        return signer == _burnSigner;
    }

    /**
     * @dev Get the hash of a burn transaction
     */
    function _getTxHash(
        string calldata burnId,
        address sender,
        address contractAddress,
        uint256[] calldata tokenIds,
        uint256 nonce
    ) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    burnId,
                    sender,
                    contractAddress,
                    tokenIds,
                    nonce
                )
            );
    }

    /**
     * @dev Send tokens to burn island
     *
     * Requirements:
     *
     * - `contractAddress` must support the IERC721 interface
     * - `signature` must be signed by the burn signer address
     * - `nonce` must be unique
     * - `tokenIds` must be owned by the sender
     */
    function sendTokensToBurnIsland(
        string calldata burnId,
        address contractAddress,
        uint256[] calldata tokenIds,
        uint256 nonce,
        bytes calldata signature
    ) external nonReentrant {
        bytes32 txHash = _getTxHash(
            burnId,
            _msgSender(),
            contractAddress,
            tokenIds,
            nonce
        );

        if (executed[txHash]) {
            revert TransactionAlreadyExecuted();
        }

        if (!_isValidBurnSignature(txHash, signature)) {
            revert InvalidBurnSignature();
        }

        executed[txHash] = true;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721(contractAddress).transferFrom(
                _msgSender(),
                address(this),
                tokenIds[i]
            );
        }
    }

    /**
     * @dev Withdraw token if we need to refund
     *
     * Requirements:
     *
     * - `contractAddress` must support the IVFToken interface
     * - the caller must be an admin role
     */
    function withdrawToken(
        address contractAddress,
        address to,
        uint256 tokenId
    ) external onlyRole(_controlContract.getAdminRole()) nonReentrant {
        IERC721(contractAddress).transferFrom(address(this), to, tokenId);
    }

    /**
     * @dev Withdraw balance on contact to msg sender
     *
     * Requirements:
     *
     * - the caller must be an admin role
     */
    function withdrawMoney()
        external
        onlyRole(_controlContract.getAdminRole())
        nonReentrant
    {
        address payable to = payable(_msgSender());
        to.transfer(address(this).balance);
    }
}

