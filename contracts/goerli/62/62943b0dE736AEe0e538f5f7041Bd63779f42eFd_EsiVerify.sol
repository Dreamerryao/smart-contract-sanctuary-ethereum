// File contracts/EsiVerify.sol

pragma solidity ^0.5.0;




contract EsiVerify {
    using Base64 for string;
    using StringUtils for *;
    using JsmnSolLib for string;
    // More info in creation-commands.txt
    address public constant owner = 0xFA47aC0E594e961cbd735313E2377343AA5B9a48;

    /**
     * @notice Validate and verify JWT token issued by ESI
     * @dev Only for EthSign signatures
     * @param _headerJson JWT header
     * @param _payloadJson JWT payload
     * @param _signature  Signature encoded as (bytes32 r, bytes32 s, uint8 v)
     */
    function verify(
        string memory _headerJson,
        string memory _payloadJson,
        bytes memory _signature
    ) public view returns (bool) {
        console.log("\nInside EsiVerify.sol smart contract...");
        require(
            _signature.length == 65,
            "SignatureValidator#recoverSigner: invalid signature length"
        );
        //Check 'alg' in header
        require(
            keccak256(bytes(getJWTProperty(_headerJson, "alg"))) ==
                keccak256(bytes("ES256K")),
            "Invalid JWT algorithm: must be ES256K"
        );
        //Check 'address' in payload
        require(
            keccak256(bytes(getJWTProperty(_payloadJson, "domain"))) ==
                keccak256(bytes("dev.ethereumsignin.com")),
            "Invalid Payload: must be blah blah"
        );

        console.log("Header JSON: ", _headerJson);
        console.log("Payload JSON: ", _payloadJson);

        (bytes32 r, bytes32 s, uint8 v) = splitSignature(_signature);
        if (
            uint256(s) >
            0x7FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF5D576E7357A4501DDFE92F46681B20A0
        ) {
            revert(
                "SignatureValidator#recoverSigner: invalid signature 's' value"
            );
        }

        // console.log("V Value: ", uint256(v));
        if (v != 27 && v != 28) {
            revert(
                "SignatureValidator#recoverSigner: invalid signature 'v' value"
            );
        }

        // Recompute JWT token hash
        string memory headerBase64 = _headerJson.encode();
        console.log("Header base64: ", headerBase64);
        string memory payloadBase64 = _payloadJson.encode();
        console.log("Payload base64: ", payloadBase64);
        StringUtils.slice[] memory slices = new StringUtils.slice[](2);
        slices[0] = headerBase64.toSlice();
        slices[1] = payloadBase64.toSlice();
        bytes32 recomputedHash = sha256(bytes(".".toSlice().join(slices)));
        console.log("Message: ", uint256(recomputedHash));

        // Recover ECDSA signer
        address signer = ecrecover(recomputedHash, v, r, s);

        // Prevent signer from being 0x0
        require(
            signer != address(0x0),
            "SignatureValidator#recoverSigner: INVALID_SIGNER"
        );

        return signer == owner;
    }

    function splitSignature(bytes memory sig)
        public
        pure
        returns (
            bytes32 r,
            bytes32 s,
            uint8 v
        )
    {
        require(sig.length == 65, "invalid signature length");

        assembly {
            /*
            First 32 bytes stores the length of the signature

            add(sig, 32) = pointer of sig + 32
            effectively, skips first 32 bytes of signature

            mload(p) loads next 32 bytes starting at the memory address p into memory
            */

            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        // implicitly return (r, s, v)
    }

    function getJWTProperty(string memory json, string memory property)
        public
        pure
        returns (string memory)
    {
        (
            uint256 exitCode,
            JsmnSolLib.Token[] memory tokens,
            uint256 ntokens
        ) = json.parse(40);
        require(exitCode == 0, "JSON parse failed");

        require(
            tokens[0].jsmnType == JsmnSolLib.JsmnType.OBJECT,
            "Expected JWT to be an object"
        );
        uint256 i = 1;
        while (i < ntokens) {
            require(
                tokens[i].jsmnType == JsmnSolLib.JsmnType.STRING,
                "Expected JWT to contain only string keys"
            );
            string memory key = json.getBytes(tokens[i].start, tokens[i].end);
            if (key.strCompare(property) == 0) {
                require(
                    tokens[i + 1].jsmnType == JsmnSolLib.JsmnType.STRING,
                    "Expected sub to be a string"
                );
                return json.getBytes(tokens[i + 1].start, tokens[i + 1].end);
            }
            i += 2;
        }

        revert("Could not find sub in JWT object");
    }
}