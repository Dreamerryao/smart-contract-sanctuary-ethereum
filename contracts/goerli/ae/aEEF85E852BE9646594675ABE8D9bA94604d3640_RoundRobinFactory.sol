pragma solidity ^0.8.9;

// Import this file to use console.log
import "hardhat/console.sol";
import "./RoundRobin.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract RoundRobinFactory {
    address[] private roundRobinContractAddresses;
    
    string private testing = "Testing";

    event CreateContract(address contractAddress);  

    constructor() {}

    function createRoundRobin(
        string memory group_name,
        uint256 contributionAmount,
        RoundRobin.ContributionFrequency frequencyOfContribution,
        uint256 expectedReserveBalance
    ) public {
        
        RoundRobin newRoundRobin = new RoundRobin(payable(msg.sender), group_name,
        contributionAmount, 
        frequencyOfContribution, expectedReserveBalance);
        roundRobinContractAddresses.push(address(newRoundRobin));
        emit CreateContract(address(newRoundRobin));
        
    }

    function getRoundRobinContractAddresses(uint256 index) public view returns(address){
        return roundRobinContractAddresses[index];
    }
    function getTestString() public view returns(string memory){
        return testing;
    }
    
}

