pragma solidity ^0.8.9;

// Import this file to use console.log
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";
import "hardhat/console.sol";

error RoundRobin__TransferFailed();
error RoundRobin__UpkeepNotNeeded(uint256 timeRemaining, uint256 amountToBeSent, uint256 accountBalance);

contract RoundRobin is  KeeperCompatibleInterface {
    /* Type declaration */
    struct Member {
        uint256 id;
        string name;
        string title;
        address memberAddress;
    }

    enum ContributionFrequency {
        DAILY,
        WEEKLY,
        MONTHLY,
        TEST
    }

    event MemberAdded(address memberAddress);  
    event ContributionReceived(uint256 contributionAmount);

    
    string private i_groupName;
    uint256 private immutable i_expectedContributionAmount;
    ContributionFrequency private immutable i_frequencyOfContribution;
    address payable immutable i_owner;
    uint256 private immutable i_expectedReserveBalance;
    uint256 private s_lastTimeStamp;

    mapping(address => Member) public members;
    //Member[] private members;
    mapping(address => uint256) private reserveBalances;
    mapping(address => uint256) private remainingContributions;
    mapping(address => uint256) private investmentBalances;
    mapping(uint256 => address) private indexToAddress;

    uint256 private trackingIndex;
    uint256 private memberCount = 0;
    uint256 private reserveBalance = 0;
    uint256 private totalAmountContributed = 0;
    uint256 private investmentContribution = 0;

    constructor(
        address payable owner,
        string memory group_name,
        uint256 expectedContributionAmount,
        ContributionFrequency frequencyOfContribution,
        uint256 expectedReserveBalance
    ) {
       
        i_owner = owner;
        i_groupName = group_name;
        i_expectedContributionAmount = expectedContributionAmount;
        i_frequencyOfContribution = frequencyOfContribution;
        i_expectedReserveBalance = expectedReserveBalance;
        s_lastTimeStamp = block.timestamp;
    }

    function addMembers(
        string memory _name,
        address _memberAddress,
        string memory _title
    ) public {
        members[_memberAddress]=Member(memberCount, _name, _title, _memberAddress);
        reserveBalances[_memberAddress] = 0;
        remainingContributions[_memberAddress] = i_expectedContributionAmount;
        investmentBalances[_memberAddress] = 0;
        indexToAddress[memberCount] = _memberAddress;
        memberCount++;
        emit MemberAdded(_memberAddress);
    }

    function makeContribution() public payable {
        uint256 memberReserveBalance = reserveBalances[msg.sender];
        
        if (memberReserveBalance < i_expectedReserveBalance) {
            uint256 remainingReserveBalance = i_expectedReserveBalance - memberReserveBalance;
            
            if (msg.value <= remainingReserveBalance) {
                reserveBalances[msg.sender] = memberReserveBalance + msg.value;
                reserveBalance += msg.value;
            } else {
                reserveBalances[msg.sender] = i_expectedReserveBalance;
                reserveBalance += remainingReserveBalance;
                uint256 remainingContributionAmount = msg.value - remainingReserveBalance;
                fillContributionAndInvestment(remainingContributionAmount);
            }
        } else {
            fillContributionAndInvestment(msg.value);
        }
        emit ContributionReceived(msg.value);
    }
    

    function fillContributionAndInvestment(uint256 contributionAmount) private{
        
        
        if (contributionAmount < remainingContributions[msg.sender]) {
            
            remainingContributions[msg.sender] =
                i_expectedContributionAmount -
                contributionAmount;
                totalAmountContributed += contributionAmount;
        } else if (contributionAmount == remainingContributions[msg.sender]) {
           
            remainingContributions[msg.sender] = 0;
            totalAmountContributed += contributionAmount;
        } else {
          
            totalAmountContributed += remainingContributions[msg.sender];
            remainingContributions[msg.sender] = 0;
            investmentBalances[msg.sender] =
                investmentBalances[msg.sender] +
                (contributionAmount - remainingContributions[msg.sender]);

                investmentContribution+=(contributionAmount - remainingContributions[msg.sender]);
                
        }
    }
    function checkUpkeep(
        bytes memory /* checkData */
    )
        public
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /* performData */
        )
    {
        bool amountToBeSent = (address(this).balance)>memberCount*i_expectedContributionAmount;
        bool timePassed = ((block.timestamp - s_lastTimeStamp) > getIntervalInSeconds(i_frequencyOfContribution));
        
        bool hasBalance = address(this).balance > 0;
        upkeepNeeded = (timePassed && amountToBeSent && hasBalance);
        return (upkeepNeeded, "0x0"); 
    }
    function performUpkeep(
        bytes calldata /* performData */
    ) external override {
        (bool upkeepNeeded, ) = checkUpkeep("");
        // require(upkeepNeeded, "Upkeep not needed");
        if (!upkeepNeeded) {
            
            uint256 timeRemaining = getIntervalInSeconds(i_frequencyOfContribution) -(block.timestamp - s_lastTimeStamp) ;
            revert RoundRobin__UpkeepNotNeeded(timeRemaining, memberCount*i_expectedContributionAmount, address(this).balance);
        }
        for (
            uint256 memberIndex = 0;
            memberIndex < memberCount;
            memberIndex++
        ) {
            address _memberAddress = indexToAddress[memberIndex];
            uint256 remainingContribution = remainingContributions[_memberAddress];
            if(remainingContribution!=0){
                reserveBalances[_memberAddress] = reserveBalances[_memberAddress] - remainingContribution;
                reserveBalance-=remainingContribution;
            }
            remainingContributions[_memberAddress] = i_expectedContributionAmount;
        }
        uint256 totalAmountToBeSent = i_expectedContributionAmount * memberCount;
        (bool success, ) = indexToAddress[trackingIndex].call{value: totalAmountToBeSent}("");
        if (!success) {
            revert RoundRobin__TransferFailed();
        }
        if(trackingIndex==memberCount-1){
            trackingIndex = 0;
        }else{
            trackingIndex++;
        }
        s_lastTimeStamp= block.timestamp;

    }

    function getIntervalInSeconds(ContributionFrequency contributionFrequency) internal pure returns(uint256){
        if(contributionFrequency == ContributionFrequency.DAILY){
            return 24*60*60;
        }else if(contributionFrequency == ContributionFrequency.WEEKLY){
            return 7*24*60*60;
        }else if(contributionFrequency == ContributionFrequency.MONTHLY){
            return 30*24*60*60;
        }else{
            return 5*60;
        }
    }

    
    function getContributionAmount() public view returns (uint256) {
        return i_expectedContributionAmount;
    }

    function getFrequencyOfContribution() public view returns (ContributionFrequency) {
        return i_frequencyOfContribution;
    }

    function getOwner() public view returns (address) {
        return i_owner;
    }

    function getMember(address memberAddress) public view returns (Member memory) {
        Member memory member = members[memberAddress];

        return member;
    }
    function getMemebrByIndex(uint256 index) public view returns(Member memory){

    }

    function getExpectedReserveBalance() public view returns (uint256) {
        return i_expectedReserveBalance;
    }

    function getMemberReserveBalance(address memberAddress) public view returns (uint256) {
        return reserveBalances[memberAddress];
    }

    function getRemainingContributionAmount(address memberAddress) public view returns (uint256) {
        return remainingContributions[memberAddress];
    }

    function getMemberInvestmentBalance(address memberAddress) public view returns (uint256) {
        return investmentBalances[memberAddress];
    }
    function getTrackingIndex() public view returns(uint256){
        return trackingIndex;
    }
    function getMemberCount() public view returns(uint256){
        return memberCount;
    }
    function getGroupName() public view returns(string memory){
        return i_groupName;
    }
    function getGroupDetails() public view returns(string memory, uint256, uint256, uint256, Member memory,
    uint256, uint256, uint256,uint256){
        uint256 timeRemaining = getIntervalInSeconds(i_frequencyOfContribution) -(block.timestamp - s_lastTimeStamp) ;
        return (i_groupName, memberCount, i_expectedContributionAmount,
         i_expectedReserveBalance,members[indexToAddress[trackingIndex]],
         reserveBalance, totalAmountContributed, investmentContribution, timeRemaining);
    }
    function nextContributionRecepient() public view returns (Member memory){
        return members[indexToAddress[trackingIndex]];
    }
    function getMemberDetails(address memberAddress) public view returns(Member memory, 
    uint256, uint256, uint256){
        return (members[memberAddress],
         reserveBalances[memberAddress], remainingContributions[memberAddress], investmentBalances[memberAddress]);
    }
    
    
}

