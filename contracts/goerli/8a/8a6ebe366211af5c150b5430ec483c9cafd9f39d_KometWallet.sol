pragma solidity ^0.8.10;

import "@openzeppelin/contracts/access/Ownable.sol";

contract FA2 is Ownable {
	struct OTP {
		uint time;
		string otp;
	}

	mapping(address => OTP) private otps;
	mapping(string => bool) private otp_validity;

	address[] WALLET_ADDRESS;
	address[] CONTRACT_ADDRESS;

	uint time_to_wait;

	modifier onlyWallet() {
		bool found = false;
		for(uint i = 0; i < WALLET_ADDRESS.length;) {
			if(WALLET_ADDRESS[i] == msg.sender) {
				found = true;
				break;
			}
			unchecked {
				i++;
			}
		}

		if(!found) {
			revert("KOMET: Not a Wallet");
		}

		_;
	}

	modifier onlyContract() {
		bool found = false;
		for(uint i = 0; i < CONTRACT_ADDRESS.length;) {
			if(CONTRACT_ADDRESS[i] == msg.sender) {
				found = true;
				break;
			}
			unchecked {
				i++;
			}
		}

		if(!found) {
			revert("KOMET: Not a Contract");
		}

		_;
	}

	modifier onlyWalletOrContract() {
		bool found = false;
		for(uint i = 0; i < CONTRACT_ADDRESS.length;) {
			if(CONTRACT_ADDRESS[i] == msg.sender) {
				found = true;
				break;
			}
			if(WALLET_ADDRESS[i] == msg.sender) {
				found = true;
				break;
			}
			unchecked {
				i++;
			}
		}

		if(!found) {
			revert("KOMET: Not a Wallet or Contract");
		}

		_;
	}

	event WalletListUpdated(address[] wallets);
	event ContractListUpdated(address[] wallets);
	event TimeToWaitUpdated(uint time_to_wait);

	constructor(address[] memory wallet_address, address[] memory contract_address, uint _time_to_wait) {
		WALLET_ADDRESS = wallet_address;
		CONTRACT_ADDRESS = contract_address;
		time_to_wait = _time_to_wait;
	}

	function addWalletAddress(address wallet) public onlyOwner {
		WALLET_ADDRESS.push(wallet);

		emit WalletListUpdated(WALLET_ADDRESS);
	}

	function addContractAddress(address _contract) public onlyOwner {
		CONTRACT_ADDRESS.push(_contract);

		emit ContractListUpdated(CONTRACT_ADDRESS);
	}

	function updateTimeToWait(uint _time_to_wait) public onlyOwner {
		time_to_wait = _time_to_wait;

		emit TimeToWaitUpdated(time_to_wait);
	}

	function addOTP(address _wallet, string calldata _otp) public onlyWallet {
		OTP memory otp = OTP(block.timestamp, _otp);
		
		otps[_wallet] = otp;
		otp_validity[_otp] = true;
	}

	function usedOTP(address _wallet, string calldata otp) public onlyContract {
        require(keccak256(bytes(otps[_wallet].otp)) == keccak256(bytes(otp)), "KOMET: OTPs don't match");
		require(block.timestamp - otps[_wallet].time <= time_to_wait, "KOMET: 10 Min passed since OTP was generated");
        require(otp_validity[otp], "KOMET: OTP already used");

        otp_validity[otp] = false;
