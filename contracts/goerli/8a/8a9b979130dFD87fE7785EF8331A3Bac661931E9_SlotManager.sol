// Import this file to use console.log
import "hardhat/console.sol";

/**
 * @title Mem Slot Manager
 * @notice Enables depositing ETH to gain ownership of a slot and its setData method
 * @author Cameron Voell
 */

contract SlotManager {

    /**
     * @notice admin account of the SlotManager
     */
    address public broker;

    /**
     * @notice tracks ids of slot returned to broker
     */
    uint[] public brokerOwnedSlots;

    /**
     * @notice Required deposit for gaining ownership of a slot
     */
    uint public slotPrice = 0.2 ether;

    /**
     * @notice Required deposit for gaining ownership of a slot
     */
    uint public slotPurchaseLockupSeconds = 7 days;

    /**
     * @notice Restricts how many slots can be owned by each address
     */
    mapping(address => uint) public slotAllowance;

    /**
     * @notice Tracks number of slots owned per address
     */
    mapping(address => uint) public numOwned;

    /**
     * @dev Only the slot owner is permissioned to update that dataURI
     */
    struct Slot {
        address owner;
        string dataURI;
        uint purchaseTimestamp;
        uint purchaseAmount;
    }

    /**
     * @notice Contains all slots created in the Slot Manager
     * @dev slots stored in uint keys sequentially from 0 to numSlots
     */
    mapping(uint => Slot) public slots;

    /**
     * @notice Number of slots in the slotMapping
     */
    uint public numSlots;

    // =========================================== CONSTRUCTOR ===========================================

    /**
     * @param _broker address of the admin account that is permissioned to transfer slot ownership
     */
    constructor(address _broker) {
        broker = _broker;
    }

    // =========================================== BROKER FUNCTIONS ======================================

    /**
     * @dev Throws if called by any account other than the broker.
     */
    modifier onlyBroker() {
        require(msg.sender == broker, "BROKER_ONLY");
        _;
    }

    function setSlotPrice(uint _slotPrice) external onlyBroker {
        slotPrice = _slotPrice;
    }

    function setSlotLockupPeriodSeconds(uint lockupSeconds) external onlyBroker {
        slotPurchaseLockupSeconds = lockupSeconds;
    }

    function setAllowance(address buyer, uint allowance) external onlyBroker {
        slotAllowance[buyer] = allowance;
    }

    /**
     * @dev Throws if called by any account other than the broker.
     */
    function transferOwnership(address newOwner, uint slotIndex) external onlyBroker {
        slots[slotIndex].owner = newOwner;
    }

    /**
     * @dev Throws if called by any account other than the broker.
     */
    function withdrawEth(address payable to) external onlyBroker {
        require(address(this).balance > 0, "ZERO_BALANCE");
        (bool sent, ) = to.call{value: address(this).balance}("");
        require(sent, "ETH_SEND_FAILURE");
    }


    // =========================================== CORE FUNCTIONS ========================================

    /**
     * @notice Address with available allowance can gain ownership by sending a deposit equal to the slotPrice
     * @param _dataURI initial data that we want to populate our new slot with
     */
    function purchaseSlot(string calldata _dataURI) external payable {
        require(msg.value == slotPrice, "INCORRECT_VALUE");
        require(numOwned[msg.sender] < slotAllowance[msg.sender], "INSUFFICIENT_ALLOWANCE");

        slots[numSlots++] = Slot(msg.sender, _dataURI, block.timestamp, slotPrice);
        numOwned[msg.sender]++;
    }

    function setData(uint slotIndex, string calldata _dataURI) external {
        require(slots[slotIndex].owner == msg.sender, "OWNER_ONLY");
        slots[slotIndex].dataURI = _dataURI;
    }

    function returnToBroker(uint slotIndex) external {
        require(slots[slotIndex].owner == msg.sender, "OWNER_ONLY");
        require(block.timestamp - slots[slotIndex].purchaseTimestamp > slotPurchaseLockupSeconds, "LOCKUP_VIOLATION");
        // set the owner of the slot to the broker
        slots[slotIndex].owner = broker;
        brokerOwnedSlots.push(slotIndex);
        // return deposit money to the calling (previous) slot owner
        (bool sent, ) = payable(msg.sender).call{value: slots[slotIndex].purchaseAmount}("");
        require(sent, "ETH_SEND_FAILURE");
    }

}

