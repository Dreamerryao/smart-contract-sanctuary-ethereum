pragma solidity 0.8.13;

import "../libraries/FixedPointMathLib.sol";
import {wadExp, unsafeWadDiv, wadDiv, wadLn} from "../utils/SignedWadMath.sol";

// both x and alpha in WAD unit
// maxDiscount: less than 1 WAD unit but must be in a range, may be 0.5 WAD  to 0.8 WAD
function sigmoidAlphaDiscount(
    int256 maxDiscount,
    int256 x,
    int256 alpha
) pure returns (int256) {
    return
        wadDiv(2 * maxDiscount, 10**18 + wadExp(-wadDiv(x, alpha))) -
        maxDiscount;
}

// discount factor, max is 1 WAD means no discount at all
function discountFactor(
    int256 maxDiscount,
    int256 x,
    int256 alpha
) pure returns (int256) {
    x -= 10**18;
    return 10**18 - sigmoidAlphaDiscount(maxDiscount, x, alpha);
}

// back to 6 decimal values
function converBackSixDecimal(int256 x) pure returns (uint256) {
    return uint256(x / 10**12);
}

