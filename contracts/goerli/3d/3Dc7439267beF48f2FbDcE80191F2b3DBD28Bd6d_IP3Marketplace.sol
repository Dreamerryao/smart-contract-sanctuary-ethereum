enum LicenseType {
    Time,
    Quantity,
    Other
}

struct NFT {
    string chainId;
    address NFTAddress;
    uint256 tokenId;
}

struct ListingIP {
    NFT nft;
    Licensor licensor;
    LicensedInfo licensedByQuantityInfo;
    LicensedInfo licensedByTimeInfo;
    uint256 listStartTime;
    uint256 listEndTime;
    string metadataUri;
    bytes signature;
}

struct Licensor {
    address nftOwnerAddress;
    address claimerAddress;
}

struct LicensedInfo {
    bool isActive;
    uint256 initialStartPrice;
    uint256 latestStartPrice;
    uint256 lastActive; // last active timestamp
}

struct Term {
    uint256 licenseStartTime;
    uint256 licenseEndTime;
    uint256 count;
    LicenseType licenseType;
}

struct License {
    NFT nft;
    Licensor licensor;
    string metadataUri;
    address licensee;
    Term term;
    uint256 price;
}

struct TransactionRecord {
    uint256 totalTransactionCount;
    uint256 totalTransactionRevenue;
}

