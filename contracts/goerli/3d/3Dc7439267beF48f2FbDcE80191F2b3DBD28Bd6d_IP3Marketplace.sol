pragma solidity 0.8.13;

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {AddressUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import {SignatureCheckerUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";
import {ERC165CheckerUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165CheckerUpgradeable.sol";
import {EnumerableSetUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import {NFT, ListingIP, License, Licensor, LicensedInfo, Term, LicenseType, ClaimInfo, LicenseTransactionRecord, PriceDynamicsParams, SmartPriceParams, ManageFee, PurchaseConditions} from "../structs/IP3MarketplaceStructs.sol";
import {IP3MarketplaceInterface} from "../interfaces/IP3MasrketplaceInterface.sol";
import {IIP3SignatureChecker} from "../interfaces/IIP3SignatureChecker.sol";
import {SmartPriceEngine} from "../libraries/SmartPriceEngine.sol";

contract IP3Marketplace is
    IP3MarketplaceInterface,
    Initializable,
    OwnableUpgradeable
{
    using SignatureCheckerUpgradeable for address;
    using AddressUpgradeable for address;
    IERC20Upgradeable public ERC20Currency;
    IIP3SignatureChecker public signatureCheckerContract;
    mapping(bytes32 => ListingIP) internal listingIPsMap; // hashed NFT => listings
    mapping(bytes32 => License) private licenseMap; // hash of License => license
    mapping(bytes32 => LicenseTransactionRecord) private licenseRecordMap; // hash of NFT => LicenseTransactionRecord
    mapping(bytes32 => ClaimInfo) private claimableMap; // hashedCerticate => isClaimed

    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.Bytes32Set;
    EnumerableSetUpgradeable.Bytes32Set private nftOnRecord; // set for NFT have transactional record (hash nft)

    uint256 managedFeeBalance; //balance of platform manage fee

    PriceDynamicsParams public priceParams;
    ManageFee public manageFee;
    PurchaseConditions public purchaseConditions;

    bool claimRevenueLock;
    bool claimManageFeeLock;

    /*//////////////////////////////////////////////////////////////
                    INITIALIZATION FUNCTION
    //////////////////////////////////////////////////////////////*/

    function initialize(
        IERC20Upgradeable _instanceAddress,
        address _manageFeeAddress,
        IIP3SignatureChecker _signatureChecker
    ) public initializer {
        __Ownable_init();
        ERC20Currency = _instanceAddress;
        signatureCheckerContract = IIP3SignatureChecker(_signatureChecker);

        SmartPriceParams memory timeBasedParams = SmartPriceParams({
            maxDiscount: 5 * 10**17, // 0.5 WAD
            alpha: 5 * 10**18, // 5 WAD
            maxThresholdPrice: 1000 * 10**6,
            minThresholdPrice: 1 * 10**6
        });

        SmartPriceParams memory quantityBasedParams = SmartPriceParams({
            maxDiscount: 5 * 10**17, // 0.5 WAD
            alpha: 50 * 10**18, // 50 WAD
            maxThresholdPrice: 1000 * 10**6,
            minThresholdPrice: 1 * 10**6
        });

        priceParams = PriceDynamicsParams({
            timeBased: timeBasedParams,
            quantityBased: quantityBasedParams,
            incrementByTx: 10**5,
            discountPerTime: 10**5,
            discountDelay: 86400 * 7
        });

        manageFee = ManageFee({
            basisPoint: 3000,
            feeDecimal: 10**4,
            feeAddress: _manageFeeAddress
        });

        purchaseConditions = PurchaseConditions({
            minTime: 1 days,
            maxTime: 10 days,
            minQuantity: 1,
            maxQuantity: 100,
            minPendingTime: 7 days
        });
    }

    /*///////////////////////////////////////////////////////////////
                                Modifiers
    //////////////////////////////////////////////////////////////*/

    /// @dev Checks whether a listing exists.
    modifier onlyExistingListing(NFT memory _nft) {
        bytes32 hashedNFT = _hashNft(_nft);
        require(
            listingIPsMap[hashedNFT].listStartTime != 0,
            "LISTING NOT EXIST"
        );
        _;
    }

    /*//////////////////////////////////////////////////////////////
                    LISTING OPERATIONS
    //////////////////////////////////////////////////////////////*/

    /// @dev IP owner list IP for authorizating others.
    function createListing(ListingIP calldata _newListingIP) public {
        _validateListing(_newListingIP);

        bytes32 hashedNFT = _hashNft(_newListingIP.nft);
        listingIPsMap[hashedNFT] = _newListingIP;
        emit CreateListing(hashedNFT, _newListingIP);
    }

    /// @dev IP owner modify existing IP.
    function updateListing(
        NFT memory _nft,
        uint256 _listStartTime,
        uint256 _listEndTime,
        address _claimerAddress,
        bytes calldata _signature
    ) external onlyExistingListing(_nft) {
        _validateUpdate(
            _nft,
            _listStartTime,
            _listEndTime,
            _claimerAddress,
            _signature
        );

        bytes32 hashedNFT = _hashNft(_nft);
        listingIPsMap[hashedNFT].listStartTime = _listStartTime;
        listingIPsMap[hashedNFT].listEndTime = _listEndTime;
        listingIPsMap[hashedNFT].licensor.claimerAddress = _claimerAddress;

        emit UpdateListing(
            hashedNFT,
            _nft,
            _listStartTime,
            _listEndTime,
            _claimerAddress
        );
    }

    /// @dev IP owner delist the existing IP.
    function cancelListing(NFT memory _nft, bytes memory _signature)
        external
        onlyExistingListing(_nft)
    {
        _validateCancel(_nft, _signature);
        bytes32 hashedNFT = _hashNft(_nft);
        delete listingIPsMap[hashedNFT];
        emit CancelListing(hashedNFT);
    }

    /*//////////////////////////////////////////////////////////////
                    purchase OPERATIONS
    //////////////////////////////////////////////////////////////*/

    /// @dev Let's a wallet purchase IP license from a listing
    function purchaseLicense(NFT memory _nft, Term memory _term)
        public
        onlyExistingListing(_nft)
    {
        // listing validation
        bytes32 hashedNFT = _hashNft(_nft);
        ListingIP memory listingIP = listingIPsMap[hashedNFT];

        require(
            listingIP.listEndTime > block.timestamp,
            "INVALID LISTING TIME"
        );

        require(
            _validateERC721Onwership(
                listingIP.nft,
                listingIP.licensor.nftOwnerAddress
            ),
            "NOT NFT OWNER"
        );

        if (_term.licenseType == LicenseType.Time) {
            // Time term validation
            require(listingIP.licensedByTimeInfo.isActive, "NOT ACTIVE");

            _purchaseByTime(listingIP, _term, msg.sender);
        } else if (_term.licenseType == LicenseType.Quantity) {
            // Quantity term validation
            require(listingIP.licensedByQuantityInfo.isActive, "NOT ACTIVE");

            _purchaseByQuantity(listingIP, _term, msg.sender);
        } else {
            revert("INVALID PURCHASE OPTION");
        }
    }

    /// @dev purchase time-based license
    function _purchaseByTime(
        ListingIP memory listingIP,
        Term memory term,
        address licensee
    ) private {
        // validation
        // check license time range
        require(term.licenseEndTime > term.licenseStartTime, "NEG TIME WINDOW");

        require(term.licenseEndTime > block.timestamp, "INVALID LICENSE TIME");

        uint256 duration = term.licenseEndTime - term.licenseStartTime;

        require(
            duration >= purchaseConditions.minTime,
            "LESS ALLOWED MIN TIME"
        );
        require(
            duration <= purchaseConditions.maxTime,
            "Over ALLOWED MAX TIME"
        );

        Term memory newTerm = Term(
            term.licenseStartTime,
            term.licenseEndTime,
            0,
            term.licenseType
        );

        bytes32 hashedNft = _hashNft(listingIP.nft);

        // get current start price
        uint256 currentStartPrice;

        // check if has record on-chain, if not default is 0
        // if has record on-chain
        uint256 latestStartPriceByTime = listingIP
            .licensedByTimeInfo
            .latestStartPrice;
        if (nftOnRecord.contains(hashedNft) && latestStartPriceByTime != 0) {
            // get the latest start price from record on chain
            currentStartPrice = getCurrentStartingPricePerNFT(
                hashedNft,
                term.licenseType
            );
        } else {
            // new NFT to be licenced
            // get the start price from the input
            nftOnRecord.add(hashedNft);
            currentStartPrice = listingIP.licensedByTimeInfo.initialStartPrice;

            require(
                currentStartPrice >= priceParams.timeBased.minThresholdPrice,
                "BELOW MIN PRICE"
            );
        }

        // update the start price into listingIPsMap
        listingIPsMap[hashedNft]
            .licensedByTimeInfo
            .latestStartPrice = currentStartPrice;

        // update the latest active with current block timestamp into listingIPsMap
        listingIPsMap[hashedNft].licensedByTimeInfo.lastActive = block
            .timestamp;

        // calculate the transaction value
        // compute the current termPrice by smart price engine
        // function computeLicensedPriceByTime(uint256 duration, uint256 durationFactor, uint256 durationOrder, uint256 currentStartPrice, uint256 maxThresholdPrice )
        uint256 termedPrice = SmartPriceEngine.computeLicensedPriceByTime(
            duration,
            priceParams.timeBased.maxDiscount,
            priceParams.timeBased.alpha,
            currentStartPrice,
            priceParams.timeBased.maxThresholdPrice
        );

        // create new license
        License memory newLicense = _createLicense(
            listingIP.nft,
            listingIP.licensor,
            listingIP.metadataUri,
            licensee,
            newTerm,
            termedPrice
        );

        // excute the transaction
        _excutePurchase(newLicense, licensee);
    }

    /// @dev purchase time-based license
    function _purchaseByQuantity(
        ListingIP memory listingIP,
        Term memory term,
        address licensee
    ) private {
        // validation
        // check license quantity
        require(
            term.count >= purchaseConditions.minQuantity,
            "LESS ALLOWED MIN QUANTITY"
        );

        require(
            term.count <= purchaseConditions.maxQuantity,
            "OVER ALLOWED MAX QUANTITY"
        );

        Term memory newTerm = Term(
            block.timestamp,
            0,
            term.count,
            term.licenseType
        );

        bytes32 hashedNft = _hashNft(listingIP.nft);

        // get current start price
        uint256 currentStartPrice;

        // check if has record on-chain, if not default is 0
        // if has record on-chain
        uint256 latestStartPriceByQuantity = listingIP
            .licensedByQuantityInfo
            .latestStartPrice;
        if (
            nftOnRecord.contains(hashedNft) && latestStartPriceByQuantity != 0
        ) {
            // get the latest start price from record on chain
            currentStartPrice = getCurrentStartingPricePerNFT(
                hashedNft,
                term.licenseType
            );
        } else {
            // new NFT to be licenced
            // get the start price from the input
            nftOnRecord.add(hashedNft);
            currentStartPrice = listingIP
                .licensedByQuantityInfo
                .initialStartPrice;

            require(
                currentStartPrice >=
                    priceParams.quantityBased.minThresholdPrice,
                "BELOW MIN PRICE"
            );
        }

        // update the start price into listingIPsMap
        listingIPsMap[hashedNft]
            .licensedByQuantityInfo
            .latestStartPrice = currentStartPrice;

        // update the latest active with current block timestamp into listingIPsMap
        listingIPsMap[hashedNft].licensedByQuantityInfo.lastActive = block
            .timestamp;

        // calculate the transaction value
        // compute the current termPrice by smart price engine
        uint256 termedPrice = SmartPriceEngine.computeLicensedPriceByQuantity(
            term.count,
            priceParams.quantityBased.maxDiscount,
            priceParams.quantityBased.alpha,
            currentStartPrice,
            priceParams.quantityBased.maxThresholdPrice
        );

        // create new license
        License memory newLicense = _createLicense(
            listingIP.nft,
            listingIP.licensor,
            listingIP.metadataUri,
            licensee,
            newTerm,
            termedPrice
        );

        // excute the transaction
        _excutePurchase(newLicense, licensee);
    }

    function lazyListingAndPurchaseLicense(
        ListingIP calldata _newListingIP,
        Term memory _term
    ) external {
        createListing(_newListingIP);
        
        purchaseLicense(_newListingIP.nft, _term);
    }

    /*//////////////////////////////////////////////////////////////
                    LICENSE OPERATIONS
    //////////////////////////////////////////////////////////////*/

    function _createLicense(
        NFT memory nft,
        Licensor memory licensor,
        string memory metadataUri,
        address licensee,
        Term memory term,
        uint256 termedPrice
    ) private pure returns (License memory) {
        License memory newLicense = License(
            nft,
            licensor,
            metadataUri,
            licensee,
            term,
            termedPrice
        );

        return newLicense;
    }

    function _hashLicense(License memory license)
        private
        pure
        returns (bytes32)
    {
        bytes32 hashedLicense = keccak256(
            bytes.concat(
                abi.encodePacked(
                    license.nft.chainId,
                    license.nft.NFTAddress,
                    license.nft.tokenId,
                    license.licensor.nftOwnerAddress,
                    license.licensor.claimerAddress
                ),
                abi.encodePacked(
                    license.metadataUri,
                    license.licensee,
                    license.term.licenseStartTime,
                    license.term.licenseEndTime,
                    license.term.count,
                    license.term.licenseType,
                    license.price
                )
            )
        );

        return hashedLicense;
    }

    function _excutePurchase(License memory license, address licensee) private {
        // register the license under the nft to licenseMap
        bytes32 hashedLicense = _hashLicense(license);
        licenseMap[hashedLicense] = license;

        // to management address
        uint256 termedPrice = license.price;
        uint256 fee = _calculateFee(termedPrice);
        managedFeeBalance += fee;

        // received for licensor after decution fee
        uint256 licensorOwn = termedPrice - fee;
        claimableMap[hashedLicense].claimableAmount = licensorOwn;

        bytes32 hashedNft = _hashNft(license.nft);
        // update the license record with transaction count and revenue
        if (license.term.licenseType == LicenseType.Time) {
            licenseRecordMap[hashedNft]
                .timeBasedRecord
                .totalTransactionCount += 1;

            licenseRecordMap[hashedNft]
                .timeBasedRecord
                .totalTransactionRevenue += termedPrice;
        } else if (license.term.licenseType == LicenseType.Quantity) {
            licenseRecordMap[hashedNft]
                .quantityBasedRecord
                .totalTransactionCount += 1;

            licenseRecordMap[hashedNft]
                .quantityBasedRecord
                .totalTransactionRevenue += termedPrice;
        } else {
            revert("INVALID PURCHASE OPTION");
        }

        // balance validation
        _validateERC20BalAndAllowance(licensee, ERC20Currency, termedPrice);

        // put transfer at the end to prevent the reentry attack
        ERC20Currency.transferFrom(licensee, address(this), termedPrice);

        emit PurchaseLicense(
            hashedNft,
            hashedLicense,
            licensee,
            license,
            termedPrice
        );
    }

    /*//////////////////////////////////////////////////////////////
                    LISTING VALIDATIONS
    //////////////////////////////////////////////////////////////*/

    /// @dev cancel listing
    function _validateCancel(NFT memory nft, bytes memory signature)
        internal
        view
    {
        bytes32 hashedNFT = _hashNft(nft);
        ListingIP memory listingIP = listingIPsMap[hashedNFT];
        address newSigner = listingIP.licensor.nftOwnerAddress;
        if (_validateERC721Onwership(listingIP.nft, msg.sender)) {
            newSigner = msg.sender;
        }
        // check nft owner, there are two cases to allow delete happen
        // case 1: nftOwnerAddress is the nft owner, signature match the nftOwnerAddress
        // case 2: msg.sender is the nft owner, signature match the msg.sender
        if (!(_validateERC721Onwership(listingIP.nft, newSigner))) {
            revert("NOT NFT OWNER");
        }

        require(
            signatureCheckerContract.verifyCancelList(
                IIP3SignatureChecker.CancelList(
                    nft.chainId,
                    nft.NFTAddress,
                    nft.tokenId
                ),
                signature,
                newSigner
            ),
            "INVALID SIGNATURE"
        );
    }

    /// @dev Validate listing
    function _validateListing(ListingIP calldata listingIP) internal view {
        // check if listing already exist
        bytes32 hashedNFT = _hashNft(listingIP.nft);
        require(
            listingIPsMap[hashedNFT].listStartTime == 0,
            "LISTING ALREADY EXIST"
        );

        require(listingIPsMap[hashedNFT].listStartTime == 0, "EXIST LISTING");
        /**
         * check:
         *  1. start price ( >= minimumPrice)
         *  2. latest price (must be zero)
         */

        // when byTime active
        if (listingIP.licensedByTimeInfo.isActive) {
            require(
                listingIP.licensedByTimeInfo.initialStartPrice >=
                    priceParams.timeBased.minThresholdPrice,
                "INVALID TIME START PRICE"
            );
            require(
                listingIP.licensedByTimeInfo.latestStartPrice == 0,
                "INVALID TIME LATEST PRICE"
            );
        }

        // when byQuantity active
        if (listingIP.licensedByQuantityInfo.isActive) {
            require(
                listingIP.licensedByQuantityInfo.initialStartPrice >=
                    priceParams.quantityBased.minThresholdPrice,
                "INVALID QUANTITY START PRICE"
            );
            require(
                listingIP.licensedByQuantityInfo.latestStartPrice == 0,
                "INVALID QUANTITY LATEST PRICE"
            );
        }

        // check if valid NFT holder
        require(
            _validateERC721Onwership(
                listingIP.nft,
                listingIP.licensor.nftOwnerAddress
            ),
            "NOT NFT OWNER"
        );

        // check if valid list time range (startTime < endTime && endTime > block.timestamp)
        require(
            listingIP.listStartTime < listingIP.listEndTime &&
                listingIP.listEndTime >= block.timestamp,
            "INVALID LISTING TIME"
        );

        // check if valid signature
        require(
            signatureCheckerContract.verifyCreateList(
                IIP3SignatureChecker.CreateList(
                    listingIP.nft.chainId,
                    listingIP.nft.NFTAddress,
                    listingIP.nft.tokenId,
                    listingIP.licensor.nftOwnerAddress,
                    listingIP.licensor.claimerAddress,
                    listingIP.licensedByQuantityInfo.isActive,
                    listingIP.licensedByQuantityInfo.initialStartPrice,
                    listingIP.licensedByQuantityInfo.latestStartPrice,
                    listingIP.licensedByQuantityInfo.lastActive,
                    listingIP.licensedByTimeInfo.isActive,
                    listingIP.licensedByTimeInfo.initialStartPrice,
                    listingIP.licensedByTimeInfo.latestStartPrice,
                    listingIP.licensedByTimeInfo.lastActive,
                    listingIP.listStartTime,
                    listingIP.listEndTime,
                    listingIP.metadataUri
                ),
                listingIP.signature,
                listingIP.licensor.nftOwnerAddress
            ),
            "INVALID SIGNATURE"
        );
    }

    /// @dev _validateUpdate(nft, listStartTime, listEndTime, claimerAddress)
    function _validateUpdate(
        NFT memory nft,
        uint256 listStartTime,
        uint256 listEndTime,
        address claimerAddress,
        bytes calldata signature
    ) internal view {
        bytes32 hashedNFT = _hashNft(nft);
        // check if time valid
        require(
            listStartTime < listEndTime && listEndTime > block.timestamp,
            "INVALID LISTING TIME"
        );

        // check if valid NFT holder
        ListingIP memory listingIP = listingIPsMap[hashedNFT];

        require(
            _validateERC721Onwership(
                listingIP.nft,
                listingIP.licensor.nftOwnerAddress
            ),
            "NOT NFT OWNER"
        );

        require(
            signatureCheckerContract.verifyUpdateList(
                IIP3SignatureChecker.UpdateList(
                    nft.chainId,
                    nft.NFTAddress,
                    nft.tokenId,
                    listStartTime,
                    listEndTime,
                    claimerAddress
                ),
                signature,
                listingIP.licensor.nftOwnerAddress
            ),
            "INVALID SIGNATURE"
        );
    }

    /// @dev Validates that `_addrToCheck` owns and has approved markeplace to transfer the appropriate amount of currency
    function _validateERC20BalAndAllowance(
        address _addrToCheck,
        IERC20Upgradeable _currency,
        uint256 _currencyAmountToCheckAgainst
    ) internal view {
        require(
            _currency.balanceOf(_addrToCheck) >=
                _currencyAmountToCheckAgainst &&
                _currency.allowance(_addrToCheck, address(this)) >=
                _currencyAmountToCheckAgainst,
            "NOT SUFFICIENT BAL"
        );
    }

    function _validateERC721Contract(address contractAddress)
        internal
        view
        returns (bool)
    {
        bytes4 interfaceId = 0x80ac58cd;
        return
            ERC165CheckerUpgradeable.supportsInterface(
                contractAddress,
                interfaceId
            );
    }

    function _validateERC721Onwership(NFT memory nft, address tokenOwner)
        internal
        view
        returns (bool)
    {
        return
            _validateERC721Contract(nft.NFTAddress) &&
            IERC721Upgradeable(nft.NFTAddress).ownerOf(nft.tokenId) ==
            tokenOwner;
    }

    function _hashNft(NFT memory nft) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(nft.chainId, nft.NFTAddress, nft.tokenId)
            );
    }

    /*//////////////////////////////////////////////////////////////
                    CLAIM REVENUE
    //////////////////////////////////////////////////////////////*/
    function claimRevenue(bytes32[] memory hashedLicenses) external {
        require(!claimRevenueLock, "LOCKED");

        uint256 totalBalance = 0;
        uint256 size = hashedLicenses.length;

        for (uint256 i = 0; i < size; i++) {
            if (isValidClaim(msg.sender, hashedLicenses[i])) {
                claimableMap[hashedLicenses[i]].isClaimed = true;
                totalBalance += claimableMap[hashedLicenses[i]].claimableAmount;
            }
        }

        require(totalBalance > 0, "ZERO BALANCE");

        ERC20Currency.transfer(msg.sender, totalBalance);

        emit ClaimRevenue(msg.sender, totalBalance);
    }

    function isValidClaim(address claimAddress, bytes32 hashedCertifcate)
        internal
        view
        returns (bool)
    {
        address nftHolderAddress = licenseMap[hashedCertifcate]
            .licensor
            .nftOwnerAddress;

        address claimAddressOnRecord = licenseMap[hashedCertifcate]
            .licensor
            .claimerAddress;

        // filter if the certifcates alreay claimed
        if (claimableMap[hashedCertifcate].isClaimed) {
            return false;
        }

        // filter if the authroizer or claim has current own the NFT addreess
        NFT memory nft = licenseMap[hashedCertifcate].nft;

        // not valid ERC721 contract address
        if (!_validateERC721Contract(nft.NFTAddress)) return false;

        address currentHolder = IERC721Upgradeable(nft.NFTAddress).ownerOf(
            nft.tokenId
        );

        // if token transfer out, check the current addr if hold this token
        if (nftHolderAddress != currentHolder) {
            if (claimAddress != currentHolder) {
                return false;
            }
        } else {
            // if current holder is same as the holder on record
            // check the if claim addr same as on record
            if (claimAddressOnRecord != claimAddress) {
                return false;
            }
        }

        // check the term based on authorzer nft type (by Duration, by amount)
        Term memory term = licenseMap[hashedCertifcate].term;
        if (!isValidByTerm(term)) {
            return false;
        }

        return true;
    }

    function isValidByTerm(Term memory term) internal view returns (bool) {
        // if by time
        if (term.licenseType == LicenseType.Time) {
            if (term.licenseEndTime > block.timestamp) {
                return false;
            }
            return true;
        } else if (term.licenseType == LicenseType.Quantity) {
            // by quantity: only allow claim after minPendingTime in days
            if (
                term.licenseStartTime + purchaseConditions.minPendingTime >
                block.timestamp
            ) {
                return false;
            }
            return true;
        } else {
            // other lincense types not suppported yet
            return false;
        }
    }

    /*//////////////////////////////////////////////////////////////
                    Transaction Fee Distribution
    //////////////////////////////////////////////////////////////*/
    function claimManageFee() external {
        require(!claimManageFeeLock, "LOCKED");
        require(msg.sender == manageFee.feeAddress, "NOT ALLOWED");
        require(managedFeeBalance > 0, "ZERO BALANCE");

        uint256 claimBalance = managedFeeBalance;
        managedFeeBalance = 0;
        ERC20Currency.transfer(msg.sender, claimBalance);

        emit ClaimManageFee(msg.sender, claimBalance);
    }

    function getManagedFeeBalance() external view returns (uint256) {
        return managedFeeBalance;
    }

    function _calculateFee(uint256 _amount) internal view returns (uint256) {
        uint256 fee = (manageFee.basisPoint * _amount) / manageFee.feeDecimal;
        return fee;
    }

    function setManageFee(ManageFee memory _managefee) public onlyOwner {
        manageFee = _managefee;
    }

    /*//////////////////////////////////////////////////////////////
                    Lock Set
    //////////////////////////////////////////////////////////////*/
    function setClaimRevenueLock(bool flag) external onlyOwner {
        claimRevenueLock = flag;
        emit ClaimRevenueLock(flag);
    }

    function setClaimManageFeeLock(bool flag) external onlyOwner {
        claimManageFeeLock = flag;
        emit ClaimManageFeeLock(flag);
    }

    /*//////////////////////////////////////////////////////////////
                    SETTER PRICE AND PURCHASE PARAMS
    //////////////////////////////////////////////////////////////*/
    function setPriceParams(PriceDynamicsParams memory _priceParams)
        public
        onlyOwner
    {
        priceParams = _priceParams;
    }

    function setPurchaseConditions(
        PurchaseConditions memory _purchaseConditions
    ) public onlyOwner {
        purchaseConditions = _purchaseConditions;
    }

    /*//////////////////////////////////////////////////////////////
                    GETTER FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    function getLicenseRecordMap(bytes32 hashedNFTInfo)
        external
        view
        returns (LicenseTransactionRecord memory)
    {
        return licenseRecordMap[hashedNFTInfo];
    }

    function getLicenseMap(bytes32 hashedLicense)
        external
        view
        returns (License memory)
    {
        return licenseMap[hashedLicense];
    }

    function getCurrentStartPriceAllNFT()
        public
        view
        returns (bytes32[] memory, uint256[][] memory)
    {
        uint256 size = nftOnRecord.length();
        bytes32[] memory hashedNfts = new bytes32[](size);
        uint256[][] memory prices = new uint256[][](size);

        for (uint256 i = 0; i < size; i++) {
            hashedNfts[i] = nftOnRecord.at(i);
            uint256[] memory singleNFTPriceforBoth = new uint256[](2);

            singleNFTPriceforBoth = getCurrentStartPrice(hashedNfts[i]);
            prices[i] = singleNFTPriceforBoth;
        }

        return (hashedNfts, prices);
    }

    function getCurrentStartPrice(bytes32 hashedNft)
        public
        view
        returns (uint256[] memory)
    {
        uint256[] memory priceBothOptions = new uint256[](2);
        // first index for byDuration option
        priceBothOptions[0] = getCurrentStartingPricePerNFT(
            hashedNft,
            LicenseType.Time
        );
        priceBothOptions[1] = getCurrentStartingPricePerNFT(
            hashedNft,
            LicenseType.Quantity
        );

        return priceBothOptions;
    }

    function getCurrentStartingPricePerNFT(
        bytes32 hashedNft,
        LicenseType licenseType
    ) internal view returns (uint256) {
        uint256 latestTimeStamp;
        uint256 latestStartPrice;
        uint256 currentTxNum;
        uint256 minPrice;

        if (licenseType == LicenseType.Time) {
            currentTxNum = licenseRecordMap[hashedNft]
                .timeBasedRecord
                .totalTransactionCount;

            latestTimeStamp = listingIPsMap[hashedNft]
                .licensedByTimeInfo
                .lastActive;

            latestStartPrice = listingIPsMap[hashedNft]
                .licensedByTimeInfo
                .latestStartPrice;

            minPrice = priceParams.timeBased.minThresholdPrice;
        } else if (licenseType == LicenseType.Quantity) {
            currentTxNum = licenseRecordMap[hashedNft]
                .quantityBasedRecord
                .totalTransactionCount;

            latestTimeStamp = listingIPsMap[hashedNft]
                .licensedByQuantityInfo
                .lastActive;

            latestStartPrice = listingIPsMap[hashedNft]
                .licensedByQuantityInfo
                .latestStartPrice;

            minPrice = priceParams.quantityBased.minThresholdPrice;
        } else {
            // another license options not supported yet
            revert("OPTION NOT SUPPORTED");
        }

        //  function updateStartingPrice(uint256 currentTxNum, uint256 latestTimeStamp, uint256 latestStartPrice, uint256 discountPerTime, uint256 incrementByTx, uint256 minThresholdPrice )
        uint256 currentStartPrice = SmartPriceEngine.updateStartingPrice(
            currentTxNum,
            latestTimeStamp,
            latestStartPrice,
            priceParams.discountPerTime,
            priceParams.incrementByTx,
            minPrice,
            priceParams.discountDelay
        );

        return currentStartPrice;
    }

    function getCurrentStartPriceOneNFT(NFT memory nft)
        external
        view
        returns (uint256[] memory)
    {
        bytes32 hashedNft = _hashNft(nft);
        // two license options: Time and Quantity
        uint256[] memory priceBothOptions = new uint256[](2);
        priceBothOptions[0] = getCurrentStartingPricePerNFT(
            hashedNft,
            LicenseType.Time
        );
        priceBothOptions[1] = getCurrentStartingPricePerNFT(
            hashedNft,
            LicenseType.Quantity
        );

        return priceBothOptions;
    }

    function getPriceParams()
        external
        view
        returns (PriceDynamicsParams memory)
    {
        return priceParams;
    }

    function getNftHash(NFT memory nft) public pure returns (bytes32) {
        return _hashNft(nft);
    }

    function getClaimableAmount(
        address claimAddress,
        bytes32[] memory hashedLicenses
    ) public view returns (uint256) {
        uint256 totalAmount = 0;
        uint256 size = hashedLicenses.length;
        for (uint256 i = 0; i < size; i++) {
            if (isValidClaim(claimAddress, hashedLicenses[i])) {
                totalAmount += claimableMap[hashedLicenses[i]].claimableAmount;
            }
        }

        return totalAmount;
    }
}

