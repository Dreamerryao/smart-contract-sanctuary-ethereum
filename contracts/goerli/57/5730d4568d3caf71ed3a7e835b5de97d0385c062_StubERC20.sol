pragma solidity ^0.8.13;

import {IAmmalgamPair} from '@core-v1/contracts/interfaces/IAmmalgamPair.sol';
import {IPeripheral} from './interfaces/IPeripheral.sol';
import {IERC20} from '@core-v1/contracts/interfaces/IERC20.sol';

contract Peripheral is IPeripheral {
    bytes4 private constant TRANSFER_FROM = bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));

    function newPosition(PositionParams calldata params) public returns (uint256) {
        IAmmalgamPair pair = IAmmalgamPair(params.pairAddress);
        address tokenX = pair.tokenX();
        address tokenY = pair.tokenY();

        _safeTransferFrom(tokenX, params.from, params.pairAddress, params.transferX);
        _safeTransferFrom(tokenY, params.from, params.pairAddress, params.transferY);

        return pair.mint(params.to);
    }

    function _safeTransferFrom(address token, address from, address to, uint256 value) private {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(TRANSFER_FROM, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'Ammalgam: TRANSFER_FAILED_FROM');
    }
}

