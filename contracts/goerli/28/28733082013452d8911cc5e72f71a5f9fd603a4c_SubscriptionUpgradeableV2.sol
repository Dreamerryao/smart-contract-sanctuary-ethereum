pragma solidity ^0.8.9;

import "./A2ANFT721.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";


contract A2ANFTFactory721 is AccessControl {
    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    //bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    A2ANFT721[] public tokens; //an array that contains different ERC721 tokens deployed
    mapping(uint256 => address) public indexToContract; //index to contract address mapping
    mapping(uint256 => address) public indexToOwner; //index to ERC721 owner address
    mapping(uint256 => mapping(uint256 => address)) public indexToBuyer; //index to ERC1155|721 buyer address
    mapping(uint256 => mapping(uint256 => string)) public indexToCID; //index to ERC1155|721 CID

    //projectId, nftId => bool
    mapping (uint256 => mapping(uint256 => bool)) public valid;

    //msg.sender, projectId, tierId => mintCounter
    mapping (address => mapping(uint256 => mapping(uint256 => uint256))) public mintCounter; 

    event ERC721Created(address owner, address tokenContract); //emited when ERC721 token is deployed
    event ERC721Minted(address owner, address tokenContract, uint amount); //emmited when ERC721 token is minted

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        //_grantRole(MINTER_ROLE, msg.sender);
    }

    function setValid(uint256 projectId, uint256 nftId, address buyer, string memory CID) public onlyRole(DEFAULT_ADMIN_ROLE) {
        valid[projectId][nftId] = true;
        indexToBuyer[projectId][nftId] = buyer;
        indexToCID[projectId][nftId] = CID;
    }
    
    function isValid(uint256 projectId, uint256 nft_id) public view returns (bool) {
        return valid[projectId][nft_id];
    }

    /*
    deployERC721 - deploys a ERC721 contract  with given parameters - returns deployed address
    _contractName - name of our ERC721 token
    _uri - URI resolving to our hosted metadata
    _ids - IDs the ERC721 token should contain
    _name - Names each ID should map to. Case-sensitive.
    */
    function deployArt2ActNFTContract(uint16 royalty, string memory name, string memory symbol) public onlyRole(DEPLOYER_ROLE) returns (address) {
        A2ANFT721 t = new A2ANFT721(payable(tx.origin), royalty, name, symbol);
        tokens.push(t);
        indexToContract[tokens.length - 1] = address(t);
        indexToOwner[tokens.length - 1] = tx.origin;
        emit ERC721Created(tx.origin, address(t));
        return address(t);
    }

    /*
    mintERC721 - mints a ERC721 token with given parameters

    _index - index position in our tokens array - represents which ERC1155 you want to interact with
    _name - Case-sensitive. Name of the token (this maps to the ID you created when deploying the token)
    _amount - amount of tokens you wish to mint
    */

    /* When claim by collector */
    function mintArt2ActNFT(uint256 projectId, uint256 tierId, uint256 tokenId, string memory tokenURI) public {
        require(indexToBuyer[projectId][tokenId] == msg.sender, "Minting by non-buyer");
        require(keccak256(bytes(indexToCID[projectId][tokenId])) == keccak256(bytes(tokenURI)), "TokenURI is wrong");
        mintCounter[msg.sender][projectId][tierId]++;
        tokens[projectId].safeMint(msg.sender, tokenURI);
        emit ERC721Minted(msg.sender, address(tokens[projectId]), 1);
    }

    function getMintCounter(address buyer, uint256 projectId, uint256 tierId) public view returns (uint256) {
        return mintCounter[buyer][projectId][tierId];
    }
   
    /* When upload by artist 
    function premintArt2ActNFT(uint projectId, address buyer, uint256 tokenId) public {
        require(indexToOwner[projectId] == msg.sender, "Pre-Minting by non-owner");
        require(isValid(projectId,tokenId), "NFT is not valid");
        indexToBuyer[projectId][tokenId] = buyer;
    }

    function isPreminted(uint256 projectId, uint256 tokenId) public view returns (bool) {
        return (indexToBuyer[projectId][tokenId] != address(0));
    }
*/
    /*
    Helper functions below retrieve contract data given an ID or name and index in the tokens array.
    */

    function getInstanceAddress(uint projectId)
        public
        view
        returns (address _contract)
    {
        A2ANFT721 token = tokens[projectId];
        return address(token);
    }
/*
    function getBalanceByAccount(uint projectId, uint tokenId, address account)
        public
        view
        returns (
            address _contract,
            string memory _uri,
            uint balance
        )
    {
        A2ANFT721 token = tokens[projectId];
        return (address(token), token.uri(tokenId), token.balanceOf(account, tokenId));
    }
*/
}

