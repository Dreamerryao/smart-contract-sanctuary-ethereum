pragma solidity ^0.8.9;

import "./SubscriptionNFT.sol";
import "./A2ANFTFactory721.sol";
import "./A2APayments.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

/**
 * @title The subscription core contract
 * @notice Allows for users to buy subscriptions, claim and set rewards
 */
contract SubscriptionUpgradeableV2 is Initializable, AccessControlUpgradeable, ReentrancyGuardUpgradeable {

    uint public basis_points;
    uint public platform_fee;
    uint32 public maxTiersPerProject;
    address payable private a2aTreasury;
    address payable private a2aPayments;
    address private a2aNFTsubscription;
    address private a2aNFTfactory;
    uint16 private startIndex;
    uint256 public subscriptionCounter;
    uint256 public projectsCounter;

    mapping(address => uint256) public collectorSubscriptionsCounter;
    mapping(address => uint256) public creatorProjectsCounter;
    mapping(uint256 => mapping(uint256 => TierInfo)) internal tiers;

    struct TierInfo {
        uint256 id;
        string backendId;
        uint256 price;
        uint16 maxBuyers;
        uint256 currentBuyers;
        uint256 monthWorked;
        TierStatus status;
    }

    enum TierStatus {
        Valid,
        Invalid
    }

    uint256 public tierCounter;
    mapping(uint256 => uint256) public projectTierCounter;
    mapping(uint256 => address) public projectOwner;
    mapping(address => mapping(uint256 => ProjectInfo)) internal projectInfo;

    struct ProjectInfo {
        /// the Project Id
        uint256 id;
        /// @notice Artist Name
        string name;
        /// @notice Artist NFT Symbol
        string symbol;
        /// @notice Artist Level
        uint8 artistLevel;
        /// @notice Artist Address
        //address artistAddress;
        /// @notice Artist Reward NFT Contract
        address nftContract;
    }

     //projectId => tierId => numberOfTolalRewards
    mapping(uint256 => mapping(uint256 => uint16)) public rewards; 
    //projectId => nftId => bool
    mapping(uint256 => mapping(uint256 => bool)) public paid;

    // projectId => collectorAddess => bool
    mapping(uint256 => mapping(address => bool)) public whitelist; 
    // projectId => whitelistEnabled
    mapping(uint256 => bool) public whitelistEnabled;
    // projectId => blocked
    mapping(uint256 => bool) public blocked;

    event CreateProjectAccount(
        uint256 id,
        address artist,
        string _name,
        string _symbol
    );

    event CreateTier(
        uint256 id,
        address indexed artist,
        uint256 projectTierCounter,
        string uuid,
        uint256 projectId,
        uint256 price,
        uint256 _maxBuyer
    );

    event BuySubscription(
        address indexed buyer,
        uint256 nftId,
        uint256 projectId,
        uint32 tierId,
        string cid
    );
    
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        address _treasury,
        address _payments,
        address _NFTSubscription,
        address _NFTArt2ActFactory
    ) initializer public {
        __AccessControl_init();
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        a2aTreasury = payable(_treasury);
        a2aPayments = payable(_payments);
        a2aNFTsubscription = _NFTSubscription;
        a2aNFTfactory = _NFTArt2ActFactory;
        startIndex = 0;
        basis_points = 10000;
        platform_fee = 1000; //10%
        maxTiersPerProject = 1;
    }

    function transferOwnership(uint256 projectId, address newOwner) public {
        require (projectOwner[projectId] == msg.sender, "You are not the project owner to change the owner");
        projectOwner[projectId] = newOwner;
    }
 
    function blockContract(uint256 projectId) public {
        require (projectOwner[projectId] == msg.sender, "You are not the project owner to block the contract");
        blocked[projectId] = true;
    }

    function enableWhitelist(uint256 projectId, bool enabled) public {
        require (projectOwner[projectId] == msg.sender, "You are not the project owner to enable the white list");
        whitelistEnabled[projectId] = enabled;
    }

    function setWhitelist(uint256 projectId, address [] memory allowlist) public {
        require (projectOwner[projectId] == msg.sender, "You are not the project owner to set the white list");
        for (uint16 i = 0; i < allowlist.length; i++) {
            whitelist[projectId][allowlist[i]] = true;
        }
    }
 

    function setTotalRewardsPerTier(uint256 projectId, uint256 tierId, uint16 totalRewards) public onlyRole(DEFAULT_ADMIN_ROLE) {
        rewards[projectId][tierId] = totalRewards;
    }

    function setFees(uint _basis_points, uint _platform_fee) public onlyRole(DEFAULT_ADMIN_ROLE) {
        basis_points = _basis_points;
        platform_fee = _platform_fee;
    }

    function setMaxTiersPerProject(uint32 max) public onlyRole(DEFAULT_ADMIN_ROLE) {
        maxTiersPerProject = max;
    }

    function refund(uint256 projectId, uint256 tierId, uint256 nft1155Id) public {
       require(
            SubscriptionNFT(a2aNFTsubscription).balanceOf(msg.sender, nft1155Id) >= 1,
            "You are not the NFT owner");
       SubscriptionNFT(a2aNFTsubscription).burn(msg.sender, nft1155Id, 1);
       uint256 claimed = A2ANFTFactory721(a2aNFTfactory).getMintCounter(msg.sender, projectId, tierId);
       uint16 total = rewards[projectId][tierId];
       uint256 price = tiers[projectId][tierId].price;
       uint256 fees = Math.mulDiv(price, platform_fee, basis_points);
       //uint256 amount = SafeMath.mul(SafeMath.sub(price, fees), SafeMath.div(SafeMath.sub(total, claimed), total)); // (price-fees) * (total-claimed)/total
       uint256 amount = SafeMath.div(SafeMath.mul(SafeMath.sub(price, fees), SafeMath.sub(total, claimed)), total); // (price-fees) * (total-claimed) / total
       A2APayments(a2aPayments).withdraw(payable(msg.sender), amount);
    }

    function claimFunds(uint256 projectId, uint256 tierId, uint256 nftId) public {
        require(projectOwner[projectId] == msg.sender &&
                A2ANFTFactory721(a2aNFTfactory).indexToOwner(projectId) == msg.sender, 
                "Funds Claimed by non-owner");
        require(A2ANFTFactory721(a2aNFTfactory).isValid(projectId, nftId),
                "NFT is not validated");
        //require(A2ANFTFactory721(a2aNFTfactory).isPreminted(projectId, nftId),    
        //        "NFT is not pre-minted");
        require(!paid[projectId][nftId], 
                "NFT already paid");
        uint16 total = rewards[projectId][tierId];
        uint256 price = tiers[projectId][tierId].price;
        uint256 fees = Math.mulDiv(price, platform_fee, basis_points); //price * platform_fee / basis_points;
        uint256 amount = SafeMath.div(SafeMath.sub(price, fees), total); //(price - fees) / total;
        //payable(msg.sender).transfer(amount);
        A2APayments(a2aPayments).withdraw(payable(msg.sender), amount);
        paid[projectId][nftId] = true;
    }

    function createProjectAndTiers(
        string memory _name,
        string memory _symbol,
        uint16 _royalty,
        uint256[] memory _price,
        uint16[] memory _maxBuyer,
        string[] memory _backendId
    ) external returns (uint256) {
        uint256 counter = createProject(_name, _symbol, _royalty);
        createMultipleTiers(_price, _maxBuyer, _backendId, counter);
        projectOwner[counter] = msg.sender;
        return counter;
    }

    function createProject(string memory _name, string memory _symbol, uint16 _royalty)
        public
        returns (uint256)
    {
        //Check if name and symbol are valid
        require(
            bytes(_name).length > 0 && bytes(_symbol).length > 0,
            "Invalid name or symbol"
        );
        //int16 royalty, string memory name, string memory symbol
        address _nft = A2ANFTFactory721(a2aNFTfactory).deployArt2ActNFTContract(_royalty, _name, _symbol);
        //Art2ActNFTFactory(NFTArt2ActFactory).grantRole(keccak256("MINTER_ROLE"), msg.sender);

        projectInfo[msg.sender][projectsCounter] = ProjectInfo(
            projectsCounter,
            _name,
            _symbol,
            1,
            _nft
        );

        creatorProjectsCounter[msg.sender]++; 

        emit CreateProjectAccount(
            projectsCounter++,
            msg.sender,
            _name,
            _symbol
        );

        return projectsCounter - 1;
    }

    function createMultipleTiers(
        uint256[] memory _price,
        uint16[] memory _maxBuyer,
        string[] memory _backendId,
        uint256 project
    ) public {
        require(
            project >= 0,
            "Project not created"
        );
        require(_price.length == _maxBuyer.length && _price.length == _backendId.length, "Invalid Array Length");

        uint8 i = 0;
        while (i < _price.length) {
            require(
                _price[i] > 0 && _maxBuyer[i] > 0,
                "Invalid Tier Parameters"
            );
            tiers[project][projectTierCounter[project]] = TierInfo(
                tierCounter,
                _backendId[i],
                _price[i],
                _maxBuyer[i],
                0,
                0,
                TierStatus.Valid
            );
            emit CreateTier(
                tierCounter++,
                msg.sender,
                projectTierCounter[project]++,
                _backendId[i],
                project,
                _price[i],
                _maxBuyer[i]
            );
            ++i;
        }
    }

    /*added to input parameters: string cid (from IPFS)*/
   
    function buySubscription(
        uint256 projectId,
        uint32 tierId,
        uint16 months,
        string memory CID
    ) public payable nonReentrant {      
        TierInfo memory _tier = getTier(projectId, tierId);
        uint256 tierPriceMonths = SafeMath.mul(_tier.price, months); //_tier.price * _months;
        uint256 fees = SafeMath.div(SafeMath.mul(tierPriceMonths, platform_fee), basis_points);  //((_tier.price * _months) * platform_fee) / basis_points;
        uint256 owner_rev = SafeMath.sub(tierPriceMonths, fees); //_tier.price * _months - fees;
        //Require not blocked
        require(
            !blocked[projectId],
            "Project blocked by the owner, you can't buy it"
        );
        //Require whitelistEnabled
        require(
            !whitelistEnabled[projectId] || whitelist[projectId][msg.sender],
            "You are not in whitelist"
        );
        //Require that value is equal to price
        require(
            _tier.price == msg.value,
            "Value is not equal to the price to buy"
        );
        //Require that max tier buyers was not reached
        require(
            _tier.currentBuyers < _tier.maxBuyers,
            "Max Tier Buyers reached"
        );

        //Require that the tier is still valid to buy
        require(
            _tier.status == TierStatus.Valid,
            "The tier is no longer for sale"
        );

        //(bool success1, ) = address(this).call{value: owner_rev}(""); //To remove
        (bool success1, ) = a2aPayments.call{value: owner_rev}("");
        require(success1, "Failed to send Amount (ETH) to this Contract");

        (bool success2, ) = a2aTreasury.call{value: fees}("");
        require(success2, "Failed to send Amount (ETH) to Treasury");
        
        subscriptionCounter++;
        //userSubscriptionCounter[msg.sender]++;
        tiers[projectId][tierId].currentBuyers++;
        //MembershipNFT(NFTMembership)._setApprovedSigner(msg.sender);
        //MembershipNFT(NFTMembership).mint(msg.sender, _artist, _months, project, artistMembershipNFTImage[_artist]);
        uint256 nftId = projectId * maxTiersPerProject + tierId;
        SubscriptionNFT(a2aNFTsubscription).mintToken(msg.sender, nftId, 1, CID);

        emit BuySubscription(msg.sender, nftId, projectId, tierId, CID);
    }

    function getTier(uint256 _project, uint256 _id)
        public
        view
        returns (TierInfo memory)
    {
        return tiers[_project][_id];
    }

    function getProject(address _artist, uint256 _id)
        public
        view
        returns (ProjectInfo memory)
    {
        return projectInfo[_artist][_id];
    }
}

