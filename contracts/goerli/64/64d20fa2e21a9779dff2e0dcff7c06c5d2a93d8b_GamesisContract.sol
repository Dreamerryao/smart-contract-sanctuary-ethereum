// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/interfaces/IERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

/**
 * @title The EthGames contract
 */
contract GamesisContract is VRFConsumerBaseV2, Ownable, AccessControl {
  VRFCoordinatorV2Interface immutable public COORDINATOR;
  LinkTokenInterface immutable public LINK_TOKEN;
  
  enum RoundState { OPEN, CLOSED, FINISHED }

  struct Round {
    RoundState roundState;
    uint16[] players;
    mapping(uint16 => uint16) playerIndexes;
    uint16 currentPlayers;
    uint16 totalPlayers;
    uint16 winner;
  }

  event RoundEliminationRequest(uint256 roundId, uint256 requestId, uint32 numWords);
  event RoundEliminations(uint256 roundId, uint16[] tokenIds);
  event RoundWinner(uint256 roundId, uint16 tokenId);

  uint64 public subscriptionId;
  bytes32 public keyHash;
  uint32 public callbackGasLimit = 2500000;
  uint16 public requestConfirmations = 3;
  uint32 public numWords = 100;

  uint256 public currentRound = 0;
  mapping(uint256 => Round) public rounds;

  mapping(uint256 => uint256) public requestIdRoundMap;

  constructor(
    uint64 subscriptionId_,
    address vrfCoordinator_,
    address link_,
    bytes32 keyHash_
  ) VRFConsumerBaseV2(vrfCoordinator_) {
    COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator_);
    LINK_TOKEN = LinkTokenInterface(link_);
    keyHash = keyHash_;
    subscriptionId = subscriptionId_;
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
  }

  // @dev views
  function playerAtIndex(uint256 roundId, uint16 index) public view virtual returns (uint16 tokenId) {
    return rounds[roundId].players[index];
  }

  function playerIndex(uint256 roundId, uint16 tokenId) public view virtual returns (uint16 index) {
    return rounds[roundId].playerIndexes[tokenId];
  }

   function playerStillInRound(uint256 roundId, uint16 tokenId) public view virtual returns (bool stillInRound) {
     uint16 index = rounds[roundId].playerIndexes[tokenId];
     return rounds[roundId].players[index] == tokenId;
   }
  // @dev end of views

  // @dev external admin functions
  function eliminatePlayers(uint32 amountOfPlayers) external onlyRole(DEFAULT_ADMIN_ROLE) isClosed {
    Round storage round = rounds[currentRound];
    uint32 castedCurrentPlayers = uint32(round.currentPlayers);
    require(castedCurrentPlayers > 0, "EthGames: Cannot run an elimination round if not at least 1 token is added.");
    require(castedCurrentPlayers >= amountOfPlayers, "EthGames: Cannot run an elimination round for more than the amount of participants.");

    // If there is one player left
    if(castedCurrentPlayers == 1) {
      _setWinner(round.players[0], round);
      return;
    }

    uint32 groups = uint32(amountOfPlayers / numWords);
    uint32 restPlayers = amountOfPlayers % numWords;

    if(groups > 0) {
      for (uint32 i = 0; i < groups; i++) {
        _requestAmount(currentRound, numWords);
      }
    }

    if(restPlayers > 0) {
      _requestAmount(currentRound, restPlayers);
    }
  }

  function setNumWords(uint32 numWords_) external onlyRole(DEFAULT_ADMIN_ROLE) {
    numWords = numWords_;
  }

  function updateVRFSettings(uint64 subscriptionId_, bytes32 keyHash_) external onlyRole(DEFAULT_ADMIN_ROLE) {
    subscriptionId = subscriptionId_;
    keyHash = keyHash_;
  }

  function close() external onlyRole(DEFAULT_ADMIN_ROLE) isOpen {
    rounds[currentRound].roundState = RoundState.CLOSED;
  }

  function fill(uint16 total) external onlyRole(DEFAULT_ADMIN_ROLE) isOpen {
    Round storage round = rounds[currentRound];
    uint16 currentCurrentPlayers = round.currentPlayers;
    for (uint16 i = 0; i < total; i++) {
      // Increase by one so that we start at 1
      _enterById(currentCurrentPlayers + i + 1);
    }
    round.totalPlayers = round.currentPlayers;
  }

  function newRound() external onlyRole(DEFAULT_ADMIN_ROLE) {
    currentRound += 1;
  }
  // @dev end of external admin functions

  // @dev this function gets called by the Chainlink provider
  function fulfillRandomWords(uint256 requestId_, uint256[] memory randomWords_) internal override {
    uint256 randomRequestCurrentRound = requestIdRoundMap[requestId_];
    Round storage round = rounds[randomRequestCurrentRound];
    require(round.roundState == RoundState.CLOSED, "EthGames: Round is not closed");
    uint256 total = randomWords_.length;
    if((round.currentPlayers - total) <= 0) {
      total = round.currentPlayers - 1;
    }
    uint16[] memory allRemovedIndexes = new uint16[](total);
    require(total > 0, "EthGames: Cannot fulfill with no total");

    for (uint i = 0; i < total; i++) {
      uint256 word = randomWords_[i];
      uint16 removeIndex = uint16(word % round.currentPlayers);
      allRemovedIndexes[i] = _removeIndex(removeIndex, round);
    }

    emit RoundEliminations(randomRequestCurrentRound, allRemovedIndexes);

    require(round.currentPlayers > 0, "EthGames: Cannot fulfill with no players left");

    if(round.currentPlayers <= 1) {
      _setWinner(round.players[0], round);
    }
  }
  // @dev end of chainlink functions

  // @dev end of private functions
  function _requestAmount(uint256 roundId, uint32 numWords_) private {
    uint256 requestId = COORDINATOR.requestRandomWords(
      keyHash,
      subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      numWords_
    );
    requestIdRoundMap[requestId] = roundId;
    emit RoundEliminationRequest(roundId, requestId, numWords_);
  }

  function _setWinner(uint16 tokenId, Round storage round) private {
    round.roundState = RoundState.FINISHED;
    round.winner = tokenId;
    round.currentPlayers = 0;
    emit RoundWinner(currentRound, round.winner);
  }

  function _removeIndex(uint16 index, Round storage round) private returns (uint16) {
    // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
    // then delete the last slot (swap and pop).
    uint16 lastTokenIndex = round.currentPlayers - 1;
    uint16 tokenIdToRemove = round.players[index];
    uint16 tokenIndex = round.playerIndexes[tokenIdToRemove];

    // When the token to delete is the last token, the swap operation is unnecessary
    if (tokenIndex != lastTokenIndex) {
      uint16 lastTokenId = round.players[lastTokenIndex];
      round.players[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
      round.playerIndexes[lastTokenId] = tokenIndex; // Update the moved token's index
    }

    // This also deletes the contents at the last position of the array
    delete round.playerIndexes[tokenIdToRemove];
    delete round.players[lastTokenIndex];
    round.currentPlayers--;
    return tokenIdToRemove;
  }

  function _enterById(uint16 tokenId) private {
    Round storage round = rounds[currentRound];
    require(round.playerIndexes[tokenId] == 0, "EthGames: TokenId already occupied");
    round.players.push(tokenId);
    // Add index, this is what we will use to remove the players
    round.playerIndexes[tokenId] = uint16(round.players.length - 1);
    round.currentPlayers += 1;
  }
  // @dev end of private functions

  // @dev modifiers
  modifier isOpen() {
    require(rounds[currentRound].roundState == RoundState.OPEN, "EthGames: Round is not open");
    _;
  }

  modifier isClosed() {
    require(rounds[currentRound].roundState == RoundState.CLOSED, "EthGames: Round is not closed");
    _;
  }

  modifier isFinished() {
    require(rounds[currentRound].roundState == RoundState.FINISHED, "EthGames: Round is not finished");
    _;
  }
  // @dev end of modifiers
}