pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract Gringotts is ERC1155, ERC1155Burnable, ERC1155Supply, Ownable {
    struct TradeRequest {
        address buyer;
        uint256 amount;
        uint256 totalPrice;
        bool isBought;
        bool exists;
    }

    // token 0 must be blank, so that we can use 0 as a null value
    uint256 private _tokenId = 0;
    mapping(uint256 => mapping(address => address))
        private _delegatorToDelegatee;
    mapping(uint256 => mapping(address => address))
        private _delegateeToDelegator;
    mapping(uint256 => uint256) private _deposites;
    mapping(uint256 => uint256) private _revenues;
    mapping(uint256 => address[]) private _holders;
    mapping(uint256 => TradeRequest[]) private _tradeRequests;

    constructor(string memory uri) ERC1155(uri) {}

    function mint(uint256 amount) public payable {
        _tokenId += 1;
        require(totalSupply(_tokenId) == 0, "Gringotts: token has been minted");
        _mint(msg.sender, _tokenId, amount, "");
        if (msg.value > 0) {
            _deposites[_tokenId] += msg.value;
        }
    }

    function join(
        address account,
        uint256 id,
        uint256 amount
    ) public payable onlyOwner {
        require(totalSupply(id) != 0, "Gringotts: token has not been minted");
        _mint(account, id, amount, "");
        if (msg.value > 0) {
            _deposites[id] += msg.value;
        }
    }

    function burn(
        address account,
        uint256 id,
        uint256 value
    ) public override onlyOwner {
        super.burn(account, id, value);
    }

    function burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory values
    ) public override onlyOwner {
        super.burnBatch(account, ids, values);
    }

    function addTradeOffer(uint256 id, uint256 amount) public payable {
        _tradeRequests[id].push(
            TradeRequest({
                buyer: msg.sender,
                amount: amount,
                totalPrice: msg.value,
                isBought: false,
                exists: true
            })
        );
    }

    function retriveTradeOffer(uint256 id, uint256 index) public {
        TradeRequest storage trade = _tradeRequests[id][index];
        require(
            trade.buyer == msg.sender,
            "Gringotts: only owner can retrive offer"
        );
        require(
            trade.isBought == false && trade.exists == true,
            "Gringotts: trade offer has been bought"
        );
        uint256 value = trade.totalPrice;
        trade.exists = false;
        payable(trade.buyer).transfer(value);
    }

    function acceptTradeOffer(uint256 id, uint256 index) public {
        TradeRequest storage trade = _tradeRequests[id][index];
        require(
            balanceOf(msg.sender, id) > trade.amount,
            "Gringotts: do not have enough balances"
        );
        _safeTransferFrom(msg.sender, trade.buyer, id, trade.amount, "");
        trade.isBought = true;
        trade.exists = false;
        payable(msg.sender).transfer(trade.totalPrice);
    }

    function costTo(
        uint256 id,
        address to,
        uint256 amount
    ) public onlyOwner {
        uint256 deposite = _deposites[id];
        require(deposite > amount, "Gringotts: Not enough deposite");
        _deposites[id] -= amount;
        payable(to).transfer(amount);
    }

    function receiveRevenue(uint256 id) public payable {
        _revenues[id] += msg.value;
    }

    function shareRevenue(uint256 id) public onlyOwner {
        uint256 revenue = _revenues[id];
        for (uint256 i = 0; i < _holders[id].length; i++) {
            uint256 value = (revenue * balanceOf(_holders[id][i], id)) /
                totalSupply(id);
            if (value > 0) {
                payable(_holders[id][i]).transfer(value);
            }
        }
    }

    function revenueToDeposite(uint256 id, uint256 amount) public onlyOwner {
        uint256 revenue = _revenues[id];
        require(revenue > amount, "Gringotts: Not enough revenue");
        _revenues[id] -= amount;
        _deposites[id] += amount;
    }

    function delegate(
        uint256 id,
        address delegator,
        address delegatee
    ) public {
        require(
            delegator == msg.sender,
            "Gringotts: only delegator can delegate"
        );
        require(
            delegatee != address(0),
            "Gringotts: delegatee cannot be zero address"
        );
        require(
            balanceOf(delegator, id) > 0,
            "Gringotts: delegator must have balance"
        );
        require(
            balanceOf(delegatee, id) > 0,
            "Gringotts: delegatee must have balance"
        );
        require(
            _delegatorToDelegatee[id][delegator] == address(0),
            "Gringotts: delegator already has delegatee"
        );
        _delegatorToDelegatee[id][delegator] = delegatee;
        _delegateeToDelegator[id][delegatee] = delegator;
    }

    function revokeDelegate(uint256 id, address delegator) public {
        require(
            delegator == msg.sender,
            "Gringotts: only delegator can delegate"
        );
        require(
            _delegatorToDelegatee[id][delegator] != address(0),
            "Gringotts: delegator does not have delegatee"
        );
        address delegatee = _delegatorToDelegatee[id][delegator];
        _delegatorToDelegatee[id][delegator] = address(0);
        _delegateeToDelegator[id][delegatee] = address(0);
    }

    function getVotePower(uint256 id, address account)
        public
        view
        returns (uint256)
    {
        uint votes = 0;
        if (_delegatorToDelegatee[id][account] == address(0)) {
            votes = balanceOf(account, id);
        }
        if (_delegateeToDelegator[id][account] != address(0)) {
            votes += balanceOf(_delegateeToDelegator[id][account], id);
        }
        return votes;
    }

    function tradeRequest(uint256 id)
        public
        view
        returns (TradeRequest[] memory)
    {
        return _tradeRequests[id];
    }

    function increaseTokenId() public returns (uint256) {
        _tokenId += 1;
        return _tokenId;
    }

    function setApprovalForAll(address operator, bool approved)
        public
        override
    {}

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
        for (uint256 i = 0; i < ids.length; i++) {
            if (to != address(0) && balanceOf(to, ids[i]) == 0) {
                _holders[ids[i]].push(to);
            }
        }
    }

    function _addHolders(uint256 id, address account) internal {
        uint256 value = balanceOf(account, id);
        if (value == 0) {
            _holders[id].push(account);
        }
    }
}

