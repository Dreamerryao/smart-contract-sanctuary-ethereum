pragma solidity ^0.8.7;

import "@openzeppelin/contracts/utils/Timers.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "./Gringotts.sol";

contract Erised {
    using Timers for Timers.BlockNumber;
    using SafeCast for uint256;

    enum ProposalState {
        Pending,
        Active,
        Canceled,
        Defeated,
        Succeeded,
        Expired,
        Executed
    }

    enum VoteOptions {
        NotVoted,
        Abstain,
        Against,
        For
    }

    struct ProposalCore {
        address proposer;
        Timers.BlockNumber voteStart;
        Timers.BlockNumber voteEnd;
        bool executed;
        bool canceled;
        uint256 value;
        bytes calldatas;
        uint256 tokenId;
        uint256 voteCounts;
    }

    Gringotts private immutable i_gringotts;
    uint64 private immutable i_votingDelay;
    uint64 private immutable i_votingPeriod;

    uint256 private _proposalId;
    mapping(uint256 => ProposalCore) private _proposals;
    mapping(uint256 => mapping(address => VoteOptions)) private _voteDetail;

    event ProposalCreated(
        uint256 proposalId,
        address proposer,
        uint256 tokenId,
        uint256 value,
        bytes calldatas,
        uint256 startBlock,
        uint256 endBlock
    );
    event ProposalCanceled(uint256 proposalId);
    event ProposalExecuted(uint256 proposalId);
    event VoteCast(
        address voter,
        uint256 proposalId,
        uint256 tokenId,
        uint8 support,
        uint256 weight,
        string reason
    );

    constructor(
        address gringottsAddress,
        uint64 votingDelay,
        uint64 votingPeriod
    ) {
        i_gringotts = Gringotts(gringottsAddress);
        i_votingDelay = votingDelay;
        i_votingPeriod = votingPeriod;
    }

    function propose(uint256 tokenId, bytes memory calldatas) public payable {
        require(
            _getVotes(msg.sender, tokenId) > 0,
            "Erised: must have tokens to propose"
        );
        _proposalId += 1;
        ProposalCore storage proposal = _proposals[_proposalId];
        require(
            proposal.voteStart.isUnset(),
            "Erised: proposal already exists"
        );

        uint64 snapshot = block.number.toUint64() + i_votingDelay;
        uint64 deadline = snapshot + i_votingPeriod;

        proposal.proposer = msg.sender;
        proposal.voteStart.setDeadline(snapshot);
        proposal.voteEnd.setDeadline(deadline);
        proposal.value = msg.value;
        proposal.calldatas = calldatas;
        proposal.tokenId = tokenId;

        emit ProposalCreated(
            _proposalId,
            msg.sender,
            tokenId,
            msg.value,
            calldatas,
            snapshot,
            deadline
        );
    }

    function execute(uint256 proposalId) public {
        ProposalState status = state(proposalId);
        require(
            status == ProposalState.Succeeded,
            "Erised: proposal not successful"
        );
        ProposalCore storage proposal = _proposals[proposalId];
        proposal.executed = true;

        emit ProposalExecuted(proposalId);

        (bool success, ) = address(i_gringotts).call{value: proposal.value}(
            proposal.calldatas
        );

        if (!success) {
            proposal.executed = false;
        }
    }

    function cancel(uint256 proposalId) public {
        ProposalState status = state(proposalId);
        require(
            status != ProposalState.Canceled &&
                status != ProposalState.Expired &&
                status != ProposalState.Executed,
            "Governor: proposal not active"
        );
        ProposalCore storage proposal = _proposals[proposalId];
        require(
            msg.sender == proposal.proposer,
            "Erised: only proposal owner can cancel"
        );
        proposal.canceled = true;

        if (proposal.value > 0) {
            payable(proposal.proposer).transfer(proposal.value);
        }
        emit ProposalCanceled(proposalId);
    }

    function castVote(
        uint256 proposalId,
        uint256 tokenId,
        uint8 voteResult,
        string memory reason
    ) public {
        require(
            state(proposalId) == ProposalState.Active,
            "Erised: vote not currently active"
        );
        require(
            _voteDetail[proposalId][msg.sender] == VoteOptions.NotVoted,
            "Erised: voter already voted"
        );
        VoteOptions voteOption = getVoteOptions(voteResult);
        uint256 weight = _getVotes(msg.sender, tokenId);
        _countVote(proposalId, msg.sender, voteOption, weight);
        emit VoteCast(
            msg.sender,
            proposalId,
            tokenId,
            voteResult,
            weight,
            reason
        );
    }

    function state(uint256 proposalId) public view returns (ProposalState) {
        ProposalCore storage proposal = _proposals[proposalId];

        if (proposal.executed) {
            return ProposalState.Executed;
        }
        if (proposal.canceled) {
            return ProposalState.Canceled;
        }
        uint256 snapshot = proposalSnapshot(proposalId);
        if (snapshot == 0) {
            revert("Erised: unknown proposal id");
        }
        if (snapshot >= block.number) {
            return ProposalState.Pending;
        }
        uint256 deadline = proposalDeadline(proposalId);
        if (deadline >= block.number) {
            return ProposalState.Active;
        }
        if (_voteSucceeded(proposalId)) {
            return ProposalState.Succeeded;
        } else {
            return ProposalState.Defeated;
        }
    }

    function proposalSnapshot(uint256 proposalId)
        public
        view
        virtual
        returns (uint256)
    {
        return _proposals[proposalId].voteStart.getDeadline();
    }

    function proposalDeadline(uint256 proposalId)
        public
        view
        virtual
        returns (uint256)
    {
        return _proposals[proposalId].voteEnd.getDeadline();
    }

    function getVoteOptions(uint8 option) public pure returns (VoteOptions) {
        if (option == 0) {
            return VoteOptions.NotVoted;
        } else if (option == 1) {
            return VoteOptions.Abstain;
        } else if (option == 2) {
            return VoteOptions.Against;
        } else if (option == 3) {
            return VoteOptions.For;
        } else {
            revert("Erised: invalid vote option");
        }
    }

    function hasVoted(uint256 proposalId, address account)
        public
        view
        returns (bool)
    {
        return _voteDetail[proposalId][account] != VoteOptions.NotVoted;
    }

    function _voteSucceeded(uint256 proposalId) internal view returns (bool) {
        ProposalCore storage proposal = _proposals[proposalId];
        uint256 totalSupply = i_gringotts.totalSupply(proposal.tokenId);
        return proposal.voteCounts * 3 > totalSupply * 2;
    }

    function _getVotes(address account, uint256 tokenId)
        internal
        view
        returns (uint256)
    {
        return i_gringotts.getVotePower(tokenId, account);
    }

    function _countVote(
        uint256 proposalId,
        address account,
        VoteOptions voteOption,
        uint256 weight
    ) internal {
        _voteDetail[proposalId][account] = voteOption;
        if (voteOption == VoteOptions.For) {
            _proposals[proposalId].voteCounts += weight;
        }
    }

    receive() external payable {
        require(msg.sender == address(i_gringotts), "Erised: only gringotts");
    }

    fallback() external payable {
        require(msg.sender == address(i_gringotts), "Erised: only gringotts");
    }
}

