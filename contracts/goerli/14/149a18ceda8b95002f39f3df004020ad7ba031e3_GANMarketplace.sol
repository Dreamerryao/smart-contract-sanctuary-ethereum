pragma solidity ^0.8.15;

import "solmate/auth/Owned.sol";
import "solmate/tokens/ERC1155.sol";
import "solmate/utils/ReentrancyGuard.sol";
import "openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";
import "openzeppelin-contracts/contracts/token/ERC721/IERC721.sol";
import "openzeppelin-contracts/contracts/token/ERC1155/IERC1155.sol";

contract GANMarketplace is ERC1155, Owned, ReentrancyGuard {

    string public name;
    string public symbol;
    uint public receiveETHToken;
    bool public receiveETHActive;

    mapping (uint => bool) public mintSingleNoLimitsActive;
    mapping (uint => bool) public mintSingleMaxSupplyActive;
    mapping (uint => bool) public mintSingleMaxSupplyMaxPerWalletActive;
    mapping (uint => uint) public tokenCost;
    mapping (uint => uint) public tokenMaxSupply;
    mapping (uint => uint) public tokenCurrentSupply;
    mapping (uint => uint) public tokenMaxPerWallet;
    mapping (uint => string) private tokenURIs;
    mapping (address => bool) public proxyMintAddresses;
    mapping (address => bool) public proxyBurnAddresses;
    mapping (address => mapping (uint => uint)) public tokenMintsByReceiverByToken;

    /*//////////////////////////////////////////////////////////////
                             EVENTS
    //////////////////////////////////////////////////////////////*/

    event URIUpdated(string value, uint indexed id);
    event MintSingleNoLimits(address indexed tokenReceiver, uint indexed id, uint indexed amount);
    event MintSingleMaxSupply(address indexed tokenReceiver, uint indexed id, uint indexed amount);
    event MintSingleMaxSupplyMaxPerWallet(address indexed tokenReceiver, uint indexed id, uint indexed amount); 

    /*//////////////////////////////////////////////////////////////
                             ENUMS
    //////////////////////////////////////////////////////////////*/

    enum ReceiveMintType {
        MintSingleNoLimits,
        MintSingleMaxSupply,
        MintSingleMaxSupplyMaxPerWallet
    }

    /*//////////////////////////////////////////////////////////////
                             CONSTRUCTOR
    //////////////////////////////////////////////////////////////*/

    constructor(
        string memory setName,
        string memory setSymbol,
        address setOwner
    )Owned(setOwner){
        name = setName;
        symbol = setSymbol;
    }

    /*//////////////////////////////////////////////////////////////
                             MODIFIERS
    //////////////////////////////////////////////////////////////*/

    modifier priceEnforced(uint id, uint amount) {
        require(msg.value == tokenCost[id] * amount, "Your payment amount is incorrect.");
        _;
    }

    modifier proxyApprovalMint {
        require(proxyMintAddresses[msg.sender], "You are not approved to mint from this address.");
        _;
    }

    modifier proxyApprovalBurn() {
        require(proxyBurnAddresses[msg.sender], "You are not approved to burn from this address.");
        _;
    } 

    /*//////////////////////////////////////////////////////////////
                             MINT LOGIC
    //////////////////////////////////////////////////////////////*/

    function mintSingleNoLimits(address tokenReceiver, uint id, uint amount) public payable priceEnforced(id, amount) nonReentrant{
        require(mintSingleNoLimitsActive[id], "Minting is not currently active from this function.");
        _mint(tokenReceiver, id, amount, "");
        tokenCurrentSupply[id] += amount;
    }

    function mintSingleMaxSupply(address tokenReceiver, uint id, uint amount) public payable priceEnforced(id, amount) nonReentrant{
        require(mintSingleMaxSupplyActive[id], "Minting is not currently active from this function.");
        require(tokenCurrentSupply[id] + amount <= tokenMaxSupply[id], "You are trying to mint more than the max supply.");
        _mint(tokenReceiver, id, amount, "");
        tokenCurrentSupply[id] += amount;
    }

    function mintSingleMaxSupplyMaxPerWallet(address tokenReceiver, uint id, uint amount) public payable priceEnforced(id, amount) nonReentrant{
        require(mintSingleMaxSupplyMaxPerWalletActive[id], "Minting is not currently active from this function.");
        require(tokenCurrentSupply[id] + amount <= tokenMaxSupply[id], "You are trying to mint more than the max supply.");
        require(tokenMintsByReceiverByToken[tokenReceiver][id] + amount <= tokenMaxPerWallet[id], "You are trying to mint more than the max per wallet.");
        _mint(tokenReceiver, id, amount, "");
        tokenCurrentSupply[id] += amount;
        tokenMintsByReceiverByToken[tokenReceiver][id] += amount;
    }

    /*//////////////////////////////////////////////////////////////
                             BURN LOGIC
    //////////////////////////////////////////////////////////////*/

    function burnSingle(address tokenOwner, uint id, uint amount) public nonReentrant {
        require(balanceOf[tokenOwner][id] >= amount, "You are trying to burn more than you own.");
        _burn(tokenOwner, id, amount);
        tokenCurrentSupply[id] -= amount;
    }

    /*//////////////////////////////////////////////////////////////
                             PROXY LOGIC
    //////////////////////////////////////////////////////////////*/

     function proxyMint(address to, uint id, uint amount, bytes memory data) public proxyApprovalMint {
        _mint(to, id, amount, data);
        tokenCurrentSupply[id] += amount;
    }

    function proxyAirdropMint(address[] memory to, uint id, uint amount, bytes memory data) public proxyApprovalMint {
        for (uint i = 0; i < to.length; i++) {
            _mint(to[i], id, amount, data);
            tokenCurrentSupply[id] += amount;
        }
    }

    function proxyBatchMint(address to, uint[] memory ids, uint[] memory amounts, bytes memory data) public proxyApprovalMint {
        _batchMint(to, ids, amounts, data);
        for (uint i = 0; i < ids.length; i++) {
            tokenCurrentSupply[ids[i]] += amounts[i];
        }
    }

    function proxyBurnSingle(address tokenOwner, uint id, uint amount) external proxyApprovalBurn {
        _burn(tokenOwner, id, amount);
        tokenCurrentSupply[id] -= amount;
    }

    function proxyBurnBatch(address tokenOwner, uint[] calldata ids, uint[] calldata amounts) external proxyApprovalBurn {
        _batchBurn(tokenOwner, ids, amounts);
        for (uint i = 0; i < ids.length; i++) {
            tokenCurrentSupply[ids[i]] -= amounts[i];
        }
    }

    /*//////////////////////////////////////////////////////////////
                             RECEIVE LOGIC
    //////////////////////////////////////////////////////////////*/

    receive() external payable {
        require(receiveETHActive);
        if(msg.value / tokenCost[receiveETHToken] < 1){
            mintSingleNoLimits(msg.sender, receiveETHToken, 1);
        }
        mintSingleNoLimits(msg.sender, receiveETHToken, msg.value / tokenCost[receiveETHToken]);
    }

    /*//////////////////////////////////////////////////////////////
                             METADATA LOGIC
    //////////////////////////////////////////////////////////////*/

    function uri(uint id) public view override returns (string memory) {
        return tokenURIs[id];
    }

    /*//////////////////////////////////////////////////////////////
                             ADMIN LOGIC
    //////////////////////////////////////////////////////////////*/
    
    function updateReceiveETHToken(uint tokenID) external onlyOwner {
        receiveETHToken = tokenID;
    }

    function flipReceiveETHActive() external onlyOwner {
        receiveETHActive = !receiveETHActive;
    }

    function flipMintSingleNoLimitsActive(uint tokenID) external onlyOwner {
        mintSingleNoLimitsActive[tokenID] = !mintSingleNoLimitsActive[tokenID];
    }

    function flipMintSingleMaxSupplyActive(uint tokenID) external onlyOwner {
        mintSingleMaxSupplyActive[tokenID] = !mintSingleMaxSupplyActive[tokenID];
    }

    function flipMintSingleMaxSupplyMaxPerWalletActive(uint tokenID) external onlyOwner {
        mintSingleMaxSupplyMaxPerWalletActive[tokenID] = !mintSingleMaxSupplyMaxPerWalletActive[tokenID];
    }

    function updateTokenCost(uint tokenID, uint newCost) external onlyOwner {
        tokenCost[tokenID] = newCost;
    }

    function updateTokenMaxSupply(uint tokenID, uint newMaxSupply) external onlyOwner {
        tokenMaxSupply[tokenID] = newMaxSupply;
    }

    function updateTokenMaxPerWallet(uint tokenID, uint newMaxPerWallet) external onlyOwner {
        tokenMaxPerWallet[tokenID] = newMaxPerWallet;
    }

    function updateTokenURI(uint tokenID, string calldata newURI) external onlyOwner {
        tokenURIs[tokenID] = newURI;
        emit URIUpdated(newURI, tokenID);
    }

    function updateTokenURIBulk(uint[] calldata tokenIDs, string[] calldata newURIs) external onlyOwner {
        require(tokenIDs.length == newURIs.length, "You must provide the same number of tokenIDs and newURIs.");
        for (uint i = 0; i < tokenIDs.length; i++) {
            tokenURIs[tokenIDs[i]] = newURIs[i];
            emit URIUpdated(newURIs[i], tokenIDs[i]);
        }
    }

    function flipProxyMintAddressActive(address proxyAddress) external onlyOwner {
        proxyMintAddresses[proxyAddress] = !proxyMintAddresses[proxyAddress];
    }

    function flipProxyBurnAddressActive(address proxyAddress) external onlyOwner {
        proxyBurnAddresses[proxyAddress] = !proxyBurnAddresses[proxyAddress];
    }

    function withdraw() external onlyOwner {
        uint balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    function withdrawERC20(address tokenAddress) external onlyOwner {
        IERC20(tokenAddress).transfer(msg.sender, IERC20(tokenAddress).balanceOf(address(this)));
    }

    function withdrawERC721(address tokenAddress, uint tokenID) external onlyOwner {
        IERC721(tokenAddress).transferFrom(address(this), msg.sender, tokenID);
    }

    function withdrawERC1155(address tokenAddress, uint tokenID, uint amount) external onlyOwner {
        IERC1155(tokenAddress).safeTransferFrom(address(this), msg.sender, tokenID, amount, "");
    }

}