// INTERFACES
interface IERC721 is IERC165 {
    // return the balance of an address
    function balanceOf(address owner) external view returns (uint256);
    // return the address that owns a specific token
    function ownerOf(uint256 tokenId) external view returns (address owner);
    // return array of tokens owned by specific address
    function tokensOfOwner(address owner) external view returns (uint256[] memory);
}

// CONTRACT
contract SrBonerTest is ERC1155, Ownable {

    // (1) CONSTANTS PUBLIC
    uint256 public constant MAX_POOL_STAGE_1 = 1000; // One Token for Each DVDA -> 1000 Total DVDA MAX
    uint256 public constant MAX_POOL_TOTAL = 6969; // The maximum available tokens (the max supply)
    uint256 public constant MAX_MINTS_STAGE_1 = 1; // Max mintable tokens for each Stage 1 (for each DVDA)
    uint256 public constant MAX_MINTS_STAGE_2 = 5; // Max mintable tokens for each user in the ecosystem (GHN, AHC, BNNO, RTBNNO, MGHN)
    uint256 public constant MAX_MINTS_PUBLIC = 5; // Max mintable tokens for public
    uint256 public constant SRBONER_PRICE_STAGE_1 = 0 ether; // Sr Boner Free for DVDA holders (1 is free)
    uint256 public constant SRBONER_PRICE_STAGE_2 = 0.069 ether; // Sr Boner Price for users in the whole ecosystem
    uint256 public constant SRBONER_PRICE_PUBLIC = 0.069 ether; // Sr Boner Price for the public
    uint256 public constant SRBONER_BURN_QUANTITY = 1; // Sr Boner Burning Quantity for Exchange:)
    uint256 public constant SRBONER_TYPE_ID = 0; // Sr Boner Token Type ID

    string public name; // The token name
    string public symbol; // The token symbol

    // (2) VARIABLES
    string private baseURI; // The base uri
    uint256 public _minted = 0; // The counter to track how many tokens are currently minted
    address SuperSrBoner; // The wallet address

    // (3) CONTROLLERS
    bool public Stage1Paused = true; // Pause flag for DVDA sale stage
    bool public Stage2Paused = true; // Pause flag for Ecosystem sale stage
    bool public PublicPaused = true; // Pause flag for Public sale stage

    // (4) MAPPINGS
    mapping(uint256 => bool) public DVDATokenIDBlacklisted; // Map DVDA token id with the blacklisted flag
    mapping (uint256 => bool) public AlreadyClaimedDVDA; // Map DVDA token id with the has already claimed its boner flag
    mapping (address => uint256) public AmountClaimedStage2; // Map address with how many tokens has already claimed (ecosystem)
    mapping (address => uint256) public AmountClaimedPublic; // Map the address with how many token has already claimed (public)

    // (5) INSTANCES (Ecosystem smart contracts)
    IERC721 DVDA = IERC721(0x5AFf73a617eb10F2e58BcFd78EdbBDec76B42033); // The DVDA TEST contract address
    IERC721 GHN = IERC721(0x61De2F511bEEb846057aD9dFdf2c8eb1dFB74d20); // The GodHatesNFTees TEST contract address
    IERC1155 BNNO = IERC1155(0xd3926f5893916E6B9a2fE832B685A587f009C634); // The Sr Bananos TEST contract address
    IERC721 AHC = IERC721(0x28cC2e34F93c5f42b7EC69458027f74403A03eA8); // The Ape Hater Club TEST contract address
    IERC1155 RTBNNO = IERC1155(0x7D78cAd230362e8bF5073cD9708510A2CF7B7c48); // The Rotten Bananos TEST contract address
    IERC721 MGHN = IERC721(0x421060E8e5022810c54B3d192BF7667355B21035); // The Mutant GodHatesNFTees TEST contract address


    // (6) CONSTRUCTOR (Executed only first time on deploy)
    constructor(string memory _uri, string memory _name, string memory _symbol) ERC1155 (_uri) {
        name = _name; // assign the token name
        symbol = _symbol; // assign the token symbol
    }


    // (7) MINT FUNCTIONALITY

    // (7.1) Mint function for Stage 1 Whitelist (the DVDA users)
    function mintStage1(uint256 DVDA_ID) public payable { // we ensure that the function can send and receive eth payable keyword and we call it with the DVDA ID
        require(!Stage1Paused, "Stage 1 Sale is paused (DVDA Whitelist)");// we ensure/require that the DVDA WL Phase 1 is not paused (so the flag is false)
        require(MAX_MINTS_STAGE_1 + _minted <= MAX_POOL_STAGE_1, "Exceed Stage 1 supply (DVDA Whitelist)"); // we ensure that there are still available mint for dvda
        address _owner = DVDA.ownerOf(DVDA_ID); // set the owner as the wallet owning the dvda token
        require(_owner == msg.sender, "Must be the Owner of the DVDA TOKEN ID to mint"); // we ensure that the caller of the mint function owns the dvda
        require(msg.value == MAX_MINTS_STAGE_1 * SRBONER_PRICE_STAGE_1, "Invalid or insufficient funds"); // we ensure that the address has enough funds to buy the token
        require(!DVDATokenIDBlacklisted[DVDA_ID], "This DVDA Token ID is Blacklisted"); // we ensure that the specific DVDA Token is not blacklisted
        require(!AlreadyClaimedDVDA[DVDA_ID], "This DVDA Token ID has already claimed its SrBoner."); // we ensure that the specific DVDA token doesn't already claimed the boner
        _minted += MAX_MINTS_STAGE_1; // we update the already minted token
        AlreadyClaimedDVDA[DVDA_ID] = true; // we set the already claimed flag to true
        _mint(msg.sender, SRBONER_TYPE_ID, MAX_MINTS_STAGE_1, ""); // we finally mint the token -> the address that called the funcion, the token type id, the amount, the data
        delete _owner; // remove the owner after mint (empty the variable/reset it)
    }

    // (7.2) Mint function for Stage 2 Whitelist (the Ecosystem users)
    function mintStage2(uint256 amount) public payable { // we ensure that the function can send and receive eth payable keyword and we call it with the amount (total number of token required -> max 5)
        require(!Stage2Paused, "Stage 2 Sale is paused (Ecosystem Whitelist)"); // we ensure/require that the Ecosystem WL Phase 2 is not paused (so the flag is false)
        require(MAX_MINTS_STAGE_2 + _minted <= MAX_POOL_TOTAL, "Exceed max supply"); // we ensure that there are still available mint
        require (isHolderEcosystem(msg.sender), "Require at least 1 NFT from GodHatesNFTees ecosystem (1 GodHates, or 1 Ape, or 1 Banano, or 1 Rotten, or 1 Mutant, or 1 DVDA)");
        require(amount > 0 && amount <= MAX_MINTS_STAGE_2, "Invalid mint amount"); // we ensure/require that the amount of token required is bigger than zero but not bigger that the max allowed
        require(msg.value == amount * SRBONER_PRICE_STAGE_2, "Invalid or insufficient funds"); // we ensure/require that the address has enough funds to buy the required tokens
        require(AmountClaimedStage2[msg.sender] < MAX_MINTS_STAGE_2, "Already minted max 5 SrBoners"); // we ensure/require that the address has not required already the max amount allowed of tokens
        AmountClaimedStage2[msg.sender] += amount; // we update the amount already claimed by the address
        _minted += amount; // we update the already minted tokens (the total not just the address)
        _mint(msg.sender, SRBONER_TYPE_ID, amount, ""); // finally we mint the tokens
    }

    // (7.3) Mint function for Public Phase (the public users)
    function mintPublic(uint256 amount) public payable { // we ensure that the function can send and receive eth payable keyword and we call it with the amount (total number of token required -> max 5)
        require(!PublicPaused, "Paused"); // we ensure/require that the Public Phase 3 is not paused (so the flag is false)
        require(amount + _minted <= MAX_POOL_TOTAL, "Exceed max supply"); // we ensure that there are still available mint
        require(amount > 0 && amount <= MAX_MINTS_PUBLIC, "Invalid mint amount"); // we ensure/require that the amount of token required is bigger than zero but not bigger that the max allowed
        require(msg.value == amount * SRBONER_PRICE_PUBLIC, "Invalid funds provided"); // we ensure/require that the address has enough funds to buy the required tokens
        require(AmountClaimedPublic[msg.sender] < MAX_MINTS_PUBLIC, "Already minted 5 SrBoners"); // we ensure/require that the address has not required already the max amount allowed of tokens
        AmountClaimedPublic[msg.sender] += amount; // we update the amount already claimed by the address
        _minted += amount; // we update the already minted tokens (the total not just the address)
        _mint(msg.sender, SRBONER_TYPE_ID, amount, "");  // finally we mint the tokens
    }


    // (8) PAUSE FUNCTIONALITY

    // (8.1) Set Pause function for Stage 1 Whitelist (the DVDA users)
    function setPauseStage1(bool _state) public onlyOwner { // allow only the creator of the contract to call this function (pass the flag)
        Stage1Paused = _state; // update the flag (toggle)
    }

    // (8.2) Set Pause function for Stage 2 Whitelist (the Ecosystem users)
    function setPauseStage2(bool _state) public onlyOwner { // allow only the creator of the contract to call this function (pass the flag)
        Stage2Paused = _state; // update the flag (toggle)
    }

    // (8.2) Set Pause function for Stage 3 Public (the public users)
    function setPausePublic(bool _state) public onlyOwner { // allow only the creator of the contract to call this function (pass the flag)
        PublicPaused = _state; // update the flag (toggle)
    }


    // (9) HELPERS

    // (9.1) Check if the the specific DVDA has already claimed the SrBoner
    function hasDVDAClaimed(uint256 DVDA_ID) public view returns (bool) { // allow to see if a specific dvda token has already required the boner (it's public function all can access)
        return AlreadyClaimedDVDA[DVDA_ID]; // return a bool
    }

    // (9.2) Burning functionality -> burns the specific boner
    function burnSrBoner(address burnTokenAddress) external { // it's an external function, it cannot be accessed internally but only externally (like public but not internally) -> cheap no writing in memory
        require(msg.sender == SuperSrBoner , "Invalid caller, must be called from SuperSrBoner Smart Contract"); // we ensure/require that the burner is a specific contract (kind of admin) -> SuperBoner
        _burn(burnTokenAddress, SRBONER_TYPE_ID, SRBONER_BURN_QUANTITY); // finally we can call the burn function
    }

    // (9.3) Set the SuperSrBoner address
    function setSuperSrBonerAddress(address SuperSrBonerAddress) external onlyOwner { // only the deployer and creator of the contract can do it
        SuperSrBoner = SuperSrBonerAddress; // set the value
    }

    // (9.4) Check how many DVDA the specific wallet owns
    function howManyDVDA(address account) public view returns (uint256) { // it's a public function and returns the amount of dvda owned by the specific wallet
        uint256 numberOfDVDA = DVDA.balanceOf(account); // check the balance of the address using the DVDA contract
        return numberOfDVDA; // return the number
    }

    // (9.5) Check how many Token were already claimed by a specific wallet in the ecosystem
    function howManyClaimedStage2(address account) public view returns (uint256) { // it's a public function and returns the amount of token already claimed by the specific wallet in the ecosystem
        return AmountClaimedStage2[account]; // return the number
    }

    // (9.6) Check how many Token were already claimed by a specific wallet in the public
    function howManyClaimedPublic(address account) public view returns (uint256) { // it's a public function and returns the amount of token already claimed by the specific wallet in the ecosystem
        return AmountClaimedPublic[account]; // return the number
    }

    // (9.7) Return an array of DVDA Token Ids owned by a specific address
    function tokenIdsDVDA(address account) public view returns (uint256[] memory) { // it's a public function and returns an array
        uint256[] memory tokensDVDA = DVDA.tokensOfOwner(account); // get the tokens owned by a specific account
        return tokensDVDA; // return the array
    }

    // (9.8) Check if an address is in the ecosystem (owns a specific token of the ecosystem)
    function isHolderEcosystem(address account) public view returns (bool) {  // it's a public function that check if the address owns any token of the ecosystem
        bool holderEcosystem; // declare the variable
        if (DVDA.balanceOf(account) > 0 || GHN.balanceOf(account) > 0 || BNNO.balanceOf(account, 0) > 0 || AHC.balanceOf(account) > 0 || RTBNNO.balanceOf(account, 0) > 0 || MGHN.balanceOf(account) > 0 ) { // check if has at least one of the ecosystem
            holderEcosystem = true; // set the flag
        }
        return holderEcosystem; // return the flag
    }

    // (9.9) Update the URI (ipfs) -> in case we want to change where the image is stored
    function updateURI(string memory _newUri) external onlyOwner { // this is can be called only by the creator of the contract and is an external function
        _setURI(_newUri);
    }

    // (9.10) Allow to add a specific DVDA Token ID to the BLACKLIST
    function addDVDATokenIDToBlackList(uint256 token_id) public onlyOwner { // this can be called only by the creator
        DVDATokenIDBlacklisted[token_id] = true; // update the list
    }

    // (9.11) Allow to add a full batch array of DVDA token ids to the blacklist
    function addDVDATokenIDsArrayToBlackList(uint256[] memory token_ids) public onlyOwner { // this can be called only by the creator
        for (uint256 i=0; i< token_ids.length; i++) // loop into the array
            DVDATokenIDBlacklisted[token_ids[i]] = true; // set the flag for the specific token
    }

    // (9.12) Allow to remove a specific DVDA tokenID from the blacklist
    function removeDVDATokenIDFromBlackList(uint256 token_id) public onlyOwner { // this can be called only by the creator
        DVDATokenIDBlacklisted[token_id] = false; // update the list
    }

    // (9.13) Check if the specific DVDA Token ID is in the Whitelist
    function isDVDATokenIDInWhitelist(uint256 token_id) private view returns (bool) { // this is a public function
        return DVDATokenIDBlacklisted[token_id]; // return the flag
    }

    // (9.14) Mint administrator functionality
    function mintAdmin(uint256 amount) external onlyOwner  { // this can be called only by the creator
        require(amount + _minted <= MAX_POOL_TOTAL, "Exceed max supply"); // ensure/require that the amount doesn't exceed the max supply
        _minted += amount; // update the already minted amount
        _mint(msg.sender, SRBONER_TYPE_ID, amount, ""); // mint the amount
    }

    // (9.15) Pull all the money out from the contract to the contract creator
    function withdrawFunds() external onlyOwner { // this can be called only by the creator
        // Call returns a boolean value indicating success or failure.
        (bool success, ) = msg.sender.call{value: address(this).balance}(""); // withdraw the money and send to the creator address
        require(success, "WITHDRAW FAILED!"); // ensure/require that we have success
    }

    // (9.16) Pull all the money to a specific address
    function withdrawFundsTo(address payoutAddress) external onlyOwner { // this can be called only by the creator
        (bool success, ) = payoutAddress.call{value: address(this).balance}(""); // withdraw the money and send to the specific address
        require(success, "WITHDRAW FAILED!"); // ensure/require that we have success
    }

    // (9.17) Pull a specific amount of money to a specific address
    function withdrawFundsAmountTo(address payoutAddress, uint256 amount) external onlyOwner { // this can be called only by the creator
        (bool success, ) = payoutAddress.call{value: amount}(""); // withdraw the specific amount of money and send to the specific address
        require(success, "WITHDRAW FAILED!"); // ensure/require that we have success
    }

}

