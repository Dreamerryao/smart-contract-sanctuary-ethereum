contract Game is AccessControl, Pausable {

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    string public gameName = "Regular Gameshow Beta v1";

    Questions private questions;
    Teams private teams;
    Prizes private prizes;

    uint public startTime;  
    uint public timestep = 2 minutes;

    event Started (uint startTime);
    event CorrectAnswer (address wallet, uint id, uint teamId, uint prize);
    event GameOver (uint numWinners);

    constructor() { 
	    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
	    _grantRole(MINTER_ROLE, msg.sender);
        teams = new Teams(address(this));
        questions = new Questions(address(this));
        prizes = new Prizes(address(this));

        // testing

        // immunity
	    _grantRole(MINTER_ROLE, 0x9aE087f4E8944cD85754c2E78C54428B0509AcB5);
        start(0);
    }

// Play

    function submitAnswer(uint _id, string memory _answer) public returns (bool) {
        // require(!questions.isAnswered(_id),"already answered");
        // require(block.timestamp > startTime + (timestep * _id), "not revealed");
        // require(questions.guess(_id,_answer) == true,"wrong answer");
        // questions.setAnswered(_id); // mark question as answered
        uint _prizeAmount = 69;
        // uint _prizeAmount = questions.prizeAmount(_id);
        uint _teamId = teams.getTeamByAddr(msg.sender);
        // teams.increaseScore(_teamId, _prizeAmount);
        // prizes.sendPrize(msg.sender, _prizeAmount);
        emit CorrectAnswer(msg.sender, _id, _teamId, _prizeAmount);
    }

    function guess(uint _id, string memory _answer) public view returns (bool) {
        return questions.guess(_id,_answer);
    }

    function claimTeamPrize(address _to) public {
        require(gameOver(),"game not over");
        uint _teamId = teams.getTeamByAddr(_to);
        require(teams.isWinning(_teamId),"team didn't win");
        uint winnerCount = 0;
        uint winningScore = teams.highestScore();
        for (uint i = 0; i < teams.numTeams();i++){
            if (teams.teamScore(i) == winningScore) {
                winnerCount += teams.teamSize(i);
            }
        }
        prizes.sendTeamPrize(_to, winnerCount);
    }

// View

    function started() public view returns (bool) {
        return (startTime > 0);
    }

    function currentQuestion() public view returns (uint) {
        return Math.min((block.timestamp - startTime) / timestep, questions.NUM_QUESTIONS());
    }

    function timerInMinutes() public view returns (uint) {
        if (started())
            return 0;
        else
            return((block.timestamp - startTime) / 60);
    }

    function gameOver() public view returns (bool) {
        return (block.timestamp > startTime + (timestep * questions.NUM_QUESTIONS()));
    }

    function contractTeams() public view returns (address) {
        return address(teams);
    }

    function contractQuestions() public view returns (address) {
        return address(questions);
    }

    function contractPrizes() public view returns (address) {
        return address(prizes);
    }

// Contract Admin

    function start(uint _startTime) public onlyRole(MINTER_ROLE) {
        // 0 startTime will start at current time
        require(questions.initComplete(), "Setup incomplete");
        if (_startTime > 0)
            startTime = _startTime;
        else
            startTime = block.timestamp;
        emit Started(startTime);
    }

    function reset() public onlyRole(MINTER_ROLE) {
        questions = new Questions(address(this));
    }

    function end() public {
        require(gameOver(),"game not over");
        emit GameOver(teams.numWinningTeams());
    }

    function setTimestep(uint _timestep) public onlyRole(MINTER_ROLE) {
        require(!started(),'already started');
        timestep = _timestep;
    }

    function pause() public onlyRole(MINTER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(MINTER_ROLE) {
        _unpause();
    }

}

