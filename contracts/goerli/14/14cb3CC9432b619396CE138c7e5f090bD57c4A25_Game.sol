pragma solidity ^0.8.0; 

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

// track team scores

interface _GameInterface {
    function started() external view returns (bool);
    function startTime() external returns (uint);  
    function timestep() external returns (uint);
}

contract Teams is AccessControl {
    using Counters for Counters.Counter;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    mapping(address => uint) public teamIds;
    mapping(bytes32 => uint) private teamByCode;
    mapping(uint => string)  private names;
    mapping(uint => uint)  private scores;
    mapping(uint => Counters.Counter) private teamCounts;
    Counters.Counter private counter;  
    _GameInterface private game;

    event NewTeam(string name, uint teamId);
    event JoinedTeam (address wallet, uint teamId);

    constructor(address _gameAddr) { 
	    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
	    _grantRole(MINTER_ROLE, msg.sender);
	    _grantRole(DEFAULT_ADMIN_ROLE, tx.origin);
	    _grantRole(MINTER_ROLE, tx.origin);
        game = _GameInterface(_gameAddr);

        // testing
	    _grantRole(MINTER_ROLE, 0x9aE087f4E8944cD85754c2E78C54428B0509AcB5);
        newTeam("apple", "Team Apple"); // pw, team name
        newTeam("banana", "Team Banana");
        newTeam("red", "Team Red");
        newTeam("blue", "Team Blue");

    }

// Register

    function joinTeam(string memory _key) public { 
        require(!game.started() , "Game started");
        require(teamIds[msg.sender] == 0, "Already on team");
        bytes32 _hash = keccak256(abi.encodePacked(_key));
        uint _teamId = teamByCode[_hash];
        require(_teamId > 0, "Not found on team");
        teamIds[msg.sender] = _teamId;
        teamCounts[_teamId].increment();
        emit JoinedTeam(msg.sender, _teamId);
    }

// View

    function getTeamByAddr(address _addr) public view returns (uint) {
        require(teamIds[_addr] != 0, 'not found');
        return teamIds[_addr];
    }

    function numTeams() public view returns (uint) {
        return counter.current();
    }

    function teamSize(uint _teamId) public view returns (uint) {
        return teamCounts[_teamId].current();
    }

    function teamName(uint _teamId) public view returns (string memory) {
        return names[_teamId];
    }

    function teamScore(uint _teamId) public view returns (uint) {
        return scores[_teamId];
    }

    function highestScore() public view returns (uint) {
        uint _highscore = 0;
        uint _winningTeam;        
        for (uint i = 0;i < numTeams();i++){
            if (scores[i] > _highscore) {
                _winningTeam = i;
                _highscore = scores[i];
            }
        }
        return _highscore;
    }

    function isWinning(uint _teamId) public view returns (bool) {
        teamScore(_teamId) == highestScore();
    }

    function numWinningTeams() public view returns (uint) {
        uint _highestScore = highestScore();
        if (_highestScore == 0)
            return 0;
        uint _count = 0;
        for (uint i = 0;i < numTeams();i++){
            if (scores[i] == _highestScore)
                _count++;
        }
        return _count;
    }

// Team Admin

    function generateInviteCode(string memory _key) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_key));
    }

// Game Admin

    function newTeamByHash(bytes32 _hash, string memory _name) public onlyRole(MINTER_ROLE) returns (bytes32) {
        counter.increment();
        teamByCode[_hash] = counter.current();
        names[counter.current()] = _name;
    }

    function newTeam(string memory _key, string memory _name) public onlyRole(MINTER_ROLE) returns (bytes32) {
        bytes32 _hash = keccak256(abi.encodePacked(_key));
        counter.increment();
        teamByCode[_hash] = counter.current();
        names[counter.current()] = _name;
        emit NewTeam(_name, counter.current());
    }

    function setName(uint _teamId, string memory _name) public onlyRole(MINTER_ROLE) {
        names[_teamId] = _name;
        emit NewTeam(_name, counter.current());
    }

    function increaseScore(uint _teamId, uint _points) public onlyRole(MINTER_ROLE) {
        scores[_teamId] += _points;
    }

}

