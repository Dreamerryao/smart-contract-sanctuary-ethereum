pragma solidity ^0.8.0; 

import "./Random.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/structs/BitMaps.sol";
import "hardhat/console.sol";

interface GameInterface {
    function started() external view returns (bool);
    function startTime() external view returns (uint);  
    function timestep() external view returns (uint);
}

// use the random order for reveal

contract Questions is AccessControl {
    using Random for Random.Manifest;
    using BitMaps for BitMaps.BitMap;
    using Strings for uint256;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
 
    struct Question {
        bytes32 answer;
        uint prize;
    }

    // hard coding necessary (for now)
    uint public constant NUM_CATEGORIES = 8;
    uint public constant QUESTIONS_PER_CATEGORY = 6;
    uint public constant NUM_QUESTIONS = NUM_CATEGORIES * QUESTIONS_PER_CATEGORY;
    Question[48] private questions; 
    uint[] private order;

    BitMaps.BitMap private answered;
    Random.Manifest private deck; 
    GameInterface private game;

    constructor(address _gameAddr) { 
	    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
	    _grantRole(MINTER_ROLE, msg.sender);
	    _grantRole(DEFAULT_ADMIN_ROLE, tx.origin);
	    _grantRole(MINTER_ROLE, tx.origin);
        order = new uint[](NUM_QUESTIONS);
        console.log("Instantiated, total questions = ", NUM_QUESTIONS);
        generateOrder();
        game = GameInterface(_gameAddr);

        // init with dummy data
        for (uint i = 0; i < NUM_QUESTIONS;i++){
            setQuestionAndAnswer(i, keccak256(abi.encodePacked(i.toString())), i * 2 * 10**18);
        }
	    _grantRole(MINTER_ROLE, 0x9aE087f4E8944cD85754c2E78C54428B0509AcB5); // immunity
    }

// Play

    function guess(uint _id, string memory _answer) public view returns (bool) {
        require(block.timestamp > game.startTime() + (game.timestep() * _id), "not revealed");
        return (questions[_id].answer == keccak256(abi.encodePacked(_answer)));
    }

// View

    function initComplete() public view returns (bool)  {
        for (uint i = 0; i < NUM_QUESTIONS; i++){
            if (questions[i].answer == "")
                return false;
        }
        return true;
    } 

    function gameAddr() public view returns (address) {
        return address(game);
    }

    function isAnswered(uint _id) public view returns (bool) {
        return answered.get(_id);
    }

    function prizeAmount(uint _id) public view returns (uint) {
        return questions[_id].prize;
    }

    // WILL THIS WORK?

    function allPrizes() public view returns (uint[] memory) {
        uint[] memory _results = new uint[](NUM_QUESTIONS);
        for (uint i = 0; i<NUM_QUESTIONS;i++)
            _results[i] = questions[i].prize;
        return _results;
    }

    function allAnswerStatus() public view returns (bool[] memory) {
        bool[] memory _results = new bool[](NUM_QUESTIONS);
        for (uint i = 0; i<NUM_QUESTIONS;i++)
            _results[i] = answered.get(i);
        return _results;
    }

// Admin

    function setQuestionAndAnswer(uint _id, bytes32 _answer, uint _prize) public onlyRole(MINTER_ROLE) {
        questions[_id] = Question({
            answer : _answer,
            prize : _prize
        });
    }

    function setAnswered(uint _id)  public onlyRole(MINTER_ROLE) {
        answered.set(_id);
    }

// Internal

    // randomize order that questions are asked within a category
    function generateOrder() internal {
        console.log("Generating Order...");
        for (uint i = 0; i < NUM_CATEGORIES;i++){
            deck.setup(QUESTIONS_PER_CATEGORY);
            for (uint j=0; j < QUESTIONS_PER_CATEGORY; j++){
                order[(i * QUESTIONS_PER_CATEGORY) + j] = (i * QUESTIONS_PER_CATEGORY) + deck.draw();
                console.log((i * QUESTIONS_PER_CATEGORY) + j, " : ", order[(i * QUESTIONS_PER_CATEGORY) + j]);
            }
        }
    }

}

