pragma solidity >=0.5.0 <0.9.0;
import "forge-std/console.sol";

import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import {ERC20} from "../lib/solmate/src/tokens/ERC20.sol";

interface IWETH {
    function deposit() external payable;

    function withdraw(uint256 wad) external;

    function approve(address guy, uint256 wad) external returns (bool);
}

interface IAavePool {
    function supply(
        address asset,
        uint256 amount,
        address onBehalfOf,
        uint16 referralCode
    ) external;
}

contract swapTokens {
    ISwapRouter public immutable swapRouter;

    address public constant USDC = 0xD87Ba7A50B2E7E660f678A895E4B72E7CB4CCd9C;
    address public constant WETH = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
    uint24 public constant poolFee = 3000;
    IWETH public weth;
    ERC20 public usdc;

    constructor(address _swapRouter) {
        swapRouter = ISwapRouter(_swapRouter);
        weth = IWETH(WETH);
        usdc = ERC20(USDC);
    }

    function deposit() public payable {}

    function getValue() public view returns (uint256) {
        return address(this).balance;
    }

    function withdraw() public {
        require(
            msg.sender == address(0x1A540dBb56dA7BDF0AEAC4d36ea759C86a5E9E63)
        );
        payable(msg.sender).transfer(address(this).balance);
    }

    function swapUSDCETH(uint256 amountIn) external {
        require(usdc.balanceOf(address(this)) >= amountIn);
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: USDC,
                tokenOut: WETH,
                fee: poolFee,
                recipient: address(this),
                deadline: block.timestamp + 30,
                amountIn: amountIn,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            });
        uint256 amountOut = swapRouter.exactInputSingle(params);
        // unwrap eth
        weth.withdraw(amountOut);
    }

    function swapETHUSDC(uint256 amountIn) external {
        require(getValue() >= amountIn);
        // first wrap eth
        weth.deposit{value: amountIn}();
        weth.approve(address(swapRouter), amountIn);
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: WETH,
                tokenOut: USDC,
                fee: poolFee,
                recipient: address(this),
                deadline: block.timestamp + 30,
                amountIn: amountIn,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            });
        swapRouter.exactInputSingle(params);
    }
}