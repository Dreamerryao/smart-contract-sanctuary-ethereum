pragma solidity >=0.8.4;
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "./AltarPoolUpgradeable.sol";
import "../ISporeYield.sol";

contract AltarUpgradeable is
    Initializable,
    OwnableUpgradeable,
    AccessControlUpgradeable,
    UUPSUpgradeable,
    AltarPoolUpgradeable
{
    bytes32 public constant POOL_MASTER_ROLE = keccak256("POOL_MASTER_ROLE");
    bool public active;
    ISporeYield public sporeYieldContract;
    error NotYetActive();
    error ArrayLengthNotMatch();

    /**
     * @dev Emitted when an NFT fails to be sacrificed
     */
    event SporeClaimed(address indexed from, uint256 claimedAmount);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        //_disableInitializers();
    }

    /**
     * @dev A modifier that reverts if the contract isn't active
     */
    modifier isActive() {
        if (active == false) revert NotYetActive();
        _;
    }

    function external_costForTargetTime(
        uint8 poolId_,
        uint256 timeStamp_,
        address owner_
    ) public view returns (uint32) {
        PoolInfo storage pool = poolInfo[poolId_];
        OwnerCommitment storage poolCommitment = pool.ownerCommitment[owner_];
        uint32 cost = external_costForTargetTimeSingleNFT(poolId_, timeStamp_);
        return cost * poolCommitment.nftCount;
    }

    function external_costForTargetTimeSingleNFT(uint8 poolId_, uint256 targetTimestamp_) public view returns (uint32) {
        PoolInfo storage pool = poolInfo[poolId_];
        return external_epochsUntilTimestamp(uint32(targetTimestamp_)) * pool.epochCost;
    }

    function external_epochsUntilTimestamp(uint32 targetTimestamp_) public view returns (uint32) {
        return (targetTimestamp_ - uint32(block.timestamp)) / sacrificeEpochPeriod;
    }

    function external_epochEndTimestamp() public view returns (uint32) {
        return (uint32(sacrificeLastProcessed) + sacrificeEpochPeriod);
    }
    function external_getNFTCountInPool() public view returns (uint32) {
        return (uint32(sacrificeLastProcessed) + sacrificeEpochPeriod);
    }
    function external_getPoolIndex(uint8 poolId_, uint32 tokenId_) external view returns (uint256 commitmentIndex) {
        PoolInfo storage pool = poolInfo[poolId_];
        uint256 i = 0;
        uint256 poolLength = pool.commitments.length;
        for (; i < poolLength; ) {
            if (pool.commitments[i].tokenId == tokenId_) {
                return i;
            }
            unchecked {
                i++;
            }
        }
        revert CommitmentNotFound();
    }

    function initialize(
        address roleAdmin_,
        uint32 sacrificeEpochPeriod_,
        address sporeContract_,
        address summonedNFTContract_,
        address sporeYieldContract_,
        address nffGenesisAddress_,
        address nffGenerativeAddress_
    ) public initializer {
        __AltarPool_init(
            sacrificeEpochPeriod_,
            sporeContract_,
            summonedNFTContract_,
            nffGenesisAddress_,
            nffGenerativeAddress_
        );
        __Ownable_init();
        __AccessControl_init();
        _setupRole(DEFAULT_ADMIN_ROLE, roleAdmin_);
        _setupRole(POOL_MASTER_ROLE, roleAdmin_);
        active = false;
        sporeYieldContract = ISporeYield(sporeYieldContract_);
    }

    function setSporeYieldContract(address newContract) external onlyRole(POOL_MASTER_ROLE) { 
        sporeYieldContract = ISporeYield(newContract);
    }

    function createPool(
        TokenType[] calldata allowedTokenTypes_,
        uint8 dailySacrifices_,
        uint8 dailyCost_,
        string calldata name_
    ) external onlyRole(POOL_MASTER_ROLE) returns (uint8 poolId) {
        return _createPool(allowedTokenTypes_, dailySacrifices_, dailyCost_, name_);
    }

    function setActive(bool active_) external onlyRole(POOL_MASTER_ROLE) {
        active = active_;
    }
    function startNow() external onlyRole(POOL_MASTER_ROLE) {
        if(sacrificeLastProcessed != 0) return;
        sacrificeLastProcessed = block.timestamp;
    }
    function setStartTime(uint256 startTime_) external onlyRole(POOL_MASTER_ROLE) {
        if(sacrificeLastProcessed != 0) return;
        sacrificeLastProcessed = startTime_;
    }

    function changePoolState(uint8 poolId_, bool enabledState_) external onlyRole(POOL_MASTER_ROLE) {
        _changePoolState(poolId_, enabledState_);
    }

    function commitManyNFTAndAddTime(
        uint8 poolId_,
        uint256[] memory packedTokenId_,
        uint32 epochs_
    ) external isActive {
        for(uint i = 0; i < packedTokenId_.length; i++)
        {
            console.log("Pool %s: Committed token with packed id %s", poolId_, packedTokenId_[i]);
            (TokenType type_, address contract_, uint256 tokenId_) = getNFTTokenType(packedTokenId_[i]);
            _commitNFT(poolId_, type_, packedTokenId_[i], tokenId_, contract_);
        }
        addToPoolTime(poolId_, epochs_);
    }

    function commitNFTAndAddTime(
        uint8 poolId_,
        uint256 packedTokenId_,
        uint32 epochs_
    ) external isActive {
        console.log("Pool %s: Committed token with packed id %s", poolId_, packedTokenId_);
        //todo: make sure all nfts committed are initialized in spore yields
        (TokenType type_, address contract_, uint256 tokenId_) = getNFTTokenType(packedTokenId_);
        _commitNFT(poolId_, type_, packedTokenId_, tokenId_, contract_);
        addToPoolTime(poolId_, epochs_);
    }

    function commitManyNFT(
        uint8 poolId_,
        uint256[] memory packedTokenId_
    ) external isActive {
        for(uint i = 0; i < packedTokenId_.length; i++)
        {
            console.log("Pool %s: Committed token with packed id %s", poolId_, packedTokenId_[i]);
            (TokenType type_, address contract_, uint256 tokenId_) = getNFTTokenType(packedTokenId_[i]);
            _commitNFT(poolId_, type_, packedTokenId_[i], tokenId_, contract_);
        }
    }

    function commitNFT(uint8 poolId_, uint256 packedTokenId_) external isActive {
        console.log("Pool %s: Committed token with packed id %s", poolId_, packedTokenId_);
        //todo: make sure all nfts committed are initialized in spore yields
        (TokenType type_, address contract_, uint256 tokenId_) = getNFTTokenType(packedTokenId_);
        _commitNFT(poolId_, type_, packedTokenId_, tokenId_, contract_);
    }

    function uncommitNFT(
        uint8 poolId_,
        uint256 poolIndex_
    ) external isActive {
        console.log("Pool %s: uncommitted token with pool index %s", poolId_, poolIndex_);
        _uncommitNFT(poolId_, poolIndex_);
    }

    function startCommitment() external isActive {
        _startCommitment();
        _processPools();
    }

    function external_getPoolTotalCommits(uint poolId) external view isActive returns(uint256){
        return poolInfo[poolId].commitments.length;
    }
    function external_getPoolCommitments(uint poolId, uint start, uint end) external view isActive returns(CommittedNFT[] memory){
        CommittedNFT[] storage commitments = poolInfo[poolId].commitments;
        if(end > commitments.length)
        {
            end = commitments.length;
        }
        uint count = start - end;
        CommittedNFT[] memory returnedCommitments = new CommittedNFT[](count);
        for(uint i = start; i < end; i++)
        {
            returnedCommitments[i - start] = commitments[i];
        }
        return returnedCommitments; 
    }
    function processPools() external isActive {
        _processPools();
    }


    //todo: multi-claim option, maybe add a multiCall function that delegatecalls?
    function claimSpores(uint8[] calldata tokenPoolIndexes_, uint8 poolId_) external poolChangesAllowed(poolId_) isActive returns (uint256 claimedCount) {
        require(tokenPoolIndexes_.length > 0, "No spores chosen");
        uint256 length = tokenPoolIndexes_.length;
        PoolInfo storage pool = poolInfo[poolId_];
        CommittedNFT storage nft;
        uint256[] memory tokenIds_ = new uint256[](length);
        uint256 i = 0;
        uint256 tokenIdCounter = 0;
        uint8 poolIndex = 0;
        for (; i < length; ) {
            poolIndex = tokenPoolIndexes_[i];
            nft = pool.commitments[poolIndex];
            if (msg.sender != nft.owner || nft.processed == true) {
                revert NotOwnerOfCommitment();
            }
            tokenIds_[tokenIdCounter] = nft.tokenId;
            unchecked {
                i++;
                tokenIdCounter++;
            }
        }
        uint256 balanceBefore = sporeContract.balanceOf(address(this));
        sporeYieldContract.claim(tokenIds_);
        uint256 claimed = sporeContract.balanceOf(address(this)) - balanceBefore;
        emit SporeClaimed(msg.sender, claimed);
        // todo: check this
        sporeContract.transfer(msg.sender, claimed);
        return claimed;
    }

    function addToPoolTime(uint8 poolId_, uint32 addedEpochs_) public poolChangesAllowed(poolId_) isActive returns (uint32 cost) {
        if(addedEpochs_ == 0) return 0;

        PoolInfo storage pool = poolInfo[poolId_];
        console.log("Adding %s epochs to pool %s", addedEpochs_, poolId_);
        OwnerCommitment storage poolCommitment = pool.ownerCommitment[msg.sender];
        if(poolCommitment.nftCount == 0) return 0;

        uint32 remainingEpochs = _getRemainingEpochs(poolCommitment.commitEndEpoch, currentEpoch);
        if (remainingEpochs == 0) poolCommitment.commitEndEpoch = currentEpoch + addedEpochs_;
        else poolCommitment.commitEndEpoch += addedEpochs_;
        cost = addedEpochs_ * pool.epochCost * poolCommitment.nftCount;
        if (cost == 0) revert NoCommittedSacrifices();
        console.log("Taking %s tokens", cost);
        if (sporeContract.transferFrom(msg.sender, address(this), getExpandedTokenCost(cost)) == false) revert FailedSporeTokenTransfer();
        emit AddedCommitment(poolId_, addedEpochs_, cost);
    }

    function external_estimateAddNFTCost(uint8 poolId, address owner) public view returns (uint256)
    {
        PoolInfo storage pool = poolInfo[poolId];
        OwnerCommitment storage ownerCommitment = pool.ownerCommitment[owner];
        return _getRemainingEpochs(ownerCommitment.commitEndEpoch, currentEpoch);
    }
    function external_estimateAddEpochCost(uint8 poolId, address owner, uint256 epochCount) public view returns (uint256)
    {
        PoolInfo storage pool = poolInfo[poolId];
        OwnerCommitment storage ownerCommitment = pool.ownerCommitment[owner];
        return ownerCommitment.nftCount * epochCount;
    }

    // Public View Functions for Helpers

    function external_getPoolSupportedTokens(uint8 poolId_)
        external
        view
        returns (
            TokenType[] memory supportedTokenTypes
        )
    {
        supportedTokenTypes = poolInfo[poolId_].supportedTokenTypes;
    }

    // todo: get poolInfoExternal for all pools
    /// @dev Returns pool information.
    function external_getPoolInfo(uint8 poolId_, address owner_)
        external
        view
        returns (
            uint256 totalCostPerEpoch,
            uint256 ownerCostPerEpoch,
            uint256[] memory ownerPoolIndexes,
            uint256[] memory ownerTokenIdsInPool,
            uint8 poolEpochCost,
            uint32 ownerRemainingCommitment,
            uint256 claimableSporeInPool,
            uint256 nftCount,
            uint256 normalInPool,
            uint256 magicInPool,
            uint256 genesisInPool
        )
    {
        // todo: this can now be optimzied as the nft count is stored with the owner address in OwnerCommitment
        //todo: this function can read the nftCount in future once we do the balance changes, avoiding a double-loop for the array allocation
        PoolInfo storage pool = poolInfo[poolId_];
        OwnerCommitment storage ownerCommitments = pool.ownerCommitment[owner_];
        ownerRemainingCommitment = _getRemainingEpochs(ownerCommitments.commitEndEpoch, currentEpoch);
        uint256 i = 0;
        nftCount = 0;
        for (; i < pool.commitments.length; ) {
            if (pool.commitments[i].owner == owner_) {
                OwnerCommitment memory ownerCommitment = pool.ownerCommitment[pool.commitments[i].owner];
                nftCount++;
            }
            (TokenType tokenType,, ) = getNFTTokenType(
                pool.commitments[i].tokenId
            );
            if (tokenType == TokenType.GENERATIVE) normalInPool++;
            else if (tokenType == TokenType.MAGIC) magicInPool++;
            else if (tokenType == TokenType.GENESIS) genesisInPool++;
            unchecked {
                i++;
            }
        }
        // Can remove extra loop if we're smart with assembly, but that's an optimization which is last since this is view only!
        ownerPoolIndexes = new uint256[](nftCount);
        ownerTokenIdsInPool = new uint256[](nftCount);
        uint256 poolIndexWrite = 0;
        i = 0;
        if(pool.commitments.length != 0)
        {
            unchecked {
                do {
                    if (pool.commitments[i].owner == owner_) {
                        ownerPoolIndexes[poolIndexWrite] = i;
                        ownerTokenIdsInPool[poolIndexWrite] = pool.commitments[i].tokenId;
                        poolIndexWrite++;
                    }
                    i++;
                } while (poolIndexWrite < nftCount);
            }
        }
        totalCostPerEpoch = pool.epochCost * ownerCommitments.nftCount;
        ownerCostPerEpoch = pool.epochCost * nftCount;
        poolEpochCost = pool.epochCost;
        claimableSporeInPool = sporeYieldContract.getPendingTokensMany(ownerTokenIdsInPool);
    }
    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
    /**
     * @dev Reserved for future NS features
     */
    uint256[50] private __gap;
}

interface ISporeYield {
    function claim(uint256[] memory tokenIds_) external returns (uint256);

    function getPendingTokensMany(uint256[] memory indexes_) external view returns (uint256);
}

// SPDX-License-Identifier: MIT
pragma solidity >=0.8.4;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "hardhat/console.sol";

abstract contract AltarPoolUpgradeable is Initializable {
    uint256 constant processAfterBlocks = 0;
    uint256 constant processMaxWaitBlocks = 250;
    uint256 constant maximumRollTries = 9999;
    enum TokenType {
        UNKNOWN,
        GENERATIVE,
        MAGIC,
        GENESIS
    }
    /**
     * @dev Emitted when an NFT is committed for sacrifice
     */
    event CommittedSacrifice(address indexed from, uint256 tokenId, uint256 amount, uint8 poolId);

    /**
     * @dev Emitted when an NFT is uncomitted for sacrifice
     */
    event UncomittedSacrifice(address indexed from, uint256 tokenId, uint256 amount, uint8 poolId);

    /**
     * @dev Emitted when an NFT is sacrificed
     */
    event Sacrificed(address indexed from, uint64 sacrificedTokenId, uint64 summonedTokenId, uint8 poolId);

    /**
     * @dev Emitted when an NFT fails to be sacrificed
     */
    event SacrificeFailed(address indexed from, uint64 sacrificedTokenId, uint8 poolId);

    /**
     * @dev Emitted when processing has been requested
     */
    event ProcessRequested(uint8 poolId);

    /**
     * @dev Emitted when a pool has been processed
     */
    event PoolProcessed(uint8 poolId, uint8 sacrifices, uint256 poolSize);
    /**
     * @dev Emitted when a pool has been processed
     */
    event PoolCreated(uint256 poolId);
    /**
     * @dev Emitted when a pool has been processed
     */
    event AddedCommitment(uint8 poolId, uint32 addedEpochs, uint32 tokenSpent);

    struct PoolInfo {
        uint8 epochCost;
        uint8 dailySacrifices;
        bool enabled;
        string name;
        TokenType[] supportedTokenTypes;
        mapping(address => OwnerCommitment) ownerCommitment;
        CommittedNFT[] commitments;
    }

    struct OwnerCommitment {
        uint32 commitEndEpoch;
        uint16 nftCount;
    }
    
    struct CommittedNFT {
        uint32 tokenId;
        bool processed; // have they won?
        address owner;
        // gap
    }

    /** Commitment Processing */
    struct CommitmentProcessing {
        bool requested;
        uint256 requestedBlock;
        uint256 timestamp;
    }
    uint32 public sacrificeEpochPeriod;
    CommitmentProcessing public processRequest;
    uint256 public sacrificeLastProcessed;
    uint32 public currentEpoch;
    /** External Contracts */
    IERC20Upgradeable public sporeContract;
    IERC721Upgradeable public summonedContract;
    IERC721Upgradeable public nffGenerative;
    IERC721Upgradeable public nffGenesis;
    uint256[] public remainingShadesForSummon;

    uint64 public summonedCounter; // TODO: don't use uint64, use an array of tokenids we have approved and then take from the array randomly so ids we give are not sequential.

    /** Exceptions */
    error PoolNotEnabled(uint8 poolId);
    error PoolNotFound();
    error TokenNotPartOfPool();
    error OwnerCheckFail();
    error NotOwnerOfCommitment();
    error PoolChangesNotAllowed(uint8 poolId);
    error CommitmentProcessingNotAllowed();
    error CommitmentAlreadyWaitingForProcess();
    error CommitmentNotFound();
    error FailedNFTTokenTransfer();
    error FailedSporeTokenTransfer();
    error CommitmentAlreadyProcessed();
    error NoCommittedSacrifices();

    PoolInfo[] public poolInfo;

    function __AltarPool_init(
        uint32 sacrificeEpochPeriod_,
        address sporeContractAddress_,
        address summonedContractAddress_,
        address nffGenesisAddress_,
        address nffGenerativeAddress_
    ) internal onlyInitializing {
        __AltarPool_init_unchained(
            sacrificeEpochPeriod_,
            sporeContractAddress_,
            summonedContractAddress_,
            nffGenesisAddress_,
            nffGenerativeAddress_
        );
    }

    function __AltarPool_init_unchained(
        uint32 sacrificeEpochPeriod_,
        address sporeContractAddress_,
        address summonedContractAddress_,
        address nffGenesisAddress_,
        address nffGenerativeAddress_
    ) internal onlyInitializing {
        sacrificeEpochPeriod = sacrificeEpochPeriod_;
        sporeContract = IERC20Upgradeable(sporeContractAddress_);
        summonedContract = IERC721Upgradeable(summonedContractAddress_);
        nffGenesis = IERC721Upgradeable(nffGenesisAddress_);
        nffGenerative = IERC721Upgradeable(nffGenerativeAddress_);
        summonedCounter = 1;
    }

    /**
     * @dev A modifier that reverts if pool changes are not allowed (normally as a result of commitment period starting)
     */
    modifier poolChangesAllowed(uint8 poolId_) {
        if (getPoolChangesAllowed(poolId_) == false) revert PoolChangesNotAllowed(poolId_);
        _;
    }

    /**
     * @dev A modifier that reverts if commitment period isn't active
     */
    modifier processCommitAllowed(uint8 poolId_) {
        poolId_;
        if (waitingForProcessing(poolId_) == false) revert CommitmentProcessingNotAllowed();
        _;
    }

    /**
     * @dev A modifier that reverts if commitment period isn't active
     */
    modifier processRequestAllowed(uint8 poolId_) {
        poolId_;
        if (waitingForProcessingRequest(poolId_) == false) revert CommitmentProcessingNotAllowed();
        _;
    }

    function getNFTTokenType(uint256 packedTokenId_) public view returns (TokenType type_, address contract_, uint256 tokenId_) {
                // 1 will be generative, 2 will be magic, and 3 will be genesis.

        if (packedTokenId_ > 10000) return (TokenType.GENESIS, address(nffGenesis), packedTokenId_ - 10000);
        // todo: this support would be an array lookup that we setup at start of contract
        // todo: add support for magic
        return (TokenType.GENERATIVE, address(nffGenerative), packedTokenId_);
    }
    function getNFTTokenIdOffset(uint256 tokenId_, address contract_) external view returns (uint256 packedTokenId_) {
        if(contract_ == address(nffGenesis)) return tokenId_ + 10000;
        return tokenId_;
    }

    function getIndexOfTokens(address[] memory contracts_, uint256[] memory tokenIds_)
        public
        view
        returns (uint256[] memory)
    {
        // Make sure the array lengths are equal
        require(contracts_.length == tokenIds_.length, "m");

        // MSTORE to save GAS
        uint256 _items = tokenIds_.length;

        // MSTORE _indexes to return
        uint256[] memory _indexes = new uint256[](_items);

        // Generate the index array
        for (uint256 i = 0; i < _items; i++) {
            _indexes[i] = getIndexOfToken(contracts_[i], tokenIds_[i], address(nffGenesis), address(nffGenerative));
        }

        // Return the _indexes array
        return _indexes;
    }

    function getIndexOfToken(
        address contract_,
        uint256 tokenId_,
        address genesisContract_,
        address generativeContract_
    ) internal pure returns (uint256) {
        // MSTORE to save GAS
        // Todo: we may not need to do this check here, could be more optimal.
        require(
            contract_ == genesisContract_ || contract_ == generativeContract_,
            "g"
        );

        // Generate the offset. If generative, offeset is 10000, else, it's 0.
        return tokenId_ + (contract_ == generativeContract_ ? 0 : 10000);
    }

    function getTokenContract(uint256 tokenId_) public view returns (IERC721Upgradeable) {
        return tokenId_ > 10000 ? nffGenesis : nffGenerative;
    }

    function getPoolChangesAllowed(uint8 poolId_) public view returns (bool) {
        return poolInfo[poolId_].enabled && !waitingForProcessing(poolId_); // For now all pools share same epoch timers, and don't get processed individually.
    }

    /**
     * @dev Returns true if we're waiting for the commitment to be processed
     */
    function waitingForProcessing(uint8 poolId_) public view returns (bool) {
        poolId_; // Just to get rid of warning about unused parameter
        if (sacrificeLastProcessed + sacrificeEpochPeriod < block.timestamp && withinProcessBlockRange(poolId_) && processRequest.requested == true) {
            return true;
        }
        return false;
    }

    /**
     * @dev Returns true if we're waiting for the commitment processing to begin
     */
    function waitingForProcessingRequest(uint8 poolId_) public view returns (bool) {
        poolId_; // Just to get rid of warning about unused parameter
        if (
            sacrificeLastProcessed + sacrificeEpochPeriod < block.timestamp &&
            (missedProcessCommitment(poolId_) || processRequest.requested == false)
        ) {
            return true;
        }
        return false;
    }

    /**
     * @dev Returns true if we're waiting for the commitment processing to begin
     */
    function withinProcessBlockRange(uint8 poolId_) public view returns (bool) {
        poolId_;
        return
            processRequest.requestedBlock + processAfterBlocks < block.number &&
            processRequest.requestedBlock + processMaxWaitBlocks > block.number;
    }

    /**
     * @dev Returns true if we missed the time range we had for processing a commitment
     */
    function missedProcessCommitment(uint8 poolId_) public view returns (bool) {
        poolId_;
        return processRequest.requestedBlock + processMaxWaitBlocks < block.number && processRequest.requested == true;
    }

    // function balanceCommitment(OwnerCommitment storage currentCommitment, int32 nftChange, int64 commitmentChange) external view returns(bool)
    // ) internal poolChangesAllowed(poolId_) {
    //     // Adding to pool
    //     if(commitmentChange > 0) {
    //         console.log("Committed nft with contract %s of token id %s", tokenContract_, tokenId_);
    //     }
    // }
    function _commitNFT(
        uint8 poolId_,
        TokenType tokenType_,
        uint256 packedTokenId_,
        uint256 tokenId_,
        address tokenContract_
    ) internal poolChangesAllowed(poolId_) {
        // Check
        PoolInfo storage pool = poolInfo[poolId_];

        OwnerCommitment storage poolCommitment = pool.ownerCommitment[msg.sender];
        uint256 poolLength = pool.supportedTokenTypes.length;
        bool isInPool = false;
        for (uint256 i = 0; i < poolLength; ) {
            if (pool.supportedTokenTypes[i] == tokenType_) {
                isInPool = true;
                break;
            }
            unchecked {
                i++;
            }
        }
        if (isInPool == false) revert TokenNotPartOfPool();
        // Effect
        uint32 remainingEpochs = _getRemainingEpochs(poolCommitment.commitEndEpoch, currentEpoch);
        uint256 expandedSporeCost = getExpandedTokenCost(pool.epochCost * remainingEpochs);
        pool.commitments.push(CommittedNFT({ tokenId: uint32(packedTokenId_), processed: false, owner: msg.sender }));
        poolCommitment.nftCount += 1;

        // Interact
        console.log("Committed nft with contract %s of token id %s", tokenContract_, tokenId_);
        console.log(
            "Committed token amount %d and pool %d",
            uint256(pool.epochCost * remainingEpochs),
            uint256(poolId_)
        );
        {
            IERC721Upgradeable nftContract = IERC721Upgradeable(tokenContract_);
            //if (nftContract.ownerOf(tokenId_) != msg.sender) revert OwnerCheckFail();
            nftContract.transferFrom(msg.sender, address(this), tokenId_);
            if (
                sporeContract.transferFrom(
                    msg.sender,
                    address(this),
                    expandedSporeCost
                ) == false
            ) revert FailedSporeTokenTransfer();
            if (nftContract.ownerOf(tokenId_) != address(this)) revert FailedNFTTokenTransfer();
        }
        emit CommittedSacrifice(msg.sender, packedTokenId_, expandedSporeCost, poolId_);
        console.log("Done transferring tokens and nft");
    }

    function _uncommitNFT(
        uint8 poolId_,
        uint256 poolIndex_
    ) internal poolChangesAllowed(poolId_) {
        // Check
        PoolInfo storage pool = poolInfo[poolId_];
        CommittedNFT memory commitment = pool.commitments[poolIndex_];
        OwnerCommitment storage poolCommitment = pool.ownerCommitment[msg.sender];
        if (commitment.owner != msg.sender) revert NotOwnerOfCommitment();
        if (commitment.processed) revert CommitmentAlreadyProcessed();
        commitment.processed = true;
        (,address contract_, uint256 tokenId_) = getNFTTokenType(commitment.tokenId);
        IERC721Upgradeable nftContract = IERC721Upgradeable(contract_);
        // Effect
        //todo: some good optimizations can be made here with variable reuse for sloads
        uint32 refundableCommitment = _getRemainingEpochs(poolCommitment.commitEndEpoch, currentEpoch);
        poolCommitment.nftCount -= 1;

        // Interact
