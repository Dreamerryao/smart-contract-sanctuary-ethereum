        // Todo: rewrite this completely, and use two arrays, with one being a mapping of eligible nfts to index in array and the other being the array of all nfts in pool
        console.log("Started processing of pool %s", poolId_);
        PoolInfo storage pool = poolInfo[poolId_];
        uint256 nftCount = pool.commitments.length;
        uint256 i = 0;
        uint8 sacrificed = 0;
        uint256 maximumAttempts = maximumRollTries;
        // Clamp
        if (maximumAttempts > nftCount) {
            console.log("Not enough nfts in pool %s, clamping size, current pool size %s", poolId_, nftCount);
