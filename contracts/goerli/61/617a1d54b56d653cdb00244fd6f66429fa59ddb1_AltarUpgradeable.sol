        processRequest.requested = true;
        processRequest.requestedBlock = block.number;
        processRequest.timestamp = block.timestamp;
        emit ProcessRequested(0);
    }

    function _changePoolState(uint8 poolId_, bool enabledState_) internal {
        PoolInfo storage targetPool = poolInfo[poolId_];
        targetPool.enabled = enabledState_;
    }

    function getExpandedTokenCost(uint32 token_) public pure returns (uint256) {
        return uint256(token_) * (10**18);
    }

    function _getRemainingEpochs(uint32 endEpoch_, uint32 currentEpoch_) internal pure returns (uint32) {
        if (endEpoch_ <= currentEpoch_) {
            return 0;
        }
        return endEpoch_ - currentEpoch_;
    }



