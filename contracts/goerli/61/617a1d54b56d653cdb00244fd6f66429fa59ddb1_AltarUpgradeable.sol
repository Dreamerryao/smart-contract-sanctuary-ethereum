        uint32 remainingEpochs = _getRemainingEpochs(ownerCommitment.commitEndEpoch, currentEpoch);
        // Send them their summoned NFT
        summonedContract.transferFrom(address(this), commitment.owner, summonedCounter); // Not safeTransferFrom, we don't want any control being given to users
        emit Sacrificed(commitment.owner, commitment.tokenId, summonedCounter, poolId_);
        summonedCounter++;

        // Below is the removal of the NFT from the pool and refund of spore remaining
        if(type_ != TokenType.GENERATIVE)
        {
            console.log("Not burning token id %s in pool %s as it's not a generative", commitment.tokenId, poolId_);
            // We only burn generative
            return;
        }
        console.log("Successfully sacrificed token id %s in pool %s", commitment.tokenId, poolId_);
        ownerCommitment.nftCount -= 1; // Reduce the amount of nfts they have
        if(ownerCommitment.nftCount == 0)
        {
            ownerCommitment.commitEndEpoch = 0; // Reset their end epoch. They have no nfts left.
        }
        uint256 returnedSpore = getExpandedTokenCost(remainingEpochs * pool.epochCost);
        if(returnedSpore > 0)
        {
            console.log("Returning tokens %s", returnedSpore);
            sporeContract.transfer(commitment.owner, returnedSpore);
        }
        pool.commitments[tokenIndex_] = pool.commitments[pool.commitments.length - 1];
        pool.commitments.pop();
    }

