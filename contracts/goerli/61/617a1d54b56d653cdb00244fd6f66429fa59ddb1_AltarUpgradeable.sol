        emit UncomittedSacrifice(msg.sender, tokenId_, expandedCost, poolId_);
        pool.commitments[poolIndex_] = pool.commitments[pool.commitments.length - 1];
        pool.commitments.pop();
        console.log("Done transferring tokens and nft");
