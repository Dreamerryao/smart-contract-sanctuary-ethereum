        PoolInfo storage pool,
        uint8 poolId_,
        uint256 seed_,
        uint256 attempt_
    ) internal returns (bool) {
        uint256 nftCount = pool.commitments.length;
        // todo: restructure to get rid of anything that is in this loop, as either way it needs to be removed.
        console.log("Generated random seed of %s for attempt %i", seed_, attempt_);
        uint256 rolledNFT = ((seed_ + attempt_) % (nftCount));
        CommittedNFT storage commitment = pool.commitments[rolledNFT];
        OwnerCommitment storage ownerCommitment = pool.ownerCommitment[commitment.owner];
        uint32 remainingEpochs = _getRemainingEpochs(ownerCommitment.commitEndEpoch, currentEpoch);
        console.log("Generated random index of %s with total %s", rolledNFT, nftCount);
        if (remainingEpochs == 0) {
