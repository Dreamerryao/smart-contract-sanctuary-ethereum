pragma solidity ^0.8.17;

import "lib/open-zeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/open-zeppelin/contracts/token/ERC20/IERC20.sol";
import "lib/open-zeppelin/contracts/token/ERC721/IERC721.sol";
import "lib/open-zeppelin/contracts/utils/Context.sol";
import "lib/open-zeppelin/contracts/utils/Timers.sol";
import "lib/open-zeppelin/contracts/utils/math/SafeCast.sol";

contract Chamber is Context {

    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using Timers for Timers.BlockNumber;

    // -----------------------------------------------------------------
    //    State Variables
    // -----------------------------------------------------------------

    /// @dev NFT The ERC721/1155 contract used for governance.
    address public immutable NFT;

    /// @dev stakingToken The ERC20 contract used for staking
    address public immutable stakingToken;

    /// @dev quorum The number of voting captains
    uint16 public quorum;

    /// @dev for proposals
    uint64 delay = 1;
    uint64 votePeriod = 100;

    /// @notice Tracks the amount of "stakingToken" staked for a given NFT ID.
    /// @dev    First element is the NFT ID.
    ///         Second element is the amount of stakingToken allocated to the NFT ID.
    mapping(uint256 => uint256) public totalStakedForNFT;

    /// @notice Tracks a given address's stake amount of "stakingToken" for a given NFT ID.
    /// @dev    First element is the user's address.
    ///         Second element is the NFT ID.
    ///         Third element is the amount of stakingToken allocated to the NFT ID.
    mapping(address => mapping(uint256 => uint256)) public userStakeIndividualNFT;

    /// @notice Tracks a user's total amount of "stakingToken" staked.
    /// @dev    First element is the user's address.
    ///         Second element is the amount staked.
    mapping(address => uint256) public userStakeTotal;

    /// @notice The list of proposals
    /// @dev    The first element is the index
    ///         The second element is the ProposalCore
    mapping(uint256 => ProposalCore) private proposals;

    /// @dev The proposal structure, comes from OZ Governor.sol
    struct ProposalCore {
        Timers.BlockNumber voteStart;
        Timers.BlockNumber voteEnd;
        bool executed;
        bool canceled;
    }

    /// @dev comes from OZ IGovernor.sol
    enum ProposalState {
        Pending,
        Active,
        Canceled,
        Defeated,
        Succeeded,
        Queued,
        Expired,
        Executed
    }

    // -----------------------------------------------------------------
    //    Constructor
    // -----------------------------------------------------------------

    /// @param _NFT The NFT collection that will gain governance power.
    /// @param _stakingToken The token staked in this contract, amplifying governance power.
    /// @param _quorum the number of voters
    constructor(
        address _NFT,
        address _stakingToken,
        uint16 _quorum
    ) {
        NFT = _NFT;
        stakingToken = _stakingToken;
        quorum = _quorum;
    }

    // -----------------------------------------------------------------
    //    Events
    // -----------------------------------------------------------------

    /// @notice Emitted upon stake().
    /// @param staker   The address staking.
    /// @param amt      The amount of "stakingToken" staked.
    /// @param tokenId  The ID of the NFT that tokens will be staked against.
    event Staked(address staker, uint256 amt, uint256 tokenId);

    /// @notice Emitted upon unstake().
    /// @param staker   The address unstaking.
    /// @param amt      The amount of "stakingToken" unstaked.
    /// @param tokenId  The ID of the NFT that tokens were staked against.
    event Unstaked(address staker, uint256 amt, uint256 tokenId);

    /// @notice Emitted upon migrate().
    /// @param staker       The address migrated.
    /// @param amt          The amount of "stakingToken" migrated.
    /// @param fromTokenId  The ID of the NFT that tokens were staked against.
    /// @param toTokenId    The ID of the NFT that tokens will be staked against.
    event StakeMigrated(address staker, uint256 amt, uint256 fromTokenId, uint256 toTokenId);

    /// @dev Emitted when a proposal is created. This comes from OZ Governor.sol
    event ProposalCreated(
        uint256 proposalId,
        address proposer,
        address[] targets,
        uint256[] values,
        string[] signatures,
        bytes[] calldatas,
        uint256 startBlock,
        uint256 endBlock,
        string description
    );

    // -----------------------------------------------------------------
    //    STAKING
    // -----------------------------------------------------------------

    /**
        Staking is when a holder of ERC20 tokens stakes and amount of stakingToken 
        into this contract and assigns the stake amount to an NFT Id. This gives the NFT
        holder more vote power.
     */

    // TODO: Refactor canVote to implement threshold logic.
    // TODO: Governable threshold state variable (threshold for # of voting NFTs).

    // function canVote(uint256 tokenId) public pure returns (bool) {
    //     return true;
    // }

    /// @notice Returns amount a user has staked against a given NFT ID ("tokenID").
    /// @param user     The address staking.
    /// @param tokenId  The ID of the NFT "user" has staked against.
    function getUserStakeIndividualNFT(address user, uint256 tokenId)
        external
        view
        returns (uint256)
    {
        return userStakeIndividualNFT[user][tokenId];
    }

    /// @notice Stakes a given amount of "stakingToken" against the provided NFT ID.
    /// @param amt      The amount of "stakingToken" to stake.
    /// @param tokenId  The ID of the NFT to stake against.
    function stake(uint256 amt, uint256 tokenId) external {
        emit Staked(_msgSender(), amt, tokenId);

        IERC20(stakingToken).safeTransferFrom(_msgSender(), address(this), amt);
        userStakeTotal[_msgSender()] += amt;
        userStakeIndividualNFT[_msgSender()][tokenId] += amt;
        totalStakedForNFT[tokenId] += amt;
    }

    /// @notice Unstakes a given amount of "stakingToken" from the provided NFT ID.
    /// @param amt      The amount of "stakingToken" to unstake.
    /// @param tokenId  The ID of the NFT to unstake from.

    function unstake(uint256 amt, uint256 tokenId) external {
        // Note: This 1st require statement is likely unnecessary / redundant.
        require(
            userStakeTotal[_msgSender()] >= amt,
            "Chamber::unstake() userStakeTotal[_msgSender()] < amt"
        );
        require(
            userStakeIndividualNFT[_msgSender()][tokenId] >= amt,
            "Chamber::unstake() userStakeIndividualNFT[_msgSender()][tokenId] < amt"
        );

        emit Unstaked(_msgSender(), amt, tokenId);

        IERC20(stakingToken).safeTransfer(_msgSender(), amt);
        userStakeTotal[_msgSender()] -= amt;
        userStakeIndividualNFT[_msgSender()][tokenId] -= amt;
        totalStakedForNFT[tokenId] -= amt;
    }

    /// @notice Migrates a staked amount of "stakingToken" from one NFT ID to another.
    /// @param amt          The amount of "stakingToken" to migrate.
    /// @param fromTokenId  The ID of the NFT that tokens are staked currently.
    /// @param toTokenId    The ID of the NFT that tokens will be migrated to.

    function migrate(
        uint256 amt,
        uint256 fromTokenId,
        uint256 toTokenId
    ) external {
        require(
            userStakeTotal[_msgSender()] >= amt,
            "Chamber::migrate() userStakeTotal[_msgSender()] < amt"
        );
        require(
            userStakeIndividualNFT[_msgSender()][fromTokenId] >= amt,
            "Chamber::migrate() userStakeIndividualNFT[_msgSender()][fromTokenId] < amt"
        );

        emit StakeMigrated(_msgSender(), amt, fromTokenId, toTokenId);

        userStakeIndividualNFT[_msgSender()][fromTokenId] -= amt;
        totalStakedForNFT[fromTokenId] -= amt;
        userStakeIndividualNFT[_msgSender()][toTokenId] += amt;
        totalStakedForNFT[toTokenId] += amt;
    }

    // -----------------------------------------------------------------
    //    PROPOSALS
    // -----------------------------------------------------------------

    /**
        This proposal code comes almost entirely from OZ governance.
        https://docs.openzeppelin.com/contracts/4.x/governance
     */

    /// NOTE hasProposal() comes from OZ Governor.sol

    function hashProposal(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash
    ) public pure virtual returns (uint256) {
        return uint256(keccak256(abi.encode(targets, values, calldatas, descriptionHash)));
    }

    /// NOTE propse() comes from OZ Governor.sol

    function propose(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        string memory description
    ) public virtual returns (uint256) {
        // This replaces the OZ require statement that the sender has a 'threshold' minimum
        // amount of erc20 tokens. Here, we are checking if the user has an NFT.
        require(
            IERC721(NFT).balanceOf(msg.sender) > 0,
            "Chamber::propose sender does not have an nft"
        );

        uint256 proposalId = hashProposal(
            targets,
            values,
            calldatas,
            keccak256(bytes(description))
        );

        require(targets.length == values.length, "Governor: invalid proposal length");
        require(targets.length == calldatas.length, "Governor: invalid proposal length");
        require(targets.length > 0, "Governor: empty proposal");

        ProposalCore storage proposal = proposals[proposalId];
        require(proposal.voteStart.isUnset(), "Governor: proposal already exists");

        /// NOTE These values were returned from Utils in OZ GovernorSettings.sol
        /// set arbitray values for now
        // snapshot is the block start time plus some delay value
        // deadline is when the block when vote time will end
        // uint64 snapshot = block.number.toUint64() + 1; // the integer comes from TimeLock
        // uint64 deadline = snapshot + 100; // voting period should be set/controlled somewhere

        uint64 snapshot = block.number.toUint64() + delay;
        uint64 deadline = snapshot + votePeriod; 

        proposal.voteStart.setDeadline(snapshot);
        proposal.voteEnd.setDeadline(deadline);

        emit ProposalCreated(
            proposalId,
            _msgSender(),
            targets,
            values,
            new string[](targets.length),
            calldatas,
            snapshot,
            deadline,
            description
        );

        return proposalId;
    }


    /// @dev isExecuted() check if the proposal has been exectued

    function isExecuted(uint16 _id) public view returns (bool) {
        return proposals[_id].executed;
    }
    
    /// @dev proposalSnapshot() returns the proposal snapshot, from OZ Governor.sol

    function proposalSnapshot(uint256 proposalId) public view virtual returns (uint256) {
        return proposals[proposalId].voteStart.getDeadline();
    }

    /// @dev proposalDeadline() returns the proposal deadline, from OZ Governor.sol

    function proposalDeadline(uint256 proposalId) public view virtual returns (uint256) {
        return proposals[proposalId].voteEnd.getDeadline();
    }

    /// @dev quorumReached() checks if quorum threshold has been reached, from OZ Governor.sol
    // TODO

    function quorumReached (uint256 proposalId) public pure returns (bool) {
        require(proposalId >= 0, "HERE");
        return false;
    }

    /// @dev voteSucceeded() checks if the vote has succeeded, from OZ Governor.sol
    /// TODO

    function voteSucceeded (uint256 proposalId) public pure returns (bool) {
        require(proposalId >= 0, "HERE");
        return false;
    }

    /// @dev state() returns the state of a proposal, from OZ Governor.sol

    function state(uint256 proposalId) public view virtual returns (ProposalState) {
        ProposalCore storage proposal = proposals[proposalId];

        if (proposal.executed) {
            return ProposalState.Executed;
        }

        if (proposal.canceled) {
            return ProposalState.Canceled;
        }

        uint256 snapshot = proposalSnapshot(proposalId);

        if (snapshot == 0) {
            revert("Governor: unknown proposal id");
        }

        if (snapshot >= block.number) {
            return ProposalState.Pending;
        }

        uint256 deadline = proposalDeadline(proposalId);

        if (deadline >= block.number) {
            return ProposalState.Active;
        }

        if (quorumReached(proposalId) && voteSucceeded(proposalId)) {
            return ProposalState.Succeeded;
        } else {
            return ProposalState.Defeated;
        }
    }

    // -----------------------------------------------------------------
    //    VOTING
    // -----------------------------------------------------------------

}

