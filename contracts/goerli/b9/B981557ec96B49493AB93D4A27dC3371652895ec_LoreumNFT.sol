import "lib/open-zeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/open-zeppelin/contracts/utils/Context.sol";

contract ChamberBasic is Context, Motion {

    using SafeERC20 for IERC20;

    // -----------------------------------------------------------------
    //    State Variables
    // -----------------------------------------------------------------

    /// @dev NFT The ERC721/1155 contract used for governance.
    address public immutable NFT;

    /// @dev stakingToken The ERC20 contract used for staking.
    address public immutable stakingToken;

    /// @dev quorum The number of voting captains.
    uint16 public quorum;

    /// @notice Tracks the amount of "stakingToken" staked for a given NFT ID.
    /// @dev    1st element -> NFT tokenID, 2nd element -> amountStaked.
    mapping(uint256 => uint256) public totalStakedForNFT;

    /// @notice Tracks a given address's stake amount of "stakingToken" for a given NFT ID.
    /// @dev    1st element -> user address, 2nd element -> NFT tokenID, 3rd element -> amountStaked.
    mapping(address => mapping(uint256 => uint256)) public userStakeIndividualNFT;

    /// @notice Tracks a user's total amount of "stakingToken" staked.
    /// @dev    First element is the user's address. Second element is the amount staked.
    mapping(address => uint256) public userStakeTotal;

    // Linked list lightweight data structure.
    struct NFTLinkedList {
        uint stake;
        uint next;
        uint prev;
    }

    uint head;

    mapping (uint => NFTLinkedList) leaderboard;



    // -----------------------------------------------------------------
    //    Constructor
    // -----------------------------------------------------------------

    /// @param _NFT The NFT collection that will gain governance power.
    /// @param _stakingToken The token staked in this contract, amplifying governance power.
    /// @param _quorum the number of voters
    constructor(address _NFT, address _stakingToken, uint16 _quorum) {
        NFT = _NFT;
        stakingToken = _stakingToken;
        quorum = _quorum;
    }



    // -----------------------------------------------------------------
    //    Events
    // -----------------------------------------------------------------

    /// @notice Emitted upon stake().
    /// @param staker   The address staking.
    /// @param amt      The amount of "stakingToken" staked.
    /// @param tokenId  The ID of the NFT that tokens will be staked against.
    event Staked(address staker, uint256 amt, uint256 tokenId);

    /// @notice Emitted upon unstake().
    /// @param staker   The address unstaking.
    /// @param amt      The amount of "stakingToken" unstaked.
    /// @param tokenId  The ID of the NFT that tokens were staked against.
    event Unstaked(address staker, uint256 amt, uint256 tokenId);

    /// @notice Emitted upon migrate().
    /// @param staker       The address migrated.
    /// @param amt          The amount of "stakingToken" migrated.
    /// @param fromTokenId  The ID of the NFT that tokens were staked against.
    /// @param toTokenId    The ID of the NFT that tokens will be staked against.
    event StakeMigrated(address staker, uint256 amt, uint256 fromTokenId, uint256 toTokenId);


    // -----------------------------------------------------------------
    //    Functions
    // -----------------------------------------------------------------

    /// @notice Returns amount a user has staked against a given NFT ID ("tokenID").
    /// @param user     The address staking.
    /// @param tokenId  The ID of the NFT "user" has staked against.
    function getUserStakeIndividualNFT(address user, uint256 tokenId) external view returns (uint256) {
        return userStakeIndividualNFT[user][tokenId];
    }

    /// @notice Stakes a given amount of "stakingToken" against the provided NFT ID.
    /// @param amt      The amount of "stakingToken" to stake.
    /// @param tokenId  The ID of the NFT to stake against.
    function stake(uint256 amt, uint256 tokenId) public {
        emit Staked(_msgSender(), amt, tokenId);

        IERC20(stakingToken).safeTransferFrom(_msgSender(), address(this), amt);
        userStakeTotal[_msgSender()] += amt;
        userStakeIndividualNFT[_msgSender()][tokenId] += amt;
        totalStakedForNFT[tokenId] += amt;
        leaderboard[tokenId].stake += amt;

        // Case 0 - First staking action.
        if (head == 0) {
            leaderboard[tokenId].prev = tokenId;
            head = tokenId;
            return;
        }
        
        if (    // Case 1 - Updates voting power, doesn't affect position.
            leaderboard[leaderboard[tokenId].next].stake <= leaderboard[tokenId].stake && 
            leaderboard[leaderboard[tokenId].prev].stake >= leaderboard[tokenId].stake
        ) { return; }
        else {  // Case 2 - Updates voting power, does affect position.
            leaderboard[leaderboard[tokenId].prev].next = leaderboard[tokenId].next; // Remove link.
            leaderboard[leaderboard[tokenId].next].prev = leaderboard[tokenId].prev; // Remove link.
            // NOTE: Update "index" with input param for efficient search.
            (uint currentStake, uint index) = (leaderboard[tokenId].stake, head);
            if (currentStake >= leaderboard[index].stake) {
                leaderboard[tokenId].prev = tokenId;   // Point to itself.
                leaderboard[tokenId].next = index;     // Point to next.
                leaderboard[leaderboard[tokenId].next].prev = tokenId;
                head = tokenId;
                return;
            }
            while (true) {
                if (leaderboard[index].next == 0) {   // Add to end of list.
                    leaderboard[tokenId].prev = index;
                    leaderboard[index].next = tokenId;
                    return;
                }
                // Add between two elements.
                else if (currentStake >= leaderboard[leaderboard[index].next].stake) { 
                    leaderboard[tokenId].prev = index;
                    leaderboard[tokenId].next = leaderboard[index].next;
                    leaderboard[leaderboard[index].next].prev = tokenId;  // Update outer element.
                    leaderboard[index].next = tokenId;                 // Update outer element.
                    return;
                }
                index = leaderboard[index].next;
            }
        }
        
    }

    function viewRankings() public view returns(uint[] memory rankings) {
        uint index = head;
        rankings = new uint256[](quorum);
        for (uint i = 0; i < quorum; i++) {
            rankings[i] = index;
            index = leaderboard[index].next;
        }
    }

    /// @notice Unstakes a given amount of "stakingToken" from the provided NFT ID.
    /// @param amt      The amount of "stakingToken" to unstake.
    /// @param tokenId  The ID of the NFT to unstake from.
    function unstake(uint256 amt, uint256 tokenId) public {
        // Note: This 1st require statement is likely unnecessary / redundant.
        require(
            userStakeTotal[_msgSender()] >= amt,
            "Chamber::unstake() userStakeTotal[_msgSender()] < amt"
        );
        require(
            userStakeIndividualNFT[_msgSender()][tokenId] >= amt,
            "Chamber::unstake() userStakeIndividualNFT[_msgSender()][tokenId] < amt"
        );

        emit Unstaked(_msgSender(), amt, tokenId);

        IERC20(stakingToken).safeTransfer(_msgSender(), amt);
        userStakeTotal[_msgSender()] -= amt;
        userStakeIndividualNFT[_msgSender()][tokenId] -= amt;
        totalStakedForNFT[tokenId] -= amt;
        leaderboard[tokenId].stake -= amt;
        
        if (    // Case 1 - Updates voting power, doesn't affect position.
            leaderboard[leaderboard[tokenId].next].stake <= leaderboard[tokenId].stake && 
            leaderboard[leaderboard[tokenId].prev].stake >= leaderboard[tokenId].stake
        ) { return; }
        else {  // Case 2 - Updates voting power, does affect position.
            leaderboard[leaderboard[tokenId].prev].next = leaderboard[tokenId].next; // Remove link.
            leaderboard[leaderboard[tokenId].next].prev = leaderboard[tokenId].prev; // Remove link.
            // NOTE: Update "index" with input param for efficient search.
            (uint currentStake, uint index) = (leaderboard[tokenId].stake, head);
            while (true) {
                if (leaderboard[index].next == 0) {   // Add to end of list.
                    leaderboard[tokenId].prev = index;
                    leaderboard[index].next = tokenId;
                    return;
                }
                // Add between two elements.
                else if (currentStake >= leaderboard[leaderboard[index].next].stake) { 
                    leaderboard[tokenId].prev = index;
                    leaderboard[tokenId].next = leaderboard[index].next;
                    leaderboard[leaderboard[index].next].prev = tokenId;    // Update outer element.
                    leaderboard[index].next = tokenId;                      // Update outer element.
                    return;
                }
                index = leaderboard[index].next;
            }
        }
    }

    /// @notice Migrates a staked amount of "stakingToken" from one NFT ID to another.
    /// @param amt          The amount of "stakingToken" to migrate.
    /// @param fromTokenId  The ID of the NFT that tokens are staked currently.
    /// @param toTokenId    The ID of the NFT that tokens will be migrated to.
    function migrate(uint256 amt, uint256 fromTokenId, uint256 toTokenId) external {
        unstake(amt, fromTokenId);
        stake(amt, toTokenId);
    }

}

