// TODO: natspec
interface IAverage {
  struct Position {
    uint256 id;
    // user given
    address source;
    address target;
    uint256 amount;
    Duration duration;
    // computed
    uint256 start;
    uint256 end;
    uint256 amountPerWindow;
    uint256 finalAmountPerWindow;
  }

  enum Duration {
    DAY,
    WEEK,
    FORTNIGHT,
    MONTH,
    YEAR
  }

  struct Transaction {
    // uint id;
    address asset;
    int256 amount;
    uint256 time;
  }

  struct SwapInfo {
    uint256 current_amount_per_window;
    uint256 last_swap_time;
    // time ==> deltas
    mapping(uint256 => int256) deltas;
    // time ==> trades
    mapping(uint256 => Trade) trades;
  }

  struct Trade {
    uint256 amountIn;
    uint256 amountOut;
    // pointer to the next trade entry
    uint256 next_swap_time;
  }

  struct Balance {
    address asset;
    uint256 amount;
  }

  event Swap(
    address source,
    address target,
    bool all_swapped,
    uint256 last_added_time
  );
  event Deposit(address user, Transaction transaction);

  // access to mappings
  function positions(address user) external view returns (Position[] memory);
}

