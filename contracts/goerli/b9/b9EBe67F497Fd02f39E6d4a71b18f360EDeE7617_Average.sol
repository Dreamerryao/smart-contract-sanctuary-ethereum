import { TransferHelper } from "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import { ISwapRouter } from "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { IAverage } from "./interfaces/IAverage.sol";
import { Math } from "@openzeppelin/contracts/utils/math/Math.sol";
import "hardhat/console.sol";

contract Average is IAverage {
  address private constant USDC_GOERLI =
    0x07865c6E87B9F70255377e024ace6630C1Eaa37F; // decimals: 6
  address private constant WETH_GOERLI =
    0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6; // decimals: 18
  address private constant WBTC_GOERLI =
    0xdA4a47eDf8ab3c5EeeB537A97c5B66eA42F49CdA; // decimals: 8

  // For this example, we will set the pool fee to 0.3%.
  uint24 public constant poolFee = 3000;

  // === TIME WINDOW RELATED VARIABLES ===
  // smallest window or processing unit of time is 60 seconds
  uint public constant base_window = 60 seconds;
  // the number of windows in each valid duration defined in enum Duration
  uint[5] public duration_windows = [1440, 10080, 20160, 43200, 525600];

  // === SWAP RELATED VARIABLES ===
  // the time the contract was deployed which is the lower bound on swap times
  uint256 internal starting_time;
  // source token => target token => SwapInfo
  mapping(address => mapping(address => SwapInfo)) internal swap_infos;

  address[] public valid_sources = [USDC_GOERLI];
  address[] public valid_targets = [WETH_GOERLI, WBTC_GOERLI];

  ISwapRouter public immutable swapRouter =
    ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);

  // === USER RELATED VARIABLES ===
  // global id maker
  uint256 public positionId;
  mapping(address => Position[]) private _positions;
  mapping(address => Transaction[]) private _transactions;

  constructor() {
    starting_time = getNowWindow();
  }

  function isObsolete() public view {}

  // === HELPERS ===

  /**
   * @dev for getting the latest time window
   * @return  now_window a start of window adjusted time reported in unix seconds.
   */
  function getNowWindow() private view returns (uint256) {
    return block.timestamp - (block.timestamp % base_window);
  }

  function _includes(address[] memory assets, address value)
    public
    pure
    returns (bool includes)
  {
    for (uint256 i = 0; i < assets.length; i++) {
      if (assets[i] == value) {
        return (true);
      }
    }
    return (false);
  }

  // === CORE FUNCTIONS ===
  /**
   * @notice  should only be called by dystopia labs
   * @param   source  token to swap from
   * @param   target  token to swap to
   */
  function swap(address source, address target) public {
    // === CHECKS ===
    require(
      _includes(valid_sources, source),
      "Swapping of that source token is not supported"
    );
    require(
      _includes(valid_targets, target),
      "Swapping of that target token is not supported"
    );
    uint256 now_window = getNowWindow();
    SwapInfo storage swap_info = swap_infos[source][target];

    require(
      swap_info.last_swap_time < now_window,
      "already swapped for this window"
    );

    // === SETUP ===
    // the first window should either be the last swap of the starting time of the contract
    uint256 first_window = (swap_info.last_swap_time < starting_time)
      ? starting_time
      : swap_info.last_swap_time;
    // goto next block
    first_window += base_window;

    uint256 amountIn;
    uint256 current_amount_per_window = swap_info.current_amount_per_window;
    uint256 loops;
    // about 2 days
    uint256 max_loop = 2880;
    uint256 last_added_time = first_window;

    while (last_added_time <= now_window && loops < max_loop) {
      // adjust the amount per window based on any stored deltas
      current_amount_per_window = uint256(
        int256(current_amount_per_window) + swap_info.deltas[last_added_time]
      );
      // added this windows ammount to the accumulator
      amountIn += current_amount_per_window;
      // count loops to paginate if needed
      loops++;
      // move to the next time window
      last_added_time += base_window;
    }
    // revert to last process time window
    last_added_time -= base_window;
    // save the final amount to per window for use next swap call
    swap_info.current_amount_per_window = current_amount_per_window;

    // if nothing in short-curcuit swap
    if (amountIn <= 0) {
      swap_info.last_swap_time = last_added_time;
      emit Swap(source, target, last_added_time == now_window, last_added_time);
      return;
    }

    // === SWAP ===

    // Approve the router to spend source
    TransferHelper.safeApprove(source, address(swapRouter), amountIn);

    // swap what has been accumulated
    ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
      .ExactInputSingleParams({
        tokenIn: source,
        tokenOut: target,
        fee: poolFee,
        recipient: address(this),
        deadline: block.timestamp,
        amountIn: amountIn,
        amountOutMinimum: 0,
        sqrtPriceLimitX96: 0
      });

    uint256 amountOut = swapRouter.exactInputSingle(params);

    // === RECORD KEEPKING ===

    // save point from last trade to this one
    swap_info.trades[swap_info.last_swap_time].next_swap_time = last_added_time;

    // save latest trade timestamp
    swap_info.last_swap_time = last_added_time;

    // save trade
    swap_info.trades[last_added_time] = Trade({
      amountIn: amountIn,
      amountOut: amountOut,
      next_swap_time: 0
    });

    emit Swap(source, target, last_added_time == now_window, last_added_time);
  }

  /**
   * @notice  going away, just not scalable at all
   * @param   user  .
   * @return  Balance[]  .
   */
  function getUserBalances(address user)
    external
    view
    returns (Balance[] memory)
  {
    Balance[] memory balances = new Balance[](
      valid_sources.length + valid_targets.length
    );
    uint256 active_balances;
    for (uint256 i; i < valid_sources.length; i++) {
      uint256 amount = getUserBalance(user, valid_sources[i]);
      if (amount > 0) {
        balances[active_balances] = Balance({
          asset: valid_sources[i],
          amount: amount
        });
        active_balances++;
      }
    }
    for (uint256 i; i < valid_targets.length; i++) {
      uint256 amount = getUserBalance(user, valid_targets[i]);
      if (amount > 0) {
        balances[active_balances] = Balance({
          asset: valid_targets[i],
          amount: amount
        });
        active_balances++;
      }
    }

    Balance[] memory return_balances = new Balance[](active_balances);
    for (uint256 i; i < active_balances; i++) {
      return_balances[i] = balances[i];
    }

    return return_balances;
  }

  /**
   * @notice  being split into source balance ~ O(positions) and target balances - which will be paginated ~ O(elapsed time) otherwise
   * @param   user  .
   * @return  Balance[]  .
   */
  function getUserBalance(address user, address asset)
    public
    view
    returns (uint256)
  {
    uint256 now_window = getNowWindow();
    int256 balance;
    // deposit and withdraw
    for (uint256 i; i < _transactions[user].length; i++) {
      if (_transactions[user][i].asset == asset) {
        balance += _transactions[user][i].amount;
      }
    }

    // update base on positions
    for (uint256 i; i < _positions[user].length; i++) {
      // check for started
      if (
        _positions[user][i].start == 0 || _positions[user][i].start > now_window
      ) continue;

      // count outflows
      if (_positions[user][i].source == asset) {
        balance -= int256(_getPositionOutflows(_positions[user][i]));
        // console.log("outflows", uint256(balance));
      }

      // count inflows
      if (_positions[user][i].target == asset) {
        balance += int256(_getPositionInflows(_positions[user][i]));
        // console.log("inflows", uint256(balance));
      }
    }

    return uint256(balance);
  }

  /**
   * @dev  computes the results of swaps for this position in terms of resulting target token
   * @param   position  .
   * @return  amount  the amount of target tokens
   */
  function _getPositionInflows(Position storage position)
    private
    view
    returns (uint256)
  {
    (
      uint256 first_time,
      uint256 my_last_swap_time,
      ,
      bool no_swap
    ) = _getPositionTimes(position);
    // if there has been no swap
    if (no_swap) return 0;

    uint256 amount_traded_in;
    uint256 my_amountIn;
    uint256 previous_time = first_time - base_window;
    uint256 last_swap_time = swap_infos[position.source][position.target]
      .last_swap_time;

    // TODO reverse this loop so we start at lastest swap and skip our way backwards
    bool no_more_swaps = false;
    for (
      uint256 time = first_time;
      time <= last_swap_time && !no_more_swaps;

    ) {
      Trade storage trade = swap_infos[position.source][position.target].trades[
        time
      ];

      if (trade.amountIn == 0) {
        // have not found a swap yet
        time += base_window;
        continue;
      }

      uint256 end_time = Math.min(my_last_swap_time, time);

      my_amountIn +=
        position.amountPerWindow *
        ((end_time - previous_time) / base_window);
      previous_time = end_time;

      if (end_time == my_last_swap_time) {
        // change amount to finalAmountPerWindow
        my_amountIn -= position.amountPerWindow;
        my_amountIn += position.finalAmountPerWindow;

        no_more_swaps = true;
      }
      // (total / mine) * amountOut
      uint256 my_share = (trade.amountIn * trade.amountOut) / my_amountIn;
      amount_traded_in += my_share;

      my_amountIn = 0;

      if (trade.next_swap_time == 0) {
        no_more_swaps = true;
      } else {
        time = trade.next_swap_time;
      }
    }
    return amount_traded_in;
  }

  function _getPositionTimes(Position storage position)
    private
    view
    returns (
      uint256 first_time,
      uint256 my_last_swap_time,
      uint256 last_time,
      bool no_swap
    )
  {
    first_time = position.start;
    my_last_swap_time = position.end - base_window;
    last_time = Math.min(
      my_last_swap_time,
      swap_infos[position.source][position.target].last_swap_time
    );

    no_swap = (first_time > last_time);
  }

  /**
   * @dev     computes the result of trading the source token away as prescribed by the position ~ O(1)
   * @param   position  .
   * @return  uint256  the accumulation of trading away the source over time
   */
  function _getPositionOutflows(Position storage position)
    private
    view
    returns (uint256)
  {
    (
      uint256 first_time,
      uint256 my_last_swap_time,
      uint256 last_time,
      bool no_swap
    ) = _getPositionTimes(position);
    // if there has been no swap
    if (no_swap) return 0;

    uint256 windows = 1 + ((last_time - first_time) / base_window); // if only first_time process it still 1 window

    uint256 amount_traded_away = position.amountPerWindow * windows;
    if (last_time == my_last_swap_time) {
      amount_traded_away =
        amount_traded_away -
        position.amountPerWindow +
        position.finalAmountPerWindow;
    }
    return amount_traded_away;
  }

  /**
   * @notice  the amount you can safely withdraw at this current time, takes into account position money pending a swap ~ will be O(positions)
   * @dev     should be changed to only be source tokens - as target tokens will require a trusted actor to get that balance through pagination
   * @param   user  .
   * @param   asset  .
   * @return  amount  .
   */
  function getUserWithdrawableBalance(address user, address asset)
    public
    view
    returns (uint256 amount)
  {
    uint256 current_balance = getUserBalance(user, asset);

    // === NOW GET PENDING OR UNREALIZED MONEY FOR SWAPS ===
    uint256 unrealized_balance_transfer;
    // any money pending on a position up until now that has not yet be part of a swap accumulate
    uint256 now_window = getNowWindow();

    for (uint256 i; i < _positions[user].length; i++) {
      SwapInfo storage swap_info = swap_infos[asset][
        _positions[user][i].target
      ];

      if (
        _positions[user][i].end > swap_info.last_swap_time &&
        _positions[user][i].start <= now_window &&
        _positions[user][i].source == asset
      ) {
        uint256 last_time = Math.min(_positions[user][i].end, now_window);
        uint256 first_time = Math.max(
          _positions[user][i].start,
          swap_info.last_swap_time
        );
        uint256 passed_windows = (last_time - first_time) / base_window;

        if (_positions[user][i].end <= now_window) {
          unrealized_balance_transfer += _positions[user][i]
            .finalAmountPerWindow;
          passed_windows -= 1;
        }

        unrealized_balance_transfer += (passed_windows *
          _positions[user][i].amountPerWindow);
      }
    }

    return current_balance - unrealized_balance_transfer;
  }

  /**
   * @notice  deposit a valid source into average contract so positions can use the money
   * @dev     scales ~ O(positions)
   * @param   amount  .
   * @param   asset  .
   */
  function deposit(uint256 amount, address asset) public {
    require(amount > 0, "Deposit must be greater than 0");
    require(
      _includes(valid_sources, asset),
      "Deposit of that token is not supported"
    );

    IERC20(asset).transferFrom(msg.sender, address(this), amount);

    Transaction memory transaction = Transaction(
      asset,
      int256(amount),
      block.timestamp
    );
    _transactions[msg.sender].push(transaction);

    _updatePositions(asset);
    emit Deposit(msg.sender, transaction);
  }

  /**
   * @notice  .
   * @dev     .
   * @param   amount  .
   * @param   asset  .
   */
  function withdraw(uint256 amount, address asset) public {
    uint256 balance = getUserWithdrawableBalance(msg.sender, asset);
    require(
      amount <= balance,
      "Withdraw is greater than your available balance"
    );

    IERC20(asset).transfer(msg.sender, amount);
    Transaction memory transaction = Transaction(
      asset,
      -int256(amount),
      block.timestamp
    );
    _transactions[msg.sender].push(transaction);

    _updatePositions(asset);
  }

  /// @inheritdoc IAverage
  function positions(address user) external view returns (Position[] memory) {
    return _positions[user];
  }

  /**
   * @notice  .
   * @dev     .
   * @param   source  .
   * @param   target  .
   * @param   amount  .
   * @param   duration  .
   */
  function createPosition(
    address source,
    address target,
    uint256 amount,
    Duration duration
  ) public {
    require(_includes(valid_sources, source), "Source is not valid");
    require(_includes(valid_targets, target), "Target is not valid");
    require(amount > 0, "Position amount must be greater than 0");

    positionId++;
    Position memory position = Position({
      source: source,
      target: target,
      amount: amount,
      duration: duration,
      id: positionId,
      start: 0,
      amountPerWindow: amount / duration_windows[uint(duration)],
      finalAmountPerWindow: 0,
      end: 0
    });

    _positions[msg.sender].push(position);

    _updatePositions(source);
  }

  /**
   * @notice  .
   * @dev     .
   * @param   closePositionId  .
   */
  function closePosition(uint256 closePositionId) public {
    uint256 now_window = getNowWindow();

    for (uint256 i; i < _positions[msg.sender].length; i++) {
      if (_positions[msg.sender][i].id == closePositionId) {
        if (
          _positions[msg.sender][i].end <= now_window &&
          _positions[msg.sender][i].start != 0
        ) {
          return; // do nothing - no op
        }

        _updatePositionAndDeltas(0, _positions[msg.sender][i], true);
        _updatePositions(_positions[msg.sender][i].source);
      }
    }
  }

  // === INTERNAL HELPERS ===

  /**
   * @dev     alters positions to spend the source so that they all end at about the same time
   * @param   source  token
   */
  function _updatePositions(address source) private {
    // === DESIRES ===
    // find the total per window of desired trading
    uint256 totalAmountPerWindow;

    for (uint256 i; i < _positions[msg.sender].length; i++) {
      if (_stillActivePosition(_positions[msg.sender][i], source)) {
        totalAmountPerWindow += _positions[msg.sender][i].amountPerWindow;
      }
    }

    if (totalAmountPerWindow <= 0) return; // do nothing - no op

    // === HAVES ===
    uint256 spendable_balance = getUserWithdrawableBalance(msg.sender, source);
    uint256 balance_left = spendable_balance;

    // === SPEND ===
    // update postion to spend its updated amount from total
    for (uint256 i; i < _positions[msg.sender].length; i++) {
      if (_stillActivePosition(_positions[msg.sender][i], source)) {
        uint256 amountToSpend = (spendable_balance *
          _positions[msg.sender][i].amountPerWindow) / totalAmountPerWindow;

        balance_left -= amountToSpend;

        if (i == _positions[msg.sender].length - 1) {
          // the last so add remainder
          amountToSpend += balance_left;
        }
        // console.log("amountToSpend", amountToSpend);
        // console.log("spendable_balance", spendable_balance);

        _updatePositionAndDeltas(
          amountToSpend,
          _positions[msg.sender][i],
          false
        );
      }
    }
  }

  function _stillActivePosition(Position storage position, address source)
    private
    view
    returns (bool)
  {
    uint256 now_window = getNowWindow();
    // end is one time window after last swap
    return
      (position.end > (now_window + base_window) || position.start == 0) &&
      position.source == source;
  }

  /**
   * @dev     given an amount update start, stop, spend rates, and swap_info's stored deltas
   * @param   amount  .
   * @param   position  .
   */
  function _updatePositionAndDeltas(
    uint256 amount,
    Position storage position,
    bool close
  ) private {
    SwapInfo storage swap_info = swap_infos[position.source][position.target];
    Position memory new_position = _updatePositionTimes(amount, position);
    uint256 now_window = getNowWindow();

    // === JUST CLOSE IF NEEDED ===
    if (close) {
      // end is one time window after last swap
      new_position.end = now_window + base_window;

      if (new_position.start == 0 || new_position.start > now_window) {
        // never started
        new_position.amountPerWindow = 0;
        new_position.finalAmountPerWindow = 0;
      } else {
        // no remainder - just end it
        new_position.finalAmountPerWindow = new_position.amountPerWindow;
      }
    }

    // === UPDATE DELTA START IF NEEDED ===
    if (new_position.start != position.start) {
      if (position.start != 0) {
        // remove old value from deltas
        swap_info.deltas[position.start] -= int256(position.amountPerWindow);
      }
      if (new_position.start != 0) {
        // add new value to deltas
        swap_info.deltas[new_position.start] += int256(
          new_position.amountPerWindow
        );
      }
    }

    // === UPDATE DELTA ENDINGS IF NEEDED ===
    if (
      new_position.end != position.end ||
      new_position.finalAmountPerWindow != position.finalAmountPerWindow
    ) {
      if (position.end != 0) {
        // remove old values from deltas
        swap_info.deltas[position.end - base_window] += int256(
          position.amountPerWindow - position.finalAmountPerWindow
        );
        swap_info.deltas[position.end] += int256(position.finalAmountPerWindow);
      }
      if (new_position.end != 0) {
        // add new values to deltas
        swap_info.deltas[new_position.end - base_window] -= int256(
          new_position.amountPerWindow - new_position.finalAmountPerWindow
        );
        swap_info.deltas[new_position.end] -= int256(
          new_position.finalAmountPerWindow
        );
      }
    }

    // === SAVE UPDATED POSITION VALUES ===
    position.start = new_position.start;
    position.end = new_position.end;
    position.amountPerWindow = new_position.amountPerWindow;
    position.finalAmountPerWindow = new_position.finalAmountPerWindow;
  }

  /**
   * @dev     computes the new start, end, and finalAmountPerWindow based spending all of amountToSpend
   * @param   amountToSpend  .
   * @param   position  .
   * @return  Position  .
   */
  function _updatePositionTimes(uint256 amountToSpend, Position memory position)
    private
    view
    returns (Position memory)
  {
    uint256 now_window = getNowWindow();

    if (amountToSpend == 0) {
      if (position.start > 0 && position.end > now_window) {
        // end is one time window after last swap
        position.end = now_window + base_window;
        // no remainder - just end it
        position.finalAmountPerWindow = position.amountPerWindow;
      }
      // already ended or never started do nothing
      return position;
    }

    if (position.start == 0) {
      position.start = now_window + base_window;
    }

    uint256 windows = amountToSpend / position.amountPerWindow;

    uint finalAmountPerWindow = amountToSpend -
      (windows * position.amountPerWindow);

    position.finalAmountPerWindow = finalAmountPerWindow;

    if (finalAmountPerWindow > 0) {
      windows++;
    }

    // console.log("timestamp", block.timestamp);
    // console.log("now_window", now_window);
    // console.log("amountToSpend", amountToSpend);
    // console.log("amountPerWindow", position.amountPerWindow);
    // console.log("finalAmountPerWindow", position.finalAmountPerWindow);
    // console.log("windows", windows);
    // console.log("start", position.start);

    if (position.start > now_window) {
      position.end = position.start + (windows * base_window);
    } else {
      position.end = now_window + (windows * base_window);
    }

    return position;
  }
}

