   */
  function _read_trusted_validators(
    uint256 p,
    bytes memory bs,
    Data memory r,
    uint[5] memory counters
  ) internal pure returns (uint) {
    /**
     * if `r` is NULL, then only counting the number of fields.
     */
    (ValidatorSet.Data memory x, uint256 sz) = _decode_ValidatorSet(p, bs);
    if (isNil(r)) {
