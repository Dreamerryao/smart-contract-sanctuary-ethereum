    function _burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory amounts
    ) internal override whenNotPaused {
        uint256 idsLength = ids.length;
        require(account != address(0), "USN: Burn From 0 Address");
        require(
            idsLength == amounts.length,
            "USN: IDS_AMOUNTS_LENGTH_MISMATCH"
        );

        address operator = _msgSender();

        _beforeTokenTransfer(operator, account, address(0), ids, amounts, "");

        for (uint256 i; i < idsLength; ) {
            uint256 id = ids[i];
            uint256 amount = amounts[i];

            uint256 accountBalance = balances[id][account];
            require(accountBalance >= amount, "USN: Amount Exceeds Balance");
            balances[id][account] = accountBalance - amount;
            _supply[id] -= amount;
            unchecked {
                ++i;
            }
        }

        emit TransferBatch(operator, account, address(0), ids, amounts);
    }

    function setApprovalForAll(
        address operator,
        bool approved
    ) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    // Override this to change birth events' _from address
    function _origin(
        uint256 /* _id */
    ) internal view virtual returns (address) {
        return address(0);
    }

    // PROXY HELPER METHODS

    function _isProxyForUser(
        address _user,
        address _address
    ) internal view virtual returns (bool) {
        if (!proxyRegistryAddress.isContract()) {
            return false;
        }
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        return address(proxyRegistry.proxies(_user)) == _address;
    }

    function doSafeTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) private {
        if (to.isContract()) {
            try
                IERC1155Receiver(to).onERC1155Received(
                    operator,
                    from,
                    id,
                    amount,
                    data
                )
            returns (bytes4 response) {
                if (
                    response != IERC1155Receiver(to).onERC1155Received.selector
                ) {
                    revert("USN: ERC1155Receiver Rejected Tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("USN: Transfer To Non ERC1155Receiver Implementer");
            }
        }
    }

    function doSafeBatchTransferAcceptanceCheck(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal {
        if (to.isContract()) {
            try
                IERC1155Receiver(to).onERC1155BatchReceived(
                    operator,
                    from,
                    ids,
                    amounts,
                    data
                )
            returns (bytes4 response) {
                if (
                    response !=
                    IERC1155Receiver(to).onERC1155BatchReceived.selector
                ) {
                    revert("USN: ERC1155Receiver Rejected Tokens");
                }
            } catch Error(string memory reason) {
                revert(reason);
            } catch {
                revert("USN: Transfer To Non ERC1155Receiver Implementer");
            }
        }
    }

    function asSingletonArray(
        uint256 element
    ) private pure returns (uint256[] memory) {
        uint256[] memory array = new uint256[](1);
        array[0] = element;

        return array;
    }

    /// @dev Returns whether royalty info can be set in the given execution context.
    function _canSetRoyaltyInfo()
        internal
        view
        virtual
        override
        returns (bool)
    {
        return _msgSender() == owner();
    }

    function owner()
        public
        view
        virtual
        override(TwoStepOwnable, UpdatableOperatorFilterer)
        returns (address)
    {
        return TwoStepOwnable.owner();
    }
}

