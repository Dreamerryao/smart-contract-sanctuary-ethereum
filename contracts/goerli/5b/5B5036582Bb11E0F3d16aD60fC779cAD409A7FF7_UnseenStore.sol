pragma solidity ^0.8.18;

import "./ERC1155Tradable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

/**
 *  @title Store contract
 *  @notice Unseen store contract for easily creating store assets with Unseen builder
 *  @author Unseen | decapinator.eth
 **/
contract StoreContract is ERC1155Tradable {
    /// @notice using strings for uints (token ID encoding)
    using Strings for uint256;

    /// @notice Emitted when a token URI is set to permanent
    event PermanentURI(string _value, uint256 indexed _id);

    uint256 constant TOKEN_SUPPLY_CAP = 1;

    string public templateURI;

    // Optional mapping for token URIs
    mapping(uint256 => string) private _tokenURI;

    // Mapping for whether a token URI is set permanently
    mapping(uint256 => bool) private _isPermanentURI;

    ///@notice check if user own _quantity to be burned
    modifier onlyTokenAmountOwned(
        address _from,
        uint256 _id,
        uint256 _quantity
    ) {
        require(
            _ownsTokenAmount(_from, _id, _quantity),
            "USN: Only Token Amount Owned Allowed"
        );
        _;
    }

    /**
     * @dev Require the URI to be impermanent
     */
    modifier onlyImpermanentURI(uint256 id) {
        require(!isPermanentURI(id), "USN: Uri Cannot Be Changed");
        _;
    }

    /**
     * Constructor
     * @param _proxyRegistryAddress - address of the OpenSea proxy registry
     * @param _templateURI - URI for the template
     * @param _royaltyRecipient - address of the royalty recipient
     * @param _royaltyBps - royalty amount in basis points
     */
    constructor(
        address _proxyRegistryAddress,
        string memory _templateURI,
        address _royaltyRecipient,
        uint256 _royaltyBps
    )
        payable
        ERC1155Tradable(
            _proxyRegistryAddress,
            _templateURI,
            _royaltyRecipient,
            _royaltyBps
        )
    {
        _setInitialOwner(msg.sender);
        if (bytes(_templateURI).length != 0) {
            setTemplateURI(_templateURI);
        }
    }

    /**
     * @return the contract version
     */
    function UnseenVersion() public pure returns (string memory) {
        return "1.0.0";
    }

    /**
     * @dev Require _from to own a specified quantity of the token
     */
    function _ownsTokenAmount(
        address _from,
        uint256 _id,
        uint256 _quantity
    ) internal view returns (bool) {
        return balanceOf(_from, _id) >= _quantity;
    }

    /**
     * @dev Set template URI
     * @param _uri - URI for the template
     */
    function setTemplateURI(string memory _uri) public onlyOwnerOrProxy {
        templateURI = _uri;
    }

    /**
     * @dev Set token URI
     * @param _id - token ID
     * @param _uri - URI for the token
     */
    function setURI(
        uint256 _id,
        string memory _uri
    ) external virtual onlyOwnerOrProxy onlyImpermanentURI(_id) {
        _setURI(_id, _uri);
    }

    /**
     * @dev Set token URI permanently
     * @param _id - token ID
     * @param _uri - URI for the token
     */
    function setPermanentURI(
        uint256 _id,
        string memory _uri
    ) external virtual onlyOwnerOrProxy onlyImpermanentURI(_id) {
        _setPermanentURI(_id, _uri);
    }

    ///@dev Returns if the id has parmanent URI
    function isPermanentURI(uint256 _id) public view returns (bool) {
        return _isPermanentURI[_id];
    }

    ///@dev Returns the URI for a token ID
    function uri(uint256 _id) public view override returns (string memory) {
        string memory tokenUri = _tokenURI[_id];
        if (bytes(tokenUri).length != 0) {
            return tokenUri;
        }
        return
            string(abi.encodePacked(super.uri(_id), _id.toString(), ".json"));
    }

    function balanceOf(
        address _owner,
        uint256 _id
    ) public view virtual override returns (uint256) {
        uint256 balance = super.balanceOf(_owner, _id);
        return
            _isCreatorOrProxy(_id, _owner)
                ? balance + _remainingSupply(_id)
                : balance;
    }

    function safeTransferFrom(
        address _from,
        address _to,
        uint256 _id,
        uint256 _amount,
        bytes memory _data
    ) public override {
        uint256 mintedBalance = super.balanceOf(_from, _id);
        if (mintedBalance < _amount) {
            // Only mint what _from doesn't already have
            mint(_to, _id, _amount - mintedBalance, _data);
            if (mintedBalance != 0) {
                super.safeTransferFrom(_from, _to, _id, mintedBalance, _data);
            }
        } else {
            super.safeTransferFrom(_from, _to, _id, _amount, _data);
        }
    }

    function safeBatchTransferFrom(
        address _from,
        address _to,
        uint256[] memory _ids,
        uint256[] memory _amounts,
        bytes memory _data
    ) public override {
        uint256 idsLength = _ids.length;
        require(idsLength == _amounts.length, "USN: Invalid Arrays Length");
        for (uint256 i; i < idsLength; ) {
            safeTransferFrom(_from, _to, _ids[i], _amounts[i], _data);
            unchecked {
                ++i;
            }
        }
    }

    function _beforeMint(
        uint256 _id,
        uint256 _quantity
    ) internal view override {
        require(
            _quantity <= _remainingSupply(_id),
            "USN: Quantity Exceeds total Supply Cap"
        );
    }

    // Overrides ERC1155Tradable burn to check for quantity owned
    function burn(
        address _from,
        uint256 _id,
        uint256 _quantity
    ) public override onlyTokenAmountOwned(_from, _id, _quantity) {
        super.burn(_from, _id, _quantity);
    }

    // Overrides ERC1155Tradable batchBurn to check for quantity owned
    function batchBurn(
        address _from,
        uint256[] memory _ids,
        uint256[] memory _quantities
    ) public override {
        uint256 idsLength = _ids.length;
        for (uint256 i; i < idsLength; ) {
            require(
                _ownsTokenAmount(_from, _ids[i], _quantities[i]),
                "USN: Only Token Amount Owned Allowed"
            );
            unchecked {
                ++i;
            }
        }
        super.batchBurn(_from, _ids, _quantities);
    }

    function _mint(
        address _to,
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) internal override {
        super._mint(_to, _id, _quantity, _data);
        if (_data.length > 1) {
            _setURI(_id, string(_data));
        }
    }

    ///@dev Returns if the address is the creator or proxy
    function _isCreatorOrProxy(
        uint256,
        address _address
    ) internal view virtual returns (bool) {
        return _isOwnerOrProxy(_address);
    }

    ///@dev Returns the remaining supply for a token ID
    function _remainingSupply(
        uint256 _id
    ) internal view virtual returns (uint256) {
        return TOKEN_SUPPLY_CAP - totalSupply(_id);
    }

    // Override ERC1155Tradable for birth events
    function _origin(
        uint256 /* _id */
    ) internal view virtual override returns (address) {
        return owner();
    }

    function _batchMint(
        address _to,
        uint256[] memory _ids,
        uint256[] memory _quantities,
        bytes memory _data
    ) internal virtual override {
        super._batchMint(_to, _ids, _quantities, _data);
        if (_data.length > 1) {
            uint256 idsLength = _ids.length;
            for (uint256 i; i < idsLength; ) {
                _setURI(_ids[i], string(_data));
                unchecked {
                    ++i;
                }
            }
        }
    }

    /**
     * @dev Set token URI
     */
    function _setURI(uint256 _id, string memory _uri) internal {
        _tokenURI[_id] = _uri;
        emit URI(_uri, _id);
    }

    /**
     * @dev Set token URI permanently
     */
    function _setPermanentURI(
        uint256 _id,
        string memory _uri
    ) internal virtual {
        require(bytes(_uri).length != 0, "USN: Only Valid Uri");
        _isPermanentURI[_id] = true;
        _setURI(_id, _uri);
        emit PermanentURI(_uri, _id);
    }
}

