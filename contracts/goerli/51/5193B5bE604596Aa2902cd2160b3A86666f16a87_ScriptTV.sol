/// @title Script TV
/// @author @n4beel & @nabeelimran852
/// @notice Contract for Script TV - watch to earn platform
contract ScriptTV is Ownable, ERC1155Holder {
  // bytes32 type for storing transaction hashes
  using ECDSA for bytes32;

  // Address of the SPAY Token
  IScriptPay public immutable spay;

  // Address of Glasses contract
  IScriptGlasses public immutable scriptGlasses;

  // Address of Voucher contract
  IScriptVoucher public immutable scriptVoucher;

  //Address of treasury
  address public treasury;

  //Recharge limit per day
  uint8 public rechargeLimitPerDay;

  //Percentage of treasury cut
  uint256 public treasuryPercentage;

  // 1 Day (24 * 60 * 60)
  uint256 private constant SECONDS_PER_DAY = 24 * 60 * 60;

  // Glass struct for storing data about different glasses types
  // maxSupply - max number of glasses that can be minted
  // mintedSupply - number of glasses already minted
  // cost - amount of SPAY burnt when minting a pair of glasses
  struct Glass {
    uint24 maxSupply;
    uint24 mintedSupply;
    uint208 glassCost;
  }

  // mapping to store glasses data
  mapping(uint256 => Glass) public glasses;

  // has a transaction been executed
  mapping(bytes32 => bool) public executed;

  // does a user own a pair of free glasses
  mapping(address => bool) public freemiumOwner;

  // No of times a glass has recharged on a particular day
  // <glassID> => <No. of day> => <Number of recharges>
  mapping(uint256 => mapping(uint256 => uint8)) public rechargeTimes;

  // gem reward claimed status of a specific pool
  // <gemID> => <poolId> => <claimed or not>
  mapping(uint256 => mapping(uint256 => bool)) public poolRewardClaimed;

  // Price for different gems
  // <glassType> => <Price of gem>, types are 1,2,3
  mapping(uint8 => uint256) public gemPrices;

  // mapping to keep track of the vouchers held by the contract
  mapping(address => mapping(uint256 => bool)) public associatedVouchers;

  /**
   * @notice Emitted when user receives reward for watching content
   * @param to address of the receiver
   * @param amount amount of SPAY to be paid out
   * @param nonce transaction ID
   */
  event PayoutEarned(
    address indexed to,
    uint256 amount,
    uint256 nonce
  );

  /**
   * @notice Emitted when the user burns SPAY for recharging their glasses
   * @param from address of the spender
   * @param glassId id of glasses being recharged
   * @param discountedAmount amount of spay burnt for recharge
   * @param nonce nonce used for signature
   */
  event GlassesRecharged(
    address from,
    uint256 glassId,
    uint256 discountedAmount,
    uint256 nonce
  );

  /**
   * @notice Emitted when the user mints a pair of glasses
   * @param user address of the user minting the glasses
   * @param tokenID ID of glass being minted
   * @param glassType type of glasses being minted
   */
  event GlassesMinted(
    address user,
    uint256 tokenID,
    ScriptNFTType glassType
  );

  /**
   * @notice Emitted when the user mints a pair of glasses
   * @param user address of the user minting the glasses
   * @param tokenID ID of glass being minted
   * @param nonce nonce
   */
  event FreeMinted(
    address indexed user,
    uint256 tokenID,
    uint256 nonce
  );

  /**
   * @notice Emitted when a pair of glasses are confiscated
   * @param user address of the user
   * @param tokenID ID of glass being confiscated
   */
  event GlassesConfiscated(address indexed user, uint256 tokenID);

  /**
   * @notice Emitted when the user equips a gem into a glasses' socket
   * @param glassID ID of the glasses
   * @param gemType type of gem equipped
   */
  event GemEquipped(
    uint256 glassID,
    uint8 gemType
  );

  /**
   * @notice Emitted when the user associate a voucher to a glass
   * @param glassID ID of the glasses
   * @param voucherType type of Voucher equipped
   */
  event VoucherAssociated(
    uint256 glassID,
    VoucherType voucherType
  );

  /**
   * @notice Emitted when the user dissociate voucher from the glass
   * @param glassID ID of the glasses
   * @param voucherType type of Voucher unequipped
   */
  event VoucherDissociated(
    uint256 glassID,
    VoucherType voucherType
  );

  /**
   * @notice Emitted when the treasury updates
   * @param oldTreasury Address of old treasury
   * @param newTreasury Address of new treasury
   */
  event TreasuryUpdated(
    address oldTreasury,
    address newTreasury
  );

  /**
   * @notice Emitted when the percentage updates
   * @param oldPercentage Old Percentage
   * @param newPercentage New Percentage
   */
  event PercentageUpdated(
    uint256 oldPercentage,
    uint256 newPercentage
  );

  /**
   * @notice Emitted when the Recharge limit updates
   * @param oldLimit Old Limit
   * @param newLimit New Limit
   */
  event LimitUpdated(uint8 oldLimit, uint8 newLimit);

  /**
   * @notice Emitted when poolReward is claimed
   * @param user will receive reward
   * @param amount amount of spay to reward
   * @param nonce nonce
   * @param poolId id of reward pool
   * @param gemId id of gem receiving reward
   */
  event EarnPoolReward(
    address indexed user,
    uint256 indexed amount,
    uint256 nonce,
    uint256 poolId,
    uint256 gemId
  );

  /**
   * @notice Emitted when price is updated
   * @param to address of the receiver
   * @param voucherType type of voucher updated
   * @param nonce nonce
   */
  event VoucherMint(
    address indexed to,
    VoucherType indexed voucherType,
    uint256 nonce
  );

  /**
   * @notice Constructor
   * @param _spay SPAY contract address
   * @param _scriptGlasses glasses contract address
   */
  constructor(IScriptPay _spay, IScriptGlasses _scriptGlasses, IScriptVoucher _scriptVoucher) {
    spay = _spay;
    scriptGlasses = _scriptGlasses;
    scriptVoucher = _scriptVoucher;

    treasury = msg.sender;
    treasuryPercentage = 7 ether;
    rechargeLimitPerDay = 3;

    // maxSupply mintedSupply glassCost
    // data of common glasses
    glasses[0] = Glass(250_000, 0, 10e18);
    // data of rare glasses
    glasses[1] = Glass(125_000, 0, 20e18);
    // data of superscript glasses
    glasses[2] = Glass(25_000, 0, 30e18);
    // data of free glasses
    glasses[3] = Glass(100_000, 0, 0);

    gemPrices[1] = 10 ether;
    gemPrices[2] = 20 ether;
    gemPrices[3] = 30 ether;
  }

  /**
   * @notice Mints a pair of Free glasses
   */
  function mintFreemiumGlasses(
    address _to,
    uint256 _nonce,
    bytes calldata _sig
  ) external {
    Glass memory glassType = glasses[uint256(ScriptNFTType.Free)];

    if (freemiumOwner[_to]) {
      revert AlreadyClaimed();
    }
    if (glassType.mintedSupply >= glassType.maxSupply) {
      revert InsufficientSupply();
    }
    bytes32 txHash = keccak256(
      abi.encodePacked(
        "\x19Ethereum Signed Message:\n",
        address(this),
        _to,
        _nonce
      )
    );
    address signer = txHash.recover(_sig);

    if (executed[txHash]) {
      revert TransactionAlreadyExecuted();
    }
    if (signer != owner()) {
      revert InvalidSignature();
    }

    glassType.mintedSupply++;
    freemiumOwner[_to] = true;
    executed[txHash] = true;

    glasses[uint256(ScriptNFTType.Free)] = glassType;

    uint256 tokenID = scriptGlasses.safeMint(_to, ScriptNFTType.Free);

    emit FreeMinted(_to, tokenID, _nonce);
  }

  /**
   * @notice Mints a recharge voucher
   */
  function mintRechargeVoucher(
    address _to,
    VoucherType _voucherType,
    uint256 _nonce,
    bytes calldata _sig
  ) external {
    bytes32 txHash = keccak256(
      abi.encodePacked(
        "\x19Ethereum Signed Message:\n",
        address(this),
        _to,
        _voucherType,
        _nonce
      )
    );
    address signer = txHash.recover(_sig);

    if (executed[txHash]) {
      revert TransactionAlreadyExecuted();
    }
    if (signer != owner()) {
      revert InvalidSignature();
    }
    executed[txHash] = true;

    scriptVoucher.mint(_to, _voucherType);

    emit VoucherMint(_to, _voucherType, _nonce);
  }

  /**
   * @notice Mints a pair of common, rare or superscript glasses, burns caller's SPAY
   * @param _type type of glasses to be minted
   */
  function mintGlasses(ScriptNFTType _type) external {
    if (uint256(_type) > 2) {
      revert TypeOutOfRange(_type);
    }
    Glass memory glassType = glasses[uint256(_type)];
    uint256 cost = glassType.glassCost;
    if (glassType.mintedSupply >= glassType.maxSupply) {
      revert InsufficientSupply();
    }
    if (spay.balanceOf(msg.sender) < cost) {
      revert InsufficientFunds();
    }

    spay.burnFrom(msg.sender, cost);
    glassType.mintedSupply++;
    glasses[uint256(_type)] = glassType;

    uint256 tokenID = scriptGlasses.safeMint(msg.sender, _type);

    emit GlassesMinted(msg.sender, tokenID, _type);
  }

  /**
   * @notice Confiscates user's glasses and transfers to Script TV
   * @param _tokenID ID of glasses to be confiscated
   */
  function confiscateGlasses(uint256 _tokenID) external onlyOwner {
    address glassesOwner = scriptGlasses.ownerOf(_tokenID);
    scriptGlasses.transferFrom(glassesOwner, address(this), _tokenID);
    emit GlassesConfiscated(glassesOwner, _tokenID);
  }

  /**
   * @notice Creates signature for verification
   * @param _to address of the receiver
   * @param _amount amount of SPAY to be sent
   * @param _nonce unique integer
   * @param _type type of transaction
   * @return bytes32 transaction signature
   */
  function getTxHash(
    address _to,
    uint256 _amount,
    uint256 _nonce,
    string memory _type
  ) public view returns (bytes32) {
    return
      keccak256(
        abi.encodePacked(
          "\x19Ethereum Signed Message:\n",
          address(this),
          _to,
          _amount,
          _nonce,
          _type
        )
      );
  }

  /**
   * @notice Modifier to check if the signature is created by the owner
   * @param _amount amount of SPAY to be sent
   * @param _nonce unique integer
   * @param _sig transaction signature
   */
  modifier verifySignature(
    uint256 _amount,
    uint256 _nonce,
    bytes memory _sig,
    string memory _type
  ) {
    if (_amount == 0) {
      revert ZeroAmount();
    }

    bytes32 txHash = getTxHash(msg.sender, _amount, _nonce, _type);
    address signer = txHash.recover(_sig);
    bool valid = signer == owner();

    if (executed[txHash]) {
      revert TransactionAlreadyExecuted();
    }

    if (!valid) {
      revert InvalidSignature();
    }

    executed[txHash] = true;

    _;
  }

  /**
   * @notice Rewards SPAY earned from watching content
   * @param _amount amount of spay to be rewarded
   * @param _nonce unique nonce of the transaction
   * @param _sig signature signed by the owner
   * @param _type type of transaction
   * @dev Signature replay prevention
   */
  function earningPayout(
    uint256 _amount,
    uint256 _nonce,
    bytes memory _sig,
    string calldata _type
  ) external verifySignature(_amount, _nonce, _sig, _type) {
    uint256 treasuryCut = uint256((_amount * treasuryPercentage) / 100 ether);
    uint256 remainingAmount = _amount - treasuryCut;

    spay.mint(msg.sender, remainingAmount);
    spay.mint(treasury, treasuryCut);

    emit PayoutEarned(
      msg.sender,
      _amount,
      _nonce
    );
  }

  /**
   * @notice Burns SPAY for recharging the glass
   * @param _amount amount of SPAY to be burnt
   * @param _nonce unique nonce of the transaction
   * @param _sig signature signed by the owner
   * @param _glassId glasses being recharged
   */
  function rechargeGlasses(
    uint256 _amount,
    uint256 _nonce,
    bytes calldata _sig,
    uint256 _glassId
  ) external {
    // new scope to verify signature
    {
      bytes32 txHash = keccak256(
        abi.encodePacked(
          "\x19Ethereum Signed Message:\n",
          address(this),
          _glassId,
          _amount,
          _nonce
        )
      );
      address signer = txHash.recover(_sig);
      if (executed[txHash]) {
        revert TransactionAlreadyExecuted();
      }
      if (signer != owner()) {
        revert InvalidSignature();
      }
      executed[txHash] = true;
    }
    uint256 _days = (block.timestamp - 1669334400) / SECONDS_PER_DAY;
    if (rechargeTimes[_glassId][_days] >= 3) {
      revert DailyRechargeLimitExceeds();
    }
    ++rechargeTimes[_glassId][_days];
    uint256 discountPercentage = 0;
    if(associatedVouchers[msg.sender][_glassId]) {
      uint8 voucherType = uint8(scriptGlasses.glassType(_glassId));
      discountPercentage = scriptVoucher.getDiscountPercentage(voucherType);
    }
    //                                            discount calculation
    uint256 discountedAmount = _amount - uint256((_amount * discountPercentage) / 100 ether);
    spay.burnFrom(msg.sender, discountedAmount);
    emit GlassesRecharged(msg.sender, _glassId, discountedAmount, _nonce);
  }

  /**
   * @notice change the address of treasury
   * @param _newTreasury address of new treasury
   */
  function setTreasury(address _newTreasury) external onlyOwner {
    address oldTreasury = treasury;

    if (_newTreasury == address(0)) {
      revert ZeroAddress();
    }
    if (_newTreasury == oldTreasury) {
      revert SameAsOld();
    }

    treasury = _newTreasury;

    emit TreasuryUpdated(oldTreasury, _newTreasury);
  }

  /**
   * @notice change the percentage of treasury cut
   * @param _newPercentage new percentage for treasury cut
   */
  function setTreasuryPercentage(uint256 _newPercentage) external onlyOwner {
    uint256 oldPercentage = treasuryPercentage;

    if (_newPercentage > 30 ether) {
      revert PercentageOutOfRange();
    }
    if (_newPercentage == oldPercentage) {
      revert SameAsOld();
    }

    treasuryPercentage = _newPercentage;

    emit PercentageUpdated(oldPercentage, _newPercentage);
  }

  /**
   * @notice change glasses recharge limit per day
   * @param _newLimit new recharge limit per day
   */
  function setRechargeLimitPerDay(uint8 _newLimit) external onlyOwner {
    uint8 oldLimit = rechargeLimitPerDay;

    if (_newLimit < 1) {
      revert LimitIsInvalid();
    }
    if (_newLimit == oldLimit) {
      revert SameAsOld();
    }

    rechargeLimitPerDay = _newLimit;

    emit LimitUpdated(oldLimit, _newLimit);
  }

  function getRechargeCount(uint256 _glassId) external view returns (uint8) {
    uint256 _days = (block.timestamp - 1669334400) / SECONDS_PER_DAY;
    return rechargeTimes[_glassId][_days];
  }

  /**
   * @notice Equip specified gem into the specified glasses and burns SPAY
   * @param _glassId id of glasses to equip the gem into
   * @param _gemType type of gem to be equipped
   * @param _nonce unique nonce of the transaction
   * @param _sig signature signed by the owner
   */
  function equipGem(
    uint256 _glassId,
    uint8 _gemType,
    uint256 _nonce,
    bytes calldata _sig
  ) external {
    address glassOwner = scriptGlasses.ownerOf(_glassId);
    if (glassOwner != msg.sender) {
      revert AssetNotOwned();
    }

    bytes32 txHash = keccak256(
      abi.encodePacked(
        "\x19Ethereum Signed Message:\n",
        address(this),
        _glassId,
        _gemType,
        _nonce
      )
    );
    address signer = txHash.recover(_sig);

    if (executed[txHash]) {
      revert TransactionAlreadyExecuted();
    }
    if (signer != owner()) {
      revert InvalidSignature();
    }

    uint256 gemCost = gemPrices[_gemType];

    executed[txHash] = true;

    spay.burnFrom(msg.sender, gemCost);
    emit GemEquipped(_glassId, _gemType);
  }

  /**
   * @notice Rewards SPAY earned from watching content
   * @param _amount amount of spay to be rewarded
   * @param _nonce unique nonce of the transaction
   * @param _sig signature signed by the owner
   * @param _poolId signature signed by the owner
   * @dev Signature replay prevention
   */
  function earningPoolReward(
    uint256 _amount,
    uint256 _nonce,
    bytes memory _sig, // memory instead of calldata to avoid stack too deep
    uint256 _poolId,
    uint256 _gemId
  ) external verifySignature(_amount, _nonce, _sig, "POOL_REWARD") {
    if (poolRewardClaimed[_gemId][_poolId]) {
      revert PoolRewardAlreadyClaimed();
    }
    poolRewardClaimed[_gemId][_poolId] = true;
    spay.mint(msg.sender, _amount);

    emit EarnPoolReward(msg.sender, _amount, _nonce, _poolId, _gemId);
  }

  /**
   * @notice update the supply of given voucher type
   * @param voucherType  type of voucher(1,2,3)
   * @param newSupply  updated supply
   */
  function updateVoucherSupply(VoucherType voucherType, uint256 newSupply) external onlyOwner {
    scriptVoucher.updateSupply(voucherType, newSupply);
  }

  /**
   * @notice update the price of given token type
   * @param voucherType  type of voucher
   * @param newPrice  updated supply
   */
  function updateVoucherMintPrice(VoucherType voucherType, uint256 newPrice) external onlyOwner {
    scriptVoucher.updateMintPrice(voucherType, newPrice);
  }

  /**
   * @notice associate the discount voucher with glasses
   * @param _voucherType  type of voucher
   * @param _glassId  glass ID
   */
  function associateVoucher(VoucherType _voucherType, uint256 _glassId) external {
    address glassOwner = scriptGlasses.ownerOf(_glassId);
    uint256 balance = scriptVoucher.balanceOf(msg.sender, uint256(_voucherType));
    uint8 glassType = uint8(scriptGlasses.glassType(_glassId));

    if (glassOwner != msg.sender || balance < 1) {
        revert AssetNotOwned();
    }
    if (glassType != uint8(_voucherType)) {
        revert VoucherNotCompatible();
    }
    if (associatedVouchers[msg.sender][_glassId]) {
        revert VoucherAlreadyEquipped();
    }
    associatedVouchers[msg.sender][_glassId] = true;
    // transfer the voucher to the contract and keep track of it
    scriptVoucher.safeTransferFrom(msg.sender, address(this), uint256(_voucherType), 1, "");

    emit VoucherAssociated(_glassId, _voucherType);
  }

  /**
   * @notice dissociate the discount voucher from glasses
   * @param _glassId  glass ID
   */
  function releaseVoucher(uint256 _glassId) external {
    // check that the caller is the owner of the tokens
    if(!associatedVouchers[msg.sender][_glassId]){
      revert AssetNotOwned();
    }
    uint8 voucherType = uint8(scriptGlasses.glassType(_glassId));
    // update the associated vouchers mapping
    associatedVouchers[msg.sender][_glassId] = false;
    // transfer the tokens back to the caller
    scriptVoucher.safeTransferFrom(address(this), msg.sender, voucherType, 1, "");

    emit VoucherDissociated(_glassId, VoucherType(voucherType));
  }
}

