abstract contract Context {
    function _msgSender() internal view virtual returns (address) {
        return msg.sender;
    }

    function _msgData() internal view virtual returns (bytes calldata) {
        return msg.data;
    }
}

abstract contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(
        address indexed previousOwner,
        address indexed newOwner
    );

    constructor() {
        _transferOwnership(_msgSender());
    }

    modifier onlyOwner() {
        _checkOwner();
        _;
    }

    function owner() public view virtual returns (address) {
        return _owner;
    }

    function _checkOwner() internal view virtual {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
    }

    function renounceOwnership() public virtual onlyOwner {
        _transferOwnership(address(0));
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(
            newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        _transferOwnership(newOwner);
    }

    function _transferOwnership(address newOwner) internal virtual {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }
}

interface Registrar {
    function query(bytes32 label, address owner) external view returns (uint256);
    function register(bytes32 label, address owner, uint256 duration) external returns (uint256);
    function rentPrice(bytes32 label, uint256 duration) external view returns (uint256);
    function available(bytes32 label) external view returns (bool);
    function available(bytes32 label, uint256 duration) external view returns (bool);
}

contract NeoTokyoENS {
    Registrar public registrar;

    constructor(address _registrarAddress) {
        registrar = Registrar(_registrarAddress);
    }

    function registerSubdomain(string memory _subdomainName, string memory _parentName, address _owner) public payable {
        bytes32 parentNamehash = keccak256(abi.encodePacked(bytes(_parentName)));
        bytes32 label = keccak256(bytes(_subdomainName));
        require(registrar.available(keccak256(abi.encodePacked(_subdomainName))), "Subdomain is already taken");
        uint256 registrationFee = registrar.rentPrice(keccak256(abi.encodePacked(_subdomainName)), 1);
        require(msg.value >= registrationFee, "Insufficient funds to register subdomain");
        registrar.register(label, _owner, registrationFee);
        if (msg.value > registrationFee) {
            payable(msg.sender).transfer(msg.value - registrationFee);
        }
    }
    function rentPrices(string memory _subdomainName) public view returns (uint256) {
        return  registrar.rentPrice(keccak256(abi.encodePacked(_subdomainName)), 1);
    }

    function isAvailable(string memory _subdomainName) public view returns (bool) {
        bool result = registrar.available(keccak256(abi.encodePacked(_subdomainName))); 
        return result;
    }
}