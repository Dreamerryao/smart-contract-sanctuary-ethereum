pragma solidity ^0.8.0;

import "forge-std/Script.sol";
import "../../src/validators/ChainlinkValidator.sol";
import "../../src/interfaces/AggregatorV3Interface.sol";

contract MockFeed is AggregatorV3Interface{
    mapping (uint256 => int256) public roundData;
    mapping (uint256 => uint256) public timestamps;

    uint80 public latestRoundId;

    function setRoundData(uint256 roundId_,uint256 timestamp_, int256 data_) external {
        roundData[roundId_] = data_;
        timestamps[roundId_] = timestamp_;
        if(uint256(latestRoundId) < roundId_){
            latestRoundId = uint80(roundId_);
        }
    }

    function decimals() external pure override(AggregatorV3Interface) returns (uint8){
        return uint8(9);
    }

    function description() external pure override(AggregatorV3Interface) returns (string memory){
        return "Mock Chainlink Feed";
    }

    function version() external pure override(AggregatorV3Interface) returns (uint256){
        return 1;
    }

    function getRoundData(uint80 roundId_)
        external
        view
        override(AggregatorV3Interface)
        returns (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        ){
        roundId = roundId_;
        answer = roundData[uint256(roundId_)];
        startedAt = uint256(roundId_);
        updatedAt = uint256(roundId_);
        answeredInRound = roundId_;
    }

    function latestRoundData()
        external
        view
        override(AggregatorV3Interface)
        returns (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        ){
        roundId = latestRoundId;
        answer = roundData[uint256(latestRoundId)];
        startedAt = uint256(latestRoundId);
        updatedAt = uint256(latestRoundId);
        answeredInRound = latestRoundId;
    }

    function getTimestamp(uint256 _roundId) external view returns (uint256){
        return timestamps[_roundId];
    }


}

contract MockChainlinkDataFeed is Script {
    function run() external {
        vm.startBroadcast();

        deploy();
        
        vm.stopBroadcast();
    }

    function deploy() public returns(MockFeed){
        return new MockFeed();
    }
}

