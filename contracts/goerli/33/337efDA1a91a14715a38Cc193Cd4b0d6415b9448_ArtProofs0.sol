    string public constant description = "Reconstruct ArtProofs0 by passing each Proof's `seed` and `params` as top level arguments to the `algorithm` (a javascript file with one dependency, the p5js library).";

    uint32[10] public initialParams = [2**31, 2**31, 2**31, 2**31, 2**31, 2**31, 2**31, 2**31, 2**31, 2**31];
    string[10] public paramNames = [
        "Color Variance",
        "Color Mean",
        "Wedge Amount Variance",
        "Wedge Amount Mean",
        "Layer Density Variance",
        "Layer Density Mean",
        "Layer Exists Variance",
        "Layer Exists Mean",
        "Layer Border Variance",
        "Layer Border Mean"
    ];
    uint256 public constant supplyCap = 1024;

    mapping(uint256 => Proof) public proofById;

    constructor() ERC721("ArtProofs0", "PROOF0") {
        // The ArtProofs0 Origin Proof, unownable
        proofById[0] = Proof({
            params: initialParams,
            seed: bytes32(0),
            mintBlock: block.number,
            mintPrice: 0
        });
    }

    function mint(uint8 paramIndex_, uint32 paramValue_) external payable nonReentrant() {
        require(paramIndex_ < initialParams.length, "overflow param index");
        uint256 lastTokenId = totalSupply();
        require(lastTokenId < supplyCap, "cannot mint above supplyCap");

        Proof memory lastProof = proofById[lastTokenId];
        uint256 mintBlock = lastProof.mintBlock;
        require(block.number > mintBlock, "cannot mint twice in same block");

        uint256 price = getMintPrice(block.number, mintBlock, lastProof.mintPrice);
        require(msg.value >= price, "not enough eth to mint");

        uint32 paramToChange = lastProof.params[paramIndex_];
        require(paramToChange != paramValue_, "param must change");

        uint32[10] memory params = lastProof.params;
        params[paramIndex_] = paramValue_;

        // @note: value is potentially manipulable (by miners), in this case security not needed
        bytes32 pseudorandomish = _getPseudorandomish(price, paramValue_);

        proofById[lastTokenId+1] = Proof({
            params: params,
            seed: computeSeed(lastProof.seed, pseudorandomish),
            mintBlock: block.number,
            mintPrice: price
        });

        _safeMint(msg.sender, lastTokenId+1);
        if (msg.value > price) {
            (bool success, ) = msg.sender.call{value: msg.value-price}("");
            require(success, "failed to refund eth");
        }
    }

    function getMintPrice (
        uint256 currentBlock_,
        uint256 mintBlock_,
        uint256 mintPrice_
    ) public pure returns (uint256) {
        uint256 adjustedMintPrice = mintPrice_ + 100 ether;
        uint256 timeSubsidy = (currentBlock_ - mintBlock_) * 0.02 ether;
        return adjustedMintPrice > timeSubsidy ? adjustedMintPrice - timeSubsidy : 0;
    }

    function computeSeed(
        bytes32 priorSeed_,
        bytes32 pseudorandom_
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(priorSeed_, pseudorandom_));
    }

    function withdraw() external onlyOwner() {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "withdraw failed");
    }

    function paramsById(uint256 tokenId_) external view returns (uint32[10] memory) {
        return proofById[tokenId_].params;
    }

    function seedById(uint256 tokenId_) external view returns (bytes32) {
        return proofById[tokenId_].seed;
    }

    function _getPseudorandomish(uint256 price_, uint32 paramValue_) internal view returns (bytes32) {
        return keccak256(abi.encodePacked(msg.sender, price_, paramValue_, block.number, block.timestamp, blockhash(block.number-1)));
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://api.artproofs.io/0/";
    }

    receive() external payable {}
}