        Asset memory asset_ = Asset(divisibleAsset, _price, block.timestamp);
        _divisibleAssets.push(asset_); 
        _divisibleAssetsMap[address(asset_.token)] = asset_;

        // create Lending & Borrowing Contract

    }

    function allAssets()
        public
        view
        returns (Asset[] memory coll)
    {
