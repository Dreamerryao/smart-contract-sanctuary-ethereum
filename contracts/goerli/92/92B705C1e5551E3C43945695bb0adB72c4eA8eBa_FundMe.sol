pragma solidity ^0.8.8;

import "./PriceConverter.sol";
import "hardhat/console.sol";

//Gas Optimization
//733,793 - initial gas
//713847 - after changing one variable to constant
// 690280 after changing owner to immutable too

//using immutbale/constant variables means they are stored in the bytecode, rather than in storage slots.

//constant and immutable can save gas

contract FundMe {
    using PriceConverter for uint256;
    uint256 public constant minimumUSD = 1 * 1e18; //making this constant saves a bit of gas as well when reading the value. send 0.0005

    address[] public funders;
    mapping(address => uint256) public addressToAmountFunded;

    uint256 public result;

    address public immutable owner; //immutable is like constant but you do it if it is set on a different line from where it is declared. naming convention is i_variable

    constructor() {
        //function gets called whenever you deploy a contract
        owner = msg.sender; // sets the owner as the person who deployed the contract.
    }

    function fund() public payable {
        uint256 USDValue = msg.value.getConversionRate();
        require(USDValue >= minimumUSD, "Didn't send enough"); //revert undoes anything that happened before and send remaining gas back
        funders.push(msg.sender);
        addressToAmountFunded[msg.sender] = msg.value;
    }

    function getAddressToAmountFunded(
        address fundingAddress
    ) public view returns (uint256) {
        return addressToAmountFunded[fundingAddress];
    }

    function getFunder(uint256 index) public view returns (address) {
        return funders[index];
    }

    function withdraw() public onlyOwner {
        for (
            uint256 funderIndex = 0;
            funderIndex < funders.length;
            funderIndex++
        ) {
            //starting index, ending index, step amount.
            address funder = funders[funderIndex];
            addressToAmountFunded[funder] = 0;
        }

        funders = new address[](0); // makes a new array of addresses with length of zero
        //three ways to send ether: transfer, send and call. transfer is simplest

        //trasnfer and send are capped at 2300 gas. call is not capped
        //transfer throws error. Send wouldn't revert the transaction but it would give a boolean. so you can do "bool sendSuccess = payable" ... and require(sendSuccess, "Send failed")
        //call is one of the first lower level commands, increditbly powerful. can use it to call any function
        //(bool callSuccess, bytes memory dataReturned) payable(msg.sender).call{value: address(this).balance}("") - this function returns both a bool and bytes, so that's why we have the brackets in front. can just do (bool callSuccess, )
        //call is recommended

        console.log("Withdrawing %s to %s", address(this).balance, msg.sender);
        payable(msg.sender).transfer(address(this).balance); // msg.sender is of type address. payable(msg.sender) is of type payable.
    }

    function cheaperWithdraw() public onlyOwner {
        address[] memory m_funders = funders; //reading the array into storage once to save on gas. Mappings cant be in memory btw

        for (
            uint256 funderIndex = 0;
            funderIndex < m_funders.length;
            funderIndex++
        ) {
            //starting index, ending index, step amount.
            address funder = m_funders[funderIndex];
            addressToAmountFunded[funder] = 0;
        }

        funders = new address[](0); // makes a new array of addresses with length of zero
        //three ways to send ether: transfer, send and call. transfer is simplest

        //trasnfer and send are capped at 2300 gas. call is not capped
        //transfer throws error. Send wouldn't revert the transaction but it would give a boolean. so you can do "bool sendSuccess = payable" ... and require(sendSuccess, "Send failed")
        //call is one of the first lower level commands, increditbly powerful. can use it to call any function
        //(bool callSuccess, bytes memory dataReturned) payable(msg.sender).call{value: address(this).balance}("") - this function returns both a bool and bytes, so that's why we have the brackets in front. can just do (bool callSuccess, )
        //call is recommended

        console.log("Withdrawing %s to %s", address(this).balance, msg.sender);
        payable(msg.sender).transfer(address(this).balance); // msg.sender is of type address. payable(msg.sender) is of type payable.
    }

    receive() external payable {
        fund(); //this is how you call a function btw
    }

    fallback() external payable {
        result = 2;
        fund();
    }

    modifier onlyOwner() {
        //condition to running the contract
        require(msg.sender == owner, "Sender is not owner!");
        _; // this part means it is running the rest of the function
    }
}

// you can replace requires with errors by saying "error notOwner". then you can replace require statement with if(msg.sender != owner) {revert notOwner();} - custom errors are new in solidity

//receive and fallback

//receive is triggered whenever there is a blank calldata, even if eth isn't sent.
//whenever callback data is sent, solidity looks for fallback instead of receive.
//fallback works even when data is sent along with the transaction.
//receive takes prioruity when there is no calldata

