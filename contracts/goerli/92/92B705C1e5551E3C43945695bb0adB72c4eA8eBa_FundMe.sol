pragma solidity ^0.8.8;

//import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol"; // imports the interface from Chainlink NPM. An interface allows you to

library PriceConverter {
    //a library can't send ether or update state variables. otherwise it acts similarly to a contract. all functions must be internal

    function getPrice() internal pure returns (uint256) {
        uint256 price = 2000;
        return uint256(price * 1e18);
    }

    function getConversionRate(
        uint256 _ethAmount
    ) internal pure returns (uint256) {
        return uint256((_ethAmount * getPrice()) / 1e18);
    }
}

