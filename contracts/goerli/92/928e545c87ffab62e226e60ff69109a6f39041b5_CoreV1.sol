pragma solidity ^0.8.0;
/// @title CoreV1
contract CoreV1 is Ownable, ReentrancyGuard, IInitialization, Multicall {
    using AssetInfoLibrary for AssetInfo;
    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    event LogAccrue(uint128 accruedAmount);
    event LogAddCollateral(address indexed from, address indexed to, uint256 amount);
    event LogRemoveCollateral(address indexed from, address indexed to, uint256 amount);
    event LogBorrow(address indexed from, address indexed to, uint256 amount, uint256 share);
    event LogRepay(address indexed from, address indexed to, uint256 amount, uint256 share);
    event LogFeeTo(address indexed newFeeTo);
    event LogWithdrawFees(address indexed feeTo, uint256 feesEarnedFraction);
    event MaxBorrow(uint256 amount);
    event MaxTotalBorrow(uint256 amount);

    // Immutables (for MasterContract and all clones)
    CoreV1 public immutable masterContract;
    IERC20 public immutable clink;

    // Settings
    uint256 public COLLATERIZATION_RATE;
    uint256 public constant COLLATERIZATION_RATE_PRECISION = 1e5; // Must be less than EXCHANGE_RATE_PRECISION (due to optimization in math)

    uint256 public constant EXCHANGE_RATE_PRECISION = 1e18;

    uint256 public LIQUIDATION_MULTIPLIER;
    uint256 public constant LIQUIDATION_MULTIPLIER_PRECISION = 1e5;

    uint256 public BORROW_OPENING_FEE;
    uint256 public constant BORROW_OPENING_FEE_PRECISION = 1e5;

    uint256 public constant DISTRIBUTION_PART = 10;
    uint256 public constant DISTRIBUTION_PRECISION = 100;

    // MasterContract variables
    address public feeTo;

    // Per clone variables
    // Clone init settings
    IERC20 public collateral;
    AggregatorV3Interface public aggregator;

    // Total amounts
    uint256 public totalCollateralAmount; // Total collateral supplied
    AssetInfo public totalBorrow; // amount = Total token amount to be repayed by borrowers, share = Total share of the debt held by borrowers

    // User balances
    mapping(address => uint256) public userCollateralAmount;
    mapping(address => uint256) public userBorrowPart;

    uint256 public maxBorrow;
    uint256 public maxTotalBorrow;

    struct AccrueInfo {
        uint64 lastAccrued;
        uint128 feesEarned;
        uint64 INTEREST_PER_SECOND;
    }

    AccrueInfo public accrueInfo;
    EnumerableSet.AddressSet private userAddr;

    /// @notice The constructor is only used for the initial master contract. Subsequent clones are initialised via `init`.
    constructor(IERC20 clink_) {
        clink = clink_;
        masterContract = this;
    }

    modifier onlyMasterContractOwner() {
        require(msg.sender == masterContract.owner(), "Caller is not the owner");
        _;
    }

    /// @notice Serves as the constructor for clones, as clones can't have a regular constructor
    /// @dev `data` is abi encoded in the format: (IERC20 collateral, IERC20 asset)
    function init(bytes calldata data) public payable override {
        require(address(collateral) == address(0), "Core: already initialized");
        (collateral, aggregator, accrueInfo.INTEREST_PER_SECOND, LIQUIDATION_MULTIPLIER, COLLATERIZATION_RATE, BORROW_OPENING_FEE) = abi.decode(
            data,
            (IERC20, AggregatorV3Interface, uint64, uint256, uint256, uint256)
        );
        require(address(collateral) != address(0), "Core: bad pair");
    }

    /// @notice Accrues the interest on the borrowed tokens and handles the accumulation of fees.
    function accrue() public {
        AccrueInfo memory _accrueInfo = accrueInfo;
        // Number of seconds since accrue was called
        uint256 elapsedTime = block.timestamp - _accrueInfo.lastAccrued;
        if (elapsedTime == 0) {
            return;
        }
        _accrueInfo.lastAccrued = uint64(block.timestamp);

        AssetInfo memory _totalBorrow = totalBorrow;
        if (_totalBorrow.share == 0) {
            accrueInfo = _accrueInfo;
            return;
        }

        // Accrue interest
        uint128 extraAmount = ((uint256(_totalBorrow.amount) * _accrueInfo.INTEREST_PER_SECOND * elapsedTime) / 1e18).toUint128();
        _totalBorrow.amount += extraAmount;

        _accrueInfo.feesEarned += extraAmount;
        totalBorrow = _totalBorrow;
        accrueInfo = _accrueInfo;

        emit LogAccrue(extraAmount);
    }

    function isSolvent(address user) public view returns (bool) {
        // accrue must have already been called!
        uint256 borrowPart = userBorrowPart[user];
        if (borrowPart == 0) return true;
        uint256 collateralAmount = userCollateralAmount[user];
        if (collateralAmount == 0) return false;
        uint256 borrowAmount = totalBorrow.toAmount(borrowPart, true);
        return getLiquidationLimit(user) >= borrowAmount;
    }

    /// @dev Checks if the user is solvent in the closed liquidation case at the end of the function body.
    modifier solvent() {
        _;
        require(isSolvent(msg.sender), "Core: user insolvent");
    }

    /// @notice Adds `collateral` from msg.sender to the account `to`.
    /// @param to The receiver of the tokens.
    /// @param skim True if the amount should be skimmed from the deposit balance of msg.sender.x
    /// False if tokens from msg.sender should be transferred.
    /// @param amount The amount of shares to add for `to`.
    function addCollateral(
        address to,
        bool skim,
        uint256 amount
    ) public nonReentrant {
        userCollateralAmount[to] += amount;
        uint256 oldTotalCollateralAmount = totalCollateralAmount;
        totalCollateralAmount = oldTotalCollateralAmount + amount;
        if (skim) {
            require(amount <= collateral.balanceOf(address(this)) - oldTotalCollateralAmount, "Core: Skim too much");
        } else {
            collateral.safeTransferFrom(msg.sender, address(this), amount);
        }
        emit LogAddCollateral(skim ? address(this) : msg.sender, to, amount);
    }

    /// @notice Removes `share` amount of collateral and transfers it to `to`.
    /// @param to The receiver of the shares.
    /// @param amount Amount of shares to remove.
    function removeCollateral(address to, uint256 amount) public solvent nonReentrant {
        // accrue must be called because we check solvency
        accrue();
        if (amount > userCollateralAmount[msg.sender]) {
            amount = userCollateralAmount[msg.sender];
        }
        require(amount > 0, "rm param err");
        userCollateralAmount[msg.sender] -= amount;
        totalCollateralAmount -= amount;
        collateral.safeTransfer(to, amount);
        emit LogRemoveCollateral(msg.sender, to, amount);
    }

    /// @notice Sender borrows `amount` and transfers it to `to`.
    function borrow(address to, uint256 amount) public solvent nonReentrant {
        accrue();
        uint256 feeAmount = (amount * BORROW_OPENING_FEE) / BORROW_OPENING_FEE_PRECISION;
        // A flat % fee is charged for any borrow
        uint256 share;
        (totalBorrow, share) = totalBorrow.add(amount + feeAmount, true);
        accrueInfo.feesEarned += uint128(feeAmount);
        require(share > 0, "share err");
        if (userBorrowPart[msg.sender] == 0) {
            userAddr.add(msg.sender);
        }
        userBorrowPart[msg.sender] += share;
        // As long as there are tokens on this contract you can 'mint'... this enables limiting borrows
        IERC20BurnMintable(address(clink)).mint(to, amount);
        if (maxBorrow > 0 && totalBorrow.share > 0) {
            uint256 userAmt = totalBorrow.toAmount(userBorrowPart[msg.sender], true);
            require(userAmt <= maxBorrow, "maxBorrow limit");
        }
        if (maxTotalBorrow > 0) {
            require(totalBorrow.amount <= maxTotalBorrow, "maxTotalBorrow limit");
        }
        emit LogBorrow(msg.sender, to, amount + feeAmount, share);
    }

    /// @notice Repays a loan.
    /// @param to Address of the user this payment should go.
    /// @param skim True if the amount should be skimmed from the deposit balance of msg.sender.
    /// False if tokens from msg.sender in `tokenVault` should be transferred.
    /// @param amount The amount to repay. See `userBorrowPart`.
    /// @return share The total share repayed.
    function repay(
        address to,
        bool skim,
        uint256 amount
    ) public nonReentrant returns (uint256 share) {
        accrue();
        share = totalBorrow.toShare(amount, false);
        if (share > userBorrowPart[to]) {
            share = userBorrowPart[to];
        }
        (totalBorrow, amount) = totalBorrow.sub(share, true);
        require(amount > 0, "rp param err");
        userBorrowPart[to] -= share;
        if (userBorrowPart[to] == 0) {
            userAddr.remove(to);
        }
        if (skim) {
            IERC20BurnMintable(address(clink)).burn(amount);
        } else {
            IERC20BurnMintable(address(clink)).burnFrom(msg.sender, amount);
        }
        emit LogRepay(skim ? address(this) : msg.sender, to, amount, share);
    }

    /// @notice Handles the liquidation of users' balances, once the users' amount of collateral is too low.
    /// @param users An array of user addresses.
    /// @param maxBorrowParts A one-to-one mapping to `users`, contains maximum (partial) borrow amounts (to liquidate) of the respective user.
    /// @param to Address of the receiver in open liquidations if `swapper` is zero.
    function liquidate(
        address[] calldata users,
        uint256[] calldata maxBorrowParts,
        address to,
        ISwapper swapper
    ) public nonReentrant {
        // Oracle can fail but we still need to allow liquidations
        accrue();

        uint256 allCollateralAmount;
        uint256 allBorrowAmount;
        uint256 allBorrowPart;
        AssetInfo memory _totalBorrow = totalBorrow;
        for (uint256 i = 0; i < users.length; i++) {
            address user = users[i];
            if (!isSolvent(user)) {
                uint256 borrowPart;
                {
                    uint256 availableBorrowPart = userBorrowPart[user];
                    borrowPart = maxBorrowParts[i] > availableBorrowPart ? availableBorrowPart : maxBorrowParts[i];
                    userBorrowPart[user] = availableBorrowPart - borrowPart;
                    if (userBorrowPart[user] == 0) {
                        userAddr.remove(user);
                    }
                }
                uint256 borrowAmount = _totalBorrow.toAmount(borrowPart, true);

                uint256 collateralAmount = (borrowAmtToCollateral(borrowAmount) * LIQUIDATION_MULTIPLIER) / LIQUIDATION_MULTIPLIER_PRECISION;

                userCollateralAmount[user] -= collateralAmount;
                emit LogRemoveCollateral(user, to, collateralAmount);
                emit LogRepay(msg.sender, user, borrowAmount, borrowPart);

                // Keep totals
                allCollateralAmount += collateralAmount;
                allBorrowAmount += borrowAmount;
                allBorrowPart += borrowPart;
            }
        }
        require(allBorrowAmount != 0, "Core: all are solvent");
        _totalBorrow.amount -= allBorrowAmount.toUint128();
        _totalBorrow.share -= allBorrowPart.toUint128();
        totalBorrow = _totalBorrow;
        totalCollateralAmount -= allCollateralAmount;

        // Swap using a swapper freely chosen by the caller
        // Open (flash) liquidation: get proceeds first and provide the borrow after
        collateral.safeTransfer(to, allCollateralAmount);
        if (swapper != ISwapper(address(0))) {
            swapper.swap(collateral, clink, msg.sender, allBorrowAmount, allCollateralAmount);
        }
        IERC20BurnMintable(address(clink)).burnFrom(msg.sender, allBorrowAmount);
    }

    /// @notice Withdraws the fees accumulated.
    function withdrawFees() public {
        accrue();
        address _feeTo = masterContract.feeTo();
        uint256 _feesEarned = accrueInfo.feesEarned;
        IERC20BurnMintable(address(clink)).mint(_feeTo, _feesEarned);
        accrueInfo.feesEarned = 0;
        emit LogWithdrawFees(_feeTo, _feesEarned);
    }

    function getLiquidationLimit(address user) public view returns (uint256) {
        uint256 value = collateralUsdValue(user);
        return (value * COLLATERIZATION_RATE) / COLLATERIZATION_RATE_PRECISION;
    }

    function borrowAmtToCollateral(uint256 amount) public view returns (uint256) {
        return (amount * EXCHANGE_RATE_PRECISION) / usdPrice();
    }

    function collateralUsdValue(address user) public view returns (uint256) {
        return (usdPrice() * userCollateralAmount[user]) / EXCHANGE_RATE_PRECISION;
    }

    function userDebt(address user) public view returns (uint256) {
        // accrue must have already been called!
        uint256 borrowPart = userBorrowPart[user];
        if (borrowPart == 0) return 0;
        return totalBorrow.toAmount(borrowPart, true);
    }

    function usdPrice() public view returns (uint256) {
        (, int256 answer, , uint256 timestamp, ) = aggregator.latestRoundData();
        require(answer > 0, "invalid_oracle_answer");
        require(timestamp != 0, "round_incomplete");
        uint8 decimals = aggregator.decimals();

        unchecked {
            //converts the answer to have 18 decimals
            return decimals > 18 ? uint256(answer) / 10**(decimals - 18) : uint256(answer) * 10**(18 - decimals);
        }
    }

    /// @notice Sets the beneficiary of interest accrued.
    /// MasterContract Only Admin function.
    /// @param newFeeTo The address of the receiver.
    function setFeeTo(address newFeeTo) public onlyOwner {
        feeTo = newFeeTo;
        emit LogFeeTo(newFeeTo);
    }

    function changeMaxBorrow(uint256 _maxBorrow) public onlyMasterContractOwner {
        maxBorrow = _maxBorrow;
        emit MaxBorrow(maxBorrow);
    }

    function changeMaxTotalBorrow(uint256 _maxTotalBorrow) public onlyMasterContractOwner {
        maxTotalBorrow = _maxTotalBorrow;
        emit MaxTotalBorrow(maxTotalBorrow);
    }
}