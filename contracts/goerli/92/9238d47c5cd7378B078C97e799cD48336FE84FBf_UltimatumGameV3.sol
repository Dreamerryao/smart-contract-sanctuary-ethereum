error UltimatumGameV3__ProposerAndResponderNotDecidedYet();

/// @title Ultimatum Game Version Randome, proposer, responder randomized
/// @notice A famous economic experiments game, see details at https://en.wikipedia.org/wiki/Ultimatum_game
/// @dev This is for learning and practise purpose
/// @custom:state-transfer-diagram
///                   (DEPOSIT)  (DEPOSIT)
///                   {must before propose}
///           DEPLOY    ->   ENROLL    ->           DECIDE PROPOSER RESPONDER                PROPOSE    ->       RESPOND  ->       (WITHDRAW)   ->   (NEXT) ENROLL
///                       DECIDED = false
///                        REQ RANDOM                Reccur, reset RANDOM
///                       {must be owner}             {must be owner}                  {must be proposer}   {must be responder}  {must be owner}    {must be owner}
///                       {address can't 0}                                             {must be decoded}
///                                                                                     {check deposited}
///                                                                                                          {must proposed}
/// s_proposal:   0            -1                                                          [0, 1]               [0, 1]             [0, 1]              -1
/// s_**Decided:  false       false                       true                             true                  true               true               false
contract UltimatumGameV3 is
    UltimatumGame,
    VRFConsumerBaseV2,
    KeeperCompatibleInterface
{
    using SplitUint256 for uint256;

    VRFCoordinatorV2Interface vrfCoordinator;

    bool private s_proposerAndResponderDecided;
    int8 private s_responderChoice; //-1 means unset
    uint256 private s_vrfRequestTime;
    uint256 private s_vrfFulfilledTime;

    uint256 private s_vrfRequestId;
    uint64 private immutable i_vrfSubscriptionId;
    bytes32 private immutable i_vrfGasLaneKeyHash;
    uint16 private immutable i_vrfRequestConfirmations;
    uint32 private immutable i_vrfCallbackGasLimit;

    uint32 constant VRF_NUM_WORDS = 1;
    int8 constant RESPOND_CHOISE_ACCEPT = 0;

    /* solhint-enable var-name-mixedcase */

    event UltimatumGameV3Ended(
        uint256 indexed vrfRequestId,
        address indexed proposer,
        address indexed responder
    );

    constructor(
        uint64 vrfSubscriptionId,
        address vrfCoordinatorAddress,
        bytes32 vrfGasLaneKeyHash,
        uint16 vrfRequestConfirmations,
        uint32 vrfCallbackGasLimit
    ) VRFConsumerBaseV2(vrfCoordinatorAddress) {
        i_vrfSubscriptionId = vrfSubscriptionId;
        i_vrfGasLaneKeyHash = vrfGasLaneKeyHash;
        i_vrfRequestConfirmations = vrfRequestConfirmations;
        i_vrfCallbackGasLimit = vrfCallbackGasLimit;
        vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorAddress);
    }

    function enroll(address payable _user1, address payable _user2)
        public
        override
        mustBeOwner
    {
        UltimatumGame.enroll(_user1, _user2);
        s_proposerAndResponderDecided = false;
        s_vrfRequestId = vrfCoordinator.requestRandomWords(
            i_vrfGasLaneKeyHash,
            i_vrfSubscriptionId,
            i_vrfRequestConfirmations,
            i_vrfCallbackGasLimit,
            VRF_NUM_WORDS
        );
        s_vrfRequestTime = block.timestamp;
        s_responderChoice = -1;
    }

    function fulfillRandomWords(
        uint256, /* requestId */
        uint256[] memory randomWords
    ) internal override {
        s_vrfFulfilledTime = block.timestamp;

        uint256 randomNumber = randomWords[0];
        if (randomNumber % 2 == 1) {
            (s_responder, s_proposer) = (s_proposer, s_responder);
        }

        s_proposerAndResponderDecided = true;
    }

    function propose(uint8 _myshare) public override {
        if (!s_proposerAndResponderDecided) {
            revert UltimatumGameV3__ProposerAndResponderNotDecidedYet();
        }

        UltimatumGame.propose(_myshare);
    }

    function respond(RespondChoice choice) external override {
        if (msg.sender != s_responder) {
            revert UltimatumGame__NotResponder();
        }
        if (s_proposal < 0) {
            revert UltimatumGame__ProposerNeedToProposeBeforeRespond();
        }
        s_responderChoice = choice == RespondChoice.Accept ? int8(0) : int8(1);
    }

    function checkUpkeep(
        bytes calldata /* checkData */
    )
        external
        view
        override
        returns (bool upkeepNeeded, bytes memory performData)
    {
        upkeepNeeded = s_responderChoice >= 0 && s_proposer != address(0);
        if (s_responderChoice == RESPOND_CHOISE_ACCEPT) {
            (uint256 moneyToProposer, uint256 moneyToResponder) = address(this)
                .balance
                .splitByPercent(uint8(s_proposal));
            performData = abi.encode(moneyToProposer, moneyToResponder);
        }
    }

    function performUpkeep(bytes calldata performData) external override {
        if (s_responderChoice != RESPOND_CHOISE_ACCEPT) {
            emit UltimatumGameV3Ended(s_vrfRequestId, s_proposer, s_responder);
            resetPlayers();
            return;
        }

        (uint256 moneyToProposer, uint256 moneyToResponder) = abi.decode(
            performData,
            (uint256, uint256)
        );

        // solhint-disable-next-line avoid-low-level-calls
        (bool callProposerSuccess, ) = s_proposer.call{value: moneyToProposer}(
            ""
        );
        if (!callProposerSuccess) {
            revert UltimatumGame__TransferToProposerFailed();
        }

        // solhint-disable-next-line avoid-low-level-calls
        (bool callResponderSuccess, ) = s_responder.call{
            value: moneyToResponder
        }("");
        if (!callResponderSuccess) {
            revert UltimatumGame__TransferToResponderFailed();
        }

        emit UltimatumGameV3Ended(s_vrfRequestId, s_proposer, s_responder);
        resetPlayers();
    }

    function getProposerAndResponderDecided() external view returns (bool) {
        return s_proposerAndResponderDecided;
    }

    function getResponderChoice() external view returns (int8) {
        return s_responderChoice;
    }

    function getVrfRequestTime() external view returns (uint256) {
        return s_vrfRequestTime;
    }

    function getVrfFulfiledTime() external view returns (uint256) {
        return s_vrfFulfilledTime;
    }

    function getVrfRequestId() external view returns (uint256) {
        return s_vrfRequestId;
    }

    function getVrfSubscriptionId() external view returns (uint64) {
        return i_vrfSubscriptionId;
    }

    function getVrfGasLaneKeyHash() external view returns (bytes32) {
        return i_vrfGasLaneKeyHash;
    }

    function getVrfRequestConfirmations() external view returns (uint16) {
        return i_vrfRequestConfirmations;
    }

    function getVrfCallbackGasLimit() external view returns (uint32) {
        return i_vrfCallbackGasLimit;
    }

    function getVrfNumWords() external pure returns (uint32) {
        return VRF_NUM_WORDS;
    }
}

