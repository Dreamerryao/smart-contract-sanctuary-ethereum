    function initialize(bytes calldata data) external initializer {
        InitializationParams memory params = abi.decode(data, (InitializationParams));

        owner = params.owner;
        approver = params.admin;
        nftfi_loan = params.nftfi_loan;
        x2y2_loan = params.x2y2_loan;
        arcade_loan = params.arcade_loan;

        uint256 length = params.priceFeeds.length;

        // store price feeds
        for (uint256 i; i < length;) {
            prices[params.priceFeeds[i].asset] = params.priceFeeds[i].priceFeed;
            //This is safe to use as its only for itterating on a for loop
            unchecked {
                ++i;
            }
        }

        address collection;
        uint256 duration;

        length = params.ruleParams.length;

        // store rules
        for (uint256 i; i < length;) {
            duration = params.ruleParams[i].duration;
            collection = params.ruleParams[i].collection;

            rules[collection][duration] = params.ruleParams[i].rules;
            //This is safe to use as its only for itterating on a for loop
            unchecked {
                ++i;
            }
        }

        validReceiver[params.nftfi_loan] = true;
        validApprover[params.admin] = true;

        ruleTypes[0] = true;
        ruleTypes[1] = true;
    }

