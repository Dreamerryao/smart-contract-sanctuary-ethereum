    /// @notice returns rule index for a valid loan, reverts if none
    function _getIndex(Loan memory loan) internal view returns (int256 idx) {
        bytes32[] memory rules_ = rules[loan.collection][loan.duration];
        uint256 length = rules_.length;

        uint8 targetInterestRate;
        uint16 min_apr;
        uint16 max_ltv;
        uint16 expectedVol;
        uint96 max_principalETH;
        uint104 max_principalStable;

        int256 i;
        while (uint256(i) < length) {
            bytes memory rule = abi.encodePacked(rules_[uint256(i)]);

            assembly {
                targetInterestRate := mload(add(rule, 1))
                min_apr := mload(add(rule, 3))
                max_ltv := mload(add(rule, 5))
                expectedVol := mload(add(rule, 7))
                max_principalETH := mload(add(rule, 19))
                max_principalStable := mload(add(rule, 32))
            }

            if (loan.denomination == weth) {
                if (loan.principal > max_principalETH) {
                    ++i;
                    continue;
                }
            } else {
                if (loan.principal > max_principalStable) {
                    ++i;
                    continue;
                }
            }

            if (loan.ltv > max_ltv) {
                ++i;
                continue;
            }

            if (loan.apr < min_apr) {
                ++i;
                continue;
            } else {
                return i;
            }
        }

        if (uint256(i) == length) revert("NO_RULES");
    }

    function setBendDaoOracle(address oracle, address nft) external onlyOwner {
        bendDaoNFTOracle[nft] = oracle;
    }

    function setChainlinkOracle(address oracle, address nft) external onlyOwner {
        chainlinkNFTOracle[nft] = oracle;
    }

    function arcadeApprove(address wallet, bool isApproved) public onlyOwner {
        IOriginationController(arcade_loan).approve(wallet, isApproved);
    }

    function getNFTFloor(address nft) internal view returns (uint256) {
        address oracle = bendDaoNFTOracle[nft];
        if (oracle != address(0)) {
            return IBendOracle(oracle).getAssetPrice(nft);
        } else {
            oracle = chainlinkNFTOracle[nft];
            if (oracle != address(0)) {
                (, int256 answer,,,) = IChainPriceFeed(oracle).latestRoundData();
                return uint256(answer);
            }
        }
        revert("NO_ORACLE");
    }

