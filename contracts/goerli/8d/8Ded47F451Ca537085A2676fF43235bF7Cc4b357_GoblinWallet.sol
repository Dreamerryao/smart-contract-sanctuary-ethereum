    /// @notice receives signature for validation and sends to appropriate function for validation
    function isValidSignature(bytes32 hash, bytes memory signature) external view virtual returns (bytes4 valid) {

        if (paused) revert LENDING_PAUSED();

        (address denomination,
        uint256 principal,
        uint256 repaymentWithFee,
        address collection,
        uint32 duration,
        uint16 adminFee_bps, int256 idx, uint8 ruleType) = validateHash(hash, signature);
        uint256 repayment;
        console2.log("DENOMINCATION");
        console2.log(denomination);

        repayment = repaymentWithFee - (repaymentWithFee - principal).mulDivDown(adminFee_bps, 10000);

        if (ruleType == 0) {
            uint256 price = getNFTFloor(collection);

            _validateStaticRule(
                Loan({
                    principal: principal,
                    repayment: repayment,
                    interest: repayment - principal,
                    ltv: 0,
                    apr: 0,
                    denomination: denomination,
                    collection: collection,
                    duration: duration,
                    index: uint256(idx)
                }),
                price
            );
            return this.isValidSignature.selector;
        }
        // We assume everything is prices in weth
        else if (ruleType == 1) {
            uint256 price = getNFTFloor(collection);
            if (denomination == weth) {
                _validateVolEvalRule(
                    VolLoan({
                        principal: principal,
                        repayment: repayment,
                        ltv: 0,
                        collection: collection,
                        duration: duration,
                        price: price
                    })
                );
                return this.isValidSignature.selector;
            }
        } else if (ruleType == 2){
            _validatePrincipalBasedLoan(
                PrincipalLoan({
                    principal: principal,
                    repayment: repayment,
                    interest: repayment - principal,
                    apr: 0,
                    denomination: denomination,
                    collection: collection,
                    duration: duration,
                    index: uint256(idx)
                })
            );
            return this.isValidSignature.selector;
        }
    }

