    function approveAsApprover(address token, uint256 amount, address spender) external nonReentrant {
        if (!validApprover[msg.sender]) revert NOT_APPROVED();
        if (!validReceiver[spender]) revert NOT_APPROVED();
        // Must add check here for the amount we want to send and check it against the amount we have
        ERC20(token).approve(spender, amount);
    }

