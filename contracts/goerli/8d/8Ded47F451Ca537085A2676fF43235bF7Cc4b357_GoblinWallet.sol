    /**
     * @dev secondary function of validate hash this needed broken up because of stack too deep error
     * @param hash The hash to validate
     * @param signature The signature to validate
     * @return encodedoffer The offer data
     * @return idx The index of the rule
     */
    function _validateHash(bytes32 hash, bytes memory signature) internal view returns (bytes memory, int256, uint8, address) { 
        address sender;
        bytes memory signatureData;
        console2.log("INSIDE OF VALIDATE HASH");
        assembly  { 
            // Extract v (uint8) from the first byte
                let r := byte(0, mload(add(signature, 32)))

                // Extract r (bytes32) from the next 32 bytes
                let s := mload(add(signature, 64))

                // Extract s (bytes32) from the next 32 bytes
                let v := mload(add(signature, 65))

                // Calculate the data length based on the remaining bytes
                let dataLength := sub(mload(signature), 65)

                // Extract data (bytes) from the remaining bytes
                signatureData := add(signature, 65)

                // Set the length of the data bytes
                mstore(signatureData, dataLength)
        }
        // console2.logBytes(signatureData);
        (sender, signatureData) = abi.decode(signatureData, (address, bytes));

        if (sender == nftfi_loan) {
         ( bytes memory offer, bytes memory sig_struct, address loan_contract, uint256 block_id, int256 idx, uint8 ruleType) =
                abi.decode(signatureData, (bytes, bytes, address, uint256, int256, uint8));
                if ( hash != keccak256(abi.encodePacked(offer, sig_struct, loan_contract, block_id)).toEthSignedMessageHash()){
                    revert("INVALID_SIGNATURE");
                }
                return (offer, idx, ruleType, sender);
        }

        if (sender == x2y2_loan) {
            (
                bytes memory offer,
                uint256 nftid,
                bytes memory sig_struct,
                address loan_contract,
                uint256 block_id,
                int256 idx,
                uint8 ruletype
            ) = abi.decode(signatureData, (bytes, uint256, bytes, address, uint256, int256, uint8));
            // assert hash == hashed signature
            if (
                hash
                    != keccak256(abi.encodePacked(offer, nftid, sig_struct, loan_contract, block_id)).toEthSignedMessageHash(
                    )
            ) revert INVALID_SIGNATURE();
            return (offer, idx, ruletype, sender);
        }

        if (sender == arcade_loan) {

            bytes memory offerData;
            int256 idx;
            uint8 ruletype;
            (
                offerData,
                idx,
                ruletype
            ) = abi.decode(signatureData, (bytes, int256, uint8));

            bytes32 loanHash = keccak256(offerData);

            // TODO add signedtypedata here to validate against arcade hash once they finalize
            // @MACRO I know this is a secrutiy concern rn but we are waiting on something from another protocol
            // // assert hash == hashed signature
            // TODO: Please adam make this better
             bytes32 _TYPE_HASH = keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)");
            _TYPE_HASH = keccak256(abi.encode(_TYPE_HASH, keccak256(bytes("OriginationController")), keccak256(bytes("2")), block.chainid, address(0xc0209D538888C7779A9C5B43224F2D49EAbF86fd)));

            if (
                hash != ECDSA.toTypedDataHash(_TYPE_HASH, loanHash)
            ) revert INVALID_SIGNATURE();

            return (offerData, idx, ruletype, sender);
        }
    }

