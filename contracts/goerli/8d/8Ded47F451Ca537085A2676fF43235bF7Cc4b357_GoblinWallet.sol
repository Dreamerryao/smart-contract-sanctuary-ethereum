 */
pragma solidity ^0.8.19;

import {GoblinFixedPointMathLib} from "./GoblinFixedPointMathLib.sol";
import "adbk-lib/ABDKMath64x64.sol";

library GoblinLoanEvaluation {
    // All numbers handled in this lib will be to the 1e10 precision

    int128 constant a1 = 4700776267876497372; // 0.254829592

    int128 constant a2 = -5248038478797710846; // -0.284496736

    int128 constant a3 = 26220455503081073509; // 1.421413741

    int128 constant a4 = -26805923542261272340; // -1.453152027

    int128 constant a5 = 19579474307208894254; // 1.061405429

    int128 constant p = 6042989182524993094; // 0.3275911

    int128 constant ONE_64x64 = 0x10000000000000000;
    int128 constant TWO_64x64 = 0x20000000000000000;

    error INVALID_LOAN();

    // We will use ABDK Math for our division
    using ABDKMath64x64 for int128;
    // We will use our own math library for the rest
    using GoblinFixedPointMathLib for int128;
    using GoblinFixedPointMathLib for uint256;

    /**
     * Calculate the net present value (NPV) of a loan.
     * @param price The current asset price ( currently in weth ) -signed 64.64-bit fixed point number
     * @param drift The drift rate -signed 64.64-bit fixed point number: set by the user
     * @param volAnnualized The annualized volatility -signed 64.64-bit fixed point number: set by the user
     * @param durationDays The duration in days denominated in seconds
     * @param principal The principal amount -signed 64.64-bit fixed point number
     * @param repaymentAmt The annual percentage rate -signed 64.64-bit fixed point number
     * @param discountRate The discount rate -signed 64.64-bit fixed point number: set by the user
     *     returns: void however we calculate the NPV of the loan if this value is below 0 we determin that the loan is invalid and we should not take it
     */
    function calculateNpvLoan(
        int128 price,
        int128 drift,
        int128 volAnnualized,
        int128 durationDays,
        int128 principal,
        int128 repaymentAmt,
        int128 discountRate
    ) internal pure {
        assembly {
            if or(lt(durationDays, 0), iszero(durationDays)) { revert(0, 0) }
        }
        //Annaualize duration days
        durationDays = durationDays.div(31557600);

        (int128 mu, int128 sigma) = futureDistribution(price, drift, volAnnualized, durationDays);

        int128 evLoan = expectedValueLoan(repaymentAmt, mu, sigma);

        npv(principal, evLoan, discountRate, durationDays);
    }

    /**
     * Calculate the future distribution of the assets price returning the mu and sigma along this distribution
     * @param price The current asset price ( currently in weth ) -signed 64.64-bit fixed point number
     * @param drift The drift rate -signed 64.64-bit fixed point number: set by the user
     * @param volAnnualized The annualized volatility -signed 64.64-bit fixed point number: set by the user
     * @param durationDays The duration in days denominated in seconds
     * @dev The formula for mu and sigma are as follows:
     *         mu = np.log(price) + (drift - (vol_annualized**2) / 2) * duration_years
     *         sigma = vol_annualized * np.sqrt_gs(duration_years)
     *
     * returns mu - mean of price
     * returns, sigma  - standard deviation for price
     */
    function futureDistribution(int128 price, int128 drift, int128 volAnnualized, int128 durationDays)
        internal
        pure
        returns (int128, int128)
    {
        // Calculate the mu and sigma of the future distribution
        int128 mu = GoblinFixedPointMathLib.lnWad_64x64((price.toWad())).add_adbk(
            drift.sub_adbk(volAnnualized.pow_2().div(TWO_64x64)).mul_adbk(durationDays)
        );
        int128 sigma = volAnnualized.mul_adbk(durationDays.sqrt_gs());
        return (mu, sigma);
    }

    /**
     * Calculate the expected value of the loan repayment for a given log-normal distribution.
     *     @param repaymentAmt: The repayment amount -signed 64.64-bit fixed point number
     *     @param mu: The mu of the log-normal distribution -signed 64.64-bit fixed point number
     *     @param sigma: The sigma of the log-normal distribution -signed 64.64-bit fixed point number
     *     @dev The formula for the expected value of the loan repayment is as follows:
     *          s = sigma
     *          scale = np.exp(mu)
     *          proba_repay = 1 - lognorm.cdf(repayment_amt, s, scale=scale)
     *          payout_default = integral_x_times_lognormal_pdf_x_scipy(repayment_amt, mu, sigma)
     *          expectedValueLoan = payout_default + proba_repay * repayment_amt
     *     returns expected value of the loan repayment for a given log-normal distribution -signed 64.64-bit fixed point number
     */
    function expectedValueLoan(int128 repaymentAmt, int128 mu, int128 sigma) internal pure returns (int128) {
        // Caluulate probablility of repayment
        int128 probaRepay = ONE_64x64.sub_adbk(lognormCdf(repaymentAmt, mu, sigma));

        // Calculate the payout for default
        int128 payoutDefault = integralXTimesLognormalPdfX(repaymentAmt, mu, sigma);

        // Return the expected value of the loan repayment
        return payoutDefault.add_adbk(probaRepay.mul_adbk(repaymentAmt));
    }

    /**
     * Calculates the cumulative distribution function (CDF) of a log-normal distribution for a given x value.
     *     @param x: The input value for which the CDF is calculated
     *     @param mu: The mu of the underlying normal distribution
     *     @param sigma: The CDF value of the log-normal distribution for the given x value
     *     @dev The formula for the CDF of a log-normal distribution is as follows:
     *          z = (math.log(x) - mu) / (math.sqrt_gs(2) * sigma)
     *          return 0.5 * (1 + erf_approx(z))
     *     returns The CDF of a log-normal distribution for the given x value -signed 64.64-bit fixed point number
     */
    function lognormCdf(int128 x, int128 mu, int128 sigma) internal pure returns (int128) {
        //We have to do some bit shifting and decimal changes to get the right precision
        int128 z = (GoblinFixedPointMathLib.lnWad_64x64((x.toWad())).sub_adbk(mu)).div(
            GoblinFixedPointMathLib.sqrt_gs(TWO_64x64).mul_adbk(sigma)
        );
        return ABDKMath64x64.div(ONE_64x64.add_adbk(erfApprox(z)), TWO_64x64);
    }

    /**
     * Calculates the integral of x * log-normal probability density function (PDF) for a given x value.
     *     @param x: The input value for which the integral is calculated -signed 64.64-bit fixed point number
     *     @param mu: The mu of the underlying lognormal distribution -signed 64.64-bit fixed point number
     *     @param sigma: The sigma of the underlying lognormal distribution -signed 64.64-bit fixed point number
     *     @dev The formual for integralXTimeLognormalPDFX is as follows:
     *          constant = math.exp(mu + (sigma**2) / 2) / 2
     *          // Calculate the erf_term based on the approximation of the error function
     *          erf_term = erf_approx((mu + (sigma**2) - math.log(x)) / (math.sqrt_gs(2) * sigma))
     *          // Compute the result by combining the constant term and the erf_term
     *          return constant - (math.exp(mu + (sigma**2) / 2) * erf_term) / 2
     *     :return: The integral of x * log-normal PDF for the given x value -signed 64.64-bit fixed point number
     */
    function integralXTimesLognormalPdfX(int128 x, int128 mu, int128 sigma) internal pure returns (int128) {
        //Calculate the constant term
        int128 const = GoblinFixedPointMathLib.expWad_64x64((mu.add_adbk(sigma.pow_2().div(TWO_64x64))).toWad())
            .div(TWO_64x64);

        // Calculate the erf_term based on the approximation of the error function
        int128 erfTerm = erfApprox(
            mu.add_adbk(sigma.pow_2()).sub_adbk(GoblinFixedPointMathLib.lnWad_64x64((x.toWad()))).div(
                GoblinFixedPointMathLib.sqrt_gs(TWO_64x64).mul_adbk(sigma)
            )
        );

        // Compute the result by combining the constant term and the erf_term
        return const.sub_adbk(
            (
                GoblinFixedPointMathLib.expWad_64x64(((mu.add_adbk(sigma.pow_2().div(TWO_64x64))).toWad()))
                    .mul_adbk(erfTerm).div(TWO_64x64)
            )
        );
    }

    /**
     * Calculates error approximation.
     *     @param x:
     *     :return:
     */
    function erfApprox(int128 x) internal pure returns (int128) {
        unchecked {
            int128 sign = ONE_64x64;

            assembly {
                if slt(x, 0) { sign := sub(0, sign) }
            }
            x = GoblinFixedPointMathLib.abs(x);

            int128 t = ONE_64x64.div(ONE_64x64.add_adbk(p.mul_adbk(x)));
            int128 y = ONE_64x64.sub_adbk(
                (calculate_erf_y_1(t)).mul_adbk(
                    t.mul_adbk(GoblinFixedPointMathLib.expWad_64x64(((-x.mul_adbk(x)).toWad())))
                )
            );
            return sign.mul_adbk(y);
        }
    }

    function calculate_erf_y_1(int128 t) internal pure returns (int128) {
        return calculate_erf_y_2(t).mul_adbk(t).add_adbk(a1);
    }

    function calculate_erf_y_2(int128 t) internal pure returns (int128) {
        return (
            (((GoblinFixedPointMathLib.mul_adbk(a5, t).add_adbk(a4)).mul_adbk(t)).add_adbk(a3)).mul_adbk(t).add_adbk(a2)
        );
    }

    /**
     * Calculate the net present value (NPV) of a future payment.
     * @param principal: The principal amount
     * @param futurePayment: The future payment amount
     * @param discountRate: The discount rate
     * @param duration: The duration in days
     * @dev The formula for the NPV of a future payment is as follows:
     *          return (future_payment / ((1 + discount_rate) ** duration)) - principal
     *     returns nothing however if the loan NPV of the loan is below 0 we determin that the loan is invalid and we should not take it
     */
    function npv(int128 principal, int128 futurePayment, int128 discountRate, int128 duration) internal pure {
        if (
            
            (
                futurePayment.div(
                    GoblinFixedPointMathLib.powWad((ONE_64x64.add_adbk(discountRate)).toWad(), (duration).toWad())
                )
            ).sub_adbk(principal) <= 0
        ) {
            revert INVALID_LOAN();
        }
    }
}

