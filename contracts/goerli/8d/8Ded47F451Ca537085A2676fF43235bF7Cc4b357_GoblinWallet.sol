    function setVolEvalLoan(VolEvalRulesParams[] calldata new_rules) external onlyOwner {
        address collection;
        uint256 length = new_rules.length;
        for (uint256 i; i < length;) {
            collection = new_rules[i].collection;

            volEvalRules[collection][0] = new_rules[i].drift;
            volEvalRules[collection][1] = new_rules[i].annualized_vol;
            volEvalRules[collection][2] = new_rules[i].discount_rate;
            volEvalRules[collection][3] = new_rules[i].max_ltv;
            volEvalRules[collection][4] = new_rules[i].max_principalETH;
            volEvalRules[collection][5] = new_rules[i].max_duration;
            volEvalRules[collection][6] = new_rules[i].min_duration;
            //This is safe to use as its only for itterating on a for loop
            unchecked {
                ++i;
            }
        }
    }

