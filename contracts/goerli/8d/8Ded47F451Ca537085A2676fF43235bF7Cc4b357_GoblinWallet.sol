pragma solidity ^0.8.19;

import {FixedPointMathLib} from "solady/utils/FixedPointMathLib.sol";

/**
 * Smart contract library of mathematical mu operating with signed
 * 64.64-bit fixed point numbers.
 */
library GoblinFixedPointMathLib {
    /*
     * Minimum value signed 64.64-bit fixed point number may have.
     */
    int128 private constant MIN_64x64 = -0x80000000000000000000000000000000;

    /*
     * Maximum value signed 64.64-bit fixed point number may have.
     */
    int128 private constant MAX_64x64 = 0x7FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF;

    /*
     * DENOMINATOR is 1^2*64 we need this for multiplication
     */
    int128 private constant DENOMINATOR = 0x10000000000000000;

    int128 private constant negative = -0x6C556425A113FC34DA8979000000000;

    int256 private constant scaled6 = 1e6;
    int256 private constant scaled12 = 1e12;
    int256 private constant scaled18 = 1e18;

    /**
     * Convert unsigned 256-bit1e18 integer number into signed 64.64-bit fixed point
     * number.  Revert on overflow.
     *
     * @param x unsigned 256-bit integer number
     * @return z signed 64.64-bit fixed point number
     */
    function fromUInt_1e18(uint256 x) internal pure returns (int128 z) {
        assembly {
            if sgt(x, 0x6C556425A113FC34DA8979000000000) { revert(0, 0) }
            x := div(mul(x, DENOMINATOR), scaled18)
            z := x
        }
    }

    /**
     * Convert unsigned 256-bit1e18 integer number into signed 64.64-bit fixed point
     * number.  Revert on overflow.
     *
     * @param x unsigned 256-bit integer number
     * @return z signed 64.64-bit fixed point number
     */
    function fromInt_1e18(int256 x) internal pure returns (int128 z) {
        assembly {
            if or(sgt(x, 0x6C556425A113FC34DA8979000000000), slt(x, negative)) { revert(0, 0) }
            x := div(mul(x, DENOMINATOR), scaled18)
            z := x
        }
    }

    /**
     * Convert unsigned 256-bit integer number into signed 64.64-bit fixed point
     * number.  Revert on overflow.
     *
     * @param x unsigned 256-bit integer number
     * @return result signed 64.64-bit fixed point number
     */
    function toWad(int128 x) internal pure returns (int256 result) {
        assembly {
            // note that this produces a 128-bit result, but we only care about the lower 64 bits
            result := sar(64, mul(x, scaled6))
            if or(slt(result, MIN_64x64), sgt(result, MAX_64x64)) { revert(0, 0) }
            result := mul(result, scaled12)
        }
    }

    /**
     * Calculate x + y.  Revert on overflow.
     *
     * @param x signed 64.64-bit fixed point number
     * @param y signed 64.64-bit fixed point number
     * @return result signed 64.64-bit fixed point number
     */
    function add_adbk(int128 x, int128 y) internal pure returns (int128) {
        int256 result;
        assembly {
            // note that this produces a 128-bit result, but we only care about the lower 64 bits
            result := add(x, y)
            if or(slt(result, MIN_64x64), sgt(result, MAX_64x64)) { revert(0, 0) }
        }
        return int128(result);
    }

    /**
     * Calculate x - y.  Revert on overflow.
     *
     * @param x signed 64.64-bit fixed point number
     * @param y signed 64.64-bit fixed point number
     * @return result signed 64.64-bit fixed point number
     */
    function sub_adbk(int128 x, int128 y) internal pure returns (int128) {
        int256 result;
        assembly {
            // note that this produces a 128-bit result, but we only care about the lower 64 bits
            result := sub(x, y)
            if or(slt(result, MIN_64x64), sgt(result, MAX_64x64)) { revert(0, 0) }
        }
        return int128(result);
    }

    /**
     * Calculate x * y rounding down.  Revert on overflow.
     *
     * @param x signed 64.64-bit fixed point number
     * @param y signed 64.64-bit fixed point number
     * @return result signed 64.64-bit fixed point number
     */
    function mul_adbk(int128 x, int128 y) internal pure returns (int128) {
        int256 result;
        assembly {
            // note that this produces a 128-bit result, but we only care about the lower 64 bits
            result := sar(64, mul(x, y))
            if or(slt(result, MIN_64x64), sgt(result, MAX_64x64)) { revert(0, 0) }
        }
        return int128(result);
    }

    /**
     * Calculate |x|.  Revert on overflow.
     *
     * @param x signed 64.64-bit fixed point number
     * @return signed 64.64-bit fixed point number
     */
    function abs(int128 x) internal pure returns (int128) {
        unchecked {
            require(x != MIN_64x64);
            return x < 0 ? -x : x;
        }
    }

    /**
     * Calculate x^n for non fractional powers scaled to the power of 1^64
     *
     * @param x signed 64.64-bit fixed point number
     * @param n unsigned 256 number
     * @return z signed 64.64-bit fixed point number
     */
    function rpow(int128 x, uint256 n) internal pure returns (int128 z) {
        int128 scalar = 18446744073709551616;
        /// @solidity memory-safe-assembly
        assembly {
            switch x
            case 0 {
                switch n
                case 0 {
                    // 0 ** 0 = 1
                    z := scalar
                }
                default {
                    // 0 ** n = 0
                    z := 0
                }
            }
            default {
                switch mod(n, 2)
                case 0 {
                    // If n is even, store scalar in z for now.
                    z := scalar
                }
                default {
                    // If n is odd, store x in z for now.
                    z := x
                }

                // Shifting right by 1 is like dividing by 2.
                let half := shr(1, scalar)

                for {
                    // Shift n right by 1 before looping to halve it.
                    n := shr(1, n)
                } n {
                    // Shift n right by 1 each iteration to halve it.
                    n := shr(1, n)
                } {
                    // Revert immediately if x ** 2 would overflow.
                    // Equivalent to iszero(eq(div(xx, x), x)) here.
                    if shr(128, x) { revert(0, 0) }

                    // Store x squared.
                    let xx := mul(x, x)

                    // Round to the nearest number.
                    let xxRound := add(xx, half)

                    // Revert if xx + half overflowed.
                    if lt(xxRound, xx) { revert(0, 0) }

                    // Set x to scaled xxRound.
                    x := sdiv(xxRound, scalar)

                    // If n is even:
                    if mod(n, 2) {
                        // Compute z * x.
                        let zx := mul(z, x)

                        // If z * x overflowed:
                        if iszero(eq(sdiv(zx, x), z)) {
                            // Revert if x is non-zero.
                            if iszero(iszero(x)) { revert(0, 0) }
                        }

                        // Round to the nearest number.
                        let zxRound := add(zx, half)

                        // Revert if zx + half overflowed.
                        if lt(zxRound, zx) { revert(0, 0) }

                        // Return properly scaled zxRound.
                        z := sdiv(zxRound, scalar)
                    }
                }
            }
        }
    }

    /**
     * Calculate x^n for non fractional powers scaled to the power of 1^64
     *
     * @param x signed 64.64-bit fixed point number
     * @return  signed 64.64-bit fixed point number
     */
    function pow_2(int128 x) internal pure returns (int128) {
        // Calcualte a number to the power of 2
        int256 result;
        assembly {
            // note that this produces a 128-bit result, but we only care about the lower 64 bits
            result := sar(64, mul(x, x))
            if or(slt(result, MIN_64x64), sgt(result, MAX_64x64)) { revert(0, 0) }
        }
        return int128(result);
    }

    /**
     * Calculate sqrt (x) rounding down.  Revert if x < 0.
     *
     * @param x signed 64.64-bit fixed point number
     * @return signed 64.64-bit fixed point number
     */
    function sqrt_gs(int128 x) internal pure returns (int128) {
        unchecked {
            require(x >= 0);
            return int128(sqrt_1e18_scaled(uint256(int256(x)) << 64));
        }
    }

    function sqrt_1e18_scaled(uint256 x) internal pure returns (uint128 z) {
        /// @solidity memory-safe-assembly

        assembly {
            // `floor(sqrt(2**15)) = 181`. `sqrt(2**15) - 181 = 2.84`.
            z := 181 // The "correct" value is 1, but this saves a multiplication later.

            // This segment is to get a reasonable initial estimate for the Babylonian method. With a bad
            // start, the correct # of bits increases ~linearly each iteration instead of ~quadratically.

            // Let `y = x / 2**r`.
            // We check `y >= 2**(k + 8)` but shift right by `k` bits
            // each branch to ensure that if `x >= 256`, then `y >= 256`.
            let r := shl(7, lt(0xffffffffffffffffffffffffffffffffff, x))
            r := or(r, shl(6, lt(0xffffffffffffffffff, shr(r, x))))
            r := or(r, shl(5, lt(0xffffffffff, shr(r, x))))
            r := or(r, shl(4, lt(0xffffff, shr(r, x))))
            z := shl(shr(1, r), z)

            // Goal was to get `z*z*y` within a small factor of `x`. More iterations could
            // get y in a tighter range. Currently, we will have y in `[256, 256*(2**16))`.
            // We ensured `y >= 256` so that the relative difference between `y` and `y+1` is small.
            // That's not possible if `x < 256` but we can just verify those cases exhaustively.

            // Now, `z*z*y <= x < z*z*(y+1)`, and `y <= 2**(16+8)`, and either `y >= 256`, or `x < 256`.
            // Correctness can be checked exhaustively for `x < 256`, so we assume `y >= 256`.
            // Then `z*sqrt(y)` is within `sqrt(257)/sqrt(256)` of `sqrt(x)`, or about 20bps.

            // For `s` in the range `[1/256, 256]`, the estimate `f(s) = (181/1024) * (s+1)`
            // is in the range `(1/2.84 * sqrt(s), 2.84 * sqrt(s))`,
            // with largest error when `s = 1` and when `s = 256` or `1/256`.

            // Since `y` is in `[256, 256*(2**16))`, let `a = y/65536`, so that `a` is in `[1/256, 256)`.
            // Then we can estimate `sqrt(y)` using
            // `sqrt(65536) * 181/1024 * (a + 1) = 181/4 * (y + 65536)/65536 = 181 * (y + 65536)/2**18`.

            // There is no overflow risk here since `y < 2**136` after the first branch above.
            z := shr(18, mul(z, add(shr(r, x), 65536))) // A `mul()` is saved from starting `z` at 181.

            // Given the worst case multiplicative error of 2.84 above, 7 iterations should be enough.
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))
            z := shr(1, add(z, div(x, z)))

            // If `x+1` is a perfect square, the Babylonian method cycles between
            // `floor(sqrt(x))` and `ceil(sqrt(x))`. This statement ensures we return floor.
            // See: https://en.wikipedia.org/wiki/Integer_square_root#Using_only_integer_division
            // Since the ceil is rare, we save gas on the assignment and repeat division in the rare case.
            // If you don't care whether the floor or ceil square root is returned, you can remove this statement.
            z := sub(z, lt(div(x, z), z))
        }
    }

    /*/////////////////////////////////////////////////////////////////////////////////////////////
                        WAD calcuations these functions are edited from solady
    //////////////////////////////////////////////////////////////////////////////////////////////*/

    /**
     * Calculate ln(x) we take in a signed int256 number here by utilizing toWad() when passing in the int256
     * Function returns a signed 64x64 number after calculating the ln of x
     *
     * @param x signed int256 fixed point number
     * @return signed 64.64-bit fixed point number
     */
    function lnWad_64x64(int256 x) internal pure returns (int128) {
        return fromInt_1e18(FixedPointMathLib.lnWad(x));
    }

    /**
     * Calculate exp(x) we take in a non 64x64 number here by utilizing toWad() when passing in the int256
     * Function returns a signed 64x64 number after calculating the exp of x
     *
     * @param x signed int256 fixed point number
     * @return signed 64.64-bit fixed point number
     */
    function expWad_64x64(int256 x) internal pure returns (int128) {
        return fromInt_1e18(FixedPointMathLib.expWad(x));
    }

    /**
     * Calculate /// @dev Equivalent to `x` to the power of `y`.
     *  because `x ** y = (e ** ln(x)) ** y = e ** (ln(x) * y)`.
     *
     * @param x signed int256 fixed point number
     * @param y signed int256 fixed point number
     * @return signed 64.64-bit fixed point number
     */
    function powWad(int256 x, int256 y) internal pure returns (int128) {
        // Using `ln(x)` means `x` must be greater than 0.
        unchecked {
            return int128((FixedPointMathLib.expWad((FixedPointMathLib.lnWad(x) * y) / int256(1e18)) << 64) / 1e18);
        }
    }
}

