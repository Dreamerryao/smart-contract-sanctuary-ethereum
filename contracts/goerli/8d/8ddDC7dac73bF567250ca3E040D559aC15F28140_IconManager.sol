import "./IIconManager.sol";
import "../AccessControl/W3JEAccessControled.sol";
import "../CreatorCollection/ICollectionFactoryStorage.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

///////////////////////////////////
// | |     / /__  /    / / ____/ //
// | | /| / / /_ <__  / / __/    //
// | |/ |/ /___/ / /_/ / /___    //
// |__/|__//____/\____/_____/    //
///////////////////////////////////

contract IconManager is IIconManager, W3JEAccessControled, Ownable {
    using EnumerableSet for EnumerableSet.AddressSet;

    struct UsingToken {
        address collectionsAddress;
        uint256 tokenId;
    }

    event Change(
        address indexed holder,
        address indexed collectionAddress,
        uint256 indexed tokenId
    );

    mapping(address => UsingToken) public currentUsingToken;
    EnumerableSet.AddressSet private _outerCollections;
    ICollectionFactoryStorage public creatorCollectionStorage;
    uint256 public defaultTokenMax = 3;

    function getUsing(address holder)
        external
        view
        returns (address collectionAddress, uint256 tokenId)
    {
        UsingToken memory current = currentUsingToken[holder];
        if (current.collectionsAddress == address(0)) {
            return (current.collectionsAddress, current.tokenId);
        } else {
            if (
                IERC721(current.collectionsAddress).ownerOf(tokenId) == holder
            ) {
                return (current.collectionsAddress, current.tokenId);
            } else {
                return (address(0), 0);
            }
        }
    }

    function isUsing(
        address holder,
        address collectionAddress,
        uint256 tokenId
    ) external view returns (bool) {
        UsingToken memory current = currentUsingToken[holder];

        if (
            collectionAddress == current.collectionsAddress &&
            tokenId == current.tokenId
        ) {
            return true;
        } else {
            return false;
        }
    }

    function change(address collectionAddress, uint256 tokenId) external {
        if (collectionAddress == address(0)) {
            require(tokenId <= defaultTokenMax, "tokenId is over max.");
        } else {
            require(
                _outerCollections.contains(collectionAddress) ||
                    creatorCollectionStorage.existsCollections(
                        collectionAddress
                    ),
                "no supported collection."
            );
            require(
                IERC721(collectionAddress).ownerOf(tokenId) == msg.sender,
                "you are not owner."
            );
        }

        UsingToken memory newUsingToken = UsingToken(
            collectionAddress,
            tokenId
        );
        currentUsingToken[msg.sender] = newUsingToken;
        emit Change(msg.sender, collectionAddress, tokenId);
    }

    function setDefaultTokenMax(uint256 value) external onlyAdmin {
        defaultTokenMax = value;
    }

    function addOuterCollection(address value) external onlyAdmin {
        _outerCollections.add(value);
    }

    function removeOuterCollection(address value) external onlyAdmin {
        _outerCollections.remove(value);
    }

    function getOuterCollection() external view returns (address[] memory) {
        return _outerCollections.values();
    }

    function setCreatorCollectionStorage(address value) external onlyAdmin {
        creatorCollectionStorage = ICollectionFactoryStorage(value);
    }

    // ==================================================================
    // For AccessControlled
    // ==================================================================
    function owner()
        public
        view
        virtual
        override(Ownable, W3JEAccessControled)
        returns (address)
    {
        return Ownable.owner();
    }
}

