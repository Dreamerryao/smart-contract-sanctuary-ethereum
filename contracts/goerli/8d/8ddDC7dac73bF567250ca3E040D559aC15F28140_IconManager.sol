interface ICollectionFactoryStorage {
    function calAddress() external view returns (address);

    function minter() external view returns (address);

    function tokenUriSupplier() external view returns (address);

    function locker() external view returns (address);

    function iconManager() external view returns (address);

    function getCollections() external view returns (address[] memory);

    function existsCollections(address collectionAddress)
        external
        view
        returns (bool);

    function addCollections(address collectionAddress) external;

    function setCalAddress(address value) external;

    function setMinter(address value) external;

    function setLocker(address value) external;

    function setTokenUriSupplier(address value) external;

    function setIconManager(address value) external;
}