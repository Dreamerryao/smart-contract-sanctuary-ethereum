    function totalSupply() public view override returns (uint256) {
        uint256 _supply = 0;
        for (uint256 i = 0; i < supplyPerClass.length; ++i) {
            _supply += supplyPerClass[i];
        }
        return _supply;
    }

    /**
     * @dev Returns the SVG and metadata for a token Id
     * @param _tokenId The tokenId to return the SVG and metadata for.
     */
    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(_tokenId));
        return
            IKyodaiDesc(officialAddys[1]).tokenURI(
                _tokenId,
                kyodais[_tokenId].traitHash,
                baseURI
            );
    }

    function claimableYen(uint256 timeDiff, uint16 lvl)
        internal
        pure
        returns (uint256)
    {
        //yen production based on level. Increase production by 1 yen per 5 lvl plus base production of 5 yen. Max output 35 yen per day
        uint256 production = lvl >= 50 ? 15 : ((lvl * 1) / 5) + 5;
        return (production * timeDiff * 1 ether) / 1 days;
    }

