    function mint(uint256 tagId) external noCheating {
        // check donation: msg.value to increase rare chance
        require(
            supplyPerClass[tagId % 3] + 1 <= 11111,
            "shatei class fully minted"
        );
        require(totalSupply() + 1 <= 39999, "fully minted");

        IDogTag(officialAddys[2]).burn(tagId, msg.sender, 1);
        mintInternal(msg.sender, tagId);
    }

    function mintInternal(address to, uint256 tagId) internal {
        uint256 tokenId = supplyPerClass[tagId % 3] + 1;
        ITraitGen traitGen = ITraitGen(officialAddys[0]);
        kyodais[tokenId] = Kyodai({
            lvlProgress: 0,
            level: traitGen.genShateiLevel(tagId, tokenId, to, 0),
            traitHash: traitGen.genShateiHash(tagId, tokenId, to, 0)
        });
        activities[tokenId] = Activity({
            location: address(this),
            action: Action.UNSTAKED,
            timestamp: uint64(block.timestamp)
        });
        supplyPerClass[tagId % 3]++;
        _mint(to, tokenId);
    }

    function burn(uint256 tokenId, address who)
        external
        override
        isKyodaiOwner(who, tokenId)
    {
        _burn(tokenId);
    }

    /*/////////////////////////////////////////
                 ACTIONS
//////////////////////////////////////////// */

    function doAction(uint256 tokenId, Action action)
        public
        isKyodaiOwner(msg.sender, tokenId)
        noCheating
    {
        _doAction(tokenId, msg.sender, action);
    }

    function doActionMany(uint256[] calldata tokenIds, Action action)
        external
        noCheating
    {
        for (uint256 i = 0; i < tokenIds.length; ++i) {
            doAction(tokenIds[i], action);
        }
    }

    function claim(uint256[] calldata tokenIds) external {
        for (uint256 i; i < tokenIds.length; ++i) {
            _claim(tokenIds[i]);
        }
    }

    function sendToUnderworld(uint256[] calldata tokenIds) external noCheating {
        for (uint256 i = 0; i < tokenIds.length; ++i) {
            require(msg.sender == ownerOf(tokenIds[i]), "kyodai not owned");
            require(
                activities[tokenIds[i]].action == Action.UNSTAKED,
                "token not available"
            );
            activities[tokenIds[i]] = Activity({
                location: officialAddys[4],
                action: Action.AWAY,
                timestamp: uint64(block.timestamp)
            });
        }
        IUnderworld(officialAddys[4]).stakeMany(tokenIds, msg.sender); //add to list in underworld contract
    }

    function returnManyFromUnderworld(
        uint256[] calldata tokenIds,
        Action action_
    ) public noCheating {
        for (uint256 i = 0; i < tokenIds.length; ++i) {
            Activity memory activity = activities[tokenIds[i]];
            require(msg.sender == ownerOf(tokenIds[i]), "kyodai not owned");
            require(activity.action == Action.AWAY, "kyodai is home");
            IUnderworld underworld = IUnderworld(activity.location);
            underworld.unstake(tokenIds[i]);
            _doAction(tokenIds[i], msg.sender, action_);
        }
    }

    // ------------------
    // Internal Functions
    // ------------------
    function _doAction(
        uint256 tokenId,
        address who,
        Action action_
    ) internal isKyodaiOwner(who, tokenId) {
        Activity memory activity = activities[tokenId];
        require(activity.action != Action.AWAY, "token is away");
        require(activity.action != action_, "already doing that");

        uint256 timestamp = block.timestamp > activity.timestamp
            ? block.timestamp
            : activity.timestamp;

        if (block.timestamp > activity.timestamp) _claim(tokenId);
        timestamp = timestamp > activity.timestamp
            ? timestamp
            : activity.timestamp;

        activities[tokenId] = Activity({
            location: address(this),
            action: action_,
            timestamp: uint64(timestamp)
        });
        emit ActionMade(who, tokenId, uint8(action_));
    }

    function _claim(uint256 tokenId) internal noCheating {
        Activity memory activity = activities[tokenId];

        if (block.timestamp <= activity.timestamp) return;

        uint64 timestamp = uint64(block.timestamp);
        uint64 timeDiff = timestamp - activity.timestamp;
        if (activity.action == Action.TRAINING) {
            // IStatManager(statAddress).addExp(
            //     tokenId,
            //     (timeDiff * 1000) / 1 days
            // );
            // shateis[tokenId].exp += (timeDiff * 1000) / 1 days;
            uint64 currentProgress = kyodais[tokenId].lvlProgress +
                ((timeDiff * 1000) / 1 days);
            if (currentProgress >= 1000) {
                uint16 addLevel = uint16(currentProgress / 1000);
                kyodais[tokenId].level += addLevel;
                kyodais[tokenId].lvlProgress =
                    currentProgress -
                    (addLevel * 1000);
            } else {
                kyodais[tokenId].lvlProgress = currentProgress;
            }
        }
        if (activity.action == Action.FARMING) {
            INeoYen(officialAddys[3]).mint(
                ownerOf(tokenId),
                claimableYen(timeDiff, checkLevel(tokenId))
            );
        }

        activities[tokenId].timestamp = timestamp;
    }

