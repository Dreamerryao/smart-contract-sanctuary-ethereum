pragma solidity ^0.8.4;

import "./PrysmProtocolNFTBase.sol";

/// @custom:security-contact [email protected]
contract PrysmProtocolNFTFinV1 is PrysmProtocolNFTBase {

    bytes32 public root;

    function initialize(address _squad) initializer public {
        __PrysmProtocolNFTBase_initialize(_squad);
    }

    function authorizeMint(bytes32 _root, string memory _mintListUrl) external onlyOwner {
        root = _root;
        emit AuthorizeMintList(owner(), _mintListUrl);
    }

    function claimMembershipNft(bytes32[] calldata proof) external {
        require(_verify(_msgSender(), proof), "invalid proof");
        _safeMintSquad(_msgSender());
    }

    function canMint(address to) external view returns (bool) {
        return _canMint(to);
    }

    function _canMint(address to) internal view returns (bool) {
        return _userToSquadToken[to] == 0;
    }

    function canClaimMembershipNft(address to, bytes32[] calldata proof) external view returns (bool canClaim) {
        canClaim = _verify(to, proof) && _canMint(to);
    }

    function _verify(address to, bytes32[] memory proof)
    internal view returns (bool)
    {
        return MerkleProof.verify(proof, root, _leaf(to));
    }

    function _leaf(address to)
    internal pure returns (bytes32)
    {
        return keccak256(abi.encodePacked(to));
    }

    function daoTransfer(address from, address to, uint256 tokenId, bytes memory /*data*/) public onlyOwner {
        require(_userToSquadToken[to] == 0,"PrysmProtocolNFTFinV1: User already owns a token");
        _userToSquadToken[to] = tokenId;
        _userToSquadToken[from] = 0;
        _safeTransfer(from, to, tokenId, "");
    }

    function daoMint(address to) public onlyOwner {
        require(_userToSquadToken[to] == 0,"PrysmProtocolNFTFinV1: User already owns a token");
        _safeMintSquad(to);
    }
}

