/// @custom:security-contact [email protected]
abstract contract PrysmProtocolNFTBase is Initializable, ERC721BurnableUpgradeable, OwnableUpgradeable, IPrysmFundingRoundV1 {
    /*, PausableUpgradeable, AccessControlUpgradeable, UUPSUpgradeable*/
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenIdCounter;

    // one token per user
    mapping(address => uint256) internal _userToSquadToken;

    string private _uri;

    function __PrysmProtocolNFTBase_initialize(address _squad) initializer public {
        __ERC721_init("Prysm Protocol NFT", "PPN");
        __ERC721Burnable_init();
        _transferOwnership(_squad);
        _uri = "https://metadata.msyrp.xyz";

        // tokens start at 1
        _tokenIdCounter.increment();
        _safeMintSquad(_squad);
        emit CreateMembershipPass(_squad);
    }


    function safeMintsSquad(address[] memory mintTo) public onlyOwner {
        for (uint i = 0; i < mintTo.length; i++) {
            _safeMintSquad(mintTo[i]);
        }
    }

    function _safeMintSquad(address to) internal {
        require(_userToSquadToken[to] == 0, "already minted");
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        _userToSquadToken[to] = tokenId;
    }


    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        TokenURIDescriptor.TokenURIParams memory params = TokenURIDescriptor.TokenURIParams({
        tokenId : tokenId,
        owner : ownerOf(tokenId),
        squadAddress : owner(),
        baseUrl: _uri,
        collectionAddress: address(this)
        });
        return TokenURIDescriptor.constructTokenURI(params);
    }

    function contractURI() public view returns (string memory) {
        ContractURIDescriptor.ContractURIParams memory params = ContractURIDescriptor.ContractURIParams({
        squadAddress : owner(),
        baseUrl: _uri,
        collectionAddress: address(this)
        });
        return ContractURIDescriptor.constructContractURI(params);
    }

    function squad() public view returns (address){
        return owner();
    }

    receive() external payable
    {
        revert("Not payable");
    }

    function transferFrom(
        address ,
        address ,
        uint256
    ) public virtual override {
        require(false, "PrysmProtocolNFTBase: transfers disabled");
    }

    function safeTransferFrom(
        address ,
        address ,
        uint256
    ) public virtual override {
        require(false, "PrysmProtocolNFTBase: transfers disabled");
    }

    function safeTransferFrom(
        address ,
        address ,
        uint256 ,
        bytes memory
    ) public virtual override {
        require(false, "PrysmProtocolNFTBase: transfers disabled");
    }

//    todo
//    function contractURI
}

