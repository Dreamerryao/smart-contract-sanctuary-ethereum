pragma solidity ^0.8.7;

import {Base64} from 'base64-sol/base64.sol';
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

library TokenURIDescriptor {
 using StringsUpgradeable for uint256;
    struct TokenURIParams {
        uint256 tokenId;
        address owner;
        address squadAddress;
        address collectionAddress;
        string baseUrl;
    }

    /**
         * @notice Construct an ERC721 token URI.
     */
    function constructTokenURI(TokenURIParams memory params)
    internal
    view
    returns (string memory)
    {
        string memory chainId = StringsUpgradeable.toString(block.chainid);
        string memory ownerString = StringsUpgradeable.toHexString(uint160(params.owner), 20);
        string memory squadString = StringsUpgradeable.toHexString(uint160(params.squadAddress), 20);
        string memory collectionAddressString = StringsUpgradeable.toHexString(uint160(params.collectionAddress), 20);
        string memory url = string(abi.encodePacked(params.baseUrl, "/api/metadata/", chainId, "/", collectionAddressString,  "/",params.tokenId.toString(), "?squad=", squadString, "&owner=", ownerString));
        return url;
    }
}

