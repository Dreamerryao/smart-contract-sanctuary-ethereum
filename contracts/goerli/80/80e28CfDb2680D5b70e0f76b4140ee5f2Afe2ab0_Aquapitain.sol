pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./AquaToken.sol";
import "./AquaNFT.sol";
import "./FloatingMaths.sol";

contract AquaReserve is Ownable {
    AquaToken public aquaToken;
    AquaNFT public aquaNFT;
    uint public deployDate;
    uint public counterDistribution;

    constructor (
        address _aquaToken,
        address _NFTFactory
    ) {
        aquaToken = AquaToken(_aquaToken);
        aquaNFT = AquaNFT(_NFTFactory);
        deployDate = block.timestamp;
        counterDistribution = 365;
    }

    /*
     * @dev Distributes 30 percent of the AquaToken it contains to all AquaNFT holders in proportion to their share.
     * Can only be called after 365 days. Then once every 30 days after that date.
     */
    function distribute() public onlyOwner {
        uint oneYear = deployDate + 365 days;
        uint lastMonth =  deployDate + counterDistribution * 1 days;
        require(block.timestamp  >= oneYear , "Need to wait 365 days before initial distribution");
        require(block.timestamp  >= lastMonth , "Need to wait 30 days after last distribution");


        uint total = aquaToken.balanceOf(address(this)) * 30 / 100; // 30 percent of the AquaTokens in the pool
        uint totalSupply = aquaNFT.totalSupply();
        uint totalShares =  aquaNFT.totalShares();
        aquaToken.approve(address(this), 9*10**50);
        //Send AquaTokens in proportion to the share of each NFT in the collection.
        for (uint i = 0; i < totalSupply; i++) {
            //address holder of the i-th NFT
            address nftOwner = aquaNFT.ownerOf(i);
            //share of the ith NFT
            uint shares = aquaNFT.tokenShares(i);
            //Transfer of the AquaaquaTokens to the ith NFT holder
            uint256 amount = FMaths.div(
                FMaths.mul(shares, total, 0, aquaToken.decimals(), aquaToken.decimals()),
                totalShares,
                aquaToken.decimals(), 0, aquaToken.decimals()
            );
            aquaToken.transferFrom(address(this), nftOwner, amount);
        }

        counterDistribution += 30;
    }
}

