        bytes32 midResHash,
        bytes32 highResHash
    );

    /*
     * @dev Certify metaData amount for value, for someone else.
     * @param certifier The certifier wallet.
     * @param amount The amount to certify.
     * @param value The value of tokens to burn for the certification.
     * @param lowResHash A low resolution of the certified data.
     * @param midResHash A medium resolution of the certified data.
     * @param highResHash A high resolution of the certified data.
     */
    function certifyFor(
        address certifier,
        uint256 amount,
        uint256 value,
        bytes32 lowResHash,
        bytes32 midResHash,
        bytes32 highResHash
    ) public onlyOwner {
        // Collect certification tokens
        aquaToken.transferFrom(certifier, address(this), value);

        // update counters
        totalCertified += amount;
        certifications[certifier] += amount;

        // Send 5% for liquidity pools
        uint256 poolPercentage = FMaths.mul(
          value,
          5,
          aquaToken.decimals(),
          0,
          aquaToken.decimals() - 2
        );
        aquaToken.transfer(owner(), poolPercentage);

        // Burn tokens
        aquaToken.burn(value - poolPercentage);

         // Emit certification events
        emit Certified(certifier, amount, value, lowResHash, midResHash, highResHash);
    }
}

