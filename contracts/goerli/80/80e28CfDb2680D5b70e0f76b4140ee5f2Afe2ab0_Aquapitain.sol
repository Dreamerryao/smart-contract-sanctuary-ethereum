pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./AquaToken.sol";
import "./AquaCertifier.sol";
import "./AquaReserve.sol";
import "./AquaNFT.sol";
import "./FloatingMaths.sol";

contract Aquapitain is Ownable {
  // $ indexed Stable coin used for minting purchase
  IERC20Metadata public buyingToken;

  // Managed contracts
  AquaToken public aquaToken;
  AquaCertifier public aquaCertifier;
  AquaReserve public aquaReserve;
  AquaNFT public aquaNFT;


  uint8 internal decimals;
  // Constants used for the bonding curve
  uint256 public step;
  uint256 public step1Weight;
  uint256 public step2Weight;
  uint256 public initialValuation;
  uint256 public one;
  // Constants used for certification
  uint256 public certificationPriceFloorThreshold;
  uint256 public certificationPriceRoofThreshold;

  /*
   * @dev Create the certification Capitain.
   * @param token The token smart-contract address.
   * @param certification The certification smart-contract address.
   * @param certifiersPool The certifiers pool smart-contract address.
   * @param nftFactory The NFT Factory smart-contract address.
   */
  constructor (
    address internalToken,
    address certification,
    address certifiersPool,
    address nftFactory,
    address buyingTokenAddress
  ) {
    // Managed contracts
    aquaToken = AquaToken(internalToken);
    aquaCertifier = AquaCertifier(certification);
    aquaReserve = AquaReserve(certifiersPool);
    aquaNFT = AquaNFT(nftFactory);

    buyingToken = IERC20Metadata(buyingTokenAddress);

    decimals = 18;
    // Constants used for the bonding curve
    step = 25 * (10 ** uint256(decimals + 8));
    step1Weight = 4 * (10 ** uint256(decimals - 8));
    step2Weight = 1 * (10 ** uint256(decimals - 8));
    initialValuation = 1 * (10 ** uint256(decimals - 2));
    // Constants used for certification
    certificationPriceRoofThreshold = 5 * (10 ** uint256(decimals - 3));
    certificationPriceFloorThreshold = 2 * (10 ** uint256(decimals - 3));
  }

  /**
   * @dev Computes bonding curve.
   * @param x The amount of token already minted.
   */
  function bondingCurve(uint256 x) private view returns (uint256) {
    uint256 res;
    if (x <= step) {
      // ((x * k1) + 1) * c
      // res = ((x * 4) / (10 ** 8) + one) / (10 ** 2);
      res = FMaths.mul(
        FMaths.add(
          FMaths.mul(x, step1Weight), // a
          1, // b
          decimals, // nb of decimals for a
          0 // nb of decimals for b
        ),
        initialValuation
      );
    } else {
      // ([(x - step) * k2] + [step * k1 + 1]) * c
      // res = (((x - step) / (10 ** 8)) + (step * 4 / (10 ** 8) + one)) / 10 ** 2;
      res = FMaths.mul(
        FMaths.add(
          FMaths.mul(FMaths.sub(x, step), step2Weight),
          FMaths.add(FMaths.mul(step, step1Weight), 1, decimals, 0)
        ),
        initialValuation
      );
    }
    return res;
  }

  /*
   * @dev Integral for the first step of the bonding curve
   * F1(x, step, k1, k2, c) => (x * c) * (((x * k1) / 2) + 1)
   * @param x The amount to create.
   */
  function F1(uint256 x) private view returns (uint256) {
    return FMaths.mul(
      FMaths.mul(x, initialValuation),
      FMaths.add(
        FMaths.div(FMaths.mul(x, step1Weight), 2, decimals, 0),
        1,
        decimals,
        0
      )
    );
    // return (x * initialValuation * ((x * step1Weight / 2 / (10 ** decimals)) + one) / (10 ** (decimals * 2)));
  }

  /*
   * @dev Integral for the second step of the bonding curve
   * F2(x, step, k1, k2, c) => c * ( k1 * step * x - k2 * step * x + ((k2*x*x) / 2.0) + x)
   * F2(x, step, k1, k2, c) => c * ( ((k1 - k2) * step * x) + ((k2*x*x) / 2.0) + x)
   * F2(x, step, k1, k2, c) => (c * x) * ([{k1 - k2} * step] + [{k2 * x} / 2.0] + 1)
   * @param x The amount to create.
   */
  function F2(uint256 x) private view returns (uint256) {
    return FMaths.mul(
      FMaths.mul(x, initialValuation),
      FMaths.add(
        FMaths.mul(
          FMaths.sub(step1Weight, step2Weight),
          step
        ),
        FMaths.add(
          FMaths.div(FMaths.mul(x, step2Weight), 2, decimals, 0),
          1,
          decimals,
          0
        )
      )
    );
    // return initialValuation * (
    //   ( (x * step * (step1Weight - step2Weight)) / (10 ** (2 * decimals)) )
    //   + ( ((step2Weight * x * x) / 2) / (10 ** (2 * decimals)) )
    //   + x
    // ) / (10 ** decimals);
  }

  /*
   * @dev Computes the integral value under the curve to calculate total price
   * @param amount The amount to create.
   */
  function bondingIntegral(uint256 x) private view returns (uint256) {
    if (x <= step) {
      return F1(x);
    } else {
      return F1(step) + F2(x) - F2(step);
    }
  }

  function estimatePrice(uint256 amount) public view returns (uint256) {
    return bondingIntegral(amount);
  }

  event MintTokens(address indexed to, uint256 amount, uint256 valueDecimals18, uint256 value);

  /*
   * @dev Create tokens.
   * @param amount The amount to be created (with AquaToken Decimals).
   */
  function mintTokens(uint256 amount) public {
    mintTokensFor(msg.sender, amount);
  }

  /*
   * @dev Create tokens for specified address.
   * @param recipient The address to mint tokens for.
   * @param amount The amount to be created (with AquaToken Decimals).
   */
  function mintTokensFor(address recipient, uint256 amount) public {
    uint256 normalisedAmount = FMaths.mul(amount, 1, aquaToken.decimals(), 0, decimals);
    // uint256 normalisedAmount = amount * (10 ** uint256(decimals - aquaToken.decimals()));
    uint256 totalMinted = aquaToken.totalTokenMinted();

    // minting value in buying token in a normalised 18 decimals format
    uint256 normalisedValue = bondingIntegral(totalMinted + normalisedAmount) - bondingIntegral(totalMinted);
    uint256 value = FMaths.mul(normalisedValue, 1, decimals, 0, buyingToken.decimals());
    // uint256 value = normalisedValue / (10 ** uint256(decimals - buyingToken.decimals()));

    buyingToken.transferFrom(recipient, address(this), value);
    // Provision NFT reserve
    buyingToken.transfer(
      address(aquaReserve),
      FMaths.mul(value, 15, buyingToken.decimals(), 0, buyingToken.decimals() - 2)
    );

    aquaToken.mint(recipient, amount);
    emit MintTokens(recipient, amount, normalisedValue, value);
  }

  /*
   * @dev Certify hashes for a given amount. Burn the tokens used for the certification.
   * @param amount The amount to be certified, i.e. the certification's value.
   * @param lowResHash Low resolution hash to certify.
   * @param midResHash Medium resolution hash to certify.
   * @param highResHash High resolution hash to certify.
   */
  function certify(
    uint256 amount,
    bytes32 lowResHash,
    bytes32 midResHash,
    bytes32 highResHash
  ) public {
    certifyFor(msg.sender, amount, lowResHash, midResHash, highResHash);
  }

  /*
   * @dev Certify hashes for a given amount. Burn the tokens used for the certification.
   * @param certifier The wallet for which the certification is triggered.
   * @param amount The amount to be certified, i.e. the certification's value (10^0).
   * @param lowResHash Low resolution hash to certify.
   * @param midResHash Medium resolution hash to certify.
   * @param highResHash High resolution hash to certify.
   */
  function certifyFor(
    address certifier,
    uint256 amount,
    bytes32 lowResHash,
    bytes32 midResHash,
    bytes32 highResHash
  ) public {
    uint256 value = calculateCertificationPrice(certifier, amount);
    // amount already certified
    uint256 totalAmountCertifier = aquaCertifier.certifications(certifier);

    // save hashes
    aquaCertifier.certifyFor(certifier, amount, value, lowResHash, midResHash, highResHash);

    // new total certified amount
    uint256 newTotalAmountCertifier = aquaCertifier.certifications(certifier);
    require(newTotalAmountCertifier > totalAmountCertifier , "Certification: Overflow?");

    emitNFTs(certifier, newTotalAmountCertifier);
  }

  /*
   * @dev Calculate certification price for given amount from certifier.
   * @param certifier Certifier wallet.
   * @param amount Amount to certify.
   */
  function calculateCertificationPrice(address certifier, uint256 amount)
    internal
    returns (uint256 value)
  {
    // calculation of certification value
    // max(floor, roof * [1 - (totalCertified / 10^13)])
    uint256 certifPrice;
    if (aquaCertifier.totalCertified() <= 6 * 10 ** 12) {
      certifPrice = FMaths.mul(
        certificationPriceRoofThreshold,
        FMaths.sub(
          1,
          FMaths.div(aquaCertifier.totalCertified(),  10 ** 13, 0 , 0),
          0,
          decimals
        )
      );
    } else {
      certifPrice = certificationPriceFloorThreshold;
    }
    uint256 value = FMaths.div(
      FMaths.mul(amount, certifPrice, 0, decimals),
      bondingCurve(FMaths.mul(
        aquaToken.totalTokenMinted(),
        1,
        aquaToken.decimals(), 0, // initial number format
        aquaToken.decimals() // internal number format
      ))
    );
    return value;
  }

  /*
   * @dev emit NFTs if amount breach threshold.
   * @param certifier The certifier wallet.
   * @param newTotalAmountCertifier The total amount certified for certifier.
   */
  function emitNFTs(address certifier, uint256 newTotalAmountCertifier) internal {
    uint256 sharesMinted = aquaNFT.sharesMinted(certifier);
    uint256[4] memory nftValues = [uint256(1), 12, 150, 1800];
    uint256[4] memory nftThresholds = [uint256(10**6), 10**7, 10**8, 10**9];

    uint256 nft4 = sharesMinted / 1963;
    uint256 nft3 = nft4 + ((sharesMinted % 1800) / 163);
    uint256 nft2 = nft3 + ((sharesMinted % 1800) % 150) / 13;
    uint256 nft1 = nft2 + ((sharesMinted % 1800) % 150) % 12;

    uint256 totalNFTised = (
      nft1 * nftThresholds[0]
      + nft2 * nftThresholds[1]
      + nft3 * nftThresholds[2]
      + nft4 * nftThresholds[3]
    );
    uint256 sumAvailable = newTotalAmountCertifier - totalNFTised;
    // nft current index. Could be saved for compute optimisation.
    uint8 nftIndex = nft1 > nft2
      ? 1
      : nft2 > nft3
        ? 2
        : nft3 > nft4
          ? 3
          : 0;
    while (nftThresholds[nftIndex] <= sumAvailable) {
      sumAvailable -= nftThresholds[nftIndex];
      aquaNFT.safeMint(certifier, nftValues[nftIndex]);
      nftIndex = (nftIndex + 1) % 4;
    }
  }

  /*
   * @dev Change buyingToken
   * @param buyingToken The new stable coin address
   */
  function setBuyingToken(address newContract) public onlyOwner {
		buyingToken = IERC20Metadata(newContract);
  }

  /*
   * @dev Distributes AquaReserve NFT pool.
   */
  function distributeAquaReserve() public {
    aquaReserve.distribute();
  }

  /*
   * @dev Withdraw the stable coin used to mint to managing DAO's wallet.
   */
  function withdrawBuyingToken() public {
    buyingToken.transfer(owner(), buyingToken.balanceOf(address(this)));
  }

  /*
   * @dev Withdraw the aquaToken to managing DAO's wallet.
   */
  function withdrawAquaToken() public {
    aquaToken.transfer(owner(), aquaToken.balanceOf(address(this)));
  }

  /*
   * @dev Withdraw the aquaToken and stable coin to managing DAO's wallet.
   */
  function withdraw() public {
    withdrawBuyingToken();
    withdrawAquaToken();
  }

  /*
   * @dev Upgrade to a new capitain smart-contract version and transfers ownership of its contracts.
   * @param newContract The new version for the orcherstrator
   */
  function upgrade(address newContract) public onlyOwner {
    // Transfer ownership over managed smart-contracts
    aquaToken.transferOwnership(newContract);
    aquaCertifier.transferOwnership(newContract);
    aquaReserve.transferOwnership(newContract);
    aquaNFT.transferOwnership(newContract);

    // Transfer any tokens leftover in wallet
    aquaToken.transfer(newContract, aquaToken.balanceOf(address(this)));
    buyingToken.transfer(newContract, buyingToken.balanceOf(address(this)));
  }
}

