pragma solidity 0.8.4;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";


contract DemandBalancer{
  // event Test(int128);
  using Strings for uint;

  address public immutable ethUsdPricefeed;

  struct Activity {
    bool activityEnded;
    address payable owner;
    string name;
    uint startingFee; // In USD
    string[] attendanceTypes;
    address[] allowedAddresses;
    mapping(address=>string) attendeeTypes;
    mapping(address=>uint) paidFees;
    uint balance;
  }
  Activity[] activities;
  int128 numerator = 3;
  int128 denominator = 2; 

  event NewActivity(string indexed _name, uint id, string[] attendanceTypes, address _owner);

  constructor(address _ethUsdPriceFeed){
    ethUsdPricefeed=_ethUsdPriceFeed;
  }

  function createActivity(string memory _name, uint _startingFee, string[] memory _attendanceTypes ) public {
    require(_attendanceTypes.length==2,'2 attendance type is required.');



    Activity storage activity = activities.push();
    emit NewActivity(_name, activities.length-1, _attendanceTypes, msg.sender);

    activity.owner = payable(msg.sender);
    activity.name = _name;
    activity.startingFee = _startingFee;
    activity.attendanceTypes = _attendanceTypes;
  }

  function getActivity(uint _id) view public _activityExists(_id) 
      returns(string memory name, 
              uint startingFeeInUsd, 
              string[] memory attendanceTypes,
              address[] memory allowedAddresses){
  
    Activity storage activity = activities[_id];

    name = activity.name;
    startingFeeInUsd = activity.startingFee;
    attendanceTypes = activity.attendanceTypes;
    allowedAddresses = activity.allowedAddresses;
  }

  function getJoinFeeAsString(uint _activityId, string calldata _type) view public 
    returns(string memory){
    int newFeeInUsdWei = _getJoinFeeInUsdWei(_activityId, _type);

    uint quotinent = uint(newFeeInUsdWei) / 1 ether;
    uint remainder = uint(newFeeInUsdWei) - quotinent * 1 ether;

    string memory resultAsString;

    if (remainder > 0) {
      resultAsString = string(abi.encodePacked(quotinent.toString(), '.', remainder.toString()));
    } else {
      resultAsString = quotinent.toString();
    }

    return addUsdSymbol(_removeZeros(resultAsString));
  }

 function join(uint _activityId, string calldata _type) payable public {
    _join(_activityId, _type);

    Activity storage activity = activities[_activityId];
    activity.attendeeTypes[msg.sender] = _type;
    activity.allowedAddresses.push(msg.sender);
    activity.balance += msg.value;
  }

 function _join(uint _activityId, string calldata _type) 
  _activityExists(_activityId)
  _activityContinues(_activityId)
  _typeExists(_activityId, _type)
  _sendEnoughEntranceFee(_activityId, _type)
   private {}

  function transferFeesToOwner(uint _activityId) public _onlyActivityOwner(_activityId) {
    Activity storage activity = activities[_activityId];
    uint balance = activity.balance;
    activity.balance = 0;
    activity.activityEnded = true;
    activity.owner.transfer(balance);
  }

  function _getJoinFee(uint _activityId, string calldata _type) private view 
  returns(uint joinFee) {
    uint joinFeeInUsdWei = uint(_getJoinFeeInUsdWei(_activityId, _type));
    
    joinFee = _calculateJoinFee(joinFeeInUsdWei);
  }

  function _calculateJoinFee(uint _joinFeeInUsdWei) private view returns(uint){
    (uint price, uint decimals) = getEtherPriceInUsd();

    return _joinFeeInUsdWei / price / (10**decimals);
  }

  function getEtherPriceInUsd() view public returns(uint, uint){
    AggregatorV3Interface priceFeed = AggregatorV3Interface(ethUsdPricefeed);

    (, int256 price, , ,) = priceFeed.latestRoundData();

    uint256 decimals = uint256(priceFeed.decimals());

    return (uint256(price), decimals);
    // return(1500,0);
  }
  
  // Make calculations in * ether for precise calculations
  function _getJoinFeeInUsdWei(uint _activityId, string calldata _type) view private
     _typeExists(_activityId, _type) returns(int){

    Activity storage activity = activities[_activityId];
    string memory type1 = activity.attendanceTypes[0];
    (int type1Count, int type2Count) = findCountOfTypes(activity, type1);

    int diff = type1Count - type2Count;
    int absDiff = diff < 0 ? -diff : diff;
    uint baseFee = activity.startingFee;
    if (absDiff <= 1) {
      return int(baseFee * 1 ether);
    } else {
      string memory __type = _type; // calldata to memory

      bool type1IsSelected = keccak256(bytes(__type)) == keccak256(bytes(type1));

      bool getHigherFee = diff > 0 && type1IsSelected || diff < 0 && !type1IsSelected;

      return calculateNewFeeInUsdWei(baseFee, getHigherFee, absDiff);
    }
  }

  function findCountOfTypes(Activity storage activity, string memory type1) private view returns(int, int){
    int type1Count;
    int type2Count;

    for (uint i=0; i<activity.allowedAddresses.length; i++){
      address addr = activity.allowedAddresses[i];
      string memory _type = activity.attendeeTypes[addr];

      keccak256(abi.encode(_type)) == keccak256(abi.encode(type1)) 
          ? type1Count++ 
          : type2Count++;
    }

    return (type1Count, type2Count);
  }

  // numerator / denominator = 50% premium for each extra demand
  function calculateNewFeeInUsdWei(uint _fee, bool _getHigherFee, int demandDifference) 
      private view returns(int){
    int feeWithDigits = int(_fee) * 1 ether;
    console.log(_fee);

    for (int i = 0; i < demandDifference; i++){
      if (_getHigherFee){
            feeWithDigits *= numerator;
            feeWithDigits /= denominator;
        } else {
          feeWithDigits /= 2;
        }    
    }

    return feeWithDigits;
  }

  function _removeZeros(string memory _string) pure private returns (string memory result) {
    bytes memory _stringBytes = bytes(_string);
    uint indexOfLastNonZero;

    for(uint i = _stringBytes.length-1; i>=0; i--) {
      if(_stringBytes[i]!=bytes('0')[0]) {
        indexOfLastNonZero = i;
        break;
        }
    }

    bytes memory resultBytes = new bytes(indexOfLastNonZero+1);

    for (uint i=0; i<=indexOfLastNonZero; i++){
      resultBytes[i]=_stringBytes[i];
    }
  
    result = string(resultBytes);
    return result;
  } 

  function addUsdSymbol(string memory fee) private pure returns (string memory result){
    bytes memory feeBytes = bytes(fee);
    bytes memory resultBytes = new bytes(feeBytes.length+1);

    for (uint i = 0; i<feeBytes.length+1; i++) {
      if (i == feeBytes.length) {
        resultBytes[i] = bytes('$')[0];}
        else {
      resultBytes[i] = feeBytes[i];
        }
    }

    result = string(resultBytes);
    return result;
  }

  modifier _onlyActivityOwner(uint _activityId){
    Activity storage activity = activities[_activityId];
    require(activity.owner == msg.sender, 'only owner');
    _;
  }

  modifier _sendEnoughEntranceFee(uint _activityId, string calldata _type){
    uint joinFeeInUsdWei = uint(_getJoinFeeInUsdWei(_activityId, _type));
    uint joinFee = _calculateJoinFee(joinFeeInUsdWei);
    console.log('join:', joinFee);
    require(msg.value >= joinFee, 'not enough entrance fee');
    require(msg.value < joinFee * 120 / 100, 'paid too much entrance fee');
    _;
  }

  modifier _activityExists(uint _id){
    require(_id <= activities.length-1, 'Activity doesnt exist.');

    _;
  }

  modifier _activityContinues(uint _activityId){
    Activity storage activity = activities[_activityId];
    require(activity.activityEnded != true, 'Activity ended.');
    _;
  }

  modifier _typeExists(uint _activityId, string calldata _type){
    Activity storage activity = activities[_activityId];
    bool typeExists = false;

    for (uint i=0; i<activity.attendanceTypes.length; i++){
      bytes32 s1 = keccak256(abi.encode(_type));
      bytes32 s2 = keccak256(abi.encode(activity.attendanceTypes[i]));

      if (s1 == s2){
        typeExists = true;
      }
    }

    console.log('Activity type checking..');
    require(typeExists, "Type doesnt exist.");

    _;
  }
}

