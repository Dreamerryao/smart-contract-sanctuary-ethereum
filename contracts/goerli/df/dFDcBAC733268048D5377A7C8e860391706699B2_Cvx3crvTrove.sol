// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import "./libraries/BoringRebase.sol";
import "./libraries/BoringERC20.sol";
import "./libraries/ArchimedesMath.sol";

import "hardhat/console.sol";

interface IOracle {
    function get() external view returns (bool, uint);
}

interface ISortedTroves {
    function upsert(address _user, uint256 _NCR, address _prevId, address _nextId) external;

    function getFirst() external view returns (address);
    function getLast() external view returns (address);

    function getNext(address _user) external view returns (address);
}

interface IVaultV1 {
    function toAmount(IERC20 token, uint256 share) external view returns (uint256);
    function toShare(IERC20 token, uint256 amount) external view returns (uint256);
    function transferShare(IERC20 token, address to, uint256 share) external returns (uint256);
    function transferShareFrom(IERC20 token, address from, uint256 share) external returns (uint256);
    function transferAmount(IERC20 token, address to, uint256 amount) external returns (uint256);
    function transferAmountFrom(IERC20 token, address from, uint256 amount) external returns (uint256);
    function addShare(IERC20 token, uint256 share) external returns (uint256);
    function addAmount(IERC20 token, uint256 amount) external returns (uint256);
}


contract Cvx3crvTrove {
    using RebaseLibrary for Rebase;
    using BoringERC20 for IERC20;

    event LogAccrue(uint256 accruedAmount);
    event LogAddCollateral(address indexed from, address indexed to, uint256 share);
    event LogWithdrawCollateral(address indexed from, address indexed to, uint256 share);
    event LogBorrow(address indexed from, address indexed to, uint256 amount, uint256 part);
    event LogRepay(address indexed from, address indexed to, uint256 amount, uint256 part);

    IVaultV1 public immutable vault;
    IERC20 public collateral;
    IERC20 public usdaToken;
    IOracle public oracle;
    ISortedTroves public sortedTroves;

    uint64 public systemDeploymentTime;

    address public feeTo;
    // Total amounts
    uint256 public totalCollateralShare; // Total collateral supplied
    Rebase public totalBorrow; // elastic = Total token amount to be repayed by borrowers, base = Total parts of the debt held by borrowers

    // User balances
    mapping(address => uint256) public userCollateralShare;
    mapping(address => uint256) public userBorrowPart;    // base + interest

    /// @notice Exchange and interest rate tracking.
    /// This is 'cached' here because calls to Oracles can be very expensive.
    uint256 public exchangeRate;

    struct AccrueInfo {
        uint64 lastAccrued;
        uint256 feesEarned;
    }

    AccrueInfo public accrueInfo;

    // Settings
    uint256 public constant DECIMAL_PRECISION = 1e18;

    uint256 public constant APR = DECIMAL_PRECISION / 1000 * 25;  // 2.5%
    uint256 private constant INTEREST_PER_SECOND = 317097920;

    uint256 public constant BORROW_OPENING_FEE = DECIMAL_PRECISION / 100 * 1; // 1%

    uint256 public constant MCR = DECIMAL_PRECISION / 100 * 80; // 80%

    uint256 public constant MaxVCR = DECIMAL_PRECISION / 100 * 90; // 90%

    // uint256 private constant COLLATERIZATION_RATE_PRECISION = 1e5; // Must be less than EXCHANGE_RATE_PRECISION (due to optimization in math)

    uint256 public constant LIQUIDATION_RATIO = DECIMAL_PRECISION / 100 * 90;  // 90%

    uint256 public constant LIQUIDATION_FEE = DECIMAL_PRECISION / 100 * 4; // 4%

    constructor(
        IVaultV1 vault_, 
        IERC20 usdaToken_, 
        IERC20 collateralToken_, 
        IOracle oracle_,
        ISortedTroves sortedTroves_
    ) {
        vault = vault_;
        usdaToken = usdaToken_;
        oracle = oracle_;
        collateral = collateralToken_;
        sortedTroves = sortedTroves_;
        systemDeploymentTime = uint64(block.timestamp);
    }

    function updateExchangeRate() public returns (bool updated, uint256 rate) {
        (updated, rate) = oracle.get();

        if (updated) {
            exchangeRate = rate;
            // emit LogExchangeRate(rate);
        } else {
            // Return the old rate if fetching wasn't successful
            rate = exchangeRate;
        }
    }

    /// @notice Accrues the interest on the borrowed tokens and handles the accumulation of fees.
    function accrue() public {
        AccrueInfo memory _accrueInfo = accrueInfo;
        // Number of seconds since accrue was called
        uint256 elapsedTime = uint256(uint64(block.timestamp) - _accrueInfo.lastAccrued);
        if (elapsedTime == 0) {
            return;
        }
        _accrueInfo.lastAccrued = uint64(block.timestamp);

        Rebase memory _totalBorrow = totalBorrow;
        if (_totalBorrow.base == 0) {
            accrueInfo = _accrueInfo;
            return;
        }

        // Accrue interest
        uint256 extraAmount = _totalBorrow.elastic * INTEREST_PER_SECOND * elapsedTime / DECIMAL_PRECISION;
        
        _totalBorrow.elastic = _totalBorrow.elastic + extraAmount;

        _accrueInfo.feesEarned = _accrueInfo.feesEarned + extraAmount;
        totalBorrow = _totalBorrow;
        accrueInfo = _accrueInfo;

        emit LogAccrue(extraAmount);
    }

    // function _addTokens(
    //     IERC20 token,
    //     uint256 share,
    //     uint256 total,
    //     bool skim
    // ) internal {
    //     // if (skim) {
    //     //     require(share <= vault.balanceOf(token, address(this)).sub(total), "Cauldron: Skim too much");
    //     // } else {
    //     //     vault.transferShareFrom(token, msg.sender, share);
    //     // }
    //     vault.transferShareFrom(token, msg.sender, share);
    // }

    function _addCollateral(
        address to,
        uint256 share
    ) internal {
        userCollateralShare[to] = userCollateralShare[to] + share;
        uint256 oldTotalCollateralShare = totalCollateralShare;
        totalCollateralShare = oldTotalCollateralShare + share;

        // _addTokens(collateral, share, oldTotalCollateralShare, skim);
        vault.transferShareFrom(collateral, msg.sender, share);

        emit LogAddCollateral(to, address(vault), share);
    }

    function _withdrawCollateral(address to, uint256 share) internal {
        userCollateralShare[to] = userCollateralShare[to] - share;
        totalCollateralShare = totalCollateralShare - share;

        // sorted for redeem
        uint256 _NCR = getNominalICR(to);
        sortedTroves.upsert(to, _NCR, address(0), sortedTroves.getLast());

        vault.transferShare(collateral, to, share);

        emit LogWithdrawCollateral(address(vault), to, share);
    }

    function withdrawCollateral(address to, uint256 share) external {
        // accrue must be called because we check solvency
        accrue();
        _withdrawCollateral(to, share);
    }

    function _borrow(address to, uint256 amount) internal returns (uint256 part, uint256 share) {
        uint256 collAmount = userCollateralShare[msg.sender];
        require(amount * exchangeRate <= collAmount * MCR, "Trove: great at MCR");
        uint256 feeAmount = amount * BORROW_OPENING_FEE / DECIMAL_PRECISION;
        console.log("borrow fee", feeAmount);
        // (totalBorrow, part) = totalBorrow.add(amount, true);
        (totalBorrow, part) = totalBorrow.add(amount, true);
        console.log("totalBorrow", part);
        userBorrowPart[to] = userBorrowPart[to] + part;
        console.log("userBorrow", userBorrowPart[to]);
        // As long as there are tokens on this contract you can 'mint'... this enables limiting borrows
        share = vault.toShare(usdaToken, amount - feeAmount);
        console.log("share", share);

        vault.transferShare(usdaToken, to, share);

        emit LogBorrow(msg.sender, to, amount, part);
    }

    // function borrow(address to, uint256 amount) external returns (uint256 part, uint256 share) {
    //     accrue();
    //     (part, share) = _borrow(to, amount);
    // }

    function multiBorrow(address to, uint _inAmount, uint _outAmount) external  returns (uint part, uint share) {
        require(_inAmount + _outAmount > 0, "Trove: not all zero");
        if (_inAmount > 0 && _outAmount == 0) {
            // only add collateral
            _addCollateral(to, _inAmount);
            part = userBorrowPart[to];
            share = userCollateralShare[to];
        } else {
            accrue();
            if (_inAmount > 0) {
                // add collateral before borrow
                _addCollateral(to, _inAmount);
            }
            // borrow
            (part, share) = _borrow(to, _outAmount);
        }

        // sorted for redeem
        uint256 _NCR = getNominalICR(to);
        console.log("NCR", _NCR);
        sortedTroves.upsert(to, _NCR, address(0), sortedTroves.getLast());
    }

    function _repay(
        address to,
        uint256 part
    ) internal returns (uint256 amount) {
        (totalBorrow, amount) = totalBorrow.sub(part, true);
        userBorrowPart[to] = userBorrowPart[to] - part;

        console.log("repay elastic amount", amount);
        uint256 share = vault.toShare(usdaToken, amount);
        
        vault.transferShareFrom(usdaToken, msg.sender, share);

        // sorted for redeem
        uint256 _NCR = getNominalICR(to);
        sortedTroves.upsert(to, _NCR, address(0), sortedTroves.getLast());

        // console.log("Repay", address(vault), amount, part);
        emit LogRepay(address(vault), to, amount, part);
    }

    // function repay(
    //     address to,
    //     uint256 part
    // ) public returns (uint256 amount) {
    //     accrue();
    //     amount = _repay(to, part);
    // }
    function repay(
        address to,
        uint256 _part,
        uint256 _outAmount
    ) public returns (uint256 amount) {
        accrue();
        amount = _repay(to, _part);
        if (_outAmount > 0) {
            _withdrawCollateral(to, _outAmount);
        } 
    }

    function withdrawFees() external {
        accrue();
        uint256 _feesEarned = accrueInfo.feesEarned;
        vault.transferAmount(usdaToken, feeTo, _feesEarned);
        accrueInfo.feesEarned = 0;

    }

    function setFeeTo(address newFeeTo) external {
        feeTo = newFeeTo;
        // emit LogFeeTo(newFeeTo);
    }

    // (抵押品价格 Collateral Price - 清算价格 Liquidation Price) / 抵押品价格Collateral Price
    function getPositionHealth(address _user) public view returns (uint256) {
        uint256 _collShare = userCollateralShare[_user];
        uint256 _debtPart = userBorrowPart[_user];
        if (_debtPart == 0 || _collShare == 0) {
            return DECIMAL_PRECISION;
        }
        uint256 _CR = _calcPositionCR(_collShare, _debtPart);
        return exchangeRate * _CR / LIQUIDATION_RATIO;
    }

    function _calcPositionCR(uint256 _collShare, uint256 _debtPart) internal pure returns (uint256) {
        return _debtPart * DECIMAL_PRECISION / _collShare;
    }

    function getVCR() public view returns (uint256) {
        if (totalCollateralShare == 0 || totalBorrow.elastic == 0) {
            return 0;
        }
        return totalBorrow.elastic * DECIMAL_PRECISION / totalCollateralShare;
    }

    function isNormalMode() external view returns (bool) {
        if (MaxVCR > getVCR()) {
            return true;
        } else {
            return false;
        }
    }

    function getBorrowSaturation() external view returns (uint256) {
        uint256 _base = totalBorrow.base;
        uint256 _coll = totalCollateralShare;
        if (_coll > 0) {
            return _base * DECIMAL_PRECISION * DECIMAL_PRECISION / (_coll * MaxVCR);
        } else {
            return DECIMAL_PRECISION / 100 * 1;
        }
        
    }

    function getNominalICR(address _user) public view returns (uint256) {
        uint256 _collShare = userCollateralShare[_user];
        uint256 _debtPart = userBorrowPart[_user];
        if (_debtPart == 0 || _collShare == 0) {
            return 0;
        }
        uint256 _CR = _debtPart * DECIMAL_PRECISION / _collShare;
        return _CR;
    }

    // redeem
    function _requireAfterBootstrapPeriod() internal view {
        require(
            block.timestamp >= systemDeploymentTime + BOOTSTRAP_PERIOD, 
            "Redemptions are not allowed during bootstrap phase"
        );
    }

    struct RedemptionTotals {
        uint remainingUSDA;
        uint totalUSDARedeem;
        uint totalCollDrawn;
        uint redeemFee;
        uint collToSendToRedeemer;
        uint totalUSDASupplyAtStart;
    }

    struct SingleRedemptionValues {
        uint USDALot;
        uint CollLot;
        bool cancelledPartial;
    }

    uint public baseRate;
    uint public lastFeeOperationTime;

    uint public constant REDEMPTION_FEE_FLOOR = DECIMAL_PRECISION / 1000 * 5; // 0.5%

    function redeemCollateral(uint _USDAamount) external {
        // check conditions
        _requireAfterBootstrapPeriod();
        require(_USDAamount > 0, "RedeemOperations: Redeem USDA amount less than 0");
        require(usdaToken.balanceOf(msg.sender) > _USDAamount, "RedeemOperations: USDA balance less than redeem");
        
        RedemptionTotals memory totals;

        totals.totalUSDASupplyAtStart = usdaToken.totalSupply();
        // calc redeem fee with redeemedUSDA
        uint _redemptionRate = _getRedemptionRateWithDecay(_USDAamount, totals.totalUSDASupplyAtStart);
        totals.redeemFee = _USDAamount * _redemptionRate / DECIMAL_PRECISION;

        totals.remainingUSDA = _USDAamount - totals.redeemFee;
        address currentId = sortedTroves.getFirst();

        // TODO: support for redeem fees to each redeemed address
        // address[] memory redeemedBorrowers = new address[](maxRedeemSize);
        // uint[] memory collOfBorrowers = new uint[](maxRedeemSize);
        address[100] memory redeemedBorrowers;
        uint[100] memory debtOfBorrowers;

        uint index = 0;

        // uint _maxIterations = maxRedeemSize;
        uint _maxIterations = 100;

        while (currentId != address(0) && totals.remainingUSDA > 0 && _maxIterations > 0) {
            _maxIterations--;
            console.log("=== loop / remainingUSDA", _maxIterations, totals.remainingUSDA);
            address nextNode = sortedTroves.getNext(currentId);
            // SingleRedemptionValues memory singleRedeem;
            SingleRedemptionValues memory singleRedeem = _redeemCollateral(
                currentId,
                totals.remainingUSDA,
                exchangeRate
            );

            if (totals.remainingUSDA < DECIMAL_PRECISION) break;

            totals.totalUSDARedeem = totals.totalUSDARedeem + singleRedeem.USDALot;
            totals.totalCollDrawn = totals.totalCollDrawn + singleRedeem.CollLot;
            totals.remainingUSDA = totals.remainingUSDA - singleRedeem.USDALot;

            // TODO: record redeemed info
            redeemedBorrowers[index] = currentId;
            debtOfBorrowers[index] = singleRedeem.USDALot;
            index++;

            currentId = nextNode;
        }

        assert(totals.totalUSDARedeem < _USDAamount);
        _redemptionRate = _updateBaseRateFromRedemption(totals.totalUSDARedeem, totals.totalUSDASupplyAtStart);
        totals.redeemFee = totals.totalUSDARedeem * _redemptionRate / DECIMAL_PRECISION;
        assert(totals.totalUSDARedeem + totals.redeemFee <= _USDAamount);
        // TODO: send collateral interface
        console.log("=== coll to send", totals.totalCollDrawn);
        vault.transferShare(collateral, msg.sender, totals.totalCollDrawn);
        // TODO: burn usda interface
        console.log("usda to redeem", totals.totalUSDARedeem);
        uint256 share = vault.toShare(usdaToken, totals.totalUSDARedeem);
        vault.transferShareFrom(usdaToken, msg.sender, share);

        console.log("=== redeemFee to distribute", totals.redeemFee);
        // TODO: distribute redeem fee 
        for (uint idx = 0; idx < index; idx++) {
            uint debtValue = debtOfBorrowers[idx];
            address feesTo = redeemedBorrowers[idx];
            if (debtValue > 0 && feesTo != address(0)) {
                // everyone redeemed user to  
                uint256 fees = _redemptionRate * debtValue / DECIMAL_PRECISION;
                console.log("--> feeto, fees", feesTo, fees);
                uint256 feeShare = vault.toShare(usdaToken, fees);
                vault.transferShare(usdaToken, feesTo, feeShare);
            }
        }

        // emit Redemption(_USDAamount, totals.totalUSDARedeem, totals.totalCollDrawn, totals.redeemFee);
    }

    function _redeemCollateral(address _borrower, uint _maxUSADAmount, uint _exchangeRate) internal returns (SingleRedemptionValues memory singleRedeem) {
        uint elastic = totalBorrow.toElastic(userBorrowPart[_borrower], true);
        singleRedeem.USDALot = ArchimedesMath._min(_maxUSADAmount, elastic);
        singleRedeem.CollLot = singleRedeem.USDALot * _exchangeRate / DECIMAL_PRECISION;
        uint part = totalBorrow.toBase(singleRedeem.USDALot, false);

        uint newDebt = userBorrowPart[_borrower] - part;
        uint newColl = userCollateralShare[_borrower] - singleRedeem.CollLot;
        // console.log("--> after redeem", newDebt, newColl);

        totalCollateralShare = totalCollateralShare - singleRedeem.CollLot;
        (totalBorrow, ) = totalBorrow.sub(part, true);

        if (newDebt == 0) {
            userBorrowPart[_borrower] = 0;
            userCollateralShare[_borrower] = 0;
            sortedTroves.upsert(_borrower, 0, address(0), address(0));
        } else {
            userBorrowPart[_borrower] = newDebt;
            userCollateralShare[_borrower] = newColl;
            uint _NCR = getNominalICR(_borrower);
            sortedTroves.upsert(_borrower, _NCR, address(0), sortedTroves.getLast());
        }
    }

    // --- BaseRate ---

    uint64 public constant BOOTSTRAP_PERIOD = 0 days;
    uint constant public SECONDS_IN_ONE_MINUTE = 60;
    // (1/2) = d^720 => d = (1/2)^(1/720)
    uint constant public MINUTE_DECAY_FACTOR = 999037758833783000;
    uint constant public BETA = 2;

    function _getRedemptionRateWithDecay(uint _redeemedUSDA, uint _totalUSDASupply) internal view returns (uint) {
        uint decayedBaseRate = _calcDecayedBaseRate();
        uint redeemedUSDAFraction = _redeemedUSDA * DECIMAL_PRECISION / _totalUSDASupply;
        uint newBaseRate = decayedBaseRate + (redeemedUSDAFraction / BETA);
        newBaseRate = ArchimedesMath._min(newBaseRate, DECIMAL_PRECISION);

        assert(newBaseRate > 0);
        return _calcRedmptionRate(newBaseRate);
    }

    function _updateBaseRateFromRedemption(
        uint _redeemedUSDA,
        uint _totalUSDASupply
    ) internal returns (uint) {
        // _requireCallerIsAllowed();
        uint decayedBaseRate = _calcDecayedBaseRate();

        uint redeemedUSDAFraction = _redeemedUSDA * DECIMAL_PRECISION / _totalUSDASupply;
        uint newBaseRate = decayedBaseRate + redeemedUSDAFraction / BETA;
        newBaseRate = ArchimedesMath._min(newBaseRate, DECIMAL_PRECISION);

        assert(newBaseRate > 0);
        baseRate = newBaseRate;

        // emit BaseRateUpdated(newBaseRate);

        _updateLastFeeOpTime();

        return newBaseRate;
    }

    function _calcRedmptionRate(uint _baseRate) internal pure returns (uint) {
        return
            ArchimedesMath._min(
                REDEMPTION_FEE_FLOOR + _baseRate,
                DECIMAL_PRECISION
            );
    }

    function _updateLastFeeOpTime() internal {
        uint timePassed = block.timestamp - lastFeeOperationTime;

        if (timePassed >= SECONDS_IN_ONE_MINUTE) {
            lastFeeOperationTime = block.timestamp;
            // emit LastFeeOpTimeUpdated(block.timestamp);
        }
    }

    function _calcDecayedBaseRate() internal view returns (uint) {
        uint minutesPassed = _minutesPassedSinceLastFeeOp();
        uint decayFactor = ArchimedesMath._decPow(
            MINUTE_DECAY_FACTOR,
            minutesPassed
        );
        return baseRate * decayFactor / DECIMAL_PRECISION;
    }

    function _minutesPassedSinceLastFeeOp() internal view returns (uint) {
        return (block.timestamp - lastFeeOperationTime) / SECONDS_IN_ONE_MINUTE;
    }


}

