// SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./IERC20.sol";

import "hardhat/console.sol";
import "./AccessControl.sol";


/// @title Trains Contract
/// @author TODO: add author
/// @notice TODO: add description
contract Trains is AccessControl, Pausable {

    /// Vars

    /// @notice Wallet -> ticket id's mapping
    mapping(address => uint[]) private _userTickets;
    /// @notice Wallet -> funds mapping
    mapping(address => uint) private _userFunds;
    /// @notice Current ticket prices
    uint[] private _ticketPrices;
    /// @notice Allowed current ticket counts
    uint[] private _ticketCounts;
    /// @notice Next ticket prices
    uint[] private _nextTicketPrices;
    /// @notice Allowed next ticket counts
    uint[] private _nextTicketCounts;

    /// @notice Withdraw allowance flag
    bool private _isWithdrawEnabled;
    /// @notice USDT contract address
    address private _usdtContract;


    /// Events

    /// @notice Emits when available ticket amounts is set
    /// @param isEnabled Withdraw allowance flag
    event WithdrawEnabled(bool isEnabled);

    /// @notice Emits when available ticket amounts is set
    /// @param prices Ticket prices array
    /// @param counts Ticket counts array
    /// @param nextPrices Ticket prices array for next trip
    /// @param nextCounts Ticket counts array for next trip
    event TicketPricesSet(uint[] prices, uint[] counts, uint[] nextPrices, uint[] nextCounts);

    /// @notice Emits when Ticket ID is added to user Tickets
    /// @param to Ticket owner wallet address
    /// @param id Ticket id
    event TicketAdded(address to, uint id);

    /// @notice Emits when user requests to opens a Ticket(s)
    /// @param to Ticket owner wallet address
    /// @param typeId Ticket type id (<index in tickets array> + 1)
    /// @param referrerId Referrer Ticket id
    /// @param count Tickets count
    /// @param value Sent value
    event TicketOpenRequest(address to, bool isNext, uint8 typeId, uint referrerId, uint count, uint value);

    /// @notice Emits when Ticket is closed and funds with interest are added to the user's funds
    /// @param to Ticket owner wallet address
    /// @param id Ticket id
    /// @param amount Earned amount
    event TicketClosed(address to, uint id, uint amount);

    /// @notice Emits when user withdrawn his funds
    /// @param sender Funds owner wallet address
    /// @param to Wallet address to withdraw funds
    /// @param amount Amount to withdraw
    event FundsWithdrawn(address sender, address to, uint amount);

    /// @notice Emits when somebody replenishes contract funds
    event FundsReplenished(address sender, uint amount);


    /// @notice Contract constructor
    /// @param ceoParam CEO wallet address
    /// @param cfoParam CFO wallet address
    /// @param cooParam COO wallet address
    /// @param usdtContractParam USDT contract address
    /// @param prices Ticket prices array in USDT
    /// @param prices Ticket counts array
    /// @param nextPrices Ticket for next trip prices array in USDT
    /// @param nextCounts Allowed for next trip counts array
    constructor(
        address ceoParam,
        address cfoParam,
        address cooParam,
        address usdtContractParam,
        uint[] memory prices,
        uint[] memory counts,
        uint[] memory nextPrices,
        uint[] memory nextCounts
    ) AccessControl(ceoParam, cfoParam, cooParam) {

        _usdtContract = usdtContractParam;

        require(prices.length == counts.length, "Trains: invalid length");
        require(nextPrices.length == nextCounts.length, "Trains: invalid length");

        _ticketPrices = prices;
        _ticketCounts = counts;
        _nextTicketPrices = nextPrices;
        _nextTicketCounts = nextCounts;

        emit TicketPricesSet(_ticketPrices, _ticketCounts, _nextTicketPrices, _nextTicketCounts);
    }

    /// Modifiers

    /// @notice Allows withdraw only when _isWithdrawEnabled == true
    modifier whenWithdrawEnabled() {
        require(_isWithdrawEnabled, "Trains: withdraw disabled");
        _;
    }

    /// @notice Pauses contract and activate whenNotPaused modifier
    function pause() public onlyCOO {
        _pause();
    }

    /// @notice Unpauses contract
    function unpause() public onlyCOO {
        _unpause();
    }

    /// @notice Set USDT contract address
    /// @param usdtContractParam USDT contract address
    function setUSDTContract(address usdtContractParam) external onlyCFO {
        _usdtContract = usdtContractParam;
    }

    /// @notice Returns USDT contract address
    /// @return USDT contract address
    function usdtContract() public view returns(address) {
        return _usdtContract;
    }

    /// @notice Set withdraw allowance flag
    /// @param value Withdrawal allowance flag
    function setWithdrawEnabled(bool value) external onlyCFO {
        _isWithdrawEnabled = value;
        emit WithdrawEnabled(value);
    }

    /// @notice Returns withdraw allowance flag
    /// @return Withdraw allowance flag
    function withdrawEnabled() public view returns(bool) {
        return _isWithdrawEnabled;
    }

    /// @notice Returns allowed ticket counts
    /// @return Ticket counts array
    function ticketCounts() public view returns(uint[] memory) {
        return _ticketCounts;
    }

    /// @notice Returns allowed ticket counts for next trip
    /// @return Ticket counts array
    function nextTicketCounts() public view returns(uint[] memory) {
        return _nextTicketCounts;
    }

    /// @notice Returns ticket prices
    /// @return Ticket prices array in USDT
    function ticketPrices() public view returns(uint[] memory) {
        return _ticketPrices;
    }

    /// @notice Returns ticket prices for next trip
    /// @return Ticket prices array in USDT
    function nextTicketPrices() public view returns(uint[] memory) {
        return _nextTicketPrices;
    }

    /// @notice Sets allowed ticket amounts
    /// @param prices Ticket prices array in USDT
    /// @param nextPrices Ticket for next trip prices array in USDT
    /// @param nextCounts Allowed for next trip counts array
    function setTicketPrices(
        uint[] calldata prices,
        uint[] calldata nextPrices,
        uint[] calldata nextCounts
    ) external onlyCOO {

        require(prices.length == _nextTicketCounts.length, "Trains: invalid length");
        require(nextPrices.length == nextCounts.length, "Trains: invalid length");

        _ticketPrices = prices;
        _ticketCounts = _nextTicketCounts;
        _nextTicketPrices = nextPrices;
        _nextTicketCounts = nextCounts;

        emit TicketPricesSet(_ticketPrices, _ticketCounts, _nextTicketPrices, _nextTicketCounts);
    }

    /// @notice Adds ticket id to user tickets after creating it in database and calculating ticket data
    /// @param to Ticket owner wallet address
    /// @param id Ticket id
    function addTicket(address to, uint id) external onlyCOO {
        _userTickets[to].push(id);

        emit TicketAdded(to, id);
    }

    /// @notice Closes user ticket and adds ticket amount with interest to user funds
    /// @param owner Ticket owner wallet address
    /// @param id Ticket id
    /// @param amount Earned amount in USDT
    function closeTicket(address owner, uint id, uint amount) external onlyCOO {
        /// Find and delete ticket id from user tickets array
        uint len = _userTickets[owner].length;
        for(uint i = 0; i < len; i++) {
            if(_userTickets[owner][i] == id) {
                _userTickets[owner][i] = _userTickets[owner][len - 1];
                _userTickets[owner].pop();

                /// Add ticket amount to user funds
                _userFunds[owner] += amount;

                emit TicketClosed(owner, id, amount);

                return;
            }
        }

        /// Revert if ticket id doesn't found
        revert("Trains: ticket not found");
    }

    function addFunds(address owner, uint amount) external onlyCFO {
        /// Add amount to user funds
        _userFunds[owner] -= amount;
    }

    /// @notice Creates new ticket
    /// @param typeId Ticket type id (<index in tickets array> + 1)
    /// @param referrerTicketId Referrer ticket id
    /// @param count Tickets count
    function ticket(bool isNext, uint8 typeId, uint referrerTicketId, uint count) external whenNotPaused  {

        IERC20 usdt = IERC20(_usdtContract);
        uint amount;

        if (isNext) {
            require(typeId > 0 && typeId <= _nextTicketPrices.length, "Trains: invalid ticket type");
            require(_nextTicketCounts[typeId-1] >= count, "Trains: no more tickets available");
            _nextTicketCounts[typeId-1] = _nextTicketCounts[typeId-1] - count;
            amount = _nextTicketPrices[typeId-1] * count;
        } else {
            require(typeId > 0 && typeId <= _ticketPrices.length, "Trains: invalid ticket type");
            require(_ticketCounts[typeId-1] >= count, "Trains: no more tickets available");
            _ticketCounts[typeId-1] = _ticketCounts[typeId-1] - count;
            amount = _ticketPrices[typeId-1] * count;
        }

        uint allowance = usdt.allowance(msg.sender, address(this));
        require(allowance >= amount, "Trains: not enough token allowance");

        usdt.transferFrom(msg.sender, address(this), amount);

        emit TicketOpenRequest(msg.sender, isNext, typeId, referrerTicketId, count, amount);
    }

    /// @notice Returns user funds in USDT
    /// @param userWallet Funds owner wallet address
    /// @return User funds in USDT
    function getUserFunds(address userWallet) public view returns (uint) {
        return _userFunds[userWallet];
    }

    /// @notice Withdraws user funds
    /// @param to Wallet address to withdraw funds
    /// @param amount Amount to withdraw in USDT
    function withdrawFunds(address payable to, uint amount) external whenWithdrawEnabled {
        require(_userFunds[msg.sender] >= amount, "Trains: insufficient funds");

        IERC20 usdt = IERC20(_usdtContract);

        usdt.transfer(to, amount);
        _userFunds[msg.sender] = _userFunds[msg.sender] - amount;
    }

    /// @notice Returns wallet ticket ids
    /// @param userWallet Tickets owner wallet address
    /// @return Ticket ids array
    function getUserTickets(address userWallet) public view returns (uint[] memory) {
        return _userTickets[userWallet];
    }

    /// @notice Sends surplus funds to the reserve fund
    /// @param to Wallet address to send funds
    /// @param amount Amount to send in USDT
    function withdrawUSDT(address payable to, uint amount) external onlyCFO {
        IERC20 usdt = IERC20(_usdtContract);
        usdt.transfer(to, amount);
    }
}

