        require(_cfo != address(0), "Trains: new CFO is the zero address");
        _CFO = _cfo;
        emit CFOSet(_cfo);
    }

    /// @notice Returns COO wallet address
    /// @return COO wallet address
    function coo() public view returns (address) {
        return _COO;
    }

    /// @notice Sets COO wallet address
    /// @param _coo COO wallet address
    function setCOO(address _coo) external onlyCEO {
        require(_coo != address(0), "Trains: new COO is the zero address");
        _COO = _coo;
        emit COOSet(_coo);
