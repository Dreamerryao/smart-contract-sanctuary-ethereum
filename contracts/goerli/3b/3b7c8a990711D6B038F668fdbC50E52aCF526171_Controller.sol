pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";

contract Escrow is Ownable {
    uint256 public affirmativeFunds;
    uint256 public negativeFunds;
    address[] public affirmativeParticipants;
    address[] public negativeParticipants;
    bytes32 public resolutionHash;
    bool public settled;
    bool public refunded;
    mapping(address => bool) public participants;

    constructor(bytes32 _resolutionHash) {
        resolutionHash = _resolutionHash;
    }

    function fund(address _participant, bool _affirmative)
        public
        payable
        onlyOwner
    {
        require(!participants[_participant], "You have already funded");
        require(!settled, "The escrow has already been settled");
        require(!refunded, "This escrow has been refunded");
        if (_affirmative) {
            affirmativeFunds += msg.value;
            affirmativeParticipants.push(_participant);
        } else {
            negativeFunds += msg.value;
            negativeParticipants.push(_participant);
        }
        participants[_participant] = true;
    }

    function settle(bool _affirmative) public onlyOwner {
        require(!settled, "Escrow has already been settled");
        require(!refunded, "Escrow has been refunded");
        uint256 totalFunds = affirmativeFunds + negativeFunds;
        if (_affirmative) {
            for (uint256 i = 0; i < affirmativeParticipants.length; i++) {
                payable(affirmativeParticipants[i]).transfer(
                    totalFunds / affirmativeParticipants.length
                );
            }
        } else {
            for (uint256 i = 0; i < negativeParticipants.length; i++) {
                payable(negativeParticipants[i]).transfer(
                    totalFunds / negativeParticipants.length
                );
            }
        }
        affirmativeFunds = 0;
        negativeFunds = 0;
        settled = true;
    }

    function refund() public onlyOwner {
        require(!settled, "Escrow has already been settled");
        // Refund all the money
        for (uint256 i = 0; i < affirmativeParticipants.length; i++) {
            payable(affirmativeParticipants[i]).transfer(
                affirmativeFunds / affirmativeParticipants.length
            );
        }
        for (uint256 i = 0; i < negativeParticipants.length; i++) {
            payable(negativeParticipants[i]).transfer(
                negativeFunds / negativeParticipants.length
            );
        }
        refunded = true;
    }

    function getParticipants()
        public
        view
        returns (address[] memory _affirmative, address[] memory _negative)
    {
        return (affirmativeParticipants, negativeParticipants);
    }

    receive() external payable onlyOwner {}

    fallback() external {}
}

