pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "../Escrow/EscrowFactory.sol";
import "../Escrow/Escrow.sol";
import "../Resolution/ResolutionFactory.sol";
import "../Resolution/Resolution.sol";
import "./IBet.sol";

contract Bet is Ownable, IBet {
    bytes32 public resolutionHash; // The hash of the resolution
    address public creator; // The address of the creator
    uint256 public expiration; // When the bet expires
    uint256 public numPerSide; // How many participants per side
    uint256 public negativeBuyIn; // How much to buy in for the negative side
    uint256 public affirmativeBuyIn; // How much to buy in for the affirmative side
    uint256 public affirmativeTakers;
    uint256 public negativeTakers;
    Status public status;
    Escrow escrow;
    Resolution resolution;

    constructor(
        bytes32 _resolution,
        address _creator,
        uint256 _expiration,
        uint256 _numPerSide,
        uint256 _negativeBuyIn,
        uint256 _affirmativeBuyIn,
        uint256 _minApprovals,
        address _resolutionFactory,
        address _escrowFactory
    ) {
        resolutionHash = _resolution;
        expiration = _expiration;
        numPerSide = _numPerSide;
        negativeBuyIn = _negativeBuyIn;
        affirmativeBuyIn = _affirmativeBuyIn;
        creator = _creator;
        resolution = ResolutionFactory(_resolutionFactory).createResolution(
            _minApprovals,
            _numPerSide * 2,
            resolutionHash
        ); // Assume no arbiter for now
        escrow = EscrowFactory(_escrowFactory).createEscrow(resolutionHash);

        status = Status.Open;
    }

    function join(address _participant, bool _side) public payable onlyOwner {
        require(status == Status.Open, "Bet is not open");
        require(block.timestamp < expiration, "Bet has expired");
        require(
            _side ? affirmativeBuyIn == msg.value : negativeBuyIn == msg.value,
            "Incorrect buy in amount"
        );
        require(
            _side
                ? affirmativeTakers < numPerSide
                : negativeTakers < numPerSide,
            "Bet is full"
        );
        escrow.fund{value: msg.value}(_participant, _side);
        _side ? affirmativeTakers++ : negativeTakers++;

        if (affirmativeTakers == numPerSide && negativeTakers == numPerSide) {
            status = Status.Closed;
        }
    }

    function resolve(address _resolver, bool _affirmative) public onlyOwner {
        require(status == Status.Closed, "Bet is not closed");
        resolution.vote(_resolver, _affirmative);
        bool _resolved = resolution.resolved();
        if (_resolved) {
            escrow.settle(resolution.resolution());
            status = Status.Settled;
        }
        if (resolution.void()) {
            escrow.refund();
            status = Status.Voided;
        }
    }

    function getInfo() public view returns (BetInfo memory) {
        (
            address[] memory _affirmativeParticipants,
            address[] memory _negativeParticipants
        ) = escrow.getParticipants();
        return
            BetInfo(
                resolutionHash,
                creator,
                expiration,
                status,
                _affirmativeParticipants,
                _negativeParticipants
            );
    }
}

