import "@openzeppelin/contracts/access/Ownable.sol";
import "./Escrow.sol";

contract EscrowFactory is Ownable {
    constructor() {}

    function createEscrow(bytes32 _resolutionHash) public returns (Escrow) {
        Escrow escrow = new Escrow(_resolutionHash);
        escrow.transferOwnership(msg.sender);
        return escrow;
    }
}

