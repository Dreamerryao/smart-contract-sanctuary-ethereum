pragma solidity ^0.8.9;

interface IBet {
    enum Status {
        Open,
        Closed,
        Settled,
        Voided
    }
    struct BetInfo {
        bytes32 resolutionHash;
        address owner;
        uint256 expiration;
        Status status;
        address[] affirmativeParticipants;
        address[] negativeParticipants;
    }
}

