pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./Resolution.sol";

contract ResolutionFactory is Ownable {
    constructor() {}

    function createResolution(
        uint256 _minVotesToResolve,
        uint256 _maxVoters,
        bytes32 _resolutionHash
    ) public returns (Resolution) {
        Resolution resolution = new Resolution(
            _minVotesToResolve,
            _maxVoters,
            _resolutionHash
        );
        resolution.transferOwnership(msg.sender);
        return resolution;
    }
