pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";

contract Resolution is Ownable {
    bool public resolution; // What was the outcome of the bet? For or against the resultion
    bool public resolved = false; // If the vote has been resolved
    bool public void; // If the resolve is void
    bytes32 public resolutionHash; // The hash of the resolution
    uint256 public minVotesToResolve; // How many votes were needed to resolve the bet
    uint256 public maxVoters; // Maximum number of voters who can vote on the bet
    mapping(address => bool) public voters; // Who has voted
    mapping(bool => uint256) public votes; // How many votes for each outcome

    constructor(
        uint256 _minVotesToResolve,
        uint256 _maxVoters,
        bytes32 _resolutionHash
    ) {
        minVotesToResolve = _minVotesToResolve;
        maxVoters = _maxVoters;
        resolutionHash = _resolutionHash;
    }

    function vote(address _voter, bool _resolution) public onlyOwner {
        require(!voters[_voter], "Cannot vote twice");
        require(!resolved, "The bet has already been resolved");
        require(!void, "This bet has been voided");
        voters[msg.sender] = true;
        votes[_resolution] += 1;
        if (votes[_resolution] >= minVotesToResolve) {
            resolved = true;
            resolution = _resolution;
        }

        if (
            votes[true] + votes[false] == maxVoters &&
            votes[true] == votes[false]
        ) {
            void = true;
        }
    }
}

