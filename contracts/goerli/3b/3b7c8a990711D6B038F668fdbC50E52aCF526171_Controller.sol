pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./Bet/BetFactory.sol";
import "./Bet/IBet.sol";
import "hardhat/console.sol";

contract Controller is Ownable {
    using EnumerableSet for EnumerableSet.Bytes32Set;

    event BetCreated(bytes32 resolutionHash, string resolutionIpfs);
    event BetJoined(bytes32 resolutionHash, bool side);

    mapping(address => EnumerableSet.Bytes32Set) private ownedBets;
    mapping(address => EnumerableSet.Bytes32Set) private participatingBets;
    mapping(bytes32 => address) public resolutionHashToBet;
    mapping(string => bytes32) public resolutionIpfsToHash;
    mapping(bytes32 => string) public resolutionHashToIpfs;
    mapping(address => mapping(bytes32 => bool)) public betToSide;
    BetFactory betFactory;

    constructor(address _betFactory) {
        betFactory = BetFactory(_betFactory);
    }

    //***************************************
    //* Bet Creation
    //***************************************
    function createSimpleBet(
        string memory _resolutionIpfs,
        uint256 _expiration,
        uint256 _negativeBuyIn,
        uint256 _affirmativeBuyIn
    ) public {
        createBet(
            _resolutionIpfs,
            _expiration,
            1,
            _negativeBuyIn,
            _affirmativeBuyIn,
            2
        );
    }

    function createBet(
        string memory _resolutionIpfs,
        uint256 _expiration,
        uint256 _numPerSide,
        uint256 _negativeBuyIn,
        uint256 _affirmativeBuyIn,
        uint256 _minApprovals
    ) public {
        bytes32 resolutionHash = keccak256(abi.encode(_resolutionIpfs));
        Bet _bet = betFactory.createBet(
            resolutionHash,
            msg.sender,
            _expiration,
            _numPerSide,
            _negativeBuyIn,
            _affirmativeBuyIn,
            _minApprovals
        );
        ownedBets[msg.sender].add(resolutionHash);
        resolutionHashToBet[resolutionHash] = address(_bet);
        resolutionHashToIpfs[resolutionHash] = _resolutionIpfs;
        resolutionIpfsToHash[_resolutionIpfs] = resolutionHash;

        emit BetCreated(resolutionHash, _resolutionIpfs);
    }

    //***************************************
    //* Player Management
    //***************************************
    function joinBet(bytes32 _resolutionHash, bool _side) public payable {
        require(
            resolutionHashToBet[_resolutionHash] != address(0),
            "Bet does not exist"
        );
        Bet _bet = Bet(resolutionHashToBet[_resolutionHash]);
        _bet.join{value: msg.value}(msg.sender, _side);
        participatingBets[msg.sender].add(_resolutionHash);
        betToSide[msg.sender][_resolutionHash] = _side;
        emit BetJoined(_resolutionHash, _side);
    }

    function resolveBet(bytes32 _resolutionHash, bool _side)
        public
        onlyBetMember(_resolutionHash, msg.sender)
    {
        require(
            resolutionHashToBet[_resolutionHash] != address(0),
            "Bet does not exist"
        );
        Bet _bet = Bet(resolutionHashToBet[_resolutionHash]);
        _bet.resolve(msg.sender, _side);
    }

    function getBets(address _participant)
        public
        view
        returns (string[] memory, string[] memory)
    {
        string[] memory created = new string[](
            ownedBets[_participant].length()
        );
        string[] memory participating = new string[](
            participatingBets[_participant].length()
        );
        for (uint256 i = 0; i < ownedBets[_participant].length(); i++) {
            created[i] = resolutionHashToIpfs[ownedBets[_participant].at(i)];
        }
        for (uint256 i = 0; i < participatingBets[_participant].length(); i++) {
            participating[i] = resolutionHashToIpfs[
                participatingBets[_participant].at(i)
            ];
        }
        return (created, participating);
    }

    function getSide(bytes32 resolutionHash, address _participant)
        public
        view
        onlyBetMember(resolutionHash, _participant)
        returns (bool)
    {
        return betToSide[_participant][resolutionHash];
    }

    //***************************************
    // Bet Info
    //***************************************
    function getBetInfo(bytes32 _resolutionHash)
        public
        view
        returns (IBet.BetInfo memory)
    {
        require(
            resolutionHashToBet[_resolutionHash] != address(0),
            "Bet does not exist"
        );
        Bet _bet = Bet(resolutionHashToBet[_resolutionHash]);
        return _bet.getInfo();
    }

    //***************************************
    // Modifers
    //***************************************
    modifier onlyBetMember(bytes32 _resolutionHash, address _participant) {
        require(
            participatingBets[_participant].contains(_resolutionHash),
            "Not a member of this bet"
        );
        _;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./Bet.sol";

contract BetFactory is Ownable {
    address public escrowFactory;
    address public resolutionFactory;

    constructor(address _escrowFactory, address _resolutionFactory) {
        escrowFactory = _escrowFactory;
        resolutionFactory = _resolutionFactory;
    }

    function createBet(
        bytes32 _resolution,
        address _creator,
        uint256 _expiration,
        uint256 _numPerSide,
        uint256 _negativeBuyIn,
        uint256 _affirmativeBuyIn,
        uint256 _minApprovals
    ) public returns (Bet) {
        Bet bet = new Bet(
            _resolution,
            _creator,
            _expiration,
            _numPerSide,
            _negativeBuyIn,
            _affirmativeBuyIn,
            _minApprovals,
            resolutionFactory,
            escrowFactory
        );
        bet.transferOwnership(msg.sender);
        return bet;
    }
}

