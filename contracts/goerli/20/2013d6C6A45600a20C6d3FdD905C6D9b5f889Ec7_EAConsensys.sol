//SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Pausable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract EAConsensys is ERC20Pausable, Ownable {
  using EnumerableMap for EnumerableMap.AddressToUintMap;

  event MandatoryTransfer(
    address owner,
    address indexed from,
    address indexed to,
    uint256 value
  );
  // ERC20's _balance is private, need to use a new one for data showing purposes
  EnumerableMap.AddressToUintMap internal sharesMap;

  constructor() ERC20("EAConsensys", "EAC") {}

  function decimals() public pure override returns (uint8) {
    return 2;
  }

  function _increaseInMap(
    EnumerableMap.AddressToUintMap storage map_,
    address address_,
    uint256 amount_
  ) internal {
    uint256 prev;
    if (EnumerableMap.contains(map_, address_)) {
      prev = EnumerableMap.get(map_, address_);
    } else {
      prev = 0;
    }
    EnumerableMap.set(map_, address_, prev + amount_);
  }

  function _decreaseInMap(
    EnumerableMap.AddressToUintMap storage map_,
    address address_,
    uint256 amount_
  ) internal {
    uint256 prev = EnumerableMap.get(map_, address_);
    uint256 curr = prev - amount_;
    if (curr > 0) {
      EnumerableMap.set(map_, address_, curr);
    } else {
      EnumerableMap.remove(map_, address_);
    }
  }

  // Sync balances (share) with the changes of ERC20's _balances in transfer, mint, burn
  function _afterTokenTransfer(
    address from,
    address to,
    uint256 amount
  ) internal override {
    super._afterTokenTransfer(from, to, amount);
    if (from == address(0)) {
      // mint case
      _increaseInMap(sharesMap, to, amount);
    } else if (to == address(0)) {
      // burn case
      _decreaseInMap(sharesMap, from, amount);
    } else {
      // transfer case
      _increaseInMap(sharesMap, to, amount);
      _decreaseInMap(sharesMap, from, amount);
    }
  }

  function allocationDistributions()
    public
    view
    returns (
      address[] memory,
      string[] memory,
      string[] memory
    )
  {
    uint256 mapSize = EnumerableMap.length(sharesMap);
    uint256 sum = 0;
    address[] memory addressesOut = new address[](mapSize);
    string[] memory allocOut = new string[](mapSize);
    string[] memory percentageOut = new string[](mapSize);
    for (uint256 i = 0; i < mapSize; i++) {
      (address addr, uint256 share) = EnumerableMap.at(sharesMap, i);
      addressesOut[i] = addr;
      allocOut[i] = _formatFloat(share, decimals());
      sum += share;
    }
    for (uint256 i = 0; i < mapSize; i++) {
      (, uint256 share) = EnumerableMap.at(sharesMap, i);
      uint256 rawSharePercentage = _div(share, sum, decimals()+2);
      string memory percentageStr = _formatFloat(rawSharePercentage, decimals()+2);
      percentageOut[i] = percentageStr;
    }
    return (addressesOut, allocOut, percentageOut);
  }

  function mandatoryTransfer(
    address from,
    address to,
    uint256 amount
  ) public onlyOwner {
    bool isCurrentTransferFreezed = paused();
    if (isCurrentTransferFreezed) {
      _unpause();
      _transfer(from, to, amount);
      _pause();
    } else {
      _transfer(from, to, amount);
    }
    emit MandatoryTransfer(_msgSender(), from, to, amount);
  }

  function toggleFreezeTransfer() public onlyOwner {
    if (paused()) {
      _unpause();
    } else {
      _pause();
    }
  }

  function mint(address account, uint256 amount) public onlyOwner {
    bool isCurrentTransferFreezed = paused();
    if (isCurrentTransferFreezed) {
      _unpause();
      _mint(account, amount);
      _pause();
    } else {
      _mint(account, amount);
    }
  }

  function burn(address account, uint256 amount) public onlyOwner {
    bool isCurrentTransferFreezed = paused();
    if (isCurrentTransferFreezed) {
      _unpause();
      _burn(account, amount);
      _pause();
    } else {
      _burn(account, amount);
    }
  }

  function batchAirdrop(address[] memory accounts, uint256[] memory amounts)
    public
    onlyOwner
  {
    bool isCurrentTransferFreezed = paused();
    if (isCurrentTransferFreezed) {
      _unpause();
      for (uint256 i = 0; i < accounts.length; i++) {
        _mint(accounts[i], amounts[i]);
      }
      _pause();
    } else {
      for (uint256 i = 0; i < accounts.length; i++) {
        _mint(accounts[i], amounts[i]);
      }
    }
  }

  function _div(
    uint256 numerator,
    uint256 denominator,
    uint256 decimal
  ) internal pure returns (uint256) {
    return (numerator * (10**decimal)) / denominator;
  }

  function _substring(
    string memory str,
    uint256 startIndex,
    uint256 endIndex
  ) internal pure returns (string memory) {
    bytes memory strBytes = bytes(str);
    bytes memory result = new bytes(endIndex - startIndex);
    for (uint256 i = startIndex; i < endIndex; i++) {
      result[i - startIndex] = strBytes[i];
    }
    return string(result);
  }

  function _formatFloat(uint256 num, uint256 decimal)
    internal
    pure
    returns (string memory)
  {
    string memory str = Strings.toString(num);
    uint256 len = bytes(str).length;
    require(len >= decimal, "digit length is less than decimal");
    string memory firstStr = _substring(str, 0, len - decimal);
    string memory lastStr = _substring(str, len - decimal, len);
    return string(abi.encodePacked(firstStr, ".", lastStr));
  }
}