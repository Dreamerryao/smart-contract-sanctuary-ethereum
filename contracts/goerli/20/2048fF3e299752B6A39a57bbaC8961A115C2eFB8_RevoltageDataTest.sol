pragma solidity >=0.8.15;

import "./Revoltage.sol";
import "./access/Ownable.sol";
import "hardhat/console.sol";

contract RevoltageDataTest is Ownable {
    string private constant _name = "Revoltage Data Test";

    struct Reward {
        uint256 amountTotal;
        uint256 amountPerToken;
        uint256 date;
        uint256 duration;
    }

    struct Staker {
        StakeItem[] items;
    }

    struct StakeItem {
        uint256 start;
        uint256 end;
        uint256 amount;
        bool running;
    }

    struct Claimer {
        ClaimItem[] items;
    }

    struct ClaimItem {
        uint256 rewardIndex;
        uint256 amount;
        uint256 date;
        uint256 duration;
    }

    Reward[] public rewards;
    address[] public stakers;
    uint256 public minStakeAmount;
    uint256 public minStakeDuration;
    Revoltage public revoltage;

    mapping(address => uint256) public balance;
    mapping(address => bool) public hasStaked;
    mapping(address => bool) public isStaking;

    mapping(address => Staker) internal stakes;
    mapping(address => Claimer) internal claims;
    mapping(address => mapping(uint256 => mapping(uint256 => bool)))
        internal rewardClaimed; // rewardIndex > stakeId //FFF

    // mapping(address => mapping(uint256 => bool)) internal rewardClaimed; // stakeIndex

    constructor(
        Revoltage _revoltage,
        uint256 _minStakeAmount,
        uint256 _minStakeDuration
    ) {
        revoltage = _revoltage;
        minStakeAmount = _minStakeAmount;
        minStakeDuration = _minStakeDuration;
    }

    /**
     * @dev Returns the name of the token.
     */
    function name() public view virtual returns (string memory) {
        return _name;
    }

    function createStakeItem(uint256 amount)
        private
        view
        returns (StakeItem memory)
    {
        StakeItem memory item;
        item.start = block.timestamp;
        item.amount = amount;
        item.running = true;
        return item;
    }

    event StakeTokens(uint256 start, uint256 end, uint256 amount, bool running);

    function stakeTokens(uint256 amount) public returns (bool) {
        require(amount > 0, "amount is less than or equal to zero");
        require(msg.sender != address(0), "sender cannot be the zero address");
        require(
            amount >= minStakeAmount,
            "amount is less than staking minimum"
        );

        uint256 revoltageBalance = revoltage.balanceOf(msg.sender);
        require(revoltageBalance >= amount, "Amount exceeds balance");

        StakeItem memory item = createStakeItem(amount);
        revoltage.transferFrom(msg.sender, address(this), amount);
        //update staking Balance
        balance[msg.sender] = balance[msg.sender] + amount;
        stakes[msg.sender].items.push(item);

        // add user to stakers array *only* if they haven't staked already
        if (!hasStaked[msg.sender]) {
            //isStaking[msg.sender] = true;
            stakers.push(msg.sender);
            hasStaked[msg.sender] = true;
        }

        emit StakeTokens(item.start, item.end, item.amount, item.running);
        return true;
    }

    event UnstakeTokens(StakeItem item);

    function unstakeTokens(uint256 index) public {
        require(msg.sender != address(0), "sender cannot be the zero address");

        StakeItem memory item = stakes[msg.sender].items[index];

        require(item.running, "stake is not running");

        item.running = false;
        item.end = block.timestamp;
        revoltage.transfer(msg.sender, item.amount);
        //Remove from balance
        //todo left rewards
        balance[msg.sender] = balance[msg.sender] - item.amount;
        
        emit UnstakeTokens(item);
    }

    function unstakeAllTokens() public {
        require(msg.sender != address(0), "sender cannot be the zero address");

        for (uint256 i = 0; i < stakes[msg.sender].items.length; i++) {
            StakeItem memory stake = stakes[msg.sender].items[i];
            if (stake.running) {
                unstakeTokens(i);
            }
        }
    }

    function getStake(uint256 index) public view returns (StakeItem memory) {
        require(
            index < stakes[msg.sender].items.length,
            "stake is not running"
        );
        return stakes[msg.sender].items[index];
    }

    function getStakes() public view returns (StakeItem[] memory) {
        return stakes[msg.sender].items;
    }

    function helper_getStakeForAccount(address account, uint256 index)
        public
        view
        returns (StakeItem memory)
    {
        return stakes[account].items[index];
    }

    function helper_getStakesForAccount(address account)
        public
        view
        returns (StakeItem[] memory)
    {
        return stakes[account].items;
    }

    event SetMinStakeAmount(uint256 minStakeAmount);

    function setMinStakeAmount(uint256 fullRevo) public onlyOwner {
        minStakeAmount = fullRevo;
        emit SetMinStakeAmount(minStakeAmount);
    }

    event SetMinStakeDuration(uint256 minStakeDuration);

    function setMinStakeDuration(uint256 duration) public onlyOwner {
        minStakeDuration = duration;
        emit SetMinStakeDuration(minStakeDuration);
    }

    // function canStakeItemClaim(uint256 index) public view returns (unit256) {
    //     StakeItem memory item = stakes[msg.sender].items[index];

    //     for (uint256 rewardIndex; rewardIndex < rewards.length; rewardIndex++) {
    //         RewardStakeIndex memory item = isRewardClaimable(rewardIndex);
    //         result.push(item);
    //     }
    // }

    // function getClaimableRewards()
    //     public
    //     view
    //     returns (RewardStakeIndex[] memory)
    // {
    //     RewardStakeIndex[] memory result = new RewardStakeIndex[](
    //         stakes[msg.sender].items.length
    //     );

    //     uint256 insertCounter = 0;

    //     for (uint256 rewardIndex; rewardIndex < rewards.length; rewardIndex++) {
    //         RewardStakeIndex memory item = isRewardClaimable(rewardIndex);
    //         result.push(item);
    //     }
    //     return result;
    // }

    function ensureClaimable(uint256 rewardIndex, uint256 stakeIndex)
        public
        view
        returns (bool)
    {
        bool claimed = rewardClaimed[msg.sender][rewardIndex][stakeIndex];
        StakeItem memory stakeItem = stakes[msg.sender].items[stakeIndex];

        require(!claimed, "has been allready claimed");
        require(stakeItem.running, "token has been unstaked");

        bool claimable = isClaimable(rewardIndex, stakeIndex);
        //require(claimable, "date is not in range");
        if (!claimable) {
            return false;
        }
        return true;
    }

    function isClaimable(uint256 rewardIndex, uint256 stakeIndex)
        public
        view
        returns (bool)
    {
        bool claimed = rewardClaimed[msg.sender][rewardIndex][stakeIndex];
        StakeItem memory stakeItem = stakes[msg.sender].items[stakeIndex];
        bool result = false;

        Reward memory reward = rewards[rewardIndex];

        if (stakeItem.running && !claimed) {
            // if (stakeItem.running) {
            return isDateCompareInDayRage(stakeItem.start, reward.date);
        }
        return result;
    }

    function isDateCompareInDayRage(uint256 date1, uint256 date2)
        public
        view
        returns (bool)
    {
        //require(date2 > date1, "second date muss be greater than initial date");
        if (date2 < date1) {
            return false;
        }

        uint256 diff = date2 - date1;
        uint256 dayDiff = toDays(diff);

        return dayDiff > minStakeDuration;
    }

    function toDays(uint256 inputSeconds) public pure returns (uint256) {
        return inputSeconds / 60 / 60 / 24;
    }

    function getRewardRate(uint256 rewardIndex, uint256 stakeIndex)
        public
        view
        returns (uint256)
    {
        uint256 result = 0;

        if (isClaimable(rewardIndex, stakeIndex)) {
            uint256 amountPerToken = rewards[rewardIndex].amountPerToken;
            uint256 amount = stakes[msg.sender].items[stakeIndex].amount;

            result = (amount * amountPerToken) / 10**8;
        }
        return result;
    }

    event ClaimToken(ClaimItem claimItem);

    function claimReward(uint256 rewardIndex, uint256 stakeIndex) public {
        ensureClaimable(rewardIndex, stakeIndex);
        uint256 amount = getRewardRate(rewardIndex, stakeIndex);

        StakeItem memory stakeItem = stakes[msg.sender].items[stakeIndex];
        Reward memory reward = rewards[rewardIndex];

        ClaimItem memory claimItem;
        claimItem.rewardIndex = rewardIndex;
        claimItem.amount = amount;
        claimItem.date = block.timestamp;
        claimItem.duration = reward.date - stakeItem.start;

        emit ClaimToken(claimItem);
        revoltage.transfer(msg.sender, claimItem.amount);
        rewardClaimed[msg.sender][rewardIndex][stakeIndex] = true;

        balance[address(this)] = balance[address(this)] - claimItem.amount;
    }

    // function claimStakeReward(uint256 rewardIndex, uint256 stakeIndex)
    //     public
    //     returns (bool)
    // {
    //     ensureClaimable(rewardIndex, stakeIndex);
    //     uint256 amount = getRewardRate(rewardIndex, stakeIndex);

    //     StakeItem memory stakeItem = stakes[msg.sender].items[stakeIndex];
    //     Reward memory reward = rewards[rewardIndex];

    //     ClaimItem memory claimItem;
    //     claimItem.rewardIndex = rewardIndex;
    //     claimItem.amount = uint256(amount);
    //     claimItem.date = block.timestamp;
    //     claimItem.duration = stakeItem.start - reward.date;

    //     emit ClaimToken(claimItem);

    //     rewardClaimed[msg.sender][rewardIndex][stakeIndex] = true;

    //     //revoltage.transferFrom(address(this), msg.sender, claimItem.amount);
    //     balance[msg.sender] = balance[msg.sender] + claimItem.amount;

    //     return true;
    // }

    // function claimRewards(uint256[] memory index) public {
    //     //claiming müssen wir noch bauen
    //     //
    //     // - search all open stakes for owner
    //     // - validate per stakeitem if there is a reward
    //     //
    // }

    function getRewards() public view returns (Reward[] memory) {
        return rewards;
    }

    event AddReward(Reward reward);

    function addReward(
        uint256 amountTotal,
        uint256 amountPerToken,
        uint256 date,
        uint256 duration
    ) public onlyOwner {
        require(amountTotal > 0, "amountTotal = 0");
        require(amountPerToken > 0, "amountPerToken = 0");

        uint256 revoltageBalance = revoltage.balanceOf(msg.sender);
        require(revoltageBalance >= amountTotal, "Amount exceeds balance");

        Reward memory reward;
        reward.amountTotal = amountTotal;
        reward.amountPerToken = amountPerToken;
        reward.date = date;
        reward.duration = duration;

        emit AddReward(reward);
        revoltage.transferFrom(msg.sender, address(this), amountTotal);
        balance[address(this)] = balance[address(this)] + amountTotal;

        rewards.push(reward);
    }
}

