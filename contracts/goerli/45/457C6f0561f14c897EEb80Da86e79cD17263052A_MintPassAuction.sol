   // EXTEND the Auction:
   function changeAuctionEndTime(uint extraTimeAmount) public onlyContractOwner() {
      console.log("\n\n===>>In 'changeAuctionEndTime()'!!!");
      console.log(" =>Incoming value of 'newEndTime' = %s", extraTimeAmount);

      // require(msg.sender == contractOwner, "'changeAuctionEndTime()' - ONLY THE CONTRACT's OWNER CAN CALL THIS FUNCTION!");

      auctionEndTime += extraTimeAmount * 1 minutes;

      console.log(" =>The value of 'auctionEndTime' has been changed to: %s", auctionEndTime);

      emit AuctionExtended(extraTimeAmount);
   }




   function endTheAuction() public onlyContractOwner() {
      console.log("\n\n===>>In 'endTheAuction()'!!!");

      // 1. First ensure that the current time in which this function is called is indeed PAST the Auction's 
      // official End Time:
      // require(block.timestamp >= auctionEndTime, "'endTheAuction()' --> Auction not yet ended!");
      
      // 2. Also make sure this function is being called WHILE the Auction IS STILL GOING ON. Cause if the Auction isn't
      //    still going - and has already ended, we shouldn't be allowed to call this function again:
      require(auctionIsRunning == true, "'endTheAuction()' has already been called!");
      console.log(" =>Got through the SECOND 'require', meaning 'auctionIsRunning' == is still TRUE");

      // If we got this far... we can indeed go on and END the Auction:
      auctionEndTime = block.timestamp;
      auctionIsRunning = false;
      auctionWasEnded = true;

      console.log(" =>Set the value of 'auctionIsRunning' to 'false'. Let's verify:\n >'auctionIsRunning' = %s", auctionIsRunning);

      emit AuctionEnded(auctionEndTime);
 
   }


  

   function pauseContract() public onlyContractOwner() {
      auctionEndTime = auctionEndTime + 46 hours;
      timeAuctionWasPaused = block.timestamp;

      _pause();

      emit AuctionPaused(contractOwner, timeAuctionWasPaused);
   }




   function unPauseContract() public onlyContractOwner() {
      timeAuctionWasUnpaused = block.timestamp;

      _unpause();

      emit AuctionUnpaused(contractOwner, timeAuctionWasUnpaused);
   }



