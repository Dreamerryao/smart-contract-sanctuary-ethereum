   function startAuction(uint auctionLengthInMinutes) public onlyContractOwner() {
      console.log("\n>In 'startAuction()'!!!");
      console.log(">Value passed in for 'auctionLengthInMinutes' = %s", auctionLengthInMinutes);

      // 1. FIrst make sure the Auction isn't ALREADY running:
      require(auctionIsRunning == false, "startAuction() ERROR: Auction already started!");

      // 2. Set the Auction's START and END-TIME:
      auctionStartTime = block.timestamp;
      auctionEndTime =  block.timestamp + auctionLengthInMinutes * 1 minutes;
      auctionIsRunning = true;

      console.log("\nIn 'startAuction()'!!!");
      console.log(">'auctionEndTime' = %s ", auctionEndTime);
      console.log(">'auctionIsRunning' = %s ", auctionIsRunning);
   
      // 3. EMIT AN EVENT TO ANNOUNCE AUCTION HAS STARTED:
      emit AuctionStarted(auctionStartTime, auctionEndTime);
   }



   function getAuctionRunningStatus() public view returns (bool auctionWasStartd, bool auctionWasEndd) {
