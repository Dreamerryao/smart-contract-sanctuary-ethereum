        for ( uint256 i = 0; i < array.length; i++ ) {

            if( array[i] == element ) return i;

        }

    }

    function includes(uint[] memory array, uint element ) internal pure returns ( bool included ) {

        for ( uint256 i = 0; i < array.length; i++ ) {

            if( array[i] == element ) return true;

        }

