pragma solidity ^0.8.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./IShlongPresale.sol";
import "hardhat/console.sol";


contract ShlongPresale is IShlongPresale, Ownable {
    bytes32 public merkleRoot;

    enum SaleStates {
        CLOSED,
        WHITELIST,
        PUBLIC,
        COMPLETE
    }

    SaleStates public saleState;
    uint256 public maxPresale = 1 ether;
    uint256 public minPresale = 0.035 ether;
    uint256 public hardCap = 55 ether;
    uint256 public raised = 0 ether;
    event SaleStateChanged(SaleStates saleState);
    mapping(address => uint256) public contributions;

    event Contributed(address contributor, uint256 amount);

    constructor() {}

    /// @notice Function used to contribute during whitelist phase. 
    /// @dev checkState to check sale state.
    function whitelistContribution(bytes32[] calldata merkleProof) 
        external
        payable 
        checkState(SaleStates.WHITELIST)
        whitelisted(merkleProof)
    {
        if (msg.value < minPresale) revert InvalidEtherAmount();
        if (msg.value + contributions[msg.sender] > maxPresale) revert InvalidEtherAmount();
        if (msg.value + raised > hardCap) revert HardCapReached();
        contributions[msg.sender] = contributions[msg.sender] + msg.value;
        raised += msg.value;
        emit Contributed(msg.sender, msg.value);
    }

    /// @notice Function used to contribute during public phase. 
    /// @dev checkState to check sale state.
    function publicContribution()
        external
        payable
        checkState(SaleStates.PUBLIC)
    {
        if (msg.value < minPresale) revert InvalidEtherAmount();
        if (msg.value + contributions[msg.sender] > maxPresale) revert InvalidEtherAmount();
        if (msg.value + raised > hardCap) revert HardCapReached();
        contributions[msg.sender] = contributions[msg.sender] + msg.value;
        raised += msg.value;
        emit Contributed(msg.sender, msg.value);
    }

    /// @notice Withdraw funds
    function withdraw() public onlyOwner {        
        (bool withdrawalSuccess, ) = payable(msg.sender).call{value: address(this).balance}("");
        if (!withdrawalSuccess) revert WithdrawFailedVault();
    }

    /// @notice Withdraw funds to certain address
    /// @param receiver address to withdraw to.
    function withdrawTo(address receiver) public onlyOwner {        
        (bool withdrawalSuccess, ) = payable(receiver).call{value: address(this).balance}("");
        if (!withdrawalSuccess) revert WithdrawFailedVault();
    }

    /// @notice Function used to set presale max contribution.
    /// @param newMaxPresale Newly intended `maxPresale` value.
    function setMaxPresale(uint256 newMaxPresale) external onlyOwner {
        maxPresale = newMaxPresale;
    }

    /// @notice Function used to set presale min contribution.
    /// @param newMinPresale Newly intended `minPresale` value.
    function setMinPresale(uint256 newMinPresale) external onlyOwner {
        minPresale = newMinPresale;
    }

    /// @notice Function used to set hard cap.
    /// @param newHardCap Newly intended `hardCap` value.
    function setHardCap(uint256 newHardCap) external onlyOwner {
        hardCap = newHardCap;
    }

    /// @notice Function used to set new merkle root
    /// @param _merkleRoot Newly intended `merkleRoot` value.
    function setMerkleRoot(bytes32 _merkleRoot) external onlyOwner {
       merkleRoot = _merkleRoot;
    }

    /// @notice Function used to change the current `saleState` value.
    /// @param newSaleState The new `saleState` value.
    /// @dev 0 = CLOSED, 1 = WHITELIST, 2 = PUBLIC
    function setSaleState(uint256 newSaleState) external onlyOwner {
        if (newSaleState > uint256(SaleStates.PUBLIC))
            revert InvalidSaleState();
        saleState = SaleStates(newSaleState);
        emit SaleStateChanged(saleState);
    }

    /// @notice Verifies the current state.
    /// @param saleState_ Sale state to verify. 
    modifier checkState(SaleStates saleState_) {
        if (saleState != saleState_) revert InvalidSaleState();
        _;
    }

    /// @notice Verifies the sender is whitelisted
    modifier whitelisted(bytes32[] calldata merkleProof) {
        if (MerkleProof.verify(merkleProof, merkleRoot, toBytes32(msg.sender)) != true) revert NotWhitelisted();
        _;
    }

    function toBytes32(address addr) pure internal returns (bytes32) {
        return bytes32(uint256(uint160(addr)));
    }
}

