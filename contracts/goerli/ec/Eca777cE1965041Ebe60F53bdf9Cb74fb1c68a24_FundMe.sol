pragma solidity ^0.8.8;

import "./PriceConverter.sol";
import "hardhat/console.sol";

// best practice to name errors ContractName__ErrorName
error FundMe__NotOwner();
error FundMe__DidntSendEnough();
error FundMe__CallFailed();

// interfaces, libraries should be included here

// natspec (natural language spec) comments are created with:
// ///
// or:
// /** */
// @notice tag is a note to people / description
// @dev a note to devs
// with solc this auto-generates documentation:
// solc --userdoc --devdoc FundMe.sol
/**
 * @title A contract for crowd funding
 * @author Jane Doe
 * @notice A Demo sample funding contract
 * @dev Implements price feeds as our library
 */
contract FundMe {
    // type delcarations should be added here (e.g. extending types--uint256, etc.)
    using PriceConverter for uint256;

    // state vars
    // global = storage vars
    // storage is a giant array; each var is slotted into a 32 bytes long slot there
    // vars are stored as hex(adecimals) (0x...), e.g.:
    // uint256 25 is 0x000...19
    // bool true is 0x000....01
    // for dynamic values (arrays, mappings) (which can change length):
    // - for arrays a sequential slot is taken up for the length of the array
    // - for mappings a sequential slot is taken up, but left blank
    // - their elements are stored using hashing a fn, i.e. the storage index for the element is calculated,
    //   as we never know how many subsequencies we will need,
    //   whereas "static" vars use seuqential spots (lowest available index) as well
    // e.g. uint256 favNum = 25;                        => storage[0]              = 0x00...19
    //      bool someBool = true;                       => storage[1]              = 0x00...01
    //      uint256[] myArray; <...> myArray.push(222); => storage[2]              = 0x00...01 // array length
    //                                                  => storage[keccak256(222)] = 0x00..0de
    //      mapping(address => uint256) myMapping;      => storage[3]              = <blank>
    // constants & immuntable vars are not stored in storage - they are part of the contract's byte code itself
    // (as if the compiler swaps their names with the declared value)
    // simple fn vars only exist for the duration of its execution (are not permanent, i.e. not added to storage, but their own memory ds that gets deleted after the fn has run)
    // arrays (incl. string) & mappings fn vars need the memory/storage keyword, as they take more storage, so solidity want to hear it from the dev where to put them

    // indicate const (not stored in storage): capsed
    uint256 public constant MINIMUM_USD = 1 * 1e18;
    // indicate immutable var (not stored in storage): prefixed with i_
    address private immutable i_ownerAddress;
    // indicate working with a storage var & we are about to spend a lot of gas: prefix with s_
    // private vars are cheaper and exposing getters allows for a more pleasant interface
    address[] private s_funderAddresses;
    mapping(address => uint256) private s_fundersFunds;

    AggregatorV3Interface private s_priceFeedAddress;

    // events should be added here

    // modifiers
    modifier onlyOwner() {
        if (msg.sender != i_ownerAddress) {
            revert FundMe__NotOwner();
        }

        _;
    }

    // fns
    // constructors
    constructor(address _priceFeedAddress) {
        i_ownerAddress = msg.sender;
        s_priceFeedAddress = AggregatorV3Interface(_priceFeedAddress);
    }

    // receive fns
    receive() external payable {
        fund();
    }

    // fallback fns
    fallback() external payable {
        fund();
    }

    // external fns (none here)

    // public fns
    /**
     * @notice Funds the contract
     */
    /*
     * @param paramName Param description
     * @return Return description
     */
    function fund() public payable {
        if (msg.value.getConversionRate(s_priceFeedAddress) < MINIMUM_USD) {
            revert FundMe__DidntSendEnough();
        }

        s_funderAddresses.push(msg.sender);
        s_fundersFunds[msg.sender] = s_fundersFunds[msg.sender] + msg.value;
    }

    function withdrawAllFunds() public onlyOwner {
        console.log("Hey");
        for (
            uint256 funderAddressIndex = 0;
            funderAddressIndex < s_funderAddresses.length;
            funderAddressIndex++
        ) {
            address funderAddress = s_funderAddresses[funderAddressIndex];
            s_fundersFunds[funderAddress] = 0;
        }

        s_funderAddresses = new address[](0);

        (bool callSuccess, ) = payable(msg.sender).call{
            value: address(this).balance
        }("");
        if (!(callSuccess)) {
            revert FundMe__CallFailed();
        }
    }

    function withdrawAllFundsMoreCheaply() public onlyOwner {
        // read entire array into memory one time, instead of reading it array.length number of time
        // and the read it from memory
        // note: can't be done for mappings - they cannot be put in memory
        // if there is one funder, though, will be more expensive because of the above line
        address[] memory funderAddresses = s_funderAddresses;
        for (
            uint256 funderAddressIndex = 0;
            funderAddressIndex < funderAddresses.length;
            funderAddressIndex++
        ) {
            address funderAddress = funderAddresses[funderAddressIndex];
            s_fundersFunds[funderAddress] = 0;
        }

        s_funderAddresses = new address[](0);
        (bool callSuccess, ) = i_ownerAddress.call{
            value: address(this).balance
        }("");
        if (!(callSuccess)) {
            revert FundMe__CallFailed();
        }
    }

    // internal fns
    // private fns

    // view / pure fns
    function getContractOwner() public view returns (address) {
        return i_ownerAddress;
    }

    function getFunderAddress(
        uint256 funderIndex
    ) public view returns (address) {
        return s_funderAddresses[funderIndex];
    }

    function getFundsByFunderAddress(
        address funderAddress
    ) public view returns (uint256) {
        return s_fundersFunds[funderAddress];
    }

    function getPriceFeedAddress() public view returns (AggregatorV3Interface) {
        return s_priceFeedAddress;
    }
}

