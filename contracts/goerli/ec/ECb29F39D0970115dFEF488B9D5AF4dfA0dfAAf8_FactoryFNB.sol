import "./FNBSwapProvider.sol";

contract FactoryFNB {
    //contains LPTokenId by ratio token0/token1 pair
    mapping(address => mapping(address => address)) public liquidityPoolId;
    address[] public exchangeAddresses;

    constructor() {}

    event FNBSwapProviderCreated(
        address indexed ownerOfContract,
        address contractAddress,
        uint256 createDate
    );

    function createExchange() public {
        FNBSwapProvider newContract = new FNBSwapProvider();

        exchangeAddresses.push(address(newContract));

        emit FNBSwapProviderCreated(
            msg.sender,
            address(newContract),
            block.timestamp
        );
    }

    function getAddressesLength() public view returns (uint256 count) {
        count = exchangeAddresses.length;
    }

    function getLastAddress() public view returns (address lastAddr) {
        uint256 count = exchangeAddresses.length;
        uint256 index = 0;
        if (count > 0) index = count - 1;
        lastAddr = exchangeAddresses[index];
    }
}

