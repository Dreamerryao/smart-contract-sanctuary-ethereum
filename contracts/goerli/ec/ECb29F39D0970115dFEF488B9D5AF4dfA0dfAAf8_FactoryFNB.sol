contract FNBSwapProvider {
    using SafeMath for uint256;

    address internal factory;
    IERC20 internal ERC20Token0;
    IERC20 internal ERC20Token1;

    address internal ProviderAddress;

    uint256 internal constant tenPOW18 = 0xDE0B6B3A7640000;
    uint256 internal constant percentageFee = 0x38D7EA4C68000; // //% 0.1 for swap 0x6A94D74F430000; //%3 for swap
    

    constructor() {
        factory = msg.sender;
    }

    mapping(address => uint256) public balanceOf;

    event PairCreated(
        address indexed token0,
        address indexed token1,
        address LPtokenId,
        uint256 token0Amount,
        uint256 token1Amount
        //uint256 liquidity
    );

    event LiquidityAdded(
        address indexed token0,
        address indexed token1,
        uint256 token0Amount,
        uint256 token1Amount
        //uint256 newLiquidity
    );

    event Swapped(
        address indexed token0,
        address indexed token1,
        uint256 token0AddAmount,
        uint256 token1SubAmount,
        uint256 Fee
    );

    event LiquidityRemoved(
        address indexed LPtokenId,
        uint256 token0Amount,
        uint256 token1Amount,
        uint256 newLiquidity
    );

    event TransferAmount(
        address indexed from,
        address indexed to,
        uint256 value
    );

    struct StructTokensPair {
        address token0Id;
        address token1Id;
    }
    mapping(address => mapping(address => address)) internal liquidityPoolId; //contains LPTokenId by ratio token0/token1 pair
    mapping(address => StructTokensPair) internal tokensPairAddress;
    mapping(address => uint256) internal tokensReserves;

    /**
     * @notice Get Address of Liquidity Pool token(contract)
     * @param token0    The address of the ERC-20 token contract
     * @param token1    The address of the ERC-20 token contract
     */
    function GetLiquidityPoolId(address token0, address token1)
        public
        view
        returns (address LPtokenId)
    {
        require(
            token0 != address(0x0),
            "FNBSwapProvider#GetPair: ID_TOKEN0_INVALID"
        );
        require(
            token1 != address(0x0),
            "FNBSwapProvider#GetPair: ID_TOKEN1_INVALID"
        );

        LPtokenId = liquidityPoolId[token0][token1];
    }

    /**
     * @notice Get object(tuple) of token0/token1 pair
     * @param LPTokenId     The address of the Liquidity Pool token(contract)
     */
    function GetPair(address LPTokenId)
        public
        view
        returns (StructTokensPair memory)
    {
        require(
            LPTokenId != address(0x0),
            "FNBSwapProvider#GetPairByLiquidityPoolId: LIQUIDITYPOOL_NOTEXISTS"
        );

        StructTokensPair memory data = tokensPairAddress[LPTokenId];

        return data;
    }

    /**
     * @notice Get reserve of token ERC-20
     * @param tokenAddress     The address of the ERC-20 token contract
     */
    function GetReserveOfToken(address tokenAddress)
        public
        view
        returns (uint256 reserve)
    {
        reserve = tokensReserves[tokenAddress];
    }

    /**
     * @notice Calculate what part of Token0/Token1 reserve provider can take, when he burn LPTokenAmount
     * @param dLP     Amounts of liquidity token for burn
     * @param reserve     Reserve of token(0 or 1)
     * @param totalLiquidity     Total balance of LPtoken
     */
    function CalcTokenRewardAfterBurn(
        uint256 dLP,
        uint256 reserve,
        uint256 totalLiquidity
    ) public pure returns (uint256 tokenAmount) {
        tokenAmount = dLP.mul(reserve);
        tokenAmount = tokenAmount.div(totalLiquidity);

        return tokenAmount;
    }

    //Get amount of tokensB by ration A/B

    /**
     * @notice Get new total liquidity for pair
     * @param dToken0       Amounts of token0 ERC-20
     * @param token0Res     Reserve of token0 ERC-20
     * @param token1Res     Reserve of token1 ERC-20
     */
    function CalcTokenAmount(
        uint256 dToken0,
        uint256 token0Res,
        uint256 token1Res
    ) public pure returns (uint256 dToken1) {
        uint256 numerator = 0;
        uint256 P10 = 0;
        if (dToken0 < token0Res) {
            P10 = _getDigits(token0Res);

            numerator = dToken0.mul(10**P10);
            dToken1 = numerator.mul(token1Res);
            dToken1 = dToken1.div(token0Res);
            dToken1 = dToken1.div(10**P10);
        } else {
            dToken1 = dToken0.mul(token1Res);
            dToken1 = dToken1.div(token0Res);
        }

        return dToken1;
    }

    /**
     * @notice Get new total liquidity for pair
     * @param subTokenAmount   Amount of token, which swap commission is imposed
     */
    function CalcLiquidity(uint256 subTokenAmount)
        public
        pure
        returns (uint256 liquidity)
    {
        //subTokenAmount * %
        liquidity = subTokenAmount.mul(percentageFee);
        liquidity = liquidity.div(tenPOW18);
    }

    /**
     * @notice Create liquditiy pool for token0/token1 pair
     * @param provider          Liquidity provider address
     * @param token0            The address of the ERC-20 token contract
     * @param token1            The address of the ERC-20 token contract
     * @param amountToken0      Amount of ERC-20 token to be sent
     * @param amountToken1      Amount of ERC-20 token to be sent
     */
    function CreatePair(
        address provider,
        address token0,
        address token1,
        uint256 amountToken0,
        uint256 amountToken1
    ) public {
        //address myAddr = address(this);
        address LPTokenId = address(this);

        require(
            token0 != token1,
            "FNBSwapProvider#CreatePair: IDENTICAL_ADDRESSES"
        );
        require(
            token0 != address(0),
            "FNBSwapProvider#CreatePair: ZERO_ADDRESS"
        );
        require(
            token1 != address(0),
            "FNBSwapProvider#CreatePair: ZERO_ADDRESS"
        );
        require(
            (liquidityPoolId[token0][token1] == address(0)) &&
                (liquidityPoolId[token1][token0] == address(0)),
            "FNBSwapProvider#CreatePair: PAIR_EXISTS"
        );

        //Inizalation instance of ERC-20 Interface Contract
        ERC20Token0 = IERC20(token0);
        ERC20Token1 = IERC20(token1);

        //Configure liquidity Pool Id
        liquidityPoolId[token0][token1] = LPTokenId;
        liquidityPoolId[token1][token0] = LPTokenId;

        //Configure tokens pair
        StructTokensPair memory pair = StructTokensPair(token0, token1);
        tokensPairAddress[LPTokenId] = pair;
        tokensReserves[token0] = amountToken0;
        tokensReserves[token1] = amountToken1;

        //save provider address
        ProviderAddress = provider;

        //Transfer amount of tokens0 to this contract
        ERC20Token0.transferFrom(provider, LPTokenId, amountToken0);
        //Transfer amount of tokens1 to this contract
        ERC20Token1.transferFrom(provider, LPTokenId, amountToken1);

        emit PairCreated(token0, token1, LPTokenId, amountToken0, amountToken1);
    }

    /**
     * @notice Add liquditiy to pool for token0/token1 pair
     * @param provider              Liquidity provider address
     * @param token0               The address of the ERC-20 token contract
     * @param token1               The address of the ERC-20 token contract
     * @param amountTokenDesired    Amount of ERC-20 token to be sent
     */
    function AddLiquidity(
        address provider,
        address token0,
        address token1,
        uint256 amountTokenDesired
    ) public {
        //Validation
        if (liquidityPoolId[token0][token1] == address(0)) {
            require(
                liquidityPoolId[token1][token0] != address(0),
                "FNBSwapProvider#AddLiquidity: PAIR_NOT_EXISTS"
            );
        }
        require(
            ProviderAddress == provider,
            "FNBSwapProvider#AddLiquidity: INVALID_PROVIDER_ADDRESS_DENIED"
        );

        //Inizalation instance of ERC-20 Interface Contract
        ERC20Token0 = IERC20(token0);
        ERC20Token1 = IERC20(token1);

        //Calc token1 amount to be sent
        //dToken1 = (dToken0 / Res0) * Res1
        uint256 amountTokenOtherDesired =
            CalcTokenAmount(
                amountTokenDesired,
                tokensReserves[token0],
                tokensReserves[token1]
            );

        //Check the available balances(in tokens 0/1) to transfer
        require(
            ERC20Token0.allowance(provider, address(this)) > amountTokenDesired,
            "FNBSwapProvider#AddLiquidity: INSUFFICIENT_FUNDS_TOKEN0"
        );
        require(
            ERC20Token1.allowance(provider, address(this)) >
                amountTokenOtherDesired,
            "FNBSwapProvider#AddLiquidity: INSUFFICIENT_FUNDS_TOKEN1"
        );

        //update reserves ratio A/B
        tokensReserves[token0] = tokensReserves[token0].add(amountTokenDesired);
        tokensReserves[token1] = tokensReserves[token1].add(
            amountTokenOtherDesired
        );

        //Transfer amount of tokens0 to this contract
        ERC20Token0.transferFrom(provider, address(this), amountTokenDesired);
        //Transfer amount of tokens1 to this contract
        ERC20Token1.transferFrom(
            provider,
            address(this),
            amountTokenOtherDesired
        );

        emit LiquidityAdded(
            token0,
            token1,
            amountTokenDesired,
            amountTokenOtherDesired
        );
    }

    /**
     * @notice Swap amount of token0 to amount token1 based on liquditiy pool
     * @param wallet               User wallet address
     * @param token0               The address of the ERC-20 token contract
     * @param token1               The address of the ERC-20 token contract
     * @param swappedAmountToken0    Amount of ERC-20 token to be swap
     */
    function SwapTokens(
        address wallet,
        address token0,
        address token1,
        uint256 swappedAmountToken0
    ) public returns (uint256 swappedAmountToken1) {
        //Validation
        if (liquidityPoolId[token0][token1] == address(0)) {
            require(
                liquidityPoolId[token1][token0] != address(0),
                "FNBSwapProvider#AddLiquidity: PAIR_NOT_EXISTS"
            );
        }

        //Inizalation instance of ERC-20 Interface Contract
        ERC20Token0 = IERC20(token0);
        ERC20Token1 = IERC20(token1);

        //Calc token1 amount to be sent
        //dToken1 = (dToken0 / Res0) * Res1
        swappedAmountToken1 = CalcTokenAmount(
            swappedAmountToken0,
            tokensReserves[token0],
            tokensReserves[token1]
        );

        //Check the available balances(in tokens 0/1) to transfer
        require(
            ERC20Token0.allowance(ProviderAddress, address(this)) >
                swappedAmountToken0,
            "FNBSwapProvider#SwapTokens: INSUFFICIENT_FUNDS_TOKEN0"
        );

        //update reserves ratio A/B
        tokensReserves[token0] = tokensReserves[token0].add(
            swappedAmountToken0
        );
        tokensReserves[token1] = tokensReserves[token1].sub(
            swappedAmountToken1
        );
        //Calc % fee for swap
        uint256 newLiquidity = CalcLiquidity(swappedAmountToken1);
        swappedAmountToken1 = swappedAmountToken1.sub(newLiquidity);

        // Mint liquidity to provider
        _mint(ProviderAddress, newLiquidity);
        //Transfer amount of tokens0 to this contract
        ERC20Token0.transferFrom(wallet, address(this), swappedAmountToken0);
        //Transfer from this contract to token1
        ERC20Token1.transfer(wallet, swappedAmountToken1);

        emit Swapped(
            token0,
            token1,
            swappedAmountToken0,
            swappedAmountToken1,
            newLiquidity
        );
    }

    function RemoveLiquidity(
        address provider,
        address liquidityTokenId,
        uint256 liquidityAmount
    ) public {
        //Validation
        require(
            liquidityTokenId != address(0x0),
            "FNBSwapProvider#RemoveLiquidity: LIQUIDITYPOOL_NOTEXISTS"
        );
        StructTokensPair memory pair = tokensPairAddress[liquidityTokenId];
        require(
            pair.token0Id != address(0x0) && pair.token1Id != address(0x0),
            "FNBSwapProvider#RemoveLiquidity: PAIR_NOT_ESTABLISHED"
        );
        require(
            ProviderAddress == provider,
            "FNBSwapProvider#RemoveLiquidity: INVALID_PROVIDER_ADDRESS_DENIED"
        );

        require(
            balanceOf[provider] >= liquidityAmount,
            "FNBSwapProvider#RemoveLiquidity: INSUFFICIENT_FUNDS_LPTOKEN"
        );

        //Inizalation instance of ERC-20 Interface Contract
        ERC20Token0 = IERC20(pair.token0Id);
        ERC20Token1 = IERC20(pair.token1Id);

        //calc new amount of tokens 0/1 for provider
        uint256 dToken0 =
            CalcTokenRewardAfterBurn(
                liquidityAmount,
                tokensReserves[pair.token0Id],
                balanceOf[provider]
            );
        uint256 dToken1 =
            CalcTokenRewardAfterBurn(
                liquidityAmount,
                tokensReserves[pair.token1Id],
                balanceOf[provider]
            );
        //Set new reserves for token0/token1
        tokensReserves[pair.token0Id] = tokensReserves[pair.token0Id].sub(
            dToken0
        );
        tokensReserves[pair.token1Id] = tokensReserves[pair.token1Id].sub(
            dToken1
        );

        // Burn liquidity for provider
        _burn(provider, liquidityAmount);
        // Transfer from contract to provier
        ERC20Token0.transfer(provider, dToken0);
        ERC20Token1.transfer(provider, dToken1);

        //emit Event
        emit LiquidityRemoved(
            provider,
            dToken0,
            dToken1,
            balanceOf[liquidityTokenId]
        );
    }

    //---y

    function _mint(address to, uint256 value) internal {
        balanceOf[to] = balanceOf[to].add(value);

        emit TransferAmount(address(0), to, value);
    }

    function _burn(address from, uint256 value) internal {
        balanceOf[from] = balanceOf[from].sub(value);

        emit TransferAmount(from, address(0), value);
    }

    /**
     * @dev get the digits of number
     */
    function _getDigits(uint256 num) internal pure returns (uint256 _d) {
        uint256 d = 0;

        while (num > 0) {
            d++;
            num = num.div(10);
        }
        _d = d;
        return _d;
    }
    //---
}

