pragma solidity >=0.7.0 <0.9.0;

import "gnosis_safe_smart_contract/handler/DefaultCallbackHandler.sol";
import "nyx_vault/interfaces/INyxVault.sol";

/// @title NyxVault Fallback Handler - fallback handler to redirect to NyxVault contract

contract NyxVaultFallbackHandler is DefaultCallbackHandler
{
    INyxVault nyx_vault;

    /**
     * DESCRIPTION
     * @dev DEV
     * @param addr blabla
     */
     function setNyxVault(address addr)
        external payable
    {
        nyx_vault = INyxVault(addr);
    }

    /**
     * DESCRIPTION
     * @dev DEV
     * @param data additional parameter to indicate from where the funds are coming, like from mint, from p&l etc
     *      Is used to correctly allocate received funds
     */
     function receiveFunds(bytes32 data)
        public payable
    {
        return nyx_vault.receiveFunds(data);
    }

    /**
     * DESCRIPTION
     * @dev DEV
     * @param to blablabla
     * @param redeemPrice blablabla
     */
     function redeem(address to, uint256 redeemPrice, uint56 nNfts, uint256 tokenId)
        external payable
    {
        return nyx_vault.redeem(to, redeemPrice, nNfts, tokenId);
    }

    /**
     * DESCRIPTION
     * @dev DEV
     * @param addr blablabla
     * @param blockNumber blablabla
     * @return the redeem power of the given <addr>, at the block <blockNumber>
     */
    function getRedeemPower(address addr, uint256 blockNumber)
        public view
        returns (uint256)
    {
        return nyx_vault.getRedeemPower(addr, blockNumber);
    }

    receive()
        external payable
    {
        receiveFunds(keccak256(""));
    }
}

