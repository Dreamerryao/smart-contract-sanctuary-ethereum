pragma solidity >=0.8.0;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "nyx_dao/NyxRoleManager.sol";
import "nyx_vault/interfaces/INyxInvestmentVault.sol";
import "@openzeppelin/contracts/utils/Checkpoints.sol";

interface GnosisSafe {
    enum Operation {
        Call,
        DelegateCall
    }

    /// @dev Allows a Module to execute a Safe transaction without any further confirmations.
    /// @param to Destination address of module transaction.
    /// @param value Ether value of module transaction.
    /// @param data Data payload of module transaction.
    /// @param operation Operation type of module transaction.
    function execTransactionFromModule(address to, uint256 value, bytes calldata data, GnosisSafe.Operation operation)
        external
        returns (bool success);

    function isModuleEnabled(address moduleAddress) external returns (bool);
}

abstract contract INyxVault is NyxRoleManager, ReentrancyGuard
{
    using Checkpoints for Checkpoints.History;

    // Enums
    ///////////////////
    enum VaultType
    {
        Crypto,
        NFT,
        VC
    }

    // Structs
    ///////////////////
    struct Allocation
    {
        uint256 cryptoAllocation;
        uint256 nftAllocation;
        uint256 vcAllocation;
    }

    struct Revenue
    {
        uint256 yield;
        uint256 reinvested;
        uint256 performanceFees;
    }

    // Attributes
    ///////////////////
    bool public redeemOpen;
    uint256 public currentRedeem;
    uint256 public maxRedeem;
    uint256 public redeemPricePerRedeemPower;

    uint256 public lockedLiquidity;
    Allocation currentAllocation;
    Revenue currentRevenueRepartition;

    address public gnosisSafeAddress;
    address public RoyaltiesVault;
    address public InvestmentProfitVault;
    mapping(address => bool) public nonPnlAddresses;
    
    GnosisSafe public gnosisSafe;
    INyxNFT public nft;
    // INyxDAO public dao;
    INyxInvestmentVault public cryptoVault;
    INyxInvestmentVault public NFTVault;
    INyxInvestmentVault public VCVault;
    // mapping(uint256 => mapping(address => Checkpoints.History)) private receiveHistory;
    Checkpoints.History internal receiveHistory;
    Checkpoints.History internal mintHistory;
    Checkpoints.History internal pnlHistory;
    mapping(address => Checkpoints.History) claimHistory;

    // Modifiers
    /////////////////////
    modifier withGnosisSettedUp
    {
        require(gnosisSafeAddress != address(0), "you have to setup gnosis address first");
        _;
    }

    modifier withGnosisModuleEnabled
    {
        require(gnosisSafeAddress != address(0), "you have to enable NyxVault as a safe application first");
        _;
    }

    // Functions
    ///////////////////
    function setupGnosis(address addr) external virtual;
    function setCryptoVault(address addr) external virtual;
    function setNftVault(address addr) external virtual;
    function setVCVault(address addr) external virtual;
    function setRoyaltiesVault(address addr) external virtual;
    function setInvestmentProfitVault(address addr) external virtual;
    // function setDAOContract(address addr) external virtual;
    function setNFTContract(address addr) external virtual;
    function addNonPnlAddresses(address addr) external virtual;
    function removeNonPnlAddresses(address addr) external virtual;
    function transfer(address token, address payable to, uint96 amount) public virtual;
    function makeAllocation(uint256[3] calldata allocation) external virtual;
    function lockLiquidity(uint256 amount) internal virtual;
    function resetLockedLiquidity() external virtual;
    function getRedeemPower(address addr, uint256 blockNumber) public view virtual returns (uint256);
    function closeRedeem() public  virtual;
    function openRedeem(uint256 liquidity, uint256 pricePerReedemPower) public virtual;
    // function getRedeemPrice(uint256 tokenId) public view virtual returns (uint256);
    // function redeem(address payable to, uint256 amount) external virtual;
    function redeem(address to, uint256 redeemPrice, uint56 nNfts, uint256 tokenId) public virtual;
    function receiveFunds(bytes32 data) external payable virtual;
}

