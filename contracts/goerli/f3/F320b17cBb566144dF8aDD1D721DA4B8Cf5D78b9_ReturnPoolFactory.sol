    constructor(address _stakedToken, address _aaveToken, address _offsetToken, address tco2, address _aavePoolManager, address _NFT, address _SBT, address _returnPoolManager, bool _communityPool, uint _decimals) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(POOL_MANAGER, msg.sender);
        _grantRole(POOL_MANAGER, _returnPoolManager);

        //RETURN_NFT = ReturnNFT(_NFT);
        //RETURN_SBT = ClimatePassport(_SBT);

        STAKED_TOKEN = _stakedToken;
        AAVE_TOKEN = _aaveToken;
        OFFSET_TOKEN = _offsetToken;
        WMATIC = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6; // mainnet: 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270, testnet: 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
        TCO2 = tco2;
        AMOUNT_MULTIPLIER = 10**18 / 10**_decimals;
        TOKEN_MULTIPLIER = 10**_decimals;

        aavePoolManager = IPool(_aavePoolManager);
        sushiRouter = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D); // Mainnet: 0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506 Testnet: 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        //swapPath = [STAKED_TOKEN, WMATIC, OFFSET_TOKEN];
        swapPath = [STAKED_TOKEN, OFFSET_TOKEN];

        COMMUNITY_POOL = _communityPool;
    }

    function onERC721Received(address, address, uint256, bytes calldata) external override pure returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    // @dev: Stake nft with id ndfID in pool for amount of stakedToken
    // @param: uint nftID: ID of NFT to stake
    // @param: uint amount: Amount of stakedToken to stake
    function stakeNFT(uint nftID, uint amount) external onlyRole(POOL_MANAGER) {
        if (getAvailableYield() > 0) { // Check for available yield
            updateOffset();            // Collect yield and purchaseOffsets
        } else {
            updateYield();
            offsetPerShare.push(0);
            adjustedOffsetPerShare.push(0);
        }

        if (numUsers == 0) {
            nftStakeInfo[nftID].correctionIndex = 0;
            nftStakeInfo[nftID].offsetCorrectionIndex = 0;
        }
        else {
            // setCorrectinIndex = yieldPerShare.length;
            nftStakeInfo[nftID].correctionIndex = yieldPerShare.length;
            nftStakeInfo[nftID].offsetCorrectionIndex = offsetPerShare.length;
        }
        nftStakeInfo[nftID].amount = amount;
        nftStakeInfo[nftID].timestamp = block.timestamp;
        nftStakeInfo[nftID].stakedToken = STAKED_TOKEN;
        nftStakeInfo[nftID].offsetToken = TCO2;
        aaveSupply(amount);
        numUsers += 1;
    }

    // @dev: Unstake nft with id ndfID in pool for amount of stakedToken
    // @param: uint nftID: ID of NFT to withdraw from
    // @param: uint amount: Amount of stakedToken to withdraw
    function withdrawStake(uint nftID, uint amount, address user) external onlyRole(POOL_MANAGER) {
        updateOffset();
        _updateStake(nftID, amount, false);
        require(IERC20(STAKED_TOKEN).transfer(user, amount));
    }

    // @dev: add funds to an existing stake
    // @param: uint nftID: ID of NFT to add funds to
    // @param: uint amount: Amount of stakedToken to add
    function addFunds(uint nftID, uint amount) external onlyRole(POOL_MANAGER) {
        updateOffset();
        _updateStake(nftID, amount, true);
    }

    // @dev: internal function to either add or remove from existing stake
    // @param: uint nftID: ID of NFT to add/remove funds to/from
    // @param: uint amount: Amount of stakedToken to add/remove
    // @param: bool add: true if adding funds, false if removing funds
    function _updateStake(uint nftID, uint amount, bool add) internal {
        (uint total, uint yield) = getTotals(nftID);
        uint stakedAmount = nftStakeInfo[nftID].amount;
        (,uint offsetEarned,) = getTotalOffset(nftID);

        if (!add) { // user withdrawing stake
            require(stakedAmount >= amount);
            if (stakedAmount == amount) { // user withdrawing entire stake
                nftStakeInfo[nftID].amount = 0;
                yield = 0;
                numUsers -= 1;
            }
            else {
                nftStakeInfo[nftID].amount = stakedAmount - amount;
            }

            aaveWithdraw(amount, false);
        }
        else { // user adding to stake
            if (stakedAmount == 0) {
                numUsers += 1;
            }
            nftStakeInfo[nftID].amount = stakedAmount + amount - yield; // TODO: Changed from TOTAL, CHECK THIS
            aaveSupply(amount);
        }
        nftStakeInfo[nftID].correctionIndex = yieldPerShare.length;
        nftStakeInfo[nftID].yieldEarn = yield;
        nftStakeInfo[nftID].timestamp = block.timestamp;
        nftStakeInfo[nftID].offsetCorrectionIndex = offsetPerShare.length;
        nftStakeInfo[nftID].offsetEarn = offsetEarned;
    }

    // @dev: Redeems Toucan offset for TCO2
    // @param: uint received: Amount of offsetToken received from purchase
    // @param: bool bulk: true if one-time purchase, false if automated purchase
    // TODO: Selectively redeem offsets for specific TCO2 (TEST)
    function redeemAndRetireOffsets(uint received, bool bulk) internal returns (uint){
        string memory retirementMessage;
        if (bulk) {
            retirementMessage = "0";
        }
        else {
            retirementMessage = "1";
        }
        INCT nct = INCT(OFFSET_TOKEN);

        address[] memory tco2_add = new address[](1);
        uint[] memory offset_bal = new uint[](1);
        tco2_add[0] = TCO2;
        offset_bal[0] = IERC20(OFFSET_TOKEN).balanceOf(address(this));
        nct.redeemMany(tco2_add, offset_bal);

        uint balance = IERC20(TCO2).balanceOf(address(this));
        IToucanCarbonOffsets tco2 = IToucanCarbonOffsets(TCO2);
        tco2.retireAndMintCertificate('Return Protocol', address(this), '', retirementMessage, balance);
        emit OffsetsRetired(OFFSET_TOKEN, TCO2, balance);
        return balance;
    }

    // TODO: REMOVE THESE TESTING FUNCITONS
    function purchasePub() external {
        purchaseOffsets(swapPath);
    }

    function redeemPub() external {
        uint bal = IERC20(OFFSET_TOKEN).balanceOf(address(this));
        redeemAndRetireOffsets(bal, true);
    }

    // @dev: Checks yield, purchases offsetToken with yield, redeems and retires offsets, set global variables
    function updateOffset() public {
        updateYield();

        if (!COMMUNITY_POOL) {
            uint retired;
            uint received = purchaseOffsets(swapPath);
            if (received > 0) {
                retired = redeemAndRetireOffsets(received, true);
            }
            uint ops;
            if (lastBalance == 0) {
                ops = 0;
            }
            else {
                ops = (retired * WEI_MULTIPLIER) / (totalStakedAmount * AMOUNT_MULTIPLIER);
            }

            offsetPerShare.push(ops);

            uint opsLength = offsetPerShare.length;
            if (opsLength == 1) {
                adjustedOffsetPerShare.push(ops);
            }
            else {
                uint aOPS = (adjustedOffsetPerShare[adjustedOffsetPerShare.length - 1] + ops);
                adjustedOffsetPerShare.push(aOPS);
            }

            totalOffset += retired;

        }
    }

    // @dev: Checks yield, updates global variables
    function updateYield() internal {
        uint yield = getAvailableYield();
        uint yps;
        if (lastBalance == 0) {
            yps = 0;
        }
        else {
            yps = (yield * WEI_MULTIPLIER) / lastBalance;
        }

        yieldPerShare.push(yps);

        uint ypsLength = yieldPerShare.length;
        if (ypsLength == 1) {
            adjustedYieldPerShare.push(yps + WEI_MULTIPLIER);
        }
        else {
            uint aYPS = (adjustedYieldPerShare[adjustedYieldPerShare.length - 1] * (yps + WEI_MULTIPLIER)) / WEI_MULTIPLIER;
            adjustedYieldPerShare.push(aYPS);
        }

        lastBalance += yield;
        totalYield += yield;
    }


    // AAVE Functions
    // @dev: Supply stakedToken to AAVE
    // @param: uint amount: Amount of stakedToken to supply
    function aaveSupply(uint amount) internal {
        IERC20(STAKED_TOKEN).approve(address(aavePoolManager), amount);
        aavePoolManager.supply(STAKED_TOKEN, amount, address(this), 0);

        totalStakedAmount += amount;
        lastBalance += amount;
    }

    // @dev: Withdraw stakedToken from AAVE
    // @param: uint amount: Amount of stakedToken to withdraw
    // @param: bool yield: true if withdrawing yield, false if withdrawing stake
    function aaveWithdraw(uint amount, bool yield) internal {
        aavePoolManager.withdraw(STAKED_TOKEN, amount, address(this));

        lastBalance -= amount;
        if (!yield) {
            totalStakedAmount -= amount;
        }
    }

    // @dev: calculates current yield value from aave
    function getAvailableYield() public view returns(uint yield) {
        uint liquidityIndex = aavePoolManager.getReserveData(STAKED_TOKEN).liquidityIndex;
        uint scaledBalance = IAToken(AAVE_TOKEN).scaledBalanceOf(address(this));
        uint balance = scaledBalance * liquidityIndex / RAY_MULTIPLIER;
        if (balance > totalStakedAmount) {
            yield = balance - totalStakedAmount;
        }
        else {
            yield = 0;
        }
    }

    // @dev: calculates current yield for a staked nft
    // @return: (uint total, uint yield): initial amount staked, yield earned
    function getTotals(uint nftID) public view returns (uint total, uint yield) {
        NFTStakeInfo memory info = nftStakeInfo[nftID];

        uint correctionIndex = info.correctionIndex;
        if (correctionIndex == yieldPerShare.length) {
            total = info.amount + info.yieldEarn;
        }
        else if (correctionIndex > 0) {
            total = ((info.amount + info.yieldEarn) * ((adjustedYieldPerShare[adjustedYieldPerShare.length - 1] * TOKEN_MULTIPLIER) / adjustedYieldPerShare[correctionIndex - 1])) / TOKEN_MULTIPLIER;
        }
        else {
            total = ((info.amount + info.yieldEarn) * adjustedYieldPerShare[adjustedYieldPerShare.length - 1]) / WEI_MULTIPLIER;
        }
        total = total - info.yieldSpend;
        yield = total - info.amount;
        return(total, yield);
    }

    // @dev: calculates offset earned for a staked nft
    // @return: uint total: total offset earned
    // @return: uint staking: offset earned from staking
    // @return: uint historical: historical offsets purchased
    function getTotalOffset(uint nftID) public view returns (uint total, uint staking, uint oneTime) {
        NFTStakeInfo memory info = nftStakeInfo[nftID];
        uint correctionIndex = info.offsetCorrectionIndex;
        if (correctionIndex == offsetPerShare.length) {
            staking = info.offsetEarn;
        }
        else if (correctionIndex > 0) {
            staking = ((info.amount * AMOUNT_MULTIPLIER) * ((adjustedOffsetPerShare[adjustedOffsetPerShare.length - 1] - adjustedOffsetPerShare[correctionIndex - 1]))) / WEI_MULTIPLIER + info.offsetEarn;
        }
        else {
            staking = ((info.amount * AMOUNT_MULTIPLIER) * adjustedOffsetPerShare[adjustedOffsetPerShare.length - 1]) / WEI_MULTIPLIER  + info.offsetEarn;
        }
        oneTime = info.oneTimeOffset;
        return(staking + oneTime, staking, oneTime);
    }

    // @dev: placeholder function for community pools, allows pool manager to withdraw all yield for use as determined by stakers
    //       this functionality will be updated to a governance model
    function unstakeCommunityYield(uint amount, address treasury) external onlyRole(POOL_MANAGER) {
        require(COMMUNITY_POOL);
        uint available = getAvailableYield();
        aaveWithdraw(available, true);
        require(IERC20(STAKED_TOKEN).transfer(treasury, available));
    }

    // @dev: swap stakedToken for offsetToken
    // @param: address[] memory path: path to swap stakedToken for offsetToken
    function purchaseOffsets(address[] memory path) internal returns(uint) {
        require(path[0] == STAKED_TOKEN && path[path.length - 1] == OFFSET_TOKEN);
        uint available = getAvailableYield();
        if (available > 0) {
            aaveWithdraw(available, true);
            uint received = swapTokenForToken(available, 0, path);
            return received;
        }
        else {
            return(0);
        }
    }

    // @dev: one time offset for a staked NFT
    // @param: uint nftID: ID of staked NFT
    // @param: uint amount: amount of stakedToken to swap for offsetToken
    // @return: uint: amount of offsetToken received

    function offsetWalletHistory(uint nftID, uint amount) external returns(uint) {
        if (nftStakeInfo[nftID].timestamp == 0) { // New NFT not staked in pool
            nftStakeInfo[nftID].offsetToken = TCO2;
            nftStakeInfo[nftID].stakedToken = STAKED_TOKEN;
        }
        uint received = swapTokenForToken(amount, 0, swapPath);
        uint retired = redeemAndRetireOffsets(received, false);

        nftStakeInfo[nftID].oneTimeOffset += retired;
        nftStakeInfo[nftID].timestamp = block.timestamp;
        totalSingleOffset += retired;

        return retired;
    }

    // @dev: swap stakedToken for offsetToken
    // @param: uint amountIn: amount of stakedToken to swap for offsetToken
    // @param: uint amountOut: minimum amount of offsetToken to receive
    // @param: address[] memory path: path to swap stakedToken for offsetToken
    // @return: uint: amount of offsetToken received
    // TODO: Slippage
    function swapTokenForToken(uint amountIn, uint amountOut, address[] memory path) internal returns(uint) {
        IERC20(path[0]).approve(address(sushiRouter), amountIn);
        uint amount = IERC20(path[path.length - 1]).balanceOf(address(this));
        sushiRouter.swapExactTokensForTokens(
            amountIn,
            amountOut,
            path,
            address(this),
            block.timestamp
        );
        amount = IERC20(path[path.length - 1]).balanceOf(address(this)) - amount;
        return amount;
    }

    function updatePaths(address[] memory _path) external onlyRole(POOL_MANAGER) {
        swapPath = _path;
    }

    function setRouterAddress(address _router) external onlyRole(POOL_MANAGER) {
        sushiRouter = IUniswapV2Router02(_router);
    }

    function setTCO2Address(address _tco2) external onlyRole(POOL_MANAGER) {
        TCO2 = _tco2;
    }


    // DANGER ZONE, EXERCISE EXTREME CAUTION WHEN CALLING THESE FUNCTIONS, COULD BREAK THE CONTRACT IF HANDLED IMPROPERLY
    function setOPSMultiplier(uint _multiplier) external onlyRole(POOL_MANAGER) {
        OPS_MULTIPLIER = _multiplier;
    }

    function setAmountMultiplier(uint _multiplier) external onlyRole(POOL_MANAGER) {
        AMOUNT_MULTIPLIER = _multiplier;
    }

    function setBasisPoints(uint _basisPoints) external onlyRole(POOL_MANAGER) {
        BASIS_POINTS = _basisPoints;
    }

    function setWeiMultiplier(uint _multiplier) external onlyRole(POOL_MANAGER) {
        WEI_MULTIPLIER = _multiplier;
    }

    function setTokenMultiplier(uint _multiplier) external onlyRole(POOL_MANAGER) {
        TOKEN_MULTIPLIER = _multiplier;
    }

    // BETA EMERGENCY FUNCTIONS
    function setEmergencyMode() external onlyRole(POOL_MANAGER) {
        EMERGENCY_MODE = true;
    }
    // @DEV: Beta function only, unstake all aave stake in case of the need to return user funds
    // TODO: TEST THIS, MIGHT HAVE TO GET BALANCE THE OTHER WAY
    function unstakeALL() external onlyRole(POOL_MANAGER) {
        require(EMERGENCY_MODE);
        uint aBal = IERC20(AAVE_TOKEN).balanceOf(address(this));
        aavePoolManager.withdraw(STAKED_TOKEN, aBal, address(this));
    }

    function userEmergencyUnstake(uint nftID) external {
        require(EMERGENCY_MODE);
        uint amt = nftStakeInfo[nftID].amount;
        nftStakeInfo[nftID].amount = 0;
        IERC20(STAKED_TOKEN).transfer(msg.sender, amt);
    }
}

