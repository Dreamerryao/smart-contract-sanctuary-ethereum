pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "./ReturnPool.sol";

contract ReturnPoolFactory is Initializable, AccessControlUpgradeable, UUPSUpgradeable {

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    address public poolManagerAddress;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyRole(UPGRADER_ROLE)
    override
    {}

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(AccessControlUpgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    // @dev: Create a new ReturnPool
    // @param: address stakedToken: The address of the token to be staked
    // @param offsetToken: offset token address, (0 address for community pool)
    // @param aaveToken: address of received aToken
    // @param aavePoolManager: address of AavePoolManager
    // @return returnPool: address of the return pool
    function createNewReturnPool(address stakedToken, address aaveToken, address offsetToken, address tco2, address aavePoolManager, address return_nft, address return_sbt, bool _communityPool, uint _decimals) external onlyRole(MANAGER_ROLE) returns (address) {
        ReturnPool newPool = new ReturnPool(stakedToken, aaveToken, offsetToken, tco2, aavePoolManager, return_nft, return_sbt, poolManagerAddress, _communityPool, _decimals);
        return address(newPool);
    }


    function setPoolManagerAddress(address _poolManagerAddress) external onlyRole(MANAGER_ROLE) {
        poolManagerAddress = _poolManagerAddress;
        _grantRole(MANAGER_ROLE, _poolManagerAddress);

    }
}

