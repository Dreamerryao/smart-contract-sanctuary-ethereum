interface IERC4626 {
  function deposit(
    uint assets,
    address receiver
  ) external;

  function withdraw(uint assets, address receiver) external;

  function redeem(
    uint shares,
    address receiver,
    address owner
  ) external returns (uint assets);

  event Deposit(
    address indexed caller,
    address indexed owner,
    uint assets);

  event Withdraw(
    address indexed caller,
    address indexed receiver,
    address indexed owner,
    uint termId,
    uint assets,
    uint shares
  );
}

