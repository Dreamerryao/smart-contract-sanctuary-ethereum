pragma solidity 0.8.9;

import "./IERC4626.sol";

interface IVault is IERC4626 {

  struct UnMintDeposit {
    uint termId;
    uint amount;
  }

  struct RedeemInfo {
    uint termId;
    uint share;
  }

  event TransferToStrategy(address strategy, uint amount);

  function collateralToken() external view returns (address);

  function getAmountsForNextTerm() external view returns (uint amountForThisTerm, uint shareAmountForThisTerm);

  function transferToStrategy(uint _amountForNextTerm) external;

  function addManageFee(uint termId, uint fee) external;

  function initialize(address collateralToken, address vaulter) external;
}

