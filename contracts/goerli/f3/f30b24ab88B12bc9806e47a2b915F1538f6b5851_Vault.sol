pragma solidity 0.8.9;

import "hardhat/console.sol";

abstract contract Term {
  struct TermInfo {
    uint expiry;
    uint strikePrice;
    uint amountForThisTerm;
    uint shareAmountForThisTerm;
    bool ended;
    uint sharePriceWithMantissa;
    uint manageFee;
    uint totalBalance;
  }

  uint internal constant maxDuration = 31 days;
  uint internal constant minDuration = 6 days;
  mapping(uint => TermInfo) public termInfos;
  uint public termLength;

  function _startTerm(uint _termId, TermInfo memory _ti) internal {
    //check term info
    require(_termId == termLength, "t._startTerm: invalid termId");
    if (_termId > 0) {
      require(termInfos[_termId - 1].ended, "t._startTerm: previous not ended");
    }
    require(
      _ti.expiry > block.timestamp + minDuration && _ti.expiry < block.timestamp + maxDuration,
      "t._startTerm: invalid expiry"
    );
    require(!_ti.ended, "t._startTerm: invalid ended");

    termInfos[_termId] = _ti;
    termLength++;
  }

  function _endTerm(
    uint _termId,
    uint _sharePriceWithMantissa,
    uint _manageFee,
    uint _totalBalance
  ) internal {
    require(_termId == termLength - 1, "t._endTerm: invalid termId");
    TermInfo storage termInfo_ = termInfos[_termId];
    require(!termInfo_.ended, "t._endTerm: term ended");

    termInfo_.ended = true;
    termInfo_.sharePriceWithMantissa = _sharePriceWithMantissa;
    termInfo_.manageFee = _manageFee;
    termInfo_.totalBalance = _totalBalance;
  }

  function getLatestTermInfo() external view returns (TermInfo memory) {
    return termLength == 0 ? TermInfo(0, 0, 0, 0, true, 10 ** 18, 0, 0) : termInfos[termLength - 1];
  }

  function getTermInfo(uint _termId) external view returns (TermInfo memory) {
    require(_termId < termLength, "t.getTermInfo: invalid termId");
    return termInfos[_termId];
  }
}

