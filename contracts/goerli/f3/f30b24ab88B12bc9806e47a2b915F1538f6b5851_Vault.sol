pragma solidity 0.8.9;

import "./interfaces/IVault.sol";
import "./utils/Term.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract Vault is IVault, ERC20 {
  address public vaulter;
  address public collateralToken;
  Term public strategy;
  address public factory;
  uint public amountForNextTerm; //the amount deposited during current term's active and next term's preparation period
  uint public shareAmountForNextTerm; //the lp amount minted during next term's preparation period
  uint public totalManageFee;
  uint public extractedManageFee;
  uint public tempAmountForNextTerm;
  uint public redeemShareNextTerm;

  // only one for address
  mapping(address => UnMintDeposit) public unMintDeposit;

  mapping(address => RedeemInfo) public lastRedeemInfo;

  constructor() ERC20("VaultShare", "VS") {
    factory = msg.sender;
  }

  function initialize(address _collateralToken, address _vaulter) external override {
    require(msg.sender == factory, "v.initialize: only factory");
    collateralToken = _collateralToken;
    vaulter = _vaulter;
  }

  function deposit(uint _amount, address _to) external override {
    require(IERC20(collateralToken).transferFrom(msg.sender, address(this), _amount), "v.deposit: transferFrom error");
    Term.TermInfo memory _latestTermInfo = strategy.getLatestTermInfo();
    UnMintDeposit memory _unMint = unMintDeposit[_to];
    if (_latestTermInfo.ended) {
      uint _tempShares = (_amount * 1e18) / _latestTermInfo.sharePriceWithMantissa;
      shareAmountForNextTerm += _tempShares;
      _mint(_to, _tempShares);
      if (_unMint.amount > 0) {
        _handleUnMint(_unMint, _to);
      }
    } else {
      uint _termLength = strategy.termLength();
      if (_unMint.amount == 0) {
        unMintDeposit[_to] = UnMintDeposit(_termLength, _amount);
      } else {
        if (_unMint.termId < _termLength) {
          Term.TermInfo memory _termInfo = strategy.getTermInfo(_unMint.termId - 1);
          uint _mintShares = (_unMint.amount * 1e18) / _termInfo.sharePriceWithMantissa;
          unMintDeposit[_to] = UnMintDeposit(_termLength, _amount);
          _mint(_to, _mintShares);
        } else {
          unMintDeposit[_to].amount += _amount;
        }
      }
      tempAmountForNextTerm += _amount;
    }
    amountForNextTerm += _amount;
    emit Deposit(msg.sender, _to, _amount);
  }

  //can withdraw assets deposited before next term starts
  function withdraw(uint assets, address receiver) external override {
    require(assets > 0, "v.withdraw: assets must be gt 0");
    require(receiver != address(0), "v.withdraw: zero address");
    require(unMintDeposit[msg.sender].termId == strategy.termLength(), "v.withdraw: not found");
    require(assets <= unMintDeposit[msg.sender].amount, "v.withdraw: balance insufficient");
    unMintDeposit[msg.sender].amount -= assets;
    tempAmountForNextTerm -= assets;
    amountForNextTerm -= assets;
    IERC20(collateralToken).transfer(receiver, assets);
  }

  //can redeem assets with lp
  function redeem(
    uint _share,
    address _to,
    address _owner
  ) external override returns (uint _amount) {
    if (msg.sender != _owner) {
      _spendAllowance(_owner, msg.sender, _share);
    }
    (uint _ownerShare, Term.TermInfo memory _latestTermInfo) = _getTermShares(_owner);
    require(_share <= _ownerShare, "v.redeem: invalid share");
    uint _termLength = strategy.termLength();
    RedeemInfo memory _lastRedeemInfo = lastRedeemInfo[_owner];
    UnMintDeposit memory _unMint = unMintDeposit[_owner];
    if (_latestTermInfo.ended) {
      if (_lastRedeemInfo.share > 0) {
        Term.TermInfo memory _termInfo = strategy.getTermInfo(_lastRedeemInfo.termId);
        _amount += (_lastRedeemInfo.share * _termInfo.sharePriceWithMantissa) / 1e18;
        lastRedeemInfo[_owner] = RedeemInfo(0, 0);
      }

      if (_unMint.amount > 0) {
        _handleUnMint(_unMint, _owner);
        if (_unMint.termId == _termLength) {
          tempAmountForNextTerm -= _unMint.amount;
          amountForNextTerm -= _unMint.amount;
        }
      }

      if (_share > 0) {
        _amount += (_share * _latestTermInfo.sharePriceWithMantissa) / 1e18;
      }
    } else {
      if (_unMint.amount > 0 && _unMint.termId < _termLength) {
        _handleUnMint(_unMint, _owner);
      }
      if (_lastRedeemInfo.share == 0) {
        lastRedeemInfo[_owner] = RedeemInfo(_termLength - 1, _share);
      } else {
        if (_termLength - 1 == _lastRedeemInfo.termId) {
          lastRedeemInfo[_owner].share += _share;
        } else {
          Term.TermInfo memory _termInfo = strategy.getTermInfo(_lastRedeemInfo.termId);
          _amount += (_lastRedeemInfo.share * _termInfo.sharePriceWithMantissa) / 1e18;
          lastRedeemInfo[_owner] = RedeemInfo(_termLength - 1, _share);
        }
      }
    }
    redeemShareNextTerm += _share;
    _burn(_owner, _share);
    if (_amount > 0) {
      IERC20(collateralToken).transfer(_to, _amount);
    }
    emit Withdraw(msg.sender, _to, _owner, strategy.termLength(), _amount, _share);
  }

  function getTermShares(address _to) external view returns (uint _shares) {
    (_shares, ) = _getTermShares(_to);
  }

  function getAmountsForNextTerm() external view override returns (uint, uint) {
    Term.TermInfo memory _latestTermInfo = strategy.getLatestTermInfo();
    require(_latestTermInfo.ended, "v.getAmountForNextTerm: current term must be end");
    require(_latestTermInfo.sharePriceWithMantissa > 0, "v.getAmountForNextTerm: sharePrice must be gl 0");
    uint otherShares = (tempAmountForNextTerm * 1e18) / _latestTermInfo.sharePriceWithMantissa;
    uint _amountForNextTerm = _latestTermInfo.totalBalance +
      amountForNextTerm -
      (redeemShareNextTerm * _latestTermInfo.sharePriceWithMantissa) /
      1e18 -
      totalManageFee +
      extractedManageFee;
    uint _shareAmountForNextTerm = _latestTermInfo.shareAmountForThisTerm +
      shareAmountForNextTerm +
      otherShares -
      redeemShareNextTerm;
    return (_amountForNextTerm, _shareAmountForNextTerm);
  }

  function addManageFee(uint _termId, uint _manageFee) external onlyStrategy {
    require(_termId == strategy.termLength() - 1, "v.addManageFee: invalid termId");
    Term.TermInfo memory _termInfo = strategy.getTermInfo(_termId);
    require(_termInfo.ended, "v.addManageFee: term must be end");
    totalManageFee += _manageFee;
  }

  function transferToStrategy(uint _amountForNextTerm) external onlyStrategy {
    IERC20(collateralToken).transfer(address(strategy), _amountForNextTerm);
    amountForNextTerm = 0;
    shareAmountForNextTerm = 0;
    tempAmountForNextTerm = 0;
    redeemShareNextTerm = 0;
  }

  function withdrawManageFee() external onlyVaulter() {
    uint _amount = totalManageFee - extractedManageFee;
    require(_amount > 0, "v.withdrawManageFee: amount must be gl 0");
    extractedManageFee += _amount;
    IERC20(collateralToken).transfer(msg.sender, _amount);
  }

  function setStrategy(address _strategy) external onlyVaulter() {
    strategy = Term(_strategy);
  }

  function setVaulter(address _vaulter) external {
    require(msg.sender == vaulter, "v.setVaulter: only vaulter");
    vaulter = _vaulter;
  }

  function _handleUnMint(UnMintDeposit memory _unMint, address _owner) internal {
    Term.TermInfo memory _termInfo = strategy.getTermInfo(_unMint.termId - 1);
    uint _mintShares = (_unMint.amount * 1e18) / _termInfo.sharePriceWithMantissa;
    unMintDeposit[_owner] = UnMintDeposit(0, 0);
    _mint(_owner, _mintShares);
  }

  function _getTermShares(address _to) internal view returns (uint _shares, Term.TermInfo memory _latestTermInfo) {
    _latestTermInfo = strategy.getLatestTermInfo();
    _shares = balanceOf(_to);
    UnMintDeposit memory _unMint = unMintDeposit[_to];
    if (_unMint.amount > 0 && (_latestTermInfo.ended || strategy.termLength() > _unMint.termId)) {
      Term.TermInfo memory _termInfo = strategy.getTermInfo(_unMint.termId - 1);
      _shares += ((_unMint.amount * 1e18) / _termInfo.sharePriceWithMantissa);
    }
  }

  modifier onlyStrategy() {
    require(msg.sender == address(strategy), "v.onlyStrategy: only strategy");
    _;
  }

  modifier onlyVaulter() {
    require(msg.sender == address(vaulter), "v.onlyVaulter: only vaulter");
    _;
  }
}

