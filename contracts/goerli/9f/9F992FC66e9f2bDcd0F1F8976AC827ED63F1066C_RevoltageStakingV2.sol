    function buy() external payable {
        uint256 ethAmount = msg.value;
        address payable buyer = payable(msg.sender);
        address payable seller = payable(address(this));

        uint256 rate = 100000000000; // 0,05 ETH für einen Revo //_rates[currency];
        //require(rate > 0, "Currency is not supported");

        require(buyer != address(0), "sender cannot be the zero address");
        uint256 ethBalance = buyer.balance;
        require(ethAmount <= ethBalance, "Not enough ETH in balance");

        uint256 rvoBanalance = balanceOf(seller);
        uint256 rvoAmount = convertEthToRvo(ethAmount, rate);

        require(rvoAmount > 0, "Amount too low");
        require(rvoAmount <= rvoBanalance, "Not enough RVO in balance");
        //seller.transfer(ethAmount);
        (bool sent,/*memory data*/) = seller.call{value: ethAmount}("");
        // console.log(sent);
        require(sent, "Failed to send Ether");

        _transfer(seller, buyer, rvoAmount);

        emit Bought(rvoAmount, 1 * 10**_decimals, ethAmount);
    }

    function convertEthToRvo(uint256 ethAmount, uint256 rate)
        public
        view
        returns (uint256)
    {
        // eth: (0,4) 40000000 0000000000
        // kurs: 1000 00000000
        // rvo: 400 00000000

        // rechnung: 400000000000000000 * 100000000000 = 40000000000000000000000000000 / 10000000000 = 40000000000000000000000000000
        // rechnung: 40000000 (0000000000) * 100000000000 = 40000000000 (00000000) / 100000000 = 40000000000

        uint256 eth8 = ethAmount / 10 ** 10;

        uint256 rvoAmount = (eth8 * rate) / 10 ** _decimals;
        return rvoAmount;
    }

