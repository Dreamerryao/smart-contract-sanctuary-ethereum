    function getStakeForAccount(address account, uint256 index) public view 
    returns (StakeItem memory) {
        //return stakes[msg.sender].stakedItems;
        require(index < stakes[account].stakedItems.length, "No stake at index");
        StakeItem memory item = _helper_getStakeForAccount(account, index);
        require(item.rewarded == false, "No running stake");
        return item;
    }

    function getStakesForAccount(address account) public view 
    returns (StakeItemResult[] memory) {
        require(account != address(0), "Zero address cannot stake");
        StakeItem[] memory items = _helper_getStakesForAccount(account);
        require(items.length > 0, "Account has no stake in the system!");
        uint256 insertCounter= 0;
        uint256 counter = 0;
        for(uint index = 0; index < items.length; index++) {
            if(items[index].rewarded == false){
                counter++;
            }     
        }
        StakeItemResult[] memory results = new StakeItemResult[](counter);
        for(uint index = 0; index < items.length; index++) {
            if(items[index].rewarded == false){
                results[insertCounter] =  StakeItemResult({index: index, item: items[index]});
                insertCounter++;
            }     
        }
        return results;
    }


