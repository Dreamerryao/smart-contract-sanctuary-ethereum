contract RevoltageStakingV2 is Ownable, ReentrancyGuard, Pausable {
    using Counters for Counters.Counter;
    using DateTime for uint256;
    string private constant _name = "RevoltageStakingV2";
     Counters.Counter private s_baseDataId;
    Counters.Counter private _itemsIds;
    Counters.Counter private _rewardsIds;
    Revoltage public revoltage;

    struct BaseData {
        uint256 baseDataId;
        uint256 stakingStartDate;               // step 1
        uint256 stakingEndDate;                 // step 1

        uint256 claimingStartDate;              // step 1
        uint256 claimingEndDate;                // step 1
        uint256 claimingExtendedEndDate;        // step 1

        uint256 rewardTotal;                    // step 2
        uint256 rewardPerToken;                 // step 2
        bool running;
    }

    struct Staker {
        StakeItem[] stakedItems;
    }

    struct StakeItem {
        uint256 id; // stakeItemId
        uint256 baseDataId; // id from BaseData
        uint256 start;  // has to be stakingEndDate -  from BaseData
        uint256 end; // has to be  claimingStartDate -  from BaseData
        uint256 amount;
        bool rewarded;
    }

    struct BaseDataStakerMapping {
        address staker;
        uint256[] indexes;
    }

    struct StakeItemResult {
        uint256 index;
        StakeItem item;
    }
 
    BaseData[] private baseDataList;
    address[] private stakeholders;
    uint256 private s_minStakeAmount;
    uint256 private stakingPeriod; // in days eg 7 for 7 days
    uint256 private earningPeriod; //in days : between s_stakingEndDate and s_claimingStartDate
    uint256 private claimingPeriod; //in days
    uint256 private claimingPeriodExtended;

    uint256 private _totalSupply;

    uint256 private s_stakingStartDate;
    uint256 private s_stakingEndDate;
    uint256 private s_claimingStartDate;
    uint256 private s_claimingEndDate;
    uint256 private s_claimingExtendedEndDate;

    mapping(address => uint256) private s_balances;
    mapping(address => Staker) internal stakes;
    mapping(address => bool) public hasStaked;
    mapping(uint256 => uint256) public baseDataIdToIndex;
    mapping(uint256 => uint256) public baseDataIdToAmount;

    //v1
    mapping(uint256 => int256[]) internal stakingProtocollPeriod;   

    //v2
    mapping(uint256 => address[]) internal stakerPerPeriod;
    mapping(uint256 => mapping(address => uint256)) internal stakesPerPeriod;


   constructor(
        Revoltage _revoltage,
        uint256 _minStakeAmount
    ) {
        revoltage = _revoltage;
        s_minStakeAmount = _minStakeAmount;
    }

    event AddBaseData(
        uint256 indexed e_baseId, 
        uint256 indexed e_stakingStartDate,
        uint256 indexed e_stakingEndDate,
        uint256 e_claimingStartDate, 
        uint256 e_claimingEndDate
    );

    function dateToTimestamp(uint256 day, uint256 month, uint256 year) private pure returns (uint256 timestamp){
        bool isAValidDate = year.isValidDate(month,day);
        if(!isAValidDate) return 0;
        timestamp = year.timestampFromDate(month,day);
        return timestamp;
    }

    function setStakingStartDate(uint256 day, uint256 month, uint256 year) public {
        s_stakingStartDate = dateToTimestamp(day,month,year);
        require(s_stakingStartDate >= block.timestamp, "Staking start date must be at least current timestamp");
    }

    function setStakingEndDate(uint256 day, uint256 month, uint256 year) public {
        s_stakingEndDate = dateToTimestamp(day,month,year);
        require(s_stakingStartDate.diffDays(s_stakingEndDate) > 1,"Staking end - must be greather than staking start");
    }

    function setClaimingStartDate(uint256 day, uint256 month, uint256 year) public {
        s_claimingStartDate = dateToTimestamp(day, month, year);
        require( s_stakingEndDate.diffDays(s_claimingStartDate) > 1,"Claiming start - staking end must be at least equal earningPeriod");
    }

    function setClaimingEndDate(uint256 day, uint256 month, uint256 year) public {
        s_claimingEndDate = dateToTimestamp(day, month, year);
        require( s_claimingStartDate.diffDays(s_claimingEndDate) > claimingPeriod,"Claiming start - claiming end must be at least equal claimingPeriod");
    }

    function setStakingPeriod() private {
        require(s_stakingStartDate.diffDays(s_stakingEndDate) > 1, "make sure to set staking start and end dates");
        stakingPeriod = s_stakingStartDate.diffDays(s_stakingEndDate);
    }

    function setEarningPeriod() private {
       require(s_stakingEndDate.diffDays(s_claimingStartDate) > 1, "make sure to set staking end and claiming start dates");
       earningPeriod = s_stakingEndDate.diffDays(s_claimingStartDate);
    }

    function setClaimingPeriod() private {
       require(s_claimingStartDate.diffDays(s_claimingEndDate) > 1, "make sure claiming start and end are set");
       claimingPeriod = s_claimingStartDate.diffDays(s_claimingEndDate);
    }
    
    function setUpPeriods () public onlyOwner {
        setStakingPeriod();
        setEarningPeriod();
        setClaimingPeriod();
    }
    
    event SetMinStakeAmount(uint256 minStakeAmount);
    function setMinStakeAmount(uint256 fullRevo) public onlyOwner {
        s_minStakeAmount = fullRevo;
        emit SetMinStakeAmount(fullRevo);
    }
    
    function addBaseData() public onlyOwner {

        //require(_stakingStartDate >= block.timestamp, "Staking start date must be at least current timestamp");
        //require(uint256.diffDays(s_stakingStartDate, s_stakingEndDate) <= stakingPeriod,"Staking end - start must be at least equal stakingPeriod");
        //require(uint256.diffDays(s_stakingStartDate, s_stakingEndDate) <= stakingPeriod,"Staking end - start must be at least equal stakingPeriod");
        //require( uint256.diffDays(s_claimingStartDate, s_claimingEndDate) >= claimingPeriod,"Claiming start - claiming end must be at least equal claimingPeriod");

        s_baseDataId.increment();
        BaseData memory baseData;
        baseData.baseDataId = s_baseDataId.current();
        baseData.stakingStartDate = s_stakingStartDate;
        baseData.stakingEndDate = s_stakingEndDate;
        baseData.claimingStartDate = s_claimingStartDate;
        baseData.claimingEndDate = s_claimingEndDate;
        baseData.running = true;
        /** Not known properties for now */
        //baseData.claimingExtendedEndDate = _claimingExtendedEndDate;
        baseData.rewardTotal ;
        baseData.rewardPerToken;

        /** How do we provide this contract with reward Token after */
        //revoltage.transferFrom(msg.sender, address(this), amountTotal);
        //_balances[address(this)] = _balances[address(this)] + amountTotal;

        baseDataList.push(baseData);
        baseDataIdToIndex[baseData.baseDataId] = baseData.baseDataId-1;
        emit AddBaseData(
            baseData.baseDataId, 
            baseData.stakingStartDate, 
            baseData.stakingEndDate, 
            baseData.claimingStartDate,
            baseData.claimingEndDate);
    }

    function removeBaseData(uint256 baseDataIndex) public onlyOwner{

        require(baseDataIndex < baseDataList.length, "index out of bound");
        require(baseDataList[baseDataIndex].running, "Not a valid(running) baseDataIndex");

        //BaseData memory currentBase = baseDataList[baseDataIndex];

        for(uint256 i = baseDataIndex; i < baseDataList.length -1; i++){
            baseDataList[i] = baseDataList[i+1];
        }
        baseDataList.pop();
    }

    function createStakeItem(uint256 amount, uint256 baseDataIndex) private
    returns (StakeItem memory) {

        require(baseDataList.length > 0, "No staking params available");
        require(baseDataList[baseDataIndex].running, "Not a valid(running) baseDataIndex");

        BaseData memory currentBase = baseDataList[baseDataIndex];

        _itemsIds.increment();
        StakeItem memory item;
        item.id = _itemsIds.current();
        item.baseDataId = currentBase.baseDataId;
        item.start = currentBase.stakingEndDate;
        item.end = currentBase.claimingStartDate;
        item.amount = amount;
        item.rewarded = false;
        return item;
    }

event StakeTokens(address indexed e_stakerAddress, uint256 indexed e_amount, uint256 indexed e_itemId);

    function stakeTokens(uint256 amount, uint256 baseDataIndex) external nonReentrant whenNotPaused 
    returns (bool) {
        require(_msgSender() != address(0), "staker cannot be the zero address");
        require(amount > 0, "amount has to greatherthan zero");
        require(amount >= s_minStakeAmount,"amount is less than minimum required");
        //require(baseDataList.length > 0, "Please you can't stake now! there is no BaseData!");
        StakeItem memory item = createStakeItem(amount, baseDataIndex);

        uint256 stakerBalance = revoltage.balanceOf(msg.sender);
        require(stakerBalance >= amount, "Amount exceeds current balance");

        revoltage.transferFrom(msg.sender, address(this), amount);
        //update staking Balance
        s_balances[msg.sender] = s_balances[msg.sender] + amount;
        s_balances[address(this)] = s_balances[address(this)] + amount;
        stakes[msg.sender].stakedItems.push(item);
        // track the amount of token staked
        baseDataIdToAmount[item.baseDataId] += amount;
        // add user to stakeholders array *only* if they haven't staked already
        if (!hasStaked[msg.sender]) {
            stakeholders.push(msg.sender);
            hasStaked[msg.sender] = true;
        }

        emit StakeTokens(msg.sender, amount, item.id);
        return true;
    }

    event UnstakeTokens(address indexed e_unstakerAddress, uint256 indexed e_amount, uint256 indexed e_itemId);

    function unstakeTokens(uint256 index) public nonReentrant 
    {
        //Reward memory reward = rewards[item.rewardId];
        //require(item.running, "stake is not running");
        //require(reward.running, "No running reward for this stake!");
        //require(block.timestamp >= item.end,"Wait till minStakeDuration date before unstake!");
        //bool canUnstake = isDateCompareInDayRage(item.start, block.timestamp); //item.end always true
        //require(canUnstake, "Wait till minStakeDuration date before unstake!");

        require(_msgSender() != address(0), "Zero address cannot unstake(receive token)");
        StakeItem memory item = _helper_getStakeForAccount(_msgSender(), index);
        require(item.rewarded == false, "The stake at this index was already rewarded");
        uint256 baseDataIndex = baseDataIdToIndex[item.baseDataId];
        BaseData memory currentBase = baseDataList[baseDataIndex];
        require(currentBase.running, "Not a valid(running) baseDataIndex");

        if(currentBase.claimingStartDate > block.timestamp) {
            //we just transfer staker's token back
            revoltage.transfer(_msgSender(), item.amount); 
            //update staker Balance on this contract
            s_balances[msg.sender] = s_balances[msg.sender] - item.amount;
            //update contract Balance on this contract
            s_balances[address(this)] = s_balances[address(this)] - item.amount;
            //removes from stakedItems array?
            delete stakes[_msgSender()].stakedItems[index];
            baseDataIdToAmount[item.baseDataId] -= item.amount;
        }

        if(currentBase.claimingStartDate <= block.timestamp) {
            //reward für dieses staker berechnen
            uint256 stakingReward = item.amount * currentBase.rewardPerToken;
            uint256 tokenPlusReward = item.amount + stakingReward;
            //transfer token plus reward tothis user
            revoltage.transfer(_msgSender(), tokenPlusReward);
            //update staker Balance on this contract
            s_balances[msg.sender] = s_balances[msg.sender] - item.amount;
            //update contract Balance on this contract
            s_balances[address(this)] = s_balances[address(this)] - tokenPlusReward;
            //removes from stakedItems array?
            delete stakes[_msgSender()].stakedItems[index];
            baseDataIdToAmount[item.baseDataId] -= item.amount;
            item.rewarded == true;

        }

        emit UnstakeTokens(msg.sender, item.amount, item.id);
    }

    function unstakeAllTokens() public nonReentrant 
    {
        StakeItem[] memory items = _helper_getStakesForAccount(_msgSender());
        require(items.length > 0, "No Stakes currently on this account");
        for (uint i = 0; i < items.length; i++) {
            if(items[i].rewarded == false){
                unstakeTokens(i); 
            } 
        }
    }

    function _helper_getStakeForAccount(address account, uint256 index) internal view
    returns (StakeItem memory) 
    {
        return stakes[account].stakedItems[index];
    }

    function _helper_getStakesForAccount(address account) internal view
    returns (StakeItem[] memory)
    {
        return stakes[account].stakedItems;
    }

    event AddReward(uint256 amountTotal, uint256 amountPerToken, uint256 baseDataId);
    function addReward(
        uint256 baseDataIndex,
        uint256 amountTotal
    ) public onlyOwner {
        BaseData memory currentBase = baseDataList[baseDataIndex];
        require(currentBase.running, "Not a valid(running) baseDataIndex");
        require(amountTotal > 0, "amountTotal should be greather than zero");
        uint256 adminBalance = revoltage.balanceOf(msg.sender);
        require(adminBalance >= amountTotal, "Amount exceeds balance");
        
        //we wanna set total rewrd
        currentBase.rewardTotal = amountTotal;
        //how do we calculate reward per token?
        uint256 totalAmountTokenStaked = baseDataIdToAmount[currentBase.baseDataId];
        currentBase.rewardPerToken = amountTotal / totalAmountTokenStaked;

        revoltage.transferFrom(msg.sender, address(this), amountTotal);
        s_balances[address(this)] = s_balances[address(this)] + amountTotal;

        emit AddReward(currentBase.rewardTotal, currentBase.rewardPerToken, currentBase.baseDataId);
    }

    //this function help us to withdraw rest token from staking contracts when we remove a uint256.
    function withdrawStakingContract(uint256 _amount) external onlyOwner {
        IERC20 rewardsToken =  IERC20(revoltage);
        uint256 stakingContractBalance = s_balances[address(this)];
        uint256 balanceOnbaseDataRunning;
        for(uint i = 0; i < baseDataList.length -1; i++){
            if(baseDataList[i].running) {
                balanceOnbaseDataRunning += baseDataList[i].rewardTotal;
            }   
        }
        uint256 amountCanBeWithdrawed = stakingContractBalance - balanceOnbaseDataRunning;
        require(amountCanBeWithdrawed >= _amount, "Cannot withdraw that much tokens!");
        rewardsToken.transfer(_msgSender(), _amount);
        unchecked {
                s_balances[_msgSender()] += _amount;
                s_balances[address(this)] -= _amount;
            } 
    }

