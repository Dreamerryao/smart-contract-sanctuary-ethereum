// File: marketplace.sol

pragma solidity ^0.8.17;

contract NFTMarket is Ownable {
    enum Status {
        Active,
        Sold,
        Cancelled
    }

    enum ListingType {
        Fixed,
        Auction
    }
    struct Listing {
        uint256 listingId;
        Status status;
        address seller;
        address contractAddress;
        uint256 tokenId;
        ListingType listingType;
        uint256 minimumPrice;
        uint256 fixedPrice;
        uint256 royalityPercentage;
    }

    event Listed(
        uint256 listingId,
        address seller,
        address contractAddress,
        uint256 tokenId,
        ListingType listingType,
        uint256 minimumPrice,
        uint256 fixedPrice,
        uint256 royalityPercentage
    );

    event Sale(
        uint256 listingId,
        address seller,
        address buyer,
        address contractAddress,
        address creatorAddress,
        uint256 tokenId,
        uint256 fixedPrice
    );

    event Cancelled(uint256 listingId, address seller);

    event Transfer(
        address contractAdress,
        address from,
        address to,
        uint256 tokenId
    );

    uint256 marketListingFee = 0;
    uint256 marketBuyFee = 0;
    uint256 transferFee = 0;

    uint256 private listingId = 0;
    mapping(uint256 => Listing) private _listings;

    //listing fee will be in fixed amount
    function updateMarketListingFee(uint256 amount) external onlyOwner {
        marketListingFee = amount;
    }

    function getListingFee() public view returns (uint256) {
        return marketListingFee;
    }

    //buying fee will be in percentage and cant exceed 15%
    function updateMarketBuyFee(uint256 amount) external onlyOwner {
        require(amount <= 15, "Buy fees can't exceed 15%");
        marketBuyFee = amount;
    }

    function getMarketBuyFee() public view returns (uint256) {
        return marketBuyFee;
    }

    //buying fee will be in fixed amount
    function updateTransferFee(uint256 amount) external onlyOwner {
        transferFee = amount;
    }

    function getTransferFee() public view returns (uint256) {
        return transferFee;
    }

    //send listing fee and Fixed and Auction type
    function listNft(
        address contractAddress,
        uint256 tokenId,
        uint256 fixedPrice,
        uint256 minimumPrice,
        uint256 royalityPercentage,
        string memory listingType
    ) external payable {
        require(
            keccak256(abi.encodePacked(listingType)) ==
                keccak256(abi.encodePacked("Fixed")) ||
                keccak256(abi.encodePacked(listingType)) ==
                keccak256(abi.encodePacked("Auction")),
            "Wrong Listing Type"
        );

        require(
            royalityPercentage <= 10,
            "Royality percentage can't more than 10%."
        );

        require(msg.value >= marketListingFee, "Insufficient fees");

        Listing memory listing;
        if (
            keccak256(abi.encodePacked(listingType)) ==
            keccak256(abi.encodePacked("Fixed"))
        ) {
            require(fixedPrice > 0, "Fixed price can't be zero");
            require(minimumPrice == 0, "Minimum price should be zero");

            payable(owner()).transfer(marketListingFee);

            IERC721(contractAddress).transferFrom(
                msg.sender,
                address(this),
                tokenId
            );

            listing = Listing(
                listingId,
                Status.Active,
                msg.sender,
                contractAddress,
                tokenId,
                ListingType.Fixed,
                0,
                fixedPrice,
                royalityPercentage
            );

            emit Listed(
                listingId,
                msg.sender,
                contractAddress,
                tokenId,
                ListingType.Fixed,
                0,
                fixedPrice,
                royalityPercentage
            );
        } else {
            require(fixedPrice == 0, "Fixed price should be zero");
            require(minimumPrice > 0, "Minimum price can't be zero");
            payable(owner()).transfer(marketListingFee);
            IERC721(contractAddress).transferFrom(
                msg.sender,
                address(this),
                tokenId
            );
            listing = Listing(
                listingId,
                Status.Active,
                msg.sender,
                contractAddress,
                tokenId,
                ListingType.Auction,
                minimumPrice,
                0,
                royalityPercentage
            );
            emit Listed(
                listingId,
                msg.sender,
                contractAddress,
                tokenId,
                ListingType.Auction,
                minimumPrice,
                0,
                royalityPercentage
            );
        }
        _listings[listingId] = listing;
        listingId++;
    }

    //get single list form here
    function getListing(uint256 _listingId)
        public
        view
        returns (Listing memory)
    {
        return _listings[_listingId];
    }

    //getAll listing from here
    function getAllListing() public view returns (Listing[] memory) {
        Listing[] memory getAll = new Listing[](listingId);
        for (uint256 i = 0; i < listingId; i++) {
            getAll[i] = _listings[i];
        }

        return getAll;
    }

    //for fixed buy eth needs to send as wei
    function buyNFT(uint256 _listingId, address creatorAddress)
        external
        payable
    {
        Listing storage listing = _listings[_listingId];
        uint256 sellAmount = listing.fixedPrice;
        uint256 royalityAmount = 0;
        require(msg.sender != listing.seller, "seller and buyer can not same");
        require(
            listing.status == Status.Active,
            "Listing status is not active"
        );
        require(msg.value >= listing.fixedPrice, "Insufficient Price");
        listing.status = Status.Sold;

        if (listing.royalityPercentage > 0) {
            royalityAmount = ((listing.fixedPrice *
                listing.royalityPercentage) / 100);

            sellAmount = listing.fixedPrice - royalityAmount;
            payable(creatorAddress).transfer(royalityAmount);
        }

        if (marketBuyFee > 0) {
            uint256 marketFee = ((listing.fixedPrice * marketBuyFee) / 100);
            sellAmount = listing.fixedPrice - (marketFee + royalityAmount);
            payable(owner()).transfer(marketFee);
        }

        payable(listing.seller).transfer(sellAmount);

        IERC721(listing.contractAddress).transferFrom(
            address(this),
            msg.sender,
            listing.tokenId
        );

        emit Sale(
            _listingId,
            listing.seller,
            msg.sender,
            listing.contractAddress,
            creatorAddress,
            listing.tokenId,
            listing.fixedPrice
        );
    }

    //for auction bidding and selling this function will call
    //wrapped eth address will change on mainnet
    //weth needs approve for marketplace contract address

    function completeOrder(
        uint256 _listingId,
        address buyer,
        uint256 highestBid,
        address creatorAddress
    ) external {
        Listing storage listing = _listings[_listingId];
        address wethContractAddress = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;
        uint256 sellAmount = highestBid;
        uint256 royalityAmount = 0;
        require(buyer != listing.seller, "seller and buyer can not same");
        require(
            listing.status == Status.Active,
            "Listing status is not active"
        );

        uint256 wethBalance = IERC20(wethContractAddress).balanceOf(buyer);

        require(wethBalance >= highestBid, "Insufficient Price");

        if (listing.royalityPercentage > 0) {
            royalityAmount = ((highestBid * listing.royalityPercentage) / 100);

            sellAmount = highestBid - royalityAmount;
            IERC20(wethContractAddress).transferFrom(
                buyer,
                creatorAddress,
                royalityAmount
            );
        }

        if (marketBuyFee > 0) {
            uint256 marketFee = ((highestBid * marketBuyFee) / 100);
            sellAmount = highestBid - (marketFee + royalityAmount);
            IERC20(wethContractAddress).transferFrom(buyer, owner(), marketFee);
        }

        IERC20(wethContractAddress).transferFrom(
            buyer,
            listing.seller,
            sellAmount
        );

        listing.status = Status.Sold;
        IERC721(listing.contractAddress).transferFrom(
            address(this),
            buyer,
            listing.tokenId
        );
        emit Sale(
            _listingId,
            listing.seller,
            buyer,
            listing.contractAddress,
            creatorAddress,
            listing.tokenId,
            highestBid
        );
    }

    //seller can only cancel list
    //after expire auction cancel list and expire extended will appear in frontend
    function cancelledListing(uint256 _listingId) public {
        Listing storage listing = _listings[_listingId];
        require(listing.seller == msg.sender, "Only seller can be cancelled");
        require(listing.status == Status.Active, "This Nft is not active");
        listing.status = Status.Cancelled;
        IERC721(listing.contractAddress).transferFrom(
            address(this),
            listing.seller,
            listing.tokenId
        );
        emit Cancelled(_listingId, listing.seller);
    }

    // approve market Contract and send transfer fee to wei
    function nftTransfer(
        address contractAdress,
        uint256 tokenId,
        address to
    ) external payable {
        require(msg.value >= transferFee, "Insufficient Fees");
        payable(owner()).transfer(transferFee);
        IERC721(contractAdress).transferFrom(msg.sender, to, tokenId);
        emit Transfer(contractAdress, msg.sender, to, tokenId);
    }
}