        uint32 tileCoordinate;
        uint256 linkedAvatarId;
        uint256 avatarId;
        uint32 LandId;
        DelegateWallet delegateWallet;
        address vault;
    }

    /**
     * @notice Delegate Wallet Protocols
     */
    enum DelegateWallet {
        None,
        DelegateCash,
        Warm
    }

