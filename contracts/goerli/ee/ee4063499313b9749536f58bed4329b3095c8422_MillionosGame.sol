import {Ownable} from "@openzeppelin/access/Ownable.sol";
import {IERC20} from "@openzeppelin/token/ERC20/IERC20.sol";
import {SafeERC20} from "@openzeppelin/token/ERC20/utils/SafeERC20.sol";
import {ReentrancyGuard} from "@openzeppelin/security/ReentrancyGuard.sol";

import {console2} from "forge-std/console2.sol";

import "./interfaces/IMillionosGame.sol";
import "./ProofVerifier.sol";

// TODO: implement proxy Upgradeable pattern.

/* solhint-disable not-rely-on-time */
contract MillionosGame is IMillionosGame, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    mapping(uint256 => Draw) public draws;

    uint256 public curDrawId = 0;

    // drawId -> gridIndex.
    mapping(uint256 => uint256) public drawToGridIndex;

    // drawId -> gridIndex -> owner
    mapping(uint256 => mapping(uint256 => address)) public drawToGridOwner;

    // drawId -> address -> gridIndex -> isClaimed
    mapping(uint256 => mapping(address => mapping(uint256 => bool)))
        public drawToGridClaimed;

    // drawId -> owner -> gridIndexes
    mapping(uint256 => mapping(address => uint256[]))
        public drawToOwnerGridIdxes;

    // drawId -> gridIndex -> mapping(uint256 => bool).
    mapping(uint256 => mapping(uint256 => mapping(uint256 => bool)))
        public drawToGrid;

    // Max grids (grids) per participation.
    uint256 public constant MAX_GRIDS = 5;

    IERC20 public immutable paymentToken;

    ProofVerifier public proofVerifier;

    uint256[13] public fundPercentagePerTier = [
        uint256(14), // Min
        uint256(10),
        uint256(3),
        uint256(2),
        uint256(1),
        uint256(1),
        uint256(1),
        uint256(1),
        uint256(1),
        uint256(1),
        uint256(2),
        uint256(3),
        uint256(50) // Max
    ];

    enum DrawStatus {
        Pending,
        Started,
        Finished
    }

    struct Draw {
        DrawStatus status;
        uint256 startTime;
        uint256 endTime;
        uint256[7] drawNumbers;
        uint256[13] winnersPerTier;
        uint256 ticketPrice;
        uint256 prizePool;
    }

    event DrawStarted(
        uint256 indexed drawId,
        uint256 indexed ticketPrice,
        uint256 indexed prizePool
    );

    event Participated(uint256 indexed drawId);

    event WinningsClaimed(
        uint256 indexed drawId,
        address indexed token,
        uint256 amount
    );

    event DrawEnded(uint256 indexed drawId);

    error InvalidDrawStatus();

    error EndTimeNotOver();

    error InvalidGridsNumber();

    error InvalidNumbersCountInGrid();

    error InvalidNumberRange(); // !(>= 1 && <= 50)

    error InvalidStarRange(); // !(>= 1 && <= 12)

    error NoDuplicateNumberWithinAGrid();

    error DrawNotEnded();

    error ParticipationsOver();

    error InvalidFact();

    error DrawNumbersOutputMismatch();

    error InvalidChainId();

    error InvalidDrawId();

    error InvalidBlockHash();

    // TODO: handle roles.
    constructor(address _paymentToken, address _proofVerifier) {
        paymentToken = IERC20(_paymentToken);
        proofVerifier = ProofVerifier(_proofVerifier);
    }

    /*
    ####################################################
    #                   PLAYER FUNCTIONS
    ####################################################
    */

    function participate(uint256 drawId, uint256[7][] memory grids)
        external
        nonReentrant
    {
        if (draws[drawId].status != DrawStatus.Started)
            revert InvalidDrawStatus();
        if (draws[drawId].endTime <= block.timestamp)
            revert ParticipationsOver();
        if (grids.length == 0 || grids.length > MAX_GRIDS)
            revert InvalidGridsNumber();

        for (uint256 i = 0; i < grids.length; ++i) {
            if (grids[i].length != 7) revert InvalidNumbersCountInGrid();

            uint256 curGridIdx = drawToGridIndex[drawId];
            drawToGridOwner[drawId][curGridIdx] = msg.sender;
            drawToOwnerGridIdxes[drawId][msg.sender].push(curGridIdx);
            ++drawToGridIndex[drawId];

            for (uint256 j = 0; j < 7; ++j) {
                uint256 number = grids[i][j];
                if (j < 5 && (number < 1 || number > 50))
                    revert InvalidNumberRange();
                else if (j >= 5 && (number < 1 || number > 12))
                    revert InvalidStarRange();
                uint256 transformedNumber = j < 5 ? number : number + 100;
                if (drawToGrid[drawId][curGridIdx][transformedNumber] == true)
                    revert NoDuplicateNumberWithinAGrid();

                drawToGrid[drawId][curGridIdx][transformedNumber] = true;
            }
        }

        // Sender *must* have approved this contract as a spender before
        // (with allowance >= (ticketPrice * number of grids)).
        paymentToken.safeTransferFrom(
            msg.sender,
            address(this),
            draws[drawId].ticketPrice * grids.length
        );

        emit Participated(drawId);
    }

    function claimWinnings(uint256 drawId, uint256[] calldata gridIndexes)
        external
        nonReentrant
    {
        if (draws[drawId].status != DrawStatus.Finished) revert DrawNotEnded();

        uint256 payout = 0;

        for (uint256 idx = 0; idx < gridIndexes.length; ++idx) {
            uint256 totalMatchNumbers = 0;
            uint256 totalMatchStars = 0;
            for (uint256 i = 0; i < draws[drawId].drawNumbers.length; ++i) {
                uint256 curGridIdx = gridIndexes[idx];

                if (drawToGridOwner[drawId][curGridIdx] != msg.sender) continue;
                if (drawToGridClaimed[drawId][msg.sender][curGridIdx] == true)
                    continue;

                for (uint256 ballI = 0; ballI < 7; ++ballI) {
                    // (Stars numbers are already transformed before reading).
                    uint256 number = draws[drawId].drawNumbers[ballI];
                    bool matchN = drawToGrid[drawId][curGridIdx][number] ==
                        true;
                    if (matchN) {
                        // 100 is the stars offset.
                        if (number > 100) ++totalMatchStars;
                        else ++totalMatchNumbers;
                    }
                }
                uint256 tierIdx = matchToPrizeTierIndex(
                    totalMatchNumbers,
                    totalMatchStars
                );
                require(tierIdx < 13, "ERR_TIER_INDEX_OUT_OF_BOUNDS");
                uint256 amountToShare = ((draws[drawId].prizePool *
                    fundPercentagePerTier[tierIdx]) / 100);
                uint256 amountPerWinner = (amountToShare / 10**18) /
                    draws[drawId].winnersPerTier[tierIdx];
                payout += amountPerWinner;
                drawToGridClaimed[drawId][msg.sender][curGridIdx] = true;
            }
        }

        if (payout > 0) {
            paymentToken.safeTransfer(msg.sender, payout * 10**18);

            emit WinningsClaimed(
                drawId,
                address(paymentToken),
                payout * 10**18
            );
        }
    }

    /*
    ####################################################
    #                   VIEW FUNCTIONS
    ####################################################
    */

    function readSenderGridIndexes(uint256 drawId)
        external
        view
        returns (uint256[] memory)
    {
        return readPlayerGridIndexes(drawId, msg.sender);
    }

    function readPlayerGridIndexes(uint256 drawId, address player)
        public
        view
        returns (uint256[] memory)
    {
        return drawToOwnerGridIdxes[drawId][player];
    }

    /*
    ####################################################
    #                   GAME LOGIC FUNCTIONS
    ####################################################
    */

    function newDraw(
        uint256 startTime,
        uint256 endTime,
        uint256 ticketPrice,
        uint256 prizePool
    ) external onlyOwner {
        require(startTime >= block.timestamp, "!startTime");
        require(endTime > block.timestamp, "!endTime");

        draws[++curDrawId] = Draw({
            status: DrawStatus.Started,
            startTime: startTime,
            endTime: endTime,
            drawNumbers: [
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0)
            ], // Will be drawn at the end.
            winnersPerTier: [
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0),
                uint256(0)
            ],
            ticketPrice: ticketPrice,
            prizePool: prizePool
        });

        uint256 curBalance = IERC20(paymentToken).balanceOf(address(this));
        if (curBalance < prizePool) {
            uint256 remainingAmount = prizePool - curBalance;
            // Top up this contract if needed to satisfy the prize pool requirement.
            paymentToken.safeTransferFrom(
                msg.sender,
                address(this),
                remainingAmount
            );
        }

        emit DrawStarted(curDrawId, ticketPrice, prizePool);
    }

    function assertOutputsMatch(uint256 drawId, uint256[] memory outputs)
        internal
        view
    {
        // TODO: uncomment those lines.
        // if (outputs[0] != block.chainid) revert InvalidChainId();
        // uint256 blockHash = uint256(blockhash(outputs[1]));
        // if (blockHash != 0 && blockHash != outputs[2])
        //     revert InvalidBlockHash();
        // if (outputs[2] != drawId) revert InvalidDrawId();

        // Check drawn numbers.
        uint256 drawNumbersOfs = 4;
        for (uint256 i = 0; i < draws[drawId].drawNumbers.length; ++i) {
            if (draws[drawId].drawNumbers[i] != outputs[drawNumbersOfs])
                revert DrawNumbersOutputMismatch();
            ++drawNumbersOfs;
        }
    }

    function endDraw(uint256 drawId, uint256[] memory outputs)
        external
        onlyOwner
    {
        if (draws[drawId].status != DrawStatus.Started)
            revert InvalidDrawStatus();
        /* solhint-disable not-rely-on-time */
        if (block.timestamp < draws[drawId].endTime) revert EndTimeNotOver();

        draws[drawId].drawNumbers = drawResults();

        assertOutputsMatch(drawId, outputs);
        // TODO: !!! check output match context variables !!!

        // Uncomment the line below to *actually* verify the fact.
        // if (proofVerifier.verifyFact(outputs) != true) revert InvalidFact();

        assignWinnersPerTier(drawId, outputs, 11, 24);

        draws[drawId].status = DrawStatus.Finished;

        emit DrawEnded(drawId);
    }

    function assignWinnersPerTier(
        uint256 drawId,
        uint256[] memory outputs,
        uint256 ofs,
        uint256 endOfs
    ) internal {
        for (uint256 idx = 0; ofs < endOfs; ++idx) {
            draws[drawId].winnersPerTier[idx] = outputs[ofs];
            ++ofs;
        }
    }

    // @dev To replace by Chainlink VRF.
    function drawResults() internal pure returns (uint256[7] memory) {
        // 5 main numbers + 2 stars numbers.
        // Stars numbers are "indexed" as (100 + number).
        uint256[7] memory fixedResult = [
            uint256(1),
            uint256(2),
            uint256(3),
            uint256(4),
            uint256(50),
            uint256(100 + 11),
            uint256(100 + 12)
        ];

        return fixedResult;
    }

    function matchToPrizeTierIndex(uint256 balls, uint256 stars)
        internal
        pure
        returns (uint256)
    {
        if (balls == 2 && stars == 0) return 0;
        if (balls == 2 && stars == 1) return 1;
        if (balls == 1 && stars == 2) return 2;
        if (balls == 3 && stars == 0) return 3;
        if (balls == 3 && stars == 1) return 4;
        if (balls == 2 && stars == 2) return 5;
        if (balls == 4 && stars == 0) return 6;
        if (balls == 3 && stars == 2) return 7;
        if (balls == 4 && stars == 1) return 8;
        if (balls == 4 && stars == 2) return 9;
        if (balls == 5 && stars == 0) return 10;
        if (balls == 5 && stars == 1) return 11;
        if (balls == 5 && stars == 2) return 12;

        return 13; // No match. Should not happen.
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ~0.8.16;

interface IFactRegistry {
    function isValid(bytes32 fact) external view returns (bool);
}

contract ProofVerifier {
    // SHARP Goerli contract
    address public constant FACT_REGISTY =
        0xAB43bA48c9edF4C2C4bB01237348D1D7B28ef168;

    uint256 public constant PROGRAM_HASH =
        0x38477aa3daf83ba977d13af8dd288d76da55cfde05ccfc7ee5438f4c56fb0b6;

    error InvalidFact();

    function verifyFact(uint256[] memory outputs) external view returns (bool) {
        bytes32 outputHash = keccak256(abi.encodePacked(outputs));
        bytes32 fact = keccak256(abi.encodePacked(PROGRAM_HASH, outputHash));

        bool valid = IFactRegistry(FACT_REGISTY).isValid(fact);
        if (!valid) revert InvalidFact();

        return true;
    }
}

