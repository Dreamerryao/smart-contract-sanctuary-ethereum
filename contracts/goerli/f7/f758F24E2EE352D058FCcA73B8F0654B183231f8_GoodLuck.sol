pragma solidity ^0.8.0;







contract ERC20 is Context, IERC20, IERC20Metadata, ReentrancyGuard, VRFConsumerBaseV2 {
    VRFCoordinatorV2Interface COORDINATOR;
    IERC20 public immutable stakingToken;
    address public _marketingWallet;
    address vrfCoordinator = 0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D;
    bytes32 public keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
    mapping(address => uint256) private _balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => uint256) public _wins;
    mapping(address => uint256) public _loss;
    mapping(address => uint256) public _userBet;
    mapping(address => uint256) public _userLastGameTime;
    mapping(address => uint32) public _userBetting;    
    mapping(address => uint256) public _userLinkBalance;
    mapping(uint256 => address) private s_rollers;
    mapping(address => uint256) private s_results;
    uint256 public LinkFee;
    uint256 private _totalSupply;
    uint256 public _totalMinted;
    uint256 public _totalBurned;
    uint256 public _marketingTokens;
    uint256 public linkReadyToBeSentToChainlink;
    uint256 public _presale;
    uint64 s_subscriptionId;
    uint32 public callbackGasLimit = 200000;
    uint16 public _totalWins;
    uint16 public _totalLoss;
    uint16 public _totalGames;
    string private _name;
    string private _symbol;
    event DiceRolled(uint256 indexed requestId, address indexed roller);
    event DiceLanded(uint256 indexed requestId, uint256 indexed result);
    constructor(uint64 subscriptionId, string memory name_, string memory symbol_) VRFConsumerBaseV2(vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_subscriptionId = subscriptionId;
        _name = name_;
        _symbol = symbol_;
        keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
        _marketingWallet = 0xD1EC8245c8850A151843ce8a3AFdca3b19747706;
        LinkFee = uint256(33*10**16); // 0.33 LINK
        //token = IERC20(LinkToken);
        stakingToken = IERC20(address(0x326C977E6efc84E512bB9C30f76E30c160eD06FB)); //Link token again lol
        _presale = 100000*10**18;
    }

        // rollDice starts the VRF request which runs the callback function _burnGame
        function rollDice(uint256 amount) public payable returns (uint256 requestId) {
            address roller = msg.sender;
            require(msg.value >= 123*10**13, "ERC20: treasury demands 0.00123 ETH payment.");
            require(_userBetting[roller] == 0, "you are not allowed to re-enter the contract mid bet.");
            require(amount <= 50000*10**18 && amount >= 1, "ERC20: cannot play with over 50,000 tokens or 0 tokens.");
            require(_userLinkBalance[roller] >= LinkFee);
            _userLastGameTime[roller] = block.timestamp;
            // sending chainlink request!
            // Will revert if subscription is not set and funded.
            requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            3,
            callbackGasLimit,
            1
            );
            // you use someone elses Link token but deposit an equal amount for future games.
            uint256 ourLink = _userLinkBalance[roller];
            uint256 bankedLink = linkReadyToBeSentToChainlink;
            _userLinkBalance[roller] = ourLink - LinkFee;
            linkReadyToBeSentToChainlink = bankedLink + LinkFee; 
            // this link will be used for other peoples games, as someone else paid for this one.
            _userBetting[roller] = 1;
            _userBet[roller] = amount;
            s_rollers[requestId] = roller;
            s_results[roller] = 444;  //444 = waiting for callback from chainink (player would win if not)
            emit DiceRolled(requestId, roller); //run fulfilrlrandomwords below on callback
        }
    function fulfillRandomWords(uint256 requestId , uint256[] memory randomWords) internal override {
        address _roller = s_rollers[requestId];
        uint256 bet = _userBet[_roller];
        uint256 d20Value = (randomWords[0] % 100); //reduce chainlinks number to get 1/100
        s_results[s_rollers[requestId]] = d20Value;  //set players result to chainlink result
        emit DiceLanded(requestId, d20Value); // emit dicelanded event
        _burnGame(_roller, bet, d20Value); // emit a burnGame 
    }
    function _burnGame(address personPlaying, uint256 coins, uint256 rolledNumber) internal {
        address owner = personPlaying;
        uint256 accountBalance = _balances[owner];
        require(accountBalance >= coins, "ERC20: tokens have disappeared");
        uint256 accountWins = _wins[owner];
        uint256 accountLoss = _loss[owner];
        _userBet[owner] = 0;
        _userBetting[owner] = 0;
        _totalGames += 1;
        if (rolledNumber <= 55) { 
        _burn(owner, coins);
        _totalBurned += coins;
        _loss[owner] = accountLoss + 1;
        _totalLoss += 1;
        } else {
        _mint(owner, coins);
        _totalMinted += coins;
        _wins[owner] = accountWins + 1;
        _totalWins += 1;
        }
    }
    function _buyPresale(uint256 amount) public payable {
        uint256 requestedTokens = amount * 1000;
        uint256 tokensLeft = _presale;
        require(msg.sender != address(0), "ERC20: mint to the zero address");
        require(msg.value >= amount*10**17, "ERC20: treasury demands 0.1 ETH payment per 1000 tokens");
        require(requestedTokens*10**18 <= tokensLeft && requestedTokens*10**18 >= 1, "ERC20: cannot buy more then the presale and cant buy 0");
        _mint(msg.sender, requestedTokens*10**18);
        _presale = tokensLeft - requestedTokens*10**18;
        if (_presale <= 10000*10**18) {
            _presale = 0;
        }
    }
    function withdrawMoney() public {
        require(msg.sender == _marketingWallet);    
        uint256 linkBalance = linkReadyToBeSentToChainlink;
        //send eth (all of it)
        address payable to = payable(_marketingWallet);
        to.transfer(getBalance());
        //send chainlink (only link thats been used in played games)
        stakingToken.approve(address(this), linkBalance);
        stakingToken.transfer(_marketingWallet, linkBalance);
        linkReadyToBeSentToChainlink = 0;
    }
    function stakeChainlink(uint256 amount) public nonReentrant {
        uint256 linkBalance = _userLinkBalance[msg.sender];
        uint256 erc20balance = stakingToken.balanceOf(msg.sender);
        require(amount <= erc20balance && amount >= 1, "cant transfer 0 or not enough tokens to transfer");
        stakingToken.transferFrom(msg.sender, address(this), amount);
        _userLinkBalance[msg.sender] = linkBalance + amount;

    }
    function unstakeChainlink(uint256 amount) public nonReentrant {
        require(_userBetting[msg.sender] == 0, "you are not allowed to re-enter the contract mid bet.");
        uint256 linkBalance = _userLinkBalance[msg.sender];
        require(amount <= linkBalance && amount >= 1, "cant transfer 0 or user doesnt own the tokens he requested to transfer");
        stakingToken.approve(address(this), amount);
        stakingToken.transfer(msg.sender, amount);
        _userLinkBalance[msg.sender] = linkBalance - amount;

    }

    function unStuck() public {
        uint256 gameTime = _userLastGameTime[msg.sender];
        uint256 nowTime = block.timestamp;
        require(nowTime - gameTime >= 3600); //when callback fails user cant trade for 1 hour (should be rare)
        _userBetting[msg.sender] = 0;
        _userBet[msg.sender] = 0;
    }
    function changeKeyHash(bytes32 newHash) public {
        require(msg.sender == _marketingWallet);
        keyHash = newHash;
    }
    function changeCallbackFee(uint32 amount) public {
        require(msg.sender == _marketingWallet);
        callbackGasLimit = amount;
    }
    function changeChainlinkFee(uint256 amount) public {
        require(msg.sender == _marketingWallet);
        LinkFee = amount;
    }
    function endPresale() public {
        require(msg.sender == _marketingWallet);
        _presale = 0;
    }
    function name() public view virtual override returns (string memory) {
        return _name;
    }
    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }
    function decimals() public view virtual override returns (uint8) {
        return 18;
    }
    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }
    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }
    function transfer(address to, uint256 amount) public virtual override returns (bool) {
        require(_userBetting[to] == 0 && _userBetting[msg.sender] == 0, "you are not allowed to re-enter the contract mid bet.");
        require(_presale <= 0, "Presale is still on");
        address owner = _msgSender();
        _transfer(owner, to, amount);
        return true;
    }
    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }
    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        address owner = _msgSender();
        _approve(owner, spender, amount);
        return true;
    }
    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public virtual override returns (bool) {
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _transfer(from, to, amount);
        return true;
    }
    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        address owner = _msgSender();
        _approve(owner, spender, allowance(owner, spender) + addedValue);
        return true;
    }
    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        address owner = _msgSender();
        uint256 currentAllowance = allowance(owner, spender);
        require(currentAllowance >= subtractedValue, "ERC20: decreased allowance below zero");
        unchecked {
            _approve(owner, spender, currentAllowance - subtractedValue);
        }

        return true;
    }
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        //TLDR every tx has a 3.33% commission this is  burned every transaction but is recorded
        //When _marketingTokens is higher then 200 it triggers a mint event and drains _marketingTokens
        require(_userBetting[to] == 0 && _userBetting[from] == 0, "you are not allowed to re-enter the contract mid bet you naughty boy");
        require(from != address(0) && to != address(0), "ERC20: transfer to/from the zero address");
        require(_presale <= 0, "Presale is still on");
        _beforeTokenTransfer(from, to, amount);
        uint commissionNumerator = 333;
        uint commissionDenominator = 10000;
        uint afterCommission = amount * commissionNumerator / commissionDenominator; // 3.33%
        uint keepNum = amount - afterCommission;
        uint startingTokens = _marketingTokens;
        uint256 fromBalance = _balances[from];
        require(fromBalance >= amount, "ERC20: transfer amount exceeds balance");
        _burn(from, afterCommission);
        unchecked {
            _balances[from] = fromBalance - amount;
        }
        _balances[to] += keepNum;
        emit Transfer(from, to, keepNum);
        _afterTokenTransfer(from, to, keepNum);
        if (startingTokens + afterCommission >= 200*10**18) {
            _mint(_marketingWallet, startingTokens + afterCommission);
            _marketingTokens = 0;
        } else {
            _marketingTokens = startingTokens + afterCommission;
        }
    }
    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);

        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);

        _afterTokenTransfer(address(0), account, amount);
    }
    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: burn from the zero address");

        _beforeTokenTransfer(account, address(0), amount);

        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount, "ERC20: burn amount exceeds balance");
        unchecked {
            _balances[account] = accountBalance - amount;
        }
        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);

        _afterTokenTransfer(account, address(0), amount);
    }
    function getBalance() public view returns (uint) {
        return address(this).balance;
    }
    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }
    function _spendAllowance(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        uint256 currentAllowance = allowance(owner, spender);
        if (currentAllowance != type(uint256).max) {
            require(currentAllowance >= amount, "ERC20: insufficient allowance");
            unchecked {
                _approve(owner, spender, currentAllowance - amount);
            }
        }
    }
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}

