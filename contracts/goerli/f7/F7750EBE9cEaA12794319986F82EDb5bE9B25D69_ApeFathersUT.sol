    uint256 private _totalSupply;
    mapping(uint256 => string) private _tokenURIs;
    mapping(address => mapping(uint16 => bool)) private approvedTokens;
    event ApprovalComplete(address indexed _sender, uint16[] _tokenIds);
    event BurnClaimComplete(address indexed user, uint256 numberMinted, uint16[] tokenIds);
    event TokenApproved(address indexed _sender, uint16 tokenId);
    event ApprovalGranted(address indexed _sender);
    // The respective share of funds to be sent to each address in payoutAddresses in basis points
    uint256[] public payoutBasisPoints = [8500, 1500];
    uint96 public royaltyFee = 1000;
    // If true, payout addresses and basis points are permanently frozen and can never be updated
    bool public payoutAddressesFrozen = false;
    // If true tokens can be burned in order to mint
    bool public isBurnClaimActive = false;
    bool public isPublicSaleActive = false;
    // Maximum supply of tokens that can be minted
    uint16 public constant maxSupply = 4000;
    uint256 public publicPrice = 0.08 ether;
    uint8 batchSizePerTx = 10;
    uint256 public totalRevealed; // Keep track of total number of tokens already revealed
    uint16 public tokensPerBatch = 500; // Number of tokens to reveal per batch
    // Permanently freezes metadata so it can never be changed
    bool public metadataFrozen = false;
    bool public revealed = false;
    string public uriPrefix = '';
    string public uriSuffix = '.json';
    string public hiddenMetadataUri;
    string public baseTokenURI =
        "ipfs://__CID__/";
    string public version;

    ApeDadsContract public apeDads = ApeDadsContract(0x6468f4243Faa8C3330bAAa0a7a138E2e5628C6f5);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __ERC721_init("The ApeFathers NFT", "DAPES");
        __Ownable_init();
        __UUPSUpgradeable_init();
        version = "1.0.0";
        apeDads = ApeDadsContract(0x6468f4243Faa8C3330bAAa0a7a138E2e5628C6f5);
        setHiddenMetadataUri("ipfs://bafybeigqicwgfc2lyro3gkvv6wj6it5epm7kcnlnbnxh4apflwmvnscmwe/hidden.json");
        revealed = false;
        metadataFrozen = false;
        royaltyFee = 500; // set initial royalty fee to 5%
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        onlyEOA
        override
    {}

        // set the royalty fee and recipient
    function setRoyaltyAddress(uint96 _fee, address _recipient) external onlyOwner {
            royaltyFee = _fee;
            royaltyAddress = _recipient;
    }
    function beforeTokenTransfer(address from, address, uint256) external payable {
            // check if royalties are due
            if (from != address(0)) {
            uint256 royalty = (msg.value * royaltyFee) / 10000; // calculate royalty based on fee
            (bool success, ) = royaltyAddress.call{value: royalty}(""); // transfer royalty to recipient
            require(success, "Royalty transfer failed");
        }
    }

    function _beforeTokenTransfer(address from, address to, uint16 tokenId) internal {
        ApeFathersUT(_msgSender()).beforeTokenTransfer(from, to, tokenId);
    }

    function _beforeTokenBurnTransfers(
        address from,
        address to,
        uint16 tokenId,
        uint16 quantity
    ) internal whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId, quantity);
        if (from != address(0)) {
            address ADowner = apeDads.ownerOf(tokenId);
            require(ADowner == msg.sender, "Only the owner of NFT can transfer or burn it");
        }
    }

    function totalSupply() public view returns (uint256) {
    return _totalSupply;
    }

    function tokensMintedFor(address account) public view returns (uint256) {
    return _tokenCounts[account];
    }
     /**
    * @notice Pause or Unpause the ERC721A contract
    */
    function pause() external onlyOwner onlyEOA {
        _pause();
    }

    function unpause() external onlyOwner onlyEOA {
        _unpause();
    }
    function isContract(address addr) internal view returns (bool) {
        uint size;
        assembly { size := extcodesize(addr) }
        return size > 0;
    }

    modifier onlyEOA() {
        require(!isContract(msg.sender), "ONLY_EOA_ALLOWED");
        _;
    }

    /**
    * @dev Overrides the default ERC721A _startTokenId() so tokens begin at 1 instead of 0
    */
    function _startTokenId() internal view virtual returns (uint256) {
        return 1;
    }

    /**
    * @notice Update the base token URI
    */
    function setBaseURI(string calldata _newBaseURI) external onlyOwner onlyEOA {
        require(!metadataFrozen, "METADATA_HAS_BEEN_FROZEN");
        baseTokenURI = _newBaseURI;
    }
    
    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
    require(_exists(_tokenId), 'ERC721Metadata: URI query for nonexistent token');

    if (revealed == false) {
      return hiddenMetadataUri;
    }
    string memory currentBaseURI = _baseURI();
    return string(abi.encodePacked(currentBaseURI, bytes32(_tokenId), uriSuffix));
    }

    function _baseURI() internal view virtual override returns (string memory) {
            return baseTokenURI;
    }

      function setUriPrefix(string memory _uriPrefix) public onlyOwner onlyEOA {
    uriPrefix = _uriPrefix;
    }

    function setUriSuffix(string memory _uriSuffix) public onlyOwner onlyEOA {
    uriSuffix = _uriSuffix;
    }

    function setHiddenMetadataUri(string memory _hiddenMetadataUri) public onlyOwner onlyEOA {
    hiddenMetadataUri = _hiddenMetadataUri;
    }
    
    function setRevealed(bool _state) public onlyOwner onlyEOA {
        revealed = _state;
    }

    function setTokensPerBatch(uint16 _tokensPerBatch) public onlyOwner onlyEOA {
    tokensPerBatch = _tokensPerBatch;
    }

    function revealTokens() public onlyOwner onlyEOA {
    require(revealed == false, "Tokens have already been revealed");
    require(totalSupply() > 0, "Token supply is 0");
    require(tokensPerBatch > 0, "Tokens per batch must be greater than 0");

    uint256 tokensToReveal = tokensPerBatch;
    if (totalRevealed + tokensPerBatch > totalSupply()) {
        tokensToReveal = totalSupply() - totalRevealed;
    }

    revealed = true;
    totalRevealed += tokensToReveal;
    }
    /**
    * @dev Used to directly approve a specific token for transfers by the current msg.sender
    * @param tokenId The ID of the token to approve for transfer
    */
    function _directApproveMsgSenderFor(uint256 tokenId) internal onlyEOA {
        assembly {
            mstore(0x00, tokenId)
            mstore(0x20, 6) // '_tokenApprovals' is at slot 6.
            sstore(keccak256(0x00, 0x40), caller())
        }
    }
