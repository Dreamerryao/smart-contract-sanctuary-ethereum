        return ERC2981Upgradeable.supportsInterface(interfaceId) ||
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }
     /**
    * @notice Change the royalty fee for the collection
    */
    function setRoyaltyFee(uint96 _feeNumerator) external onlyOwner onlyEOA {
        royaltyFee = _feeNumerator;
        _setDefaultRoyalty(royaltyAddress, royaltyFee);
    }

    /**
    * @notice Change the royalty address where royalty payouts are sent
    */
    function setRoyaltyAddress(address _royaltyAddress) external onlyOwner onlyEOA {
        royaltyAddress = _royaltyAddress;
        _setDefaultRoyalty(royaltyAddress, royaltyFee);
    }
     /**
    * @notice Wraps and exposes publicly _numberMinted() from ERC721A
    */
    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }
function _numberMinted(address owner) internal view returns (uint256) {
  return _tokenCounts[owner];
}
     /**
    * @notice Allow owner to send gifts to multiple addresses
    */
    function gift(address[] calldata receivers, uint8[] calldata mintNumber)
        external
        onlyOwner
        onlyEOA
    {
        require(
            receivers.length == mintNumber.length,
            "RECEIVERS_AND_MINT_NUMBERS_MUST_BE_SAME_LENGTH"
        );
        uint16 totalMint = 0;
        for (uint8 i = 0; i < mintNumber.length; i++) {
            totalMint += mintNumber[i];
        }
        require(
            totalSupply() + totalMint <= 4000, 
            "maxSupply_EXCEEDED"
        );
        for (uint16 i = 0; i < receivers.length; i++) {
            _safeMint(receivers[i], mintNumber[i]);
        }
    }
    /**
    * @notice Update the public mint price
    */
    function setPublicPrice(uint256 _publicPrice) external onlyOwner onlyEOA {
        publicPrice = _publicPrice;
    }
    /**
    * @notice Set the maximum public mints allowed per a given transaction
    */
    function setBatchSizePerTx(uint8 _batchSizePerTx)
        external
        onlyOwner
        onlyEOA
    {
        batchSizePerTx = _batchSizePerTx;
    }
    /**
    * @notice To be updated by contract owner to allow public sale minting
    */
    function setPublicSaleState(bool _saleActiveState)
        external
        onlyOwner
        onlyEOA
    {
        require(
            isPublicSaleActive != _saleActiveState, "NEW_STATE_IDENTICAL_TO_OLD_STATE"
        );
        isPublicSaleActive = _saleActiveState;
    }
    /**
    * @notice To be updated by contract owner to allow burning to claim a token for free + gas
    */
    function setBurnClaimState(bool _burnClaimActive) 
        external
        onlyOwner
        onlyEOA
    {
        require(
            isBurnClaimActive != _burnClaimActive,
            "BURN CLAIM CLOSED"
        );
    isBurnClaimActive = _burnClaimActive;
    }

    /**
    * @notice Allow for public minting of tokens
    */
    function mint(uint256 numTokens)
        external
        payable
        nonReentrant
    {
        require(
            isPublicSaleActive,
            "PUBLIC_NOT_ACTIVE"
        );
        require(
            numTokens <= batchSizePerTx,
            "MAX_MINTS_PER_TX_EXCEEDED"
        );
        require(
            totalSupply() + numTokens <= maxSupply,
            "maxSupply_EXCEEDED"
        );
        require(
            msg.value == publicPrice * numTokens,
            "PAYMENT_INCORRECT"
        );
        _safeMint(msg.sender, numTokens);
        _tokenCounts[_msgSender()]++;
        // Increment the total supply
        _totalSupply++;
        if (totalSupply() >= maxSupply) {
            isPublicSaleActive = false;
        }
    }
    /**
    * @notice Withdraws all funds held within contract
    */
    function withdraw() external nonReentrant onlyOwner {
        require(address(this).balance > 0, "NO_FUNDS");
        uint256 balance = address(this).balance;
        for (uint256 i = 0; i < payoutAddresses.length; i++) {
            require(
                payable(payoutAddresses[i]).send(
                    (balance * payoutBasisPoints[i]) / 10000
                )
            );
        }
    }
   
    /**
    * @dev Ask ApeDadsNFT owner for confirmation of transaction to burn ApeDads NFTs
    */
    function askForApproval(string memory message) internal returns (bool) {
        bytes32 tokenId = keccak256(abi.encodePacked(message));
        address spender = address(apeDads); // or whichever address you want to grant approval to
        require(IERC721Upgradeable(address(this)).ownerOf(uint256(tokenId)) == msg.sender, "Only token owner can approve");
        IERC721Upgradeable(address(this)).approve(spender, uint256(tokenId));
        return true;
    }
    /**
    * @dev Ask User for confirmation of transaction.
    */
    function askForConfirmation(string memory message) internal returns (bool) {
        require(askForApproval(message), "Failed to show confirmation prompt");
        return true;
    }

    /*
    * @notice Approves the selected # of ONLY ApeDadNFT token IDs as indicated by the token owner 
    **/
    function approveAllTokensForBurn() external onlyEOA {
        // Inform the user of what they are approving
        string memory warning = "WARNING: You are approving the transfer of all your ApeDad NFTs to be burned by this contract. Please ensure you have selected the correct tokens to burn.";
        bytes memory data = abi.encodeWithSignature("warning(string)", warning);
        (bool sendersuccess, ) = msg.sender.call(data);
        require(sendersuccess, "Failed to show Metamask warning");

        // Approve the contract for burning all tokens held by the message sender
        (bool success,) = address(apeDads).call(
            abi.encodeWithSignature("setApprovalForAll(address,bool)", address(this), true)
        );
        require(success, "Failed to Approve All");
        emit ApprovalGranted(_msgSender());
    }

    function approveTokensForBurn(uint16[] calldata tokenIds) external onlyEOA {
        // Inform the user of what they are approving
        string memory warning = "WARNING: You are approving the transfer of all your ApeDad NFTs to be burned by this contract. Please ensure you have selected the correct tokens to burn.";
        bytes memory data = abi.encodeWithSignature("warning(string)", warning);
        (bool success, ) = msg.sender.call(data);
        require(success, "Failed to show Metamask warning");

        // Approve the contract for burning all specified tokens held by the message sender
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(apeDads.ownerOf(tokenIds[i]) == _msgSender(), "TOKENS NOT OWNED BY CALLER OR YOU ENTERED THE WRONG TOKEN ID");
            IERC721Upgradeable(address(apeDads)).approve(address(this), tokenIds[i]);
        }

        emit ApprovalComplete(_msgSender(), tokenIds);
    }


    function _getNextTokenId() private view returns (uint256) {
        return totalSupply().add(1);
    }

    /**
    * @dev Allows for greater efficiency when minting more than 1 token at a time. (included in BurnClaim to reduce gas)
    */
    function batchMint(address to, uint256 numTokens) public nonReentrant onlyOwner onlyEOA {
        require(numTokens > 0, "Number of tokens must be greater than zero");
        for (uint256 i = 0; i < numTokens; i++) {
            uint256 tokenId = _getNextTokenId();
            _safeMint(to, tokenId);
        }
    }
    /**
    * @dev Burn ApeDads Tokens and Receive ApeFathers tokens in return - WARNING: THIS ACTION IS IRREVERSABLE. ENSURE YOU ARE AWARE OF THE RISKS OF BURNING YOUR ASSETS.
    */
    error TokenCountVerificationFailed();
    error BurnClaimIsNotActive();
    error InvalidClaim();
    error CannotClaimTokenYouDontHold();
    function burnClaim(uint16[] calldata _tokenIds, uint8 verifyNumberOfTokens) external nonReentrant onlyEOA {
        if (!isBurnClaimActive) {
            revert BurnClaimIsNotActive();
        }
        if (_tokenIds.length != verifyNumberOfTokens) {
            revert TokenCountVerificationFailed();
        }
        if (_tokenIds.length < 1) {
            revert InvalidClaim();
        }

        address[] memory owners = new address[](_tokenIds.length);
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            address owner = apeDads.ownerOf(_tokenIds[i]);
            if (owner != _msgSender()) {
                revert CannotClaimTokenYouDontHold();
            }
            owners[i] = owner;
            apeDads.burn(_tokenIds[i]);
        }

        if (_tokenIds.length == 1) {
            _safeMint(_msgSender(), _tokenIds[0]);
        } else if (_tokenIds.length > 1) {
            batchMint(_msgSender(), _tokenIds.length);
        }
        _tokenCounts[_msgSender()]++;
        // Increment the total supply
        _totalSupply++;
        emit BurnClaimComplete(_msgSender(), _tokenIds.length, _tokenIds);
    }

    /**
    * @notice Batch Transfer ApeFather NFTs Easier and gas efficiently
    */
    function batchTransfer(uint256[] memory _tokenIds, address _to) public {
        uint256 len = _tokenIds.length;
        require(len >= 2, "Must provide at least 2 IDs");
        for (uint256 i = 0; i < len; i += batchSizePerTx) {
            uint256 end = i + batchSizePerTx;
            if (end > len) {
                end = len;
            }
            uint256[] memory batchIds = new uint256[](end - i);
            for (uint256 j = i; j < end; j++) {
                batchIds[j - i] = _tokenIds[j];
            }
            for (uint256 j = 0; j < batchIds.length; j++) {
                this.safeTransferFrom(_msgSender(), _to, batchIds[j]);
            }
        }
    }
}


