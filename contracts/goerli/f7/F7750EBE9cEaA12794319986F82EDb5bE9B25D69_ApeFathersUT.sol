    address public royaltyAddress = 0xa8750034896B0747b290f98439B6f5969070084A;
    address[] public payoutAddresses = [
        0xa8750034896B0747b290f98439B6f5969070084A,
        0x1FA95261FA842bC9c6AB4C0e925daee53feFE430
