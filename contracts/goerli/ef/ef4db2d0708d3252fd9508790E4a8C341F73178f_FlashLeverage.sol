            - use that to repay the loan ,  take out the collateral (LOOKS)

            - use the collateral to make uniswap whole  

 





               
    */ 


    ///to be honest, this function is completely useless and can always be avoided by user anyways

    /// @param params The parameters necessary for flash and the callback, passed in as FlashParams
    /// @notice Calls the pools flash function with data needed in `uniswapV3FlashCallback`
    function initSwap(SwapParams calldata params) external {
        IUniswapV3Pool pool = IUniswapV3Pool(
            PoolAddress.computeAddress(factory, params.poolKey)
        );

        bool zeroForOne = params.amount0 > 0; //The direction of the swap, true for token0 to token1, false for token1 to token0

        require(
            (params.amount1 > 0 && params.amount0 <= 0) ||
                (params.amount0 > 0 && params.amount1 <= 0),
            "FlashLeverage: Invalid token amounts"
        );

        uint160 MIN_TICK_RATIO = 4295128740;
        uint160 sqrtPriceLimitX96 = MIN_TICK_RATIO; //for now

        int256 amountSpecified = zeroForOne
            ? int256(params.amount0)
            : int256(params.amount1);

            

        swapRegister = SwapRegister({
            user: msg.sender,
            active: true
        });

        /*
        address recipient,
        bool zeroForOne,
        int256 amountSpecified,
        uint160 sqrtPriceLimitX96,
        bytes calldata data
        */
        pool.swap(
            address(this), //recipient
            zeroForOne,
            amountSpecified,
            sqrtPriceLimitX96,
            //could we simplify this or does msg sender need to be here ?
            abi.encode(params)
        );
    }




    /// @param delta0 The delta from calling flash for token0
    /// @param delta1 The delta from calling flash for token1
    /// @param data The data needed in the callback passed as FlashCallbackData from `initFlash`
    /// @notice implements the callback called from flash
    /// @dev fails if the flash is not profitable, meaning the amountOut from the flash is less than the amount borrowed
    /// @dev be careful because this is public and can be called thru uniswap directly
    function uniswapV3SwapCallback(
        int256 delta0,
        int256 delta1,
        bytes calldata data
    ) public virtual {
        SwapParams memory params = abi.decode(data, (SwapParams));
        //require that only the pool can call this !!
        CallbackValidation.verifyCallback(factory, params.poolKey);

        address token0 = params.poolKey.token0;
        address token1 = params.poolKey.token1;

        // prevent calling this directly 
        require(swapRegister.active, "swap register not active");

        address borrower = swapRegister.user;


        for( uint i=0; i < params.actions.length; i++ ){
            _performAction( params.actions[i], borrower, delta0, delta1 );
        }
 

        IUniswapV3Pool pool = IUniswapV3Pool(
            PoolAddress.computeAddress(factory, params.poolKey)
        );

        _repayPool(token0, token1, delta0, delta1, address(pool));

        // pay the required amounts back to the pair

        //        // if profitable pay profits to payer
        //        if (amountOut0 > amount0Min) {
        //            uint256 profit0 = amountOut0 - amount0Min;
        //            pay(token0, address(this), decoded.payer, profit0);
        //        }
        //        if (amountOut1 > amount1Min) {
        //            uint256 profit1 = amountOut1 - amount1Min;
        //            pay(token1, address(this), decoded.payer, profit1);
        //        }

        _resetSwapRegister();
    }

