        uint256 collateralDepositAmount;
        uint256 tellerLoanId;
    }

 
    struct SwapRegister {
        address user; //defend against spoofing 
        bool active; //defend against re-enter
    }

    /*
        Only either amount 0 or amount 1 may be positive (what we are borrowing) , the other must be  0 or negative (becomes the min) 
        The one that is positive 

    */
    struct SwapParams {
        int256 amount0;
        int256 amount1;
        PoolAddress.PoolKey poolKey;

        Action[] actions;  
    }

    enum ActionType {
        OpenPosition,
        ClosePosition,
        RolloverPosition
    }

    //user will pass in an array of these
    struct Action {
        ActionType actionType;
        bytes actionData;
    }

    struct OpenPositionArgs {
