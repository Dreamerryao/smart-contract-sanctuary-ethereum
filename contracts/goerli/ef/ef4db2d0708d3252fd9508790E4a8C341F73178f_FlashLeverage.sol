    function _resetSwapRegister() internal {
        swapRegister = SwapRegister({
            user: address(0),
            active: false
        });
    }

    function _repayPool(
        address token0,
        address token1,
        int256 delta0,
        int256 delta1,
        address pool
    ) internal virtual {
        //PeripheryPayments.pay(address token, address payer, address recipient, uint256 value)

      
        
        if (delta0 > 0) pay(token0, address(this), pool, uint256(delta0));
        if (delta1 > 0) pay(token1, address(this), pool, uint256(delta1));
    }

    function _getNewPositionId() internal returns (uint256 posId_) {
        posId_ = _positionCounter.current();
        _positionCounter.increment();
    }



    function _performAction( 
        Action memory action, 
        address borrower, 
        int256 delta0, 
        int256 delta1 
    ) virtual internal {

          // we must receive at least the required payment from the action
        // uint256 amount0Min = LowGasSafeMath.add(decoded.params.amount0, fee0);
        // uint256 amount1Min = LowGasSafeMath.add(decoded.params.amount1, fee1);

        ActionType actionType = action.actionType;

        if (actionType == ActionType.OpenPosition) {
            OpenPositionArgs memory args = abi.decode(
                action.actionData,
                (OpenPositionArgs)
            );
            uint256 psnId = _openPosition(args, delta0, delta1);

          ///we know that our borrowAmount is args.borrowAmount
        //we need to repay the pool with the borrowed amount


        } else if (actionType == ActionType.ClosePosition) {
            ClosePositionArgs memory args = abi.decode(
                action.actionData,
                (ClosePositionArgs)
            );
            _closePosition(args);
        } else if (actionType == ActionType.RolloverPosition) {
            RolloverPositionArgs memory args = abi.decode(
                action.actionData,
                (RolloverPositionArgs)
            );
            _rolloverPosition(args);
        } else {
            revert("Invalid action");
        }





    }




    function _openPosition(
        OpenPositionArgs memory _args,
        int256 delta0, // do we need to pass in  ?
        int256 delta1 // do we need to pass in  ?
    ) internal virtual returns (uint256 posId_) {
        //create a new position
        // 1. pull collateral asset from owner
        // 2. flash swap collateral asset from Uniswap
        // 4. accept commitment from Teller
        //  4a. deposit collateral
        //  4b. take out loan
        // 5. swap borrowed asset from Teller for collateral asset

        //pull collateral into this contract so it can go into acceptCommitment
        //the principal paid out by acceptCommitment will be sent to uniswap pool

        //approve the collateral to the commitmentforwarder  . Need to handle diff if erc721
        IERC20( _args.collateralAsset ).transferFrom(_args.borrower, address(this), _args.collateralDepositAmount); 
        IERC20( _args.collateralAsset ).approve(address(commitmentForwarder), _args.collateralDepositAmount);


        uint256 bidId = commitmentForwarder.acceptCommitment(
            _args.commitmentId, // _commitmentId,
            _args.borrowAmount, //_principalAmount,
            _args.collateralDepositAmount, //_collateralAmount,
            _args.collateralTokenId, //_collateralTokenId,
            _args.collateralAsset,
            _args.commitmentInterestRate,
            _args.commitmentLoanDuration
        ); 
        //somehow change the borrower so its not this contract ?

        posId_ = _getNewPositionId();
        Position memory pos = positions[posId_];
        pos.borrower = _args.borrower;
        pos.borrowAsset = _args.borrowAsset;
        pos.borrowAmount = _args.borrowAmount;
        pos.collateralAsset = _args.collateralAsset;
        pos.collateralDepositAmount = _args.collateralDepositAmount;
        pos.tellerLoanId = bidId;

        // send the principal amount into the uni pool as repayment - no need for another swap [?]
    }

    function _closePosition(ClosePositionArgs memory _args) internal virtual {
        //close a position
        // 1. repay the teller loan
        // 2. withdraw collateral
        // 3. transfer collateral to owner


        /*
        
            //need to knnow the bidId  (from psn id) 

            //call repayLoanInFull on teller contract 

            //withdraw collateral from collateral manager -- make that withdrawable by the owner of the psn 


            
            //repay uniswap flash swap 

        */


        uint256 psnId = _args.positionId;
        Position memory position = positions[psnId];
        uint256 bidId = position.tellerLoanId;

        //repay the teller loan so we can access collateral 
        ITellerV2(tellerV2).repayLoanFull(bidId);


        //withdraw collateral from collateral manager
       /* address collateralEscrow = ITellerV2(tellerV2).getCollateralEscrow(bidId);

        ICollateralEscrowV1(collateralEscrow).withdraw(
            position.collateralAsset,
            position.collateralDepositAmount,
            position.borrower
        );*/


        //discuss modifying the TellerV2 Interface(s) for withdrawing collateral 



        //use the collateral to repay the uniswap flash swap ? 

    }



    /*
        is this not just a  ClosePosition and then an OpenPosition action ?
    */
    function _rolloverPosition(
        RolloverPositionArgs memory _args
    ) internal virtual returns (uint256 newPosId_) {
        //rollover a position
        // 1. flash swap Teller borrowed asset from Uniswap
        // 2. repay Teller loan
        // 3. accept commitment from Teller
        //  3a. deposit collateral
        //  3b. take out loan
        //    _closePosition(_args.positionId);
        //   newPosId_ = _openPosition();
