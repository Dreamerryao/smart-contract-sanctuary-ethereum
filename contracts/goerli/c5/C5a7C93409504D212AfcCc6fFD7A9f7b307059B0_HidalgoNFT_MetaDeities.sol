pragma solidity ^0.8.14;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract HidalgoNFT_MetaDeities is ERC721, ERC721URIStorage, ReentrancyGuard, Ownable {
    
    using Counters for Counters.Counter;
    using Strings for uint256;
    using SafeMath for uint256;
    using Address for address;
    Counters.Counter private tokenIDCounter;

    //COUNTERS FOR SPECIFIC IDs
    mapping(uint256 => string) private _tokenURIs;
    mapping(address => bool) whitelistedAddresses;
    mapping(address => bool) whitelistPurchased;
    mapping(address => bool) private betaUsers;
    mapping(address => bool) private betaUsersPurchased;

    //BASIC NFT PARAMETERS
    string private BASE_URI;
    string public PRE_REVEAL_URI;
    string public BASE_EXTENSION = ".json";
    uint256 public MAX_NFT_SUPPLY = 10000;
    uint256 public MINT_PRICE = 0.035 ether;
    uint256 public DISCOUNT_PERCENTAGE = 40;
    uint256 public MAX_NFT_PER_MINT = 20;
    uint256 public NFT_LIMIT_PER_ADDRESS = 200;
    bool public SALE_IS_ACTIVE = false;
    bool private BETA_PHASE = true;
    bool public ONLY_WHITELISTED = true;
    bool public REVEALED = false;

    //ADDRESSES OF FOUNDERS
    address private PAYOUT_ADDRESS_1;
    address private PAYOUT_ADDRESS_2;
    address private PAYOUT_ADDRESS_3;
    address private PAYOUT_ADDRESS_4;
    address private PAYOUT_ADDRESS_5;

    constructor(
        
        string memory BASE_URI_,
        string memory PRE_REVEAL_URI_,
        address PAYOUT_ADDRESS_1_,
        address PAYOUT_ADDRESS_2_,
        address PAYOUT_ADDRESS_3_,
        address PAYOUT_ADDRESS_4_,
        address PAYOUT_ADDRESS_5_
    
    ) ERC721("Hidalgo NFT: Meta Deities", "DALGO") {

        setBaseURI(BASE_URI_);
        setPreRevealURI(PRE_REVEAL_URI_);
        setPayOutAddress_1(PAYOUT_ADDRESS_1_);
        setPayOutAddress_2(PAYOUT_ADDRESS_2_);
        setPayOutAddress_3(PAYOUT_ADDRESS_3_);
        setPayOutAddress_4(PAYOUT_ADDRESS_4_);
        setPayOutAddress_5(PAYOUT_ADDRESS_5_);

    }

    function _baseURI() internal view virtual override returns (string memory) {

        return BASE_URI;

    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {

        require(_exists(tokenId), "ERC721URIStorage: URI query for nonexistent token");

        if(REVEALED == false) {

            return PRE_REVEAL_URI;

        }

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = _baseURI();

        if (bytes(base).length == 0) {

            return _tokenURI;

        }

        if (bytes(_tokenURI).length > 0) {

            return string(abi.encodePacked(base, _tokenURI));

        }

        return string(abi.encodePacked(super.tokenURI(tokenId), BASE_EXTENSION));

    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {

        super._burn(tokenId);

    }

    function totalSupply() public view returns (uint256) {

        return tokenIDCounter.current();

    }

    //Minting Process
    function Mint(uint256 _MINT_QUANTITY) public payable nonReentrant{

        require(SALE_IS_ACTIVE, "Minting is not active, please wait for futher announcements.");
        uint256 supply = totalSupply();
        uint256 ownerTokenCount = balanceOf(msg.sender);
        require(supply < MAX_NFT_SUPPLY, "Exceeded the maximum NFT Supply.");
        require(ownerTokenCount <= NFT_LIMIT_PER_ADDRESS, "Exceeded the maximum mint per wallet.");
        require(_MINT_QUANTITY > 0, "Number of NFTs to be minted cannot be 0 or lower.");
        require(_MINT_QUANTITY <= MAX_NFT_PER_MINT, "Exceeded the maximum amount of NFTs minted per transaction.");
        require(supply.add(_MINT_QUANTITY) <= MAX_NFT_SUPPLY, "Exceeded the maximum supply to mint.");

        if(msg.sender != owner()){

            if(BETA_PHASE){//Beta test period
                
                require(verifyBetaUsers(msg.sender), "Beta test phase ongoing, only selected users only have the access to mint.");
                require(_MINT_QUANTITY == 1, "Beta users can only mint one NFT for protection and security.");
                require(!verifybetaUsersPurchased(msg.sender), "You already have minted an NFT.");
                require(msg.value >= MINT_PRICE, "Please check the MATIC price and your balance.");
                betaUsersPurchased[msg.sender] = true;

            }else{

                if(verifyWhiteListedAddress(msg.sender) && !verifyWhiteListPurchase(msg.sender) && _MINT_QUANTITY == 1){//Whitelisted users can only mint an nft with a discounted price once
                    
                    require(msg.value >= MINT_PRICE.mul(_MINT_QUANTITY) - (MINT_PRICE.mul(_MINT_QUANTITY) * DISCOUNT_PERCENTAGE / 100), "Please check the discounted MATIC price and your balance.");
                    whitelistPurchased[msg.sender] = true;

                }else{

                    if(ONLY_WHITELISTED){//Whitelist period

                        require(verifyWhiteListedAddress(msg.sender), "Whitelist period ongoing, please wait for further announcements.");

                    }

                    require(msg.value >= MINT_PRICE.mul(_MINT_QUANTITY), "Please check the MATIC price and your balance.");

                }

            }

        }

        for(uint256 i = 0; i < _MINT_QUANTITY; i++) {

            _safeMint(msg.sender, supply + i);
            tokenIDCounter.increment();

        }

    }

    //Only Owner Access
    function MintToAccount(address _TO_ADDRESS, uint256 _QUANTITY) public onlyOwner nonReentrant{

        uint256 supply = totalSupply();
        require(supply < MAX_NFT_SUPPLY, "Exceeded the maximum NFT Supply.");
        require(_QUANTITY > 0, "Number of NFTs to be minted cannot be 0 or lower.");
        require(supply.add(_QUANTITY) <= MAX_NFT_SUPPLY, "Exceeded the maximum supply to mint.");

        for(uint256 i = 0; i < _QUANTITY; i++) {

            _safeMint(_TO_ADDRESS, supply + i);
            tokenIDCounter.increment();

        }

    }

    function flipSaleIsActive() public onlyOwner {//sets sale state to be active or not

        SALE_IS_ACTIVE = !SALE_IS_ACTIVE;

    }

    function flipBetaPhase() public onlyOwner {//sets beta phase period to be active or not

        BETA_PHASE = !BETA_PHASE;

    }

    function flipOnlyWhitelisted() public onlyOwner {//sets whitelisted period to be active or not

        ONLY_WHITELISTED = !ONLY_WHITELISTED;

    }

    function flipReveal() public onlyOwner {//sets reveal period to be active or not

        REVEALED = !REVEALED;

    }

    function setBaseURI(string memory _BASE_URI) public onlyOwner returns(string memory){//sets a new base uri

        return BASE_URI = _BASE_URI;

    }

    function setPreRevealURI(string memory _PRE_REVEAL_URI) public onlyOwner returns(string memory){//sets a new pre-reveal uri

        return PRE_REVEAL_URI = _PRE_REVEAL_URI;

    }

    function setBaseExtension(string memory _BASE_EXTENSION) public onlyOwner returns(string memory){//sets a new base extension

        return BASE_EXTENSION = _BASE_EXTENSION;

    }

    function setMaxNFTSupply(uint256 _MAX_NFT_SUPPLY) public onlyOwner returns(uint256){//sets the supply for the entire collection

        return MAX_NFT_SUPPLY = _MAX_NFT_SUPPLY;

    }

    function setMintPrice(uint256 _MINT_PRICE) public onlyOwner returns(uint256){//sets the mint prices for the NFTs

        return MINT_PRICE = _MINT_PRICE;
        
    }

    function setDiscountPercentage(uint256 _DISCOUNT_PERCENTAGE) public onlyOwner returns(uint256){//sets the discounted percentage for whitelisted users

        return DISCOUNT_PERCENTAGE = _DISCOUNT_PERCENTAGE;

    }

    function setMaxNFTPerMint(uint256 _MAX_NFT_PER_MINT) public onlyOwner returns(uint256){//sets the maximum threshold for minting NFTs in a single transaction

        return MAX_NFT_PER_MINT = _MAX_NFT_PER_MINT;

    }

    function setMaxNFTperAddress(uint256 _NFT_LIMIT_PER_ADDRESS) public onlyOwner returns(uint256){//sets how many nfts can one Wallet Address have

        return NFT_LIMIT_PER_ADDRESS = _NFT_LIMIT_PER_ADDRESS;

    }

    //Sets User Wallet Addresses
    function setPayOutAddress_1(address _PAYOUT_ADDRESS_1) public onlyOwner returns(address){return PAYOUT_ADDRESS_1 = _PAYOUT_ADDRESS_1;}
    function setPayOutAddress_2(address _PAYOUT_ADDRESS_2) public onlyOwner returns(address){return PAYOUT_ADDRESS_2 = _PAYOUT_ADDRESS_2;}
    function setPayOutAddress_3(address _PAYOUT_ADDRESS_3) public onlyOwner returns(address){return PAYOUT_ADDRESS_3 = _PAYOUT_ADDRESS_3;}
    function setPayOutAddress_4(address _PAYOUT_ADDRESS_4) public onlyOwner returns(address){return PAYOUT_ADDRESS_4 = _PAYOUT_ADDRESS_4;}
    function setPayOutAddress_5(address _PAYOUT_ADDRESS_5) public onlyOwner returns(address){return PAYOUT_ADDRESS_5 = _PAYOUT_ADDRESS_5;}

    //Adds a list of whitelisted users
    function addUserWhiteListAddress(address[] calldata _addressToWhiteList) external onlyOwner {

        for(uint i = 0;i < _addressToWhiteList.length;i++){

            whitelistedAddresses[_addressToWhiteList[i]] = true;
            whitelistPurchased[_addressToWhiteList[i]] = false;

        }

    }

    //Removes a list of whitelised users
    function removeUserWhiteListAddress(address[] calldata _addressToUnWhiteList) external onlyOwner {

        for(uint i = 0;i < _addressToUnWhiteList.length;i++){

            whitelistedAddresses[_addressToUnWhiteList[i]] = false;
            whitelistPurchased[_addressToUnWhiteList[i]] = true;

        }

    }

    //Sets first purchase of previous Whitelisted Address to false
    function setWhiteListPurchaseFalse(address[] calldata _addressToWhiteListPurchase) external onlyOwner{

        for(uint i = 0;i < _addressToWhiteListPurchase.length;i++){
            
            whitelistPurchased[_addressToWhiteListPurchase[i]] = false;

        }

    }

    //Verifies Whitelisted Addresses
    function verifyWhiteListedAddress(address _whitelistAddress) public view returns(bool) {

        return whitelistedAddresses[_whitelistAddress];

    }

    //Verifies if the Whitelist Addresses minted an nft with a discounted price one time
    function verifyWhiteListPurchase(address _whitelistAddress) public view returns(bool) {

        return whitelistPurchased[_whitelistAddress];

    }

    //Adds a list of beta users
    function addBetaUsers(address[] calldata _betaUser) external onlyOwner {

        for(uint i = 0;i < _betaUser.length;i++){

            betaUsers[_betaUser[i]] = true;
            betaUsersPurchased[_betaUser[i]] = false;

        }

    }

    //Removes a list of beta users
    function removeBetaUsers(address[] calldata _betaUser) external onlyOwner {

        for(uint i = 0;i < _betaUser.length;i++){

            betaUsers[_betaUser[i]] = false;
            betaUsersPurchased[_betaUser[i]] = true;

        }

    }

    //Sets first purchase of previous Beta Users to false
    function setbetaUsersPurchaseFalse(address[] calldata _betaUser) external onlyOwner{

        for(uint i = 0;i < _betaUser.length;i++){
            
            betaUsersPurchased[_betaUser[i]] = false;

        }

    }

    //Verifies beta users who were specifially chosen to test the contract
    function verifyBetaUsers(address _betaUser) private view returns(bool){

        return betaUsers[_betaUser];

    }

    //Verifies beta users who tested the mint at the beta testing phase
    function verifybetaUsersPurchased(address _betaUser) private view returns(bool){

        return betaUsersPurchased[_betaUser];

    }

    //Functions that withdraw the funds with specific distributions to the founders
    function withdrawToHidalgo100() public payable nonReentrant onlyOwner {

        (bool toOwner, ) = payable(owner()).call{value: address(this).balance }("");//100%
        require(toOwner, "Transaction Failed (Owner)");

    }

    function withdrawToHidalgo52() public payable nonReentrant onlyOwner {

        (bool toFounder5, ) = payable(PAYOUT_ADDRESS_5).call{value: address(this).balance * 9 / 100}("");//9% of TOTAL MATIC (9% of 100)
        require(toFounder5, "Transaction Failed (Payout Address 5)");
        
        (bool toFounder4, ) = payable(PAYOUT_ADDRESS_4).call{value: address(this).balance * 10 / 100}("");//9% of NEW TOTAL MATIC (10% of 91)
        require(toFounder4, "Transaction Failed (Payout Address 4)");

        (bool toFounder3, ) = payable(PAYOUT_ADDRESS_3).call{value: address(this).balance * 11 / 100}("");//9% of NEW TOTAL MATIC (11% of 82)
        require(toFounder3, "Transaction Failed (Payout Address 3)");

        (bool toFounder2, ) = payable(PAYOUT_ADDRESS_2).call{value: address(this).balance * 13 / 100}("");//9% of NEW TOTAL MATIC (13% of 73)
        require(toFounder2, "Transaction Failed (Payout Address 2)");
        
        (bool toFounder1, ) = payable(PAYOUT_ADDRESS_1).call{value: address(this).balance * 19 / 100}("");//12% of NEW TOTAL MATIC (19% of 64)
        require(toFounder1, "Transaction Failed (Payout Address 1)");

        (bool toOwner, ) = payable(owner()).call{value: address(this).balance}("");//100% of NEW TOTAL MATIC (100% of 52)
        require(toOwner, "Transaction Failed (Owner)");

    }

}

