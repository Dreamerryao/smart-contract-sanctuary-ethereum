pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./interfaces/IMember.sol";
import "./interfaces/IMembership.sol";
import "./interfaces/IMembershipFactory.sol";
import "./structs/MemberStruct.sol";
import "./structs/BackupApprovalStruct.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "./structs/MembershipStruct.sol";
//DEV
import "hardhat/console.sol";

contract Member is
    IMember,
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable
{
    // Event when creating member
    event memberCreated(string uid, uint256 dateCreated);
    // Event when updating primary wallet
    event walletUpdated(
        string uid,
        uint256 dateCreated,
        address[] deployedFactories,
        address[] backupWallets,
        address[] wallets,
        uint256 primaryWallet
    );
    // Event when updating factories
    event factoryUpdated(
        string uid,
        uint256 dateCreated,
        address[] deployedFactories,
        address[] backupWallets,
        address[] wallets,
        uint256 primaryWallet
    );

    // Mapping to return member when uid is passed
    mapping(string => member) public members;

    // Variable to store all member information
    member[] public allMembers;

    // IMembershipAddress for getting Membership Information
    mapping(address => address) private UserMembershipAddress;

    // Storing ApprovalId for different approvals stored
    uint256 private _approvalId;

    // Returns token backup Approvals for specific UID
    mapping(string => BackUpApprovals[]) private MemberApprovals;

    // Event for Querying Approvals
    event BackUpApprovalsEvent(
        string uid,
        address approvedWallet,
        address[] backupaddress,
        uint256 tokenId,
        address tokenAddress,
        string tokenType,
        uint256 tokensAllocated,
        uint256 dateApproved,
        bool claimed,
        uint256 approvalId
    );

    // Address of Whitelist Users Contract
    address private IWhiteListUsersAddress;


    /**
     * @dev initialize - Initializes the function for Ownable and Reentrancy.
     */
    function initialize() public initializer {
        __Context_init_unchained();
        __Ownable_init();
        __ReentrancyGuard_init();
        _approvalId = 0;
    }

    /**
     * @dev checkWalletExists - Modifier to check if Wallet Exists among users.
     */
    modifier checkWalletExists(address _walletAddress) {
        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            address[] storage wallets_ = member_.wallets;
            for (uint256 j = 0; j < wallets_.length; j++) {
                if (wallets_[j] == _walletAddress) {
                    revert("Wallet Address already exists for another user");
                }
            }
        }
        _;
    }

    function checkIfWalletHasNFT(
        address _contractAddress,
        string memory _NFTType,
        uint256 tokenId,
        address userAddress
    ) public view {
        // check if wallet has nft
        bool status = false;
        if (
            keccak256(abi.encodePacked((_NFTType))) ==
            keccak256(abi.encodePacked(("ERC721")))
        ) {
            if (IERC721(_contractAddress).ownerOf(tokenId) == userAddress) {
                status = true;
            } else if (
                IERC721Upgradeable(_contractAddress).ownerOf(tokenId) ==
                userAddress
            ) {
                status = true;
            }
        }

        if (
            keccak256(abi.encodePacked((_NFTType))) ==
            keccak256(abi.encodePacked(("ERC1155")))
        ) {
            if (
                IERC1155(_contractAddress).balanceOf(userAddress, tokenId) != 0
            ) {
                status = true;
            } else if (
                IERC1155Upgradeable(_contractAddress).balanceOf(
                    userAddress,
                    tokenId
                ) != 0
            ) {
                status = true;
            }
        }

        if (status == false) {
            revert(
                "The user does not own a token of the supporting NFT Collection"
            );
        }
    }

    /*
    @dev createMember
    params: uid 
    Allows to create a member onChain with a unique UID passed.
    */
    function createMember(string memory uid, address _walletAddress) public {
        if (
            (keccak256(abi.encodePacked((members[uid].uid))) !=
                keccak256(abi.encodePacked((uid))))
        ) {
            require(bytes(uid).length != 0, "UID is empty");
            address[] memory _factories;
            address[] memory _backUpWallets;
            address[] memory _wallets;
            member memory _member = member(
                uid,
                block.timestamp,
                _factories,
                _backUpWallets,
                _wallets,
                0
            );
            members[uid] = _member;
            allMembers.push(_member);
            addWallet(uid, _walletAddress, true);
            emit memberCreated(_member.uid, _member.dateCreated);
        }
    }

    /*
    @dev getMember
    params: uid 
    Allows to get member information stored onChain with a unique UID passed.
    */
    function getMember(string memory uid)
        public
        view
        override
        returns (member memory)
    {
        return members[uid];
    }

    /*
    @dev getAllMembers
    Allows to get all member information stored onChain
    */
    function getAllMembers() external view override returns (member[] memory) {
        return allMembers;
    }

    /*
    @dev addWallet
    Allows to add Wallet to the user
    */
    function addWallet(
        string memory uid,
        address _wallet,
        bool _primary
    ) public override checkWalletExists(_wallet) {
        member storage _member = members[uid];
        _member.wallets.push(_wallet);
        if (_primary) {
            _member.primaryWallet = _member.wallets.length - 1;
        }

        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            if (
                keccak256(abi.encodePacked((member_.uid))) ==
                keccak256(abi.encodePacked((uid)))
            ) {
                member_.wallets.push(_wallet);
                if (_primary) {
                    member_.primaryWallet = member_.wallets.length - 1;
                }
            }
        }

        emit walletUpdated(
            _member.uid,
            _member.dateCreated,
            _member.deployedFactories,
            _member.backupWallets,
            _member.wallets,
            _member.primaryWallet
        );
    }

    /* 
    @dev deleteWallet
    Allows to delete  wallets of a specific user
    */
    function deleteWallet(string calldata uid, uint256 _walletIndex)
        external
        override
    {
        member storage _member = members[uid];
        require(
            _walletIndex != _member.primaryWallet,
            "You cannot delete your primary wallet"
        );
        require(
            _member.wallets[_member.primaryWallet] == msg.sender,
            "Only primary wallet holder can delete wallets"
        );
        delete members[uid].wallets[_walletIndex];
        address[] storage wallets = members[uid].wallets;
        for (uint256 i = _walletIndex; i < wallets.length - 1; i++) {
            wallets[i] = wallets[i + 1];
        }

        if (members[uid].primaryWallet >= _walletIndex) {
            members[uid].primaryWallet--;
        }
        wallets.pop();

        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            if (
                keccak256(abi.encodePacked((member_.uid))) ==
                keccak256(abi.encodePacked((uid)))
            ) {
                address[] storage wallets_ = member_.wallets;
                for (uint256 j = _walletIndex; j < wallets_.length - 1; j++) {
                    wallets_[j] = wallets_[j + 1];
                }
                wallets_.pop();
                if (member_.primaryWallet >= _walletIndex) {
                    member_.primaryWallet--;
                }
            }
        }
    }

    /* 
    @dev setPrimaryWallet
    Allows to set a specific wallet as the primary wallet
    */
    function setPrimaryWallet(string calldata uid, uint256 _walletIndex)
        external
        override
    {
        require(
            _walletIndex != members[uid].primaryWallet,
            "Current Wallet is already the primary Wallet"
        );
        require(
            members[uid].wallets[members[uid].primaryWallet] == msg.sender,
            "Only primary wallet can update to a new primary wallet"
        );
        members[uid].primaryWallet = _walletIndex;
        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            if (
                keccak256(abi.encodePacked((member_.uid))) ==
                keccak256(abi.encodePacked((uid)))
            ) {
                member_.primaryWallet = _walletIndex;
            }
        }
    }

    /*
    @dev getWallets
    Allows to get all wallets of the user
    */
    function getWallets(string calldata uid)
        external
        view
        override
        returns (address[] memory)
    {
        return members[uid].wallets;
    }

    /*
    @dev getPrimaryWallets
    Allows to get primary wallet of the user
    */
    function getPrimaryWallet(string memory uid)
        public
        view
        override
        returns (address)
    {
        return members[uid].wallets[members[uid].primaryWallet];
    }

    /*
    @dev addBackupWallet
    Allows to add Backup Wallets to the user
    */
    function addBackupWallet(string calldata uid, address[] memory _wallets)
        public
        override
    {
        createMember(uid, msg.sender);
        member storage _member = members[uid];
        require(
            _member.wallets.length != 0,
            "User should have a primary wallet"
        );
        for (uint256 i = 0; i < _wallets.length; i++) {
            _member.backupWallets.push(_wallets[i]);
        }

        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            if (
                keccak256(abi.encodePacked((member_.uid))) ==
                keccak256(abi.encodePacked((uid)))
            ) {
                for (uint256 j = 0; j < _wallets.length; j++) {
                    member_.backupWallets.push(_wallets[i]);
                }
            }
        }

        emit walletUpdated(
            _member.uid,
            _member.dateCreated,
            _member.deployedFactories,
            _member.backupWallets,
            _member.wallets,
            _member.primaryWallet
        );
    }

    /*
    @dev getBackupWallets
    Allows to get all Backup wallets of the user
    */
    function getBackupWallets(string calldata uid)
        external
        view
        override
        returns (address[] memory)
    {
        return members[uid].backupWallets;
    }

    /*
    @dev checkBackupWallet
    Allows to check if a wallet is a Backup wallets of the user
    */
    function checkBackupWallet(
        address[] calldata _backUpWallet,
        string memory uid
    ) internal view returns (bool) {
        address[] memory backupWallets = members[uid].backupWallets;
        bool walletExists = false;
        for (uint256 i = 0; i < backupWallets.length; i++) {
            for (uint256 j = 0; j < _backUpWallet.length; j++) {
                if (backupWallets[i] == _backUpWallet[j]) {
                    walletExists = true;
                }
            }
        }
        return walletExists;
    }

    /*
    @dev addFactory
    Allows to add a Factory to the user
    */
    function addFactory(string calldata uid, address _factory)
        external
        override
    {
        createMember(uid, msg.sender);
        //TODO: only allow supporting factories
        member storage _member = members[uid];
        require(
            _member.wallets.length != 0,
            "User should have a primary wallet"
        );
        _member.deployedFactories.push(_factory);

        for (uint256 i = 0; i < allMembers.length; i++) {
            member storage member_ = allMembers[i];
            if (
                keccak256(abi.encodePacked((member_.uid))) ==
                keccak256(abi.encodePacked((uid)))
            ) {
                member_.deployedFactories.push(_factory);
            }
        }

        emit factoryUpdated(
            _member.uid,
            _member.dateCreated,
            _member.deployedFactories,
            _member.backupWallets,
            _member.wallets,
            _member.primaryWallet
        );
    }

    /*
    @dev getFactories
    Allows to get all factories of the user
    */
    function getFactories(string calldata uid)
        external
        view
        override
        returns (address[] memory)
    {
        return members[uid].deployedFactories;
    }

    /*
    @dev getUID
    Allows user to pass walletAddress and return UID 
    */
    function getUID(address _walletAddress)
        public
        view
        override
        returns (string memory)
    {
        string memory memberuid;
        for (uint256 i = 0; i < allMembers.length; i++) {
            address[] memory _wallets = allMembers[i].wallets;
            if (_wallets.length != 0) {
                for (uint256 j = 0; j < _wallets.length; j++) {
                    if (_wallets[j] == _walletAddress) {
                        memberuid = allMembers[i].uid;
                    }
                }
            }
        }
        require(bytes(memberuid).length != 0, "No UID found");
        return memberuid;
    }

    /**
     * @dev storeBackupAssetsApprovals - Function to store All Types Approvals by the user for backup
     */
    function storeBackupAssetsApprovals(
        address[] calldata _contractAddress,
        uint256[] calldata _tokenIds,
        address[] calldata _backUpWallets,
        uint256[] calldata _tokenAmount,
        string[] calldata _tokenTypes,
        string calldata _memberUID,
        address _userAddress
    ) public {
        require(
            _tokenIds.length == _contractAddress.length &&
                _tokenAmount.length == _tokenTypes.length,
            "Lengths of parameters need to be equal"
        );

        createMember(_memberUID, _userAddress);
        member memory _member = getMember(_memberUID);
        checkUserHasMembership(_userAddress);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            address contractAddress = _contractAddress[i];
            uint256 tokenId_ = _tokenIds[i];
            string memory tokenType = _tokenTypes[i];
            uint256 tokenAmount = _tokenAmount[i];

            checkAssetContract(contractAddress, tokenType);
            Token memory _token = Token(
                tokenId_,
                contractAddress,
                tokenType,
                tokenAmount
            );

            _storeAssets(
                _memberUID,
                _member,
                _userAddress,
                _backUpWallets,
                _token
            );
        }
        IMembership(UserMembershipAddress[_userAddress]).redeemUpdate(
            _userAddress
        );
    }

    /**
     * @dev _storeAssets - Internal function to store assets approvals for backup
     */
    function _storeAssets(
        string calldata uid,
        member memory _member,
        address user,
        address[] calldata _backUpWallet,
        Token memory _token
    ) internal {
        uint256 _dateApproved = block.timestamp;

        if (checkBackupWallet(_backUpWallet, uid) == false) {
            addBackupWallet(uid, _backUpWallet);
        }

        BackUpApprovals memory approval = BackUpApprovals(
            _member,
            user,
            _backUpWallet,
            _token,
            _dateApproved,
            false,
            ++_approvalId
        );

        MemberApprovals[uid].push(approval);
        emit BackUpApprovalsEvent(
            _member.uid,
            user,
            _backUpWallet,
            _token.tokenId,
            _token.tokenAddress,
            _token.tokenType,
            _token.tokensAllocated,
            _dateApproved,
            false,
            _approvalId
        );
    }

    // executePanic() - Public function to transfer assets from one user to another
    function executePanic(address _backUpWallet, string memory _memberUID) external {
        require(
            MemberApprovals[_memberUID].length > 0,
            "User should backup assets prior to executing panic button"
        );
        _panic(_memberUID, _backUpWallet);
    }

    // _checkBackUpExists - Internal function that checks backup approvals if the backup Wallet Exists
    function _checkBackUpExists(
        BackUpApprovals memory _approvals,
        address _backUpWallet
    ) internal pure {
        bool backUpExists = false;
        for (uint256 j = 0; j < _approvals.backUpWallet.length; j++) {
            if (_approvals.backUpWallet[j] == _backUpWallet) {
                backUpExists = true;
            }
        }
        if (!backUpExists) {
            revert("BackUp Wallet specified is not the users backup Wallet");
        }
    }

    // _panic - Private Function to test panic functionality in order to execute and transfer all assets from one user to another
    function _panic(string memory uid, address _backUpWallet) internal {
        BackUpApprovals[] storage _approvals = MemberApprovals[uid];
        for (uint256 i = 0; i < _approvals.length; i++) {
            if (_approvals[i].claimed == false) {
                _checkBackUpExists(_approvals[i], _backUpWallet);
                if (
                    keccak256(
                        abi.encodePacked((_approvals[i].token.tokenType))
                    ) == keccak256(abi.encodePacked(("ERC20")))
                ) {
                    IERC20 ERC20 = IERC20(_approvals[i].token.tokenAddress);

                    ERC20.transferFrom(
                        _approvals[i].approvedWallet,
                        _backUpWallet,
                        _approvals[i].token.tokensAllocated
                    );

                    _approvals[i].claimed = true;
                }
                if (
                    keccak256(
                        abi.encodePacked((_approvals[i].token.tokenType))
                    ) == keccak256(abi.encodePacked(("ERC721")))
                ) {
                    IERC721 ERC721 = IERC721(_approvals[i].token.tokenAddress);

                    ERC721.safeTransferFrom(
                        _approvals[i].approvedWallet,
                        _backUpWallet,
                        _approvals[i].token.tokenId
                    );
                    _approvals[i].claimed = true;
                }
                if (
                    keccak256(
                        abi.encodePacked((_approvals[i].token.tokenType))
                    ) == keccak256(abi.encodePacked(("ERC1155")))
                ) {
                    IERC1155 ERC1155 = IERC1155(
                        _approvals[i].token.tokenAddress
                    );

                    bytes memory data;

                    ERC1155.safeTransferFrom(
                        _approvals[i].approvedWallet,
                        _backUpWallet,
                        _approvals[i].token.tokenId,
                        _approvals[i].token.tokensAllocated,
                        data
                    );
                    _approvals[i].claimed = true;
                }
            }
        }
    }

    // getBackupApprovals - function to return all backupapprovals for a specific UID
    function getBackupApprovals(string memory uid)
        external
        view
        returns (BackUpApprovals[] memory)
    {
        return MemberApprovals[uid];
    }

    /**
     * @dev editBackup - Function to edit individual backup approvals
     */
    function editBackUp(
        uint256 approvalId_,
        address _contractAddress,
        uint256 _tokenIds,
        uint256 _tokenAmount,
        string calldata _tokenType
    ) external {
        string memory _memberUID = getUID(msg.sender);
        member memory _member = getMember(_memberUID);
        checkUserHasMembership(msg.sender);

        BackUpApprovals[] storage _approvals = MemberApprovals[_member.uid];
        for (uint256 i = 0; i < _approvals.length; i++) {
            if (_approvals[i].approvalId == approvalId_) {
                _approvals[i].token.tokenAddress = _contractAddress;
                _approvals[i].token.tokenId = _tokenIds;
                _approvals[i].token.tokensAllocated = _tokenAmount;
                _approvals[i].token.tokenType = _tokenType;
            }
        }
        IMembership(UserMembershipAddress[msg.sender]).redeemUpdate(msg.sender);
    }

    /**
     * @dev editAllBackUp - Function to delete and add new approvals for backup
     */
    function editAllBackUp(
        address[] calldata _contractAddress,
        uint256[] calldata _tokenIds,
        address[] calldata _backUpWallets,
        uint256[] calldata _tokenAmount,
        string[] calldata _tokenTypes,
        string calldata _memberUID
    ) external {
        checkUserHasMembership(msg.sender);
        member memory _member = getMember(_memberUID);
        delete MemberApprovals[_member.uid];
        storeBackupAssetsApprovals(
            _contractAddress,
            _tokenIds,
            _backUpWallets,
            _tokenAmount,
            _tokenTypes,
            _memberUID,
            msg.sender
        );
    }

    /**
     * @dev deleteAllBackUp - Function to delete all backup approvals
     */
    function deleteAllBackUp() external {
        string memory _memberUID = getUID(msg.sender);
        member memory _member = getMember(_memberUID);
        delete MemberApprovals[_member.uid];
    }

    // Function to check if user has membership
    function checkUserHasMembership(address _user) public view {
        IMembership _membership = IMembership(UserMembershipAddress[_user]);
        bool _MembershipActive = _membership.checkIfMembershipActive(_user);
        if (_MembershipActive == false) {
            revert("User does not have a membership");
        } else {
            MembershipStruct memory Membership = IMembership(
                UserMembershipAddress[_user]
            ).getMembership(_user);
            require(
                Membership.updatesPerYear > 0,
                "User does not have sufficient topUp Updates in order to store approvals"
            );
        }
    }

    //Function set IMembershipAddress
    function setIMembershipAddress(
        address _user,
        address _Membership,
        address _factoryAddress
    ) external {
        require(
            IMembershipFactory(_factoryAddress).getUserMembershipAddress(
                _user
            ) == _Membership,
            "User should have its deployed Membership Address"
        );
        UserMembershipAddress[_user] = _Membership;
    }

    // Function to get IMembershipAddress
    function getIMembershipAddress(address _user)
        external
        view
        returns (address)
    {
        return UserMembershipAddress[_user];
    }

    /**
    * @dev checkAssetContract

     Checks if the assets passed through are assets of the determined type or not.
     */
    function checkAssetContract(
        address _contractAddress,
        string memory _tokenType
    ) public view {
        if (
            (keccak256(abi.encodePacked((_tokenType))) ==
                keccak256(abi.encodePacked(("ERC721"))))
        ) {
            require(
                (IERC721Upgradeable(_contractAddress).supportsInterface(
                    type(IERC721Upgradeable).interfaceId
                ) ||
                    IERC721(_contractAddress).supportsInterface(
                        type(IERC721).interfaceId
                    )),
                "Does not support a Supported ERC721 Interface"
            );
        } else if (
            keccak256(abi.encodePacked((_tokenType))) ==
            keccak256(abi.encodePacked(("ERC20")))
        ) {
            require(
                (IERC20(_contractAddress).totalSupply() >= 0 ||
                    IERC20Upgradeable(_contractAddress).totalSupply() >= 0),
                "Is not an ERC20 Contract Address"
            );
        } else if (
            keccak256(abi.encodePacked((_tokenType))) ==
            keccak256(abi.encodePacked(("ERC1155")))
        ) {
            require(
                (IERC1155Upgradeable(_contractAddress).supportsInterface(
                    type(IERC1155Upgradeable).interfaceId
                ) ||
                    IERC1155(_contractAddress).supportsInterface(
                        type(IERC1155).interfaceId
                    )),
                "Does not support a Supported ERC1155 Interface"
            );
        } else {
            revert("Invalid token type");
        }
    }

    // Function to get whitelist user contract address
    function getWhiteListAddress() external view returns (address) {
        return IWhiteListUsersAddress;
    }

    // Function to set whitelist user address
    function setWhiteListAddress(address _whiteListAddress) external onlyOwner {
        IWhiteListUsersAddress = _whiteListAddress;
    }
}

