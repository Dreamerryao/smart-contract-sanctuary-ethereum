pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@solidstate/contracts/token/ERC1155/SolidStateERC1155.sol";

import './lib/AccessControl.sol';
import "./lib/WithdrawAble.sol";
import "./lib/SalesActivation.sol";
import "./lib/Whitelist.sol";

// BeatlesOG 1155
contract BeatlesOG is SolidStateERC1155, Whitelist, SalesActivation, WithdrawAble {

    string public constant name = "BeatlesOG";
    string public constant symbol = "BeatlesOG";

    // ---------------------------------------- const
    // pre sales price
    // idx 1: red 0.01, 2: blue 0.2, 3: black 100
    uint256[] public nftPrePrice = [
        0 ether,
        0.01 ether,     // red
        0.2 ether,      // blue
        100 ether       // black
    ];

    // public sales price
    // idx 1: red 0.05, 2: blue 1, 3, black 500
    uint256[] public nftPublicPrice = [
        0 ether,
        0.05 ether,     // red
        1 ether,        // blue
        500 ether       // black
    ];

    // every card of NFTs every wallet can buy
    uint256 constant public maxQtyPerMinterForPerCard = 1;

    // ---------------------------------------- variable
    // the total number of every nft
    // idx 1: red, 2: blue, 3, black
    uint256[] public totalNftNumber = [
        0,
        3000,   // red
        300,    // blue
        30      // black
        ];

    // pre sold number
    // idx 1: red, 2: blue, 3, black
    uint256[] public preSoldNum = [0,0,0,0];
    // public sold number
    // idx 1: red, 2: blue, 3, black
    uint256[] public publicSoldNum = [0,0,0,0];

    // pre per minter
    mapping(address => mapping(uint256 => uint256)) public publicSalesMinterToTokenQty;
    // public per minter
    mapping(address => mapping(uint256 => uint256)) public preSalesMinterToTokenQty;

    constructor() {
    }

    // ---------------------------------------- admin
    function setBaseURI(string memory baseURI)
    public
    onlyOwner
    {
        _setBaseURI(baseURI);
    }
    function setTokenURI(uint256 tokenId, string memory tokenURI)
    public
    onlyOwner
    {
        _setTokenURI(tokenId, tokenURI);
    }

    // airdrop
    function airdrop(address[] memory accounts, uint256 id, uint256 amounts)
    public
    isTheNftAvailableForSales( id, accounts.length * amounts )
    onlyOwner
    {
        for (uint256 i = 0; i < accounts.length; i++) {
            _mint(accounts[i], id, amounts, "");
        }
    }

    // pre mint
    function preMint( uint256 id, uint256 amount)
    public
    isPreSalesActive
    isValidNft(id)
    isTheNftAvailableForSales(id, amount)
    isSenderInWhiteList
    payable
    callerIsUser
    {
        require(
            msg.value >= amount * nftPrePrice[id],
            "Insufficient ETHW!"
        );

        // the number of the per minter could buy.
        uint256 mintedNum = preSalesMinterToTokenQty[msg.sender][id];
        require(
            mintedNum + amount <= maxQtyPerMinterForPerCard,
            "Exceed max mint per minter!"
        );

        preSalesMinterToTokenQty[msg.sender][id] += amount;
        preSoldNum[id] += amount;

        // mint
        _mint(msg.sender, id, amount, "");
    }

    // public mint
    function publicMint(uint256 id, uint256 amount)
    public
    isPublicSalesActive
    isValidNft(id)
    isTheNftAvailableForSales(id, amount)
    payable
    callerIsUser
    {

        require(
            msg.value >= amount * nftPublicPrice[id],
            "Insufficient ETHW!"
        );
        // the number of the per minter could buy.
        uint256 mintedNum = publicSalesMinterToTokenQty[msg.sender][id];
        require(
            mintedNum + amount <= maxQtyPerMinterForPerCard,
            "Exceed max mint per minter!"
        );

        publicSalesMinterToTokenQty[msg.sender][id] += amount;
        publicSoldNum[id] += amount;

        // mint
        _mint(msg.sender, id, amount, "");
    }



    // sales amount condition
    modifier isTheNftAvailableForSales( uint256 id, uint256 amount )
    {
        require( amount + totalSupply(id) <= totalNftNumber[id], "Max Sales Supply Exceeded.");
        _;
    }
    // not other contract
    modifier callerIsUser()
    {
        require(tx.origin == msg.sender, "Not user!");
        _;
    }

    // is the nft id can mint
    modifier isValidNft(uint256 id)
    {
        require( 1 == id || 2 == id || 3 == id, "The NFT not available" );
        _;
    }


}

