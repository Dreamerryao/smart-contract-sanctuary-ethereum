import "./AirdropControl.sol";

error ApplyToAddToken__TransferFailed();
error ApplyToAddToken__AlreadyAdded();

// Heirarchy is TwoLevelReferral.sol > AirDropControl > AddSlotTokenERC20  > SlotMachine
contract AddSlotTokenERC20 is AirdropControl {
    uint256 public feeToAddToken = 3 ether;

    address[] public allTokenArray;

    mapping(address => TokenBundle) public allTokenBundles;

    event TokenContractAdded(address tokenContract);
    event TokenPrizesAdded(address tokenContract, uint256 amountOftokens);

    struct TokenBundle {
        address tokenAddress;
        uint256 oneRollPrice;
        bool isBanned; // false
        bool isAdded; // default true
    }

    //-------------------------------------------------------------------------
    // EXTERNAL FUNCTIONS
    //-------------------------------------------------------------------------

    function applyToAddToken(address _tokenContractAddress, uint256 _oneRollPrice) public payable {
        if (msg.value < feeToAddToken) {
            revert ApplyToAddToken__TransferFailed();
        }

        if (allTokenBundles[_tokenContractAddress].tokenAddress != address(0)) {
            revert ApplyToAddToken__AlreadyAdded();
        }

        address rootOwner = owner();

        (bool sent, ) = rootOwner.call{value: msg.value}("");
        if (!sent) {
            revert ApplyToAddToken__TransferFailed();
        }

        allTokenBundles[_tokenContractAddress] = TokenBundle(_tokenContractAddress, _oneRollPrice, false, true);
        allTokenArray.push(_tokenContractAddress);
        emit TokenContractAdded(_tokenContractAddress);
    }

    function addTokenPrizes(address _tokenContractAddress, uint256 _amount) external {
        IERC20(_tokenContractAddress).transferFrom(msg.sender, address(this), _amount);
        emit TokenPrizesAdded(_tokenContractAddress, _amount);
    }

    //-------------------------------------------------------------------------
    // VIEW FUNCTIONS
    //-------------------------------------------------------------------------

    function getAllTokens() external view returns (TokenBundle[] memory) {
        return _getAllTokens();
    }

    function getTokenByAddress(address _tokenContractAddress) external view returns (TokenBundle memory) {
        return allTokenBundles[_tokenContractAddress];
    }

    //-------------------------------------------------------------------------
    // INTERNAL FUNCTIONS
    //-------------------------------------------------------------------------

    function _getAllTokens() internal view returns (TokenBundle[] memory) {
        TokenBundle[] memory returnTokenBundles = new TokenBundle[](allTokenArray.length);
        for (uint256 i = 0; i < allTokenArray.length; i++) {
            returnTokenBundles[i] = allTokenBundles[allTokenArray[i]];
        }
        return returnTokenBundles;
    }

    //-------------------------------------------------------------------------
    // ONLY OWNER
    //-------------------------------------------------------------------------

    function banTokenType(address _tokenContractAddress) external onlyOwner {
        // ban the token address
    }

    function removeTokenType(address _tokenContractAddress) external onlyOwner {
        // remove the token address
    }

    function clearOtherTokens(IERC20 _tokenAddress, address _to) external onlyOwner {
        _tokenAddress.transfer(_to, _tokenAddress.balanceOf(address(this)));
    }

    function updateFeeToAddToken(uint256 _fee) external onlyOwner {
        feeToAddToken = _fee;
    }
}

