    using Counters for Counters.Counter;
    bool public startContract = true;

    Counters.Counter public nonVRFRequestIdCounter;

    VRFCoordinatorV2Interface public vrfCoordinatorV2;
    uint256 private randomSeedStamp = 0;
    uint256 private randomSeed2 = 0;
    uint256 private randomSeed3 = 0;

    struct VRFRequest {
        RollState rollState;
        address playersAddress;
        address tokenAddress;
        uint256 requestId;
        uint256 amount;
        uint256 rand1;
        uint256 rand2;
        uint256 rand3;
        uint256 reward;
        bytes32 winType;
    }

    enum RollState {
        ETHRoll,
        ERC20Roll,
        Idle
    }

    address public rootOwner;

    uint256 public coinPrice = 0.01 ether;
    uint64 public subscriptionId;
    uint32 public callbackGasLimit;
    uint32 public smallWinDivider = 1000;
    uint8 public constant NUM_WORDS = 3;
    uint8 public constant REQUEST_CONFIRMATIONS = 3;
    uint8 public constant SLOT_ITEMS = 15;
    uint8 public smallWin = 100; //10 percent

    bool public shouldUseVRF = false; // default to false since it is expensive

    uint256[] public requestIdArray;
    address[] public playersArray;

    // requestId -> structRequest
    mapping(uint256 => VRFRequest) public vrfRequestsMap;
    // requestId => playersAddress
    mapping(uint256 => address) public requestToPlayersAddress;
    // useraddress => reward
    mapping(address => uint256) public pendingWithdrawals;
    // useraddress => erc20 address => number of tokens won for that contract
    mapping(address => mapping(address => uint256)) public pendingERC20Withdrawals;
    // useraddress => bool
    mapping(address => bool) playersMapping;

    bytes32 public gasLane;

    //-------------------------------------------------------------------------
    // EVENTS
    //-------------------------------------------------------------------------
    event RollFulfilled(
        uint256 indexed requestId,
        address indexed sender,
        bytes32 indexed winType,
        uint256 timestamp,
        VRFRequest vrfRequest
    );
    event RequestRandomness(uint256 requestId);
    event WithdrawPrizes(address indexed winner, uint256 indexed amount, RollState rollState);
    event Receive(address indexed sender, uint256 value);
    event Jackpot();
    event UltraWin();
    event SuperWin();
    event Winning();

    //-------------------------------------------------------------------------
    // CONSTRUCTOR
    //-------------------------------------------------------------------------

    constructor(
        address _mainAirdropToken,
        uint256 _airdropAmount,
        address _vrfCoordinatorV2,
        uint64 _subscriptionId,
        uint32 _callbackGasLimit,
        bytes32 _gasLane
    ) VRFConsumerBaseV2(_vrfCoordinatorV2) {
        rootOwner = owner();
        mainAirdropToken = _mainAirdropToken;
        addAirdropBundle(_mainAirdropToken, _airdropAmount);
        vrfCoordinatorV2 = VRFCoordinatorV2Interface(_vrfCoordinatorV2);
        subscriptionId = _subscriptionId;
        callbackGasLimit = _callbackGasLimit;
        gasLane = _gasLane;
        randomSeedStamp = block.timestamp + 88;
        randomSeed2 = randomSeedStamp + 99;
        randomSeed3 = uint256(keccak256(abi.encodePacked(msg.sender, randomSeed2)));
    }

    receive() external payable {
        emit Receive(msg.sender, msg.value);
    }

    //-------------------------------------------------------------------------
    // MODIFIERS
    //-------------------------------------------------------------------------
    modifier isNotPaused() {
        require(startContract, "Contract is Paused");
        _;
    }

    //-------------------------------------------------------------------------
    // EXTERNAL FUNCTIONS
    //-------------------------------------------------------------------------

    function oneRollERC20Token(
        address _referrerAddress,
        address _tokenAddress, //erc20 address for spin
        uint256 _amount
    ) external payable nonReentrant isNotPaused {
        // incase owner changes
        rootOwner = owner();

        if (!allTokenBundles[_tokenAddress].isAdded) {
            revert SlotMachine__TokenNotAdded();
        }

        if (allTokenBundles[_tokenAddress].isBanned) {
            revert SlotMachine__TokenBanned();
        }

        if (allTokenBundles[_tokenAddress].oneRollPrice > _amount) {
            revert SlotMachine__RollPriceExceedsAmount();
        }

        bool success;

        // need to approve token on ui side for the token contract
        success = IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
        if (!success) {
            revert SlotMachine__TransferFailed();
        }

        // prepare transfer tokens to root owner and referals
        success = IERC20(_tokenAddress).approve(address(rootOwner), _amount);
        if (!success) {
            revert SlotMachine__ApproveFailed();
        }

        if (address(_referrerAddress) != address(0)) {
            IERC20(_tokenAddress).approve(address(_referrerAddress), _amount);
        }

        if (!playersMapping[msg.sender]) {
            playersArray.push(msg.sender);
            playersMapping[msg.sender] = true;
        }

        // pay refferals 2 level
        payReferralERC20(msg.sender, _referrerAddress, _amount, _tokenAddress);
        requestRandomnessOracle(RollState.ERC20Roll, _amount, _tokenAddress);
    }

    // For Gas Token Rolls on Eth or Matic
    function oneRoll(address _referrerAddress, bool _useWinnings) external payable nonReentrant isNotPaused {
        if (_useWinnings) {
            if (coinPrice > pendingWithdrawals[msg.sender]) {
                revert SlotMachine__CoinPriceExceedsValue();
            } else {
                pendingWithdrawals[msg.sender] -= coinPrice;
            }
        } else {
            if (coinPrice > msg.value) {
                revert SlotMachine__CoinPriceExceedsValue();
            }
        }

        if (!playersMapping[msg.sender]) {
            playersArray.push(msg.sender);
            playersMapping[msg.sender] = true;
        }

        // TwoLevelReferrral
        payReferral(msg.sender, _referrerAddress, coinPrice);
        requestRandomnessOracle(RollState.ETHRoll, 0, mainAirdropToken);
    }

    function withdrawEth() external nonReentrant isNotPaused {
        uint256 amount = pendingWithdrawals[msg.sender];
        if (amount <= 0) {
            revert SlotMachine__AmountZero();
        }

        pendingWithdrawals[msg.sender] = 0;

        (bool sent, ) = msg.sender.call{value: amount}("");
        if (!sent) {
            revert SlotMachine__TransferFailed();
        }

        emit WithdrawPrizes(msg.sender, amount, RollState.ETHRoll);
    }

    function withdrawERC20Prizes(address _tokenAddress) external nonReentrant isNotPaused {
        if (!allTokenBundles[_tokenAddress].isAdded) {
            revert SlotMachine__TokenNotAdded();
        }

        if (allTokenBundles[_tokenAddress].isBanned) {
            revert SlotMachine__TokenBanned();
        }

        uint256 amount = pendingERC20Withdrawals[msg.sender][_tokenAddress];
        if (amount <= 0) {
            revert SlotMachine__AmountZero();
        }

        pendingERC20Withdrawals[msg.sender][_tokenAddress] = 0;
        bool success = IERC20(_tokenAddress).transfer(msg.sender, amount);
        if (!success) {
            revert SlotMachine__TokenPaymentFail();
        }
        emit WithdrawPrizes(msg.sender, amount, RollState.ERC20Roll);
    }

    function withdrawAllERC20Prizes(address _tokenAddress) external nonReentrant isNotPaused {
        //todo for loop through pendingERC20Withdrawals
    }

    function depositEthToEarnings() external payable nonReentrant isNotPaused {
        if (coinPrice > msg.value) {
            revert SlotMachine__CoinPriceExceedsValue();
        }
        pendingWithdrawals[msg.sender] += msg.value;
    }

    function depositERC20ToEarnings(address _tokenAddress, uint256 _amount) external nonReentrant isNotPaused {
        if (!allTokenBundles[_tokenAddress].isAdded) {
            revert SlotMachine__TokenNotAdded();
        }

        if (allTokenBundles[_tokenAddress].isBanned) {
            revert SlotMachine__TokenBanned();
        }

        bool success = IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
        if (success) {
            pendingERC20Withdrawals[msg.sender][_tokenAddress] += _amount;
        } else {
            revert SlotMachine__TokenPaymentFail();
        }
    }

    //-------------------------------------------------------------------------
    // INTERNAL FUNCTIONS
    //-------------------------------------------------------------------------

    function calculatePrize(
        uint256 rand1,
        uint256 rand2,
        uint256 rand3,
        uint256 price
    ) internal virtual returns (uint256, bytes32) {
        if (rand1 == 5 && rand2 == 5 && rand3 == 5) {
            return (price * 30, "jackpot");
        }

        if (rand1 == 6 && rand2 == 5 && rand3 == 6) {
            return (price * 20, "ultrawin");
        }

        if (rand1 == 4 && rand2 == 4 && rand3 == 4) {
            return (price * 15, "superwin");
        }

        if (rand1 == 3 && rand2 == 3 && rand3 == 3) {
            return (price * 12, "winning");
        }

        if (rand1 == 2 && rand2 == 2 && rand3 == 2) {
            return (price * 10, "winning");
        }

        if (rand1 == 1 && rand2 == 1 && rand3 == 1) {
            return (price * 5, "winning");
        }

        if ((rand1 == rand2) || (rand1 == rand3) || (rand2 == rand3)) {
            return ((price * 8) / 10, "winning");
        }

        return ((price * smallWin) / smallWinDivider, "");
    }

    function emitWinType(bytes32 winType) internal {
        if (winType == "jackpot") {
            emit Jackpot();
        }

        if (winType == "ultrawin") {
            emit UltraWin();
        }

        if (winType == "superwin") {
            emit SuperWin();
        }

        if (winType == "winning") {
            emit Winning();
        }
    }

    function requestRandomnessOracle(
        RollState _rollState,
        uint256 _amount, // 0 if gas
        address _token // if gas token roll then is airdrop address
    ) internal {
        if (shouldUseVRF) {
            uint256 requestId = vrfCoordinatorV2.requestRandomWords(
                gasLane,
                subscriptionId,
                REQUEST_CONFIRMATIONS,
                callbackGasLimit,
                NUM_WORDS
            );
            vrfRequestsMap[requestId] = VRFRequest(_rollState, msg.sender, _token, requestId, _amount, 0, 0, 0, 0, "");
            requestToPlayersAddress[requestId] = msg.sender;
            requestIdArray.push(requestId);

            emit RequestRandomness(requestId);
        } else {
            requestRandomnessNonOracle(_rollState, _amount, _token);
        }
    }

    function requestRandomnessNonOracle(
        RollState _rollState,
        uint256 _amount,
        address _token
    ) internal {
        uint256[] memory rand = new uint256[](3);
        uint256 counterChange = nonVRFRequestIdCounter.current() + 33;
        rand[0] = uint256(keccak256(abi.encodePacked(msg.sender, counterChange, randomSeedStamp))) + 1;
        rand[1] = uint256(keccak256(abi.encodePacked(rand[0] + 2, counterChange, randomSeedStamp, randomSeed2))) + 3;
        rand[2] = uint256(
            keccak256(abi.encodePacked(msg.sender, rand[1] + 2, counterChange, randomSeedStamp, randomSeed3))
        );

        uint256 requestId = nonVRFRequestIdCounter.current();
        if (vrfRequestsMap[requestId].requestId >= 0) {
            // check if id exists already
            requestId = (nonVRFRequestIdCounter.current() + rand[0]);
        }
        vrfRequestsMap[requestId] = VRFRequest(_rollState, msg.sender, _token, requestId, _amount, 0, 0, 0, 0, "");
        requestToPlayersAddress[requestId] = msg.sender;
        requestIdArray.push(requestId);

        nonVRFRequestIdCounter.increment();

        emit RequestRandomness(requestId);
        implementRandomWords(requestId, rand);
    }

    function randomGenInternal(uint256 _seed) internal virtual returns (uint256 randomNumber) {
        uint256 source = block.difficulty + _seed;
        bytes memory source_b = toBytes(source);
        return (uint256(keccak256(abi.encodePacked(source_b, msg.sender))) % SLOT_ITEMS) + 1;
    }

    function toBytes(uint256 x) internal pure returns (bytes memory b) {
        b = new bytes(32);
        assembly {
            mstore(add(b, 32), x)
        }
    }

    /**
     * @dev Callback function used by VRF Coordinator
     */
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        implementRandomWords(requestId, randomWords);
    }

    //Separate Call back from implementation
    function implementRandomWords(uint256 requestId, uint256[] memory randomWords) internal {
        VRFRequest memory vrfRequest = vrfRequestsMap[requestId];

        if (vrfRequest.playersAddress == address(0)) {
            revert SlotMachine__NoCurrentRolledPlayer();
        }

        uint256 rand1 = randomGenInternal(randomWords[0]);
        uint256 rand2 = randomGenInternal(randomWords[1]);
        uint256 rand3 = randomGenInternal(randomWords[2]);

        if (vrfRequest.rollState == RollState.ERC20Roll) {
            (uint256 reward, bytes32 winType) = calculatePrize(rand1, rand2, rand3, vrfRequest.amount);
            emitWinType(winType);

            if (vrfRequest.playersAddress == address(0)) {
                revert SlotMachine__NoCurrentRolledToken();
            }

            pendingERC20Withdrawals[vrfRequest.playersAddress][vrfRequest.tokenAddress] += reward;

            vrfRequestsMap[requestId].rand1 = rand1;
            vrfRequestsMap[requestId].rand2 = rand2;
            vrfRequestsMap[requestId].rand3 = rand3;
            vrfRequestsMap[requestId].reward = reward;
            vrfRequestsMap[requestId].winType = winType;

            emit RollFulfilled(
                requestId,
                vrfRequest.playersAddress,
                winType,
                block.timestamp,
                vrfRequestsMap[requestId]
            );
        }

        if (vrfRequest.rollState == RollState.ETHRoll) {
            (uint256 reward, bytes32 winType) = calculatePrize(rand1, rand2, rand3, coinPrice);
            emitWinType(winType);

            pendingWithdrawals[vrfRequest.playersAddress] += reward;
            sendAirdropToken(mainAirdropToken, vrfRequest.playersAddress);

            vrfRequestsMap[requestId].rand1 = rand1;
            vrfRequestsMap[requestId].rand2 = rand2;
            vrfRequestsMap[requestId].rand3 = rand3;
            vrfRequestsMap[requestId].reward = reward;
            vrfRequestsMap[requestId].winType = winType;

            emit RollFulfilled(
                requestId,
                vrfRequest.playersAddress,
                winType,
                block.timestamp,
                vrfRequestsMap[requestId]
            );
        }
    }

    //-------------------------------------------------------------------------
    // VIEW FUNCTIONS
    //-------------------------------------------------------------------------

    function getPendingWithdrawals(address _user) external view returns (uint256) {
        return pendingWithdrawals[_user];
    }

    function getPendingERC20Withdrawals(address _user, address _token) external view returns (uint256) {
        return pendingERC20Withdrawals[_user][_token];
    }

    function getRequestIdArray() external view returns (uint256[] memory) {
        return requestIdArray;
    }

    function getVRFRequestInfo(uint256 _requestId) external view returns (VRFRequest memory) {
        return vrfRequestsMap[_requestId];
    }

    function getPlayersAddresses() external view returns (address[] memory) {
        return playersArray;
    }

    //-------------------------------------------------------------------------
    // ONLY OWNER FUNCTIONS
    //-------------------------------------------------------------------------

    function setStartContract(bool _contractState) external onlyOwner {
        startContract = _contractState;
    }

    function setCoinPrice(uint256 _coinPrice) external onlyOwner {
        coinPrice = _coinPrice;
    }

    function cashout(uint256 _amount) external nonReentrant onlyOwner {
        rootOwner = owner();
        (bool sent, ) = rootOwner.call{value: _amount}("");
        if (!sent) {
            revert SlotMachine__TransferFailed();
        }
    }

    function removeAllGas() external nonReentrant onlyOwner {
        rootOwner = owner();
        (bool sent, ) = rootOwner.call{value: address(this).balance}("");
        if (!sent) {
            revert SlotMachine__TransferFailed();
        }
    }

    function setSmallWin(uint8 _amount) external onlyOwner {
        smallWin = _amount;
    }

    function emitWinTypeExternal(bytes32 _winType) external onlyOwner {
        emitWinType(_winType);
    }

    function setChainlinkSubID(uint64 _subscriptionId) external onlyOwner {
        subscriptionId = _subscriptionId;
    }

    function toggleShouldUseVRF(bool _shouldUseVRF) external onlyOwner {
        shouldUseVRF = _shouldUseVRF;
    }

    //-------------------------------------------------------------------------
    // MIGRATION FUNCTIONS (ONLY OWNER)
    //-------------------------------------------------------------------------

    function migrateVRFRequest(address previousContract) external onlyOwner {
        ISlotMachine prevSlotMachineContract = ISlotMachine(previousContract);
        uint256[] memory prevRequestIdArray = prevSlotMachineContract.getRequestIdArray();

        for (uint256 i = 0; i < prevRequestIdArray.length; i++) {
            uint256 prevRequestId = prevRequestIdArray[i];
            VRFRequest memory prevVRFRequest = prevSlotMachineContract.getVRFRequestInfo(prevRequestId);

            vrfRequestsMap[prevRequestId] = prevVRFRequest;
            requestToPlayersAddress[prevRequestId] = prevVRFRequest.playersAddress;
            requestIdArray.push(prevRequestId);
        }
    }

    function migratePendingWithdrawals(address previousContract) external onlyOwner {
        ISlotMachine prevSlotMachineContract = ISlotMachine(previousContract);
        address[] memory prevPlayersArray = prevSlotMachineContract.getPlayersAddresses();

        for (uint256 i = 0; i < prevPlayersArray.length; i++) {
            address prevPlayer = prevPlayersArray[i];
            uint256 balance = prevSlotMachineContract.getPendingWithdrawals(prevPlayer);
            pendingWithdrawals[prevPlayer] = balance;

            if (!playersMapping[prevPlayer]) {
                playersArray.push(prevPlayer);
                playersMapping[prevPlayer] = true;
            }
        }
    }

    function migrateERC20Tokens(address previousContract) external onlyOwner {
        ISlotMachine prevSlotMachineContract = ISlotMachine(previousContract);
        TokenBundle[] memory prevTokensArray = prevSlotMachineContract.getAllTokens();

        for (uint256 i = 0; i < prevTokensArray.length; i++) {
            TokenBundle memory prevToken = prevSlotMachineContract.getTokenByAddress(prevTokensArray[i].tokenAddress);

            if (allTokenBundles[prevToken.tokenAddress].tokenAddress == address(0)) {
                allTokenBundles[prevToken.tokenAddress] = TokenBundle(
                    prevToken.tokenAddress,
                    prevToken.oneRollPrice,
                    prevToken.isBanned,
                    prevToken.isAdded
                );
                allTokenArray.push(prevToken.tokenAddress);
            }
        }
    }

    function migratePendingERC20Withdrawals(address previousContract) external onlyOwner {
        ISlotMachine prevSlotMachineContract = ISlotMachine(previousContract);
        address[] memory prevPlayersArray = prevSlotMachineContract.getPlayersAddresses();
        TokenBundle[] memory tokensArray = _getAllTokens();

        for (uint256 i = 0; i < prevPlayersArray.length; i++) {
            for (uint256 j = 0; j < tokensArray.length; j++) {
                address prevPlayer = prevPlayersArray[i];
                TokenBundle memory prevToken = tokensArray[j];

                uint256 balance = prevSlotMachineContract.getPendingERC20Withdrawals(
                    prevPlayer,
                    prevToken.tokenAddress
                );

                if (balance > 0) {
                    pendingERC20Withdrawals[prevPlayer][prevToken.tokenAddress] = balance;
                    if (!playersMapping[prevPlayer]) {
                        playersArray.push(prevPlayer);
                        playersMapping[prevPlayer] = true;
                    }
                }
            }
        }
    }

    function migrateReferrals(address previousContract) external onlyOwner {
        ISlotMachine prevSlotMachineContract = ISlotMachine(previousContract);
        address[] memory referralKeys = prevSlotMachineContract.getAllReferralKeys();

        for (uint256 i = 0; i < referralKeys.length; i++) {
            address referralKey = referralKeys[i];
            referralMap[referralKey] = prevSlotMachineContract.referralMap(referralKey);
            allReferralKeys.push(referralKey);
        }
    }
}

