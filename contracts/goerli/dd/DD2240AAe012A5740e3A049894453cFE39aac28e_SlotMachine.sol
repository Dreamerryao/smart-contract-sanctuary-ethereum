pragma solidity ^0.8.9;

import "./TwoLevelReferral.sol";

// Heirarchy is TwoLevelReferral.sol > AirDropControl > AddSlotTokenERC20  > SlotMachine
contract AirdropControl is TwoLevelReferral {
    address public mainAirdropToken;
    event AirdropSent(address tokenContract);

    struct AirdropBundle {
        address tokenAddress;
        uint256 airdropAmount;
        bool isBanned; // false
        bool isAdded; // default true
    }

    // public list of all tokens
    address[] public allAirdropArray;
    // mapping to get token info right away
    mapping(address => AirdropBundle) public allAirdropBundles;

    function sendAirdropToken(address _tokenAddress, address _reciever) internal {
        // todo: let others airdrop their tokens per spin
        require(!allAirdropBundles[_tokenAddress].isBanned, "Address banned");
        IERC20(_tokenAddress).transfer(_reciever, allAirdropBundles[_tokenAddress].airdropAmount);
        emit AirdropSent(_tokenAddress);
    }

    function setMainAirdropToken(address _tokenAddress) external nonReentrant onlyOwner {
        mainAirdropToken = _tokenAddress;
    }

    function addAirdropBundle(address _tokenAddress, uint256 _amount) public onlyOwner {
        // if not added then update the array, else just update
        if (!allAirdropBundles[_tokenAddress].isAdded) {
            allAirdropArray.push(_tokenAddress);
        }

        allAirdropBundles[_tokenAddress] = AirdropBundle(_tokenAddress, _amount, false, true);
    }
}

