import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

error TwoLevelReferral__TransferFailed();
error TwoLevelReferral__InvalidDenomination();

contract TwoLevelReferral is Ownable, ReentrancyGuard {
    mapping(address => address) public referralMap;
    address[] public allReferralKeys;

    enum ReferralLevel {
        OneLevel,
        TwoLevel
    }

    enum RollType {
        ETHRoll,
        ERC20Roll
    }

    // Adjustable by rootOwner of contract
    uint256 public firstLevelPercentage = 20;
    uint256 public secondLevelPercentage = 10;
    uint256 public rootOwnerPercentage = 50;

    event Referral(
        address indexed depositor,
        address indexed referrer,
        uint256 indexed firstLevelAmount,
        address twoLevelReferrer,
        uint256 secondLevelAmount,
        ReferralLevel level,
        RollType rollType
    );

    function payReferral(
        address _depositor,
        address _referrerAddress,
        uint256 _denomination
    ) internal {
        address rootOwner = owner();
        uint256 firstLevelAmount = (_denomination * firstLevelPercentage) / 100;
        uint256 secondLevelAmount = (_denomination * secondLevelPercentage) / 100;
        uint256 rootOwnerAmount = (_denomination * rootOwnerPercentage) / 100;

        if (_denomination == 0) {
            revert TwoLevelReferral__InvalidDenomination();
        }

        bool success;

        if (_referrerAddress != address(0)) {
            referralMap[_depositor] = _referrerAddress;
            allReferralKeys.push(_depositor);

            // Second Level Exists
            if (referralMap[_referrerAddress] != address(0)) {
                // Send firstLevelPercentage % to the refferer
                (success, ) = _referrerAddress.call{value: firstLevelAmount}("");
                if (!success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send secondLevelPercentage to the second level refferer
                address secondLevelReferrer = referralMap[_referrerAddress];
                (success, ) = secondLevelReferrer.call{value: secondLevelAmount}("");
                if (!success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send 0.4% to the root owner
                (success, ) = rootOwner.call{value: rootOwnerAmount}("");

                // Leave only 20% for rewards on the contract
                if (!success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                emit Referral(
                    _depositor,
                    _referrerAddress,
                    firstLevelAmount,
                    secondLevelReferrer,
                    secondLevelAmount,
                    ReferralLevel.TwoLevel,
                    RollType.ETHRoll
                );
            } else {
                // No second Level
                // Send 0.5% to the refferer
                (success, ) = _referrerAddress.call{value: firstLevelAmount}("");
                if (!success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send root owner
                (success, ) = rootOwner.call{value: rootOwnerAmount}("");
                if (!success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                emit Referral(
                    _depositor,
                    _referrerAddress,
                    firstLevelAmount,
                    address(0),
                    0,
                    ReferralLevel.OneLevel,
                    RollType.ETHRoll
                );
            }
        } else {
            // Send rest% to the root owner
            (success, ) = rootOwner.call{value: rootOwnerAmount}("");
            if (!success) {
                revert TwoLevelReferral__TransferFailed();
            }
        }
    }

    // ERC20 Referrals
    function payReferralERC20(
        address _depositor,
        address _referrerAddress,
        uint256 _denomination,
        address _tokenAddress
    ) internal {
        address rootOwner = owner();

        IERC20 slotToken = IERC20(_tokenAddress);
        uint256 firstLevelAmount = (_denomination * firstLevelPercentage) / 100;
        uint256 secondLevelAmount = (_denomination * secondLevelPercentage) / 100;
        uint256 rootOwnerAmount = (_denomination * rootOwnerPercentage) / 100;

        if (_denomination == 0) {
            revert TwoLevelReferral__InvalidDenomination();
        }

        bool erc20success;

        if (_referrerAddress != address(0)) {
            referralMap[_depositor] = _referrerAddress;
            allReferralKeys.push(_depositor);

            // Second Level Exists
            if (referralMap[_referrerAddress] != address(0)) {
                // Send firstLevelPercentage % to the refferer
                erc20success = slotToken.transfer(_referrerAddress, firstLevelAmount);
                if (!erc20success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send secondLevelPercentage to the second level refferer
                address secondLevelReferrer = referralMap[_referrerAddress];
                erc20success = slotToken.transfer(secondLevelReferrer, secondLevelAmount);
                if (!erc20success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send to the root owner
                erc20success = slotToken.transfer(rootOwner, rootOwnerAmount);
                // Leave only 20% for rewards on the contract
                if (!erc20success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                emit Referral(
                    _depositor,
                    _referrerAddress,
                    firstLevelAmount,
                    secondLevelReferrer,
                    secondLevelAmount,
                    ReferralLevel.TwoLevel,
                    RollType.ERC20Roll
                );
            } else {
                // No second Level
                // Send 0.5% to the refferer
                erc20success = slotToken.transfer(_referrerAddress, firstLevelAmount);
                if (!erc20success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                // Send root owner
                erc20success = slotToken.transfer(rootOwner, rootOwnerAmount);
                if (!erc20success) {
                    revert TwoLevelReferral__TransferFailed();
                }

                emit Referral(
                    _depositor,
                    _referrerAddress,
                    firstLevelAmount,
                    address(0),
                    0,
                    ReferralLevel.OneLevel,
                    RollType.ERC20Roll
                );
            }
        } else {
            // Send rest% to the root owner
            erc20success = slotToken.transfer(rootOwner, rootOwnerAmount);
            if (!erc20success) {
                revert TwoLevelReferral__TransferFailed();
            }
        }
    }

    function getAllReferralKeys() external view returns (address[] memory) {
        return allReferralKeys;
    }

    // rootOwner functions
    function updateFirstLevelReferral(uint256 _percentage) external nonReentrant onlyOwner {
        firstLevelPercentage = _percentage;
    }

    function updateSecondLevelReferral(uint256 _percentage) external nonReentrant onlyOwner {
        secondLevelPercentage = _percentage;
    }
}

