pragma solidity ^0.8.9;




error NoResourceAtId();
error InvalidZKProof();
error BelowMinimumPrice();
error SendFailed();
error IndexOutOfBounds();
error InvalidStringLength();
error OnlyOwner();

contract Token is ERC721 {
    
    uint256 public constant MAX_VALUE = 21888242871839275222246405745257275088548364400416034343698204186575808495617;
    uint public constant BASE = 4;
    uint public constant FACTOR = 1024;
    uint public constant RESOURCE_COUNT = 9;
    uint public constant HASH_KEY = 5;

    string[] private resourceNames = [
        "Alpha",
        "Beta",
        "Gamma",
        "Delta",
        "Epsilon",
        "Zeta",
        "Eta",
        "Theta",
        "Iota"
    ];

    // Minimum user must send to claim a token
    uint256 public minimumPrice = 0.0 ether;
    // Current beneficiary of all donations
    address public beneficiary;
    // Current owner of contract who can change price and beneficiary
    address public owner;
    // uint256 private _tokenCounter = 0;
    uint256[] private _allTokens;
    // used for string manipulation
    bytes16 private constant _SYMBOLS = "0123456789abcdef";

    event Claim(address indexed to, uint256 indexed hash, uint8 indexed resource);

    mapping(uint8 => uint256) private _resourceSupply;

    constructor(address _beneficiary, uint256 _minimumPrice) ERC721("Token", "ZKT") {
        owner = msg.sender;
        beneficiary = _beneficiary;
        minimumPrice = _minimumPrice;
    }

    // when the contract is frozen, the owner revokes control and the minimum cost is set to zero
    // this immortalizes the contract and prevents any future changes
    function lock () public {
        if (msg.sender != owner) revert OnlyOwner();
        owner = address(0);
        beneficiary = address(0);
        minimumPrice = 0;
    }

    function setBeneficiary(address _beneficiary) public {
        if (msg.sender != owner) revert OnlyOwner();
        beneficiary = _beneficiary;
    }

    function setMinimumPrice(uint _minimumPrice) public {
        if (msg.sender != owner) revert OnlyOwner();
        minimumPrice = _minimumPrice;
    }

    function setOwner(address _owner) public {
        if (msg.sender != owner) revert OnlyOwner();
        owner = _owner;
    }

    function resourceAt(uint256 tokenId) public pure returns(int) {
        for (uint i = RESOURCE_COUNT; i >= 0; i--) {
            if (i <= 0) return -1;
            uint exp = i - 1;
            uint rarity = FACTOR * (BASE ** exp);
            if (tokenId < MAX_VALUE / rarity) {
                return int(exp);
            }
        }
        return -1;
    }

    function exists (uint256 tokenId) public view returns (bool) {
        return _exists(tokenId);
    }

    function totalSupply() public view returns (uint256) {
        return _allTokens.length;
    }

    function tokenByIndex(uint256 index) public view returns (uint256) {
        if (index >= _allTokens.length) revert IndexOutOfBounds();
        return _allTokens[index];
    }

    function resourceSupplyOf (uint8 resource) public view returns(uint256) {
        return _resourceSupply[resource];
    }

    function claim (address to, uint256 tokenId, bytes memory proof) public payable {
        // If a min price is set
        if (minimumPrice > 0) {
            // Ensure user is paying correct amount
            if (msg.value < minimumPrice) revert BelowMinimumPrice();
            // Send to beneficiary
            (bool success, ) = beneficiary.call{value: msg.value}("");
            if (!success) revert SendFailed();
        }

        // ensure a resource is present here
        int r = resourceAt(tokenId);
        if (r < 0) revert NoResourceAtId();

        // handle zk proof verification
        // to ensure the user knows the (x, y) coordinates that leads to this hash
        // and to ensure the (x, y) coordinates are within N bits
        // and also to ensure that (address) given in transaction matches the one
        // used to build the proof 
        uint[] memory pubSignals = new uint[](3);
        pubSignals[0] = tokenId;
        pubSignals[1] = HASH_KEY;
        pubSignals[2] = uint256(uint160(to));
        if (!PlonkVerifier.verifyProof(proof, pubSignals)) revert InvalidZKProof();

        // mint a non-fungible token, ensuring this resource can only be claimed once
        _safeMint(to, tokenId);

        // increment resource supply
        uint8 idx = uint8(uint256(r));
        _resourceSupply[idx] += 1;

        // increment all token counter for enumeration
        _allTokens.push(tokenId);

        emit Claim(to, tokenId, idx);
    }

    function toHexString (uint256 value) private pure returns (string memory) {
        uint256 valLen = Math.log256(value) + 1;
        uint valBufferLen = 2 * valLen + 2;
        bytes memory valBuffer = new bytes(valBufferLen);
        valBuffer[0] = "0";
        valBuffer[1] = "x";
        for (uint256 i = 2 * valLen + 1; i > 1; --i) {
            valBuffer[i] = _SYMBOLS[value & 0xf];
            value >>= 4;
        }
        if (value != 0) revert InvalidStringLength();
        return string(valBuffer);
    }

    function tokenIdToHash(uint256 tokenId) public view returns (string memory) {
        _requireMinted(tokenId);
        return toHexString(tokenId);
    }

    function _metaData (uint256 tokenId) private view returns (
            string memory,
            string memory,
            string memory
    ) {
        _requireMinted(tokenId);

        int r = resourceAt(tokenId);
        if (r < 0) revert NoResourceAtId();
        uint8 resourceIndex = uint8(uint256(r));

        uint256 tokenIdLength = Math.log256(tokenId) + 1;

        // long hash
        uint tokenIdBufferLen = 2 * tokenIdLength + 2;
        bytes memory tokenIdBuffer = new bytes(tokenIdBufferLen);
        uint256 _tmpTokenValue = tokenId;
        tokenIdBuffer[0] = "0";
        tokenIdBuffer[1] = "x";
        for (uint256 i = 2 * tokenIdLength + 1; i > 1; --i) {
            tokenIdBuffer[i] = _SYMBOLS[_tmpTokenValue & 0xf];
            _tmpTokenValue >>= 4;
        }
        if (_tmpTokenValue != 0) revert InvalidStringLength();
        string memory tokenIdStr = string(tokenIdBuffer);
        
        // short hash (0xA8...dA05)
        bytes memory tokenIdShortBufferA = new bytes(4);
        bytes memory tokenIdShortBufferB = new bytes(4);
        for (uint i = 0; i < 4; i++) tokenIdShortBufferA[i] = tokenIdBuffer[i];
        for (uint i = 0; i < 4; i++) tokenIdShortBufferB[i] = tokenIdBuffer[tokenIdBufferLen - 4 + i];

        string memory resourceName = resourceNames[resourceIndex];
        string memory resourceHexStr = toHexString(resourceIndex);
        string memory name = string(abi.encodePacked(
            resourceName,
            " (",
            string(abi.encodePacked(tokenIdShortBufferA, "...", tokenIdShortBufferB)), // short string
            ")"
        ));
        return (
            tokenIdStr,
            name,
            resourceHexStr
        );
    }

    function _buildSVG (string memory name) private pure returns (string memory) {
        string memory imagePrefix = 'data:image/svg+xml;base64,';
        string memory image = Base64.encode(bytes(
            string(
                abi.encodePacked(
                    '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style><rect width="100%" height="100%" fill="black" /><text x="10" y="20" class="base">',
                    name,
                    '</text></svg>'
                )
            )
        ));
        return string(abi.encodePacked(
            imagePrefix,
            image
        ));
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        // build JSON
        // string memory name = string(abi.encodePacked(
        //     resourceName,
        //     " (",
        //     string(abi.encodePacked(tokenIdShortBufferA, "...", tokenIdShortBufferB)), // short string
        //     ")"
        // ));
        (string memory tokenIdStr, string memory name, ) = _metaData(tokenId);
        string memory description = string(abi.encodePacked("Hash: ", tokenIdStr));
        // string memory imagePrefix = 'data:text/plain;charset=utf-8,';
        // string memory image = string(abi.encodePacked('zk:', resourceHexStr, ':', tokenIdStr));
        // string memory imagePrefix = 'data:image/svg+xml;base64,';
        // string memory image = Base64.encode(bytes(
        //     string(
        //         abi.encodePacked(
        //             '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style><rect width="100%" height="100%" fill="black" /><text x="10" y="20" class="base">',
        //             resourceName,
        //             '</text><text x="10" y="40" class="base">',
        //             tokenIdShortStr,
        //             '</text></svg>'
        //         )
        //     )
        // ));

        return
            string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                name,
                                '", "description":"',
                                description,
                                '", "image": "',
                                _buildSVG(name),
                                // 'data:text/plain;charset=utf-8,',
                                // string(abi.encodePacked('zk:', resourceHexStr, ':', tokenIdStr)),
                                '"}'
                            )
                        )
                    )
                )
            );
    }
}