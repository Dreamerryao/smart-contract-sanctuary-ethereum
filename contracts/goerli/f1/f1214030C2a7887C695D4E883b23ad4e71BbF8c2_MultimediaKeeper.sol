pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

import "../interfaces/INFT.sol";
import "../utils/Parser.sol";
import "./MultimediaSchema.sol";

library MultimediaMeta {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.multimedia.nft.storage");
  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly { ds.slot := position }
  }


  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙           PARSING HELPERS           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function getTokenIDs(uint256 tokenId) private pure returns (uint256 nftId, uint256 canvasId) {
    nftId = tokenId;
    canvasId = 0;
    if (tokenId > 1000000000000) {
      nftId = Parser.safeParseInt(Parser.substring(Parser.uint2str(tokenId), bytes(Parser.uint2str(tokenId)).length.sub(9), bytes(Parser.uint2str(tokenId)).length));
      canvasId = tokenId.sub(nftId).div(1000000000000);
    }

    return (nftId, canvasId);
  }

  function toBase64(string memory text) private pure returns (string memory) {
    return string(abi.encodePacked(Base64.encode(bytes(text))));
  }


  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙         TOKEN CUSTOMIZATION         ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function checkValidLicense(
    uint256 tokenId,
    MultimediaSchema.TokenRef memory ref
  ) private view returns (
    bool,
    uint256,
    string memory,
    string memory
  ) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    (bool success, bytes memory data) = ds.registryAddress.staticcall(
      abi.encodeWithSignature(('validLicense(address,uint256,address,uint256)'),
      ds.multimediaAddress, tokenId, ref.tokenAddress, ref.tokenId
    ));

    require(success, 'Registry call error');

    (bool isValid, uint256 licenseId, string memory tokenUri, string memory contentType) = abi.decode(data, (bool,uint256,string,string));
    return (isValid, licenseId, tokenUri, contentType);
  }

  function customizeToken(address msgSender, address owner, uint256 tokenId, string calldata text, string calldata textType, MultimediaSchema.TokenRef[] calldata refs) external {
    require (owner == msgSender, 'No permission');
    MultimediaSchema.Storage storage ds = multimediaStorage();

    if (keccak256(bytes(text)) != keccak256(bytes(''))) {
      if (keccak256(bytes(textType)) == keccak256(bytes('title'))) ds.tokenTitle[tokenId] = text;
      else if (keccak256(bytes(textType)) == keccak256(bytes('subtitle'))) ds.tokenSubtitle[tokenId] = text;
    }

    if (refs.length > 0) {
      setTokenRefs(msgSender, owner, tokenId, refs);
    }
  }

  function setTokenRefs(address msgSender, address owner, uint256 tokenId, MultimediaSchema.TokenRef[] calldata refs) private {
    require (owner == msgSender, 'No permission');
    MultimediaSchema.Storage storage ds = multimediaStorage();

    (,uint256 canvasId) = getTokenIDs(tokenId);
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    delete ds.refs[tokenId];

    for (uint256 i = 0; i < refs.length; i++) {
      if (ds.canvasData[canvasId].restrictRefAddress) require(ds.refAddresses[canvasId][refs[i].tokenAddress], 'Restricted token');

      if (canvas.requireLicense) {
        (bool isValid,,,string memory contentType) = checkValidLicense(tokenId, refs[i]);
        require(isValid, 'Invalid license');

        ds.refs[tokenId].push(MultimediaSchema.TokenRef({
          tokenAddress: refs[i].tokenAddress,
          tokenId: refs[i].tokenId,
          contentType: contentType
        }));
      } else {
        if (canvas.requireOwnership) require(INFT(refs[i].tokenAddress).ownerOf(refs[i].tokenId) == msgSender, 'No permission');
        ds.refs[tokenId].push(MultimediaSchema.TokenRef({
          tokenAddress: refs[i].tokenAddress,
          tokenId: refs[i].tokenId,
          contentType: refs[i].contentType
        }));
      }
    }
  }




  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙     TOKEN URI PROPERTIES PARSING    ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function getCustomTitles(uint256 tokenId) private view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    string memory params = '';
    if (keccak256(bytes(ds.tokenTitle[tokenId])) != keccak256(bytes('')))
      params = string(abi.encodePacked(params,'&n=',toBase64(ds.tokenTitle[tokenId])));

    if (keccak256(bytes(ds.tokenSubtitle[tokenId])) != keccak256(bytes('')))
      params = string(abi.encodePacked(params,'&s=',toBase64(ds.tokenSubtitle[tokenId])));

    return params;
  }

  function getRefUri(uint256 tokenId) private view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    (,uint256 canvasId) = getTokenIDs(tokenId);
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    string[] memory uris = new string[](5);
    uris[0] = '?a=[';
    uris[1] = '&r=[';
    uris[2] = '&t=[';
    uris[3] = '&i=[';
    uris[4] = '&f=[';

    bool[] memory found = new bool[](5);

    for (uint256 i = 0; i < ds.refs[tokenId].length; i++) {
      bool isValid = true;
      string memory uri;
      string memory contentType;

      if (canvas.requireLicense)
        (isValid,,uri,contentType) = checkValidLicense(tokenId, ds.refs[tokenId][i]);
      else {
        uri = INFT(ds.refs[tokenId][i].tokenAddress).tokenURI(ds.refs[tokenId][i].tokenId);
        contentType = ds.refs[tokenId][i].contentType;
      }

      // Validate token reference is not restricted, if enabled
      if (ds.canvasData[canvasId].restrictRefOverride &&
          !ds.refAddresses[canvasId][ds.refs[tokenId][i].tokenAddress]
      ) isValid = false;

      // Validate ownership of token reference, if enabled
      if (ds.canvasData[canvasId].requireOwnershipOverride &&
          INFT(ds.refs[tokenId][i].tokenAddress).ownerOf(ds.refs[tokenId][i].tokenId) !=
          INFT(ds.multimediaAddress).ownerOf(tokenId)
      ) isValid = false;

      if (isValid) {
        if (keccak256(bytes(contentType)) == keccak256(bytes('audio'))) {
          uris[0] = string(abi.encodePacked(uris[0], "'", toBase64(uri), "',"));
          found[0] = true;
        } else if (keccak256(bytes(contentType)) == keccak256(bytes('javascript')) ||
                   keccak256(bytes(contentType)) == keccak256(bytes('html'))) {
          uris[1] = string(abi.encodePacked(uris[1], "'", toBase64(uri), "',"));
          found[1] = true;
        } else if (keccak256(bytes(contentType)) == keccak256(bytes('text'))) {
          uris[2] = string(abi.encodePacked(uris[2], "'", toBase64(uri), "',"));
          found[2] = true;
        } else if (keccak256(bytes(contentType)) == keccak256(bytes('image')) ||
                   keccak256(bytes(contentType)) == keccak256(bytes('video')) ||
                   keccak256(bytes(contentType)) == keccak256(bytes('gif')) ||
                   keccak256(bytes(contentType)) == keccak256(bytes('svg')) ||
                   keccak256(bytes(contentType)) == keccak256(bytes('model'))) {
          uris[3] = string(abi.encodePacked(uris[3], "'", toBase64(uri), "',"));
          found[3] = true;
        } else if (keccak256(bytes(contentType)) == keccak256(bytes('file'))) {
          uris[4] = string(abi.encodePacked(uris[4], "'", toBase64(uri), "',"));
          found[4] = true;
        }
      }
    }

    for (uint i = 0; i <= 4; i++) {
      uris[i] = found[i] ? Parser.substring(uris[i], 0, bytes(uris[i]).length.sub(1)) : uris[i];
    }

    string memory refUri = '';
    return refUri = string(abi.encodePacked(uris[0], ']', uris[1], ']', uris[2], ']', uris[3], ']', uris[4], ']'));
  }


  /* 
   ⁙  Takes chance distribution and sums each chance into a percentage value array. The last element equals the full sum.
   ⁙
   ⁙   INPUT: [
   ⁙     [100, 200, 300, 500, 1000],
   ⁙     [5000, 5000, 5000, 10000]
   ⁙   ]
   ⁙
   ⁙   OUTPUT: [
   ⁙     [100, 300, 600, 1100, 2100],
   ⁙     [5000, 10000, 15000, 25000] 
   ⁙   ]
   ⁙
   ⁙   Random number % 2100 can now step through array to determine which element it has picked.
   */
  function getValueChances(uint16[][] memory chances) private pure returns (uint16[][] memory percentChances) {
    uint16[][] memory valueChances = new uint16[][](chances.length);
    for (uint i = 0; i < chances.length; i++) {
        uint16 propertySum = 0;
        uint16[] memory propertyChances = new uint16[](chances[i].length);
        for (uint j = 0; j < chances[i].length; j++) {
            propertySum += chances[i][j];
            propertyChances[j] = propertySum;
        }
        valueChances[i] = propertyChances;
    }

    return valueChances;
  }

  function getStaticTraits(uint256 canvasId) private view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.CanvasTraits storage canvas = multimediaStorage().canvasRequestTraits[ds.canvas[canvasId]];

    string memory encodedTraits = '';
    for (uint256 i = 0; i < canvas.traits.length; i ++) {
      encodedTraits = string(abi.encodePacked(
        encodedTraits,
        '{"trait_type":"',
        canvas.traits[i],
        '", "value":"',
        canvas.values[0][i],
        '"},')
      );
    }

    return Parser.substring(encodedTraits, 0, bytes(encodedTraits).length.sub(1));
  }

  function getRandomTraits(uint256 canvasId, uint256 nftId, uint256 vrfResult) private view returns (string memory, string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.CanvasTraits storage canvas = multimediaStorage().canvasRequestTraits[ds.canvas[canvasId]];

    bool hasTrait = false;
    string memory encodedTraits = '';
    string memory specialParams = '';
    uint256 entropy = 1;
    uint16[][] memory valuesChance = getValueChances(canvas.valuesChance);
    for (uint i = 0; i < canvas.traits.length; i++) {
      // If trait will exist or not
      if (uint(keccak256(abi.encodePacked(
          vrfResult, nftId, entropy
      ))) % 10000 < canvas.traitsChance[i]) {
        uint randomNumber = uint(keccak256(abi.encodePacked(
          vrfResult, nftId, entropy
        ))) % valuesChance[i][valuesChance[i].length - 1];

        for (uint j = 0; j < canvas.values[i].length; j++) {
          entropy = entropy.add(1);

          // Find random number in percentage selection range
          if (randomNumber <= valuesChance[i][j] && (j == 0 || randomNumber > valuesChance[i][j - 1])) {
            encodedTraits = string(abi.encodePacked(
              encodedTraits,
              '{"trait_type":"',
              canvas.traits[i],
              '", "value":"',
              canvas.values[i][j],
              '"},')
            );

            if (keccak256(bytes(canvas.urlParams[i])) != keccak256(bytes('')))
              specialParams = string(abi.encodePacked(specialParams,'&',canvas.urlParams[i],'=',Parser.uint2str(j)));

            hasTrait = true;
            break;
          }
        }
      }

      entropy = entropy.add(1);
    }

    encodedTraits = hasTrait ? 
      Parser.substring(encodedTraits, 0, bytes(encodedTraits).length.sub(1)) :
      encodedTraits;
    return (encodedTraits, specialParams);
  }

  function usageDescription(uint256 canvasId) private view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    string memory description = canvas.description;
    if (ds.canvasData[canvasId].requireOwnershipOverride) {
      description = string(abi.encodePacked(
        description,
        '\\n\\nReferenced & embedded tokens within this asset are owned by this asset owner.'
      ));
    } else if (canvas.requireOwnership) {
      description = string(abi.encodePacked(
        description,
        '\\n\\nReferenced & embedded tokens within this asset are either owned by this asset owner OR a past owner of this asset.'
      ));
    } else {
      description = string(abi.encodePacked(
        description,
        '\\n\\nReferenced & embedded tokens within this asset are NOT reflective of ownership over the embedded tokens.'
      ));
    }

    description = string(abi.encodePacked(
      description,
      '\\nPlease refresh the metadata to ensure you are seeing the most current references.'
    ));

    return description;
  }

  function tokenURI(uint256 tokenId) external view returns (string memory) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    (uint256 nftId, uint256 canvasId) = getTokenIDs(tokenId);
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    require (nftId < ds.canvasData[canvasId].tokenIdCounter, 'Token does not exist');

    string memory refUri = getRefUri(tokenId);

    // Randomized traits
    string memory encodedTraits = '';
    string memory specialParams = '';
    if (canvas.randomizeTraits) {
      if (nftId < ds.canvasData[canvasId].randomizedTraitsCounter) {
        bool found = false;
        uint256 findIndex = nftId;
        while (!found) {
          if (ds.randomizedTraits[findIndex] > 0) {
            (encodedTraits, specialParams) = getRandomTraits(
              canvasId,
              nftId,
              ds.randomizedTraits[findIndex]
            );
            found = true;
          } else {
            findIndex = findIndex.sub(1);
          }
        }
      } else {
        encodedTraits = string(abi.encodePacked(
          '{"trait_type":"',
          "Multimedia",
          '", "value":"',
          "Pending randomization",
          '"}'));
      }
    } else encodedTraits = getStaticTraits(canvasId);


    string memory nftIdString = Parser.uint2str(nftId);
    string memory tokenName = string(abi.encodePacked(
      canvas.name,
      canvas.editioned ? ' #' : '',
      canvas.editioned ? nftIdString : ''));

    string memory titleParams = getCustomTitles(tokenId);

    return string(abi.encodePacked(
      "data:application/json;base64,",
      Base64.encode(bytes(abi.encodePacked(
        '{"name":"', tokenName,
        '", "description":"', usageDescription(canvasId),
        '","image":"',canvas.thumbnailUri,
        '","animation_url":"',
          canvas.baseUri,
          refUri,
          specialParams,
          titleParams,
          '&id=',
          nftIdString,
        '","attributes": [', encodedTraits,
        '] }'
      ))))
    );
  }
}

