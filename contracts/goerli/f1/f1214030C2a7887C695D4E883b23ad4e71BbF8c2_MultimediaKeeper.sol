pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

import "../utils/Parser.sol";
import "./MultimediaSchema.sol";

interface ERC677 {
    function transferAndCall(address receiver, uint amount, bytes calldata data) external returns (bool success);
    function balanceOf(address user) external view returns(uint);
    function transfer(address _to, uint _amount) external;
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
}

interface KeepersRegistry {
    function getRegistrar() external view returns(address);
}

interface MultimediaVRFInterface {
  function createNewSubscription(uint256 canvasId) external returns (uint64);
  function topUpSubscription(uint256 canvasId, uint256 amount) external;
  function cancelSubscription(uint256 canvasId) external;
}

library MultimediaCanvas {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.multimedia.nft.storage");
  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly { ds.slot := position }
  }



  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙            DAO FUNCTIONS            ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function approveCanvas(address msgSender, uint256 canvasRequestId) external returns (uint256 canvasCounter) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msgSender == ds.curatorAddress, "No permission");

    ds.canvas[ds.canvasCounter] = canvasRequestId;
    MultimediaSchema.CanvasData storage canvasData = ds.canvasData[ds.canvasCounter];
    canvasData.creators = [ds.canvasRequest[canvasRequestId].admin];
    canvasData.tokenIdCounter = 1;
    canvasData.randomizedTraitsCounter = 1;
    canvasData.vrfMinuteInterval = uint16(5);

    ds.canvasCounter = ds.canvasCounter.add(1);
    return ds.canvasCounter.sub(1);
  }



  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙          CANVAS FUNCTIONS           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function createCanvas(MultimediaSchema.Canvas memory canvas, MultimediaSchema.CanvasTraits calldata canvasTraits) external returns (uint256 canvasCounter) {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    // TODO: full trait length validation
    if (canvas.randomizeTraits) {
      require(canvasTraits.traits.length == canvasTraits.traitsChance.length, 'Trait mismatch');
      require(canvasTraits.values.length == canvasTraits.valuesChance.length, 'Trait mismatch');
    } else {
      require(canvasTraits.traits.length == canvasTraits.values[0].length, 'Trait mismatch');
    }

    ds.canvasRequest[ds.canvasRequestCounter] = canvas;
    ds.canvasRequestTraits[ds.canvasRequestCounter] = canvasTraits;

    ds.canvasRequestCounter = ds.canvasRequestCounter.add(1);
    return ds.canvasRequestCounter.sub(1);
  }

  // Two step admin transfer controls
  function changeAdmin(
    address msgSender,
    uint256 canvasId,
    address newAdminRequest, // unused when accepting admin
    bool isAccepting
  ) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    if (isAccepting) {
      require(msgSender == ds.canvasData[canvasId].newAdminRequest, "No permission");
      canvas.admin = ds.canvasData[canvasId].newAdminRequest;
    } else {
      require(msgSender == ds.daoAddress || msgSender == canvas.admin, "No permission");
      ds.canvasData[canvasId].newAdminRequest = newAdminRequest;
    }
  }

  function updateCanvas(
    address msgSender,
    uint256 canvasOrRequestId,
    bool isRequest,
    string memory parameter,
    address[] calldata addressVal,
    string memory stringVal,
    uint256 numberVal,
    bytes32 bytesVal,
    bool boolVal
  ) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas;

    if (isRequest) canvas = ds.canvasRequest[canvasOrRequestId];
    else canvas = ds.canvasRequest[ds.canvas[canvasOrRequestId]];

    require(msgSender == canvas.admin, "No permission");

    bytes32 parameterChange = keccak256(bytes(parameter));
    if (parameterChange == keccak256(bytes('name'))) {
      canvas.name = stringVal;
    } else if (parameterChange == keccak256(bytes('description'))) {
      canvas.description = stringVal;
    } else if (parameterChange == keccak256(bytes('thumbnailUri'))) {
      canvas.thumbnailUri = stringVal;
    } else if (parameterChange == keccak256(bytes('baseUri'))) {
      canvas.thumbnailUri = stringVal;
    } else if (parameterChange == keccak256(bytes('feeRecipient'))) {
      canvas.feeRecipient = addressVal[0];
    } else if (parameterChange == keccak256(bytes('feeBps'))) {
      canvas.feeBps = uint16(numberVal);
    } else if (parameterChange == keccak256(bytes('saleToken'))) {
      canvas.saleToken = addressVal[0];
    } else if (parameterChange == keccak256(bytes('salePrice'))) {
      canvas.salePrice = numberVal;
    } else if (parameterChange == keccak256(bytes('saleEndPrice'))) {
      canvas.saleEndPrice = numberVal;
    } else if (parameterChange == keccak256(bytes('totalQuantity'))) {
      canvas.totalQuantity = numberVal;
    } else if (parameterChange == keccak256(bytes('editioned'))) {
      canvas.editioned = boolVal;
    } else if (parameterChange == keccak256(bytes('requireOwnership'))) {
      canvas.requireOwnership = boolVal;
    } else if (parameterChange == keccak256(bytes('requireLicense'))) {
      canvas.requireLicense = boolVal;
    } else if (!isRequest) {
      if (parameterChange == keccak256(bytes('creators'))) {
        ds.canvasData[canvasOrRequestId].creators = addressVal;
      } else if (parameterChange == keccak256(bytes('merkleRoot'))) {
        ds.canvasData[canvasOrRequestId].merkleRoot = bytesVal;
      } else if (parameterChange == keccak256(bytes('restrictToAllowList'))) {
        ds.canvasData[canvasOrRequestId].restrictToAllowList = boolVal;
      } else if (parameterChange == keccak256(bytes('vrfMinuteInterval'))) {
        ds.canvasData[canvasOrRequestId].vrfMinuteInterval = uint16(numberVal);
      } else if (parameterChange == keccak256(bytes('saleActive'))) {
        ds.canvasData[canvasOrRequestId].saleActive = boolVal;
      } else if (parameterChange == keccak256(bytes('presaleActive'))) {
        ds.canvasData[canvasOrRequestId].presaleActive = boolVal;
      } else if (parameterChange == keccak256(bytes('restrictRefAddress'))) {
        ds.canvasData[canvasOrRequestId].restrictRefAddress = boolVal;
      } else if (parameterChange == keccak256(bytes('restrictRefOverride'))) {
        ds.canvasData[canvasOrRequestId].restrictRefOverride = boolVal;
      } else if (parameterChange == keccak256(bytes('requireOwnershipOverride'))) {
        ds.canvasData[canvasOrRequestId].requireOwnershipOverride = boolVal;
      } else if (parameterChange == keccak256(bytes('refAddresses'))) {
        for (uint256 i = 0; i < addressVal.length; i++) {
          ds.refAddresses[canvasOrRequestId][addressVal[i]] = true;
        }
      } else if (parameterChange == keccak256(bytes('vrfCreate'))) {
        vrfCreate(canvasOrRequestId);
      } else if (parameterChange == keccak256(bytes('vrfCancel'))) {
        vrfCancel(canvasOrRequestId);
      }
    }
  }
  
  function updateCanvasTraits(address msgSender, uint256 canvasRequestId, MultimediaSchema.CanvasTraits calldata canvasTraits) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msgSender == ds.canvasRequest[canvasRequestId].admin, "No permission");
    ds.canvasRequestTraits[ds.canvasRequestCounter] = canvasTraits;
  }




  /* 
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙         CHAINLINK VRF LOGIC         ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function vrfCreate(uint256 canvasId) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    ds.canvasData[canvasId].vrfSubscriptionId = MultimediaVRFInterface(ds.vrfAddress).createNewSubscription(canvasId);
  }

  function vrfCancel(uint256 canvasId) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaVRFInterface(ds.vrfAddress).cancelSubscription(canvasId);
    ds.canvasData[canvasId].vrfSubscriptionId = 0;
  }

  function setVrfResult(address msgSender, uint256 canvasId, uint256 randomWord, uint256 maxBulk, string memory action) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msgSender == ds.vrfAddress, "No permission");

    if (keccak256(bytes(action)) == keccak256(bytes('requested'))) {
      ds.canvasData[canvasId].vrfPending = true;
    } else if (keccak256(bytes(action)) == keccak256(bytes('reset'))) {
      ds.canvasData[canvasId].vrfPending = false;
    } else if (keccak256(bytes(action)) == keccak256(bytes('fulfilled'))) {
      uint256 tokenCounter = ds.canvasData[canvasId].tokenIdCounter;
      uint256 remainder = tokenCounter.sub(ds.canvasData[canvasId].randomizedTraitsCounter);
      remainder = remainder > maxBulk ? maxBulk : remainder;

      if (ds.canvasData[canvasId].vrfPending && remainder > 0) {
        ds.randomizedTraits[ds.canvasData[canvasId].randomizedTraitsCounter] = randomWord;
        ds.canvasData[canvasId].randomizedTraitsCounter = ds.canvasData[canvasId].randomizedTraitsCounter.add(remainder);
        ds.canvasData[canvasId].vrfPending = false;
        ds.canvasData[canvasId].vrfLastRunTimestamp = block.timestamp;
      }
    }
  }




  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙             MINTING LOGIC           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function isAllowListed(address msgSender, uint256 canvasId, uint256 quantity, uint256 allowance, bytes32[] calldata proof) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(ds.allowListMintCount[canvasId][msgSender] + quantity < allowance + 1, "Over allowance");
    bool allowListed = MerkleProof.verify(proof, ds.canvasData[canvasId].merkleRoot, keccak256(abi.encodePacked(msgSender, allowance)));
    require(allowListed, "Not allowlisted");
    ds.allowListMintCount[canvasId][msgSender] = ds.allowListMintCount[canvasId][msgSender] + quantity;
  }

  function checkCanMint(address msgSender, uint256 canvasId, uint256 quantity, uint256 allowance, bytes32[] calldata proof) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];
    if (ds.canvasData[canvasId].presaleActive && !ds.canvasData[canvasId].saleActive) {
      isAllowListed(msgSender, canvasId, quantity, allowance, proof);
    } else {
      if (canvas.saleStart > 0)
        require(canvas.saleStart < block.timestamp && canvas.saleEnd > block.timestamp, 'Sale inactive');
      require(ds.canvasData[canvasId].saleActive, 'Sale inactive');
      if (ds.canvasData[canvasId].restrictToAllowList)
        isAllowListed(msgSender, canvasId, quantity, allowance, proof);
    }
  }

  function getPricing(uint256 canvasId) private view returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];
    uint256 price = canvas.salePrice;
    if (canvas.saleEndPrice > 0) {
      price = canvas.saleEndPrice.add(
        canvas.salePrice.sub(canvas.saleEndPrice)
          .mul(canvas.saleEnd.sub(block.timestamp))
          .div(canvas.saleEnd.sub(canvas.saleStart))
      );
    }

    return price;
  }

  // TODO: 
  //   - Check if minting with incorrect value drains user's funds
  //   - Check for overflow
  function mint(address msgSender, uint256 canvasId, uint256 quantity, uint256 ethValue, uint256 tokenValue, uint256 allowance, bytes32[] calldata proof) external returns (uint256) {
    checkCanMint(msgSender, canvasId, quantity, allowance, proof);
    uint256 tokenId = mintUpdateData(msgSender, canvasId, quantity);
    handleFunds(msgSender, canvasId, quantity, getPricing(canvasId), ethValue, tokenValue);

    return tokenId;
  }

  function mintUpdateData(address msgSender, uint256 canvasId, uint256 quantity) private returns (uint256) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    uint256 tokenIdCounter = ds.canvasData[canvasId].tokenIdCounter;

    if (canvas.totalQuantity > 0)
    require(tokenIdCounter.add(quantity).sub(1) < canvas.totalQuantity + 1, 'Sold out');

    uint256 tokenId = uint256(canvasId.mul(1000000000000)) + uint256(tokenIdCounter);
    ds.canvasData[canvasId].tokenIdCounter = tokenIdCounter.add(quantity);

    for (uint256 i = 0; i < quantity; i++) {
      ds.minter[tokenId] = msgSender;
      ds.mintTimestamp[tokenId] = block.timestamp;
    }

    return tokenId;
  }

  function handleFunds(address msgSender, uint256 canvasId, uint256 quantity, uint256 price, uint256 ethValue, uint256 tokenValue) private {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];

    if (canvas.saleToken == address(0)) {
      require(ethValue == price.mul(quantity), "Incorrect value");
      uint256 protocolFee = tokenValue.mul(ethValue).div(10000);
      (bool success,) = ds.protocolFeeRecipient.call{value: protocolFee}("");
      require(success, 'Issue sending ETH');
      (success,) = ds.canvasData[canvasId].creators[0].call{value: tokenValue.sub(protocolFee)}("");
      require(success, 'Issue sending ETH');
    } else {
      ERC677 token = ERC677(canvas.saleToken);

      require(tokenValue < token.balanceOf(msgSender) + 1, "Not enough balance");
      require(tokenValue == price.mul(quantity), "Incorrect value");

      uint256 protocolFee = tokenValue.mul(ds.protocolFeeBps).div(10000);
      token.transfer(ds.protocolFeeRecipient, protocolFee);
      token.transfer(ds.canvasData[canvasId].creators[0], tokenValue.sub(protocolFee));
    }
  }
}

