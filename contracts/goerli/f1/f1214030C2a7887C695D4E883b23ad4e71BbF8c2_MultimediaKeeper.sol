pragma solidity ^0.8.15;

import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

import "../multimedia/Multimedia.sol";
import "../multimedia/MultimediaSchema.sol";

contract MultimediaVRF is VRFConsumerBaseV2 {
  VRFCoordinatorV2Interface public vrfCoordinator;
  LinkTokenInterface public linkToken;
  bytes32 public keyHash;

  address public deployerAddress;
  address public multimediaAddress;
  address public keeperRegistryAddress;
  address public keeperAddress;
  uint256 public bulkRandomizeMax = 10;

  mapping (uint256 => uint64) public canvasIdToSubscriptionId;
  mapping(uint256 => uint256) public vrfToCanvasId;

  constructor(
    address _multimediaAddress,
    address _vrfCoordinator,
    address _linkToken,
    bytes32 _keyHash,
    address _keeperRegistryAddress
  ) VRFConsumerBaseV2(_vrfCoordinator) {
    multimediaAddress = _multimediaAddress;
    vrfCoordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
    linkToken = LinkTokenInterface(_linkToken);
    keyHash = _keyHash;
    keeperRegistryAddress = _keeperRegistryAddress;
  }

  modifier onlyDAO() {
    (address daoAddress,,,,,,,,,) = Multimedia(multimediaAddress).contractInfo();
    require(msg.sender == daoAddress, "Not allowed");
    _;
  }

  function setKeeperAddress(address _keeperAddress) external onlyDAO {
    keeperAddress = _keeperAddress;
  }

  function createNewSubscription(uint256 canvasId) external returns (uint64) {
    require(msg.sender == multimediaAddress, "No permission");

    canvasIdToSubscriptionId[canvasId] = vrfCoordinator.createSubscription();
    vrfCoordinator.addConsumer(canvasIdToSubscriptionId[canvasId], address(this));
    return canvasIdToSubscriptionId[canvasId];
  }

  function cancelSubscription(uint256 canvasId) external {
    require(msg.sender == multimediaAddress, "No permission");

    (MultimediaSchema.Canvas memory canvas,,) = Multimedia(multimediaAddress).getCanvas(canvasId);
    vrfCoordinator.cancelSubscription(canvasIdToSubscriptionId[canvasId], canvas.admin);
    canvasIdToSubscriptionId[canvasId] = 0;
  }

  function changeBulkRandomizeMax(uint256 newBulkRandomizeMax) external onlyDAO() {
    bulkRandomizeMax = newBulkRandomizeMax;
  }

  function resetVRF(uint256 canvasId) external onlyDAO() {
    Multimedia(multimediaAddress).setVrfResult(canvasId, 0, 0, "reset");
  }

  // Called by public or keeper
  function requestVRF(uint256 canvasId) external {
    (,,MultimediaSchema.CanvasData memory canvasData) = Multimedia(multimediaAddress).getCanvas(canvasId);

    require(!canvasData.vrfPending &&
            canvasData.tokenIdCounter - canvasData.randomizedTraitsCounter > 0 &&
            canvasData.vrfLastRunTimestamp + (canvasData.vrfMinuteInterval * 60) < block.timestamp
            , 'VRF not needed');

    uint256 requestId = vrfCoordinator.requestRandomWords(keyHash, canvasIdToSubscriptionId[canvasId], 7, 300000, 1);

    vrfToCanvasId[requestId] = canvasId;
    Multimedia(multimediaAddress).setVrfResult(canvasId, 0, 0, "requested");
  }

  // Chainlink VRF Callback
  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    uint256 canvasId = vrfToCanvasId[requestId];
    Multimedia(multimediaAddress).setVrfResult(canvasId, randomWords[0], bulkRandomizeMax, "fulfilled");
  }

  function onTokenTransfer(
    address sender,
    uint256 amount,
    bytes memory data
  ) public {
    (bytes32 callType, uint256 canvasId) = abi.decode(data, (bytes32, uint256));
    (MultimediaSchema.Canvas memory canvas,,) = Multimedia(multimediaAddress).getCanvas(canvasId);
    require(msg.sender == address(linkToken), "Must use LINK token");
    require(sender == canvas.admin, "Invalid admin");

    if (callType == bytes32('fundVrf'))
      linkToken.transferAndCall(address(vrfCoordinator), amount, abi.encode(canvasIdToSubscriptionId[canvasId]));
    else if (callType == bytes32('automateVrf')) {
      uint8 source = 110;

      bytes memory callData = abi.encodeWithSelector(
        bytes4(keccak256("register(string,bytes,address,uint32,address,bytes,uint96,uint8,address)")),
        string(abi.encodePacked("Canvas #", canvasId)),
        hex"",
        keeperAddress,
        uint32(300000),
        sender,
        abi.encode(canvasId),
        uint96(amount),
        source,
        address(this)
      );

      linkToken.transferAndCall(keeperRegistryAddress, amount, callData);
    }
  }
}

