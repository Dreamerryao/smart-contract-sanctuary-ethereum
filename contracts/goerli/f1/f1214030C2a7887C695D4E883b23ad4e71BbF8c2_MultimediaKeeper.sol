pragma solidity ^0.8.15;

library MultimediaSchema {
  struct TokenRef {
    address tokenAddress;
    uint256 tokenId;
    string contentType;
  }

  // Symbol Legend
  // * => DAO setting
  // ~ => Admin setting
  // # => Contract immutable
  // ! => Immutable once approved
  struct Canvas {
    address admin;            // * ~
    string name;              // ~
    string description;       // ~
    string thumbnailUri;      // ~
    string baseUri;           // *
    address saleToken;        // ~
    uint256 salePrice;        // ~
    uint256 saleEndPrice;     // ~
    uint256 totalQuantity;    // ~
    uint256 saleStart;        // ~
    uint256 saleEnd;          // ~
    address feeRecipient;     // ~
    uint16 feeBps;            // ~
    bool nonTransferable;     // ! ~
    bool editioned;           // ~
    bool requireOwnership;    // ~
    bool requireLicense;      // ~
    bool randomizeTraits;     // ~
  }

  struct CanvasTraits {
    string[] urlParams;       // ! ~
    string[] traits;          // ! ~
    string[][] values;        // ! ~
    uint16[] traitsChance;    // ! ~
    uint16[][] valuesChance;  // ! ~
  }

  struct CanvasData {
    address[] creators;
    address newAdminRequest;          // two step variable to update admin
    uint256 tokenIdCounter;           // #
    uint256 randomizedTraitsCounter;  // #
    uint256 vrfLastRunTimestamp;      // #
    uint256 vrfSubscriptionId;        // # ~
    uint16 vrfMinuteInterval;         // ~
    bytes32 merkleRoot;               // ~
    bool isImmutable;                 // ~
    bool restrictToAllowList;         // ~
    bool presaleActive;               // ~
    bool saleActive;                  // ~
    bool restrictRefAddress;          // ~
    bool restrictRefOverride;         // ~
    bool requireOwnershipOverride;    // ~
    bool vrfPending;                  // #
  }

  struct Storage {
    address daoAddress;
    address newDaoRequest;
    address curatorAddress;
    address newCuratorRequest;
    address multimediaAddress;
    address registryAddress;
    address vrfAddress;
    address keeperAddress;
    address keeperRegistryAddress;
    address linkTokenAddress;
    address protocolFeeRecipient;
    uint256 protocolFeeBps;
    uint256 canvasRequestCounter;
    uint256 canvasCounter;

    // canvasId => config (starting from index 0)
    mapping (uint256 => uint256) canvas; // canvasId => canvasRequestId
    mapping (uint256 => CanvasData) canvasData;
    mapping (uint256 => Canvas) canvasRequest;
    mapping (uint256 => CanvasTraits) canvasRequestTraits;
    mapping (uint256 => uint256) randomizedTraits;
    mapping (uint256 => mapping (address => bool)) refAddresses; // restrict token refs to these addresses
    mapping (uint256 => mapping (address => uint256)) allowListMintCount;

    // canvasId => owner customization
    mapping (uint256 => TokenRef[]) refs;
    mapping (uint256 => string) tokenTitle;
    mapping (uint256 => string) tokenSubtitle;

    // tokenId => info
    mapping (uint256 => address) minter;
    mapping (uint256 => uint256) mintTimestamp;

  }
}

