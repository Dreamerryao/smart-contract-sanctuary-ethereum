pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "base64-sol/base64.sol";
import "hardhat/console.sol";

import "../interfaces/INFT.sol";
import "../utils/Parser.sol";
import "./MultimediaSchema.sol";

library MultimediaCore {
  using SafeMath for uint256;

  bytes32 constant private STORAGE_POSITION = keccak256("com.multimedia.nft.storage");
  function multimediaStorage() internal pure returns (MultimediaSchema.Storage storage ds) {
    bytes32 position = STORAGE_POSITION;
    assembly { ds.slot := position }
  }


  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙           PARSING HELPERS           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function getTokenIDs(uint256 tokenId) private pure returns (uint256 nftId, uint256 canvasId) {
    nftId = tokenId;
    canvasId = 0;
    if (tokenId > 1000000000000) {
      nftId = Parser.safeParseInt(Parser.substring(Parser.uint2str(tokenId), bytes(Parser.uint2str(tokenId)).length.sub(9), bytes(Parser.uint2str(tokenId)).length));
      canvasId = tokenId.sub(nftId).div(1000000000000);
    }

    return (nftId, canvasId);
  }


  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙            DAO FUNCTIONS            ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function changeOwnership(
    address msgSender,
    address payable daoOrCuratorAddress, // unused if accepting
    bool isCurator,
    bool isAccepting
  ) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();

    if (isCurator) {
      if (isAccepting) {
        require(msgSender == ds.newCuratorRequest, "No permission");
        ds.curatorAddress = ds.newCuratorRequest;
        ds.newCuratorRequest = address(0);
      } else {
        require(msgSender == ds.curatorAddress, "No permission");
        ds.newCuratorRequest = daoOrCuratorAddress;
      }
    } else {
      if (isAccepting) {
        require(msgSender == ds.newDaoRequest, "No permission");
        ds.daoAddress = ds.newDaoRequest;
        ds.newDaoRequest = address(0);
      } else {
        require(msgSender == ds.daoAddress, "No permission");
        ds.newDaoRequest = daoOrCuratorAddress;
      }
    }
  }

  function changeProtocolParameter(address msgSender, string calldata parameter, address addressVal, uint256 numberVal) external {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    require(msgSender == ds.daoAddress, "No permission");
    if (keccak256(bytes(parameter)) == keccak256(bytes('vrf')))
      ds.vrfAddress = addressVal;
    else if (keccak256(bytes(parameter)) == keccak256(bytes('keeper')))
      ds.keeperAddress = addressVal;
    else if (keccak256(bytes(parameter)) == keccak256(bytes('registry')))
      ds.registryAddress = addressVal;
    else if (keccak256(bytes(parameter)) == keccak256(bytes('protocolFeeRecipient')))
      ds.protocolFeeRecipient = addressVal;
    else if (keccak256(bytes(parameter)) == keccak256(bytes('protocolFeeBps'))) {
      require(numberVal < 3001, 'Invalid fee'); // Maximum 30% protocol fee possible
      ds.protocolFeeBps = numberVal;
    }
  }


  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙           PUBLIC VARIABLES          ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function contractInfo() external view returns (
    address daoAddress,
    address newDaoRequest,
    address curatorAddress,
    address newCuratorRequest,
    address registryAddress,
    address vrfAddress,
    uint256 protocolFeeBps,
    address protocolFeeRecipient,
    uint256 canvasRequestCounter,
    uint256 canvasCounter
  ) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    return (
      ds.daoAddress,
      ds.newDaoRequest,
      ds.curatorAddress,
      ds.newCuratorRequest,
      ds.registryAddress,
      ds.vrfAddress,
      ds.protocolFeeBps,
      ds.protocolFeeRecipient,
      ds.canvasRequestCounter,
      ds.canvasCounter
    );
  }

  function tokenCreator(uint256 tokenId) external view returns (address) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    (,uint256 canvasId) = getTokenIDs(tokenId);
    return ds.canvasData[canvasId].creators[0];
  }
  
  function getCanvas(uint256 canvasId) public view returns (
    MultimediaSchema.Canvas memory canvas,
    MultimediaSchema.CanvasTraits memory canvasTraits,
    MultimediaSchema.CanvasData memory canvasData
  ) {
    MultimediaSchema.Storage storage ds = MultimediaCore.multimediaStorage();

    return (
      ds.canvasRequest[ds.canvas[canvasId]],
      ds.canvasRequestTraits[ds.canvas[canvasId]],
      ds.canvasData[canvasId]
    );
  }



  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙     ROYALTY AND TRANSFERABILITY     ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function royaltyInfo(uint256 tokenId, uint256 value) external view returns (address recipient, uint256 amount) {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    (, uint256 canvasId) = getTokenIDs(tokenId);
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];
    return (canvas.feeRecipient, uint256(canvas.feeBps).div(10000).mul(value));
  }

  function canTransfer(uint256 tokenId) external view {
    MultimediaSchema.Storage storage ds = multimediaStorage();
    (,uint256 canvasId) = getTokenIDs(tokenId);
    MultimediaSchema.Canvas storage canvas = ds.canvasRequest[ds.canvas[canvasId]];
    require(!canvas.nonTransferable, 'Non-transferrable');
  }
}

