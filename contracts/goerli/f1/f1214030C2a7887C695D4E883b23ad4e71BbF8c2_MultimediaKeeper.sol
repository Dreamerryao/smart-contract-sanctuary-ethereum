pragma solidity ^0.8.15;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

import "../interfaces/INFT.sol";
import "./MultimediaSchema.sol";
import "./MultimediaCore.sol";
import "./MultimediaMeta.sol";
import "./MultimediaCanvas.sol";

// TODO:
//       - 1/1 for tokenIDs < 1 trillion
//       - 10% of sale if VRF upkeep managed by Multimedia
//       - Fully custodial VRF per canvas?
//       - baseURI to DAO only & other canvas parameters check if should be updated pre/post sale (e.g. ref addresses)

contract Multimedia is ERC721 {

  event ContractUpdate();
  event CustomizeToken(uint256 tokenId);
  event Canvas(uint256 canvasCounter);
  event CanvasRequest(uint256 canvasRequestCounter);

  constructor (
    string memory name,
    string memory symbol,
    address linkTokenAddress,
    address keeperRegistryAddress
  ) ERC721(name, symbol) {
    MultimediaSchema.Storage storage ds = MultimediaMeta.multimediaStorage();

    // Adjustable via DAO
    ds.multimediaAddress = address(this);
    ds.daoAddress = payable(msg.sender);
    ds.linkTokenAddress = linkTokenAddress;
    ds.keeperRegistryAddress = keeperRegistryAddress;
    ds.curatorAddress = msg.sender;
    ds.protocolFeeRecipient = msg.sender;
    ds.protocolFeeBps = 1000;

    emit ContractUpdate();
  }

  bytes4 private constant _INTERFACE_ID_ROYALTIES_EIP2981 = 0x2a55205a;
  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721) returns (bool) {
    return interfaceId == _INTERFACE_ID_ROYALTIES_EIP2981 || super.supportsInterface(interfaceId);
  }



  /* 
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙          MULTIMEDIA CANVAS          ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function createCanvas(MultimediaSchema.Canvas calldata canvas, MultimediaSchema.CanvasTraits calldata canvasTraits) external {
    emit CanvasRequest(MultimediaCanvas.createCanvas(canvas, canvasTraits));
  }

  function approveCanvas(uint256 canvasRequestId) external {
    emit Canvas(MultimediaCanvas.approveCanvas(msg.sender, canvasRequestId));
  }

  function changeAdmin(uint256 canvasId, address newAdminRequest, bool isAccepting) external {
    MultimediaCanvas.changeAdmin(msg.sender, canvasId, newAdminRequest, isAccepting);
    emit Canvas(canvasId);
  }

  function updateCanvas(
    uint256 canvasOrRequestId,
    bool isRequest,
    string memory parameter,
    address[] calldata addressVal,
    string memory stringVal,
    uint256 numberVal,
    bytes32 bytesVal,
    bool boolVal
  ) external {
    MultimediaCanvas.updateCanvas(msg.sender, canvasOrRequestId, isRequest, parameter, addressVal, stringVal, numberVal, bytesVal, boolVal);
    emit Canvas(canvasOrRequestId);
  }

  function updateCanvasTraits(uint256 canvasRequestId, MultimediaSchema.CanvasTraits calldata canvasTraits) external {
    MultimediaCanvas.updateCanvasTraits(msg.sender, canvasRequestId, canvasTraits);
    emit CanvasRequest(canvasRequestId);
  }

  function setVrfResult(uint256 canvasId, uint256 vrfResult, uint256 maxBulk, string memory action) external {
    MultimediaCanvas.setVrfResult(msg.sender, canvasId, vrfResult, maxBulk, action);
    emit Canvas(canvasId);
  }

  function mint(uint256 canvasId, uint256 quantity, uint256 tokenValue, uint256 allowance, bytes32[] calldata proof) external payable {
    uint256 tokenId = MultimediaCanvas.mint(msg.sender, canvasId, quantity, msg.value, tokenValue, allowance, proof);
    for (uint256 i = tokenId; i < tokenId + quantity; i++) {
      _mint(msg.sender, i);
    }
  }




  /*
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙           MULTIMEDIA META           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */


  function customizeToken(uint256 tokenId, string memory text, string memory textType, MultimediaSchema.TokenRef[] calldata refs) external {
    MultimediaMeta.customizeToken(msg.sender, ownerOf(tokenId), tokenId, text, textType, refs);
    emit CustomizeToken(tokenId);
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    return MultimediaMeta.tokenURI(tokenId);
  }


  /* 
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
    ⁙           MULTIMEDIA CORE           ⁙
    ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙ ⁙
  */

  function changeOwnership(address payable daoOrCuratorAddress, bool isCurator, bool isAccepting) external {
    MultimediaCore.changeOwnership(msg.sender, daoOrCuratorAddress, isCurator, isAccepting);
    emit ContractUpdate();
  }

  function changeProtocolParameter(string memory parameter, address addressVal, uint256 numberVal) external {
    MultimediaCore.changeProtocolParameter(msg.sender, parameter, addressVal, numberVal);
    emit ContractUpdate();
  }

  function getCanvas(uint256 canvasId) public view returns (
    MultimediaSchema.Canvas memory canvas,
    MultimediaSchema.CanvasTraits memory canvasTraits,
    MultimediaSchema.CanvasData memory canvasData
  ) {
    return MultimediaCore.getCanvas(canvasId);
  }

  function contractInfo() external view returns (
    address daoAddress,
    address newDaoRequest,
    address curatorAddress,
    address newCuratorRequest,
    address registryAddress,
    address vrfAddress,
    uint256 protocolFeeBps,
    address protocolFeeRecipient,
    uint256 canvasRequestCounter,
    uint256 canvasCounter
  ) {
    return MultimediaCore.contractInfo();
  }

  function tokenCreator(uint256 tokenId) external view returns (address creator) {
    return MultimediaCore.tokenCreator(tokenId);
  }

  function owner() external view returns (address daoAddress) {
    return MultimediaCore.multimediaStorage().daoAddress;
  }

  function mintedBy(uint256 tokenId) external view returns (address minter) {
    return MultimediaCore.multimediaStorage().minter[tokenId];
  }

  function mintedOn(uint256 tokenId) external view returns (uint256 timestamp) {
    return MultimediaCore.multimediaStorage().mintTimestamp[tokenId];
  }

  function royaltyInfo(uint256 tokenId, uint256 value) external view returns (address recipient, uint256 amount) {
    return MultimediaCore.royaltyInfo(tokenId, value);
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public override {
    MultimediaCore.canTransfer(tokenId);
    super.transferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public override {
    MultimediaCore.canTransfer(tokenId);
    super.safeTransferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  ) public override {
    MultimediaCore.canTransfer(tokenId);
    super.safeTransferFrom(from, to, tokenId, data);
  }
}

