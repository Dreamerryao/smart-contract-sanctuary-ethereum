        for(uint x; x < ArtistIDs.length; x++)
        {
            for(uint y; y < Ranges[x]; y++)
            {
                TimeSlotsBooked[x][y] = SelectedTimeslot[ArtistIDs[x]][y];
            }
        }
        return TimeSlotsBooked;
    }

