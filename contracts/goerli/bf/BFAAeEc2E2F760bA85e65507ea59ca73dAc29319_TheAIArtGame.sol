        uint256 yoinks = generateNumber(generateSeed(tokenId + 420), 5) + 1;
        bool freePhrase = generateNumber(tokenId, 1337) == 420 ||
            generateNumber(tokenId, 1337) == 69;
        bool freeProtection = generateNumber(tokenId, 500) == 420 ||
            generateNumber(tokenId, 1337) == 69;
        return [
