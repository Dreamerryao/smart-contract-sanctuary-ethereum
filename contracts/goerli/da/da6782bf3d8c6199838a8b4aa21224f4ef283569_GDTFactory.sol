// File: GDTFactory.sol


pragma solidity >=0.8.10 < 0.9;







contract GDTFactory is AccessControl{

    address immutable primaryToken;
    address[] public rotationTokens;
    address public GDTlauncher;
    IUniswapV2Router02 immutable public uniswapV2Router;
    uint256 index=1;
    uint256 indexG;
    uint256 count;
    uint256 n = 0.0001 ether;

    address public locker = 0x02A137F39543567740315fcD91FE8555f6A6d752;

    uint256 public ethNeeded;
    uint256 public constant growthRate = 0.0001 ether;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant LAUNCHER_ROLE = keccak256("LAUNCHER_ROLE");

    mapping(address => address[]) public launcherToTokens;


    mapping(address => bool) approved;

    struct TokenToLaunch{
        address creator;
        address token;
        address[3] tokens;
    }

    TokenToLaunch[] public tokensToFinalize;
    uint256 indexL;

    constructor(){
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );
        uniswapV2Router = _uniswapV2Router;
        ethNeeded = 1 ether;

        primaryToken = address(0xFFE93E63E94da7A44739b6aBFA25B81CEd979a6b);
        rotationTokens.push(0x5d95F7998AcE0B8310bf1Ae153a255C226A1612B); //BTC
        rotationTokens.push(0x7060C6C2d37B8b6f865302aF09d0e4ed9DdEcEBc); //DAI
        rotationTokens.push(0xBa2AB017154D69A11f46518552CB7b206f6759fc); //polygon

        _setupRole(ADMIN_ROLE, msg.sender);
        _setupRole(LAUNCHER_ROLE, msg.sender);
        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _setRoleAdmin(LAUNCHER_ROLE, ADMIN_ROLE);
    }

    function launchToken(string memory name, string memory symbol, uint256 totalSupply)public onlyRole(LAUNCHER_ROLE){
        require(totalSupply >99999 && totalSupply <50000000001, "Total Supply out of bounds.");//between 100k and 50 billion
        require(address(this).balance > ethNeeded, "Not enough ETH to launch,");
        require(indexL == tokensToFinalize.length, "Need to finalize");
        _revokeRole(LAUNCHER_ROLE, msg.sender);
        uint256 tS = totalSupply * 1 ether;
        address newToken = IGDTLauncher(GDTlauncher).launch(name, symbol, address(this), msg.sender, tS);
        address[3] memory tokens = pickTokens();
        for(uint8 i = 0; i < 3; i++){
            swapEthForTokens(ethNeeded/10, tokens[i], address(this));
        }
        tokensToFinalize.push(TokenToLaunch(msg.sender, newToken, tokens));
        launcherToTokens[msg.sender].push(newToken);
    }

    function changeEthNeeded(uint256 eth)external onlyRole(ADMIN_ROLE){
        ethNeeded = eth;
    }
    
    function setGDTLauncher(address account)external onlyRole(ADMIN_ROLE){
        GDTlauncher = account;
    }

    receive() external payable {}

    function finalizeToken()external {
        require(indexL < tokensToFinalize.length,"No tokens to launch.");
        require(address(this).balance > (ethNeeded*7)/10, "Not enough ETH to launch,");

        uint256 ethForTokens = ethNeeded/10;
        TokenToLaunch memory t = tokensToFinalize[indexL];
        IERC20 tP = IERC20(t.token);
        uint256 liquidityBalance = tP.balanceOf(address(this))/10;
        for(uint8 i =0; i < 3; i++){        
            addLiquidityAndDrop(t.token, t.tokens[i], liquidityBalance);
        }

        uniswapV2Router.addLiquidityETH{value: ethForTokens*7}(t.token, liquidityBalance*7, 0, 0, address(0xdead), block.timestamp);
        rotationTokens.push(t.token);
        indexL++;
    }

    function addLiquidity(address token0, address token1, uint256 amount0, uint256 amount1)internal {
        _resetApproval(token0);
        _resetApproval(token1);
        uniswapV2Router.addLiquidity(token0, token1, amount0, amount1, 0, 0, address(this), block.timestamp);
    }

    function addLiquidityAndDrop(address token0, address token1, uint256 amount)internal {
        addLiquidity(token0, token1, amount, IERC20(token1).balanceOf(address(this)));
        address pair = IUniswapV2Factory(uniswapV2Router.factory()).getPair(token0, token1);
        IERC20(pair).transfer(locker, IERC20(pair).balanceOf(address(this))/2);
        IERC20(pair).transfer(address(0xdead), IERC20(pair).balanceOf(address(this)));
    }

    function swapEthForTokens(uint256 ethAmount, address token, address to) private {
        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = token;
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: ethAmount}(0, path, to, block.timestamp);
    }

    function pickTokens()internal returns(address[3] memory tokens){
        tokens[0] = primaryToken;
        for(uint8 i = 1; i < 3; i++){
            index = (index+1)%rotationTokens.length;
            tokens[i] = rotationTokens[index];
        }
    }

    function _resetApproval(address token)internal{
        IERC20(token).approve(address(uniswapV2Router), type(uint256).max);
    }

    function growth()external returns(bool success, address pair){
        //Allows for other future contracts to call this without reverting their flow
        if(canGrow()){
            if(n >= 10 * growthRate){
                swapEthForTokens(4*growthRate, primaryToken, address(this));
                _resetApproval(primaryToken);
                uniswapV2Router.addLiquidityETH{value: 4*growthRate}(primaryToken, IERC20(primaryToken).balanceOf(address(this)), 0, 0, locker, block.timestamp);
                n = growthRate;
            }else{  
                (success, ) = address(locker).call{
                    value: growthRate
                }("");
                success = false;
                pair = IUniswapV2Factory(uniswapV2Router.factory()).getPair(primaryToken, rotationTokens[indexG]);        

                if(pair != address(0)){
                    swapEthForTokens(growthRate, primaryToken, pair);
                    IUniswapV2Pair(pair).sync();
                    count++;
                    success = true;
                }
                if(count >= rotationTokens.length){
                    n += growthRate;
                    count =0;
                }
                indexG = (indexG+1) % rotationTokens.length;
            }
        }
    }

    function canGrow()public view returns(bool){
        return ethNeeded + n >= address(this).balance && indexL >= tokensToFinalize.length;
    }

    function sendTokensToLocker(address token)external onlyRole(ADMIN_ROLE){
        require(indexL == tokensToFinalize.length, "Need to finalize All tokens first.");
        IERC20(token).transfer(locker, IERC20(token).balanceOf(address(this)));
    }
}