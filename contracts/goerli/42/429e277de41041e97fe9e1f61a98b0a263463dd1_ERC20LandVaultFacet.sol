import "../libraries/LibVaultErc20.sol";
import "../interfaces/IERC20LandVault.sol";
import "../MockLandContract/MockIERC721.sol";

import "../utils/Context.sol";

contract ERC20LandVaultFacet is Context, IERC20LandVault {
    /// @notice Initialises the ERC721's name, symbol and mockDcllandAddress
    //  Implements ERC721 constructor data for the vault in the whole proxy pattern.
    function initErc20LandVault(
        string memory _name,
        string memory _symbol,
        address _dclLandContract
    ) external returns (bool success) {
        LibVaultErc20.VaultErc20Storage storage erc20 = LibVaultErc20
            .vaultErc20Storage();
        require(!erc20.initialized, "ERC20 Storage already initialized");

        erc20.initialized = true;
        erc20._name = _name;
        erc20._symbol = _symbol;
        erc20.dclLandAddress = _dclLandContract;
        success = true;
    }

    function name() external view returns (string memory) {
        return LibVaultErc20.name();
    }

    function symbol() external view returns (string memory) {
        return LibVaultErc20.symbol();
    }

    function decimals() external view returns (uint8) {
        return LibVaultErc20.decimals();
    }

    function getLandAddress() external view returns (address) {
        return LibVaultErc20.getLandAddress();
    }

    function totalSupply() external view returns (uint256) {
        return LibVaultErc20.totalSupply();
    }

    function balanceOf(address account) external view returns (uint256) {
        return LibVaultErc20.balanceOf(account);
    }

    function transfer(address to, uint256 amount) external returns (bool) {
        return LibVaultErc20.transfer(to, amount);
    }

    function allowance(address owner, address spender)
        external
        view
        returns (uint256)
    {
        return LibVaultErc20.allowance(owner, spender);
    }

    function approve(address spender, uint256 amount) external returns (bool) {
        return LibVaultErc20.approve(spender, amount);
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool) {
        return LibVaultErc20.transferFrom(from, to, amount);
    }

    function increaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool)
    {
        return LibVaultErc20.increaseAllowance(spender, addedValue);
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        external
        returns (bool)
    {
        return LibVaultErc20.decreaseAllowance(spender, subtractedValue);
    }

    function getAllCurrentlyStaked()
        external
        view
        returns (uint256[] memory currentlyStaked)
    {
        return LibVaultErc20.getAllCurrentlyStaked();
    }

    function batchStake(uint256[] memory _tokenIds)
        external
        returns (bool success)
    {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _stake(_tokenIds[i]);
        }
        success = true;
    }

    function stake(uint256 _tokenId) external returns (bool success) {
        _stake(_tokenId);
        success = true;
    }

    function _stake(uint256 _tokenId) internal returns (bool success) {
        LibVaultErc20.VaultErc20Storage storage erc20 = LibVaultErc20
            .vaultErc20Storage();
        require(
            MockIERC721(erc20.dclLandAddress).ownerOf(_tokenId) == _msgSender(),
            "Msg.sender must be land owner to stake"
        );
        require(
            MockIERC721(erc20.dclLandAddress).isAuthorized(
                address(this),
                _tokenId
            ),
            "WeMeta Diamond Vault must be given approval permission of land to be staked"
        );
        MockIERC721(erc20.dclLandAddress).transferFrom(
            _msgSender(),
            address(this),
            _tokenId
        );

        //no registry permitted at present
        LibVaultErc20._mint(_msgSender(), 1);

        //register currently and previously staked tokens
        bool previouslyStakedResult = LibVaultErc20._previouslyStaked(_tokenId);
        if (!previouslyStakedResult) {
            erc20.depositedTokenIds.push(_tokenId);
        }
        erc20.currentStakeStatus[_tokenId] = true;
        emit Added(_msgSender(), _tokenId, block.timestamp);
        success = true;
    }

    function batchUnstakeRandom(uint256 _tokensToWithdraw)
        external
        returns (bool success)
    {
        require(
            LibVaultErc20.balanceOf(_msgSender()) > _tokensToWithdraw,
            "Must Hold Enough Erc20Vault Tokens to Withdraw Multiple Properties"
        );
        for (uint256 i = 0; i < _tokensToWithdraw; i++) {
            _unstake();
        }
        success = true;
    }

    function batchUnstakeByTokenId(uint256[] calldata _tokenIds)
        external
        returns (bool success)
    {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _unstakeByTokenId(_tokenIds[i]);
        }
        success = true;
    }

    function unstakeByTokenId(uint256 _tokenId)
        external
        returns (bool success)
    {
        _unstakeByTokenId(_tokenId);
        success = true;
    }

    function _unstakeByTokenId(uint256 _tokenId)
        internal
        returns (bool success)
    {
        LibVaultErc20.VaultErc20Storage storage erc20 = LibVaultErc20
            .vaultErc20Storage();

        require(
            LibVaultErc20.balanceOf(_msgSender()) > 0,
            "Must Hold Erc20Vault Token to Withdraw Property"
        );

        uint256[] memory currenltyStaked = LibVaultErc20
            .getAllCurrentlyStaked();
        bool tokenIsStaked;
        for (uint256 i = 0; i < currenltyStaked.length; i++) {
            if (currenltyStaked[i] == _tokenId) {
                tokenIsStaked = true;
            }
        }
        require(tokenIsStaked, "Selected token not currently staked");

        MockIERC721(erc20.dclLandAddress).transferFrom(
            address(this),
            _msgSender(),
            _tokenId
        );
        LibVaultErc20._burn(_msgSender(), 1);
        erc20.currentStakeStatus[_tokenId] = false;
        emit Removed(_msgSender(), _tokenId, block.timestamp);
        success = true;
    }

    function unstake() external returns (bool success) {
        _unstake();
        success = true;
    }

    function _unstake() internal returns (bool success) {
        LibVaultErc20.VaultErc20Storage storage erc20 = LibVaultErc20
            .vaultErc20Storage();

        require(
            LibVaultErc20.balanceOf(_msgSender()) > 0,
            "Must Hold Erc20Vault Token to Withdraw Property"
        );
        uint256 firstAvailableTokenId = MockIERC721(erc20.dclLandAddress)
            .tokenOfOwnerByIndex(address(this), 0);

        MockIERC721(erc20.dclLandAddress).transferFrom(
            address(this),
            _msgSender(),
            firstAvailableTokenId
        );
        LibVaultErc20._burn(_msgSender(), 1);
        erc20.currentStakeStatus[firstAvailableTokenId] = false;
        emit Removed(_msgSender(), firstAvailableTokenId, block.timestamp);
        success = true;
    }
}

