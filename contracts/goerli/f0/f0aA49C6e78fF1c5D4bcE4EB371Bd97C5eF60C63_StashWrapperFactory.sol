// SPDX-License-Identifier: MIT
pragma solidity ^0.8.12;

import "../../libraries/TokenTransferLibrary.sol";

import "./P2ERoyalties.sol";

abstract contract P2ERoyaltyPayments is P2ERoyalties {
  using TokenTransferLibrary for address;

  event P2ERoyaltyPaid(
    address indexed operator,
    address indexed forUser,
    address indexed to,
    address paymentToken,
    uint256 amount
  );

  function payP2ERoyalties(
    address user,
    uint256 tokenIdOrRecordId,
    address paymentToken,
    uint256 amount
  ) external payable {
    Recipient[] memory recipients = getP2ERecipients(user, tokenIdOrRecordId);
    for (uint256 i = 0; i < recipients.length; ++i) {
      Recipient memory recipient = recipients[i];
      uint256 royaltyAmount = (amount * recipient.shareInBasisPoints) / BASIS_POINTS;
      paymentToken.transferFunds(msg.sender, recipient.recipient, royaltyAmount);
      emit P2ERoyaltyPaid(msg.sender, user, recipient.recipient, paymentToken, royaltyAmount);
    }
  }
}

