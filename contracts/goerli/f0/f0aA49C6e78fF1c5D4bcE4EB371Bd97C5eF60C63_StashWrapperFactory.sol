pragma solidity ^0.8.12;

import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import "../interfaces/IERC4907.sol";

import "./ContractFactory.sol";
import "./P2ERoyalties.sol";
import "./WrappedNFT.sol";

/**
 * @title Rental details implementation.
 * @dev Modified from https://eips.ethereum.org/EIPS/eip-4907 reference impl
 * @author batu-inal & HardlyDifficult
 */
abstract contract ERC4907 is IERC4907, ERC165, ContractFactory, ERC721Upgradeable, WrappedNFT {
  struct UserInfo {
    address user; // address of user role
    uint64 expires; // unix timestamp, user expires
    address operator; // address that created the rental
  }

  mapping(uint256 => UserInfo) private _users;

  function setUser(
    uint256 tokenId,
    address user,
    uint64 expires
  ) external override {
    require(expires > block.timestamp, "Rental terms have expired");
    require(_isApprovedOrOwner(msg.sender, tokenId), "ERC4907: transfer caller is not owner nor approved");
    UserInfo storage info = _users[tokenId];
    require(info.expires < block.timestamp, "NFT already rented");
    info.user = user;
    info.expires = expires;
    info.operator = msg.sender;
    emit UpdateUser(tokenId, user, expires);
  }

  function userOf(uint256 tokenId) public view override returns (address renter) {
    UserInfo memory info = _users[tokenId];
    if (info.expires >= block.timestamp) {
      return info.user;
    }
  }

  function userExpires(uint256 tokenId) external view override returns (uint256 expires) {
    UserInfo storage info = _users[tokenId];
    if (info.expires >= block.timestamp) {
      expires = info.expires;
    }
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(ERC165, ERC721Upgradeable, WrappedNFT)
    returns (bool)
  {
    return (interfaceId == type(IERC4907).interfaceId || super.supportsInterface(interfaceId));
  }

  function _getRental(uint256 tokenId) internal view returns (address renter, address operator) {
    UserInfo memory info = _users[tokenId];
    if (info.expires >= block.timestamp) {
      renter = info.user;
      operator = info.operator;
    }
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal virtual override {
    super._beforeTokenTransfer(from, to, tokenId);

    require(_users[tokenId].expires < block.timestamp || _users[tokenId].user == to, "NFT is rented");

    if (from != to && _users[tokenId].user != address(0)) {
      delete _users[tokenId];
      emit UpdateUser(tokenId, address(0), 0);
    }
  }
}

