import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";

import "./interfaces/IERC4907.sol";
import "./interfaces/IStashWrapped721.sol";

import "../libraries/SupportsInterfaceUnchecked.sol";

import "./mixins/ContractFactory.sol";
import "./mixins/ERC4907.sol";
import "./mixins/P2ERoyalties.sol";
import "./mixins/P2ERoyaltyPayments.sol";
import "./mixins/WrappedNFT.sol";

/**
 * @title A wrapped version of an ERC721 NFT to support rentals.
 * @author batu-inal & HardlyDifficult
 */
contract StashWrapped721 is
  IERC4907,
  IStashWrapped721,
  Initializable,
  ERC165,
  ERC721Upgradeable,
  P2ERoyalties,
  P2ERoyaltyPayments,
  WrappedNFT,
  ERC4907
{
  using ERC165Checker for address;
  using SupportsInterfaceUnchecked for address;

  constructor(
    address _contractFactory,
    uint16 _renterRoyaltyInBasisPoints,
    uint16 _operatorRoyaltyInBasisPoints
  )
    ContractFactory(_contractFactory)
    P2ERoyalties(_renterRoyaltyInBasisPoints, _operatorRoyaltyInBasisPoints)
  // solhint-disable-next-line no-empty-blocks
  {

  }

  function initialize(address _originalNFTContract) external initializer {
    require(
      _originalNFTContract.supportsERC165InterfaceUnchecked(type(IERC721Metadata).interfaceId),
      "Must support ERC721Metadata"
    );

    // TODO: save gas with a custom initializer, based on original != 0?
    _initializeWrappedNFT(_originalNFTContract);

    // ERC721 init not required since the name/symbol are overridden below.
  }

  function wrap(uint256 tokenId) external {
    _wrap(msg.sender, tokenId);
  }

  function factoryWrap(address user, uint256 tokenId) external onlyContractFactory {
    _wrap(user, tokenId);
  }

  function wrapAndSetApprovalForAll(uint256 tokenId, address operator) external {
    _wrap(msg.sender, tokenId);
    _setApprovalForAll(msg.sender, operator, true);
  }

  function factoryWrapAndSetApprovalForAll(
    address owner,
    uint256 tokenId,
    address operator
  ) external onlyContractFactory {
    _wrap(owner, tokenId);
    _setApprovalForAll(owner, operator, true);
  }

  function _wrap(address owner, uint256 tokenId) private {
    IERC721(originalNFTContract).transferFrom(owner, address(this), tokenId);
    _mint(owner, tokenId);
  }

  function unwrap(uint256 tokenId) external {
    require(ownerOf(tokenId) == msg.sender, "Must be called by the owner");
    IERC721(originalNFTContract).transferFrom(address(this), msg.sender, tokenId);

    // Burn will revert if the NFT is currently rented
    _burn(tokenId);
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal override(ERC4907, ERC721Upgradeable) {
    super._beforeTokenTransfer(from, to, tokenId);
  }

  function name() public view override returns (string memory) {
    return string.concat("Wrapped ", IERC721Metadata(originalNFTContract).name());
  }

  function symbol() public view override returns (string memory) {
    return string.concat("W", IERC721Metadata(originalNFTContract).symbol());
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    return IERC721Metadata(originalNFTContract).tokenURI(tokenId);
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC165, ERC721Upgradeable, ERC4907, P2ERoyalties, WrappedNFT)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function _getUsersOf(
    address, /* user */
    uint256 tokenId
  )
    internal
    view
    override
    returns (
      address owner,
      address renter,
      address operator
    )
  {
    (renter, operator) = _getRental(tokenId);
    owner = ownerOf(tokenId);
  }
}

