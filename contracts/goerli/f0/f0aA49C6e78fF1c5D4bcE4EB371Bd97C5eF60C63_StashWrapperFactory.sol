import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "./interfaces/IStashWrapperFactory.sol";
import "./interfaces/IStashWrapped721.sol";
import "./interfaces/IStashWrapped1155.sol";

import "../libraries/SupportsInterfaceUnchecked.sol";
import "../libraries/NFTTypes.sol";

import "./mixins/ERC4907.sol";
import "./mixins/P2ERoyalties.sol";
import "./StashWrapped721.sol";

/**
 * @title Allows wrapping and unwrapping of NFTs to enable rentals.
 * @author batu-inal & HardlyDifficult
 */
contract StashWrapperFactory is IStashWrapperFactory, OwnableUpgradeable {
  using ERC165Checker for address;
  using SupportsInterfaceUnchecked for address;
  using NFTTypes for address;
  using AddressUpgradeable for address;
  using Clones for address;

  address public implementationStashWrapped721;

  uint32 public versionStashWrapped721;

  address public implementationStashWrapped1155;

  uint32 public versionStashWrapped1155;

  // solhint-disable-next-line no-empty-blocks
  constructor() initializer {}

  function initialize() external initializer {
    __Ownable_init();
  }

  function adminUpdateImplementation(address _implementation) external onlyOwner {
    require(_implementation.supportsERC165(), "NFT contract does not support ERC165");
    NFTType nftType = _implementation.checkNftType(true);
    if (nftType == NFTType.ERC721) {
      implementationStashWrapped721 = _implementation;
      // Version will not realistically overflow 32 bits.
      ++versionStashWrapped721;
      emit ImplementationUpdated(_implementation, nftType, versionStashWrapped721);
    } else {
      implementationStashWrapped1155 = _implementation;
      // Version will not realistically overflow 32 bits.
      ++versionStashWrapped1155;
      emit ImplementationUpdated(_implementation, nftType, versionStashWrapped721);
    }
  }

  function wrap(
    address nftContract,
    uint256 tokenId,
    uint256 amount
  ) external returns (address wrappedNFTContract) {
    NFTType nftType;
    (nftType, wrappedNFTContract) = _wrapContract(nftContract);
    if (nftType == NFTType.ERC721) {
      IStashWrapped721(wrappedNFTContract).factoryWrap(msg.sender, tokenId);
    } else {
      IStashWrapped1155(wrappedNFTContract).factoryWrap(msg.sender, tokenId, amount);
    }
  }

  function wrapAndSetApprovalForAll(
    address nftContract,
    uint256 tokenId,
    uint256 amount,
    address operator
  ) external returns (address wrappedNFTContract) {
    NFTType nftType;
    (nftType, wrappedNFTContract) = _wrapContract(nftContract);
    if (nftType == NFTType.ERC721) {
      IStashWrapped721(wrappedNFTContract).factoryWrapAndSetApprovalForAll(msg.sender, tokenId, operator);
    } else {
      IStashWrapped1155(wrappedNFTContract).factoryWrapAndSetApprovalForAll(msg.sender, tokenId, amount, operator);
    }
  }

  function _wrapContract(address nftContract) private returns (NFTType nftType, address wrappedNFTContract) {
    (nftType, wrappedNFTContract) = _getWrappedAddress(nftContract);
    // TODO refactor to share logic
    if (nftType == NFTType.ERC721) {
      // TODO: before wrapping we should require address != 0 -- currently does not revert
      if (!wrappedNFTContract.isContract()) {
        // The first time wrap is called for a collection, it needs to be deployed.
        implementationStashWrapped721.cloneDeterministic(_getSalt(nftContract));
        IStashWrapped721(wrappedNFTContract).initialize(nftContract);
        emit Wrapped(nftContract, nftType, wrappedNFTContract);
      }
    } else {
      if (!wrappedNFTContract.isContract()) {
        // The first time wrap is called for a collection, it needs to be deployed.
        implementationStashWrapped1155.cloneDeterministic(_getSalt(nftContract));
        IStashWrapped1155(wrappedNFTContract).initialize(nftContract);
        emit Wrapped(nftContract, nftType, wrappedNFTContract);
      }
    }
  }

  function getWrappedAddress(address nftContract) public view returns (address wrappedNFTContract) {
    (, wrappedNFTContract) = _getWrappedAddress(nftContract);
  }

  function _getWrappedAddress(address nftContract) private view returns (NFTType nftType, address wrappedNFTContract) {
    // TODO is it possible to only do the 165 checks if the contract is not already wrapped? (maybe via storage?)

    // TODO do we want to remove the top-level 165 check? (here and anywhere else we do it currently)
    require(nftContract.supportsERC165(), "NFT contract does not support ERC165");
    // TODO: require the ERC4907 and P2ERoyalties interfaces are supported here as well?
    nftType = nftContract.checkNftType(false);
    if (nftType == NFTType.ERC721) {
      wrappedNFTContract = implementationStashWrapped721.predictDeterministicAddress(_getSalt(nftContract));
    } else {
      wrappedNFTContract = implementationStashWrapped1155.predictDeterministicAddress(_getSalt(nftContract));
    }
  }

  function _getSalt(address nftContract) private pure returns (bytes32) {
    return keccak256(abi.encodePacked(nftContract));
  }
}

