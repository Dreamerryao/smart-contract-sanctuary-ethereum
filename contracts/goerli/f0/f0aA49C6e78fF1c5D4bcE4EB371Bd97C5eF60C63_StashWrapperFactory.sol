pragma solidity ^0.8.12;

import "@openzeppelin/contracts/utils/introspection/ERC165.sol";

import "../interfaces/IP2ERoyalties.sol";

import "../../shared/Constants.sol";

/**
 * @title Play to earn royalties implementation.
 * @author batu-inal & HardlyDifficult
 */
abstract contract P2ERoyalties is IP2ERoyalties, ERC165 {
  uint16 public immutable ownerRoyaltyInBasisPoints;
  uint16 public immutable renterRoyaltyInBasisPoints;
  uint16 public immutable operatorRoyaltyInBasisPoints;

  constructor(uint16 _renterRoyaltyInBasisPoints, uint16 _operatorRoyaltyInBasisPoints) {
    require(
      _renterRoyaltyInBasisPoints + _operatorRoyaltyInBasisPoints < BASIS_POINTS,
      "Royalties must be less than 100%"
    );
    ownerRoyaltyInBasisPoints = BASIS_POINTS - _renterRoyaltyInBasisPoints - _operatorRoyaltyInBasisPoints;
    renterRoyaltyInBasisPoints = _renterRoyaltyInBasisPoints;
    operatorRoyaltyInBasisPoints = _operatorRoyaltyInBasisPoints;
  }

  function getP2ERecipients(address user, uint256 tokenIdOrRecordId)
    public
    view
    returns (Recipient[] memory recipients)
  {
    (address owner, address renter, address operator) = _getUsersOf(user, tokenIdOrRecordId);
    require(user == renter || (renter == address(0) && user == owner), "User is not a renter or owner");

    if (renter != address(0)) {
      recipients = new Recipient[](3);
      recipients[0] = Recipient(owner, ownerRoyaltyInBasisPoints);
      recipients[1] = Recipient(renter, renterRoyaltyInBasisPoints);
      recipients[2] = Recipient(operator, operatorRoyaltyInBasisPoints);
    } else {
      recipients = new Recipient[](1);
      recipients[0] = Recipient(owner, BASIS_POINTS);
    }
  }

  function _getUsersOf(address user, uint256 tokenIdOrRecordId)
    internal
    view
    virtual
    returns (
      address owner,
      address renter,
      address operator
    );

  function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
    if (interfaceId == type(IP2ERoyalties).interfaceId) {
      return true;
    }
    return super.supportsInterface(interfaceId);
  }
}

