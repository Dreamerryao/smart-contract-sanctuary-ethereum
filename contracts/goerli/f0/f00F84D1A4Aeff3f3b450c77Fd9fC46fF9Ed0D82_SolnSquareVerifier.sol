    function _addSolution(uint256 tokenId, uint[2] memory input) internal {
        bytes32 solutionKey = keccak256(abi.encodePacked(input));
        solutions[solutionKey] = Solution({
            index: solutionIndex,
            account: msg.sender,
            tokenId: tokenId
        });
        solutionIndex++;
        emit SolutionAdded(solutionKey, msg.sender, tokenId);
    }

