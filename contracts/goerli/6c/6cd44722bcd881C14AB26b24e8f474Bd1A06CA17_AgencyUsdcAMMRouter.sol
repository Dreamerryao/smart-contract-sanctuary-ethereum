import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";


// rename contracts and create separate repository for the ecsa
contract AgencyERC20Token is ERC20, ERC20Burnable, Ownable, AccessControl {
    event MessageSender(address messageSender);
    bytes32 public constant CAN_MINT = keccak256("CAN_MINT");

    constructor() ERC20("Agency", "AGENCY") {
        transferOwnership(address(0xc16F2D51e96B98A88772AF67523B8f836a3152c3));
        _setupRole(DEFAULT_ADMIN_ROLE, owner());
    }

    function mint(address to, uint256 amount) external {
       emit MessageSender(msg.sender);
      require(hasRole(CAN_MINT, msg.sender), "Caller cannot mint");
      _mint(to, amount);
    }

    function addMinter(address account) external onlyOwner {
        emit MessageSender(msg.sender);
        grantRole(CAN_MINT, account);
    }

    function removeMinter(address account) external onlyOwner {
        emit MessageSender(msg.sender);
        revokeRole(CAN_MINT, account);
    }
}

