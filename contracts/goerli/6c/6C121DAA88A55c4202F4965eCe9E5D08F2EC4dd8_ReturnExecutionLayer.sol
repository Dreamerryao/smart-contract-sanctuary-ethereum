    // User purchases NFT
    function buyNFT(uint amount, uint offsetPercentage, uint afterOffset, address offsetToken, address paymentToken) external {
        require(validPaymentTokens[paymentToken]);
        require(IERC20(paymentToken).transferFrom(msg.sender, address(this), amount), "Failed to transfer payment"); // Transfer Funds

        (bool exists, uint id) = DATA_LAYER.tryGetUserSBT(msg.sender);
        if (!exists) { // User has no SBT
            id = _mintSBT(msg.sender); // mint sbt
        }

        uint tokenId = _mintNFT(msg.sender, amount * DATA_LAYER.AMOUNT_MULTIPLIER(), offsetPercentage, offsetToken, paymentToken, afterOffset); // Mint NFT

        DATA_LAYER.addNFTtoSBT(id, tokenId);

        _stake(tokenId, amount, paymentToken); // Stake Amount
    }

    // Add Additional funds to NFT
    function _fundNFT(uint nftID, uint amount, address paymentToken) external {
        require(RETURN_NFT.ownerOf(nftID) == msg.sender, "NFT not owned by user");

        require(validPaymentTokens[paymentToken]);

        require(IERC20(paymentToken).transferFrom(msg.sender, address(this), amount), "Failed to transfer payment"); // Transfer Funds

        _updateStake(nftID, amount, true, paymentToken); // Stake Amount, stake info should be updated in this function

        emit FundNFT(amount, nftID, msg.sender);

    }

    function _withdrawFunds(uint nftID, uint amount, address paymentToken) external {
        require(RETURN_NFT.ownerOf(nftID) == msg.sender, "NFT not owned by user");

        _updateStake(nftID, amount, false, paymentToken);

        require(IERC20(paymentToken).transfer(msg.sender, amount), "Transfer Failed"); // Transfer Funds
    }

    function _mintSBT(address user) internal returns(uint) {
        // mint sbt to user
        uint newID = RETURN_SBT.testMint(user);
        // Update user -> SBT in data layer
        DATA_LAYER.setUserSBT(msg.sender, newID);
        DATA_LAYER.setSBTToUser(newID, user);

        emit MintSBT(newID, user);

        return newID;
    }

    function _mintNFT(address user, uint _amount, uint _percentage, address _offsetToken, address _paymentToken, uint _afterOffset) internal returns(uint) {
        uint sbtID = DATA_LAYER.getUserSBT(user); // Normal get, should revert if user doesn't have an sbt
        // mint nft to user
        uint nftID = RETURN_NFT.testMint(user);

        // TODO: These mappings may cause trouble in the case when users transfer/trade nft's
        //       Potential Solutions:
        //       1. Require people to re-register an nft when they purchased one on the secondary market
        //       2. Remove nft to sbt mapping, instead query nft --> owner --> sbt. Update sbt --> nft to be sbt --> registered nft?
        //       3. Build sbt minting functionality and update the data layer within the nft transfer function (my favorite?)
        //       4. Remove both nft -> sbt and sbt -> nft[] mappings, rely on only dealing with nft's and querying ownerOf
        // Update NFT -> SBT in data layer
        DATA_LAYER.setNFTToSBT(sbtID, nftID);
        // Update SBT -> NFT[] in data layer
        DATA_LAYER.addNFTtoSBT(sbtID, nftID);

        // Write Offset Info
        DATA_LAYER.setNFTOffsetInfo(nftID, _amount, _percentage, _paymentToken, _offsetToken, _afterOffset);

        // uint nftID, uint sbtID, uint amount, uint offsetPercentage, uint afterOffset, address offsetToken, address user
        emit MintNFT(nftID, sbtID, _amount, _percentage, _afterOffset, _offsetToken, user);

        return nftID;

    }

    // User Purchasing new nft creates new stake
    function _stake(uint nftID, uint amount, address paymentToken) internal {
        // Update yield and calculate correction
        updateYield(paymentToken);
        //testUpdateYield(paymentToken);

        // set correction index (no longer + 1 since we push beforehand)
        if (nftID == 0) {
            DATA_LAYER.setCorrectionIndex(nftID, 0);
        }
        else {
            DATA_LAYER.setCorrectionIndex(nftID, DATA_LAYER.getYieldPerShareLength(paymentToken));
        }
        // deposit
        aaveSupply(amount, paymentToken);

    }

    // Update stake corresponding to an existing NFT
    function _updateStake(uint nftID, uint amount, bool add, address paymentToken) internal {
        // Update yield and calculate YPS & AYPS
        updateYield(paymentToken);
        //testUpdateYield(paymentToken);

        // Get nft earnings up to this point
        (uint total, uint yield) = DATA_LAYER.getTotals(nftID);


        if (!add) { // User removing from stake
            require(total >= amount * DATA_LAYER.AMOUNT_MULTIPLIER(), "You cant unstake more than your balance");
            if (total / DATA_LAYER.AMOUNT_MULTIPLIER() == amount) { // User unstaking entire stake (including yield) (round a little here)
                DATA_LAYER.setNFTOffsetLoadedAmount(nftID, 0);
                yield = 0;
            }
            else if (total - yield < (amount * DATA_LAYER.AMOUNT_MULTIPLIER())) { // unstaking more than their initial balance
                uint newStake = total - (amount * DATA_LAYER.AMOUNT_MULTIPLIER());
                DATA_LAYER.setNFTOffsetLoadedAmount(nftID, newStake);
                yield = 0;
            }
            else {
                DATA_LAYER.setNFTOffsetLoadedAmount(nftID, total - (amount * DATA_LAYER.AMOUNT_MULTIPLIER()) - yield);
            }

            aaveWithdraw(amount, paymentToken);
        }
        else {
            DATA_LAYER.setNFTOffsetLoadedAmount(nftID, total + (amount * DATA_LAYER.AMOUNT_MULTIPLIER()) - yield);
            aaveSupply(amount, paymentToken);
        }
        DATA_LAYER.setCorrectionIndex(nftID, DATA_LAYER.getYieldPerShareLength(paymentToken));
        DATA_LAYER.setYieldEarn(nftID, yield, false);

        emit UpdateStake(nftID, amount, total, yield, total + (amount * DATA_LAYER.AMOUNT_MULTIPLIER()) - yield);

    }

