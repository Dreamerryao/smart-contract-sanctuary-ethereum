    function updateYield(address paymentToken) internal {
        address aaveToken = DATA_LAYER.tokenToAAVEToken(paymentToken);
        // 1. Claim All Pending Rewards
        uint balanceBefore = DATA_LAYER.poolAmounts(paymentToken);
        uint yield = (IERC20(aaveToken).balanceOf(address(this)) - balanceBefore) * DATA_LAYER.AMOUNT_MULTIPLIER();

        // 2. Update yieldPerShare // TODO: if yield > 0
        //uint yps = (_simulatedYield * YPS_MULTIPLIER) / totalStakedAmount;

        uint yieldPerShare;
        if (DATA_LAYER.poolAmounts(paymentToken) == 0) {
            yieldPerShare = 0;
        }
        else {
            yieldPerShare = yield * DATA_LAYER.YPS_MULTIPLIER() / DATA_LAYER.poolAmounts(paymentToken);
        }

        DATA_LAYER.updateYieldPerShare(paymentToken, yieldPerShare);

        //3. Update adjustedYieldPerShare
        uint ypsLength = DATA_LAYER.getYieldPerShareLength(paymentToken);
        if (ypsLength == 1) {
            DATA_LAYER.updateAdjustedYieldPerShare(paymentToken, yieldPerShare + DATA_LAYER.YPS_MULTIPLIER());
        }
        else {
            uint aYPS = (DATA_LAYER.adjustedYieldPerShare(paymentToken, DATA_LAYER.getAdjustedYieldPerShareLength(paymentToken) - 1) * (yieldPerShare + DATA_LAYER.YPS_MULTIPLIER())) / DATA_LAYER.YPS_MULTIPLIER();
            DATA_LAYER.updateAdjustedYieldPerShare(paymentToken, aYPS);
        }

        // 4. Update Total Staked and Pool Staked to Reflect Yield
        DATA_LAYER.setTotalStakedAmount(yield, true);
        DATA_LAYER.updatePoolAmounts(yield, paymentToken, true);
    }


    function _setAAVEPool(address _poolAddress) internal {
        aavePoolManager = IPool(_poolAddress);
    }

    function setAAVEPool(address _poolAddress) public onlyRole(EXECUTOR_ROLE) {
        _setAAVEPool(_poolAddress);
    }

    function aaveWithdraw(uint amount, address paymentToken) internal {
        aavePoolManager.withdraw(paymentToken, amount, address(this)); // TODO: Probably more efficient to just pass withdrawal address


        DATA_LAYER.updatePoolAmounts(amount * DATA_LAYER.AMOUNT_MULTIPLIER(), paymentToken, false);

        DATA_LAYER.setTotalStakedAmount(amount * DATA_LAYER.AMOUNT_MULTIPLIER(), false);
        emit Unstake(paymentToken, DATA_LAYER.tokenToAAVEToken(paymentToken), amount);
    }

    function aaveSupply(uint amount, address paymentToken) internal {
        IERC20(paymentToken).approve(address(aavePoolManager), amount);
        aavePoolManager.supply(paymentToken, amount, address(this), 0);

        DATA_LAYER.setTotalStakedAmount(amount * DATA_LAYER.AMOUNT_MULTIPLIER(), true);
        DATA_LAYER.updatePoolAmounts(amount * DATA_LAYER.AMOUNT_MULTIPLIER(), paymentToken, true);
        emit Stake(paymentToken, DATA_LAYER.tokenToAAVEToken(paymentToken), amount);
    }

    function swapTokenForToken(uint amountIn, uint amountOut, address[] calldata path) internal returns(uint) {
        IERC20(path[0]).approve(address(sushiRouter), amountIn);
        uint amount = IERC20(path[path.length - 1]).balanceOf(address(this));
        sushiRouter.swapExactTokensForTokens(
            amountIn,
            amountOut,
            path,
            address(this),
            block.timestamp
        );
        amount = IERC20(path[path.length - 1]).balanceOf(address(this)) - amount;
        emit Swap(path[0], path[path.length - 1], amountIn, amount);
        return amount;
    }

    function purchaseOffset(uint amountToSpend, uint amountTokenExpected, address[] calldata path) external onlyRole(EXECUTOR_ROLE) returns (uint) {
        updateYield(path[0]);
        // testUpdateYield(path[0]);
        aaveWithdraw(amountToSpend, path[0]);
        uint received = swapTokenForToken(amountToSpend, amountTokenExpected, path);
        // set total offset amount
        // any type of timestamp stuff
        return received;
        // TODO: EMIT EVENT HERE
    }

    function processBatchOffsets(uint[] calldata nftIDs, uint[] calldata offsetCost) external onlyRole(EXECUTOR_ROLE) {
        for (uint i = 0; i < nftIDs.length; i++) {
            DATA_LAYER.updateYieldSpend(nftIDs[i], offsetCost[i]);
        }
    }


    // TODO: Rebalancing logic, do we want this on chain or called from the backend?
    // pools: array of pool addresses to stake
    // ratios: percentage of total TVL to be staked in each pool (in basis points)
    function rebalanceStakes(address[] calldata pools, uint[] calldata ratios) external onlyRole(EXECUTOR_ROLE) {
        //updateYield();

        // 3. Restake following rebalancing logic
        // TODO: Option 1: Unstake all and restake according to params, much simpler but will most likely result in extra unnecessary work being done
