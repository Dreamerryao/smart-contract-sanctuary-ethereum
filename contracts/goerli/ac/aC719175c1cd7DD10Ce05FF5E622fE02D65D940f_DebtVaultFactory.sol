pragma solidity >=0.8.0 <=0.9.0;

import "./Administrable.sol";
import "./Globals.sol";
import "./utils/Helper.sol";
import "./utils/CloneFactory.sol";
import "./interfaces/IDebtVaultInitializable.sol";
import "./interfaces/IDebtVault.sol";
import "./interfaces/IAdministrable.sol";
import "./interfaces/IPermissionControl.sol";
import "./interfaces/IOperationsProxy.sol";
import "./openzeppelin/Clones.sol";
import "./DebtVault.sol";

contract DebtVaultFactory is Administrable, CloneFactory {
    using Clones for address;

    struct Record {
        IDebtVault debtVault;
        address prototype;
        uint256 registered;
        uint256 defuncted;
    }

    struct DebtVaultParams {
        string name;
        string symbol;
        string vaultName;
        address owner;
    }

    struct DebtVaultInfo {
        address addr;
        string vaultName;
        Globals.TotalValueLocked tvlUsd;
        ConfigInfo[] configs;
    }

    struct ConfigInfo {
        uint256 id;
        uint256 lockupPeriod;
        uint256 minAmount;
        IERC20 depositedIn;
    }

    event DebtVaultCreated(
        IDebtVaultInitializable indexed addr,
        string name,
        uint256 index
    );

    address public prototype;
    Record[] _registry;
    uint256 _index;
    IPermissionControl public permissionControl;
    event Cloned(address addr);

    constructor(address __prototype, address permissionContralAddress)
        Administrable()
    {
        prototype = __prototype;
        permissionControl = IPermissionControl(permissionContralAddress);
    }

    function updatePrototype(address __prototype) external onlyOwner {
        prototype = __prototype;
    }

    function createDebtVault(
        address[] memory depositTokens,
        IDebtVault.Config[] memory configs,
        DebtVaultParams memory vars
    ) external onlyOwner returns (IDebtVaultInitializable newDebtVault) {
        require(
            _isWhitelisted(vars.owner) || vars.owner == address(this),
            "DebtVault:Must be whitelisted"
        );
        address newDebtVaultAddress = createClone(prototype);
        DebtVault prototype = DebtVault(prototype);
        newDebtVault = IDebtVaultInitializable(newDebtVaultAddress);


        newDebtVault.initialize(
            address(this),
            vars.name,
            vars.symbol,
            vars.vaultName,
            depositTokens,
            configs
        );

        newDebtVault.setOperationsProxy(prototype.operationsProxy.address);
        newDebtVault.setPermissionControl(prototype.permissionControl.address);
        newDebtVault.setSwapStableCoin(IERC20(prototype.swapStableCoin.address));
        newDebtVault.setUniswapRouter(prototype.getUniswapRouter());
        

        IAdministrable(newDebtVaultAddress).transferOwnership(vars.owner);
        _registry.push(
            _createRecord(newDebtVault, address(prototype), block.timestamp, 0)
        );
        emit DebtVaultCreated(newDebtVault, vars.vaultName, _index);
    }

    function _createRecord(
        IDebtVaultInitializable debtVault,
        address prototype,
        uint256 registered,
        uint256 defuncted
    ) internal pure returns (Record memory) {
        return Record(debtVault, prototype, registered, defuncted);
    }

    function registerDebtVault(Record memory record)
        external
        onlyOwner
        returns (uint256)
    {
        _registry.push(record);
        return _index;
    }

    function isRecordActive(Record memory record) public view returns (bool) {
        return record.registered > 0 && record.defuncted < block.timestamp;
    }

    function getActiveDebtVaults()
        public
        view
        returns (Record[] memory active)
    {
        uint256 activeCount = 0;
        for (uint256 i = 0; i < _registry.length; i++) {
            if (isRecordActive(_registry[i])) {
                activeCount++;
            }
        }

        active = new Record[](activeCount);
        uint256 c = 0;
        for (uint256 i = 0; i < _registry.length; i++) {
            Record memory r = _registry[i];
            if (isRecordActive(r)) {
                active[c] = r;
                c++;
            }
        }
    }

    function getActiveDebtValutsInfo()
        public
        view
        returns (DebtVaultInfo[] memory)
    {
        Record[] memory active = getActiveDebtVaults();
        DebtVaultInfo[] memory vaults = new DebtVaultInfo[](active.length);
        for (uint256 i = 0; i < vaults.length; i++) {
            vaults[i] = createDebtValutInfo(active[i].debtVault);
        }
        return vaults;
    }

    function createDebtValutInfo(IDebtVault debtVault)
        public
        view
        returns (DebtVaultInfo memory)
    {
        IDebtVault.Config[] memory configs = debtVault.getAllConfigs();
        ConfigInfo[] memory configInfos = new ConfigInfo[](configs.length);
        for (uint256 i = 0; i < configInfos.length; i++) {
            configInfos[i] = _createConfigInfo(configs[i], i);
        }

        return
            DebtVaultInfo(
                address(debtVault),
                debtVault.getVaultName(),
                debtVault.getTotalValueLockedUSD(),
                configInfos
            );
    }

    function _createConfigInfo(IDebtVault.Config memory config, uint256 id)
        internal
        pure
        returns (ConfigInfo memory)
    {
        return
            ConfigInfo(
                id,
                config.lockupPeriod,
                config.minAmount,
                config.depositedIn
            );
    }

    function _isWhitelisted(address addr) internal view returns (bool) {
        require(
            address(permissionControl) != address(0),
            "DebtVault: Permission Control is not set"
        );
        return
            permissionControl.getStatus(addr) == Globals.UserStatus.WHITELISTED;
    }

    function setPermissionControl(address addr) public onlyAdmin {
        require(
            addr != address(0),
            "DebtVault: PermissionControl at zero address"
        );
        permissionControl = IPermissionControl(addr);
    }
}

