        IERC20 token,
        uint256 amount,
        uint256 configId
    ) public override onlyWhitelisted returns (uint256) {
        Config memory config = _configs[configId];
        _contribute(_msgSender(), token, amount, config, 0);
    }

    function contributeIDO(
        address addr,
        IERC20 token,
        uint256 amount,
        uint256 configId,
        uint256 timestamp
    ) public override onlyAdmin returns (uint256) {
        require(_isWhitelisted(addr), "DebtVault:User is not whitelisted.");
        Config memory config = _configs[configId];
        return _contribute(addr, token, amount, config, timestamp);
    }

    function contributeAndSwap(
        IERC20 token,
        uint256 amount,
        uint256 configId
    ) public override returns (uint256 id) {
        require(
            address(swapStableCoin) != address(0),
            "DebtVault:swapStableCoin not set."
        );
        Config memory config = _configs[configId];
        id = _contribute(_msgSender(), token, amount, config, block.timestamp);

        token.approve(address(operationsProxy.getUniswapRouter()), amount);

        uint256[] memory amounts = swapTokens(token, swapStableCoin, amount, 0);
        _contributions[id].amountUSD = amounts[0];
    }

    function _contribute(
        address addr,
        IERC20 token,
        uint256 amount,
        Config memory config,
        uint256 timestamp
    ) internal returns (uint256 id) {
        require(!_locked, "DebtVault: Locked");

        require(
            amount > 0 && config.minAmount <= amount,
            "DebtVault:Insufficient amount"
        );
        require(token == config.depositedIn, "DebtVault:Unsupported token");
        _shouldLock(addr, token, amount, config);

        id = getId();
        _contributions[id] = _buildContribution(config, amount, timestamp);
        _contributionsLookup[addr].push(id);
        _holders.push(addr);
        _increaseTVL(token, amount);
        emit Contributed(addr, address(token), amount);
        emit ContributedId(addr, id);
        _safeMint(addr, id);
        _takeFee(config, amount);
        _transferFromERC20(addr, address(this), amount, address(token));
    }

    function _shouldLock(
        address addr,
        IERC20 token,
        uint256 amount,
        Config memory config
    ) internal view returns (bool) {
        return _locked;
    }

    function _takeFee(Config memory config, uint256 amount)
        internal
        returns (uint256)
    {
        return operationsProxy.takeFee(amount);
    }

    function _buildContribution(
        Config memory config,
        uint256 amount,
        uint256 timestamp
    ) internal view returns (Globals.ContributionLite memory) {
        return
            Globals.ContributionLite(
                amount,
                operationsProxy.denominateInUsd(config.depositedIn, amount),
                timestamp,
                config.lockupPeriod,
                config.depositedIn,
                false
            );
    }

    function _transferFromERC20(
        address from,
        address to,
        uint256 amount,
        address token
    ) internal {
        require(_isSupportedToken(token), "DebtVault:Token not supported");
        IERC20 erc20 = IERC20(token);
        erc20.safeTransferFrom(from, to, amount);
    }

    function _transferERC20(
        address to,
        uint256 amount,
        address token
    ) internal {
        require(_isSupportedToken(token), "DebtVault:Token not supported");
        IERC20 erc20 = IERC20(token);
        erc20.safeTransfer(to, amount);
    }

    function _isSupportedToken(address token) internal view returns (bool) {
        address[] memory tmpTokens = new address[](_tokens.length);
        for (uint256 i = 0; i < tmpTokens.length; i++) {
            tmpTokens[i] = address(_tokens[i]);
        }
        return Helper.doesArrayContainValueAddress(tmpTokens, token);
    }

    function getId() public override onlyWhitelisted returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.difficulty,
                        block.timestamp,
                        _msgSender(),
                        ++nonce
                    )
                )
            );
    }

    function redeem(uint256 contributionId, address to)
        public
        override
        onlyWhitelisted
    {
        Globals.ContributionLite memory contribution = _contributions[
            contributionId
        ];
        _preRedeem(contributionId, to, contribution);
        _transferERC20(
            to,
            contribution.amount,
            address(contribution.depositedIn)
        );
    }

    function redeemAndSwap(uint256 contributionId, address to)
        public
        override
        onlyWhitelisted
    {
        require(
            address(swapStableCoin) != address(0),
            "DebtVault:swapStableCoin not set."
        );

        Globals.ContributionLite memory contribution = _contributions[
            contributionId
        ];
        uint256 usdValue = contribution.amountUSD;
        require(usdValue > 0, "DebtVault:Not swapable");
        require(
            swapStableCoin.balanceOf(address(this)) > usdValue,
            "DebtVault:Insufficient swap balance"
        );
        swapStableCoin.approve(address(operationsProxy), usdValue);
        uint256[] memory amounts = swapTokens(
            swapStableCoin,
            contribution.depositedIn,
            usdValue,
            0
        );
        _contributions[contributionId].amount = amounts[0];
        redeem(contributionId, to);
    }

    function partiallyRedeem(
        uint256 contributionId,
        uint256 amount,
        address to
    ) public override onlyWhitelisted returns (uint256) {
        Globals.ContributionLite memory contribution = _contributions[
            contributionId
        ];
        require(
            amount < contribution.amount,
            "DebtValut: Amount must be less then contributed"
        );
        _preRedeem(contributionId, to, contribution);
        uint256 newAmount = contribution.amount - amount;
        _transferERC20(to, amount, address(contribution.depositedIn));
        Config memory config = Config(
            contribution.lockupPeriod,
            0,
            contribution.depositedIn
        );
        return
            _contribute(
                _msgSender(),
                contribution.depositedIn,
                newAmount,
                config,
                0
            );
    }

    function _preRedeem(
        uint256 contributionId,
        address to,
        Globals.ContributionLite memory contribution
    ) internal {
        _checkRedemptionContditions(contributionId, to, contribution);
        uint256[] memory userContributions = _contributionsLookup[_msgSender()];
        userContributions = Helper.removeFirstFromArrayByValue(
            userContributions,
            contributionId
        );
        _contributionsLookup[_msgSender()] = userContributions;
        delete _contributions[contributionId];
        _decreaseTVL(contribution.depositedIn, contribution.amount);
        emit Redeemed(
            to,
            address(contribution.depositedIn),
            contribution.amount
        );
        _burn(contributionId);
    }

    function _checkRedemptionContditions(
        uint256 contributionId,
        address to,
        Globals.ContributionLite memory contribution
    ) internal view returns (bool) {
        require(
            balanceOf(_msgSender()) >= 1 &&
                ownerOf(contributionId) == _msgSender(),
            "DebtVault:Caller is not owner."
        );
        require(
            !contribution.locked,
            "DebtVault: Contribution is temporary locked."
        );
        require(
            contribution.burnableAfter > 0 &&
                block.timestamp > contribution.burnableAfter,
            "DebtVault: Contribution is not yet redeemable"
        );
        require(_isWhitelisted(to), "DebtVault: Receiver must be whitelisted");
        return true;
    }

    function announceRedemption(uint256 contributionId)
        external
        override
        onlyWhitelisted
    {
        require(_checkAnnouncmentConditions(contributionId));
        _contributions[contributionId].burnableAfter = Helper.addDuration(
            block.timestamp,
            _contributions[contributionId].lockupPeriod
        );
    }

    function _checkAnnouncmentConditions(uint256 contributionId)
        internal
        view
        returns (bool)
    {
        require(
            ownerOf(contributionId) == _msgSender(),
            "DebtValut: Must be token owner"
        );
        Globals.ContributionLite memory contribution = _contributions[
            contributionId
        ];
        require(
            contribution.burnableAfter == 0,
            "DebtVault: Contribution already announced"
        );
        return true;
    }

    function withdraw(
        address addr,
        IERC20 token,
        uint256 amount
    ) public override onlyAdmin {
        emit Withdrawn(addr, address(token), amount);
        _transferERC20(addr, amount, address(token));
    }

    function transfer(
        address from,
        address to,
        uint256 id
    ) public override onlyWhitelisted {
        require(_isWhitelisted(to), "DebtVault: Receiver must be whitelisted");
        uint256[] memory contribtions = _contributionsLookup[from];
        contribtions = Helper.removeFirstFromArrayByValue(contribtions, id);
        _contributionsLookup[to].push(id);
        _contributionsLookup[to] = contribtions;
        emit ContributionTransferred(from, to, id);
        safeTransferFrom(from, to, id);
    }

    function getContributionsByAddress(address addr)
        external
        view
        returns (uint256[] memory userContributions)
    {
        userContributions = new uint256[](_contributionsLookup[addr].length);
        for (uint256 i = 0; i < _contributionsLookup[addr].length; i++) {
            userContributions[i] = _contributionsLookup[addr][i];
        }
    }

    function _isWhitelisted(address addr) internal view returns (bool) {
        require(
            address(permissionControl) != address(0),
            "DebtVault: Permission Control is not set"
        );
        return
            permissionControl.getStatus(addr) == Globals.UserStatus.WHITELISTED;
    }

    function reportComprmised(address addr)
        external
        override
        onlyOperator
        returns (bool)
    {
        return permissionControl.limit(addr);
    }

    function addConfig(uint256 id, Config calldata conf)
        external
        override
        onlyAdmin
        returns (bool)
    {
        if (id > _configs.length) {
            _configs.push(conf);
        } else {
            _configs[id] = conf;
        }

        Globals.StateOperation op = address(conf.depositedIn) != address(0)
            ? Globals.StateOperation.UPDATE
            : Globals.StateOperation.ADD;
        emit ConfigChanged(uint256(op), id, address(_msgSender()));
        return true;
    }

    function addConfigs(Config[] memory configs) public onlyAdmin {
        delete _configs;
        for (uint256 i = 0; i < configs.length; i++) {
            _configs.push(configs[i]);
        }
    }

    function removeConfig(uint256 id)
        external
        override
        onlyAdmin
        returns (bool)
    {
        delete _configs[id];
        emit ConfigChanged(
            uint256(Globals.StateOperation.REMOVE),
            id,
            address(_msgSender())
        );

        return true;
    }

    function getConfig(uint256 id)
        public
        view
        override
        returns (Config memory)
    {
        return _configs[id];
    }

    function getAllConfigs()
        public
        view
        override
        returns (Config[] memory configs)
    {
        configs = new Config[](_configs.length);
        for (uint256 i = 0; i < configs.length; i++) {
            configs[i] = _configs[i];
        }
    }

    function setPermissionControl(address addr) public override onlyAdmin {
        require(
            addr != address(0),
            "DebtVault: PermissionControl at zero address"
        );
        permissionControl = IPermissionControl(addr);
    }

    function setOperationsProxy(address addr) public override onlyAdmin {
        require(addr != address(0), "DebtVault: proxy at zero address");
        operationsProxy = IOperationsProxy(addr);
    }

    function getContribution(uint256 contributionId)
        public
        view
        override
        returns (Globals.ContributionLite memory)
    {
        return _contributions[contributionId];
    }

    function setContributionLockedStatus(uint256 contributionId, bool status)
        external
        override
        onlyAdmin
        returns (bool)
    {
        require(
            _contributions[contributionId].amount > 0,
            "DebtVault:Wrong contribution id."
        );
        _contributions[contributionId].locked = status;
        return status;
    }

    function getVaultName() external view override returns (string memory) {
        return _vaultName;
    }

    function _increaseTVL(IERC20 token, uint256 amount) internal {
        Globals.TotalValueLocked storage tvl = _totalValueLockedPerToken[token];
        tvl.current += amount;
        tvl.allTime += amount;
    }

    function _decreaseTVL(IERC20 token, uint256 amount) internal {
        Globals.TotalValueLocked storage tvl = _totalValueLockedPerToken[token];
        tvl.current -= amount;
    }

    function getTotalValueLocked(IERC20 token)
        external
        view
        override
        returns (Globals.TotalValueLocked memory tvl)
    {
        tvl = _totalValueLockedPerToken[token];
    }

    function getTotalValueLockedUSD()
        external
        view
        override
        returns (Globals.TotalValueLocked memory tvlAll)
    {
        for (uint256 i = 0; i < _tokens.length; i++) {
            Globals.TotalValueLocked
                memory tokenTvl = _totalValueLockedPerToken[_tokens[i]];
            tvlAll.current += operationsProxy.denominateInUsd(
                _tokens[i],
                tokenTvl.current
            );
            tvlAll.allTime += operationsProxy.denominateInUsd(
                _tokens[i],
                tokenTvl.allTime
            );
        }
    }

    function setSwapStableCoin(IERC20 swapCoin) external override onlyAdmin {
        swapStableCoin = swapCoin;
    }

    function getUniswapRouter()
        external
        view
        override
        returns (IUniswapV2Router)
    {
        return _router;
    }

    function setUniswapRouter(IUniswapV2Router router) external override {
        _router = router;
    }

    function swapTokens(
        IERC20 fromToken,
        IERC20 toToken,
        uint256 amountIn,
        uint256 amountOutMin
    ) public override returns (uint256[] memory) {
        require(address(_router) != address(0), "DebtVault: Router not set");
        address[] memory path = new address[](2);
        path[0] = address(fromToken);
        path[1] = address(toToken);
        fromToken.approve(address(_router), amountIn);
        return
            _router.swapExactTokensForTokens(
                amountIn,
                amountOutMin,
                path,
                address(this),
                block.timestamp + 60 * 5 seconds
            );
    }
}

