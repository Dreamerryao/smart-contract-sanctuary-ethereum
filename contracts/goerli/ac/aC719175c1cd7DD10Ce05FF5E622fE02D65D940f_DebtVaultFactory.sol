import "./Administrable.sol";
import "./interfaces/IPermissionControl.sol";
import "./interfaces/IOperationsProxy.sol";
import "./interfaces/IDebtVaultInitializable.sol";
import "./openzeppelin/IERC20.sol";
import "./Globals.sol";
import "./openzeppelin/SafeERC20.sol";
import "./openzeppelin/ERC721/ERC721Lite.sol";
import "./openzeppelin/Initializable.sol";
import "./utils/Helper.sol";
import "hardhat/console.sol";
import "./uniswap/IUniswapV2Router.sol";

contract DebtVault is
    IDebtVaultInitializable,
    Administrable,
    ERC721Lite,
    Initializable
{
    using SafeERC20 for IERC20;
    modifier onlyWhitelisted() {
        require(
            _isWhitelisted(_msgSender()),
            "DebtVault:User is not whitelisted."
        );
        _;
    }

    IPermissionControl public permissionControl;
    IOperationsProxy public operationsProxy;
    IUniswapV2Router _router;
    uint256 periodicity = 7; //in days
    int256 nonce;
    uint256 tokenCount;
    IERC20 public swapStableCoin;
    bool _locked = false;
    mapping(IERC20 => Globals.TotalValueLocked) _totalValueLockedPerToken;
    IERC20[] _tokens;
    Config[] _configs;
    mapping(uint256 => Globals.ContributionLite) _contributions;
    mapping(address => uint256[]) _contributionsLookup;

    string _vaultName;

    address[] _holders;

    constructor(address permissionControl_, address operationsProxy_)
        ERC721Lite("PLACE_HOLDER", "PH")
    {
        permissionControl = IPermissionControl(permissionControl_);
        operationsProxy = IOperationsProxy(operationsProxy_);
    }

    function initialize(
        address owner_,
        string memory name_,
        string memory symbol_,
        string memory vaultName_,
        address[] memory depositTokens,
        Config[] memory configs
    ) public override initializer {
        _owner = owner_;
        _addAdmin(owner_);
        _name = name_;
        _symbol = symbol_;
        _vaultName = vaultName_;
        for (uint256 i = 0; i < depositTokens.length; i++) {
            _tokens.push(IERC20(depositTokens[i]));
        }
        addConfigs(configs);
        tokenCount = depositTokens.length;
    }

