pragma solidity >=0.8.0 <=0.9.0;

library Helper {
    function addDuration(uint256 timestamp, uint256 duration)
        public
        pure
        returns (uint256)
    {
        return timestamp + duration * 1 seconds;
    }

    function removeFirstFromArrayByValue(uint256[] memory array, uint256 value)
        public
        pure
        returns (uint256[] memory)
    {
        uint256 index = getFirstIndexByValue(array, value);
        uint256 tmp = array[array.length - 1];
        array[array.length - 1] = array[index];
        array[index] = tmp;
        uint256[] memory newArray = new uint256[](array.length - 1);
        for (uint256 i = 0; i < newArray.length; i++) {
            newArray[i] = array[i];
        }
        return newArray;
    }

    function getFirstIndexByValue(uint256[] memory array, uint256 value)
        public
        pure
        returns (uint256)
    {
        for (uint256 i = 0; i < array.length; i++) {
            if (array[i] == value) {
                return i;
            }
        }
        return array.length;
    }

    function doesArrayContainValue(uint256[] calldata array, uint256 value)
        public
        pure
        returns (bool)
    {
        return getFirstIndexByValue(array, value) != array.length;
    }

    function getFirstIndexByValueAddress(address[] memory array, address value)
        public
        pure
        returns (uint256)
    {
        for (uint256 i = 0; i < array.length; i++) {
            if (array[i] == value) {
                return i;
            }
        }
        return array.length;
    }

    function doesArrayContainValueAddress(
        address[] calldata array,
        address value
    ) public pure returns (bool) {
        return getFirstIndexByValueAddress(array, value) != array.length;
    }
}

