        address owner,
        address from,
        address to,
        uint tokenId
    ) private {
        require(from == owner, "not owner");

        _approve(owner, address(0), tokenId);

        balances[from] -= 1;
        balances[to] += 1;
        owners[tokenId] = to;

        emit Transfer(from, to, tokenId);
    }

