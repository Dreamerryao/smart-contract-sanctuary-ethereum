import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract BlockBlog is ERC20 {
    // 博文
    enum BlogType { // 博文类型
        New,
        Comment,
        Retweet
    }
    struct Blog {
        address Author; // 作者
        uint CoinCount; // 热度
        uint EntryIndex; // 所处话题
        uint Love; // 点赞
        BlogType BlogType; // 类型
        uint FatherBlogIndex; // 父博文
        uint[] ChildBlogIndex; // 子博文
        string Content; // 内容
        uint TimeStamp; // 时间戳
    }
    Blog[] public Blogs; // 第一个没有话题

    // 话题
    struct Entry {
        string EntryWord;
        uint[] Blogs; // 涉及的博文
        uint CoinCount;
    }
    Entry[] Entries; // 话题

    // 用户
    struct User {
        string Name;
        address[] Following;
        address[] Follower;
        uint[] Blogs;
        uint CoinCount;
    }
    mapping(address => User) Users;

    uint MinPublicBlogCoinCount = 10;


    // 手续费
    uint PublishBlogFee;
    address FeeAddress;

    constructor(uint256 initialSupply,address _feeAddress) ERC20("BBG", "BBG") {
        _mint(msg.sender, initialSupply);
        PublishBlogFee = 10;
        FeeAddress = _feeAddress;

        // 创建无标题博文
        publishBlog(" ",BlogType.New,"For Freedom",0,0);
    }

    function getEntry(uint index) public view returns (Entry memory){
        require(index < Entries.length,"index err");
        return Entries[index];
    }
    function getEntryBlogIndex(uint index,uint blogIndex) public view returns (uint){
        require(index < Entries.length,"index err");
        require(blogIndex < Entries[index].Blogs.length,"blogIndex err");
        return Entries[index].Blogs[blogIndex];
    }

    function getUser(address addr) public view returns(User memory){
        return Users[addr];
    }

    function getBlogChildIndex(uint index,uint childIndex)public view returns (uint){
        require(index < Blogs.length,"index err");
        require(childIndex < Blogs[index].ChildBlogIndex.length,"childIndex err");
        return Blogs[index].ChildBlogIndex[childIndex];
    }

    function love(uint blogIndex) public {
        require(blogIndex < Blogs.length,"blog err");

        uint maxCoin = 5;
        uint feeCoin = (maxCoin * 2) /5;
        Blogs[blogIndex].Love += 1;

        uint flagIndex = blogIndex;
        for(uint i = 0 ;i< type(uint).max;i++){
            Blogs[flagIndex].CoinCount += maxCoin;
            if(Blogs[flagIndex].BlogType == BlogType.New){
                break;
            }
            flagIndex = Blogs[flagIndex].FatherBlogIndex;
        }

        Entries[Blogs[blogIndex].EntryIndex].CoinCount+=maxCoin;
        Users[Blogs[blogIndex].Author].CoinCount+=maxCoin;

        transfer(FeeAddress,feeCoin);
        transfer(Blogs[blogIndex].Author,maxCoin-feeCoin);
    }

    function publishBlog(string memory entryWord, BlogType blogType, string memory content,uint coinCount,uint FatherBlogIndex) public {
        if(blogType != BlogType.New && FatherBlogIndex == 0){
            revert("not new blog need father blog");
        }
        if(blogType == BlogType.New && coinCount != 0){
            revert("the new blog not need extra fee");
        }
        if(blogType != BlogType.New && keccak256(abi.encodePacked(entryWord)) != keccak256(abi.encodePacked(""))){
            revert("not new blog has entry");
        }
        if(blogType != BlogType.New && coinCount <= MinPublicBlogCoinCount){
            revert("coin count err");
        }
        if(FatherBlogIndex !=0 && FatherBlogIndex >= Blogs.length){
            revert("father blog err");
        }

        address author = msg.sender;

        // 创建博文
        uint blogIndex = Blogs.length;
        uint[] memory ChildBlogs;
        Blogs.push(Blog(author,PublishBlogFee+coinCount,0,0,blogType,0,ChildBlogs,content,block.timestamp));


        // 找话题
        uint entryIndex = 0;
        if(blogType != BlogType.New){
            // 评论，转发
            entryIndex = Blogs[FatherBlogIndex].EntryIndex;
            Entries[entryIndex].CoinCount+=Blogs[blogIndex].CoinCount;
            Blogs[blogIndex].FatherBlogIndex = FatherBlogIndex;
            Users[Blogs[FatherBlogIndex].Author].CoinCount+=Blogs[blogIndex].CoinCount;
        }else{
            uint i;
            for (i = 0; i < Entries.length; i++) {
                if (keccak256(abi.encodePacked(entryWord)) == keccak256(abi.encodePacked(Entries[i].EntryWord))) {
                    break;
                }
            }

            if(i==Entries.length){
                // 新话题
                entryIndex = Entries.length;
                uint[] memory EntryBlogs;
                Entries.push(Entry(entryWord,EntryBlogs,Blogs[blogIndex].CoinCount));
                Entries[Entries.length-1].Blogs.push(blogIndex);
                Users[author].CoinCount+=Blogs[blogIndex].CoinCount;
            }else{
                // 已有话题
                entryIndex = i;
                Entries[i].CoinCount+=PublishBlogFee+coinCount;
                Entries[entryIndex].Blogs.push(blogIndex);
                Users[author].CoinCount+=Blogs[blogIndex].CoinCount;
            }
        }
        Blogs[blogIndex].EntryIndex = entryIndex;

        // 记录用户信息
        Users[author].Blogs.push(blogIndex);

        // 热度
        if(blogType != BlogType.New){
            // 父博文添加子博文索引
            Blogs[Blogs[blogIndex].FatherBlogIndex].ChildBlogIndex.push(blogIndex);

            // 自己以及父博文都加
            uint flagIndex = Blogs[blogIndex].FatherBlogIndex;
            for(uint i = 0 ;i<type(uint).max;i++){
                Blogs[flagIndex].CoinCount += PublishBlogFee+coinCount;
                if(Blogs[flagIndex].BlogType == BlogType.New){
                    break;
                }
                flagIndex = Blogs[flagIndex].FatherBlogIndex;
            }
        }

        // 金额转移
        // 新博文收手续费
        transfer(FeeAddress,PublishBlogFee);
        if(blogType != BlogType.New){
            // 40%到父，10%到主
            uint fatherFee = uint((int(coinCount) * 2)/5);
            uint mainFee = uint(int(coinCount) / 10);
            uint fee = uint(int(coinCount) - int(fatherFee) - int(mainFee));

            transfer(Blogs[FatherBlogIndex].Author,fatherFee);
            transfer(Blogs[Entries[Blogs[blogIndex].EntryIndex].Blogs[0]].Author,mainFee);
            transfer(FeeAddress,fee);
        }

    }
}

