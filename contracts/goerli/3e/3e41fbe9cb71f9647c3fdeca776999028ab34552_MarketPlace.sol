contract MarketPlace is ChainlinkClient, ConfirmedOwner {
     using Chainlink for Chainlink.Request;

    uint256 public volume;
    bytes32 private jobId;
    uint256 private fee;
    string public api;

    event RequestVolume(bytes32 indexed requestId, uint256 volume);
 AggregatorV3Interface internal priceFeed;
    uint public priceOfNFT;
    IERC721 public NFT;
    // IERC20 public usdt;
    // uint public startTime; 

    struct Info {
        address owner;
        uint salePrice;
        bool saleStatus;
        bool firstTimeSell;
    }
    using SafeERC20 for IERC20;
    uint public napaTokenAmount=2.5*10**18;
  
    IERC20 public NapaToken;
    IERC20 public Usdt;

   

    // mapping ( address=> uint ) public Bids;
    mapping ( uint => Info )  public nftInfo;
    // mapping ( uint=> Bids )  public bidsInfo;

    event ForSale(uint  , Info );
   


    constructor( IERC20 _usdt, IERC20 _NapaToken, IERC721 _NFTAddress )ConfirmedOwner(msg.sender){
        // usdt= IERC20(_usdtAdress);
        NapaToken=_NapaToken;
        Usdt=_usdt;
        NFT = _NFTAddress;
        priceFeed = AggregatorV3Interface(
            0xD4a33860578De61DBAbDc8BFdb98FD742fA7028e
        );
    }

    function getLatestPrice() public view returns (uint) {
        (
            ,
            /*uint80 roundID*/ int price /*uint startedAt*/ /*uint timeStamp*/ /*uint80 answeredInRound*/,
            ,
            ,

        ) = priceFeed.latestRoundData();
        return uint(price);
    }

    function setSale( uint _tokenId, uint salePrice, address _address) external { 
        require(msg.sender== address(NFT),"onlyNapasociety can call");
      
        require (nftInfo[_tokenId].saleStatus==false,"Already in Marketplace");
       
        nftInfo[_tokenId] = Info(_address, salePrice, true, false );
        emit ForSale(_tokenId, nftInfo[_tokenId] );
    }

    function setSaleFromWallet( uint _tokenId, uint salePrice) public { 
        requestVolumeData();
        if(nftInfo[_tokenId].firstTimeSell==false){
        require( NFT.minters(_tokenId)==msg.sender || NFT.ownerOf(_tokenId)==msg.sender,"setSale:Caller Not Owner/Minter");
            
        }else{
        require( NFT.ownerOf(_tokenId)==msg.sender ,"setSale:Caller Not Owner");

        }
        require (nftInfo[_tokenId].saleStatus==false,"Already in Marketplace");
       
      
        nftInfo[_tokenId] = Info(msg.sender, salePrice, true, false  );

        emit ForSale(_tokenId, nftInfo[_tokenId] );
    }

    function buyNftToken(uint _tokenSelect, uint _tokenId) public payable {
        require (nftInfo[_tokenId].saleStatus==true,"NFT not added yet");
        require(NFT.isApprovedForAll(NFT.ownerOf(_tokenId),address(this))==true,"No Allowance For TokenId");
        requestVolumeData();
        if(_tokenSelect==0){
        require(NapaToken.allowance(msg.sender,address(this))>=(nftInfo[_tokenId].salePrice)/napaTokenAmount,"No NAPATOKEN allowance");

        }else if(_tokenSelect==1){
        require(Usdt.allowance(msg.sender,address(this)) >= nftInfo[_tokenId].salePrice,"No USDT allowance");
        
        }else{
        require (msg.value>=(nftInfo[_tokenId].salePrice)/(getLatestPrice()*(10**10)),"Send Correct Eth Value");
        }

        if(nftInfo[_tokenId].firstTimeSell==false){
            feeTransferFirstTime( _tokenSelect,  _tokenId);
            nftInfo[_tokenId].firstTimeSell==true;
        }else{
            feeTransferResell(_tokenSelect,_tokenId);
            nftInfo[_tokenId].firstTimeSell==true;
        }
        
        NFT.transferFrom(NFT.ownerOf(_tokenId), msg.sender, _tokenId );
        nftInfo[_tokenId].saleStatus=false;
        nftInfo[_tokenId].owner=msg.sender;
        // NFT.updateOwnership(_tokenId, msg.sender);
       
    }

    function feeTransferFirstTime(uint _tokenSelect, uint _tokenId) internal{
        requestVolumeData();
        if(_tokenSelect == 0){
            NapaToken.safeTransferFrom(msg.sender,NFT.minters(_tokenId),( nftInfo[_tokenId].salePrice)/volume);

        }else if(_tokenSelect==1){
            Usdt.safeTransferFrom(msg.sender,NFT.minters(_tokenId), nftInfo[_tokenId].salePrice);
        }else{
             payable(NFT.minters(_tokenId)).transfer(msg.value);

        }
    }

    function feeTransferResell(uint _tokenSelect, uint _tokenId) internal{
        requestVolumeData();
        if(_tokenSelect == 0){
            NapaToken.safeTransferFrom(msg.sender,NFT.ownerOf(_tokenId),( nftInfo[_tokenId].salePrice)/volume);

        }else if(_tokenSelect==1){
            Usdt.safeTransferFrom(msg.sender,NFT.ownerOf(_tokenId), nftInfo[_tokenId].salePrice);
        }else{
             payable(NFT.ownerOf(_tokenId)).transfer(msg.value);

        }
    }


    // data from api

    
    function requestVolumeData() public returns (bytes32 requestId) {
        Chainlink.Request memory req = buildChainlinkRequest(
            jobId,
            address(this),
            this.fulfill.selector
        );

        // Set the URL to perform the GET request on
        req.add(
            "get",
            api
        );

      
        req.add("path", "uptime"); // Chainlink nodes 1.0.0 and later support this format

        // Multiply the result by 1000000000000000000 to remove decimals
        int256 timesAmount = 10 ** 18;
        req.addInt("times", timesAmount);

        // Sends the request
        return sendChainlinkRequest(req, fee);
    }

    /**
     * Receive the response in the form of uint256
     */
    function fulfill(
        bytes32 _requestId,
        uint256 _volume
    ) public recordChainlinkFulfillment(_requestId) {
        emit RequestVolume(_requestId, _volume);
        volume = _volume;
    }

    function updateApiLink(string calldata _api) public onlyOwner {
        api=_api;
    }

    /**
     * Allow withdraw of Link tokens from the contract
     */
    function withdrawLink() public onlyOwner {
        LinkTokenInterface link = LinkTokenInterface(chainlinkTokenAddress());
        require(
            link.transfer(msg.sender, link.balanceOf(address(this))),
            "Unable to transfer"
        );
    }

 
  
}