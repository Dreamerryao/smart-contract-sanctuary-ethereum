        address anOwner,
        address aGuardian,
        IERC20 erc20token,
        address paymaster
    ) public initializer {
        require(anOwner != address(0), "Owner cannot be zero");

        _setGuardianDelay(1 days);

        if (aGuardian != address(0)) {
            _setGuardian(aGuardian);
        }

        AccountStorage.Layout storage layout = AccountStorage.layout();
        layout.entryPoint = anEntryPoint;

        _setRoleAdmin(OWNER_ROLE, OWNER_ROLE);
        _grantRole(OWNER_ROLE, anOwner);

        // approve paymaster to transfer tokens from this wallet on deploy
        if (address(erc20token) != address(0)) {
            require(erc20token.approve(paymaster, type(uint).max));
        }
