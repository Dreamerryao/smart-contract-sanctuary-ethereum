pragma solidity ^0.8.17;






contract GuardianControl is IGuardianControl {
    using AccountStorage for AccountStorage.Layout;

    SenderCreator private immutable senderCreator = new SenderCreator();

    function guardianInfo() public view returns (GuardianLayout memory) {
        GuardianLayout memory l = AccountStorage.layout().guardian;
        return l;
    }

    function guardianProcess() public returns (bool) {
        IGuardianControl.GuardianLayout storage layout = AccountStorage
            .layout()
            .guardian;
        return _guardianProcess(layout);
    }

    function _guardianProcess(
        IGuardianControl.GuardianLayout storage layout
    ) private returns (bool) {
        if (
            layout.activateTime != 0 && layout.activateTime >= block.timestamp
        ) {
            layout.activateTime = 0;
            __setGuardian(layout, layout.pendingGuardian);
            return true;
        } else {
            return false;
        }
    }

    function __setGuardian(
        IGuardianControl.GuardianLayout storage layout,
        address guardian
    ) private {
        emit GuardianSet(guardian, layout.guardian);
        layout.guardian = guardian;
    }

    function _setGuardianDelay(uint64 guardianDelay) internal {
        // set guardian delay
        AccountStorage.layout().guardian.guardianDelay = guardianDelay;
    }

    function _setGuardian(address guardian) internal {
        IGuardianControl.GuardianLayout storage layout = AccountStorage
            .layout()
            .guardian;

        _guardianProcess(layout);

        require(
            layout.guardianDelay > 0,
            "GuardianControl: guardian delay not set"
        );

        if (layout.guardian == address(0)) {
            __setGuardian(layout, guardian);
        } else {
            layout.pendingGuardian = guardian;
            layout.activateTime = uint64(
                block.timestamp + layout.guardianDelay
            );
        }
    }

    function _recovery(
        address newOwner,
        address oldOwner,
        bytes32 hash,
        bytes memory signature,
        bytes calldata guardianDeployByteCode
    ) internal {
        IGuardianControl.GuardianLayout storage layout = AccountStorage
            .layout()
            .guardian;

        _guardianProcess(layout);

        address guardian = layout.guardian;

        if (guardianDeployByteCode.length > 0) {
            senderCreator.createSender(guardianDeployByteCode);
        }

        if (!Address.isContract(guardian)) {
            revert("GuardianControl: guardian contract not deployed");
        }

        require(
            SignatureChecker.isValidSignatureNow(guardian, hash, signature),
            "GuardianControl: invalid signature"
        );

        emit Recovered(newOwner, oldOwner);
    }
}

