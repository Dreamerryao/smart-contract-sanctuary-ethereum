pragma solidity ^0.8.17;

interface IStoryNFT{
  enum ActionSet { Default, PendingOrder, Auction}
  struct Auction{
    //bidder->bidprice
    mapping(address => uint256) bidderMap;
    address[] bidderList;
    uint256 startTime;
    uint256 endTime;
    uint256 startPrice;
    uint256 increment;
  }
  struct Inquire{
    uint256 inquireStartTime;
    uint256 inquirePrice;
  }
  struct TradeInfo{
    ActionSet status;
    uint256 sellPrice; // price for pending order
    Auction auctionInfo;
    mapping(address => Inquire) inquiryMap;
    address[] inquirers; // record inquirers
  }
  /**
  * @dev Emitted when `quantity` token is minted by owner.
  */
  event OwnerMint(address indexed to, uint256 quantity, string baseURI, string baseLetterURI);

  /**
  * @dev Emitted when `quantity` token is minted by EOA.
  */
  event EOAMint(uint256 quantity, string baseURI, string baseLetterURI);
  
  /**
  * @dev Emitted when `tokenId` token is presented from `from` to `to`.
  */
  event PresentTo(address indexed from, address indexed to, uint256 tokenId);
  
  /**
  * @dev mint `quantity` tokens from `owner` to `to`,set BaseURI and baseLetterURI
  *
  * Requirements:
  *
  * - `msg.sender` must be the owner.
  *
  * Emits a {OwnerMint} event.
  */
  function ownerMint(
      address to,
      uint256 quantity,
      string memory baseURI,
      string memory baseLetterURI
  ) external;

  /**
  * @dev mint `quantity` tokens from `EOA` to `to`,set BaseURI and baseLetterURI
  *
  * Requirements:
  *
  * - `NFT_PRICE` cannot be zero.
  *
  * Emits a {EOAMint} event.
  */
  function eoaMint(
      uint256 quantity,
      string memory baseURI,
      string memory baseLetterURI
  ) external payable;

  /**
  * @dev transfers `tokenId` token from `from` to `to`.
  *
  * Requirements:
  *
  * - 'to' address cannot be 0x0 address or msg.sender.
  * - 'tokenId' must belong to message sender
  *
  * Emits a {PresentTo} event.
  */
  function presentTo(address to, uint256 tokenId) external payable;
  
  /**
  * @dev transfer the owner of the story
  *
  * @notice when storyfactory create a new collection.the collection owner is storyfactory.
  * so need use this function to transfer ownership from storyFactory to creator 
  */
  function transferOwner(address newOwner) external;
}

