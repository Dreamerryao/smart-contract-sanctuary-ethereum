pragma solidity ^0.8.17;

import "erc721a/contracts/IERC721A.sol";
import "../interfaces/IStoryNFT.sol";
import "../interfaces/IProxyFactory.sol";
import "../interfaces/IProxy.sol";
import "./TradeStorage.sol";
import "hardhat/console.sol";

contract Auction is TradeStorage{

    address private immutable admin;

    event Bid(address indexed storyAddress, uint256 tokenId, address indexed bidder, uint256 bidPrice);
    event Create(address indexed storyAddress, uint256 tokenId, uint256 startPrice, uint256 increment);
    event Cancle(address indexed storyAddress, uint256 tokenId);
    event Deal(address indexed bidder, uint256 bidPrice);
    event Unsold(address storyAddress, uint256 tokenId);

    constructor(){
        admin = msg.sender; 
    }

    modifier nftOwnerProxy(address storyAddress, uint256 tokenId){
        address owner = IERC721A(storyAddress).ownerOf(tokenId);
        address proxyAddress = IProxyFactory(proxyRegister).getProxyAddress(owner);
        require(msg.sender == proxyAddress, "The requester must be the proxy contract of the current NFT owner.");
        _;
    }
    modifier onlyAdmin(){
        require(admin == msg.sender,"The requester is not an administrator.");
        _;
    }

    struct BidInfo{
        address bidder;
        uint256 bidderCount;
        address latestBidder;
        uint256 latestBidPrice;
        address bidderProxy;
        uint256 lastBidPrice;
    }

    //创建竞拍
    function create(
        address storyAddress,
        uint256 tokenId,
        uint256 startPrice,
        uint256 increment,//加价幅度
        uint256 endTime
        ) external nftOwnerProxy(storyAddress, tokenId){
        string memory reason;
        bool verification = false;
        bool isApproved = IERC721A(storyAddress).isApprovedForAll(tx.origin,  msg.sender);
        (verification, reason) = verifyCreatable(isApproved, tokenId, startPrice, increment, endTime);
        require(verification, reason);
        //清除询价信息
        clearInquire(storyAddress, tokenId);
        spotInfo[tokenId].status = IStoryNFT.ActionSet.Auction;
        spotInfo[tokenId].auctionInfo.startTime = block.timestamp;
        spotInfo[tokenId].auctionInfo.endTime = endTime;
        spotInfo[tokenId].auctionInfo.startPrice = startPrice;
        spotInfo[tokenId].auctionInfo.increment = increment;
        emit Create(storyAddress, tokenId, startPrice, increment);
    }

    function verifyCreatable(
        bool isApproved,
        uint256 tokenId,
        uint256 startPrice,
        uint256 increment,//加价幅度
        uint256 endTime
        ) internal view returns(bool result, string memory reason){
        result = false;

        if(!isApproved){
            reason = "The NFT is not authorized to the proxy contract.";
        } else if(spotInfo[tokenId].status != IStoryNFT.ActionSet.Default){
            reason = "The current NFT is being listed or put up for sale.";
        } else if (increment <= 0){
            reason = "The increment price must be greater than 0.";
        } else if (startPrice <= 0){
            reason = "The initial price must be greater than 0.";
        } else if(endTime < block.timestamp){
            reason = "The end time must be greater than the current time.";
        } else{
            result = true;
        }
    }

    //撤销竞拍
    function cancle(address storyAddress, uint256 tokenId) external nftOwnerProxy(storyAddress, tokenId){
        //验证nft交易状态
        require(spotInfo[tokenId].status == IStoryNFT.ActionSet.Auction, "The spot is not in auction.");
        //已经有报价不可撤销
        require(spotInfo[tokenId].auctionInfo.bidderList.length == 0, "Bidders already exist, the current auction cannot be revoked.");
        //修改状态
        spotInfo[tokenId].status = IStoryNFT.ActionSet.Default;
        delete spotInfo[tokenId].auctionInfo;
        emit Cancle(storyAddress, tokenId);
    }

    //出价
    function bid(address storyAddress, uint256 tokenId, uint256 bidPrice) external nftOwnerProxy(storyAddress, tokenId){
        //验证nft交易状态
        require(spotInfo[tokenId].status == IStoryNFT.ActionSet.Auction, "The spot is not in auction.");
        //竞拍开始结束时间验证
        require(spotInfo[tokenId].auctionInfo.endTime > block.timestamp, "The current auction has ended.");
        //不可以自己买自己的
        address owner = IERC721A(storyAddress).ownerOf(tokenId);
        BidInfo memory bidInfo;
        bidInfo.bidder = tx.origin;
        require(bidInfo.bidder != owner, "The NFT already belongs to you.");
        
        bidInfo.bidderCount = spotInfo[tokenId].auctionInfo.bidderList.length;
        if(bidInfo.bidderCount > 0){
            bidInfo.latestBidder = spotInfo[tokenId].auctionInfo.bidderList[bidInfo.bidderCount - 1];
            bidInfo.latestBidPrice = spotInfo[tokenId].auctionInfo.bidderMap[bidInfo.latestBidder];
            require(bidPrice - bidInfo.latestBidPrice >= spotInfo[tokenId].auctionInfo.increment, "The bid increment is not enough.");
        }else{
            require(bidPrice >= spotInfo[tokenId].auctionInfo.startPrice, "The bid price must be greater than the starting price.");
        }
        //锁定保证金(判断之前交过保证金没)
        bidInfo.lastBidPrice =  spotInfo[tokenId].auctionInfo.bidderMap[bidInfo.bidder];
        uint256 lockDeposit = bidPrice - bidInfo.lastBidPrice;
        address bidderProxy = IProxyFactory(proxyRegister).getProxyAddress(bidInfo.bidder);
        IProxy(bidderProxy).lock(storyAddress, lockDeposit, owner);
        //更新竞拍出价信息
        spotInfo[tokenId].auctionInfo.bidderList.push(tx.origin);
        updatePrice(tokenId, bidInfo.bidder, bidPrice);
        emit Bid(storyAddress, tokenId, bidInfo.bidder, bidPrice);
    }
    

    function updatePrice(uint256 tokenId, address bidder, uint256 bidPrice) internal {
        spotInfo[tokenId].auctionInfo.bidderMap[bidder] = bidPrice;
    }
    
    function initBidInfo(uint256 tokenId) internal{
        spotInfo[tokenId].status = IStoryNFT.ActionSet.Default;
        delete spotInfo[tokenId].auctionInfo;
    }

    //到时间成交
    function deal(address storyAddress, uint256 tokenId) external onlyAdmin{
        address owner = IERC721A(storyAddress).ownerOf(tokenId);
        address ownerProxy = IProxyFactory(proxyRegister).getProxyAddress(owner);
        //验证nft交易状态
        require(spotInfo[tokenId].status == IStoryNFT.ActionSet.Auction, "The spot is not in auction.");
        //获取最高出价
        uint bidderCount = spotInfo[tokenId].auctionInfo.bidderList.length;
        //未流拍卖
        if(bidderCount>0){
            address latestBidder = spotInfo[tokenId].auctionInfo.bidderList[bidderCount - 1];
            uint256 latestBidPrice = spotInfo[tokenId].auctionInfo.bidderMap[latestBidder];
            address latestBidderProxy = IProxyFactory(proxyRegister).getProxyAddress(latestBidder);
            uint256 lockedDeposit = IProxy(latestBidderProxy).getlockedDepositAmountTo(owner);
            //锁定保证金>出价
            require(lockedDeposit >= latestBidPrice, "The locked margin must be greater than the current highest bid.");
            //eth交易 
            IProxy(latestBidderProxy).transferDepositTo(storyAddress, payable(owner), latestBidPrice, TRADE_FEE_RATE, TX_SERVICE_FEE_ADDRESS);  
            //交易后清除hammer的竞拍价格，保证不会在解锁保证金里解锁其他部分
            delete spotInfo[tokenId].auctionInfo.bidderMap[latestBidder];
            //解除锁定保证金
            clearAuction(storyAddress, tokenId);
            //修改nft交易信息
            initBidInfo(tokenId);
            // delete spotInfo[tokenId].auctionInfo;
            IProxy(ownerProxy).transferNftTo(storyAddress, tokenId, latestBidder);
            emit Deal(latestBidder, latestBidPrice);
        }else{
            initBidInfo(tokenId);
            emit Unsold(storyAddress, tokenId);
        }
    }

    //清除询价信息
    function clearInquire(address storyAddress, uint256 tokenId) internal {
        address owner = IERC721A(storyAddress).ownerOf(tokenId);
        while(spotInfo[tokenId].inquirers.length > 0){
            uint curLength = spotInfo[tokenId].inquirers.length;
            address inquirer = spotInfo[tokenId].inquirers[curLength];
            uint256 inquiryPrice = spotInfo[tokenId].inquiryMap[inquirer].inquirePrice;
             //退保证金
            address inquirerProxy = IProxyFactory(proxyRegister).getProxyAddress(inquirer);
            uint256 lockedDeposit = IProxy(inquirerProxy).getlockedDepositAmountTo(owner);
            
            //也避免了重复清除同一个bidder,注意：只清除他对当前spot出价,会出现对spot1 spot2出价且 spot1 2的owner为同一个address
            if(inquiryPrice > 0 && inquiryPrice <= lockedDeposit) {
                IProxy(inquirerProxy).unlock(storyAddress, inquiryPrice, owner);
                delete spotInfo[tokenId].inquiryMap[inquirer];
            }
            spotInfo[tokenId].inquirers.pop();
        }
    }

    function clearAuction(address storyAddress,  uint256 tokenId) internal {
        address owner = IERC721A(storyAddress).ownerOf(tokenId);
        //有风险,如果有一个退还失败则eth会被锁在合约里
        while(spotInfo[tokenId].auctionInfo.bidderList.length > 0){
            uint curLength = spotInfo[tokenId].auctionInfo.bidderList.length;
            address bidder = spotInfo[tokenId].auctionInfo.bidderList[curLength - 1];
            uint256 bidPrice = spotInfo[tokenId].auctionInfo.bidderMap[bidder];
             //退保证金
            address bidderProxy = IProxyFactory(proxyRegister).getProxyAddress(bidder);
            uint256 lockedDeposit = IProxy(bidderProxy).getlockedDepositAmountTo(owner);
         
            if(bidPrice > 0 && bidPrice <= lockedDeposit) {
                IProxy(bidderProxy).unlock(storyAddress, bidPrice, owner);
                delete spotInfo[tokenId].auctionInfo.bidderMap[bidder];
            }
            spotInfo[tokenId].auctionInfo.bidderList.pop();
        }
    }
}

