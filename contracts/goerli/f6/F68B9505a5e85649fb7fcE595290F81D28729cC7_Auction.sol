pragma solidity ^0.8.17;

interface IProxyFactory{
  /**
  * @dev Emitted when `newDepositAddress` is created by `depositOwner`.
  */
  event NewProxyCreated(address indexed depositOwner, address indexed newDepositAddress);
  /**
  * @dev if user dont have a proxy, user can create a new proxy
  */
  function registerProxy() external returns(address);
  /**
  * @dev get the user proxy address
  */
  function getProxyAddress(address targetAddress) external view returns(address);
}

