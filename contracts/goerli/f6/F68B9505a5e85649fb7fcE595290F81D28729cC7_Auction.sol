pragma solidity ^0.8.17;

interface IProxy{
  /**
  * @dev Emitted when charge deposit with `amount`
  */
  event ChargeDeposit(uint256 amount);
  /**
  * @dev Emitted when withdraw deposit with `amount`
  */
  event Withdraw(uint256 amount);
  /**
  * @dev Emitted when lock deposit with `amount` by 'to' contract
  */
  event Lock(uint256 amount, address indexed to);
  /**
  * @dev Emitted when unlock deposit with `amount` by 'to' contract
  */
  event Unlock(uint256 amount, address indexed to);
  /**
  * @dev Emitted when recieve eth with `amount` by 'sender' 
  */
  event Received(address indexed sender, uint256 amount);
  /**
  * @dev Emitted when transfer `tokenId` of `storyAddress` to `reciever`
  */
  event TransferNft(address indexed storyAddress, address indexed reciever, uint256 tokenId);
  /**
  * @dev Emitted when transfer `platformFee` to `txServiceFeeAddr` and `ownerIncome` to `income`
  */
  event TransferDeposit(address indexed reciever, address indexed txServiceFeeAddr, uint256 ownerIncome, uint256 platformFee);
  /**
  * @dev transfer `tokenId` of  `storyAddress` to `reciever`
  *
  * @notice   
  * - If `reciever` refers to a smart contract, it must implement
  * {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
  */
  function transferNftTo(address storyAddress, uint256 tokenId, address reciever) external;
  /**
  * @dev after deal(auction or inquire),transfer locked deposit of current nft to its owner and platform
  *
  * @notice `storyAddress` is caller,`tradeFeeRate` to calculate how much `txServiceFeeAddr`(platform) will get,and `reciever` get left amount
  */
  function transferDepositTo(address storyAddress,address payable reciever,uint256 amount,uint tradeFeeRate,address payable txServiceFeeAddr) external;
  /**
  * @dev when auction or inquire a nft,caller need to lock `amount` eth in its own proxy contract
  *
  * @notice `storyAddress` is caller,`locker` is current nft owner,`amount` is inquire price or bid price
  */
  function lock(address storyAddress, uint256 amount, address locker) external;
  /**
  * @dev when auction or inquire failed,platform need to unlock `amount` eth in its own proxy contract
  *
  * @notice `storyAddress` is caller,`locker` is current nft owner,`amount` is locked deposit
  */
  function unlock(address storyAddress, uint256 amount, address locker) external;
  /**
  * @dev get all amount by `locker` locked
  *
  */
  function getlockedDepositAmountTo(address locker) external returns(uint256);
}

