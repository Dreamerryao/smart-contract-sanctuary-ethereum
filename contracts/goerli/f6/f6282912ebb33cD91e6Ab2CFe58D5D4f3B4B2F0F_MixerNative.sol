contract MixerNative is Mixer {
    constructor(
        Semaphore _semaphore,
        DepositVerifier _dv,
        WithdrawVerifier _wv,
        uint256 _depth
    ) Mixer(_semaphore, _dv, _wv, _depth) {}

    function _currency() internal virtual override returns (uint256) {
        return 0;
    }

    function _deposit(uint256 _amount) internal virtual override {
        require(msg.value == _amount, "Incorrect amount of Ether sent");
    }

    function _withdraw(
        address payable _recipient,
        address payable _relayer,
        uint256 _amount,
        uint256 _fee
    ) internal virtual override {
        (bool success, ) = _recipient.call{value: _amount - _fee}("");
        require(success, "payment to _recipient failed");
        if (_fee > 0) {
            (success, ) = _relayer.call{value: _fee}("");
            require(success, "payment to _relayer failed");
        }
    }
}

