pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@zk-kit/incremental-merkle-tree.sol/IncrementalBinaryTree.sol";

import "./Semaphore.sol";
import "./deposit_verifier.sol";
import "./withdraw_verifier.sol";

struct Proof {
    uint256[2] A;
    uint256[2][2] B;
    uint256[2] C;
}

abstract contract Mixer is Pausable, AccessControl {
    using IncrementalBinaryTree for IncrementalTreeData;

    uint256 public constant FIELD_SIZE =
        21888242871839275222246405745257275088548364400416034343698204186575808495617;
    uint256 public constant ZERO = // uint256(keccak256("coinmask")) % FIELD_SIZE;
        20679137053952074631741420175723774548361419686356676616925638257927477574299;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant SEMAPHORE_ROLE = keccak256("SEMAPHORE_ROLE");

    Semaphore semaphore;
    DepositVerifier dv;
    WithdrawVerifier wv;

    uint256 public depth;
    IncrementalTreeData public tree;
    mapping(uint256 => bool) public preimages;
    mapping(bytes32 => bool) public commitments;

    event Deposit(bytes32 indexed commitment, uint256 index, uint256 timestamp);
    event Withdrawal(
        address recipient,
        uint256 preimage,
        address indexed relayer,
        uint256 fee,
        uint256 timestamp
    );

    function _currency() internal virtual returns (uint256);

    function _deposit(uint256 _amount) internal virtual;

    function _withdraw(
        address payable _recipient,
        address payable _relayer,
        uint256 _amount,
        uint256 _fee
    ) internal virtual;

    constructor(
        Semaphore _semaphore,
        DepositVerifier _dv,
        WithdrawVerifier _wv,
        uint256 _depth
    ) {
        semaphore = _semaphore;

        dv = _dv;
        wv = _wv;

        depth = _depth;

        tree.init(_depth, ZERO);

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);

        _grantRole(SEMAPHORE_ROLE, address(_semaphore));
    }

    function deposit(
        bytes calldata _proof,
        uint256 _root,
        bytes32 _commitment,
        uint256 _amount
    ) external payable whenNotPaused {
        require(!commitments[_commitment], "_commitment has been submitted");

        Proof memory _p = _parse(_proof);
        uint256[6] memory _input = [
            tree.root,
            _root,
            block.chainid,
            _currency(),
            _amount,
            uint256(_commitment)
        ];
        require(
            dv.verifyProof(_p.A, _p.B, _p.C, _input),
            "_proof could not be verified"
        );

        tree.insert(uint256(_commitment));
        commitments[_commitment] = true;

        require(tree.root == _root, "_root does not match");

        _deposit(_amount);

        emit Deposit(_commitment, tree.numberOfLeaves - 1, block.timestamp);
    }

    function withdraw(
        bytes calldata _proof,
        uint256 _preimage,
        uint256 _amount,
        address payable _recipient,
        address payable _relayer,
        uint256 _fee
    ) external payable whenNotPaused {
        require(!preimages[_preimage], "_preimage has been spent");

        Proof memory _p = _parse(_proof);
        uint256[6] memory _input = [
            tree.root,
            block.chainid,
            _currency(),
            _amount,
            uint256(uint160(address(_recipient))),
            _preimage
        ];
        require(
            wv.verifyProof(_p.A, _p.B, _p.C, _input),
            "_proof could not be verified"
        );

        preimages[_preimage] = true;

        _withdraw(_recipient, _relayer, _amount, _fee);

        emit Withdrawal(_recipient, _preimage, _relayer, _fee, block.timestamp);
    }

    function paused() public view override returns (bool) {
        if (semaphore.paused()) {
            return true;
        }

        return super.paused();
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function setSemaphore(
        Semaphore _semaphore
    ) external onlyRole(SEMAPHORE_ROLE) {
        semaphore = _semaphore;
    }

    function _parse(
        bytes calldata proof
    ) internal pure returns (Proof memory) {
        uint256[8] memory _points = abi.decode(proof, (uint256[8]));

        Proof memory _proof;
        _proof.A = [_points[0], _points[1]];
        _proof.B =[[_points[2], _points[3]], [_points[4], _points[5]]];
        _proof.C =[_points[6], _points[7]];

        return _proof;
    }
}

