import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract RouterProxy {
    using Address for address;
    address private router;
    address private owner;
    address private creator;

    // error ForwardCallFail();
    error ZeroReturnAmount();
    // error ForwardReturnAmountNotEnough();
    // error backwardReturnAmountNotEnough();

    error ProfitNotEnough(
        // uint256 amountIn,
        // uint256 mediumAmount,
        // uint256 amountOut,
        uint256 beforeBalance,
        uint256 afterBalance,
        uint256 beforeGas,
        uint256 afterGas,
        uint256 gasprice
    );
    error OnlyOwner(string err);
    error OnlyCreatorAndOwner();
    error WithdrawFaild(string err);

    event Received(address, uint);

    constructor(address _router, address _owner) {
        router = _router;
        owner = _owner;
        creator = msg.sender;
    }

    function callRouter(
        uint256 amountIn,
        IERC20 srcToken,
        IERC20 dstToken,
        bytes calldata forwardData,
        bytes calldata backwardData,
        uint256 idx
    ) external payable onlyCreatorAndOwner {
        console.log("callRouter");

        uint256 beforeBalance = address(this).balance;
        console.log("beforeBalance: %d", beforeBalance);
        uint256 beforeGas = gasleft();
        console.log("beforeGas: %d", beforeGas);

        //1.2 forward call
        bytes memory frwdreturndata = address(router).functionCallWithValue(
            forwardData,
            amountIn,
            "RouterProxy: call AggregationRouter forward failed"
        );

        uint256 dstMediumAmount = uniBalanceOf(dstToken, address(this));
        // uint256 dstMediumAmount = dstToken.uniBalanceOf(address(this));

        console.log("forward returndata:  %s", string(frwdreturndata));
        console.log("dstMediumAmount: %d", dstMediumAmount);

        if (dstMediumAmount == 0) revert ZeroReturnAmount();

        //2.1 approve backward amount
        IERC20(dstToken).approve(router, dstMediumAmount);
        console.log("approved %d of %s, ", dstMediumAmount, address(dstToken));
        console.log(" from %s to %s ", address(this), router);

        //2.1.2 modify backward call data , replace the origin amount to real dstMediumAmount
        console.log("replace backward build data:");
        console.logBytes(backwardData);
        console.log("from %d to %d", idx, idx + 64);
        console.log("with %d", dstMediumAmount);

        bytes memory bd = _replace(backwardData, idx, dstMediumAmount);

        console.log("after replace:");
        console.logBytes(bd);

        //2.2 backward call
        address(router).functionCall(
            bd,
            "RouterProxy: call AggregationRouter backward failed"
        );

        // Don't use backward return data, to save gas and avoid stack too deep

        // bytes memory bkreturndata = address(router).functionCall(
        //     bd,
        //     "RouterProxy: call AggregationRouter backward failed"
        // );

        // (uint256 returnAmount, uint256 spentAmount) = abi.decode(
        //     bkreturndata,
        //     (uint256, uint256)
        // );
        // console.log("backward spende: %d", spentAmount);
        // console.log("backward return: %d", returnAmount);

        //3. calculate profit, revert if not enough
        uint256 afterBalance = address(this).balance;
        console.log("afterBalance: %d", afterBalance);
        uint256 afterGas = gasleft();
        console.log("afterGas: %d", afterGas);

        if (
            // returnAmount < amountIn ||
            afterBalance < beforeBalance + (beforeGas - afterGas )* tx.gasprice
        )
            revert ProfitNotEnough(
                // amountIn,
                // dstMediumAmount,
                // returnAmount,
                beforeBalance,
                afterBalance,
                beforeGas,
                afterGas,
                tx.gasprice
            );
    }

    function _replace(
        bytes calldata data,
        uint256 start_idx,
        uint256 amount
    ) private view returns (bytes memory) {
        bytes memory rdata = data;
        console.log("start_idx: %d", start_idx);
        assembly {
            //store amount in data from start_idx
            // mstore(add(rdata,  start_idx), amount)
            mstore(add(add(rdata, 32), start_idx), amount)
        }
        return rdata;
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

    modifier onlyOwner() {
        // owner is storage variable is set during constructor
        if (msg.sender != owner) {
            revert OnlyOwner('OnlyOwer');
        }
        _;
    }

    modifier onlyCreatorAndOwner() {
        // owner is storage variable is set during constructor
        if (msg.sender != owner && msg.sender != creator) {
            revert OnlyCreatorAndOwner();
        }
        _;
    }

    uint256 private constant _RAW_CALL_GAS_LIMIT = 5000;

    IERC20 private constant _ETH_ADDRESS =
        IERC20(0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE);
    IERC20 private constant _ZERO_ADDRESS = IERC20(address(0));

    function isETH(IERC20 token) internal pure returns (bool) {
        return (token == _ZERO_ADDRESS || token == _ETH_ADDRESS);
    }

    function uniBalanceOf(
        IERC20 token,
        address account
    ) internal view returns (uint256) {
        if (isETH(token)) {
            return account.balance;
        } else {
            return token.balanceOf(account);
        }
    }

    /**
     * with draw token from this contract, must call befor selfdesctruct
     * @param token token to withdraw
     */
    function withdraw(IERC20 token) external onlyOwner {
        if (isETH(token)) {
            // solhint-disable-next-line avoid-low-level-calls
            (bool success, ) = owner.call{
                value: uniBalanceOf(token, address(this)),
                gas: _RAW_CALL_GAS_LIMIT
            }("");
            if (!success) revert WithdrawFaild("ETH withdraw faild");
        } else {
            token.transfer(owner, uniBalanceOf(token, address(this)));
        }
    }

    /**
     * selfdestroy, onlyowner can call
     */
    function destroy() external onlyOwner {
        selfdestruct(payable(owner));
    }
}

