contract Lock {
    
    function sum_value(uint a, uint b) public pure returns(uint){
        return a + b;
    }

    function multiply_value(uint a, uint b) public pure returns(uint){
        return a * b;
    }

    function subtract_value(uint a, uint b) public pure returns(uint){
        return a - b;
    }
    
}