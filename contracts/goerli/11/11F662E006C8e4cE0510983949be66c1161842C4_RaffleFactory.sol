pragma solidity ^0.8.15;

// Import this file to use console.log
import "hardhat/console.sol";
import "contracts/ICryptoPunk.sol";

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/PullPayment.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract Raffle is Ownable, ERC721Holder, PullPayment, VRFConsumerBaseV2 {

    enum State {
        WaitingForNFT,
        WaitingForStart,
        SellingTickets,
        WaitingForRNG,
        Completed,
        Cancelled
    }
    State public state;

    address public immutable nftContract;
    uint public immutable nftTokenId;
    uint16 public immutable tickets;
    uint public immutable ticketPrice;
    uint public immutable startTimestamp;
    uint public immutable endTimestamp;

    uint16 public ticketsLeft;
    uint16 public soldTickets;
    uint16 public giveawayTickets;

    struct TicketNumberRange {
        address owner;
        uint16 from;
        uint16 to;
    }
    TicketNumberRange[] public ticketNumberRangeOwners;
    mapping(address => uint16) private addressToPurchasedCountMap;
    mapping(address => uint16) private addressToAssignedCountMap;
    mapping(address => uint16[]) private addressToAssignedTicketNumberRangesMap;

    VRFCoordinatorV2Interface immutable VRF_COORDINATOR;
    uint64 immutable vrfSubscriptionId;
    bytes32 immutable vrfKeyHash;
    uint256[] public vrfRandomWords;
    uint256 public vrfRequestId;
    address vrfOwner;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 constant VRF_CALLBACK_GAS_LIMIT = 100000;

    // The default is 3, but you can set this higher.
    uint16 constant VRF_REQUEST_CONFIRMATIONS = 3;

    bool public winnerSelected;
    uint16 public winningTicketNumber;
    address public winnerAddress;

    event ReturnedRandomness(uint256[] randomWords);

    constructor(
        address _nftContract,
        uint _nftTokenId,
        uint16 _tickets,
        uint _ticketPrice,
        uint _startTimestamp,
        uint _endTimestamp,
        uint64 _vrfSubscriptionId,
        address _vrfCoordinator,
        bytes32 _vrfKeyHash
    ) VRFConsumerBaseV2(_vrfCoordinator) {
        require(block.timestamp < _startTimestamp, "Start timestamp cannot be in the past");
        require(_endTimestamp > _startTimestamp, "End timestamp must be after start timestamp");
        require(_tickets > 0, "Number of tickets must be greater than 0");
        require(_tickets <= 50_000, "Number of tickets cannot exceed 50_000");

        nftContract = _nftContract;
        nftTokenId = _nftTokenId;
        tickets = _tickets;
        ticketPrice = _ticketPrice;
        startTimestamp = _startTimestamp;
        endTimestamp = _endTimestamp;
        VRF_COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        vrfKeyHash = _vrfKeyHash;
        vrfOwner = msg.sender;
        vrfSubscriptionId = _vrfSubscriptionId;

        state = State.WaitingForNFT;
        ticketsLeft = _tickets;
    }

    // TODO move somehere
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        require(state == State.WaitingForRNG, "Incorrect state, must be in WaitingForRNG");

        vrfRandomWords = randomWords;
        winningTicketNumber = uint16(randomWords[0] % tickets);
        winnerSelected = true;
        // TODO find winner in ticketNumberRangeOwners
    }

    function purchaseTicket(uint16 count) payable external {
        if (state == State.WaitingForStart && block.timestamp > startTimestamp && block.timestamp < endTimestamp) {
            state = State.SellingTickets;
        }
        require(state == State.SellingTickets, "Incorrect state, must be in SellingTickets");
        require(ticketsLeft >= count, "Purchasing too many tickets at once");
        require(msg.value == ticketPrice * count, "Incorrect purchase amount (must be ticketPrice * count)");

        _assignTickets(msg.sender, count);
        addressToPurchasedCountMap[msg.sender] += count;

        if (ticketsLeft == 0) {
            state = State.WaitingForRNG;
            _requestRandomWords();
        }
    }

    struct AddressAndCount {
        address receiverAddress;
        uint16 count;
    }

    function giveawayTicket(AddressAndCount[] memory receivers) external onlyOwner {
        require(state == State.WaitingForStart || state == State.SellingTickets,
            "Incorrect state, must be in WaitingForStart or SellingTickets");

        for (uint i = 0; i < receivers.length; i++) {
            AddressAndCount memory item = receivers[i];
            _assignTickets(item.receiverAddress, item.count);
        }

        if (ticketsLeft == 0) {
            _requestRandomWords();
        }
    }

    function verifyERC721PresenceBeforeStart() external {
        require(state == State.WaitingForNFT, "Incorrect state, must be in WaitingForNFT");

        if (IERC721(nftContract).ownerOf(nftTokenId) == address(this)) {
            state = State.WaitingForStart;
        }
    }

    function verifyCryptoPunkPresenceBeforeStart() external {
        require(state == State.WaitingForNFT, "Incorrect state, must be in WaitingForNFT");

        if (ICryptoPunk(nftContract).punkIndexToAddress(nftTokenId) == address(this)) {
            state = State.WaitingForStart;
        }
    }

    function cancelBeforeStart() external onlyOwner {
        require(state == State.WaitingForNFT || state == State.WaitingForStart,
            "Incorrect state, must be in WaitingForNFT or WaitingForStart");

        state = State.Cancelled;
    }

    function cancelIfUnsold() external {
        require(state == State.SellingTickets, "Incorrect state, must be in SellingTickets");
        require(block.timestamp > endTimestamp, "End timestamp must be in the past");

        state = State.Cancelled;
    }

    function cancelIfNoRNG() external {
        require(state == State.WaitingForRNG, "Incorrect state, must be in WaitingForNFT");
        require(block.timestamp > endTimestamp + 1 days, "End timestamp + 1 day must be in the past");

        state = State.Cancelled;
    }

    function transferTicketRefundIfCancelled() external {
        require(state == State.Cancelled, "Incorrect state, must be in Cancelled");

        uint refundAmount = addressToPurchasedCountMap[msg.sender] * ticketPrice;
        if (refundAmount > 0) {
            addressToPurchasedCountMap[msg.sender] = 0;
            _asyncTransfer(msg.sender, refundAmount);
        }
    }

    function transferERC721ToOwnerIfCancelled(address contractAddress, uint tokenId) external {
        require(state == State.Cancelled, "Incorrect state, must be in Cancelled");

        IERC721(contractAddress).safeTransferFrom(address(this), owner(), tokenId);
    }

    function transferCryptoPunkToOwnerIfCancelled(address contractAddress, uint tokenId) external {
        require(state == State.Cancelled, "Incorrect state, must be in Cancelled");

        ICryptoPunk(contractAddress).transferPunk(address(owner()), tokenId);
    }

    function transferERC721ToWinnerIfCompleted() external {
        require(state == State.Completed, "Incorrect state, must be in Completed");
        assert(winnerAddress != address(0));

        IERC721(nftContract).safeTransferFrom(address(this), winnerAddress, nftTokenId);
    }

    function transferCryptoPunkToWinnerIfCompleted() external {
        require(state == State.Completed, "Incorrect state, must be in Completed");
        assert(winnerAddress != address(0));

        ICryptoPunk(nftContract).transferPunk(winnerAddress, nftTokenId);
    }

    function transferETHToOwnerIfCompleted() external {
        require(state == State.Completed, "Incorrect state, must be in Completed");

        _asyncTransfer(owner(), address(this).balance);
    }

    function requestRandomWords() external onlyOwner {
        _requestRandomWords();
    }

    function _requestRandomWords() private {
        require(state == State.WaitingForRNG, "Incorrect state, must be in WaitingForRNG");

        vrfRequestId = VRF_COORDINATOR.requestRandomWords(
            vrfKeyHash,
            vrfSubscriptionId,
            VRF_REQUEST_CONFIRMATIONS,
            VRF_CALLBACK_GAS_LIMIT,
            1
        );
    }

    function _assignTickets(address owner, uint16 count) private {
        require(ticketsLeft >= count, "Assigning too many tickets at once");

        uint16 from = tickets - ticketsLeft;
        ticketsLeft -= count;
        soldTickets += count;
        ticketNumberRangeOwners.push(TicketNumberRange({
        owner : owner,
        from : from,
        to : from + count}));
        addressToAssignedCountMap[msg.sender] += count;
        addressToAssignedTicketNumberRangesMap[msg.sender].push(uint16(ticketNumberRangeOwners.length - 1));

        assert(ticketNumberRangeOwners[ticketNumberRangeOwners.length - 1].to == tickets - ticketsLeft);
        assert(tickets == ticketsLeft + soldTickets + giveawayTickets);
    }

    // todo review
    function _findWinner() private {
        require(state == State.WaitingForRNG, "Incorrect state, must be in WaitingForRNG");
        require(winnerSelected == true, "Winner must be selected");
        require(winnerAddress == address(0), "Winner address must be 0 at this point");

        uint16 ticketNumberRangeOwnersLength = uint16(ticketNumberRangeOwners.length);
        assert(ticketNumberRangeOwners[0].from == 0);
        assert(ticketNumberRangeOwners[ticketNumberRangeOwnersLength - 1].from == tickets - 1);
        assert(winningTicketNumber < tickets);

        uint16 pivot = ticketNumberRangeOwnersLength / 2;
        while (winnerAddress != address(0)) {
            TicketNumberRange memory ticketNumberRange = ticketNumberRangeOwners[pivot];
            if (ticketNumberRange.from >= winningTicketNumber && ticketNumberRange.to <= winningTicketNumber) {
                winnerAddress = ticketNumberRange.owner;
            } else if (ticketNumberRange.from < winningTicketNumber) {
                pivot -= pivot / 2;
            } else {
                pivot += pivot / 2;
            }
        }
    }
}

