import "../interfaces/ILidoOracle.sol";
import "../StakingRewards.sol";

import "hardhat/console.sol";

/**
 * @title StETH token wrapper with static balances.
 * @dev It's an ERC20 token that represents the account's share of the total
 * supply of stETH tokens. WstETH token's balance only changes on transfers,
 * unlike StETH that is also changed when oracles report staking rewards and
 * penalties. It's a "power user" token for DeFi protocols which don't
 * support rebasable tokens.
 *
 * The contract is also a trustless wrapper that accepts stETH tokens and mints
 * wstETH in return. Then the user unwraps, the contract burns user's wstETH
 * and sends user locked stETH in return.
 *
 * The contract provides the staking shortcut: user can send ETH with regular
 * transfer and get wstETH in return. The contract will send ETH to Lido submit
 * method, staking it and wrapping the received stETH.
 *
 */
contract UstETH is ERC20Permit, AccessControl, StakingRewards {
    IStETH public stETH;
    ILidoOracle public lidoOracle;
    address payable public treasury;
    uint256 public wrappingFee;
    uint256 public basisPoints = 10000;

    event StEthWrapped(address indexed sender, uint256 stETHAmount);
    event UstEthUnwrapped(address indexed sender, uint256 ustETHAmount);
    event Received(address indexed sender, uint256 value);

    /**
     * @param _stETH address of the StETH token to wrap
     */
    constructor(IStETH _stETH, ILidoOracle _lidoOracle, address payable _treasury, uint256 _wrappingFee)
        ERC20Permit("Wrapped ultra liquid staked Ether 2.0")
        ERC20("Wrapped ultra liquid staked Ether 2.0", "UstETH")
    {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        stETH = _stETH;
        lidoOracle = _lidoOracle;
        treasury = _treasury;
        wrappingFee = _wrappingFee;
    }

    /**
     * @notice Exchanges stETH to ustETH
     * @param _stETHAmount amount of stETH to wrap in exchange for wstETH
     * @dev Requirements:
     *  - `_stETHAmount` must be non-zero
     *  - msg.sender must approve at least `_stETHAmount` stETH to this
     *    contract.
     *  - msg.sender must have at least `_stETHAmount` of stETH.
     * User should first approve _stETHAmount to the WstETH contract
     * @return Amount of wstETH user receives after wrap
     */
/*     function wrap(uint256 _stETHAmount) external returns (uint256) {
        require(_stETHAmount > 0, "ustETH: can't wrap zero stETH");
        uint256 wstETHAmount = stETH.getSharesByPooledEth(_stETHAmount);
        _mint(msg.sender, wstETHAmount);
        stETH.transferFrom(msg.sender, address(this), _stETHAmount);
        return wstETHAmount;
    } */

    function wrap(uint256 _stETHAmount) external returns (uint256) {
        require(_stETHAmount > 0, "ustETH: can't wrap zero stETH");
        uint256 wstETHAmount = _stETHAmount;
        IStETH(stETH).transferFrom(msg.sender, address(this), _stETHAmount);
        _mint(msg.sender, wstETHAmount);
        emit StEthWrapped(msg.sender, wstETHAmount);
        return wstETHAmount;
    }

    /**
     * @notice Exchanges ustETH to stETH
     * @param _wstETHAmount amount of wstETH to uwrap in exchange for stETH
     * @dev Requirements:
     *  - `_wstETHAmount` must be non-zero
     *  - msg.sender must have at least `_wstETHAmount` wstETH.
     * @return Amount of stETH user receives after unwrap
     */
    function unwrap(uint256 _wstETHAmount) external returns (uint256) {
        require(_wstETHAmount > 0, "ustETH: zero amount unwrap not allowed");
        // нужна проверка на наличие с учетом вычитания комиссии
        uint256 stETHAmount = _wstETHAmount;
        uint256 wrappingFeeAmount = stETHAmount * wrappingFee / basisPoints;
        IStETH(stETH).transfer(treasury, wrappingFeeAmount);
        IStETH(stETH).transfer(msg.sender, stETHAmount - wrappingFeeAmount);
        _burn(msg.sender, _wstETHAmount);
        emit UstEthUnwrapped(msg.sender, stETHAmount);
        return stETHAmount;
    }

    function setLidoOracle(ILidoOracle _lidoOracle) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        lidoOracle = _lidoOracle;
    }

    function setTreasury(address payable _treasury) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        require(_treasury != address(0), "Treasury must be non-zero address");
        treasury = _treasury;
    }

    function setWrappingFee(uint256 _wrappingFee) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender));
        require(_wrappingFee >= 0 && _wrappingFee <= basisPoints, "Wrapping Fee must be valid");
        wrappingFee = _wrappingFee;
    }

    function postTotalSupplyAmount() external view returns (uint256) {
        (   uint256 postTotalPooledEther, 
            uint256 preTotalPooledEther, 
            uint256 timeElapsed
        ) = ILidoOracle(lidoOracle).getLastCompletedReportDelta();
        uint256 lastRebaseFactor = postTotalPooledEther / preTotalPooledEther;
        console.log("lastRebaseFactor", lastRebaseFactor);
        uint256 _postTotalSupplyAmount = totalSupply() * postTotalPooledEther / preTotalPooledEther;
        console.log("postTotalSupplyAmount", _postTotalSupplyAmount);
        return _postTotalSupplyAmount;
    }

    function postTotalRebaseAmount() external view returns (uint256) {
    (   uint256 postTotalPooledEther, 
        uint256 preTotalPooledEther, 
        uint256 timeElapsed
    ) = ILidoOracle(lidoOracle).getLastCompletedReportDelta();
    uint256 lastRebaseFactor = postTotalPooledEther / preTotalPooledEther;
    console.log("lastRebaseFactor", lastRebaseFactor);
    uint256 _postTotalRebaseAmount = totalSupply() * postTotalPooledEther / preTotalPooledEther - totalSupply();
    console.log("postTotalSupplyAmount", _postTotalRebaseAmount);
    return _postTotalRebaseAmount;
    }

     /**
    * Add functionality like burn to the _stake afunction
    *
     */
    function stake(uint256 _amount) public {
      // Make sure staker actually is good for it
      require(_amount < balanceOf(msg.sender), "DevToken: Cannot stake more than you own");

        _stake(_amount);
                // Burn the amount of tokens on the sender
        _burn(msg.sender, _amount);
    }

    /**
    * @notice withdrawStake is used to withdraw stakes from the account holder
     */
    function withdrawStake(uint256 amount, uint256 stake_index)  public {

      uint256 amount_to_mint = _withdrawStake(amount, stake_index);
      // Return staked tokens to user
      _mint(msg.sender, amount_to_mint);
    }

    /**
    * @notice Shortcut to receive ETH
    */
    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

}

