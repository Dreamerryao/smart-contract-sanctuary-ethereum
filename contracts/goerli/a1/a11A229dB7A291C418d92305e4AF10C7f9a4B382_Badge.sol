pragma solidity ^0.8.13;

import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/security/Pausable.sol";
import "lib/openzeppelin-contracts/contracts/utils/Counters.sol";

import "./ERC1155Permit.sol";

contract Badge is ERC1155Permit, Ownable, Pausable {
  using Counters for Counters.Counter;

  string private _contractName;

  /*
   * We need to keep track the next available id for creating of new BADGE type.
   * We can do the mapping of tokens owned by an address off-chain;
   * @todo Check solidstate-network for their ERC1155 implementation
   */
  Counters.Counter private _nextTokenId;

  mapping(uint256 => string) private _tokenURIs;
  mapping(uint256 => uint256) private _tokenTotalSupply;

  constructor(string memory contractName) ERC1155Permit(contractName) {
    _contractName = contractName;
    // nextTokenId is initialized to 1
    _nextTokenId.increment();
  }

  function mintGuildBadge(
    address account,
    uint256 tokenId,
    uint256 amount,
    bytes memory data
  ) external onlyOwner whenNotPaused {
    _setTokenTotalSupply(tokenId, amount);
    _mint(account, tokenId, amount, data);
  }

  function mintGuildBadges(
    address account,
    uint256[] memory tokenIds,
    uint256[] memory amounts,
    bytes memory data
  ) external onlyOwner whenNotPaused {
    require(tokenIds.length == amounts.length, "Badge/invalid-tokenIds-amounts");

    for (uint256 i = 0; i < tokenIds.length; i++) {
      _setTokenTotalSupply(tokenIds[i], amounts[i]);
    }

    _mintBatch(account, tokenIds, amounts, data);
  }

  function transferWithPermit(
    address from,
    address to,
    address operator,
    bool approved,
    uint256 tokenId,
    uint256 amounts,
    uint256 deadline,
    uint8 v,
    bytes32 r,
    bytes32 s
  ) public whenNotPaused {
    permit(from, operator, approved, deadline, v, r, s);
    safeTransferFrom(from, to, tokenId, amounts, "");
  }

  function batchTransferWithPermit(
    address from,
    address to,
    address operator,
    bool approved,
    uint256[] memory tokenIds,
    uint256[] memory amounts,
    uint256 deadline,
    uint8 v,
    bytes32 r,
    bytes32 s
  ) public whenNotPaused {
    permit(from, operator, approved, deadline, v, r, s);
    safeBatchTransferFrom(from, to, tokenIds, amounts, "");
  }

  function name() public view returns (string memory) {
    return _contractName;
  }

  function totalSupply() public view returns (uint256) {
    return _nextTokenId.current() - 1;
  }

  function tokenTotalSupply(uint256 tokenId) public view returns (uint256) {
    return _tokenTotalSupply[tokenId];
  }

  function _setTokenTotalSupply(uint256 tokenId, uint256 amount) internal {
    if (!exists(tokenId)) {
      _nextTokenId.increment();
    }

    _tokenTotalSupply[tokenId] += amount;
  }

  function nextTokenId() public view returns (uint256) {
    return _nextTokenId.current();
  }

  function setTokenURI(uint256 tokenId, string memory newTokenUri) external onlyOwner {
    _tokenURIs[tokenId] = newTokenUri;
  }

  function uri(uint256 tokenId) public view override returns (string memory) {
    return _tokenURIs[tokenId];
  }

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function exists(uint256 tokenId) public view returns (bool) {
    return _tokenTotalSupply[tokenId] > 0;
  }
}

