pragma solidity ^0.8.9;


import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/ConfirmedOwner.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";


import "hardhat/console.sol";

contract CoinFlipGame is VRFConsumerBaseV2, ConfirmedOwner {
    using Address for address;
    using SafeMath for uint;

    uint128 public minEntryFees = 0.01 ether;
    uint64 subscriptionId;
    bytes32 keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15;
    uint32 constant callbackGasLimit = 1_000_000;
    uint16 requstConfirmations = 3;
    VRFCoordinatorV2Interface COORDINATOR;
    address gameOwner;
    bool public gameEnabled = true;
    uint256 public pointsPerReferral = 10;
    address public gameAndMaintainersFeeAddress = 0xA96F688fbB9D0457E0236E2c6352f66376124825; //for devs and paying for Link subscrition
    uint256 public gameAndMaintainersFeePercentage = 50; // 5%

    struct GameTicket {
        bool exists;
        uint[] randomWords;
        address player;
        bool didWin;
        bool fulfilled;
        uint totalOdds;
        uint betAmount;
        uint wonAmount;
        address betToken;
        CoinFlipSelection[] player_choice;
    }

    struct UserData {
        uint[] ticketNumbers;
        uint256 won;
    }

    enum CoinFlipSelection {
        HEADS,
        TAILS
    }

    mapping(address => UserData) public userData; //save a user's ticket numbers
    mapping(uint256 => GameTicket) public gameTickets;
    mapping(address => bool) public isBlacklisted;
    mapping(address => uint256) public minAmountForBetToken;
    mapping(address => uint256) public maxAmountForBetToken;
    mapping(address => bool) public allowedGamingTokens;
    mapping(address => address) public referrals;
    mapping(address => uint) public referralPoints;

    // EVENTS 
    event CoinFlipRequest(uint256 requestId);
    event CoinFlipResult(uint256 requestId, bool didWin);
    event FeeChange(uint256 oldFee, uint256 newFee);
    event FeeAddressChange(address oldAddress, address newAddress);
    event EntryFeesChange(uint256 newFees);
    event GameModeChanged(bool newMode);
    event AddressWhitlisted(address _address);
    event AddressBlacklisted(address _address);
    event MinAmountForTokenChanged(address _token, uint256 oldAmount, uint256 newAmount);
    event MaxAmountForTokenChanged(address _token, uint256 oldAmount, uint256 newAmount);
    event GamingTokenAdded(address _token);
    event GamingTokenRemoved(address _token);
    event ReferralPointsChanged(uint256 _oldAmount, uint256 _newAmount);

    constructor (
        uint64 _subscriptionId,
        address _coordinatorAddress
    ) payable VRFConsumerBaseV2(_coordinatorAddress) 
      ConfirmedOwner(msg.sender) 
    {
        COORDINATOR = VRFCoordinatorV2Interface(_coordinatorAddress);
        subscriptionId = _subscriptionId;
        gameOwner = msg.sender;
    }

    modifier noneContract {
        require(!(msg.sender.isContract()), "Other contracts cannot play this game");

        _;
    }

    // constructor (
    //     uint64 _subscriptionId
    // ) payable VRFConsumerBaseV2(0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D) 
    //   ConfirmedOwner(msg.sender) 
    // {
    //     COORDINATOR = VRFCoordinatorV2Interface(0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D);
    //     subscriptionId = _subscriptionId;
    //     gameOwner = msg.sender;
    // }

    // modifier noneContract {
    //     require(!(msg.sender.isContract()), "Other contracts cannot play this game");

    //     _;
    // }

    // Can directly receive ether
    receive() external payable {}

    function flip(CoinFlipSelection[] memory __player_choice, address _token, uint256 _betAmount, address _referrer) external payable noneContract returns (uint256) {
        require(gameEnabled, "Game is not enebled"); //check to see if game is enabled
        require(!isBlacklisted[msg.sender], "This user is blacklisted"); // make sure user is not blacklisted
        require(__player_choice.length <= 10, "Cannot make morethan ten selections"); // user cannot make morethan 10 multiple bets in one ticket
        require(msg.sender == tx.origin, "Msg.sender must be same as transaction originator");

        // init some local variables 
        uint32 NUM_WORDS = uint32( __player_choice.length);
        IERC20 ticketToken = IERC20(_token);

        if (_token != address(0x0)) {
            require(allowedGamingTokens[_token], "Unsupported Token, Please Choose another token");

            // verify betting amounts 
            require(_betAmount >= minAmountForBetToken[_token], "Amount is less than the minimum bet amount for this token");
            require(_betAmount <= maxAmountForBetToken[_token], "Amount is greater than the maximum bet amount for this token");

            require(ticketToken.balanceOf(address(this)).div(NUM_WORDS) >= _betAmount, "There is no enough tokens to pay user");
            ticketToken.transferFrom(msg.sender, address(this), _betAmount);
        } else {
            require(msg.value > minEntryFees, "Entry Fees Not Sent"); //validate the amount sent to the contract 
            require(address(this).balance.div(NUM_WORDS) >= _betAmount, "There is no enough coins to pay user");
            require(msg.value == _betAmount, "Not enough tokens sent");
        }

        
        if (_referrer != address(0) && _referrer != msg.sender && referrals[msg.sender] == address(0x0)) {
            referrals[msg.sender] = _referrer;
        }

        // request for a randomnumber 
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId, 
            requstConfirmations,
            callbackGasLimit, 
            NUM_WORDS
        );

        // calculate the total odds 
        uint _totalOdds = NUM_WORDS * 2;

        // store request using the requestId
        gameTickets[requestId] = GameTicket({
            exists: true,
            player: msg.sender,
            didWin: false,
            fulfilled: false,
            totalOdds: _totalOdds,
            betAmount: _betAmount,
            wonAmount: 0,
            player_choice: __player_choice,
            betToken: _token,
            randomWords: new uint[](NUM_WORDS)
        });

        userData[msg.sender].ticketNumbers.push(requestId);

        emit CoinFlipRequest(requestId); //emit event
        return requestId; //return requestId
    }

    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        require(gameTickets[_requestId].exists, "Request does nor exist");

        gameTickets[_requestId].fulfilled = true;
        gameTickets[_requestId].randomWords = _randomWords;
        
        uint256 hasWon = 0; //count how many numbers user won

        for (uint256 index = 0; index < gameTickets[_requestId].randomWords.length; index++) {
            CoinFlipSelection result = CoinFlipSelection.HEADS;
            if (_randomWords[index] % 2 == 0) {
                result = CoinFlipSelection.TAILS;
            }

            if (gameTickets[_requestId].player_choice[index] == result) {
                hasWon++;
            }
        }

        if (hasWon == _randomWords.length) { //user won all their bets
            gameTickets[_requestId].didWin = true;
            userData[msg.sender].won++;
            uint amountWon = gameTickets[_requestId].totalOdds * gameTickets[_requestId].betAmount;
            gameTickets[_requestId].wonAmount = amountWon;

            uint gameAndMaintainersFee = amountWon.mul(gameAndMaintainersFeePercentage).div(1000);
            uint amountToPay = amountWon.sub(gameAndMaintainersFee);

            if (gameTickets[_requestId].betToken == address(0x0)) {
                gameTickets[_requestId].player.call{value: amountToPay}("");
                gameAndMaintainersFeeAddress.call{value: gameAndMaintainersFee}("");
            }else {
                IERC20 _betToken = IERC20(gameTickets[_requestId].betToken);
                _betToken.transfer(gameTickets[_requestId].player, amountToPay);
                _betToken.transfer(gameAndMaintainersFeeAddress, gameAndMaintainersFee);
            }
           
        }

        referralPoints[referrals[gameTickets[_requestId].player]] = referralPoints[referrals[gameTickets[_requestId].player]] + pointsPerReferral;

        emit CoinFlipResult(_requestId, gameTickets[_requestId].didWin);
    }

    function getStatus(uint256 _requestId) public view returns (GameTicket memory) {
        return gameTickets[_requestId];
    }

    function getUserData(address _address) public view returns (UserData memory) {
        return userData[_address];
    }

    function getTokenBalance(address _token) public view returns(uint) {
        IERC20 token = IERC20(_token);

        if (_token != address(0x0)) {
            return token.balanceOf(address(this));
        } else {
            return address(this).balance;
        }
    }

    function withdraw(uint256 _amount) public onlyOwner {
        (bool sent, bytes memory data) = msg.sender.call{value: _amount}("");

        require(sent, "Failed to transfer funds");
    }

    function withdrawToken(address _token, uint256 _amount) public onlyOwner {
        IERC20 token = IERC20(_token);

        token.transfer(msg.sender, _amount);
    }

    function setminEntryFees(uint128 _minEntryFees) onlyOwner public {
        require(minEntryFees != _minEntryFees, "Fee is already what you are chaning to");
        minEntryFees = _minEntryFees;

        emit EntryFeesChange(_minEntryFees);
    }

    function setGameEnabled(bool _enabled) onlyOwner public {
        require(gameEnabled != _enabled, "Game is already in the mode you are changing to");
        gameEnabled = _enabled;

        emit GameModeChanged(_enabled);
    }

    function blacklistAddress(address _address) public onlyOwner {
        isBlacklisted[_address] = true;

        emit AddressBlacklisted(_address);
    }

    function whitelistAddress(address _address) public onlyOwner {
        isBlacklisted[_address] = false;

        emit AddressWhitlisted(_address);
    }

    function setMinAmountForBetToken(address _token, uint _amount) public onlyOwner {
        emit MinAmountForTokenChanged(_token, minAmountForBetToken[_token], _amount);

        minAmountForBetToken[_token] = _amount;
    }

    function setMaxAmountForBetToken(address _token, uint _amount) public onlyOwner {
        emit MaxAmountForTokenChanged(_token, maxAmountForBetToken[_token], _amount);

        maxAmountForBetToken[_token] = _amount;
    }

    function addGamingToken(address _token) public onlyOwner {
        allowedGamingTokens[_token] = true;

        emit GamingTokenAdded(_token);
    }

    function removeGamingToken(address _token) public onlyOwner {
        allowedGamingTokens[_token] = false;

        emit GamingTokenRemoved(_token);
    }

    function setPointsPerReferral(uint256 _points) public onlyOwner {
        emit ReferralPointsChanged(pointsPerReferral, _points);
        pointsPerReferral = _points;
    }

    function getReferralPoints(address _address) public view returns (uint) {
        return referralPoints[_address];
    }

    function setGameAndMaintainersFeePercentage(uint256 _newAmount) public onlyOwner {
        require(_newAmount != gameAndMaintainersFeePercentage, "You are not making any new changes");
        require(_newAmount <= 50, "Fee can not be morethan 5%");
        
        // emit event 
        emit FeeChange(gameAndMaintainersFeePercentage, _newAmount);

        gameAndMaintainersFeePercentage = _newAmount;
    }

    function setGameAndMaintainersFeeAddress(address _newAddress) public onlyOwner {
        require(_newAddress != gameAndMaintainersFeeAddress, "You are not making any new changes");
        require(!(_newAddress.isContract()), "Another contract cannot hold contribution fees");

        // emit event 
        emit FeeAddressChange(gameAndMaintainersFeeAddress, _newAddress);
        gameAndMaintainersFeeAddress = _newAddress;
    }

}

