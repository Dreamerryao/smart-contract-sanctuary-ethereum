    // =============================================================
    //                   Minting + Randomness OPERATIONS
    // =============================================================
    function publicMint(address to, uint256 _quantity) public {
        require(saleStatus != SaleStatus.PAUSED && saleStatus != SaleStatus.PRESALE, "PUBLIC_OFF");
        require(_quantity < TOKENS_PER_PERSON_PUB_LIMIT, "Reached limit");
        require(_quantity < TOKENS_PER_TX_LIMIT, "Reached limit");
        if (saleStatus == SaleStatus.PUBLIC) {
            VRFMint(to, _quantity);
        }
        if (saleStatus == SaleStatus.COMMON_SUPPLIED) {
            require(JokerIds.current() < COLLECTION_JOKER_SIZE, "Reached JOKER cap");
            jokerMint(to);
        }
        if (saleStatus == SaleStatus.JOKER_SUPPLIED) {
            require(AlligatorIds.current() < COLLECTION_SIZE, "Reached Alligators cap");
            alligatorMint(to, _quantity);
        }
    }

    function VRFMint(address to, uint256 _quantity) internal {
        require(saleStatus == SaleStatus.PUBLIC);

        uint moddedRng = random() % MAX_CHANCE_VALUE;

        uint256[2] memory chanceArracy = getChanceArray();
        uint256[2] memory chanceArrayPrize = getChanceArrayPrize();

        if (moddedRng < chanceArracy[0]) {
            alligatorMint(to, _quantity);

        } else {
            jokerMint(to);
            alligatorMint(to, _quantity - 1);
        }

        if (moddedRng > chanceArrayPrize[0]) {
            alligatorMint(to, 1);        
        }

        emit hashed(moddedRng);
    }  

    function requestRandomWords() external onlyOwner returns (uint256 requestId) {
        // Will revert if subscription is not set and funded.
        requestId = i_vrfCoordinator.requestRandomWords(
            i_gasLane,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );
        vrf_requests[requestId] = RequestStatus({randomWords: new uint256[](0), exists: true, fulfilled: false});
        lastRequestId = requestId;
        emit RequestSent(requestId, NUM_WORDS);
        return requestId;
    }

    
    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        require(vrf_requests[_requestId].exists, 'request not found');
        vrf_requests[_requestId].fulfilled = true;
        vrf_requests[_requestId].randomWords = _randomWords;
        emit RequestFulfilled(_requestId, _randomWords);
    }

    function random() internal returns(uint256){
        uint256[] memory _randomWords = vrf_requests[lastRequestId].randomWords;
        return uint256(keccak256(abi.encodePacked(block.timestamp,block.difficulty,  
            _randomWords[0])));
    }

    
    function getChanceArray() public view returns (uint256[2] memory) {
        return [MIN_CHANCE_VALUE, MAX_CHANCE_VALUE];
    }
    function getChanceArrayPrize() public view returns (uint256[2] memory) {
        return [MIN_CHANCE_VALUE_PRIZE, MAX_CHANCE_VALUE];
    }

    function setChanceArray(uint256 _min) external onlyOwner {
        require(_min < MAX_CHANCE_VALUE, "invalid");
        MIN_CHANCE_VALUE = _min;
    }

    function setChancePrizeArray(uint256 _min) external onlyOwner {
        require(_min < MAX_CHANCE_VALUE, "invalid");
        MIN_CHANCE_VALUE_PRIZE = _min;
    }

    //
    function alligatorMint(address to, uint256 _quantity) internal {
        //require(saleStatus == SaleStatus.JOKER_SUPPLIED);
        for (uint index = 0; index < _quantity; index++) {
            _safeMint(to, 1);
            _setTokenURI(_currentIndex - 1, "ALLIGATOR");
            AlligatorIds.increment(1);
        }            
    }

    function jokerMint(address to) internal {
        // Alligators are Distributed.
        // ONLY 1 NFT (JOKER) is allowed because alligators are supplied :)
        JokerIds.increment(1);
        _safeMint(to, 1);
        _setTokenURI(JokerIds.current(), "JOKER");
    }

    function mergeMint(uint256 _1st, uint256 _2nd, uint256 _3rd, address to) external onlyMerger {
        _burn(_1st);
        _burn(_2nd);
        _burn(_3rd);
        alligatorMint(to, 1);
        _setTokenURI(MergeIds.current(), "MERGE");
        MergeIds.increment(1);
    }

    function _isVerifiedCoupon(bytes32 digest, Coupon memory coupon) internal view returns (bool) {
        address signer = ecrecover(digest, coupon.v, coupon.r, coupon.s);
        require(signer != address(0), "ECDSA: invalid signature");
        return signer == _WLSigner;
    }

    function whitelistMint(address to, uint count, Coupon memory coupon) external payable {    
        require(saleStatus != SaleStatus.PAUSED, "SALES_OFF");

        require(count > 0, "MIN_QTY_1");
        require(_currentIndex + count <= COLLECTION_SIZE, "COLLECTION_LIMIT");

        if (saleStatus == SaleStatus.PRESALE) {
            //check coupon
            bytes32 digest = keccak256(abi.encode(to));
            require(_isVerifiedCoupon(digest, coupon), "INVALID_COUPON");

            //check limits - per wallet
            require(_whitelistMintedCount[to] + count <= TOKENS_PER_PERSON_WL_LIMIT, "PRESALE_ALLOWANCE_LIMIT");

            //check limits - WL sale allowance
            require(_currentIndex + count <= COLLECTION_SIZE_WL, "COLLECTION_LIMIT_WL");

            _whitelistMintedCount[to] += count;
        }

        _mint(to, count);
        AlligatorIds.increment(count);
        //_setTokenURI(current(index))
    }

