// Heirarchy is TwoLevelReferal > AirDropControl > AddSlotTokenERC20  > SlotMachine
contract SlotMachine is VRFConsumerBaseV2, AddSlotTokenERC20 {
    VRFCoordinatorV2Interface public vrfCoordinatorV2;

    enum RollState {
        ETHRoll,
        ERC20Roll,
        Idle
    }

    RollState public currentRollState;

    uint256 public coinPrice = 0.01 ether;

    uint64 public immutable subscriptionId;
    uint32 public immutable callbackGasLimit;

    uint8 public constant NUM_WORDS = 3;
    uint8 public constant REQUEST_CONFIRMATIONS = 3;
    uint8 public constant SLOT_ITEMS = 6;

    address public rootOwner;
    address public currentRolledTokenAddress;
    address public currentRolledPlayer;

    bytes32 public gasLane;

    // Events
    event Rolled(address sender, uint256 rand1, uint256 rand2, uint256 rand3);
    event ERC20Rolled(address sender, uint256 rand1, uint256 rand2, uint256 rand3);
    event RequestRandomness(uint256 requestId);
    event ERC20RolledSlot(string rollString);
    event Jackpot(string rollString);
    event UltraWin(string rollString);
    event SuperWin(string rollString);
    event Winning(string rollString);
    event WithdrawMyPrizes(string rollString);
    event SlotMachineFulfilled(uint256 slot1, uint256 slot2, uint256 slo3, uint256 reward);

    mapping(address => uint256) public pendingWithdrawals;

    // mapping (address => mapping (address => uint256))
    // useraddress -> erc20 address -> number of tokens won for that contract
    mapping(address => mapping(address => uint256)) public pendingERC20Withdrawals;

    constructor(
        address _mainAirdropToken,
        uint256 _airdropAmount,
        address _vrfCoordinatorV2,
        uint64 _subscriptionId,
        uint32 _callbackGasLimit,
        bytes32 _gasLane
    ) VRFConsumerBaseV2(_vrfCoordinatorV2) {
        rootOwner = owner();
        // set airdrop token and amount
        mainAirdropToken = _mainAirdropToken;
        addAirdropBundle(_mainAirdropToken, _airdropAmount);

        vrfCoordinatorV2 = VRFCoordinatorV2Interface(_vrfCoordinatorV2);
        subscriptionId = _subscriptionId;
        callbackGasLimit = _callbackGasLimit;
        gasLane = _gasLane;
    }

    function oneRollERC20Token(
        address _referrerAddress,
        address _tokenAddress,
        uint256 _amount
    ) external payable nonReentrant {
        // incase owner changes
        rootOwner = owner();
        // check allTokenBundles in AddSlotTokenERC20
        // allTokenBundles
        require(allTokenBundles[_tokenAddress].isAdded, "Token not added");
        require(!allTokenBundles[_tokenAddress].isBanned, "Address banned");

        // check oneRollPrice
        require(_amount >= allTokenBundles[_tokenAddress].oneRollPrice, "Roll price exceeds amount");

        // need to approve token on ui side for the token contract
        IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
        // prepare transfer tokens to root owner and referals
        IERC20(_tokenAddress).approve(address(rootOwner), _amount);
        IERC20(_tokenAddress).approve(address(_referrerAddress), _amount);

        currentRolledTokenAddress = _tokenAddress;

        // pay refferals 2 level
        payReferralERC20(msg.sender, _referrerAddress, _amount, _tokenAddress);

        currentRollState = RollState.ERC20Roll;
        currentRolledPlayer = msg.sender;
        requestRandomness();
    }

    // For Gas Token Rolls on Eth or Matic
    function oneRoll(address _referrerAddress) external payable nonReentrant {
        // check coin type
        require(msg.value >= coinPrice, "Insufficient value");
        payReferral(msg.sender, _referrerAddress, coinPrice); // TwoLevelReferrral

        currentRollState = RollState.ETHRoll;
        currentRolledPlayer = msg.sender;
        requestRandomness();
    }

    function contractBalance() external view returns (uint256) {
        return address(this).balance;
    }

    function calculatePrize(
        uint256 rand1,
        uint256 rand2,
        uint256 rand3
    ) public virtual returns (uint256) {
        if (rand1 == 5 && rand2 == 5 && rand3 == 5) {
            emit Jackpot("JackPot!!! 30X");
            return coinPrice * 30;
        }

        if (rand1 == 6 && rand2 == 5 && rand3 == 6) {
            emit UltraWin("UltraWin!!! 20X");
            return coinPrice * 20;
        }

        if (rand1 == 4 && rand2 == 4 && rand3 == 4) {
            emit SuperWin("SuperWin!!! 15X");
            return coinPrice * 15;
        }

        if (rand1 == 3 && rand2 == 3 && rand3 == 3) {
            emit Winning("Winning!!! 12X");
            return coinPrice * 12;
        }

        if (rand1 == 2 && rand2 == 2 && rand3 == 2) {
            emit Winning("Winning!!! 10X");
            return coinPrice * 10;
        }

        if (rand1 == 1 && rand2 == 1 && rand3 == 1) {
            emit Winning("Winning!!! 5X");
            return coinPrice * 5;
        }

        if ((rand1 == rand2) || (rand1 == rand3) || (rand2 == rand3)) {
            emit Winning("Winning!!!");
            return (coinPrice * 8) / 10;
        }

        return 0;
    }

    function requestRandomness() internal {
        uint256 requestId = vrfCoordinatorV2.requestRandomWords(
            gasLane,
            subscriptionId,
            REQUEST_CONFIRMATIONS,
            callbackGasLimit,
            NUM_WORDS
        );

        emit RequestRandomness(requestId);
    }

    /**
     * @dev Callback function used by VRF Coordinator
     */
    function fulfillRandomWords(uint256, uint256[] memory randomWords) internal override {
        require(currentRolledPlayer != address(0), "No current rolled player");

        uint256 rand1 = (randomWords[0] % SLOT_ITEMS) + 1;
        uint256 rand2 = (randomWords[1] % SLOT_ITEMS) + 1;
        uint256 rand3 = (randomWords[2] % SLOT_ITEMS) + 1;

        uint256 reward = calculatePrize(rand1, rand2, rand3);

        if (currentRollState == RollState.ERC20Roll) {
            require(currentRolledTokenAddress != address(0), "No current rolled token");

            pendingERC20Withdrawals[currentRolledPlayer][currentRolledTokenAddress] += reward;
            emit ERC20Rolled(currentRolledPlayer, rand1, rand2, rand3);
        }

        if (currentRollState == RollState.ETHRoll) {
            pendingWithdrawals[currentRolledPlayer] += reward;
            sendAirdropToken(mainAirdropToken, currentRolledPlayer);
            emit Rolled(currentRolledPlayer, rand1, rand2, rand3);
        }

        currentRollState = RollState.Idle;
        currentRolledTokenAddress = address(0);
        currentRolledPlayer = address(0);

        emit SlotMachineFulfilled(rand1, rand2, rand3, reward);
    }

    function withdrawMyCoins() external nonReentrant {
        uint256 amount = pendingWithdrawals[msg.sender];
        require(amount > 0, "amount zero");

        pendingWithdrawals[msg.sender] = 0;

        //msg.sender.transfer(amount);
        emit WithdrawMyPrizes("WithdrawMyPrizes");
        (bool sent, ) = msg.sender.call{value: amount}("");
        require(sent, "Transfer failed!");
    }

    function withdrawERC20Prizes(address _tokenContractAddress) external nonReentrant {
        // withdraw all my token looping //https://solidity-by-example.org/mapping/
        uint256 amount = pendingERC20Withdrawals[msg.sender][_tokenContractAddress];
        require(amount > 0, "amount zero");
        pendingERC20Withdrawals[msg.sender][_tokenContractAddress] = 0;
        emit WithdrawMyPrizes("WithdrawMyPrizes");
        IERC20(_tokenContractAddress).transfer(msg.sender, amount);
    }

    function withdrawAllERC20Prizes(address _tokenContractAddress) external nonReentrant {
        // todo : withdraw specific token
        // uint256 amount = pendingERC20Withdrawals[msg.sender][_tokenAddress];
        // require(amount > 0, "amount zero");
        // pendingERC20Withdrawals[msg.sender][_tokenAddress] = 0;
        // emit WithdrawMyPrizes("WithdrawMyPrizes");
        // IERC20(_tokenAddress).transfer(msg.sender, amount);
    }

    function getPendingWithdrawals(address user) external view returns (uint256) {
        return pendingWithdrawals[user];
    }

    function setCoinPrice(uint256 _coinPrice) external onlyOwner {
        coinPrice = _coinPrice;
    }

    function cashout(uint256 _amount) external nonReentrant onlyOwner {
        rootOwner = owner();
        (bool sent, ) = rootOwner.call{value: _amount}("");
        require(sent, "Transfer failed!");
    }

    // emits
    // event ERC20RolledSlot(string rollString);
    // event Jackpot(string rollString);
    // event UltraWin(string rollString);
    // event SuperWin(string rollString);
    // event Wining(string rollString);

    function eRC20RolledSlot(string calldata _roll) external nonReentrant {
        emit ERC20RolledSlot(_roll);
    }

    function jackpot(string calldata _roll) external nonReentrant {
        emit Jackpot(_roll);
    }

    function ultraWin(string calldata _roll) external nonReentrant {
        emit UltraWin(_roll);
    }

    function superWin(string calldata _roll) external nonReentrant {
        emit SuperWin(_roll);
    }

    function winning(string calldata _roll) external nonReentrant {
        emit Winning(_roll);
    }

    // event WithdrawMyPrizes(string rollString);
    function withdrawMyPrizes(string calldata _roll) external nonReentrant {
        emit WithdrawMyPrizes(_roll);
    }
}

