pragma solidity ^0.8.0;

import "../core/BumperCoreNFT.sol";
import "../interfaces/IMarket.sol";
import "../interfaces/IMarketPositionManagement.sol";
import "../interfaces/IProtocolConfig.sol";

import "hardhat/console.sol";

/// @notice NFT token for taker position
contract MakerPositionNFT is BumperCoreNFT {
    function initialize(
        string memory name,
        string memory symbol,
        address _token,
        address _config
    ) external initializer {
        __BumperCoreNFT__init(name, symbol, _token, _config);
    }

    function _exists(uint tokenId)
        internal
        view
        virtual
        override
        returns (bool)
    {
        return IMarketPositionManagement(market).existsMakerPosition(tokenId);
    }

    // Terrible name. Should be getOwnerById
    function ownerOf(uint tokenId) public view override returns (address) {
        return IMarketPositionManagement(market).makerPositionOwner(tokenId);
    }

    function _transfer(
        address from,
        address to,
        uint tokenId
    ) internal override {
        super._transfer(from, to, tokenId);
        IMarketPositionManagement(market).safeTransferMakerPosition(
            from,
            to,
            tokenId
        );
    }

    function balanceOf(address owner) public view override returns (uint) {
        return IMarket(market).getMakerPositions(owner).length;
    }
}

