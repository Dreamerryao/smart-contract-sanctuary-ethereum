      //masterchef = _masterchef;
      //sushi = _sushi;
   }
   function deposit(uint256 _amount, address _tokenAddress)
        public 
        isWhitelisted(_tokenAddress) returns (uint256)
   {
        address _user = msg.sender;
        ERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
        Balances[msg.sender][_tokenAddress] += _amount;
            //getAmountOut for 50%
            //Approve to router for swap
            //Swap 50% amount
        uint256 _amountIn = SafeMath.div(_amount, 2);
        uint256 _amountOut = UniswapV2Router02(router).getAmountOut(_amountIn, 1000995255998182956717690, 1000000000000000000000000);
        uint256 _amountOutMin = SafeMath.div(SafeMath.mul(_amountOut, 9), 10);
        //withdraw(_amountOut, _tokenAddress);
        ERC20(_tokenAddress).approve(router, _amountIn);
        address[] memory _path;
        _path = new address[](2);
        _path[0] = token0;
        _path[1] = token1;
        
        uint256 _deadline = now + 600;
        UniswapV2Router02(router).swapExactTokensForTokens(_amountIn, _amountOutMin, _path, _user, _deadline);

        
            //LP to pair via router
            //getBalance SLP
            //approve SLP to masterchef
            //deposit to masterchef
   }
   function depositAll(address _tokenAddress) external {
      uint256 _amount = ERC20(_tokenAddress).balanceOf(msg.sender);
      ERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);
      Balances[msg.sender][_tokenAddress] += _amount;
   }
    /*function _approve(address _asset, uint256 _amount) internal public view returns (string memory) {
        return string(abi.encodePacked("_approve OK for ", _asset, " and ", _amount));

    }*/

   function withdraw(
      uint256 _amount,
      address _tokenAddress)
   public 
   {
      require(
         _amount <= Balances[msg.sender][_tokenAddress],
         "Not enough funds"
      );
      ERC20(_tokenAddress).transfer(msg.sender, _amount);
      Balances[msg.sender][_tokenAddress] -= _amount;
   }
   function withdrawEmergency() external onlyOwner {
        ERC20(token0).transfer(
            msg.sender,
            ERC20(token0).balanceOf(address(this))
        );
        ERC20(token0).transfer(
            msg.sender,
            ERC20(token1).balanceOf(address(this))
        );
   }
   function _withdrawAll(address _tokenAddress) 
      internal
   {
      ERC20(_tokenAddress).transfer(
         msg.sender,
         Balances[msg.sender][_tokenAddress]
      );
      Balances[msg.sender][_tokenAddress] = 0;
   }
