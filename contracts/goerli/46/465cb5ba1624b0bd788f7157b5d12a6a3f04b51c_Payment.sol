pragma solidity ^0.8.9;

import {IGuarantee} from "./IGuarantee.sol";
import {IPayment} from "./IPayment.sol";
import {IOnTimeMetadata} from "./IOnTimeMetadata.sol";

interface IOnTime is IOnTimeMetadata {
    /// @param guarantee_ IGuarantee Contract
    event NewGuarantee(address guarantee_);

    /// @param payment_ IPayment Contract
    event NewPayment(address payment_);

    /// @param opinion_ Text of client approval
    event ClientApproval(string opinion_);

    /// @param opinion_ Text of approver approval
    event ApproverApproval(string opinion_);

    event ClientDeclined();

    event ApproverDeclined();

    event InCompliance();

    event NonCompliance();

    event InForce();

    event Validated(bool result);

    event GuaranteesExecuted();

    /// @notice Add a new guarantee to the contract
    /// @dev    guarantee_ must to be a IGuarantee Contract.
    ///         guarantee_'s ensured must to be this contract and the taker must to be the client
    ///         msg.sender must to be the client or an allowed approved.
    ///         All approves must to be reset to initial status
    ///         approve expiration date must to be valid
    ///         restart the approve expiration date
    ///         inForce must to be false
    ///         Emits NewGuarantee event
    /// @param  guarantee_ IGuarantee Contract
    /// @param  approverId_ Id of the approver who is calling the function
    /// @return success boolean
    function addGuarantee(IGuarantee guarantee_, uint256 approverId_)
        external
        returns (bool success);

    /// @notice Add a new payment condition to the contract
    /// @dev    payment_ must to be a IPayment Contract.
    ///         payment_'s ensured must to be this contract and the payer must to be the client
    ///         msg.sender must to be the client or an allowed approved.
    ///         All approves must to be reset to initial status
    ///         approve expiration date must to be valid
    ///         restart the approve expiration date
    ///         inForce must to be false
    ///         Emits NewPayment event
    /// @param  payment_ IPayment Contract
    /// @param  approverId_ Id of the approver who is calling the function
    /// @return success boolean
    function addPayment(IPayment payment_, uint256 approverId_)
        external
        returns (bool success);

    /// @notice Approve all conditions in the contract
    /// @dev    msg.sender must to be the client or an allowed approved.
    ///         approve expiration date must to be valid
    ///         inForce must to be false
    ///         Emits ClientApproval event if msg.sender is the client.
    ///         Emits ApproverApproval event if msg.sender is an allowed approver.
    ///         Emits InForce event if client and approver is true
    /// @param  approverId_ Id of the approver who is calling the function
    /// @param  opinion_ text explain your approval
    /// @return success boolean
    function approve(uint256 approverId_, string memory opinion_)
        external
        returns (bool success);

    /// @notice decline all conditions in the contract
    /// @dev    msg.sender must to be the client or an allowed approved.
    ///         approve expiration date must to be valid
    ///         isInForce must to be false
    ///         Emits ClientDeclined event if msg.sender is the client.
    ///         Emits ApproverDeclined event if msg.sender is an allowed approver.
    /// @param  approverId_ Id of the approver who is calling the function
    /// @return success boolean
    function decline(uint256 approverId_) external returns (bool success);

    /// @notice check all payments and due dates to see if everything was paid correctly
    /// @dev    isInForce must to be true
    ///         Emits Validated event
    /// @return result boolean
    function validate() external returns (bool result);

    /// @notice execute all guarantees
    /// @dev    msg.sender must to be an allowed approved
    ///         isInForce must to be true
    ///         contract must to be non-compliance
    ///         Emits GuaranteesExecuted event and change inForce to false
    /// @return result boolean
    function executeGuarantees(uint256 approverId_)
        external
        returns (bool result);

    /// @notice Verify if approval is expirated
    /// @return result a boolean
    function isApproveExpirated() external view returns (bool result);

    /// @notice Verify if client already approved
    /// @return result a boolean
    function isClientApprove() external view returns (bool result);

    /// @notice Verify if bank already approved
    /// @return result a boolean
    function isBankApprove() external view returns (bool result);

    /// @notice Verify if contract is in force
    /// @return result a boolean
    function isInForce() external view returns (bool result);
}

