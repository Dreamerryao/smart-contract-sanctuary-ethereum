interface IPayment is IPaymentMetadata {
    event NewEnsured(address newEnsured_);

    event Paid(uint value_);

    event Closed();

    /// @notice Set a new ensured
    /// @dev    msg.sender must to be this payment_'s owner
    ///         contract ensured's isInForce must to be false
    ///         Emits NewEnsured event
    /// @return success boolean
    function changeEnsured(IOnTime newEnsured_) external returns (bool success);

    /// @notice Receive a payment
    /// @dev    contract isInForce must to be true
    ///         msg.sender must to be contract's client
    ///         paidValue + msg.value must to be small or equal total value
    ///         msg.value and time must (Payment Structure) to be save in payments mapping
    ///         closeDate must to be 0
    ///         Emits Paid event
    /// @return success boolean
    function pay() external payable returns (bool success);

    /// @notice Close the payment (this contract)
    /// @dev    msg.sender must to be this payment's owner
    ///         contract isInForce must to be false
    ///         Emits Closed event
    /// @return success boolean
    function close() external returns (bool success);

    /// @notice Check if all value was paid untill due date
    /// @dev    contract isInForce must to be true
    /// @return result boolean
    function validate() external view returns (bool result);

    /// @notice Verify if an address is the owner of this payment
    /// @param address_ the address that will be compared
    /// @return result a boolean
    function isOwner(address address_) external view returns (bool result);

    /// @notice Verify if an address is the payer of this payment
    /// @param address_ the address that will be compared
    /// @return result a boolean
    function isPayer(address address_) external view returns (bool result);

    /// @notice Verify if an address is the ensured of this payment
    /// @param address_ the address that will be compared
    /// @return result a boolean
    function isEnsured(address address_) external view returns (bool result);

    /// @notice return if payment is not valid / closed
    /// @return result a boolean
    function isClosed() external view returns (bool result);

    /// @notice return if all value was paid
    /// @return result a boolean
    function isPaid() external view returns (bool result);
}

