pragma solidity ^0.8.9;

import "hardhat/console.sol";
import {IPayment} from "../interfaces/IPayment.sol";
import {PaymentStruct} from "../interfaces/IPaymentMetadata.sol";
import {IOnTime} from "../interfaces/IOnTime.sol";

contract Payment is IPayment {
    address private _owner;
    address private _payer;
    IOnTime private _ensured;
    uint256 private _dueDate;
    uint256 private _closeDate;
    uint private _value;
    uint private _paidValue;
    uint256 private _paidAmount;

    mapping(uint256 => PaymentStruct) _payments;

    constructor(
        address payer_,
        IOnTime ensured_,
        uint256 dueDate_,
        uint value_
    ) {
        _owner = msg.sender;
        _payer = payer_;
        _ensured = ensured_;
        _dueDate = dueDate_;
        _closeDate = 0;
        _value = value_;
        _paidValue = 0;
        _paidAmount = 0;
    }

    modifier onlyOwner() {
        require(msg.sender == _owner, "msg.sender MUST to be owner");
        _;
    }

    modifier onlyEnsured() {
        require(msg.sender == address(_ensured), "msg.sender MUST to be owner");
        _;
    }

    modifier onlyClient() {
        require(
            msg.sender == _ensured.client(),
            "msg.sender MUST to be ensured contract's client"
        );
        _;
    }

    modifier inForce() {
        require(
            _ensured.isInForce() == true,
            "Ensured of the payment is not in force!"
        );
        _;
    }

    modifier notInForce() {
        require(
            _ensured.isInForce() == false,
            "Ensured of the payment is in force!"
        );
        _;
    }

    modifier onlyNotInCompliance() {
        require(
            _ensured.inCompliance() == true,
            "Warranty Assured is in compliance"
        );
        _;
    }

    modifier closed() {
        require(_closeDate > 0, "the payment is not closed!");
        _;
    }

    modifier notClosed() {
        require(_closeDate == 0, "the payment is closed!");
        _;
    }

    /**
     * See IPayment Interface (changeEnsured function)
     */
    function changeEnsured(IOnTime newEnsured_)
        external
        override
        onlyOwner
        notInForce
        returns (bool success)
    {
        _ensured = newEnsured_;
        emit NewEnsured(address(newEnsured_));
        return true;
    }

    /**
     * See IPayment Interface (pay function)
     */
    function pay()
        external
        payable
        override
        onlyClient
        notClosed
        inForce
        returns (bool success)
    {
        require(msg.value + _paidValue <= _value, "contract value was exceed!");
        _paidAmount++;
        _paidValue += msg.value;
        _payments[_paidAmount] = PaymentStruct(block.timestamp, msg.value);
        emit Paid(msg.value);
        return true;
    }

    /**
     * See IPayment Interface (close function)
     */
    function close()
        external
        override
        onlyEnsured
        notInForce
        returns (bool success)
    {
        _closeDate = block.timestamp;
        emit Closed();
        return true;
    }

    /**
     * See IPayment Interface (validate function)
     */
    function validate() external view override inForce returns (bool result) {
        if (block.timestamp <= _dueDate) {
            return true;
        } else if (_paidValue == _value) {
            return true;
        } else {
            return false;
        }
    }

    /**
     * See IPayment Interface (isOwner function)
     */
    function isOwner(address address_)
        external
        view
        override
        returns (bool result)
    {
        return _owner == address_;
    }

    /**
     * See IPayment Interface (isPayer function)
     */
    function isPayer(address address_)
        external
        view
        override
        returns (bool result)
    {
        return _payer == address_;
    }

    /**
     * See IPayment Interface (isEnsured function)
     */
    function isEnsured(address address_)
        external
        view
        override
        returns (bool result)
    {
        return address(_ensured) == address_;
    }

    /**
     * See IPayment Interface (isClosed function)
     */
    function isClosed() external view override returns (bool result) {
        return _closeDate > 0;
    }

    /**
     * See IPayment Interface (isPaid function)
     */
    function isPaid() external view override returns (bool result) {
        return _value == _paidValue;
    }

    /**
     * See IPayment Interface (owner function)
     */
    function owner() external view override returns (address owner_) {
        return _owner;
    }

    /**
     * See IPayment Interface (payer function)
     */
    function payer() external view override returns (address payer_) {
        return _payer;
    }

    /**
     * See IPayment Interface (ensured function)
     */
    function ensured() external view override returns (address ensured_) {
        return address(_ensured);
    }

    /**
     * See IPayment Interface (dueDate function)
     */
    function dueDate() external view override returns (uint256 dueDate_) {
        return _dueDate;
    }

    /**
     * See IPayment Interface (value function)
     */
    function value() external view override returns (uint value_) {
        return _value;
    }

    /**
     * See IPayment Interface (paidValue function)
     */
    function paidValue() external view override returns (uint paidValue_) {
        return _paidValue;
    }

    /**
     * See IPayment Interface (paidAmount function)
     */
    function paidAmount() external view override returns (uint256 paidAmount_) {
        return _paidAmount;
    }

    /**
     * See IPayment Interface (dueDate function)
     */
    function payments(uint256 paymentId_)
        external
        view
        override
        returns (PaymentStruct memory payment_)
    {
        require(paymentId_ <= _paidAmount, "paymentId must to be valid");
        return _payments[paymentId_];
    }

    /**
     * See IPayment Interface (closeDate function)
     */
    function closeDate() external view override returns (uint256 closeDate_) {
        return _closeDate;
    }
}

