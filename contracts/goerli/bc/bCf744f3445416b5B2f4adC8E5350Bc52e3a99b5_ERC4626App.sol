import {MockERC20} from "./MockERC20.sol";
import {MockERC4626} from "./MockERC4626.sol";
import {ERC4626User} from "../src/test/utils/users/ERC4626User.sol";
import "hardhat/console.sol";

contract ERC4626App {
    MockERC20 underlying;
    MockERC4626 vault;
    ERC4626User alice;
    ERC4626User bob;
    address owner; 
    mapping (address => ERC4626User) _users;

    event DepositAssetsEvent(
        address sender,
        uint value
    );

    event LogBalanceEvent (
        address user,
        uint ENVT,
        uint ETH
    );

    constructor() {
        owner=msg.sender;
            
        //Setup underlying asset token 
        underlying = new MockERC20("Envision Ethereum", "EETH", 18);
        
        //Setup vault token
        vault = new MockERC4626(underlying, "ENV Vault", "ENVT");
        
        // //Setup users
        // alice = new ERC4626User(vault, underlying);
        // bob = new ERC4626User(vault, underlying);
        // uint256 aliceUnderlyingAmount = 50000; 
        // uint256 bobUnderlyingAmount = 50000;  
        
        // console.log("Alice address:", address(alice));
        // console.log("Bob address  :", address(bob));
        // console.log("Unerlying address:", address(underlying));
        // console.log("Vault address    :", address(vault));

        // //Setup initial assets for Alice and Bob
        // underlying.mint(address(alice), aliceUnderlyingAmount);
        // underlying.mint(address(bob), bobUnderlyingAmount);
        // alice.approve(address(vault), aliceUnderlyingAmount);
        // bob.approve(address(vault), bobUnderlyingAmount);

    }

    function destroySmartContract(address payable contractAddress) public {
      if (msg.sender == owner) 
         selfdestruct(contractAddress);
    }

    //Receive Ether from outside
    receive() 
        payable
        external {

    }

    function Deposit_Assets () 
        internal {

        //Record event
        emit DepositAssetsEvent(msg.sender, msg.value);

        //Get the users's Dapp wallet for sender
        ERC4626User user = _users[msg.sender];

        //If new user, crete new Dapp wallet
        if (user.UserCreated() == 0) {
            user = new ERC4626User(vault, underlying);
            _users[msg.sender] = user;
        }

        uint amount = msg.value;
        // user.approve(address(vault), amount);
        uint shareAmount = user.deposit(amount, address(user));
        user.approve(address(vault), amount);
        
        emit LogBalanceEvent(msg.sender, shareAmount, vault.convertToAssets(vault.balanceOf(msg.sender)));
    }
    
    function CompareStrings(string memory string1, string memory string2) 
        internal 
        pure 
        returns (bool) {
        
        return keccak256(bytes(abi.encodePacked(string1))) == keccak256(bytes(abi.encodePacked(string2)));
    }

    function Assets_Withdraw(string calldata name, uint amount) public {
        // uint shareAmount;
        if (CompareStrings(name, "alice")) {
            alice.withdraw(amount, address(alice), address(alice));
        }
        else if (CompareStrings(name,"bob")) {
            bob.withdraw(amount, address(bob), address(bob));
        }
        else 
        {
            //Invalid name
        }

        ShowBalances();
    }

    function Shares_Buy(string calldata name, uint shares) public {
        // uint amount = vault.convertToAssets(shares);
            
        if (CompareStrings(name, "alice")) {
            //alice.approve(address(vault), amount);
            alice.mint(shares, address(alice));
        }
        else if (CompareStrings(name,"bob")) {
            // bob.approve(address(vault), amount);
            bob.mint(shares, address(bob));
        }
        else 
        {
            //Invalid name
        }

        ShowBalances();
    }
    
    function Shares_Redeem(string calldata name, uint shares) public {
        // uint shareAmount;
        if (CompareStrings(name, "alice")) {
            alice.redeem(shares, address(alice), address(alice));
        }
        else if (CompareStrings(name,"bob")) {
            bob.redeem(shares, address(bob), address(bob));
        }
        else 
        {
            //Invalid name
        }

        ShowBalances();
    }
    
    
    function Mutate_Vault(uint amount) public {
        underlying.mint(address(vault), amount);
        ShowBalances();
    }

    function ShowBalances() internal view {
        underlying.logAllBalances("Underlying");
        vault.logAllBalances("Vault");
        console.log("Alice Assets", 
            vault.convertToAssets(vault.balanceOf(address(alice))));
        console.log("Bob Assets", 
            vault.convertToAssets(vault.balanceOf(address(bob))));
    }

    function ShareholderAddress(string memory name) public view returns (address) {
        address emptyAddr;

        if (CompareStrings(name, "alice")) {
            return address(alice);
        }
        else if (CompareStrings(name,"bob")) {
            return address(bob);
        }
        else 
            return emptyAddr;
    }

    function UnderlyingAddress() public view returns (address) {
        return address(underlying);
    }

    function VaultAddress() public view returns (address) {
        return address(vault);
    }

    function Alice_Assets() public view returns (uint) {
        return vault.convertToAssets(vault.balanceOf(address(alice)));
    }

    function Bob_Assets() public view returns (uint) {
        return vault.convertToAssets(vault.balanceOf(address(bob)));
    }

    function Alice_Shares() public view returns (uint) {
        return vault.balanceOf(address(alice));
    }

    function Bob_Shares() public view returns (uint) {
        return vault.balanceOf(address(bob));
    }

    function Alice_Wallet() public view returns (uint) {
        return underlying.balanceOf(address(alice));
    }

    function Bob_Wallet() public view returns (uint) {
        return underlying.balanceOf(address(bob));
    }

    function Vault_UL_Balance() public view returns (uint) {
        return underlying.balanceOf(address(vault));
    }
}

