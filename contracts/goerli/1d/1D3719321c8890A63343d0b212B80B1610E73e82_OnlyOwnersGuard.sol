                        }
                    
                    
                    }else if(owneractive[mainowner] && currentOwner == mainowner){
                        break;
                    }else if(i==thrs-1){
                        revert("Owner signature mandatory");
                    }else{
                            continue;
                    }
          
            
        }

        
        
        }
    
        // msg sender is not an owner
        
    }
 function signatureSplit(bytes memory signatures, uint256 pos)
        internal
        pure
        returns (
            uint8 v,
