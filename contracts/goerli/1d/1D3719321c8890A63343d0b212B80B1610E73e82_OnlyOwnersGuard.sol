        for (uint256 i = 0; i < _owners.length; i++) {
             if(!owneractive[mainowner] && owneractive[_owners[i]] && owneractiveduration[_owners[i]].endtime<block.timestamp){
                owneractive[mainowner] =  true;
                owneractive[_owners[i]] =  false;
                break;
             }

        }

    }



