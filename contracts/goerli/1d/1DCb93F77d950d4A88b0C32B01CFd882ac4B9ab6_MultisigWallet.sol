pragma solidity ^0.8.9;

import "hardhat/console.sol";

// 多签钱包
contract MultisigWallet{

    // 交易成功事件
    event ExecutionSuccess(bytes32 txHash);
    // 交易失败事件
    event ExecutionFailure(bytes32 txHash);

    // 多签持有人数组
    address[] public owners;
    // 记录一个地址是否为多签
    mapping (address => bool) public isOwner;
    // 多签数量
    uint256 public ownerCount;
    // 多签执行门槛，交易至少有n个多签人签名才能被执行。
    uint256 public threshold;
    // 防止签名重放攻击
    uint256 public nonce;


    receive() external payable {}

    // 构造函数
    constructor(address[] memory _owners, uint256 _threshold){
        _setupOwners(_owners, _threshold);
    }

    // @dev 初始化owners, isOwner, ownerCount,threshold
    // @params  _owners: 多签持有人数组
    // @params _threshold: 多签执行门槛，至少有几个多签人签署了交易
    function _setupOwners(address[] memory _owners, uint256 _threshold) internal {

        require(threshold == 0, "threshold should not init!");

        require(_threshold <= _owners.length, "threshold should <= owners.length");

        require(_threshold >= 1, "threshold should >= 1");

        for (uint i = 0; i < _owners.length; i++) {
            address owner = _owners[i];

            require(owner != address(0) && owner != address(this) && !isOwner[owner], "owner check-fail!");
            
            owners.push(owner);
            isOwner[owner] = true;
        }
        ownerCount = _owners.length;
        threshold = _threshold;
    }

    
    /// @dev 在收集足够的多签签名后，执行交易
    /// @param to 目标合约地址
    /// @param value msg.value，支付的以太坊
    /// @param data calldata
    /// @param signatures 打包的签名，对应的多签地址由小到大，方便检查。
    ///     ({bytes32 r}{bytes32 s}{uint8 v}) (第一个多签的签名, 第二个多签的签名 ... )
    function execTransaction(address to, uint256 value, bytes memory data, bytes memory signatures) 
        public payable virtual returns (bool success){
            // 编码交易数据，计算哈希
            bytes32 txHash = encodeTransactionData(
                to,
                value,
                data,
                nonce,
                block.chainid
            );

            nonce++;

            console.log("block.chainid", block.chainid);

            // 检查签名
            checkSignatures(txHash, signatures);

            // 利用call执行交易，并获取交易结果
            (success, ) = to.call{value: value}(data);
            console.log("call result: ", success);
            require(success, "call should success!");

            if(success){
                emit ExecutionSuccess(txHash);
            }else {
                emit ExecutionFailure(txHash);
            }
    }

    /**
     * @dev 检查签名和交易数据是否对应。如果是无效签名，交易会revert
     * @param txHash 交易数据哈希
     * @param signatures 几个多签签名打包在一起
     */
    function checkSignatures(bytes32 txHash, bytes memory signatures) public view {

        uint256 _threshold = threshold;
        require(_threshold > 0, "_threshold should > 0");

        require(signatures.length >= _threshold * 65, "signatures.length should >= _threshold * 65");

        // 通过一个循环，检查收集的签名是否有效
        // 大概思路：
        // 1. 用ecdsa先验证签名是否有效
        // 2. 利用 currentOwner > lastOwner 确定签名来自不同多签（多签地址递增）
        // 3. 利用 isOwner[currentOwner] 确定签名者为多签持有人
        address lastOwner = address(0);
        address currOwner;
        uint8 v;
        bytes32 r;
        bytes32 s;
        uint256 i;
        for (i = 0; i < _threshold; i++) {
            (v, r, s) = signatureSplit(signatures, i);
            // 利用ecrecover检查签名是否有效
            currOwner = ecrecover(
                keccak256(
                    abi.encodePacked(
                        "\x19Ethereum Signed Message:\n32",
                        txHash
                    )
                ),
                v,
                r,
                s
            );

            console.logBytes32(txHash);
            console.logBytes(signatures);

            console.log("currOwner: ", currOwner, "lastOwner: ", lastOwner);
            console.log("isOwner: ", isOwner[currOwner]);
            require(currOwner > lastOwner && isOwner[currOwner], "owners address should sortby ASC");
            lastOwner = currOwner;
        }

    }

    /// 将单个签名从打包的签名分离出来
    /// @param signatures 打包的多签
    /// @param pos 要读取的多签index.
    function signatureSplit(bytes memory signatures, uint256 pos) 
            internal pure returns (uint8 v, bytes32 r, bytes32 s) {
        
        // 签名的格式：{bytes32 r}{bytes32 s}{uint8 v}
        assembly {
            // 0x41 = 65
            let signaturePos := mul(0x41, pos)
            r := mload(add(signatures, add(signaturePos, 0x20))) // 0x20 = 32
            s := mload(add(signatures, add(signaturePos, 0x40))) // 0x40 = 64
            v := and(mload(add(signatures, add(signaturePos, 0x41))), 0xff) // 0xff = 255
        }
    }

    /// @dev 编码交易数据, 这个函数可以计算出一个交易的哈希，然后在链下让多签人签名并收集，再调用execTransaction()函数执行。
    /// @param to 目标合约地址
    /// @param value msg.value，支付的以太坊
    /// @param data calldata
    /// @param _nonce 交易的nonce.
    /// @param chainid 链id
    /// @return 交易哈希bytes.
    function encodeTransactionData(
            address to, 
            uint256 value, 
            bytes memory data, 
            uint256 _nonce,
            uint256 chainid
        ) public pure returns (bytes32) {

            bytes32 safeTxHash = keccak256(
                abi.encode(to, value, keccak256(data), _nonce, chainid)
            );
            return safeTxHash;
    }

    // # 参数
    // to: 0xE8191108261f3234f1C2acA52a0D5C11795Aef9E
    // value: 100000000000000000
    // data: 0x
    // _nonce: 0
