pragma solidity ^0.8.9;

import "./LoanCommon.sol";
import "hardhat/console.sol";

contract LoanServiceExtension {

    /// @notice create loan validate
    /// @param _loanParametersAddress The Loan Parameters Address
    /// @param _nftContracts list of nft contracts
    /// @param _nftTokenIds list of nft token Ids
    /// @param _nftTokenAmounts list of nft token amounts
    /// @param _standards list of nft standards
    /// @param _nftEstimatedValue The nft estimated value
    /// @param _loanAmount The loan amount
    /// @param _loanDuration The duration of the loan
    function createLoanValidate(
        address _loanParametersAddress,
        address[] memory _nftContracts,
        uint256[] memory _nftTokenIds,
        uint256[] memory _nftTokenAmounts,
        LoanCommon.Standard[] memory _standards,
        uint256 _nftEstimatedValue,
        uint256 _loanAmount,
        uint256 _loanDuration) public view returns (bool) {

        require(_loanParametersAddress != address(0x0), "Loan Parameter Address must be set");

        /// #########################################
        /// TFL-0001 TFL-0002, TFL-0003, TFL-0004,
        /// TFL-0005 TFL-0006, TFL-0010, TFL-0007
        /// #########################################

        /////////////////////////////////////////
        // Reference TFL-0018
        /////////////////////////////////////////
        require(
            LoanParameters_I(_loanParametersAddress).enabled(),
            "The ability to create new loan campaign is disabled"
        );

        /////////////////////////////////////////
        // Reference TFL-0013
        /////////////////////////////////////////
        require(
            _loanDuration >= LoanParameters_I(_loanParametersAddress).minLoanDurationInDays(),
            "The loan duration must be >= to the minimum duration in days"
        );

        /////////////////////////////////////////
        // Reference TFL-0014
        /////////////////////////////////////////
        require(
            _loanDuration <= LoanParameters_I(_loanParametersAddress).maxLoanDurationInDays(),
            "The loan duration must be less than or equal ot the maximum duration in days"
        );

        require(
            _nftContracts.length > 0,
            "At least one NFT contract is required"
        );

        require(
            _nftContracts.length == _nftTokenIds.length &&
            _nftContracts.length == _nftTokenAmounts.length &&
            _nftContracts.length == _standards.length,
            "the NFT arrays must be equal length"
        );

        /////////////////////////////////////////////
        // Reference TFL-0011
        /////////////////////////////////////////////
        require(
            _loanAmount >= LoanCommon.calculatePercentage(
            _nftEstimatedValue,
            LoanParameters_I(_loanParametersAddress).minLoanPercentageOfCollateral(),
            LoanParameters_I(_loanParametersAddress).precision()
        ), "The loan amount must be >= to the minimum loan percentage of collateral"
        );

        /////////////////////////////////////////////
        // Reference TFL-0012
        /////////////////////////////////////////////
        require(
            _loanAmount <= LoanCommon.calculatePercentage(
                _nftEstimatedValue,
                LoanParameters_I(_loanParametersAddress).maxLoanPercentageOfCollateral(),
                LoanParameters_I(_loanParametersAddress).precision()
            ), "The loan amount must be <= to the maximum loan percentage of collateral"
        );

        return true;
    }
}

