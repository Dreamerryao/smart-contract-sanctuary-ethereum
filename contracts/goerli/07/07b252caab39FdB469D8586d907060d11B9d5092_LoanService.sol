pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

import "./LoanCommon.sol";
import "./LoanServiceExtension.sol";

contract LoanService is Ownable {
    using SafeMath for uint256;

    modifier onlyServiceContractsOrOwner() {
        address loanServicePaymentAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServicePaymentAddress();
        require(loanServicePaymentAddress != address(0x0), "loan service payment address must be set");
        require(
            msg.sender == loanServicePaymentAddress || msg.sender == owner(),
            "caller must be a service contract or owner"
        );
        _;
    }

    /// @notice loan service activities
    enum LoanServiceEvent {
        CREATED,      // 0
        ACTIVATED     // 1
    }

    /// @notice loan service event
    event LoanServiceActivity(uint256 indexed loanId, LoanServiceEvent activity, uint256 timestamp);

    /// @notice The loan oracle address
    address public loanOracleAddress;

    /// @notice Set the oracle address
    /// @param _address The new loan storage address
    function setLoanOracleAddress(address _address) public onlyOwner {
        loanOracleAddress = _address;
    }

    /// @notice internal function to get the loan oracle address
    function getLoanOracleAddress() internal view returns (address) {
        require (loanOracleAddress != address(0x0), "loan oracle address must be set");
        return loanOracleAddress;
    }


    /// @notice Loan Service Extension
    /// @dev Extension was added as loan length exceeded maximum size i.e.
    /// @dev   (exceeds 24576 bytes (a limit introduced in Spurious Dragon))
    LoanServiceExtension loanServiceExtension;

    constructor() {
        loanServiceExtension = new LoanServiceExtension();
    }

    /// @notice internal function to prevent stack too deep solidity compile error
    /// @notice Crate collateral record
    /// @param _loanId The loan Id
    /// @param _nftContracts list of nft contracts
    /// @param _nftTokenIds list of nft token Ids
    /// @param _nftTokenAmounts list of nft token amounts
    /// @param _standards list of nft standards
    /// @param _nftEstimatedValue The nft estimated value
    function createCollateralRecord(
        uint256 _loanId,
        address[] memory _nftContracts,
        uint256[] memory _nftTokenIds,
        uint256[] memory _nftTokenAmounts,
        LoanCommon.Standard[] memory _standards,
        uint256 _nftEstimatedValue
    ) internal {
        address loanServiceNftCollateralAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceNftCollateralAddress();
        LoanServiceNftCollateral_I(loanServiceNftCollateralAddress).create(
            _loanId,
            _nftContracts,
            _nftTokenIds,
            _nftTokenAmounts,
            _standards,
            _nftEstimatedValue
        );
    }

    /// @notice create loan
    /// @param _nftContracts list of nft contracts
    /// @param _nftTokenIds list of nft token Ids
    /// @param _nftTokenAmounts list of nft token amounts
    /// @param _standards list of nft standards
    /// @param _nftEstimatedValue The nft estimated value
    /// @param _loanAmount The loan amount
    /// @param _loanDuration The duration of the loan
    function createLoan(
        address[] memory _nftContracts,
        uint256[] memory _nftTokenIds,
        uint256[] memory _nftTokenAmounts,
        LoanCommon.Standard[] memory _standards,
        uint256 _nftEstimatedValue,
        uint256 _loanAmount,
        uint256 _loanDuration) public payable {

        address loanParametersAddress = LoanOracle_I(getLoanOracleAddress()).getLoanParametersAddress();
        require (loanParametersAddress != address(0x0), "loan parameter address must be set");

        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        require(storageAddress != address(0x0), "Loan Storage Address must be set");

        /////////////////////////////////////////
        // reference TFL-0016
        /////////////////////////////////////////
        uint256 amount = calculateCreateLoanFee(_nftEstimatedValue);
        require(msg.value == amount, "eth must be equal to fee amount");

        loanServiceExtension.createLoanValidate(
            loanParametersAddress,
            _nftContracts,
            _nftTokenIds,
            _nftTokenAmounts,
            _standards,
            _nftEstimatedValue,
            _loanAmount,
            _loanDuration
        );

        //////////////////
        // Create Loan
        //////////////////
        uint256 loanId = LoanStorage_I(storageAddress).create(msg.sender, _loanAmount, _loanDuration);

        //////////////////
        // Create Collateral
        //////////////////
        createCollateralRecord(loanId, _nftContracts, _nftTokenIds, _nftTokenAmounts, _standards, _nftEstimatedValue);

        address payable loanStorageTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageTreasuryAddress();
        loanStorageTreasuryAddress.transfer(msg.value);
        address loanServiceTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceTreasuryAddress();
        address profitAddress = LoanServiceTreasury_I(loanServiceTreasuryAddress).profitAddress();
        require (profitAddress != address(0x0), "profit address required");
        LoanServiceTreasury_I(loanServiceTreasuryAddress).addFunds(profitAddress, msg.value);

        // Create Loan Created Event
        emit LoanServiceActivity(loanId, LoanServiceEvent.CREATED, block.timestamp);

    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice calculate number of fractionalized shares
    /// @param _loanAmount The amount of the loan
    /// @param _acceptedAmount The offer accepted amount
    function calculateShares(uint256 _loanAmount, uint256 _acceptedAmount) internal view returns (uint256) {
        address loanParametersAddress = LoanOracle_I(getLoanOracleAddress()).getLoanParametersAddress();
        return LoanCommon.calculateShares(
            _loanAmount,
            _acceptedAmount,
            LoanParameters_I(loanParametersAddress).fractionalizeSupply(),
            LoanParameters_I(loanParametersAddress).precision()
        );
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice mint fractionalized tokens
    function mintFractionalizeToken() internal returns (uint256) {
        address loanParametersAddress = LoanOracle_I(getLoanOracleAddress()).getLoanParametersAddress();
        address loanServiceTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceTreasuryAddress();
        // Mint Fractionalize Loan
        return LoanServiceTreasury_I(loanServiceTreasuryAddress)
            .mintFractionalizeToken(LoanParameters_I(loanParametersAddress).fractionalizeSupply());
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice get the profit address
    function getProfitAddress() internal view returns (address) {
        return LoanStorageTreasury_I(getLoanStorageTreasuryAddress()).profitAddress();
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice get the loan storage treasury address
    function getLoanStorageTreasuryAddress() internal view returns (address payable) {
        return LoanOracle_I(getLoanOracleAddress()).getLoanStorageTreasuryAddress();
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice update the loan collateral nft token id
    /// @param _loanId the loan id
    /// @param _nftTokenId the nft token id
    function updateLoanCollateralNftTokenId(uint256 _loanId, uint256 _nftTokenId) internal {
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanStorage_I(storageAddress).updateLoanCollateralNftTokenId(_loanId, _nftTokenId);
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice update the loan calculated amount
    /// @param _loanId the loan id
    /// @param _calculatedInterest the calculated interest
    function updateLoanCalculatedInterest(uint256 _loanId, uint256 _calculatedInterest) internal {
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanStorage_I(storageAddress).updateCalculatedInterest(_loanId, _calculatedInterest);
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice update the loan calculated amount
    /// @param _loanId the loan id
    /// @param _calculatedAmount the calculated amount
    function updateLoanCalculatedAmount(uint256 _loanId, uint256 _calculatedAmount) internal {
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanStorage_I(storageAddress).updateCalculatedAmount(_loanId, _calculatedAmount);
    }

    /// @notice internal function to prevent stack too deep solidity error
    /// @notice handles calculated amount and calculated interest
    /// @param _loan The loan
    /// @param _offers The list of offers
    function processOffers(LoanCommon.Loan memory _loan, uint256[] memory _offers) internal {
        address loanServiceTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceTreasuryAddress();
        address loanServiceOfferAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceOfferAddress();
        uint256 loanRemainingBalance = _loan.amount;
        uint256 loanCalculatedAmount = 0;
        for (uint256 i = 0; i < _offers.length; i++) {
            uint256 offerId = _offers[i];
            LoanCommon.Offer memory offer = LoanServiceOffer_I(loanServiceOfferAddress).offers(offerId);

            if (offer.offeredAmount <= loanRemainingBalance) {
                LoanServiceOffer_I(loanServiceOfferAddress).updateAcceptedAmount(offerId, offer.offeredAmount);
                offer.acceptedAmount = offer.offeredAmount;

            } else {
                // If the bid offered is greater then the loanRemainingBalance it must be the last bid in the array
                require(
                    i == _offers.length - 1,
                    "The first accumulating bid that exceeds loan amount should be the last bid item"
                );
                // We set the bid accepted amount to the loan remaining balance to give the exact amount the borrower
                // is asking for. This means the lenders original offer is being reduced. This contract is depending
                // on the front end to supply the bid in the correct order per the business requirements.
                LoanServiceOffer_I(loanServiceOfferAddress).updateAcceptedAmount(offerId, loanRemainingBalance);
                offer.acceptedAmount = loanRemainingBalance;
            }

            uint256 ethAmount = LoanServiceTreasury_I(loanServiceTreasuryAddress).account(offer.lender);
            require(ethAmount >= offer.acceptedAmount, "lender does not have enough funds");

            // Transfer loan unit to Borrower
            LoanServiceTreasury_I(loanServiceTreasuryAddress).transferFunds(offer.lender, _loan.holder, offer.acceptedAmount);

            LoanServiceOffer_I(loanServiceOfferAddress).updateStatus(offerId, LoanCommon.OfferStatus.ACCEPTED);
            loanRemainingBalance = loanRemainingBalance.sub(offer.acceptedAmount);
            loanCalculatedAmount = loanCalculatedAmount.add(offer.acceptedAmount);
        }

        // Update loan calculated amount
        updateLoanCalculatedAmount(_loan.loanId, loanCalculatedAmount);

        // Update loan calculated interest
        updateLoanCalculatedInterest(_loan.loanId, calculateInterest(loanCalculatedAmount, _offers));
    }

    /// @notice calculate the interest of a list of offers for a given amount
    /// @dev the accepted amount on the offer needs to be set otherwise the calculation is zero
    /// @dev can only be called for active loans
    /// @param _amount The amount to calculate against
    /// @param _offerIds A list of offer ids
    function calculateInterest(uint256 _amount, uint256[] memory _offerIds) public view returns (uint256) {
        address loanServiceOfferAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceOfferAddress();

        uint256 earnings = 0;
        for (uint256 i = 0; i < _offerIds.length; i++) {
            uint256 offerId = _offerIds[i];
            LoanCommon.Offer memory offer = LoanServiceOffer_I(loanServiceOfferAddress).offers(offerId);
            earnings = earnings.add(offer.acceptedAmount.mul(offer.interest));
        }

        return earnings.mul(10).div(_amount);
    }

    /// @notice Activate Loan
    /// @dev _loanBids should be passed in sorted by least interest and earliest date so depending on the application
    /// @param _loanId The ID of the loan
    /// @param _offers The bids that the borrower is applying against their loan
    /// @param _tokenUri The token uri
    function activateLoan(uint256 _loanId, uint256[] memory _offers, string memory _tokenUri) public payable {
        address loanServiceNftCollateralAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceNftCollateralAddress();

        // Get Loan
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanCommon.Loan memory loan = LoanStorage_I(storageAddress).loans(_loanId);
        LoanCommon.NftCollateral memory collateral = LoanServiceNftCollateral_I(loanServiceNftCollateralAddress).collaterals(_loanId);

        /////////////////////////////////////////
        // reference TFL-0017
        /////////////////////////////////////////
        uint256 fee = calculateActivationFee(collateral.nftEstimatedValue);
        require(msg.value == fee, "must pass in the exact fee amount");

        getLoanStorageTreasuryAddress().transfer(msg.value);
        address loanServiceTreasuryAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceTreasuryAddress();
        LoanServiceTreasury_I(loanServiceTreasuryAddress).addFunds(getProfitAddress(), msg.value);

        // The loan borrower must be the one to activate the loan
        require(
            msg.sender == loan.holder,
            "The nft holder associated with the loan must be the one to activate the loan"
        );

        processOffers(loan, _offers);

        address loanServiceOfferAddress = LoanOracle_I(getLoanOracleAddress()).getLoanServiceOfferAddress();
        /// Deny remaining bids
        /// TODO do we need this? - waste of gas just to set a status
        uint256[] memory offers = LoanServiceOffer_I(loanServiceOfferAddress).offersByLoanId(_loanId);
        for (uint256 i = 0; i < offers.length; i++) {
            uint256 offerId = offers[i];
            LoanCommon.Offer memory offer = LoanServiceOffer_I(loanServiceOfferAddress).offers(offerId);
            if (offer.status != LoanCommon.OfferStatus.ACCEPTED) {
                LoanServiceOffer_I(loanServiceOfferAddress).updateStatus(offerId, LoanCommon.OfferStatus.DENIED);
            }
        }

        // Update Status ACTIVE
        LoanStorage_I(storageAddress).updateStatus(_loanId, LoanCommon.LoanStatus.ACTIVE);

        // Update loanStart - the date the loan became active
        LoanStorage_I(storageAddress).updateLoanActivationDate(_loanId, block.timestamp);

        // Mint Fractionalize Loan
        uint256 tokenId = mintFractionalizeToken();
        LoanStorage_I(storageAddress).updateFractionalizedTokenId(loan.loanId, tokenId);
        loan.fractionalizedTokenId = tokenId;

        /// ##############################
        /// ALLOCATE FRACTIONALIZED TOKENS
        /// ##############################
        for (uint256 i = 0; i < _offers.length; i++) {
            uint256 offerId = _offers[i];
            LoanCommon.Offer memory offer = LoanServiceOffer_I(loanServiceOfferAddress).offers(offerId);
            uint256 shares = calculateShares(loan.amount, offer.acceptedAmount);
            LoanServiceTreasury_I(loanServiceTreasuryAddress).fractionalizeTokenSafeTransferFrom(offer.lender, tokenId, shares);
        }

        /// ##############################
        /// ESCROW THE NFT
        /// ##############################
        for (uint256 i = 0; i < collateral.nftContracts.length; i++) {
            if (collateral.standards[i] == LoanCommon.Standard.ERC721) {
                ERC721_I(collateral.nftContracts[i]).safeTransferFrom(
                    msg.sender,
                    getLoanStorageTreasuryAddress(),
                    collateral.nftTokenIds[i]
                );
            } else if (collateral.standards[i] == LoanCommon.Standard.ERC1155) {
                ERC1155_I(collateral.nftContracts[i]).safeTransferFrom(
                    msg.sender,
                    getLoanStorageTreasuryAddress(),
                    collateral.nftTokenIds[i],
                    collateral.nftTokenAmounts[i],
                    ""
                );
            }
        }

        uint256 nftTokenId = LoanServiceTreasury_I(loanServiceTreasuryAddress).mintCollateralToken(_tokenUri);
        LoanServiceTreasury_I(loanServiceTreasuryAddress).collateralTokenSafeTransferFrom(loan.holder, nftTokenId);
        updateLoanCollateralNftTokenId(_loanId, nftTokenId);

        // Create Loan Activated Event
        emit LoanServiceActivity(_loanId, LoanServiceEvent.ACTIVATED, block.timestamp);
    }

    /// @notice get the calculated activation fee
    /// @param _estimatedValue The estimated value of the nft
    function calculateActivationFee(uint256 _estimatedValue) public view returns (uint256) {
        address loanParametersAddress = LoanOracle_I(getLoanOracleAddress()).getLoanParametersAddress();
        require (loanParametersAddress != address(0x0), "loan parameter address must be set");
        return LoanCommon.calculatePercentage(
            _estimatedValue,
            LoanParameters_I(loanParametersAddress).loanProcessingFeePercent(),
            LoanParameters_I(loanParametersAddress).precision());
    }

    /////////////////////////////////////////
    // reference TFL-0016
    /////////////////////////////////////////
    /// @notice get the create loan fee
    /// @param _estimatedValue The estimated value of the loan
    function calculateCreateLoanFee(uint256 _estimatedValue) public view returns (uint256) {
        address loanParametersAddress = LoanOracle_I(getLoanOracleAddress()).getLoanParametersAddress();
        require (loanParametersAddress != address(0x0), "loan parameter address must be set");
        return LoanCommon.createLoanFee(
            _estimatedValue,
            LoanParameters_I(loanParametersAddress).loanPostingFeePercent(),
            LoanParameters_I(loanParametersAddress).precision()
        );
    }

    /// @notice convenient method to get loan by loan ID
    /// @param _loanId The loan ID
    /// @return The loan object
    function loans(uint256 _loanId) public view returns(LoanCommon.Loan memory) {
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        return LoanStorage_I(storageAddress).loans(_loanId);
    }

    /// @notice convenient method to get a list of loans
    /// @param _start The starting index
    /// @param _end The ending index
    function list(uint256 _start, uint256 _end) public view returns(LoanCommon.Loan[] memory) {
        require(_end >= _start, "The end index must be greater than or equal to the start index");
        LoanCommon.Loan[] memory loanList = new LoanCommon.Loan[](_end.sub(_start).add(1));

        uint256 index = 0;
        for (uint256 i = _start; i <= _end; i++) {
            loanList[index] = loans(i);
            index++;
        }

        return loanList;
    }

    /// @notice update loan status
    /// @param _loanId The Loan Id for which the status is being updated
    /// @param _status The new status
    function updateStatus(uint256 _loanId, LoanCommon.LoanStatus _status) public onlyServiceContractsOrOwner {
        address storageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanStorage_I(storageAddress).updateStatus(_loanId, _status);
    }

    /// @notice cancel a loan that is current in recruiting phase
    /// @param _loanId The loan id
    function cancelLoan(uint256 _loanId) public {
        LoanCommon.Loan memory loan = loans(_loanId);
        require(loan.holder == msg.sender, "NFT Holder must be the one cancelling the loan");
        require(loan.status == LoanCommon.LoanStatus.RECRUITING, "Loan must be in recruiting status");
        address loanStorageAddress = LoanOracle_I(getLoanOracleAddress()).getLoanStorageAddress();
        LoanStorage_I(loanStorageAddress).cancelLoan(_loanId);
    }
}

