/// @title CalculationBase
/// @notice Base calculation contract with nessesary constants and functions
abstract contract CalculationBase is Precisions {

    uint32 public constant CLOSE_WINDOW = 2 days;
    uint32 public constant EXTRA_PREMIUM_PER_DAY_MULTIPLIER = 200; // 200/100 100% higher
    uint32 public constant EXTRA_YIELD_PER_DAY_MULTIPLIER = 50; // 50/100 50%

    IProtocolConfig public immutable config;
    IMarketStates public immutable state;
    IMarket public immutable market;
    IERC20 public immutable token;
    IERC20 public immutable stable;

    constructor(address _config, address _state, address _token) {
        config = IProtocolConfig(_config);
        state = IMarketStates(_state);
        market = IMarket( IProtocolConfig(_config).getMarket(_token) );
        token = IERC20(_token);
        stable = IERC20( IProtocolConfig(_config).getStable() );
    }

    function getState()
        public
        view
        returns (
            int _AP,
            int _AR,
            int _CP,
            int _CR,
            int _B,
            int _L,
            int _D
        )
    {
        (uint AP, uint AR, uint CP, uint CR, uint B, uint L, uint D) = market.getState();
        return (
            DIVIDER * int(AP) / int(DIVIDER_ASSET),
            DIVIDER * int(AR) / int(DIVIDER_ASSET),
            DIVIDER * int(CP) / int(DIVIDER_STABLE),
            DIVIDER * int(CR) / int(DIVIDER_STABLE),
            DIVIDER * int(B) / int(DIVIDER_ASSET),
            DIVIDER * int(L) / int(DIVIDER_ORACLE),
            DIVIDER * int(D) / int(DIVIDER_STABLE)
        );
    }

    function getStateRaw()
        internal
        view
        returns (
            uint _AP,
            uint _AR,
            uint _CP,
            uint _CR,
            uint _B,
            uint _L,
            uint _D
        )
    {
        return market.getState();        
    }

    function abs(int x) public pure returns (int) {
        return x > 0 ? x : -x;
    }

    /// @notice max of two numbers
    function max(int x, int y) public pure returns (int) {
        return x > y ? x : y;
    }

    /// @notice min of two numbers
    function min(int x, int y) public pure returns (int) {
        return x > y ? y : x;
    }
}

