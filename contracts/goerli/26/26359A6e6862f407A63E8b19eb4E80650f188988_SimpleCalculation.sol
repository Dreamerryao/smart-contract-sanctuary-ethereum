/// @title RiskCalculation (ABDK version)
/// @notice Calculate current risk using current prices from price oracles, balances from asset pools and capital pools etc.
/// @notice Provides historical data based on time.
contract SimpleCalculation is
    IRiskCalculation,
    IRebalanceCalculation,
    TakerPositionRate,
    CalculationBase
{
    constructor(
        address _config,
        address _state,
        address _token
    ) CalculationBase(_config, _state, _token) {}

    function rebalanceAmount()
        external
        view
        override
        returns (
            int deltaAP,
            int deltaAR,
            int deltaCP,
            int deltaCR,
            uint sellToken,
            uint sellStable
        )
    {
        (, uint ar, , , , , ) = getStateRaw();
        return calculateRebalanceAmount(0, int(ar), 0, 0, 0, 0, 0, 0, 0, 0);
    }

    function calculateRebalanceAmount(
        int,
        int AR,
        int,
        int,
        int,
        int,
        int,
        int,
        int,
        int
    )
        public
        pure
        override
        returns (
            int,
            int deltaAR,
            int,
            int,
            uint sellToken,
            uint
        )
    {
        deltaAR = -AR;
        sellToken = uint(AR);
    }

    function PAP(uint80 roundId) public view override returns (int _PAP) {
        uint b = market.B();
        return int(b / 2);
    }

    function epsilon(uint ci) external view override returns (int) {}

    function premium(uint id, uint ci) external view override returns (uint) {
        TakerPosition memory pos = market.getTakerPosition(id);
        uint b = market.B();

        if (ci == 0) {
            return
                ((uint(int(getRate(pos.risk, pos.term))) * pos.assetAmount) /
                    10**18) / 2;
        } else {
            return ((uint(int(getRate(pos.risk, pos.term))) *
                ((pos.assetAmount * uint(int(PAP(0)))) / b)) / 10**18);
        }
    }

    function premiumWithPenaulty(uint id, uint ci)
        external
        view
        override
        returns (uint)
    {}

    function yield(uint id, uint ci) external view override returns (int) {
        MakerPosition memory pos = market.getMakerPosition(id);
        uint cp = market.CP();
        uint d = market.D();

        return int((pos.stableAmount * cp) / d) - int(pos.stableAmount);
    }

    function yieldWithPenaulty(uint id, uint ci)
        external
        view
        override
        returns (int)
    {}
}

