pragma solidity ^0.8.0;

import "../interfaces/ITakerPositionRate.sol";

/// @notice NFT token for taker position
abstract contract TakerPositionRate is ITakerPositionRate {
    
    int128 public constant M_30_80 = 0.035718046987284806 * (10**18);
    int128 public constant M_60_80 = 0.07342024995668095 * (10**18);
    int128 public constant M_90_80 = 0.10463304916186243 * (10**18);

    int128 public constant M_30_90 = 0.07351808097304989 * (10**18);
    int128 public constant M_60_90 = 0.12069098118177554 * (10**18);
    int128 public constant M_90_90 = 0.1571040164882781 * (10**18);

    int128 public constant M_30_95 = 0.09875150395875265 * (10**18);
    int128 public constant M_60_95 = 0.1489907700572694 * (10**18);
    int128 public constant M_90_95 = 0.1871733012350463 * (10**18);

    function getRate(uint16 risk, uint16 term) public pure override returns (int128) {
        if (risk == 8000 || risk == 2000) {
            if (term == 30) return M_30_80;
            if (term == 60) return M_60_80;
            if (term == 90) return M_90_80;
        }
        else if (risk == 9000 || risk == 1000)
        {
            if (term == 30) return M_30_90;
            if (term == 60) return M_60_90;
            if (term == 90) return M_90_90;
        }
        else if (risk == 9500 || risk == 500) {
            if (term == 30) return M_30_95;
            if (term == 60) return M_60_95;
            if (term == 90) return M_90_95;
        }
         
        return int128(0);
    }
}

