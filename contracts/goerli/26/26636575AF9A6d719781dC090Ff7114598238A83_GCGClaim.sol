pragma solidity 0.8.15;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "./interface/IGDogs.sol";


contract GCGClaim is Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using Math for uint256;

    IERC20 public immutable gcgToken;
    IERC1155 public immutable catToken;
    IERC1155 public immutable ratToken;
    IGDogs public immutable pigeonToken;
    IGDogs public immutable dogToken;
    IERC721 public immutable cloneToken;

    mapping(TokenType => mapping(uint256 => bool)) private claimedIDs;
    bool public isClaimActive;

    modifier whenClaimActive() {
        require(isClaimActive, "GCGclaim: not allowed to claim");
        _;
    }

    modifier whenClaimInactive() {
        require(!isClaimActive, "GCGclaim: claim period is active");
        _;
    }


    address private signer;

    uint256 public constant GCG_PRECISION = 1e18;
    uint256 public CAT_TOKEN_AMOUNT;
    uint256 public RAT_TOKEN_AMOUNT;
    uint256 public PIGEON_TOKEN_AMOUNT;
    uint256 public DOG_TOKEN_AMOUNT;
    uint256 public D1_TOKEN_AMOUNT;
    uint256 public D2_TOKEN_AMOUNT;
    uint256 public D3_TOKEN_AMOUNT;
    uint256 public FULL_SET_TOKEN_AMOUNT;
    uint256 public KING_PIN_TOKEN_AMOUNT;

    bytes32 private FREECLAIM_TYPEHASH = keccak256(
        "FreeClaim(address claimer,uint256[] d1CloneIDs,uint256[] d2CloneIDs,uint256[] d3CloneIDs)"
    );
    bytes32 private immutable DOMAIN_SEPARATOR;
    bytes32 public EIP712_DOMAIN_TYPE_HASH =
        keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)");

    // FreeClaim
    struct FreeClaim {
        address claimer;
        uint256[] d1CloneIDs;
        uint256[] d2CloneIDs;
        uint256[] d3CloneIDs;
    }

    // ClaimData
    struct ClaimData {
        address claimer;
        uint256[] catIDs;
        uint256[] ratIDs;
        uint256[] pigeonIDs;
        uint256[] dogIDs;
        uint256[] d1CloneIDs;
        uint256[] d2CloneIDs;
        uint256[] d3CloneIDs;
    }

    // EligibleTokenInfo
    struct EligibleTokenInfo {
        uint256 catTokenAmount;
        uint256 ratTokenAmount;
        uint256 pigeonTokenAmount;
        uint256 dogTokenAmount;
        uint256 d1TokenAmount;
        uint256 d2TokenAmount;
        uint256 d3TokenAmount;
        uint256 fullSetTokenAmount;
        uint256 kingPinTokenAmount;
        uint256 totalTokenAmount;
    }

    enum TokenType {
        CAT,
        RAT,
        PIGEON,
        DOG,
        CLONE
    }

    constructor(
        address _gcgAddress,
        address _catAddress,
        address _ratAddress,
        address _pigeonAddress,
        address _dogAddress,
        address _cloneAddress,
        address _signer,
        uint256 chainId_)
    {
        gcgToken = IERC20(_gcgAddress);
        catToken = IERC1155(_catAddress);
        ratToken = IERC1155(_ratAddress);
        pigeonToken = IGDogs(_pigeonAddress);
        dogToken = IGDogs(_dogAddress);
        cloneToken = IERC721(_cloneAddress);
        signer = _signer;

        DOMAIN_SEPARATOR = keccak256(
            abi.encode(
                EIP712_DOMAIN_TYPE_HASH,
                keccak256(bytes("DNA Clone Signature")),
                keccak256(bytes("1")),
                chainId_,
                address(this)
            )
        );
    }

    function setUnitClaimAmount(
        uint256 _catTokenAmount,
        uint256 _ratTokenAmount,
        uint256 _pigeonTokenAmount,
        uint256 _dogTokenAmount,
        uint256 _d1TokenAmount,
        uint256 _d2TokenAmount,
        uint256 _d3TokenAmount,
        uint256 _fullSetTokenAmount,
        uint256 _kingPinTokenAmount
    ) external onlyOwner {
        CAT_TOKEN_AMOUNT = _catTokenAmount;
        RAT_TOKEN_AMOUNT = _ratTokenAmount;
        PIGEON_TOKEN_AMOUNT = _pigeonTokenAmount;
        DOG_TOKEN_AMOUNT = _dogTokenAmount;
        D1_TOKEN_AMOUNT = _d1TokenAmount;
        D2_TOKEN_AMOUNT = _d2TokenAmount;
        D3_TOKEN_AMOUNT = _d3TokenAmount;
        FULL_SET_TOKEN_AMOUNT = _fullSetTokenAmount;
        KING_PIN_TOKEN_AMOUNT = _kingPinTokenAmount;
    }

    function setSigner(address _newSigner) external onlyOwner {
        signer = _newSigner;
    }

    function setClaimActive(bool _isAllowed) public onlyOwner {
        isClaimActive = _isAllowed;
    }

    function claimTokens(
        ClaimData memory claimData,
        bytes memory signature) external nonReentrant whenClaimActive
    {
        require(claimData.claimer == _msgSender(), "Claimer address mismatch");
        FreeClaim memory freeClaim = FreeClaim(
            claimData.claimer,
            claimData.d1CloneIDs,
            claimData.d2CloneIDs,
            claimData.d3CloneIDs
        );
        require(validateClaimerSignature(freeClaim, signature), "Invalid signature");

        EligibleTokenInfo memory eligibleToken = getEligibleTokens(claimData);

        uint256 index;
        for (; index < claimData.catIDs.length; ++index) {
            claimedIDs[TokenType.CAT][claimData.catIDs[index]] = true;
        }
        for (index = 0; index < claimData.ratIDs.length; ++index) {
            claimedIDs[TokenType.RAT][claimData.ratIDs[index]] = true;
        }
        for (index = 0; index < claimData.pigeonIDs.length; ++index) {
            claimedIDs[TokenType.PIGEON][claimData.pigeonIDs[index]] = true;
        }
        for (index = 0; index < claimData.dogIDs.length; ++index) {
            claimedIDs[TokenType.DOG][claimData.dogIDs[index]] = true;
        }
        for (index = 0; index < claimData.d1CloneIDs.length; ++index) {
            claimedIDs[TokenType.CLONE][claimData.d1CloneIDs[index]] = true;
        }
        for (index = 0; index < claimData.d2CloneIDs.length; ++index) {
            claimedIDs[TokenType.CLONE][claimData.d2CloneIDs[index]] = true;
        }
        for (index = 0; index < claimData.d3CloneIDs.length; ++index) {
            claimedIDs[TokenType.CLONE][claimData.d3CloneIDs[index]] = true;
        }

        gcgToken.safeTransfer(claimData.claimer, eligibleToken.totalTokenAmount * GCG_PRECISION);
    }

    function getEligibleTokens(ClaimData memory claimData) public view returns (EligibleTokenInfo memory) 
    {
        uint256 index;
        for (; index < claimData.catIDs.length; ++index) {
            require(catToken.balanceOf(claimData.claimer, claimData.catIDs[index]) == 1, "Invalid Cat owner");
            require(!claimedIDs[TokenType.CAT][claimData.catIDs[index]], "Claimed Cat");
        }
        for (index = 0; index < claimData.ratIDs.length; ++index) {
            require(ratToken.balanceOf(claimData.claimer, claimData.ratIDs[index]) == 1, "Invalid Rat owner");
            require(!claimedIDs[TokenType.RAT][claimData.ratIDs[index]], "Claimed Rat");
        }
        for (index = 0; index < claimData.pigeonIDs.length; ++index) {
            require(pigeonToken.ownerOf(claimData.pigeonIDs[index]) == claimData.claimer, "Invalid Pigeon owner");
            require(!claimedIDs[TokenType.PIGEON][claimData.pigeonIDs[index]], "Claimed Pigeon");
        }
        for (index = 0; index < claimData.dogIDs.length; ++index) {
            require(dogToken.ownerOf(claimData.dogIDs[index]) == claimData.claimer, "Invalid Dog owner");
            require(!claimedIDs[TokenType.DOG][claimData.dogIDs[index]], "Claimed Dog");
        }
        for (index = 0; index < claimData.d1CloneIDs.length; ++index) {
            require(cloneToken.ownerOf(claimData.d1CloneIDs[index]) == claimData.claimer, "Invalid Clone owner");
            require(!claimedIDs[TokenType.CLONE][claimData.d1CloneIDs[index]], "Claimed Clone");
        }
        for (index = 0; index < claimData.d2CloneIDs.length; ++index) {
            require(cloneToken.ownerOf(claimData.d2CloneIDs[index]) == claimData.claimer, "Invalid Clone owner");
            require(!claimedIDs[TokenType.CLONE][claimData.d2CloneIDs[index]], "Claimed Clone");
        }
        for (index = 0; index < claimData.d3CloneIDs.length; ++index) {
            require(cloneToken.ownerOf(claimData.d3CloneIDs[index]) == claimData.claimer, "Invalid Clone owner");
            require(!claimedIDs[TokenType.CLONE][claimData.d3CloneIDs[index]], "Claimed Clone");
        }

        EligibleTokenInfo memory eligibleToken;

        // Per NFT Allowance
        eligibleToken.catTokenAmount = claimData.catIDs.length * CAT_TOKEN_AMOUNT;
        eligibleToken.ratTokenAmount = claimData.ratIDs.length * RAT_TOKEN_AMOUNT;
        eligibleToken.pigeonTokenAmount = claimData.pigeonIDs.length * PIGEON_TOKEN_AMOUNT;
        eligibleToken.dogTokenAmount = claimData.dogIDs.length * DOG_TOKEN_AMOUNT;
        eligibleToken.d1TokenAmount = claimData.d1CloneIDs.length * D1_TOKEN_AMOUNT;
        eligibleToken.d2TokenAmount = claimData.d2CloneIDs.length * D2_TOKEN_AMOUNT;
        eligibleToken.d3TokenAmount = claimData.d3CloneIDs.length * D3_TOKEN_AMOUNT;

        // Full-Set Bonus
        uint fullSetAmt = claimData.catIDs.length
            .min(claimData.ratIDs.length)
            .min(claimData.pigeonIDs.length)
            .min(claimData.dogIDs.length);
        eligibleToken.fullSetTokenAmount = fullSetAmt * FULL_SET_TOKEN_AMOUNT;

        // KingPin Bonus
        eligibleToken.kingPinTokenAmount = claimData.catIDs.length / 5 * KING_PIN_TOKEN_AMOUNT;

        eligibleToken.totalTokenAmount = eligibleToken.catTokenAmount
            + eligibleToken.ratTokenAmount
            + eligibleToken.pigeonTokenAmount
            + eligibleToken.dogTokenAmount
            + eligibleToken.d1TokenAmount
            + eligibleToken.d2TokenAmount
            + eligibleToken.d3TokenAmount
            + eligibleToken.fullSetTokenAmount
            + eligibleToken.kingPinTokenAmount;

        return eligibleToken;
    }

    /*
     * @dev Validate the claim amount against the signature of the meta-transaction.
     */
    function validateClaimerSignature(
        FreeClaim memory freeClaim,
        bytes memory signature
    ) public view returns (bool) {
        bytes32 structHash = keccak256(
            abi.encode(
                FREECLAIM_TYPEHASH,
                freeClaim.claimer,
                keccak256(abi.encodePacked(freeClaim.d1CloneIDs)),
                keccak256(abi.encodePacked(freeClaim.d2CloneIDs)),
                keccak256(abi.encodePacked(freeClaim.d3CloneIDs))
            )
        );
        address recoveredSignerAddress = ECDSA.recover(
            ECDSA.toTypedDataHash(DOMAIN_SEPARATOR, structHash), signature
        );
        return recoveredSignerAddress == signer;
    }

    function withdraw(IERC20 _token, address _recipient) external onlyOwner whenClaimInactive {
        if (_token == IERC20(address(0))) {
            // allow to rescue ether
            payable(_recipient).transfer(address(this).balance);
        } else {
            uint256 withdrawAmount = _token.balanceOf(address(this));
            if (withdrawAmount > 0) {
                _token.safeTransfer(_recipient, withdrawAmount);
            }
        }
    }
}

