pragma solidity ^0.8.17;

import { IRavelMarket } from "./interfaces/IRavelMarket.sol";
import { IRavelVault } from "./interfaces/IRavelVault.sol";

contract RavelMarket is IRavelMarket {
    address public baseToken;
    address public counterToken;
    address public vault;
    address public admin;
    address public exchange;
    bool public deactivated;
    uint256 public orderCounter;

    mapping(uint256 => Order) public orders;

    constructor(
        address _baseToken,
        address _counterToken,
        address _vault,
        address _admin
    ) public {
        baseToken = _baseToken;
        counterToken = _counterToken;
        vault = _vault;
        admin = _admin;
        exchange = address(msg.sender);
    }

    modifier active() {
        require(!deactivated, "This market is deactivated");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin || msg.sender == exchange, "Only admin is allowed to call this function");
        _;
    }

    function deactivate() external override onlyAdmin {
        deactivated = true;
        for (uint orderId = 1; orderId <= orderCounter; orderId++) {
            if (orders[orderId].orderId != 0) {
                _removeOrder(orderId);
            }
        }
    }

    function placeOrder(bytes32 orderData) external override active returns (uint256 orderId) {
        orderCounter++;
        orderId = orderCounter;
        orders[orderId] = Order(orderId, msg.sender, orderData);
        _lockVault(msg.sender);
        emit OrderPlaced(orderId, msg.sender, orderData);
    }

    function cancelOrder(uint256 orderId) external override active returns (bool success) {
        if (orders[orderId].orderId != orderId) {
            success = false;
        } else {
            success = true;
            address user = orders[orderId].user;
            delete orders[orderId];
            _unlockVault(user);
            emit OrderCanceled(orderId);
        }
    }

    function executeTransactions(
        uint256[] memory buyOrderIds,
        uint256[] memory sellOrderIds,
        uint256[] memory quantities,
        uint256[] memory prices,
        uint256[] memory removeIds
    ) external override active onlyAdmin {
        uint256 txCnt = buyOrderIds.length;
        require(sellOrderIds.length == txCnt, "sellOrderIds.length not correct");
        require(buyOrderIds.length == txCnt, "buyOrderIds.length not correct");
        require(quantities.length == txCnt, "quantities.length not correct");
        require(prices.length == txCnt, "prices.length not correct");
        require(sellOrderIds.length == txCnt, "sellOrderIds.length not correct");

        for (uint256 i = 0; i < txCnt; i++) {
            _executeTransaction(buyOrderIds[i], sellOrderIds[i], quantities[i], prices[i]);
        }

        for (uint256 i = 0; i < removeIds.length; i++) {
            _removeOrder(removeIds[i]);
        }
    }

    function _executeTransaction(
        uint256 buyOrderId,
        uint256 sellOrderId,
        uint256 quantity,
        uint256 price
    ) private {
        address buyer = orders[buyOrderId].user;
        address seller = orders[sellOrderId].user;
        require(buyer != address(0), "buyOrderId is incorrect");
        require(seller != address(0), "sellOrderId is incorrect");

        uint256 quantityB = (quantity * price) / 10**18;

        IRavelVault(vault).executeTransaction(buyer, seller, baseToken, counterToken, quantity, quantityB);

        emit TransactionExecuted(buyOrderId, sellOrderId, quantity, price);
    }

    function _removeOrder(uint256 orderId) private {
        address user = orders[orderId].user;
        delete orders[orderId];
        _unlockVault(user);
        emit OrderRemoved(orderId);
    }

    function _lockVault(address user) private {
        IRavelVault(vault).lockUser(user, baseToken, true);
        IRavelVault(vault).lockUser(user, counterToken, true);
    }

    function _unlockVault(address user) private {
        IRavelVault(vault).lockUser(user, baseToken, false);
        IRavelVault(vault).lockUser(user, counterToken, false);
    }
}

