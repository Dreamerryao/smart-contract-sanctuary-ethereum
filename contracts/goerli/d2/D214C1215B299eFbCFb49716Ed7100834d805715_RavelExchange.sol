pragma solidity ^0.8.17;

interface IRavelVault {
    function setRavelExchange(address _ravelExchange) external;

    function whitelistMarket(address market, bool addOrRemove) external;

    function whitelistToken(address token, bool addOrRemove) external;

    function deposit(address token, uint256 amount) external;

    function withdraw(address token, uint256 amount) external;

    function executeTransaction(
        address userA,
        address userB,
        address tokenA,
        address tokenB,
        uint256 quantityA,
        uint256 quantityB
    ) external;

    function lockUser(
        address user,
        address token,
        bool flag
    ) external;

    event Deposited(address token, address user, uint256 amount);
    event Withdrawn(address token, address user, uint256 amount);
    event TokenWhitelisted(address token, bool addOrRemove);
    event VaultTransactionExecuted(
        address userA,
        address userB,
        address tokenA,
        address tokenB,
        uint256 quantityA,
        uint256 quantityB
    );
}

