pragma solidity ^0.8.17;

interface IRavelMarket {
    struct Order {
        uint256 orderId;
        address user;
        bytes32 orderData;
    }

    function deactivate() external;

    function placeOrder(bytes32 orderData) external returns (uint256 orderId);

    function cancelOrder(uint256 orderId) external returns (bool success);

    function executeTransactions(
        uint256[] memory buyOrderIds,
        uint256[] memory sellOrderIds,
        uint256[] memory quantities,
        uint256[] memory prices,
        uint256[] memory removeIds
    ) external;

    event OrderPlaced(uint256 indexed orderId, address from, bytes32 orderData);
    event OrderCanceled(uint256 indexed orderId);
    event TransactionExecuted(uint256 indexed buyOrderId, uint256 indexed sellOrderId, uint256 quantity, uint256 price);
    event OrderRemoved(uint256 indexed orderId);
}

