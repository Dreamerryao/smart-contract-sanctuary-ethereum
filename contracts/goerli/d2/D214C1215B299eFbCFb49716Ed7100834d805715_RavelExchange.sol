pragma solidity ^0.8.17;

import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";

import { IRavelMarket } from "./interfaces/IRavelMarket.sol";
import { IRavelVault } from "./interfaces/IRavelVault.sol";
import { RavelMarket } from "./RavelMarket.sol";

import "hardhat/console.sol";

contract RavelExchange is Ownable {
    address public vault;
    uint256 public marketCounter;
    mapping(uint256 => address) public markets;
    mapping(address => mapping(address => uint256)) public marketIds;

    constructor(address _vault) {
        vault = _vault;
        emit VaultUpdated(vault);
    }

    function updateVault(address _vault) external onlyOwner {
        vault = _vault;
        emit VaultUpdated(vault);
    }

    function whitelistMarket(
        address baseToken,
        address counterToken,
        bool addOrRemove // true means add
    ) external onlyOwner returns (uint256 marketId, address market) {
        require(baseToken != address(0), "baseToken must not be zero");
        require(counterToken != address(0), "counterToken must not be zero");
        require(baseToken != counterToken, "baseToken and counterToken must be different");
        if (addOrRemove) {
            if (marketIds[baseToken][counterToken] != 0) {
                marketId = marketIds[baseToken][counterToken];
                market = markets[marketId];
            } else {
                marketId = ++marketCounter;
                marketIds[baseToken][counterToken] = marketId;
                marketIds[counterToken][baseToken] = marketId;
                RavelMarket _market = new RavelMarket(baseToken, counterToken, vault, msg.sender);
                market = address(_market);
                markets[marketId] = address(market);
                IRavelVault(vault).whitelistMarket(market, true);
            }
        } else {
            if (marketIds[baseToken][counterToken] != 0) {
                marketId = marketIds[baseToken][counterToken];
                delete marketIds[baseToken][counterToken];
            }
            if (marketIds[counterToken][baseToken] != 0) delete marketIds[counterToken][baseToken];
            address _market = markets[marketId];
            market = address(_market);
            IRavelVault(vault).whitelistMarket(market, false);
            _deactivateMarket(_market);
            delete markets[marketId];
        }
        emit MarketWhitelisted(market, baseToken, counterToken, addOrRemove, marketId);
    }

    function getMarket(address baseToken, address counterToken) public view returns (address) {
        uint256 marketId = marketIds[baseToken][counterToken];
        return marketId == 0 ? address(0) : markets[marketId];
    }

    function _deactivateMarket(address market) private {
        IRavelMarket(market).deactivate();
    }

    event VaultUpdated(address indexed vaultAddress);

    event MarketWhitelisted(
        address indexed marketAddress,
        address baseToken,
        address counterToken,
        bool addOrRemove,
        uint256 marketId
    );
}

