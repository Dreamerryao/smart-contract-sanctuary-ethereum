pragma solidity 0.8.17;

uint64 constant UINT64_MAX = 2**64 - 1;
uint8 constant MAX_TIERS = 10;

enum TokenMintType {
    /**
     * @notice requires Token contract to implement 
     * ERC721 with mint function signature
     * "function mint(address to) public / external"
     *                  (OR)
     * ERC721Enumerable with mint function signature
     * "function mint(address to, uint256 tokenId) public / external"
     *                  (OR) 
     * ERC721Consecutive with mint function signature
     * "function mint(address to, uint96 amount) public / external"
     */
    ERC721_PUBLIC_MINTABLE,
    /**
     * @notice requires Token contract to implement 
     * ERC721 with mint function signature
     * "function mint(address to) public / external onlyRole(MINTER_ROLE)"
     *                  (OR)
     * ERC721Enumerable with mint function signature
     * "function mint(address to, uint256 tokenId) public / external onlyRole(MINTER_ROLE)"
     *                  (OR) 
     * ERC721Consecutive with mint function signature
     * "function mint(address to, uint96 amount) public / external onlyRole(MINTER_ROLE)"
     */
    ERC721_ROLE_BASED_MINTABLE,
    /**
     * @notice requires mintMaxSupply to be availabe for transfer from publisher / specified wallet
     */
    ERC721_TRANSFERABLE,
    /**
     * @notice requires the below function to be present in the token contract
     * "function mint(address to, uint256 tokenId, uint256 amount) public / external"
     */
    ERC1155_PUBLIC_MINTABLE,
    /**
     * @notice requires the below function to be present in the token contract with MINTER_ROLE granted to address(this)
     * "funtion mint(address to, uint256 tokenId, uint256 amount) public / external onlyRole(MINTER_ROLE)"
     */
    ERC1155_ROLE_BASED_MINTABLE,
    /**
     * @notice requires mintMaxSupply to be availabe for transfer from publisher / specified wallet
     */
    ERC1155_TRANSFERABLE
}

struct ConfigData {
    TokenMintType tokenMintType; // token type and minting type
    address token; // address of token contract
    uint256 mintMaxSupply; // max supply of token to be minted
    uint256 mintedSupply; // current supply of token minted
    uint256 mintPricePerToken; // price of token in currency
    uint256 mintLimitPerWallet; // max number of tokens that can be minted per wallet
    uint256 mintStartTime; // start time of minting
    uint256 mintEndTime; // end time of minting
    bytes32 whitelistMerkleRoot; // merkle root of whitelisted addresses
    bytes params; // additional params eg. transfer from address, MINTER_ROLE id etc.
}

struct TierData {
    uint8 tier; // tier number
    uint256 minTierCap; // min capital investment in currency required in this tier
    uint256 maxTierCap; // max capital investment in currency allowed in this tier
}

