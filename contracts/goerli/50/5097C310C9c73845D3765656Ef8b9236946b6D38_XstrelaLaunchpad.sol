pragma solidity ^0.8.17;

// OpenZeppelin dependencies
import { IAccessControl } from "@openzeppelin/contracts/access/IAccessControl.sol";
import { ERC165Checker } from "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import { IERC721 } from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import { IERC1155 } from "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

// Xstrela dependencies
import { TokenMintType, ConfigData } from "./LaunchpadStructs.sol";

library TypeChecker {
  // ERC721 interfaceID
  bytes4 private constant _INTERFACE_ID_ERC721 = 0x80ac58cd;
  // ERC1155 interfaceID
  bytes4 private constant _INTERFACE_ID_ERC1155 = 0xd9b67a26;

  function verifyType(
    TokenMintType tokenMintType,
    address token,
    bytes memory params,
    uint256 mintMaxSupply
  ) internal view returns (bool) {
    if (tokenMintType == TokenMintType.ERC721_ROLE_BASED_MINTABLE)
      return verifyERC721Minter(token, params);
    else if (tokenMintType == TokenMintType.ERC721_TRANSFERABLE)
      return verifyERC721Approval(token, params, mintMaxSupply);
    else if (tokenMintType == TokenMintType.ERC1155_ROLE_BASED_MINTABLE)
      return verifyERC1155Minter(token, params);
    else if (tokenMintType == TokenMintType.ERC1155_TRANSFERABLE)
      return verifyERC1155Approval(token, params, mintMaxSupply);
    else if (tokenMintType == TokenMintType.ERC721_PUBLIC_MINTABLE)
      return verifyIsERC721(token);
    else if (tokenMintType == TokenMintType.ERC1155_PUBLIC_MINTABLE)
      return verifyIsERC1155(token);
    else return false;
  }

  function verifyERC721Minter(
    address token,
    bytes memory params
  ) internal view returns (bool) {
    bool isERC721 = verifyIsERC721(token);
    if (isERC721) {
      bytes32 minterRole = abi.decode(params, (bytes32));
      try IAccessControl(token).hasRole(minterRole, address(this)) returns (
        bool hasRole
      ) {
        if (hasRole) return true;
        return false;
      } catch {
        return false;
      }
    }
    return false;
  }

  function verifyERC721Approval(
    address token,
    bytes memory params,
    uint256 mintMaxSupply
  ) internal view returns (bool) {
    bool isERC721 = verifyIsERC721(token);
    if (isERC721) {
      address tokenOwner = abi.decode(params, (address));
      uint256 availabeSupply = IERC721(token).balanceOf(tokenOwner);
      bool isApproved = IERC721(token).isApprovedForAll(
        tokenOwner,
        address(this)
      );
      if (isApproved && availabeSupply >= mintMaxSupply) return true;
    }
    return false;
  }

  function verifyERC1155Minter(
    address token,
    bytes memory params
  ) internal view returns (bool) {
    bool isERC1155 = verifyIsERC1155(token);
    if (isERC1155) {
      bytes32 minterRole = abi.decode(params, (bytes32));
      try IAccessControl(token).hasRole(minterRole, address(this)) returns (
        bool hasRole
      ) {
        if (hasRole) return true;
        return false;
      } catch {
        return false;
      }
    }
    return false;
  }

  function verifyERC1155Approval(
    address token,
    bytes memory params,
    uint256
  ) internal view returns (bool) {
    bool isERC1155 = verifyIsERC1155(token);
    if (isERC1155) {
      address tokenOwner = abi.decode(params, (address));
      bool isApproved = IERC1155(token).isApprovedForAll(
        tokenOwner,
        address(this)
      );
      if (isApproved) return true;
    }
    return false;
  }

  function verifyIsERC721(address token) internal view returns (bool) {
    if (ERC165Checker.supportsERC165(token)) {
      return ERC165Checker.supportsInterface(token, _INTERFACE_ID_ERC721);
    }
    return false;
  }

  function verifyIsERC1155(address token) internal view returns (bool) {
    if (ERC165Checker.supportsERC165(token)) {
      return ERC165Checker.supportsInterface(token, _INTERFACE_ID_ERC1155);
    }
    return false;
  }
}

