pragma solidity 0.8.17;

// OpenZeppelin dependencies
import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";

// Interfaces
import { IGameLaunchpad } from "./interfaces/IGameLaunchpad.sol";

// Libraries
import { ConfigData, TokenMintType, TierData } from "./libraries/LaunchpadStructs.sol";
import "./libraries/LaunchpadErrors.sol";

// Contracts
import { GameLaunchpad } from "./GameLaunchpad.sol";
import { XGameERC721Consecutive } from "./tokens/XGameERC721Consecutive.sol";

contract XstrelaLaunchpad is Ownable, ReentrancyGuard {
  // solhint-disable-next-line var-name-mixedcase
  address public immutable WETH;
  uint256 public launchpadFee;

  // publisher => gameLaunchpad contract address
  mapping(address => address) public publisherLaunchpads;
  mapping(address => address) public tokenLaunchpads;

  constructor(
    address _WETH, // solhint-disable-line
    uint256 _launchpadFee
  ) {
    if (_WETH == address(0)) revert LaunchpadError(ZERO_ADDRESS);
    if (_launchpadFee == 0) revert LaunchpadError(INVALID_LAUNCHPAD_FEE);
    WETH = _WETH;
    launchpadFee = _launchpadFee;
  }

  function createGameLaunchpad(
    address publisher,
    address currency,
    uint256 publisherLaunchpadFee 
  ) external nonReentrant returns (address) {
    if (publisher == address(0)) revert LaunchpadError(ZERO_ADDRESS);
    if (currency == address(0)) revert LaunchpadError(ZERO_ADDRESS);
    if (currency != WETH) revert LaunchpadError(INVALID_CURRENCY);
    if (publisherLaunchpads[publisher] != address(0))
      revert LaunchpadError(LAUNCHPAD_ALREADY_EXISTS);
    uint256 fee = publisherLaunchpadFee > 0
      ? publisherLaunchpadFee
      : launchpadFee;
    return _createGameLaunchpad(publisher, currency, fee);
  }

  function createGameToken(
    address publisher,
    string memory tokenName,
    string memory tokenSymbol,
    TokenMintType
  ) external nonReentrant returns (address) {
    if (publisher == address(0)) revert LaunchpadError(ZERO_ADDRESS);
    if (publisherLaunchpads[publisher] == address(0))
      revert LaunchpadError(LAUNCHPAD_DOES_NOT_EXIST);
    address gameLaunchpad = publisherLaunchpads[publisher];
    address token = _createXGameERC721(tokenName, tokenSymbol);
    tokenLaunchpads[token] = gameLaunchpad;
    return token;
  }

  function addTokenConfig(
    address publisher,
    address token,
    TokenMintType tokenMintType,
    uint256 mintMaxSupply,
    uint256 mintPricePerToken,
    uint256 mintLimitPerWallet,
    uint256 mintStartTime,
    uint256 mintEndTime,
    bytes32 whitelistMerkleRoot,
    bytes memory params
  ) external nonReentrant {
    if (publisher == address(0)) revert LaunchpadError(ZERO_ADDRESS);
    if (publisherLaunchpads[publisher] == address(0))
      revert LaunchpadError(LAUNCHPAD_DOES_NOT_EXIST);

    address gameLaunchpad = publisherLaunchpads[publisher];
    address tokenLaunchpad = tokenLaunchpads[token];

    if (tokenLaunchpad != address(0) && tokenLaunchpad != gameLaunchpad) {
      revert LaunchpadError(INVALID_PUBLISHER_OR_TOKEN_LAUNCHPAD);
    }
    if (tokenLaunchpad == address(0)) {
      tokenLaunchpads[token] = gameLaunchpad;
    }

    ConfigData memory tokenConfig;
    tokenConfig.tokenMintType = tokenMintType;
    tokenConfig.token = token;
    tokenConfig.mintMaxSupply = mintMaxSupply;
    tokenConfig.mintedSupply = 0;
    tokenConfig.mintPricePerToken = mintPricePerToken;
    tokenConfig.mintLimitPerWallet = mintLimitPerWallet;
    tokenConfig.mintStartTime = mintStartTime;
    tokenConfig.mintEndTime = mintEndTime;
    tokenConfig.whitelistMerkleRoot = whitelistMerkleRoot;
    tokenConfig.params = params;

    // configure token config data in game launchpad
    IGameLaunchpad(gameLaunchpad).configureTokenLaunch(tokenConfig);
  }

  function setLaunchpadFee(uint256 _launchpadFee) external onlyOwner {
    launchpadFee = _launchpadFee;
  }

  function _createGameLaunchpad(
    address publisher,
    address currency,
    uint256 launchpadFeeForPublisher
  ) private returns (address) {
    GameLaunchpad gameLaunchpad = new GameLaunchpad(
      publisher,
      currency,
      owner(),
      launchpadFeeForPublisher
    );
    publisherLaunchpads[publisher] = address(gameLaunchpad);
    return address(gameLaunchpad);
  }

  function _createXGameERC721(
    string memory name,
    string memory symbol
  ) private returns (address) {
    XGameERC721Consecutive xGameERC721 = new XGameERC721Consecutive(
      name,
      symbol
    );
    return address(xGameERC721);
  }
}

