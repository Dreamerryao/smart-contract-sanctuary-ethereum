pragma solidity ^0.8.13;

import {EnumerableMap} from "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {Pausable} from "@openzeppelin/contracts/security/Pausable.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {NarwhalPool} from "./NarwhalPool.sol";
import {NarwhalPoolFactory} from "./NarwhalPoolFactory.sol";

contract Narwhal is Ownable, Pausable {
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using SafeERC20 for IERC20;

    event TokenVaultSet(address token, address vault);
    event AdminSettled(bool poolSettle);
    event DepositOpened(bool poolOpen);

    bool public poolOpen = false;
    bool public poolSettle = false;
    mapping(address => address) public getTokenVault;
    address[] internal supportedTokens;
    address[] internal pools;
    mapping(address => mapping(address => uint256)) public poolTokenAum;

    uint256 internal immutable treasuryFee;

    NarwhalPoolFactory internal immutable factory;
    address internal immutable treasury;

    constructor(address treasuryAddr, uint256 fee) {
        factory = new NarwhalPoolFactory();
        treasury = treasuryAddr;
        treasuryFee = fee;
    }

    function getPool(uint256 poolId) public view returns (address) {
        return factory.getPool(poolId);
    }

    function getPoolTokenAum(address pool, address token)
        external
        view
        returns (uint256)
    {
        // TODO: check if pool exists
        require(
            getTokenVault[token] != address(0),
            "getPoolTokenAum: invalid token"
        );
        return poolTokenAum[pool][token];
    }

    function getSupportedTokens() external view returns (address[] memory) {
        return supportedTokens;
    }

    function adminSettle(bool settle) external whenNotPaused onlyOwner {
        poolSettle = settle;
        emit AdminSettled(settle);
    }

    function adminSetTokenVault(address token, address vault)
        external
        whenNotPaused
        onlyOwner
    {
        require(token != address(0), "adminSetTokenVault: token is null");
        require(vault != address(0), "adminSetTokenVault: vault is null");
        // TODO: handle Dopex contract address upgrade
        // require(
        //     getTokenVault[token] == address(0),
        //     "adminSetTokenVault: token whitelisted"
        // );
        if (getTokenVault[token] == address(0)) {
            getTokenVault[token] = vault;
            supportedTokens.push(token);
            emit TokenVaultSet(token, vault);
        }
    }

    function adminOpenDeposit(bool open) external whenNotPaused onlyOwner {
        poolOpen = open;

        if (open) {
            poolSettle = false;
        }
        // take AUM snapshot at closing
        else {
            for (uint256 i = 0; i < supportedTokens.length; ++i) {
                address token = supportedTokens[i];
                for (uint256 j = 0; j < pools.length; ++j) {
                    address pool = pools[j];
                    poolTokenAum[pool][token] = IERC20(token).balanceOf(pool);
                }
            }
        }

        emit DepositOpened(open);
    }

    function adminFactoryCreatePool(address mgr) external returns (address) {
        (, address pool) = factory.createPool(
            address(this),
            mgr,
            treasury,
            treasuryFee
        );
        pools.push(pool);
        return pool;
    }
}

