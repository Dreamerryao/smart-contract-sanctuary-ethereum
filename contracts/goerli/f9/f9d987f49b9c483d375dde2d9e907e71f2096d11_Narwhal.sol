    event MgrPoolFeeSet(uint256 mgrFee);
    event MgrPoolTokenCapSet(address token, uint256 capacity);
    event MgrSsovDeposited(
        address token,
        uint256 strikeIndex,
        uint256 amount,
        uint256 tokenId
    );
    event MgrCallPurchased(
        address token,
        uint256 strikeIndex,
        uint256 amount,
        uint256 premium,
        uint256 totalFee
    );
    event CallSettled(
        address vault,
        uint256 epoch,
        uint256 balanceBefore,
        uint256 balanceAfter
    );
    event SsovSettled(address vault, uint256 epoch, uint256 withdrawSum);
    event MgrSettled(address token);
    event CarryDistributed(address token, uint256 usersPnl, uint256 carry);
    event RewardsDistributed(address token);
    event MiscRewardsDistributed(address token);

    /**
     * @notice Deposits `amount` token into this Pool.
     * @dev Assumes the user has given infinite spend approval
     * to the strategy. Assumes token is supported.
     * @param token Address of token to deposit
     * @param amount Amount of token to deposit
     */
    function deposit(address token, uint256 amount) external returns (uint256);

    /**
     * @notice Withdraws `amount` token from the Pool.
     * @dev This withdrawal is optimistic, returned amount might be less than
     * the amount specified.
     * @param token Address of token to withdraw
     * @param amount Amount of token to withdraw
     */
    function withdraw(address token, uint256 amount) external returns (bool);

    function claimRewards() external returns (bool);

    function mgrSetPoolFee(uint256 mgrFee) external returns (bool);

    function mgrSetTokenCap(address token, uint256 capacity)
        external
        returns (bool);

    function mgrDepositSsov(
        address token,
        uint256 strikeIndex,
        uint256 amount
    ) external returns (bool);

    function mgrPurchaseCall(
        address token,
        uint256 strikeIndex,
        uint256 amount
    ) external returns (uint256, uint256);

    function mgrSettle(address token) external returns (bool);
}

