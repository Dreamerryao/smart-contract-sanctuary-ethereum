pragma solidity ^0.8.13;

import {ABDKMath64x64} from "abdk-libraries-solidity/ABDKMath64x64.sol";
import {AccessControl} from "@openzeppelin/contracts/access/AccessControl.sol";
import {Context} from "@openzeppelin/contracts/utils/Context.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {Pausable} from "@openzeppelin/contracts/security/Pausable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {ERC721Holder} from "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import {EnumerableMap} from "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import {console} from "forge-std/console.sol";

import {Narwhal} from "./Narwhal.sol";
import {INarwhalPool} from "./interfaces/INarwhalPool.sol";
import {ISsov} from "./interfaces/ISsov.sol";

contract NarwhalPool is
    INarwhalPool,
    Context,
    Ownable,
    AccessControl,
    ReentrancyGuard,
    Pausable,
    ERC721Holder
{
    using ABDKMath64x64 for int128;
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using SafeERC20 for IERC20;

    bytes32 MANAGER = keccak256("MANAGER");

    address internal immutable mgr;
    address internal immutable narwhal;
    address internal immutable treasury;
    uint256 internal immutable treasuryFeeCut;
    uint256 public mgrCarry = 0;

    EnumerableMap.AddressToUintMap tokenCapacity;
    mapping(address => uint256) public getSsovAmount;
    mapping(address => EnumerableMap.AddressToUintMap)
        internal tokenUserBalance;
    mapping(address => EnumerableMap.AddressToUintMap)
        internal userRewardBalance;
    mapping(address => uint256[]) getNftIds;

    modifier onlyManager() {
        require(hasRole(MANAGER, _msgSender()), "onlyManager: call is not mgr");
        _;
    }

    modifier onlySupportedToken(address token) {
        require(
            Narwhal(narwhal).getTokenVault(token) != address(0),
            "onlySupportedToken: token is not supported"
        );
        _;
    }

    modifier onlyPoolOpen() {
        require(Narwhal(narwhal).poolOpen(), "onlyPoolOpen: pool is close");
        _;
    }

    modifier onlyPoolClose() {
        require(
            !Narwhal(narwhal).poolOpen(),
            "onlyPoolOpen: pool is still open"
        );
        _;
    }

    modifier onlyPoolSettled() {
        require(
            Narwhal(narwhal).poolSettle(),
            "onlyPoolSettled: pool has not settled"
        );
        _;
    }

    constructor(
        address admin,
        address narwhalAddr,
        address treasuryAddr,
        uint256 treasuryFee,
        address mgrAddr,
        uint256 poolId
    ) {
        require(treasuryFee < 100, "ctor: treasury fee cut must be < 100%");

        transferOwnership(admin);
        narwhal = narwhalAddr;

        treasury = treasuryAddr;
        treasuryFeeCut = treasuryFee;

        mgr = mgrAddr;
        _setupRole(MANAGER, mgr);

        emit PoolCreated(mgr, poolId);
    }

    function poolTokenVault(address token) public returns (address) {
        require(token != address(0), "poolTokenVault: null token");
        return Narwhal(narwhal).getTokenVault(token);
    }

    function getPoolOpen() public view returns (bool) {
        return Narwhal(narwhal).poolOpen();
    }

    function getTokenCapacity(address token) public view returns (uint256) {
        (bool exists, uint256 amount) = tokenCapacity.tryGet(token);
        return exists ? amount : 0;
    }

    function getTokenAum(address token) public view returns (uint256) {
        return Narwhal(narwhal).getPoolTokenAum(address(this), token);
    }

    function getTokenUserBalance(address token, address user)
        public
        view
        onlySupportedToken(token)
        returns (uint256)
    {
        (bool exists, uint256 balance) = tokenUserBalance[token].tryGet(user);
        return exists ? balance : 0;
    }

    function getUserRewardBalance(address user, address token)
        public
        view
        returns (uint256)
    {
        (bool exists, uint256 balance) = userRewardBalance[user].tryGet(token);
        return exists ? balance : 0;
    }

    function adminEmergencyPause() external onlyOwner {
        _pause();
    }

    function approveToken(address token, uint256 amount)
        external
        whenNotPaused
        onlySupportedToken(token)
        returns (bool)
    {
        IERC20(token).safeIncreaseAllowance(address(this), amount);
        return true;
    }

    function mgrApproveTokenForSsov(address token, uint256 amount)
        external
        whenNotPaused
        onlyManager
        onlySupportedToken(token)
        returns (bool)
    {
        IERC20(token).safeIncreaseAllowance(poolTokenVault(token), amount);
        return true;
    }

    function deposit(address token, uint256 amount)
        external
        nonReentrant
        whenNotPaused
        onlyPoolOpen
        onlySupportedToken(token)
        returns (uint256)
    {
        address user = _msgSender();
        require(
            IERC20(token).balanceOf(user) >= amount,
            "deposit: insufficient token"
        );
        (bool exists, uint256 cap) = tokenCapacity.tryGet(token);
        cap = exists ? cap : 0;
        require(
            amount + IERC20(token).balanceOf(address(this)) <= cap,
            "deposit: vault hits capacity"
        );

        uint256 treasuryFee = ABDKMath64x64.mulu(
            ABDKMath64x64.divu(treasuryFeeCut, 100),
            amount
        );

        IERC20(token).safeTransferFrom(user, treasury, treasuryFee);

        amount -= treasuryFee;

        (bool userExists, uint256 prev) = tokenUserBalance[token].tryGet(user);
        if (userExists) {
            tokenUserBalance[token].set(user, prev + amount);
        } else {
            tokenUserBalance[token].set(user, amount);
        }

        IERC20(token).safeTransferFrom(user, address(this), amount);

        emit UserDeposited(token, amount);
        return amount;
    }

    function withdraw(address token, uint256 amount)
        external
        nonReentrant
        whenNotPaused
        onlySupportedToken(token)
        onlyPoolSettled
        returns (bool)
    {
        address user = _msgSender();

        // TODO: test if user does not exist
        (, uint256 balance) = tokenUserBalance[token].tryGet(user);
        require(balance >= amount, "withdraw: insufficient token");

        uint256 left = balance - amount;

        tokenUserBalance[token].set(user, left);

        IERC20(token).safeTransfer(user, amount);

        emit UserWithdrawn(token, amount);
        return true;
    }

    function claimRewards() external whenNotPaused nonReentrant returns (bool) {
        address user = _msgSender();

        EnumerableMap.AddressToUintMap
            storage rewardBalance = userRewardBalance[user];

        require(
            rewardBalance.length() > 0,
            "claimRewards: user has no rewards to claim"
        );

        for (uint256 i = 0; i < rewardBalance.length(); ++i) {
            (address reward, uint256 amount) = rewardBalance.at(i);
            rewardBalance.set(reward, 0);
            IERC20(reward).safeTransfer(user, amount);
        }

        delete userRewardBalance[user];

        emit UserRewardsClaimed(user);
        return true;
    }

    // TODO: effects of changing this during and after an epoch?
    function mgrSetPoolFee(uint256 mgrFee)
        external
        onlyPoolOpen
        onlyManager
        whenNotPaused
        returns (bool)
    {
        require(mgrFee < 100, "mgrSetPoolFee: mgrFee cut must be < 100%");
        mgrCarry = mgrFee;
        emit MgrPoolFeeSet(mgrFee);
        return true;
    }

    // TODO: effects of changing this during and after an epoch?
    function mgrSetTokenCap(address token, uint256 capacity)
        external
        onlySupportedToken(token)
        onlyPoolOpen
        onlyManager
        whenNotPaused
        returns (bool)
    {
        tokenCapacity.set(token, capacity);
        emit MgrPoolTokenCapSet(token, capacity);
        return true;
    }

    function mgrDepositSsov(
        address token,
        uint256 strikeIndex,
        uint256 amount
    )
        external
        onlySupportedToken(token)
        onlyPoolClose
        onlyManager
        nonReentrant
        whenNotPaused
        returns (bool)
    {
        require(
            IERC20(token).balanceOf(address(this)) >= amount,
            "mgrDepositSsov: insufficient balance"
        );
        address vault = poolTokenVault(token);
        uint256 tokenId = ISsov(vault).deposit(
            strikeIndex,
            amount,
            address(this)
        );
        getNftIds[token].push(tokenId);
        getSsovAmount[token] += amount;
        emit MgrSsovDeposited(token, strikeIndex, amount, tokenId);
        return true;
    }

    function mgrSettleSsov(
        address token,
        address vault,
        uint256 epoch
    )
        internal
        returns (
            uint256,
            address[] memory,
            uint256[] memory
        )
    {
        uint256[] memory tokenIdsFromSsov = getNftIds[token];
        delete getNftIds[token];

        uint256 withdrawAmount = 0;
        uint256 withdrawSum = 0;

        address[] memory rewardTokensAddrs = ISsov(vault)
            .epochData(epoch)
            .rewardTokensToDistribute;

        uint256 numRewardTokens = rewardTokensAddrs.length;
        uint256[] memory rewardTokensSum = new uint256[](numRewardTokens);
        uint256[] memory rewardTokensAmount;

        for (uint256 i = 0; i < tokenIdsFromSsov.length; ++i) {
            (withdrawAmount, rewardTokensAmount) = ISsov(vault).withdraw(
                tokenIdsFromSsov[i],
                address(this)
            );

            withdrawSum += withdrawAmount;

            for (uint256 j = 0; j < numRewardTokens; ++j) {
                rewardTokensSum[j] += rewardTokensAmount[j];
            }
        }
        emit SsovSettled(vault, epoch, withdrawSum);
        return (withdrawSum, rewardTokensAddrs, rewardTokensSum);
    }

    function mgrPurchaseCall(
        address token,
        uint256 strikeIndex,
        uint256 amount
    )
        external
        onlySupportedToken(token)
        onlyPoolClose
        onlyManager
        nonReentrant
        whenNotPaused
        returns (uint256, uint256)
    {
        address vault = poolTokenVault(token);
        (uint256 premium, uint256 totalFee) = ISsov(vault).purchase(
            strikeIndex,
            amount,
            address(this)
        );
        emit MgrCallPurchased(token, strikeIndex, amount, premium, totalFee);
        return (premium, totalFee);
    }

    function mgrSettleCall(
        address token,
        address vault,
        uint256 epoch
    ) internal returns (uint256, uint256) {
        uint256[] memory strikes = ISsov(vault).getEpochStrikes(epoch);

        uint256 balanceBefore = IERC20(token).balanceOf(address(this));

        // `pnl` returned from Dopex contract is more than the tokens received
        // because the contract takes fee. `token.transfer(pnl - fees)` is called
        // but `settle()` returns `pnl`
        for (uint256 i = 0; i < strikes.length; ++i) {
            uint256 strike = strikes[i];
            address strikeTokenAddr = ISsov(vault)
                .getEpochStrikeData(epoch, strike)
                .strikeToken;
            uint256 strikeAmount = IERC20(strikeTokenAddr).balanceOf(
                address(this)
            );
            if (strikeAmount != 0) {
                ISsov(vault).settle(i, strikeAmount, epoch);
            }
        }

        uint256 balanceAfter = IERC20(token).balanceOf(address(this));

        emit CallSettled(vault, epoch, balanceBefore, balanceAfter);

        return (balanceBefore, balanceAfter);
    }

    function mgrSettle(address token)
        external
        onlyManager
        onlySupportedToken(token)
        nonReentrant
        whenNotPaused
        returns (bool)
    {
        address vault = poolTokenVault(token);
        uint256 epoch = ISsov(vault).currentEpoch();

        (uint256 callBefore, uint256 callAfter) = mgrSettleCall(
            token,
            vault,
            epoch
        );

        uint256 ssovBefore = getSsovAmount[token];
        (
            uint256 ssovAfter,
            address[] memory rewardTokensAddrs,
            uint256[] memory rewardTokensSum
        ) = mgrSettleSsov(token, vault, epoch);

        uint256 usersProfit = distributeCarry(
            token,
            callBefore,
            callAfter,
            ssovBefore,
            ssovAfter
        );

        require(
            distributeMiscRewards(token, rewardTokensAddrs, rewardTokensSum),
            "mgrSettle: fail to distribute misc rewards"
        );

        require(
            distributeRewards(
                token,
                usersProfit,
                usersProfit == 0 ? ssovBefore - ssovAfter : 0
            ),
            "mgrSettle: fail to distribute rewards"
        );

        getSsovAmount[token] = 0;

        emit MgrSettled(token);

        return true;
    }

    function distributeCarry(
        address token,
        uint256 callBefore,
        uint256 callAfter,
        uint256 ssovBefore,
        uint256 ssovAfter
    ) internal returns (uint256) {
        uint256 callPnl = 0;
        if (callAfter > callBefore) {
            callPnl = callAfter - callBefore;
        }

        uint256 ssovPnl = 0;
        if (ssovAfter > ssovBefore) {
            ssovPnl = ssovAfter - ssovBefore;
        }

        uint256 poolPnl = ssovPnl + callPnl;

        // if mgr profits, takes fee cut
        uint256 carry = 0;
        if (poolPnl != 0) {
            int128 carryPct = ABDKMath64x64.divu(mgrCarry, 100);
            carry = ABDKMath64x64.mulu(carryPct, poolPnl);
            IERC20(token).safeTransfer(mgr, carry);
        }

        uint256 usersPnl = poolPnl - carry;

        emit CarryDistributed(token, usersPnl, carry);

        return usersPnl;
    }

    function distributeMiscRewards(
        address token,
        address[] memory rewardTokensAddrs,
        uint256[] memory rewardTokensSum
    ) internal returns (bool) {
        EnumerableMap.AddressToUintMap storage userBalance = tokenUserBalance[
            token
        ];

        uint256 aum = getTokenAum(token);

        for (uint256 i = 0; i < userBalance.length(); ++i) {
            (address user, uint256 invested) = userBalance.at(i);
            int128 share = ABDKMath64x64.divu(invested, aum);

            bool miscRewardDist = handleMiscRewards(
                user,
                share,
                rewardTokensAddrs,
                rewardTokensSum
            );

            require(
                miscRewardDist,
                "distributeRewards: fail to distribute misc rewards to user"
            );
        }

        emit MiscRewardsDistributed(token);

        return true;
    }

    function distributeRewards(
        address token,
        uint256 usersProfit,
        uint256 usersLoss
    ) internal returns (bool) {
        uint256 usersPnl = usersProfit != 0 ? usersProfit : usersLoss;

        EnumerableMap.AddressToUintMap storage userBalance = tokenUserBalance[
            token
        ];

        uint256 aum = getTokenAum(token);

        for (uint256 i = 0; i < userBalance.length(); ++i) {
            (address user, uint256 invested) = userBalance.at(i);

            int128 share = ABDKMath64x64.divu(invested, aum);
            uint256 pnl = ABDKMath64x64.mulu(share, usersPnl);

            if (usersProfit != 0) {
                userBalance.set(user, pnl + invested);
            } else {
                userBalance.set(user, invested - pnl);
            }
        }

        emit RewardsDistributed(token);

        return true;
    }

    function handleMiscRewards(
        address user,
        int128 share,
        address[] memory rewardTokensAddrs,
        uint256[] memory rewardTokensSum
    ) internal returns (bool) {
        for (uint256 j = 0; j < rewardTokensAddrs.length; ++j) {
            address rewardToken = rewardTokensAddrs[j];
            uint256 reward = ABDKMath64x64.mulu(share, rewardTokensSum[j]);

            (bool exists, uint256 prev) = userRewardBalance[user].tryGet(
                rewardToken
            );
            if (exists) {
                userRewardBalance[user].set(rewardToken, prev + reward);
            } else {
                userRewardBalance[user].set(rewardToken, reward);
            }
        }
        return true;
    }
}

