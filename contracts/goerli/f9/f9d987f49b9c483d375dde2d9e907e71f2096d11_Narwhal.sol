pragma solidity ^0.8.13;

import {Context} from "@openzeppelin/contracts/utils/Context.sol";
import {Counters} from "@openzeppelin/contracts/utils/Counters.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {NarwhalPool} from "./NarwhalPool.sol";
import {INarwhalPoolFactory} from "./interfaces/INarwhalPoolFactory.sol";

contract NarwhalPoolFactory is
    Context,
    INarwhalPoolFactory,
    Ownable,
    ReentrancyGuard
{
    using Counters for Counters.Counter;

    Counters.Counter private poolCounter;
    mapping(uint256 => address) internal pools;

    constructor() {
        transferOwnership(msg.sender);
    }

    function createPool(
        address narwhal,
        address mgr,
        address treasury,
        uint256 treasuryFee
    ) external onlyOwner nonReentrant returns (uint256, address) {
        poolCounter.increment();
        uint256 poolId = poolCounter.current();

        NarwhalPool pool = new NarwhalPool(
            _msgSender(),
            narwhal,
            treasury,
            treasuryFee,
            mgr,
            poolId
        );
        pools[poolId] = address(pool);

        emit PoolCreated(mgr, poolId);

        return (poolId, address(pool));
    }

    function getPool(uint256 poolId) external view returns (address) {
        return pools[poolId];
    }
}

