    function revertIfParentOwned(uint64 _id) private view {
        uint64 parentId = _id;
        while (parentId != rootId) { // NOTE: don't need to check the parent of X2048
            parentId = getParentId(parentId);
            QuadTree memory parent = qtrees[parentId];
            require(parent.owner == address(0x0), "NFTG: parent owned");
        }
    }

    /**
     * @dev symetric: should be kept up-to-date with JS implementation
     * @notice calculates a parent square id from a child - it is known that the parents w/h will be 2x the child,
     * and from that we can determine the quad using it's x/y
     */
    function getParentRange(uint64 _id) public pure returns(Rectangle memory parent) {
        // parent is child until assignment (to save gas)...
        parent = getRangeFromId(_id);
        uint16 width = 2 * parent.w;
        uint16 height = 2 * parent.h;
        uint16 squareIndexX = calculateIndex(parent.x, parent.w);
        uint16 squareIndexY = calculateIndex(parent.y, parent.h);
        parent.x = squareIndexX / 2 * width + width / 2 - 1;
        parent.y = squareIndexY / 2 * height + height / 2 - 1;
        parent.w = width;
        parent.h = height;
        validate(parent);
    }

    /**
     * @param value is x or y
     * @param dimension is w or h (respectively)
     * @return index is the index starting at 0 and going to w/GRID_W - 1 or h/GRID_H - 1
     *      the indexes of the squares are the id of the column or row of that square (based on dimension)
     *      _0___1__   ____0___
     *   0 /   /   /  /       /
     *    /---+---/ 0/       /
     * 1 /___/___/  /_______/
     * @dev this function does not check values - it is presumed that the values have already passed 'validate'
     * x=127+256,y=127 w=256   x=0,y=0 w=1  special case for dimension of 1 since we move up and left
     * x=w/2-1+index*w         x=index*w
     * index*w=x-w/2+1
     * index=(x-w/2+1)/w
     */
    function calculateIndex(uint16 value, uint16 dimension) public pure returns(uint16 index) {
        index = (dimension == 1) ? (value / dimension) : ((value + 1 - dimension/2) / dimension);
    }

    /**
     * @dev symetric: should be kept up-to-date with JS implementation
     * @notice calculates a parent square id from a child
     */
    function getParentId(uint64 _id) public pure returns(uint64 parentId) {
        parentId = getIdFromRangeNoCheck(getParentRange(_id));
    }

    /**
     * @notice splits a square into a quarter (a.k.a. quad)
     * @dev there are ne, nw, se, sw quads on the QuadTrees
     * note: the quads are stored as Ids here not actual other QuadTrees
     *       also, special care was taken when writing this function so that this function does not transfer any ownership!
     */
    function subdivide(uint _id) public { 
        QuadTree storage qtree = qtrees[uint64(_id)];
        require(!qtree.divided, "NFTG: already divided");
        require(qtree.owner == msg.sender, "NFTG: only owner can subdivide");
        subdivideQTree(qtree);
    }

    /**
     * quad coordinates are at the center of the quad - this make dividing coords relative...
     * for root: x=1023, y=1023, w=2048, h=2048
     *  wChild = wParent/2 = 1024
     *  currently: xParent + wChild/2 > xParent + wParent/4 > 1023 + 512 = 1535
     */
    function subdivideQTree(QuadTree storage qtree) private { 
        (uint16 x,uint16 y,uint16 w,uint16 h) = getValues(qtree.boundary);
        require(w > 0 && h > 0, "NFTG: attempt to divide non-existing node");
        qtree.northeast = createQT(qtree.owner, x + w/4, y - h/4, w/2, h/2);
        qtree.northwest = createQT(qtree.owner, x - w/4, y - h/4, w/2, h/2);
        qtree.southeast = createQT(qtree.owner, x + w/4, y + h/4, w/2, h/2);
        qtree.southwest = createQT(qtree.owner, x - w/4, y + h/4, w/2, h/2);
        qtree.divided = true;
        qtree.owner = address(0x0);
    }

    // TODO: add ability to recombine?

    function getValues(Rectangle memory _rect) public pure returns(uint16 x, uint16 y, uint16 w, uint16 h) {
        x = _rect.x;
        y = _rect.y;
        w = _rect.w;
        h = _rect.h; // TODO: change rect to square and combine w & h to reduce gas
    }

    /**
     * @notice creates a QuadTree 
     * @return id the Id of the quad
     */
    function createQT(address _owner, uint16 _x, uint16 _y, uint16 _w, uint16 _h) public returns(uint64 id) {
        Rectangle memory boundary = Rectangle(_x, _y, _w, _h);
        id = getIdFromRange(boundary);
        QuadTree storage qtree = qtrees[id];
        qtree.boundary = boundary;
        qtree.owner = _owner;
    }

