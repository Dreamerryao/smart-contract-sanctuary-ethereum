contract KIBToken is IKIBToken, ERC20 {
    using WadRayMath for uint256;

    uint256 public constant MAX_YIELD = 1e29;
    uint256 public constant MAX_EPOCH_LENGTH = 365 days;
    uint256 public constant MIN_YIELD = WadRayMath.RAY;

