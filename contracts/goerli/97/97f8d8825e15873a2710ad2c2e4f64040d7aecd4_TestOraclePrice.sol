contract TestOraclePrice is  Ownable {
    using Strings for uint256;

    int256 public price = 50000000000;
    // 8 for USD
    uint8 public priceDecimal = 8;

    bool usePriceAggregator = true;
    // should be ETH/USD
    address public firstAggregatorFeed =  0xD4a33860578De61DBAbDc8BFdb98FD742fA7028e;
    // should be USD/EUR
    address public secondAggregatorFeed = 0x779877A7B0D9E8603169DdbD7836e478b4624789;

    constructor()  {
    }


    function payableAmount()  public view returns (int256) {
        int256 payableAmount = price;
        int256 oraclePrice = 1;
        int256 testPrice = 1;
        console.log("contract payable amount");
        console.log(uint(payableAmount));
        console.log("firstAggregatorFeed", firstAggregatorFeed);
        console.log("secondAggregatorFeed", secondAggregatorFeed);
        if (firstAggregatorFeed != address(0)) {
            if(secondAggregatorFeed != address(0)) {
                int256 price2 = getDerivedPrice(firstAggregatorFeed, secondAggregatorFeed, priceDecimal);
                console.log("oraclePrice (2feeds)");
                testPrice = price2;
            } else {
                (, int256 price1,,,) = AggregatorV3Interface(firstAggregatorFeed).latestRoundData();
                console.log("oraclePrice (1 feed)" );
                testPrice = price1;

            }
        }
        console.log("testPrice", uint(testPrice));
        console.log("final price to pay", uint(payableAmount * oraclePrice));
        return payableAmount * oraclePrice;
    }

    function updateAggregatorFeeds(address feed1, address feed2) public onlyOwner {
        firstAggregatorFeed = feed1;
        secondAggregatorFeed = feed2;
    }
    function getFeed1Price() public returns (int256) {
        return getFeedPrice(firstAggregatorFeed);
    }
    function getFeed2Price() public returns (int256) {
        return getFeedPrice(secondAggregatorFeed);
    }
    function getFeedPrice(address feed) internal returns (int256){
        (, int256 price,,,) = AggregatorV3Interface(firstAggregatorFeed).latestRoundData();
            return price;
        }

    function getDerivedPrice(
        address _base,
        address _quote,
        uint8 _decimals
    ) public view returns (int256) {
        require(_decimals > uint8(0) && _decimals <= uint8(18), "Invalid _decimals");
        int256 decimals = int256(10 ** uint256(_decimals));
        (, int256 basePrice,,,) = AggregatorV3Interface(_base).latestRoundData();
        uint8 baseDecimals = AggregatorV3Interface(_base).decimals();
        basePrice = scalePrice(basePrice, baseDecimals, _decimals);

        (, int256 quotePrice,,, ) = AggregatorV3Interface(_quote).latestRoundData();
        uint8 quoteDecimals = AggregatorV3Interface(_quote).decimals();
        quotePrice = scalePrice(quotePrice, quoteDecimals, _decimals);

        return (basePrice * decimals) / quotePrice;
    }

    function scalePrice(
        int256 _price,
        uint8 _priceDecimals,
        uint8 _decimals
    ) internal pure returns (int256) {
        if (_priceDecimals < _decimals) {
            return _price * int256(10 ** uint256(_decimals - _priceDecimals));
        } else if (_priceDecimals > _decimals) {
            return _price / int256(10 ** uint256(_priceDecimals - _decimals));
        }
        return _price;
    }

}

