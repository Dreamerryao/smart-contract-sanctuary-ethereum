import "./interfaces/IStrategy.sol";
import "./interfaces/IVault.sol";
import "./interfaces/IAggregator.sol";
import "./interfaces/IExchanger.sol";
import "./utils/Term.sol";
import "./utils/ActionBase.sol";
import "./utils/ManagerAccess.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract StrategyCoveredCall is IStrategy, ReentrancyGuard, Term, ManagerAccess, ActionBaseShortCall, ActionBaseSettle {
  using SafeMath for uint;

  uint internal constant feeRate = 10; //means 10%
  IVault public vault;
  IExchanger public exchanger;
  IERC20 public baseToken; //collateral token
  IERC20 public quoteToken;

  constructor(
    address _baseToken,
    address _quoteToken,
    address _aggregator,
    address _vault,
    address _exchanger
  ) ActionBaseShortCall(_aggregator) ActionBaseSettle(_aggregator) {
    require(_baseToken != address(0) && _quoteToken != address(0), "scc: invalid token");
    baseToken = IERC20(_baseToken);
    quoteToken = IERC20(_quoteToken);
    vault = IVault(_vault);
    exchanger = IExchanger(_exchanger);
    require(vault.collateralToken() == _baseToken, "scc: collateral mismatch");
  }

  /**
   * @dev Start a term by manager.
   *
   * @param _termId The term to start, like 0, 1, 2, 3
   * @param _expiry End timestamp of this term
   * @param _strikePrice Strike price of option, decided by manager
   */
  function startTerm(
    uint _termId,
    uint _expiry,
    uint _strikePrice
  ) external override onlyManager nonReentrant {
    //check before start
    (uint amountForThisTerm_, uint shareAmountForThisTerm_) = vault.getAmountsForNextTerm();
    require(amountForThisTerm_ > 0 && shareAmountForThisTerm_ > 0, "scc.startTerm: invalidAmount");

    _startTerm(_termId, TermInfo(_expiry, _strikePrice, amountForThisTerm_, shareAmountForThisTerm_, false, 0, 0, 0));

    vault.transferToStrategy();

    emit StartTerm(_termId);
  }

  /**
   * @dev Open a position by manager.
   *
   * @param _optionMsg The off-chain options with sig to fill
   * @param _aamMsg The on-chain option to open
   * @param _commonMsg The summary of option
   */
  function openPosition(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) external override onlyManager nonReentrant {
    openShortCall(_optionMsg, _aamMsg, _commonMsg);

    emit OpenPosition(_optionMsg, _aamMsg, _commonMsg);
  }

  /**
   * @dev Close a position by manager.
   *
   * @param _optionMsg The off-chain options with sig to fill
   * @param _aamMsg The on-chain option to open
   * @param _commonMsg The summary of option
   */
  function closePosition(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) external override onlyManager nonReentrant {
    closeShortCall(_optionMsg, _aamMsg, _commonMsg);

    emit ClosePosition(_optionMsg, _aamMsg, _commonMsg);
  }

  /**
   * @dev Settle a position by anyone.
   *
   * @param _market The option market of aggregator
   * @param _optionId The option id to be settled
   */
  function settlePosition(address _market, uint _optionId) external override nonReentrant {
    settleOption(_market, _optionId);

    emit SettlePosition(_market, _optionId);
  }

  /**
   * @dev End a term by anyone.
   *
   * @param _termId The term id to be ended
   */
  function endTerm(uint _termId) external override nonReentrant {
    TermInfo memory term_ = termInfos[_termId];
    require(block.timestamp > term_.expiry && term_.expiry != 0, "scc.endTerm: till expiry");

    //swap premium token to collateralToken
    uint premiumAmount_ = quoteToken.balanceOf(address(this));
    if (premiumAmount_ > 0) {
      quoteToken.approve(address(exchanger), premiumAmount_);
      //todo what if something wrong with this amount, such as 'UniswapV2: INSUFFICIENT_OUTPUT_AMOUNT'
      exchanger.exchange(address(quoteToken), address(baseToken), premiumAmount_);
    }

    uint finalBalance_ = baseToken.balanceOf(address(this));
    uint manageFee_;
    if (finalBalance_ > term_.amountForThisTerm) {
      manageFee_ = ((finalBalance_ - term_.amountForThisTerm) * feeRate) / 100;
    }

    uint sharePriceWithMantissa_ = Lib.divWithMantissa(finalBalance_ - manageFee_, term_.shareAmountForThisTerm);
    _endTerm(_termId, sharePriceWithMantissa_, manageFee_, finalBalance_);
    vault.addManageFee(_termId, manageFee_);
    baseToken.transfer(address(vault), finalBalance_);

    emit EndTerm(_termId);
  }

  function setExchanger(address _exchanger) external onlyManager {
    require(_exchanger != address(0) && _exchanger != address(exchanger), "scc.setExchanger: invalid exchanger");
    exchanger = IExchanger(_exchanger);
  }
}

