import "./IAggregator.sol";

interface IStrategy {
  function startTerm(
    uint termId,
    uint expiry,
    uint strikePrice
  ) external;

  function openPosition(
    IAggregator.AAOptionMsg[] memory optionMsg,
    IAggregator.AAMMsg memory aamMsg,
    IAggregator.CommonMsg memory commonMsg
  ) external;

  function closePosition(
    IAggregator.AAOptionMsg[] memory optionMsg,
    IAggregator.AAMMsg memory aamMsg,
    IAggregator.CommonMsg memory commonMsg
  ) external;

  function settlePosition(address market, uint optionId) external;

  function endTerm(uint termId) external;

  event StartTerm(uint termId);

  event OpenPosition(IAggregator.AAOptionMsg[] optionMsg, IAggregator.AAMMsg aamMsg, IAggregator.CommonMsg commonMsg);

  event ClosePosition(IAggregator.AAOptionMsg[] optionMsg, IAggregator.AAMMsg aamMsg, IAggregator.CommonMsg commonMsg);

  event SettlePosition(address market, uint optionId);

  event EndTerm(uint termId);
}

