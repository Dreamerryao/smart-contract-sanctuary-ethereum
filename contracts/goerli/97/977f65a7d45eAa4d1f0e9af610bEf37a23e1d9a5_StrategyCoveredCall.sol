pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../interfaces/IAggregator.sol";
import "../interfaces/IAAOption.sol";
import "../libraries/Lib.sol";
import "hardhat/console.sol";

// abstract contract can not deploy directly
abstract contract ActionBaseLongCall {
  IAggregator private aggregator;

  constructor(address _aggregator) {
    require(_aggregator != address(0), "ablc: invalid aggregator");
    aggregator = IAggregator(_aggregator);
  }
}

abstract contract ActionBaseLongPut {
  IAggregator private aggregator;

  constructor(address _aggregator) {
    require(_aggregator != address(0), "ablp: invalid aggregator");
    aggregator = IAggregator(_aggregator);
  }
}

abstract contract ActionBaseShortCall {
  IAggregator private aggregator;

  constructor(address _aggregator) {
    require(_aggregator != address(0), "absc: invalid aggregator");
    aggregator = IAggregator(_aggregator);
  }

  function openShortCall(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) internal {
    IAAOption.AAOptionDetail memory detail_ = Lib.getOptionDetail(aggregator, _commonMsg.market, _commonMsg.optionId);
    require(
      _commonMsg.tradeType == IAggregator.TradeType.SHORT && detail_.optionType,
      "absc.openShortCall: need shortcall"
    );

    uint collateralAmount_ = _commonMsg.amount;
    IERC20(_commonMsg.market).approve(address(aggregator), collateralAmount_);
    aggregator.openPosition(_optionMsg, _aamMsg, _commonMsg);
  }

  function closeShortCall(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) internal {
    IAAOption.AAOptionDetail memory detail_ = Lib.getOptionDetail(aggregator, _commonMsg.market, _commonMsg.optionId);
    require(
      _commonMsg.tradeType == IAggregator.TradeType.SHORT && detail_.optionType,
      "absc.closeShortCall: need shortcall"
    );

    aggregator.closePosition(_optionMsg, _aamMsg, _commonMsg);
  }
}

abstract contract ActionBaseShortPut {
  IAggregator private aggregator;

  constructor(address _aggregator) {
    require(_aggregator != address(0), "absp: invalid aggregator");
    aggregator = IAggregator(_aggregator);
  }

  function openShortPut(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) internal {
    IAAOption.AAOptionDetail memory detail_ = Lib.getOptionDetail(aggregator, _commonMsg.market, _commonMsg.optionId);
    require(
      _commonMsg.tradeType == IAggregator.TradeType.SHORT && !detail_.optionType,
      "absp.openShortPut: need shortput"
    );
    uint collateralAmount_ = _commonMsg.amount;
    IERC20(_commonMsg.market).approve(address(aggregator), collateralAmount_);
    aggregator.openPosition(_optionMsg, _aamMsg, _commonMsg);
  }

  function closeShortPut(
    IAggregator.AAOptionMsg[] memory _optionMsg,
    IAggregator.AAMMsg memory _aamMsg,
    IAggregator.CommonMsg memory _commonMsg
  ) internal {
    IAAOption.AAOptionDetail memory detail_ = Lib.getOptionDetail(aggregator, _commonMsg.market, _commonMsg.optionId);
    require(
      _commonMsg.tradeType == IAggregator.TradeType.SHORT && !detail_.optionType,
      "absp.closeShortPut: need shortput"
    );

    aggregator.closePosition(_optionMsg, _aamMsg, _commonMsg);
  }
}

abstract contract ActionBaseSettle {
  IAggregator private aggregator;

  constructor(address _aggregator) {
    require(_aggregator != address(0), "abs: invalid aggregator");
    aggregator = IAggregator(_aggregator);
  }

  function settleOption(address _market, uint _optionId) internal {
    IAAOption.AAOptionDetail memory detail_ = Lib.getOptionDetail(aggregator, _market, _optionId);

    require(block.timestamp > detail_.expireTime, "abs.settleOption: need expire");
    aggregator.settleOptions(_market, _optionId);
  }
}

