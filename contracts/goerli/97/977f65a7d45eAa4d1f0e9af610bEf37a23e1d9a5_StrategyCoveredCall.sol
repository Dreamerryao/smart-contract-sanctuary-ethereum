import "./IAAOption.sol";

//IAggregator is IAAOptionRouter
interface IAggregator {
  struct AAOptionMsg {
    address maker;
    uint amount;
    uint signAmount;
    uint signTime;
    uint price;
    bytes sign;
  }

  struct AAMMsg {
    uint amount; // aam match amount
    uint price; // aam avg price
    uint slippage; // aam slippage 10000 means 1,so 10 means 0.001,1 means 0.0001
    uint listingId; // aam option id expireTime,strike must be equal aaoption id
  }

  struct CommonMsg {
    uint optionId; // aaoption id
    uint amount; // order amount ,means total amount
    TradeType tradeType; //LONG SHORT
    address market; // aaoption market address
  }

  struct RouterMarketInfo {
    address baseAsset;
    address quoteAsset;
    address aaOption;
    address aaProof;
    address aamOption;
    address aamMarket;
  }

  enum TradeType {
    LONG,
    SHORT
  }

  function openPosition(
    AAOptionMsg[] memory optionMsg,
    AAMMsg memory aamMsg,
    CommonMsg memory commonMsg
  ) external returns (uint totalCost);

  function closePosition(
    AAOptionMsg[] memory optionMsg,
    AAMMsg memory aamMsg,
    CommonMsg memory commonMsg
  ) external returns (uint totalCost);

  function settleOptions(address market, uint optionId) external;

  function marketInfo(address) external view returns (RouterMarketInfo memory);
}

