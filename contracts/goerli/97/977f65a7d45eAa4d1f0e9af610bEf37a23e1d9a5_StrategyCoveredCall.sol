pragma solidity 0.8.9;

import "../interfaces/IAggregator.sol";
import "../interfaces/IAAOption.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

library Lib {
  using SafeMath for uint;

  uint internal constant ONE_MANTISSA = 10 ** 18;

  function calcAmount(IAggregator.AAOptionMsg[] memory optionMsg) internal pure returns (uint aaoAmount) {
    uint step;
    while (step < optionMsg.length) {
      aaoAmount += optionMsg[step].amount;
      step++;
    }
  }

  function getOptionDetail(
    IAggregator _aggregator,
    address _market,
    uint _optionId
  ) internal view returns (IAAOption.AAOptionDetail memory detail) {
    require(_market != address(0), "lib.getOptionDetail: zero market");

    IAggregator.RouterMarketInfo memory marketInfo_ = _aggregator.marketInfo(_market);
    require(marketInfo_.aaOption != address(0), "lib.getOptionDetail: invalid market");
    detail = IAAOption(marketInfo_.aaOption).getOptionDetail(_optionId);
  }

  //a*1e18/b
  //eg.1 a is 3eth(3e18), b is 2shareToken(2e18), then sharePriceWithMantissa is 1.5*1*1e18, which means price 1.5
  //eg.2 a is 3usdc(3e6), b is 2shareToken(2e18), then sharePriceWithMantissa is 1.5*(1e-12)*1e18, which means price 1.5
  function divWithMantissa(uint a, uint b) internal pure returns (uint) {
    return a.mul(ONE_MANTISSA).div(b);
  }
}

