import "@openzeppelin/contracts/utils/math/Math.sol";
import {ABDKMath64x64 as ABDK} from "../math/abdk.sol";

import "../interfaces/IMarketStates.sol";
import "../interfaces/IMarket.sol";
import "../interfaces/IProtocolConfig.sol";
import "../interfaces/IRiskCalculation.sol";

import "hardhat/console.sol";

/// @title RiskCalculation (ABDK version)
/// @notice Calculate current risk using current prices from price oracles, balances from asset pools and capital pools etc.
/// @notice Provides historical data based on time.
contract RiskCalculation is IRiskCalculation {
    address public config;
    address public state;

    int128 public constant ONE = 1 << 64; // 1
    int128 public constant MAX = 147573952589676412928; // 8
    int128 public EPS;

    uint public constant DIVIDER = 10**18;
    uint public constant DIVIDER_STABLE = 10**6;
    uint public constant DIVIDER_ORACLE = 10**8;

    constructor(address _config, address _state) {
        config = _config;
        state = _state;
        EPS = 1846010811935603350; // 0.1
    }

    /// @notice Calculate premium against the accet pool
    function PAP(address token) public view override returns (int128 _PAP) {
        MarketState memory s = IMarketStates(state).getCurrentState(token);

        (int128 AP, , , , , int128 L, ) = getState(token);

        // Fill U_alpha vector
        int128[4] memory U_alpha = [s.VelNorm, s.AccelNorm, s.shock, s.surge];

        // Calculate Price Risk Factor
        int128 _PRF = PRF(token, U_alpha);
        //console.log("PRF: %s", uint(int(_PRF)));

        // Calculate VRF (Liability Risk Factor)
        int128 v = AP > L ? min(MAX, ABDK.div(ABDK.div(AP, MAX), AP - L)) : MAX;
        console.log(
            "price: %s, V_Max: %s, L: %s",
            uint(int(s.price)),
            uint(int(MAX)),
            uint(int(L))
        );
        //console.log("v: %s", uint(int(v)));
        int128 _VRF = ABDK.mul(_PRF, v);
        //console.log("VRF: %s", uint(int(_VRF)));

        int128 Pclaim = ABDK.div(_VRF, MAX); // virtual probability of claim for USDC
        //console.log("Pclaim: %s", uint(int(Pclaim)));

        // Calculate Risk Liquidity Ratios and reference values
        int128[6] memory _W_Ref = RLRef(token, Pclaim);
        int128[6] memory _W = RLR(token);

        // Calculate Liquidity Risk Factor
        int128 _LRF = LRF(token, _W, _W_Ref);
        //console.log("LRF: %s", uint(int(_LRF)));

        // Calculate the Premium against the Asset Pool
        //console.log("AP: %s", uint(int(AP)));
        _PAP = ABDK.mul(_PRF + _LRF, AP);
        //console.log("PAP: %s", uint(int(_PAP)));
    }

    /// @notice Calculate epsilon for all makers
    function epsilon(address token) public view override returns (int128) {
        MarketState memory s = IMarketStates(state).getCurrentState(token);
        return ABDK.div(s.PAP, 180 << 64);
    }

    /// @notice Calculate premium for given position (we use DIVIDER_ORACLE decimal)
    function premium(address token, TakerPosition memory pos)
        public
        view
        override
        returns (uint)
    {
        MarketState memory s = IMarketStates(state).getCurrentState(token);

        // Individual Taker Premium
        int128 multiplier = IProtocolConfig(config).getPremiumMultiplier(
            token,
            pos.risk,
            pos.term
        );
/*        console.log(
            "PAP: %s, Mul: %s",
            uint(int(s.PAP)),
            uint(int(multiplier))
        ); */
        return
            uint(
                (int(ABDK.mul(multiplier, s.PAP)) / ONE) * int(DIVIDER_ORACLE)
            );
    }

    /// @notice calculate yield for given position
    function yield(address token, MakerPosition memory pos)
        public
        view
        override
        returns (int)
    {
        MarketState memory s = IMarketStates(state).getCurrentState(token);

        // Individual Maker yield
        int128 multiplier = IProtocolConfig(config).getYieldMultiplier(
            token,
            pos.risk,
            pos.term
        );
        console.log(
            "EPS: %s, Mul: %s",
            uint(int(s.EPS)),
            uint(int(multiplier))
        );
        return
            (int(ABDK.mul(multiplier, s.EPS)) / int(ONE)) * int(DIVIDER_ORACLE);
    }

    function getState(address token)
        public
        view
        returns (
            int128 AP,
            int128 AR,
            int128 CP,
            int128 CR,
            int128 B,
            int128 L,
            int128 D
        )
    {
        address market = IProtocolConfig(config).getMarket(token);
        (int price, ) = IMarket(market).price();
        (
            ,
            uint _AP,
            uint _AR,
            uint _CP,
            uint _CR,
            uint _B,
            uint _L,
            uint _D
        ) = IMarket(market).getState();
        AP = toInt128o(_AP);
        AR = toInt128o(_AR);
        CP = toInt128o(_CP);
        CR = toInt128o(_CR);
        B = toInt128o(_B);
        L = toInt128o(_L);
        D = toInt128o(_D);
    }

    /// @notice Calculate Risk Liquidity Ratio
    function RLRef(address token, int128 Pclaim)
        public
        view
        returns (int128[6] memory _W_Ref)
    {
        (
            int128 AP,
            int128 AR,
            int128 CP,
            int128 CR,
            int128 B,
            int128 L,
            int128 D
        ) = getState(token);
        console.log("L:", uint(int(L)));
        console.log("D:", uint(int(D)));
        console.log("B:", uint(int(B)));

        int128 deltaL = ABDK.mul(ABDK.mul(ONE, Pclaim), L);
        int128 gammaB = ABDK.mul(ABDK.mul(ONE, ONE - Pclaim), B);

        _W_Ref[0] = gammaB > 0 ? ABDK.div(AP, gammaB) : MAX;
        _W_Ref[1] = gammaB > 0 ? ABDK.div(AP + AR, gammaB) : MAX;
        _W_Ref[2] = deltaL > 0 ? ABDK.div(CP, deltaL) : MAX;
        _W_Ref[3] = deltaL > 0 ? ABDK.div(CP + CR, deltaL) : MAX;
        _W_Ref[4] = D > 0 ? ABDK.div(CP - deltaL, D + EPS) : MAX;
        _W_Ref[5] = D > 0 ? ABDK.div(CP + CR - deltaL, D + EPS) : MAX;

        /*for (uint i = 0; i < 6; i++){
            console.log("WRef%s = %s", i, uint(int( _W_Ref[i]) ) );
        }*/
    }

    function RLR(address token) public view returns (int128[6] memory _W) {
        (
            int128 AP,
            int128 AR,
            int128 CP,
            int128 CR,
            int128 B,
            int128 L,
            int128 D
        ) = getState(token);

        _W[0] = min(B > 0 ? ABDK.div(AP, B) : MAX, MAX);
        _W[1] = min(B > 0 ? ABDK.div(AP + AR, B) : MAX, MAX);
        _W[2] = min(L > 0 ? ABDK.div(CP, L) : MAX, MAX);
        _W[3] = min(L > 0 ? ABDK.div(CP + CR, L) : MAX, MAX);
        _W[4] = min(D > 0 ? ABDK.div(CP - L, D) : MAX, MAX);
        _W[5] = min(D > 0 ? ABDK.div(CP + CR - L, D) : MAX, MAX);

        /*for (uint i = 0; i < 6; i++){
            console.log("W%s = %s", i, uint(int( _W[i]) ) );
        }*/
    }

    /// @notice Calculate Price Risk Factor
    function PRF(address token, int128[4] memory U_alpha)
        public
        view
        returns (int128 _PRF)
    {
        MarketConfig memory c = IProtocolConfig(config).getConfig(token);
        int128[4] memory U_val = [
            U(U_alpha[0], c.U_Ref[0], c.U_Lambda[0]),
            U(U_alpha[1], c.U_Ref[1], c.U_Lambda[1]),
            U(U_alpha[2], c.U_Ref[2], c.U_Lambda[2]),
            U(U_alpha[3], c.U_Ref[3], c.U_Lambda[3])
        ];

        int128 PRF2 = ABDK.mul(U_val[2], U_val[3]);
        _PRF = ABDK.mul(U_val[0], PRF2);
    }

    /// @notice Calculate Liquidity Risk Factors
    function LRF(
        address token,
        int128[6] memory W_ratio,
        int128[6] memory W_ref
    ) public view returns (int128 _LRF) {
        MarketConfig memory c = IProtocolConfig(config).getConfig(token);
        int128[6] memory W_val = [
            W(W_ratio[0], W_ref[0]),
            W(W_ratio[1], W_ref[1]),
            W(W_ratio[2], W_ref[2]),
            W(W_ratio[3], W_ref[3]),
            W(W_ratio[4], W_ref[4]),
            W(W_ratio[5], W_ref[5])
        ];
        _LRF = ABDK.mul(W_val[0], c.W_Lambda[0]);
        for (uint i = 1; i < 6; i++)
            _LRF = ABDK.mul(_LRF, ABDK.mul(W_val[i], c.W_Lambda[i]));
    }

    /// @notice Calculate U
    function U(
        int128 alpha,
        int128 r,
        int128 lambda
    ) public view returns (int128 u) {
        if (alpha == r) return MAX;
        u = ABDK.abs(alpha - r);
        if (u < ABDK.MIN_64x64) return MAX;
        u = ABDK.div(ONE, u);
        u = MAX < u ? MAX : u;
        u = ABDK.mul(u, lambda);
    }

    /// @notice Calculate W
    function W(int128 ratio, int128 r) public pure returns (int128 w) {
        if (ratio == r) {
            w = MAX;
        } else {
            w = min(MAX, ABDK.div(ONE, ABDK.abs(ratio - r)));
        }
    }

    /// @notice max of two numbers 64.64
    function max(int128 x, int128 y) public pure returns (int128) {
        return x > y ? x : y;
    }

    /// @notice min of two numbers 64.64
    function min(int128 x, int128 y) public pure returns (int128) {
        return x > y ? y : x;
    }

    // converters
    function toInt128t(uint a) public pure returns (int128) {
        return int128(int((a * (2**64)) / DIVIDER));
    }

    function toInt128s(uint a) public pure returns (int128) {
        return int128(int((a * (2**64)) / DIVIDER_STABLE));
    }

    function toInt128o(uint a) public pure returns (int128) {
        return int128(int((a * (2**64)) / DIVIDER_ORACLE));
    }
}

