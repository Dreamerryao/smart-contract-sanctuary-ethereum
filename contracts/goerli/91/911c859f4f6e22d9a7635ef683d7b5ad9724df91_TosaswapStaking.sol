pragma solidity ^0.8.9;

abstract contract Stakeable is Ownable, AccessControl {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using SignedSafeMath for int256;

    bytes32 public constant REWARD_UPDATER = keccak256("REWARD_UPDATER");

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many tokens the user has provided.
        int256 rewardDebt; // Reward debt. See explanation below.
    }

    struct ReferralInfo {
        int256 rewardDebt;
        uint256 percentage; // percentage refferal will receive
        uint256 amount;
        address referrer; // referrer address
    }

    // Info of  pool.
    struct PoolInfo {
        IERC20 stakingToken; // Token to stake
        uint256 lastRewardBlock; // Last block number that tosa distribution occurs.
        uint256 accTosaPerShare; // Accumulated tosa per share, times 1e12. See below.
        uint256 depositStartBlock;
        uint256 depositEndBlock;
    }

    struct RewardInfo {
        uint256 rewardPerBlock;
        uint256 rewardStartBlock;
        uint256 rewardEndBlock;
    }

    uint256 public referralPercentage; // 1 * 1e3 = 1%

    PuppyToken public rewardToken;

    // Info of  pool.
    PoolInfo[] public poolInfo;

    //Info of reward
    RewardInfo[] public rewardInfo;

    // Keeps reward tokens
    StakingTreasury public treasury;

    // keeps track of total amount staked. Also includes the referral amounts;
    mapping(uint256 => uint256) public totalStaked;

    // Info of each user that stakes token tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;

    mapping(uint256 => mapping(address => ReferralInfo)) public referralInfo;

    // events which will emit on different tasks
    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 amount);
    event FeesUpdated(uint256 depositFee, uint256 withdrawFee);
    event Harvest(address indexed user, uint256 amount);

    constructor(PuppyToken _rewardToken, uint256 _referralPercentage) {
        require(_referralPercentage <= 100e3, "exceding referral percentage");
        rewardToken = _rewardToken;
        referralPercentage = _referralPercentage;

        treasury = new StakingTreasury();
        treasury.allowClaiming(_rewardToken);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    modifier onlyOwnerOrRewardRole() {
        require(
            owner() == _msgSender() || hasRole(REWARD_UPDATER, _msgSender()),
            "Caller is not the owner or reward role"
        );
        _;
    }

    // Add a new pool. Can only be called by the owner.
    // XXX DO NOT add the same  token more than once. Rewards will be messed up if you do.
    function setPool(
        IERC20 _stakingToken,
        uint256 _rewardPerBlock,
        uint256 _depositStartBlock,
        uint256 _depositEndBlock,
        uint256 _rewardStartBlock,
        uint256 _rewardEndBlock
    ) public onlyOwner {
        require(
            _depositStartBlock < _depositEndBlock,
            "deposit end block must be after start block"
        );
        require(
            _rewardStartBlock >= _depositEndBlock,
            "invalid reward start block"
        );
        require(
            _rewardStartBlock < _rewardEndBlock,
            "invalid reward end block"
        );
        poolInfo.push(
            PoolInfo(_stakingToken, 0, 0, _depositStartBlock, _depositEndBlock)
        );
        rewardInfo.push(
            RewardInfo(_rewardPerBlock, _rewardStartBlock, _rewardEndBlock)
        );
    }

    function updateDepositState(
        uint256 _pid,
        uint256 _depositStartBlock,
        uint256 _depositEndBlock
    ) external onlyOwner {
        PoolInfo storage pool = poolInfo[_pid];
        require(
            _depositStartBlock < _depositEndBlock,
            "deposit end block must be after start block"
        );
        pool.depositStartBlock = _depositStartBlock;
        pool.depositEndBlock = _depositEndBlock;
    }

    function updateRewardState(
        uint256 _pid,
        uint256 _rewardStartBlock,
        uint256 _rewardEndBlock
    ) external onlyOwner {
        PoolInfo memory pool = poolInfo[_pid];
        RewardInfo storage reward = rewardInfo[_pid];
        require(
            _rewardStartBlock >= pool.depositEndBlock,
            "invalid reward start block"
        );
        require(
            _rewardStartBlock < _rewardEndBlock,
            "invalid reward end block"
        );
        require(
            block.number < reward.rewardEndBlock,
            "cannot change: reward end block reached"
        );
        reward.rewardStartBlock = _rewardStartBlock;
        reward.rewardEndBlock = _rewardEndBlock;
    }

    function updateRewardToken(PuppyToken _rewardToken) public onlyOwner {
        rewardToken = _rewardToken;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        pure
        returns (uint256)
    {
        return _to.sub(_from);
    }

    // Update reward variables of the  pool to be up-to-date.
    function updatePool(uint256 _pid) public returns (PoolInfo memory pool) {
        pool = poolInfo[_pid];
        RewardInfo memory reward = rewardInfo[_pid];
        if (
            block.number > reward.rewardEndBlock &&
            reward.rewardEndBlock > pool.lastRewardBlock
        ) {
            uint256 tokenSupply = totalStaked[_pid];
            if (tokenSupply > 0) {
                uint256 multiplier = getMultiplier(
                    reward.rewardStartBlock,
                    reward.rewardEndBlock
                );
                uint256 pendingReward = multiplier.mul(reward.rewardPerBlock);
                pool.accTosaPerShare = pool.accTosaPerShare.add(
                    pendingReward.mul(1e12).div(tokenSupply)
                );
                if (!rewardToken.mintingFinished()) {
                    rewardToken.mint(address(treasury), pendingReward);
                }
            }
            pool.lastRewardBlock = block.number;
            poolInfo[_pid] = pool;
        }
    }

    function getReferrerAmount(uint256 amount, uint256 percentage)
        internal
        pure
        returns (uint256)
    {
        return amount.mul(percentage).div(1e5);
    }

    function getReferrer(
        uint256 _pid,
        address account,
        address _referrer
    ) internal returns (ReferralInfo storage) {
        UserInfo memory user = userInfo[_pid][account];
        ReferralInfo storage referrer = referralInfo[_pid][account];
        if (user.amount == 0) {
            if (_referrer != address(0) && _referrer != account) {
                referrer.referrer = _referrer;
                referrer.percentage = referralPercentage;
            }
        }

        return referrer;
    }

    function depositWithReferrer(
        uint256 _pid,
        uint256 _amount,
        address _referrer
    ) public virtual {
        address account = msg.sender;
        UserInfo memory user = userInfo[_pid][account];
        UserInfo memory refuser = userInfo[_pid][_referrer];
        require(account != _referrer, "Referrer should be different");
        require(refuser.amount > 0, "Invalid Referrer");
        require(user.amount == 0, "User has already staked");
        _deposit(_pid, _amount, _referrer);
    }

    function deposit(uint256 _pid, uint256 _amount) public virtual {
        _deposit(_pid, _amount, address(0));
    }

    // Deposit  tokens  for tosa allocation.
    function _deposit(
        uint256 _pid,
        uint256 _amount,
        address _referrer
    ) internal {
        address account = msg.sender;
        PoolInfo memory pool = updatePool(_pid);
        UserInfo storage user = userInfo[_pid][account];
        ReferralInfo storage referrer = getReferrer(_pid, account, _referrer);
        require(
            block.number >= pool.depositStartBlock &&
                block.number <= pool.depositEndBlock,
            "invalid stake block"
        );
        user.amount = user.amount.add(_amount);
        user.rewardDebt = user.rewardDebt.add(
            int256(_amount.mul(pool.accTosaPerShare).div(1e12))
        );
        uint256 _referrerAmount = getReferrerAmount(
            _amount,
            referrer.percentage
        );
        if (referrer.referrer != address(0)) {
            referrer.amount = _referrerAmount;
            referrer.rewardDebt = referrer.rewardDebt.add(
                int256(_referrerAmount.mul(pool.accTosaPerShare).div(1e12))
            );
        }
        uint256 amount = _amount.add(_referrerAmount);
        totalStaked[_pid] = totalStaked[_pid].add(amount);
        pool.stakingToken.safeTransferFrom(
            address(account),
            address(this),
            _amount
        );
        emit Deposit(account, _amount);
    }

    // View function to see pending Tosa on frontend.
    function pendingTosa(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo memory pool = poolInfo[_pid];
        RewardInfo memory reward = rewardInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accTosaPerShare = pool.accTosaPerShare;
        uint256 supply = totalStaked[_pid];
        if (
            block.number > reward.rewardStartBlock &&
            block.number <= reward.rewardEndBlock &&
            supply != 0
        ) {
            uint256 multiplier = getMultiplier(
                reward.rewardStartBlock,
                block.number
            );
            uint256 tosaReward = multiplier.mul(reward.rewardPerBlock);
            accTosaPerShare = accTosaPerShare.add(
                tosaReward.mul(1e12).div(supply)
            );
        }
        return
            uint256(
                int256(user.amount.mul(accTosaPerShare).div(1e12)).sub(
                    user.rewardDebt
                )
            );
    }

    function harvest(uint256 _pid, address to) public virtual {
        address account = msg.sender;
        PoolInfo memory pool = updatePool(_pid);
        RewardInfo memory reward = rewardInfo[_pid];

        UserInfo storage user = userInfo[_pid][account];
        ReferralInfo storage referrer = referralInfo[_pid][account];
        require(
            block.number >= reward.rewardEndBlock,
            "reward block not reached"
        );
        int256 accumulatedTosa = int256(
            user.amount.mul(pool.accTosaPerShare).div(1e12)
        );
        uint256 _pendingTosa = uint256(accumulatedTosa.sub(user.rewardDebt));

        // Effects
        user.rewardDebt = accumulatedTosa;

        // Interactions
        safeRewardTransfer(to, _pendingTosa);
        if (referrer.referrer != address(0) && referrer.amount > 0) {
            int256 accReferrerTosa = int256(
                referrer.amount.mul(pool.accTosaPerShare).div(1e12)
            );
            uint256 referrerPendingTosa = uint256(
                accReferrerTosa.sub(referrer.rewardDebt)
            );
            referrer.rewardDebt = accReferrerTosa;
            safeRewardTransfer(referrer.referrer, referrerPendingTosa);
        }
        //implement referrer here

        emit Harvest(msg.sender, _pendingTosa);
    }

    // Withdraw and harvest tokens from  user
    function withdrawAndHarvest(uint256 _pid, uint256 _amount) public virtual {
        address account = msg.sender;
        PoolInfo memory pool = updatePool(_pid);
        RewardInfo memory reward = rewardInfo[_pid];
        UserInfo storage user = userInfo[_pid][account];
        ReferralInfo storage referrer = referralInfo[_pid][account];

        require(user.amount >= _amount, "withdraw: not good");
        require(
            block.number >= reward.rewardEndBlock,
            "reward block not reached"
        );
        int256 accumulatedTosa = int256(
            user.amount.mul(pool.accTosaPerShare).div(1e12)
        );
        uint256 pending = uint256(accumulatedTosa.sub(user.rewardDebt));
        safeRewardTransfer(account, pending);
        uint256 referrerAmount = referrer.amount;
        if (referrerAmount > 0 && referrer.referrer != address(0)) {
            uint256 referrerPending = uint256(
                int256(referrerAmount.mul(pool.accTosaPerShare).div(1e12)).sub(
                    referrer.rewardDebt
                )
            );
            safeRewardTransfer(referrer.referrer, referrerPending);
        }
        user.rewardDebt = accumulatedTosa.sub(
            int256(_amount.mul(pool.accTosaPerShare).div(1e12))
        );
        user.amount = user.amount.sub(_amount);
        uint256 _referrerAmount = getReferrerAmount(
            _amount,
            referrer.percentage
        );
        referrer.amount = _referrerAmount;
        referrer.rewardDebt = int256(
            referrerAmount.mul(pool.accTosaPerShare).div(1e12)
        ).sub(int256(_referrerAmount.mul(pool.accTosaPerShare).div(1e12)));
        if (user.amount <= 0) {
            referrer.referrer = address(0);
            referrer.percentage = 0;
        }
        uint256 amount = _amount.add(_referrerAmount);
        totalStaked[_pid] = totalStaked[_pid].sub(amount);
        pool.stakingToken.safeTransfer(address(account), _amount);
        emit Withdraw(account, _amount);
        emit Harvest(account, pending);
    }

    // Withdraw tokens from  user
    function withdraw(uint256 _pid, uint256 _amount) public virtual {
        address account = msg.sender;
        PoolInfo memory pool = updatePool(_pid);
        UserInfo storage user = userInfo[_pid][account];
        ReferralInfo storage referrer = referralInfo[_pid][account];

        require(user.amount >= _amount, "withdraw: not good");
        user.rewardDebt = user.rewardDebt.sub(
            int256(_amount.mul(pool.accTosaPerShare).div(1e12))
        );
        user.amount = user.amount.sub(_amount);
        uint256 _referrerAmount = getReferrerAmount(
            _amount,
            referrer.percentage
        );
        referrer.rewardDebt = referrer.rewardDebt.sub(
            int256(_referrerAmount.mul(pool.accTosaPerShare).div(1e12))
        );
        referrer.amount = getReferrerAmount(user.amount, referrer.percentage);
        if (user.amount <= 0) {
            referrer.referrer = address(0);
            referrer.percentage = 0;
        }
        uint256 amount = _amount.add(_referrerAmount);
        totalStaked[_pid] = totalStaked[_pid].sub(amount);
        pool.stakingToken.safeTransfer(address(account), _amount);
        emit Withdraw(account, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        ReferralInfo storage referrer = referralInfo[_pid][msg.sender];
        pool.stakingToken.safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, user.amount);
        totalStaked[_pid] = totalStaked[_pid].sub(
            user.amount.add(referrer.amount)
        );
        user.amount = 0;
        user.rewardDebt = 0;
        referrer.amount = 0;
        referrer.rewardDebt = 0;
        referrer.referrer = address(0);
        referrer.percentage = 0;
    }

    function safeRewardTransfer(address to, uint256 amount)
        internal
        returns (uint256)
    {
        uint256 balance = rewardToken.balanceOf(address(treasury));
        require(amount > 0, "Reward amount must be more than zero");
        require(balance > 0, "Not enough reward tokens for transfer");
        if (amount > balance) {
            IERC20(rewardToken).safeTransferFrom(
                address(treasury),
                to,
                balance
            );
            return balance;
        }
        IERC20(rewardToken).safeTransferFrom(address(treasury), to, amount);
        return amount;
    }

    function updateRewardPerBlock(uint256 _pid, uint256 _rewardPerBlock)
        external
        onlyOwnerOrRewardRole
    {
        RewardInfo memory reward = rewardInfo[_pid];
        require(
            block.number < reward.rewardEndBlock,
            "cannot change: reward end block reached"
        );
        rewardInfo[_pid].rewardPerBlock = _rewardPerBlock;
    }

    function updateReferralPercentage(uint256 _referralPercentage)
        external
        onlyOwner
    {
        require(_referralPercentage <= 100e3, "exceding referral percentage");
        referralPercentage = _referralPercentage;
    }
}

