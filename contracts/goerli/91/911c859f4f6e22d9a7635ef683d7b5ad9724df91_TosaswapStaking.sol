pragma solidity ^0.8.9;

contract TosaswapStaking is Ownable, Stakeable {
    using SafeERC20 for IERC20;

    bool public halted = false;

    event Halted(bool status);

    constructor(PuppyToken _rewardToken, uint256 _referralPercentage)
        Stakeable(_rewardToken, _referralPercentage)
    {}

    function halt(bool status) external onlyOwner {
        halted = status;
        emit Halted(status);
    }

    modifier notHalted() {
        require(!halted, "Deposits are paused");
        _;
    }

    function depositWithReferrer(
        uint256 _pid,
        uint256 amount,
        address _referrer
    ) public override notHalted {
        super.depositWithReferrer(_pid, amount, _referrer);
    }

    function deposit(uint256 _pid, uint256 amount) public override notHalted {
        super.deposit(_pid, amount);
    }

    function withdraw(uint256 _pid, uint256 amount) public override {
        super.withdraw(_pid, amount);
    }

    function withdrawAndHarvest(uint256 _pid, uint256 amount) public override {
        super.withdrawAndHarvest(_pid, amount);
    }

    function harvest(uint256 _pid, address to) public override {
        super.harvest(_pid, to);
    }

    function withdrawRewards() external onlyOwner {
        treasury.withdrawRewards(rewardToken, _msgSender());
    }

    function allowClaiming() external onlyOwner {
        treasury.allowClaiming(rewardToken);
    }
}