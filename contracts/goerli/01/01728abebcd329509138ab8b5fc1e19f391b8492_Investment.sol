pragma solidity 0.8.4;



contract poolHelpers {

    event poolAddedTo(
        address from,
        uint256 ethAmount,
        uint256 poolTokensRecieved
    );

    event poolRemovedFrom(
        address from,
        uint256 poolTokensBurned,
        uint256 ethReceived
    );

    event tokensSwapped(
        address tokenIn,
        address tokenOut,
        uint256 tokenInAmount,
        uint256 tokenOutAmount
    );


    function getContractTotalBalance(address[] memory _tokensHeld, IUniswapV2Router02 uniswapV2Router) public view returns (uint256) {
        uint256 contractEtherValue = address(this).balance;
        for (uint i = 0; i < _tokensHeld.length; i++){
            uint256 tokensHeld = IERC20(_tokensHeld[i]).balanceOf(address(this));
            if(_tokensHeld[i] == uniswapV2Router.WETH()){
                contractEtherValue += tokensHeld;
            } else {
                address[] memory path = new address[](2);
                path[0] = _tokensHeld[i];
                path[1] = uniswapV2Router.WETH();
                uint256[] memory tokensValue = uniswapV2Router.getAmountsOut(tokensHeld, path);
                contractEtherValue += tokensValue[1];
            }
        }
        return contractEtherValue;
    }

    function isTokenHeld(address[] memory _tokensHeld, address _address) public view returns (bool) {
    for (uint i = 0; i < _tokensHeld.length; i++) {
        if (_tokensHeld[i] == _address) {
            return true;
        }
    }
    return false;
    }

    function getIndexOfToken(address[] memory _tokensHeld, address _address) public view returns (uint256) {
        require(isTokenHeld(_tokensHeld, _address) == true, "This token is not held.");
        for (uint i = 0; i < _tokensHeld.length; i++){
            if(_tokensHeld[i] == _address){
                return i;
            }
        }
    }

    function getPath(address tokenIn, address tokenOut, IUniswapV2Router02 uniswapV2Router) public view returns (address[] memory) {
        uint256 pathNum;
        if(tokenIn == uniswapV2Router.WETH() || tokenOut == uniswapV2Router.WETH()){
            pathNum = 2;
        } else {
            pathNum = 3;
        }
        address[] memory path = new address[](pathNum);
        if(pathNum == 2){
            path[0] = tokenIn;
            path[1] = tokenOut;
        } else {
            path[0] = tokenIn;
            path[1] = uniswapV2Router.WETH();
            path[2] = tokenOut;
        }
        return path;
    }

    
    
}
