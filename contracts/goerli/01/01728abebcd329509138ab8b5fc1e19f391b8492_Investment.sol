        _feeHandlerAddress = feeHandlerAddress_;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        uniswapV2Router = _uniswapV2Router;

    }

    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual override returns (uint8) {
        return 18;
    }

    function getAllTokensHeld() public view returns (address[] memory) {
        return _tokensHeld;
    }


    function risk() public view virtual returns (string memory) {
        return _riskLevel;
    }

    function changeTokensToSwapIntoOnDeposit(address[] memory addresses, uint256[] memory slippages, uint256[] memory ratios) public onlyOwner {
        _tokensToSwapIntoOnDeposit = addresses;
        for (uint i = 0; i < _tokensToSwapIntoOnDeposit.length; i++){
            address token = _tokensToSwapIntoOnDeposit[i];
            _depositTokenSwapProperties[token]["slippage"] = slippages[i];
            _depositTokenSwapProperties[token]["ratio"] = ratios[i];
        }
    }

    function crashProtect(uint256[] memory slippages, uint256[] memory percentagesToSwap) public onlyOwner {
        _ownerIsSwapping = true;
        for (uint i = 0; i < _tokensHeld.length; i++){
            if(percentagesToSwap[i] != 0){
                uint256 tokenBalance = IERC20(_tokensHeld[i]).balanceOf(address(this));
                uint256 amountToSwap = tokenBalance * percentagesToSwap[i] / 10**18;
                address[] memory path = new address[](2);
                path[0] = uniswapV2Router.WETH();
                path[1] = _tokensHeld[i];
                uint256[] memory amountsOut = uniswapV2Router.getAmountsOut(amountToSwap, path);
                swapTokensForTokens(_tokensHeld[i], _stableCoin, amountToSwap , amountsOut[1] * slippages[i] / 10**18);
            }
        }
        _ownerIsSwapping = false;
    }

    function removeTokenFromTokensHeld(address token) public onlyOwner {
        require(isTokenHeld(_tokensHeld, token) == true, "This token is not in the held tokens list!");

        uint256 tokenIndex = getIndexOfToken(_tokensHeld, token);
        _removeAddressFromTokensHeld(tokenIndex);
    }

    function _removeAddressFromTokensHeld(uint256 index) internal {
        require(index < _tokensHeld.length, "Index is too large!");
        _tokensHeld[index] = _tokensHeld[_tokensHeld.length-1];
        _tokensHeld.pop();
    }

    function addToPool() public payable {
        if(totalSupply() == 0){
            require(msg.value >= 10000000000000000 , "You must initiate the pool with at least 0.01 ETH!");
        }
        uint256 poolValue = getContractTotalBalance(_tokensHeld, uniswapV2Router) - msg.value;
        uint256 poolTokensMinted;
        uint256 factoryCut = msg.value * _factoryFee / 10**18;
        if(totalSupply() != 0) {      
            uint256 percentIncrease = (msg.value - factoryCut) * 10**18 / poolValue;
            percentIncrease += 10**18;

            uint256 newSupply = totalSupply() * percentIncrease / 10**18;
            uint256 poolTokensToMint = newSupply - totalSupply();
            poolTokensMinted = poolTokensToMint;
            _mint(msg.sender, poolTokensToMint);  

            if(balanceOf(msg.sender) - poolTokensToMint == 0){
                _totalHolders++;
            }

        } else {
            _mint(msg.sender, (msg.value - factoryCut)  / 10000000000000000);
            poolTokensMinted = (msg.value - factoryCut)  / 10000000000000000;
            _totalHolders++;
        }
        IWETH WETH = IWETH(uniswapV2Router.WETH());
        for(uint i = 0; i < _tokensToSwapIntoOnDeposit.length; i++){ 
            address tokenOut = _tokensToSwapIntoOnDeposit[i];
            uint256 amountIn = (msg.value - factoryCut) * _depositTokenSwapProperties[tokenOut]["ratio"] / 10**18;
            if(tokenOut == uniswapV2Router.WETH()){
                WETH.deposit{value : amountIn}();
            } else {
                address[] memory path = new address[](2);
                path[0] = uniswapV2Router.WETH();
                path[1] = tokenOut;
                uint256[] memory amountsOut = uniswapV2Router.getAmountsOut(amountIn, path);
                uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{ value: amountIn }(
                    amountsOut[1] * _depositTokenSwapProperties[tokenOut]["slippage"] / 10**18,
                    path,
                    address(this),
                    block.timestamp
                );
            }
            if(isTokenHeld(_tokensHeld, _tokensToSwapIntoOnDeposit[i]) == false){
                    _tokensHeld.push(_tokensToSwapIntoOnDeposit[i]);
                }
        }
        WETH.deposit{value : factoryCut}();
        IERC20(uniswapV2Router.WETH()).approve(_feeHandlerAddress, factoryCut);
        IFeeHandler(_feeHandlerAddress).collectFees(factoryCut);
       emit poolAddedTo(msg.sender, msg.value, poolTokensMinted);
    }

    function removeFromPool(uint256 amountToRemove, uint256[] memory slippages) public {
        require(balanceOf(msg.sender) > 0, "You need to have pool tokens if you want to burn them!");
        require(amountToRemove > 0, "You can't remove 0 of your tokens!");
        require(amountToRemove <= balanceOf(msg.sender), "You can only remove 100% of your share!");
        require(_ownerIsSwapping == false, "The owner of this pool is currently swapping some tokens held by the pool. Please wait a few seconds");

        _burn(msg.sender, amountToRemove);   

        if(balanceOf(msg.sender) == 0){
            _totalHolders--;
        }   

        uint256 payout = 0;

        for(uint i = 0; i < _tokensHeld.length; i++){
            IERC20 tokenContract = IERC20(_tokensHeld[i]);
            uint256 percentageOfTokenToSwap = amountToRemove * 10**18 / (totalSupply() + amountToRemove);
            uint256 amountToSwap = tokenContract.balanceOf(address(this)) * percentageOfTokenToSwap / 10**18;
            if(_tokensHeld[i] != uniswapV2Router.WETH()){
                tokenContract.approve(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D, amountToSwap);
                address[] memory path = new address[](2);
                path[0] = _tokensHeld[i];
                path[1] = uniswapV2Router.WETH();
                uint256[] memory amountsOut = uniswapV2Router.getAmountsOut(amountToSwap, path);
                uint256[] memory amounts = uniswapV2Router.swapExactTokensForTokens(
                    amountToSwap,
                    amountsOut[1] * slippages[i] / 10**18,
                    path,
                    address(this),
                    block.timestamp
                );
                payout += amounts[1];
                if(tokenContract.balanceOf(address(this)) == 0){
                    uint256 tokenIndex = getIndexOfToken(_tokensHeld, _tokensHeld[i]);
                    _removeAddressFromTokensHeld(tokenIndex);
                }
            } else {
                payout += amountToSwap;
            }
        }
        

        uint256 ownerCut = payout * _poolFee / 10**18;
        uint256 factoryCut = payout * _factoryFee / 10**18;

        IERC20 WETHContract = IERC20(uniswapV2Router.WETH());   
        WETHContract.transfer(owner(), ownerCut);
        WETHContract.approve(_feeHandlerAddress, factoryCut);
        IFeeHandler(_feeHandlerAddress).collectFees(factoryCut);
        WETHContract.transfer(msg.sender, payout - ownerCut - factoryCut);
        
        emit poolRemovedFrom(msg.sender, amountToRemove, payout - ownerCut - factoryCut);
    }

    receive() payable external {}

    function swapTokensForTokens(address tokenIn, address tokenOut, uint256 tokenInAmount, uint256 minimumTokensOut) public {
        IERC20 tokenInContract = IERC20(tokenIn);
        require(tokenInContract.balanceOf(address(this)) >= tokenInAmount, "The pool does not have enough of these tokens to swap!");
        require(tokenInAmount != 0, "You cannot swap 0 tokens!");
        require(msg.sender == owner() || msg.sender == address(this), "You are not authorized to use this function!");

        tokenInContract.approve(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D, tokenInAmount);
        if(msg.sender == owner()){
            _ownerIsSwapping = true;
        }

        address[] memory path = getPath(tokenIn, tokenOut, uniswapV2Router);
        uint256 tokenOutBalanceBeforeSwap = IERC20(tokenOut).balanceOf(address(this));
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenInAmount,
            minimumTokensOut,
            path,
            address(this),
            block.timestamp
        );
        uint256 tokenOutBalanceAfterSwap = IERC20(tokenOut).balanceOf(address(this));
        if(tokenInContract.balanceOf(address(this)) == 0){
            uint256 indexOfToken = getIndexOfToken(_tokensHeld, tokenIn);
            _removeAddressFromTokensHeld(indexOfToken);
        }

        if(isTokenHeld(_tokensHeld, tokenOut) == false){
            _tokensHeld.push(tokenOut);
        }
        uint256 feeToPay = (tokenOutBalanceAfterSwap - tokenOutBalanceBeforeSwap) * 15000000000000000 / 10**18;

        address[] memory wethSwapPath = new address[](2);
        wethSwapPath[0] = tokenOut;
        wethSwapPath[1] = uniswapV2Router.WETH();

        uint256[] memory amountsOut = uniswapV2Router.getAmountsOut(feeToPay, wethSwapPath);
        
        uint256 wethBalanceBeforeSwap = IERC20(uniswapV2Router.WETH()).balanceOf(address(this));
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            feeToPay,
            amountsOut[1] * 500000000000000000 / 10**18,
            wethSwapPath,
            address(this),
            block.timestamp
        );
        uint256 wethBalanceAfterSwap = IERC20(uniswapV2Router.WETH()).balanceOf(address(this));
        IFeeHandler(_feeHandlerAddress).collectFees(wethBalanceAfterSwap - wethBalanceBeforeSwap);
        if(msg.sender == owner()){
            _ownerIsSwapping = false;
        }
        emit tokensSwapped(tokenIn, tokenOut, tokenInAmount, tokenOutBalanceAfterSwap - tokenOutBalanceBeforeSwap);
    }

}