pragma solidity ^0.8.11;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./IVOXLand721.sol";
import "./signing.sol";
import "hardhat/console.sol";

contract VOXLandAuction is Pausable, AccessControl, Signing {
    event AuctionCreated(
        uint256 indexed id,
        uint256 startTime,
        uint256 endTime,
        uint256 price
    );

    event AuctionEdited(
        uint256 indexed id,
        uint256 startTime,
        uint256 endTime,
        uint256 price,
        AuctionStatus status
    );

    event AuctionCancelled(uint256 indexed id);

    event Bid(address indexed bidder, uint256 indexed tokenId, uint256 price);

    event BidReturned(
        address indexed bidder,
        uint256 indexed tokenId,
        uint256 price
    );

    event Claimed(
        address indexed bidder,
        uint256 indexed tokenId,
        uint256 price
    );

    event Sale(address indexed bidder, uint256 indexed tokenId, uint256 amount);

    enum AuctionStatus {
        STARTED,
        ENDED,
        CANCELED
    }

    struct EnglishAuction {
        uint256 tokenId;
        uint256 startTime;
        uint256 endTime;
        uint256 startPrice;
        string uri;
        AuctionStatus status;
    }

    struct EnglishBid {
        address bidder;
        uint256 amount;
        bool claimed;
    }

    mapping(uint256 => bool) public soldPlots;
    mapping(address => bool) public allowList;
    mapping(uint256 => EnglishAuction) public auctions;
    mapping(uint256 => EnglishBid) public bids;
    uint256 public generalSalePrice;
    uint256 public generalSaleStartTime;
    IVOXLand721 public NFT;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    constructor(
        uint256 _generalSalePrice,
        uint256 _generalSaleStartTime,
        IVOXLand721 _nft,
        address _signer
    ) Signing(_signer) {
        generalSalePrice = _generalSalePrice;
        generalSaleStartTime = _generalSaleStartTime;
        NFT = IVOXLand721(_nft);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);
    }

    function setAdmin(address _admin) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _grantRole(ADMIN_ROLE, _admin);
    }

    function revokeAdmin(address _admin) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _revokeRole(ADMIN_ROLE, _admin);
    }

    function editAuction(EnglishAuction memory _auction)
        public
        onlyRole(ADMIN_ROLE)
    {
        auctions[_auction.tokenId] = _auction;
    }

    function isValidBid(address _bidder, bytes memory sig)
        public
        view
        returns (bool)
    {
        bytes32 message = keccak256(abi.encodePacked(_bidder));
        return recoverSigner(message, sig) == importantAddress;
    }

    function createEnglishAuction(
        uint256 _tokenId,
        uint256 startTime,
        uint256 endTime,
        uint256 startPrice,
        string calldata _uri
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        EnglishAuction memory englishAuction = EnglishAuction(
            _tokenId,
            startTime,
            endTime,
            startPrice,
            _uri,
            AuctionStatus.STARTED
        );
        auctions[_tokenId] = englishAuction;
        emit AuctionCreated(_tokenId, startTime, endTime, startPrice);
    }

    function createEnglishAuctions(EnglishAuction[] memory _auctions)
        public
        onlyRole(ADMIN_ROLE)
    {
        for (uint256 i = 0; i < _auctions.length; i++) {
            EnglishAuction memory englishAuction = EnglishAuction(
                _auctions[i].tokenId,
                _auctions[i].startTime,
                _auctions[i].endTime,
                _auctions[i].startPrice,
                _auctions[i].uri,
                AuctionStatus.STARTED
            );
            auctions[_auctions[i].tokenId] = englishAuction;
            emit AuctionCreated(
                _auctions[i].tokenId,
                _auctions[i].startTime,
                _auctions[i].endTime,
                _auctions[i].startPrice
            );
        }
    }

    function cancelAuction(uint256 _tokenId) public onlyRole(ADMIN_ROLE) {
        EnglishAuction memory auction = auctions[_tokenId];
        require(auction.startTime != 0, "Auction doesnt exist.");
        require(auction.endTime > block.timestamp, "Auction already ended");
        EnglishBid memory bid = bids[_tokenId];
        if (bid.amount > 0) {
            payable(bid.bidder).transfer(bid.amount);
            emit BidReturned(bid.bidder, _tokenId, bid.amount);
        }

        auctions[_tokenId].endTime = block.timestamp;
        auctions[_tokenId].status = AuctionStatus.CANCELED;

        emit AuctionCancelled(_tokenId);
    }

    function getBid(uint256 _tokenId) public view returns (EnglishBid memory) {
        return bids[_tokenId];
    }

    function setAuctionStart(uint256 _tokenId, uint256 _startTime)
        public
        onlyRole(ADMIN_ROLE)
    {
        EnglishAuction memory auction = auctions[_tokenId];
        require(auction.tokenId != 0, "Auction does not exist");
        require(auction.endTime > block.timestamp, "Auction already ended");
        auctions[_tokenId].startTime = _startTime;
        emit AuctionEdited(
            _tokenId,
            _startTime,
            auction.endTime,
            auction.startPrice,
            auction.status
        );
    }

    function setAuctionEnd(uint256 _tokenId, uint256 _endTime)
        public
        onlyRole(ADMIN_ROLE)
    {
        EnglishAuction memory auction = auctions[_tokenId];
        require(auction.tokenId != 0, "Auction does not exist");
        require(auction.endTime > block.timestamp, "Auction already ended");
        auctions[_tokenId].endTime = _endTime;
        emit AuctionEdited(
            _tokenId,
            auction.startTime,
            _endTime,
            auction.startPrice,
            auction.status
        );
    }

    function setGeneralSaleStart(uint256 _startTime)
        public
        onlyRole(ADMIN_ROLE)
    {
        generalSaleStartTime = _startTime;
    }

    function englishBid(
        address _bidder,
        uint256[] memory _tokenId,
        uint256[] memory amount,
        bytes memory sig
    ) public payable whenNotPaused {
        require(_tokenId.length == amount.length, "Invalid input");
        require(isValidBid(_bidder, sig), "Invalid signature");

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < _tokenId.length; i++) {
            totalAmount += amount[i];
        }

        require(msg.value == totalAmount, "Invalid amount");

        for (uint256 i = 0; i < _tokenId.length; i++) {
            require(
                auctions[_tokenId[i]].startTime != 0,
                "Auction does not exist"
            );

            require(
                block.timestamp >= auctions[_tokenId[i]].startTime,
                "Auction has not started"
            );

            require(
                auctions[_tokenId[i]].endTime > block.timestamp,
                "Auction has ended"
            );

            require(
                amount[i] >= auctions[_tokenId[i]].startPrice,
                "Bid amount is less than start price"
            );

            require(
                amount[i] > bids[_tokenId[i]].amount,
                "Bid amount is less than previous bid"
            );

            // If there's a previous bid, send the ammount back to the bidder who placed it.
            if (bids[_tokenId[i]].bidder != address(0)) {
                payable(bids[_tokenId[i]].bidder).transfer(
                    bids[_tokenId[i]].amount
                );
                emit BidReturned(
                    bids[_tokenId[i]].bidder,
                    _tokenId[i],
                    bids[_tokenId[i]].amount
                );
            }

            bids[_tokenId[i]] = EnglishBid(_bidder, amount[i], false);
            emit Bid(_bidder, _tokenId[i], amount[i]);
        }
    }

    function claimEnglishAuction(uint256 _tokenId) public whenNotPaused {
        require(auctions[_tokenId].tokenId != 0, "Auction does not exist");

        require(
            block.timestamp >= auctions[_tokenId].endTime,
            "Auction has not ended"
        );

        require(
            bids[_tokenId].bidder != address(0),
            "No bids for this auction"
        );

        require(bids[_tokenId].claimed == false, "Bid already claimed");

        bids[_tokenId].claimed = true;
        NFT.mintLand(bids[_tokenId].bidder, _tokenId, auctions[_tokenId].uri);
        emit Claimed(bids[_tokenId].bidder, _tokenId, bids[_tokenId].amount);
    }

    function generalSale(
        address _buyer,
        uint256[] memory _tokenId,
        string[] calldata _uri,
        bytes calldata _sig
    ) public payable whenNotPaused {
        require(_tokenId.length == _uri.length, "Invalid input");
        require(isValidBid(_buyer, _sig), "Invalid signature");

        require(
            msg.value == _tokenId.length * generalSalePrice,
            "Not enough eth for sale"
        );

        for (uint256 i = 0; i < _tokenId.length; i++) {
            require(
                block.timestamp >= generalSaleStartTime,
                "General sale has not started"
            );

            NFT.mintLand(_buyer, _tokenId[i], _uri[i]);
            emit Claimed(_buyer, _tokenId[0], msg.value);
        }
    }

    function withdrawl() public onlyRole(DEFAULT_ADMIN_ROLE) {
        payable(msg.sender).transfer(address(this).balance);
    }
}

