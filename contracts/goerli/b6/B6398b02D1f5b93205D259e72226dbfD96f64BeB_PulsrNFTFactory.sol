    // -----------------------------------------
    event TokenMinted(address indexed to, uint256 indexed id, uint256 amount);

    // -----------------------------------------
    // PulsrERC1155 Constructor
    // -----------------------------------------
    /**
     * @dev Initialize the contract by setting baseURI and owner address
     *
     * @param _owner Conntract owner
     * @param _baseURI Base URI for metadata
     * @param _tokenSupply Total supply of tokens
     */
    constructor(
        address _owner,
        string memory _baseURI,
        uint256 _tokenSupply
    ) ERC1155(_baseURI) {
        require(_tokenSupply > 0, "PulsrERC1155: Token supply must be greater than 0");
        tokenSupply = _tokenSupply;
        // transfer ownership
        _transferOwnership(_owner);
        // Use ERC2981 to set royalty receiver and fee
        _setDefaultRoyalty(_owner, ROYALTY_FEE);
    }

    // -----------------------------------------
