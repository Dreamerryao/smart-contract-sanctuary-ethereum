import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "hardhat/console.sol";

contract PulsrERC721A is ERC721A, ReentrancyGuard, Ownable, Pausable, ERC2981 {
    uint96 public constant ROYALTY_FEE = 500;

    string public baseURI;
    uint256 public immutable tokenSupply;

    // -----------------------------------------
    // PulsrERC721A Events
    // -----------------------------------------
    event TokenMinted(address indexed to, uint256 amount);

    // -----------------------------------------
    // PulsrERC721A Constructor
    // -----------------------------------------
    /**
     * @dev Initialize the contract by setting baseURI and owner address
     *
     * @param _owner Conntract owner
     * @param _name Token name
     * @param _symbol Token symbol
     * @param __baseURI Base URI for metadata
     * @param _tokenSupply Total supply of tokens
     */
    constructor(
        address _owner,
        string memory _name,
        string memory _symbol,
        string memory __baseURI,
        uint256 _tokenSupply
    ) ERC721A(_name, _symbol) {
        require(_tokenSupply > 0, "PulsrERC721A: Token supply must be greater than 0");
        require(bytes(__baseURI).length > 0, "PulsrERC721A: Invalid base URI");

        baseURI = __baseURI;
        tokenSupply = _tokenSupply;
        // transfer ownership
        _transferOwnership(_owner);
        // Use ERC2981 to set royalty receiver and fee
        _setDefaultRoyalty(_owner, ROYALTY_FEE);
    }

    // -----------------------------------------
    // PulsrERC721A Modifiers
    // -----------------------------------------

    modifier nonContract() {
        require(msg.sender.code.length <= 0, "PulsrERC721A: caller not a user");
        _;
    }

    // -----------------------------------------
    // PulsrERC721A Mutative Functions
    // -----------------------------------------

    /**
     * @dev The function is called by owner to airdrop tokens
     *
     * @param _amount The amount of tokens to mint
     */
    function mint(uint256 _amount) external nonContract nonReentrant {
        require(totalSupply() + _amount < tokenSupply, "PulsrERC721A: exceed total supply");

        _mint(msg.sender, _amount);

        emit TokenMinted(msg.sender, _amount);
    }

    // -----------------------------------------
    // PulsrERC721A Overriden Functions
    // -----------------------------------------

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, it can be overridden in child contracts.
     */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721A, ERC2981) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

