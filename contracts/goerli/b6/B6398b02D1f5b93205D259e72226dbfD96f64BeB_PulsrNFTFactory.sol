pragma solidity >=0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "./PulsrERC721A.sol";
import "./PulsrERC1155.sol";
import "hardhat/console.sol";

contract PulsrNFTFactory is
    Initializable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    EIP712Upgradeable
{
    string private constant SIGNING_DOMAIN = "PulsrNFTFactoryVoucher";
    string private constant SIGNATURE_VERSION = "1";

    // signer address
    address private signer;

    // PulsrNFTVoucher nonces
    mapping(address => uint256) public nonces;

    // deployed contracts
    mapping(address => DeployedNFT[]) public deployedContracts;
    DeployedNFT[] public deployedContractsArray;

    struct PulsrNFTVoucher {
        address owner;
        string name;
        string symbol;
        string baseURI;
        uint256 totalSupply;
        uint256 typeOfNFT; //  0: ERC721A collection 1: ERC1155 collection,
        uint256 nonce;
        uint256 expiry;
        bytes signature;
    }

    struct DeployedNFT {
        address owner;
        address nftAddress;
        uint256 typeOfNFT; //  0: ERC721A collection 1: ERC1155 collection,
    }

    // -----------------------------------------
    // PulsrNFTFactory Events
    // -----------------------------------------
    event SignerUpdated(address indexed signer);
    event NFTCreated(address indexed owner, address nftAddress, uint256 typeOfNFT);

    // -----------------------------------------
    // PulsrNFTFactory Initializer
    // -----------------------------------------

    /**
     * @dev Initializer function
     * @param _signer address of the signer
     */
    function initialize(address _signer) external initializer {
        __Context_init();
        __Ownable_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        __EIP712_init(SIGNING_DOMAIN, SIGNATURE_VERSION);

        require(_signer != address(0), "PulsrNFTFactory: Invalid signer address");

        signer = _signer;
    }

    // -----------------------------------------
    // PulsrNFTFactory Modifiers
    // -----------------------------------------

    modifier nonContract() {
        require(msg.sender.code.length <= 0, "PulsrNFTFactory: caller not a user");
        _;
    }

    // -----------------------------------------
    // PulsrNFTFactory Owner Functions
    // -----------------------------------------
    /**
     * @dev Set signer address
     * @param _signer Signer address
     */
    function setSigner(address _signer) external onlyOwner {
        require(_signer != address(0), "PulsrNFTFactory: Invalid signer address");
        signer = _signer;

        emit SignerUpdated(_signer);
    }

    // -----------------------------------------
    // PulsrNFTFactory Mutative Functions
    // -----------------------------------------

    /**
     * @dev Create an NFT
     * @param _voucher The PulsrNFTVoucher
     */
    function createNFT(PulsrNFTVoucher calldata _voucher) external nonContract {
        address _signer = _verify(_voucher);
        require(signer == _signer, "PulsrNFTFactory: invalid signature");
        require(msg.sender == _voucher.owner, "PulsrNFTFactory: invalid owner");
        require(_voucher.nonce == nonces[_voucher.owner], "PulsrNFTFactory: invalid nonce");
        require(_voucher.expiry == 0 || block.timestamp <= _voucher.expiry, "PulsrNFTFactory: signature is expired");
        require(_voucher.typeOfNFT >= 0 && _voucher.typeOfNFT < 2, "PulsrNFTFactory: invalid type of NFT");
        // check nft data for ERC721A
        require(
            _voucher.typeOfNFT == 0 &&
                bytes(_voucher.name).length > 0 &&
                bytes(_voucher.symbol).length > 0 &&
                bytes(_voucher.baseURI).length > 0 &&
                _voucher.totalSupply > 0,
            "PulsrNFTFactory: invalid NFT data"
        );
        // check nft data for ERC1155
        require(
            _voucher.typeOfNFT == 1 && bytes(_voucher.baseURI).length > 0 && _voucher.totalSupply > 0,
            "PulsrNFTFactory: invalid NFT data"
        );

        nonces[_voucher.owner]++;

        if (_voucher.typeOfNFT == 0) {
            // create a ERC721A NFT
            PulsrERC721A erc721A = new PulsrERC721A(
                _voucher.owner,
                _voucher.name,
                _voucher.symbol,
                _voucher.baseURI,
                _voucher.totalSupply
            );
            DeployedNFT memory deployedNFT = DeployedNFT(_voucher.owner, address(erc721A), _voucher.typeOfNFT);
            deployedContractsArray.push(deployedNFT);
            deployedContracts[_voucher.owner].push(deployedNFT);

            emit NFTCreated(_voucher.owner, address(erc721A), _voucher.typeOfNFT);
        } else if (_voucher.typeOfNFT == 1) {
            // create a ERC1155 NFT
            PulsrERC1155 erc1155 = new PulsrERC1155(_voucher.owner, _voucher.baseURI, _voucher.totalSupply);
            DeployedNFT memory deployedNFT = DeployedNFT(_voucher.owner, address(erc1155), _voucher.typeOfNFT);
            deployedContractsArray.push(deployedNFT);
            deployedContracts[_voucher.owner].push(deployedNFT);

            emit NFTCreated(_voucher.owner, address(erc1155), _voucher.typeOfNFT);
        }
    }

    /**
     * @dev return a hash of the givne PulsrNFTVoucher
     */
    function _hash(PulsrNFTVoucher calldata _voucher) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256(
                            "PulsrNFTVoucher(address owner,string name,string symbol,string baseURI,uint256 totalSupply,uint256 typeOfNFT,uint256 nonce,uint256 expiry)"
                        ),
                        _voucher.owner,
                        keccak256(bytes(_voucher.name)),
                        keccak256(bytes(_voucher.symbol)),
                        keccak256(bytes(_voucher.baseURI)),
                        _voucher.totalSupply,
                        _voucher.typeOfNFT,
                        _voucher.nonce,
                        _voucher.expiry
                    )
                )
            );
    }

    /**
     * @dev verify the signature of a given PulsrNFTVoucher
     * @param _voucher PulsrNFTVoucher
     */
    function _verify(PulsrNFTVoucher calldata _voucher) internal view returns (address) {
        bytes32 digest = _hash(_voucher);
        return ECDSAUpgradeable.recover(digest, _voucher.signature);
    }
}

