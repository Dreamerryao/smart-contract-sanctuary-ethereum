/// @title Contract Facet for Native Token Transaction
/// @author Ather Labs
/******************************************************************************/

import {AppStorage} from "../libraries/AppStorage.sol";
import {LibWithdrawBalance} from "../libraries/LibWithdrawBalance.sol";
import {LibSafeCast} from "../libraries/LibSafeCast.sol";
import {LibStatusControl} from "../libraries/LibStatusControl.sol";
import {IOperations} from "../interfaces/IOperations.sol";
import {IERC20} from "../interfaces/IERC20.sol";
import {IERC721} from "../interfaces/IERC721.sol";
import {IERC1155} from "../interfaces/IERC1155.sol";
import {ReentrancyGuard} from "../ReentrancyGuard.sol";

contract WithdrawFacet is IOperations, ReentrancyGuard {
    /* ------ EVENTS ------ */
    /// @notice
    event Withdrawn(bytes32 withdrawKey, uint128 amount);

    /* ------ EXTERNAL FUNTIONS ------ */
    /// @notice
    function withdrawPendingBalance(
        address _recipient,
        uint88 _tokenId,
        uint8 _listingId
    ) external nonReentrant {
        LibStatusControl._requireWithdrawEnable();

        bytes32 withdrawKey = bytes32(abi.encodePacked(_recipient, _listingId, _tokenId));
        uint128 balance = s.pendingWithdrawBalance[withdrawKey];
        require(balance > 0, "WF:invalid withdraw amount");

        uint128 releasedAmount = LibWithdrawBalance._releaseAsset(_recipient, _listingId, _tokenId, balance, true);
        s.pendingWithdrawBalance[withdrawKey] = balance - releasedAmount;
        emit Withdrawn(withdrawKey, releasedAmount);
    }

    /// @notice
    /// @return balance Witdrawable balance
    function getWithdrawableBalance(
        address _recipient,
        uint88 _tokenId,
        uint8 _listingId
    ) external view returns (uint128 balance) {
        return s.pendingWithdrawBalance[bytes32(abi.encodePacked(_recipient, _listingId, _tokenId))];
    }

    /// @notice
    /// @return balance Witdrawable balance
    function getPendingDepositRecordData(uint256 _requestId) external view returns (PendingDepositRecord memory) {
        return s.pendingDepositRecord[_requestId];
    }
}

