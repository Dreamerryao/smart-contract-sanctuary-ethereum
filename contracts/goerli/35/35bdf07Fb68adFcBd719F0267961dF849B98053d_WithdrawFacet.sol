import {LibAppStorage, AppStorage} from "./AppStorage.sol";
import {LibSafeCast} from "./LibSafeCast.sol";
import {LibTokenManagement} from "./LibTokenManagement.sol";
import {IERC20} from "../interfaces/IERC20.sol";
import {IERC721} from "../interfaces/IERC721.sol";
import {IERC1155} from "../interfaces/IERC1155.sol";
import "hardhat/console.sol";

library LibWithdrawBalance {
    event PendingWithdraw(bytes32 withdrawKey, uint128 amount);

    function _increasePendingBalance(
        address _to,
        uint8 _listingId,
        uint88 _tokenId,
        uint88 _amount
    ) internal {
        AppStorage storage s = LibAppStorage.diamondStorage();
        bytes32 withdrawKey = bytes32(abi.encodePacked(_to, _listingId, _tokenId));
        uint128 balance = s.pendingWithdrawBalance[withdrawKey];
        s.pendingWithdrawBalance[withdrawKey] = balance + _amount;
        emit PendingWithdraw(withdrawKey, _amount);
    }

    /// @notice
    function _transferERC20(
        IERC20 _tokenAddress,
        address _to,
        uint128 _amount,
        uint256 _minLiquidity
    ) internal returns (uint128) {
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this));
        _tokenAddress.transfer(_to, _amount);
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this));
        require(balanceAfter >= _minLiquidity, "LWB:Minimum liquidity limit is reached");
        uint128 balanceDiff = LibSafeCast.toUint128(balanceBefore - balanceAfter);
        require(balanceDiff == _amount, "LWB:invalid transfer amount");

        return balanceDiff;
    }

    function _transferERC721(
        IERC721 _tokenAddress,
        address _to,
        uint128 _tokenId
    ) internal returns (uint128) {
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this));
        _tokenAddress.transferFrom(address(this), _to, _tokenId);
        require(_tokenAddress.ownerOf(_tokenId) == _to, "LWB:not found transfered NFT");
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this));
        uint128 balanceDiff = LibSafeCast.toUint128(balanceBefore - balanceAfter);
        require(balanceDiff == 1, "LWB:invalid transfer amount");

        return balanceDiff;
    }

    function _transferERC1155(
        IERC1155 _tokenAddress,
        address _to,
        uint88 _tokenId,
        uint128 _amount,
        uint256 _minLiquidity
    ) internal returns (uint128) {
        uint256 balanceBefore = _tokenAddress.balanceOf(address(this), _tokenId);
        _tokenAddress.safeTransferFrom(address(this), _to, _tokenId, _amount, "");
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this), _tokenId);
        require(balanceAfter >= _minLiquidity, "LWB:Minimum liquidity limit is reached");
        uint128 balanceDiff = LibSafeCast.toUint128(balanceBefore - balanceAfter);
        require(balanceDiff == _amount, "LWB:invalid transfer amount");

        return balanceDiff;
    }

    function _releaseAsset(
        address _recipient,
        uint8 _listingId,
        uint88 _tokenId,
        uint128 _amount,
        bool _enableLimit
    ) internal returns (uint128 releasedAmount) {
        AppStorage storage s = LibAppStorage.diamondStorage();
        uint256 limit;
        if (_enableLimit) {
            limit = s.minLiquidity[_listingId];
        }
        if (_listingId == 0) {
            require(address(this).balance - _amount >= limit, "LWB:Minimum liquidity limit is reached");
            address payable toPayable = payable(_recipient);
            (bool success, ) = toPayable.call{value: _amount}("");
            require(success, "LWB:native token withdrawal failed");
            releasedAmount = _amount;
        } else {
            address tokenAddress = LibTokenManagement._validateLisitingId(_listingId);
            uint8 assetType = s.tokenTypes[_listingId];
            if (assetType == 1) {
                require(
                    IERC20(tokenAddress).balanceOf(address(this)) - _amount >= limit,
                    "LWB:Minimum liquidity limit is reached"
                );
                releasedAmount = LibWithdrawBalance._transferERC20(IERC20(tokenAddress), _recipient, _amount, limit);
            } else if (assetType == 2) {
                require(IERC721(tokenAddress).ownerOf(_tokenId) == address(this), "LWB:NFT not found");
                releasedAmount = LibWithdrawBalance._transferERC721(IERC721(tokenAddress), _recipient, _tokenId);
            } else if (assetType == 3 || assetType == 4) {
                require(
                    IERC1155(tokenAddress).balanceOf(address(this), _tokenId) - _amount >= limit,
                    "LWB:Minimum liquidity limit is reached"
                );
                releasedAmount = LibWithdrawBalance._transferERC1155(
                    IERC1155(tokenAddress),
                    _recipient,
                    _tokenId,
                    _amount,
                    limit
                );
            } else {
                revert("");
            }
        }
    }
}

