// File: Clarity.sol


pragma solidity ^0.8.7;




contract Clarity is ReentrancyGuard{
    address private owner;
    mapping(address => Score) private riskScore;
    mapping(address => address[]) private reqToDapp;
    event WalletRiskCheckRequest(address, string, address);
    event LogRequests(address, address, string, uint, bool);
    mapping(address => uint256) private balances;
    mapping(address => UsdtBalance) public balancesUsdt;
    address private tokenUSDTAddress;
    uint256 public priceUsdt = 1000000;
    uint256 public availableWithdraw;

    receive() external payable {
        balances[msg.sender] += msg.value;
        console.log(balances[msg.sender]);
    }

    fallback() external payable {}

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function getBalanceDeposited(address _addr) public view returns (uint256) {
        return balances[_addr];
    }

    //goerli 0x5b3303582159ef63e0d10637d001113decb279ec
    //mainnet 0xdac17f958d2ee523a2206206994597c13d831ec7

    constructor(address _usdt) {
        console.log("Deploying a Clarity contract");
        owner = msg.sender;
        tokenUSDTAddress = _usdt;
    }

    modifier _ownerOnly() {
        require(msg.sender == owner);
        console.log("This is owner");
        _;
    }

    struct Score {
        string network;
        uint value;
        bool isValid;
    }

    struct UsdtBalance {
        uint256 balance;
        uint64 lastTrackedBlock;
    }

    function changeOwner (address _address) public _ownerOnly {
        owner = _address;
        console.log("Owner changed");
    }

    function changePrice (uint256 _price) public _ownerOnly {
        priceUsdt = _price;
    }

    function updateBalance (address _from, uint256 _balance, uint64 _lastblock) public _ownerOnly {
        uint256 bal = (balancesUsdt[_from].balance) + _balance;
        balancesUsdt[_from] = UsdtBalance(bal, _lastblock);
    }

    function withdrawUsdt (address _to, uint256 val) public _ownerOnly {
        require((val <= availableWithdraw),"Not enough for withdraw");
        bytes4  SELECTOR_TRANSFER = bytes4(keccak256(bytes('transfer(address,uint256)')));
        (bool success, bytes memory data) = tokenUSDTAddress.call(abi.encodeWithSelector(SELECTOR_TRANSFER, _to, val));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'USDT transfer from sender failed');
    }

    function balanceOfUsdt() public view returns (uint256 value) {
        IERC20 erc20Usdt = IERC20(tokenUSDTAddress);
        return erc20Usdt.balanceOf(address(this));
    }

    function isOwner() public view returns(bool) {
        return msg.sender == owner;
    }

    function riskCheck(address _add, string calldata _network) public nonReentrant{
        emit WalletRiskCheckRequest(_add, _network, msg.sender);
        console.log("WalletRiskCheckRequest emited from ", msg.sender);
    }

    function getRiskScore(address _add, string calldata _network) public view returns (string memory, uint) {
        bool isGranted = false;
        for (uint i=0; i<reqToDapp[_add].length; i++) {
            if (reqToDapp[_add][i] == msg.sender) {
                isGranted = true;
                break;
            }
        }
        require(isGranted, "You are not paid for this request");
        if ((!riskScore[_add].isValid) || (keccak256(bytes(riskScore[_add].network)) != keccak256(bytes(_network)))) {
            return (_network,0);
        }
        return (riskScore[_add].network, riskScore[_add].value);
    }

    function storeRiskScore(address _add, address _dapp, string calldata _network, uint _value) public payable _ownerOnly nonReentrant {
        uint256 paidgas = tx.gasprice+1;
        uint256 startGas = gasleft();
        require(balances[_dapp] >= paidgas, "Not enough balance for transaction");
        require(balancesUsdt[_dapp].balance >= priceUsdt, "Not enough USDT for transaction");
        uint256 bal = (balancesUsdt[_dapp].balance) - priceUsdt;
        balancesUsdt[_dapp] = UsdtBalance(bal, balancesUsdt[_dapp].lastTrackedBlock);
        availableWithdraw += priceUsdt;
        riskScore[_add] = Score(_network, _value, true);
        (bool sent, ) = payable(msg.sender).call{value: paidgas}("");
        require(sent, "Failed to send Ether");
        uint256 gasUsed = startGas - gasleft();
        balances[_dapp] -= (paidgas + gasUsed);
        (bool success, ) = _dapp.call(
            abi.encodeWithSignature("storeRiskScore(address,string,uint256)", _add,_network, _value)
        );
        if (success) {
            //to dapp/wallet, req address, network, value, is dapp
            emit LogRequests(_dapp, _add, _network, _value, true);
        } else {
            emit LogRequests(_dapp, _add, _network, _value, false);
        }
        reqToDapp[_add].push(_dapp);
    }

    function storeRiskScoreTestMode(address _add, address _dapp, string memory _network, uint _value) public  _ownerOnly nonReentrant  {
        riskScore[_add] = Score(_network, _value, true);
        (bool success, ) = _dapp.call(
            abi.encodeWithSignature("storeRiskScore(address,string,uint256)", _add,_network, _value)
        );
        if (success) {
            //to dapp/wallet, req address, network, value, is dapp
            emit LogRequests(_dapp, _add, _network, _value, true);
        } else {
            emit LogRequests(_dapp, _add, _network, _value, false);
        }
        reqToDapp[_add].push(_dapp);
    }
}
