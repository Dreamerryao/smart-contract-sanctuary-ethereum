pragma solidity ^0.8.6;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import { IAngelDescriptor } from "./interfaces/IAngelDescriptor.sol";
import { NFTDescriptor } from "./libs/NFTDescriptor.sol";

contract AngelDescriptor is IAngelDescriptor, Ownable {
    using Strings for uint256;

    // prettier-ignore
    // https://creativecommons.org/publicdomain/zero/1.0/legalcode.txt
    bytes32 constant COPYRIGHT_CC0_1_0_UNIVERSAL_LICENSE = 0xa2010f343487d3f7618affe54f789f5487602331c0a8d03f49e9a7c547cf0499;

    // Whether or not `tokenURI` should be returned as a data URI (Default: true)
    bool public override isDataURIEnabled = true;

    // The URL of contract-level metadata
    string public override contractURI;
    // Image URL of an unopened mystery box
    string public override unrevealedImageURI;
    // Base URI
    string public override baseURI;
    // Artwork base URI
    string public override artworkBaseURI;
    // The base URL of NFT on the DApp
    //EX: https://angeldao.com/angel/${tokenId}
    string public override externalBaseURI;

    function tokenURI(
        uint256 tokenId,
        uint256 price,
        bool isRevealed
    ) external view override returns (string memory) {
        if (isDataURIEnabled) {
            if (!isRevealed) {
                return dataURI(tokenId, price, unrevealedImageURI);
            }
            return dataURI(tokenId, price, artworkURL(tokenId));
        }
        return string(abi.encodePacked(baseURI, tokenId.toString()));
    }

    function dataURI(
        uint256 tokenId,
        uint256 price,
        string memory imageUrl
    ) public view override returns (string memory) {
        string memory angelId = tokenId.toString();
        string memory name = string(abi.encodePacked("Angel ", angelId));
        string memory description = string(
            abi.encodePacked("Angel ", angelId, " is a member of the AngelDAO")
        );
        string memory priceString = price.toString();
        string memory externalURL = string(abi.encodePacked(externalBaseURI, tokenId.toString()));
        return genericDataURI(name, description, priceString, externalURL, imageUrl);
    }

    function genericDataURI(
        string memory name,
        string memory description,
        string memory price,
        string memory externalUrl,
        string memory imageUrl
    ) public pure override returns (string memory) {
        NFTDescriptor.TokenURIParams memory params = NFTDescriptor.TokenURIParams({
            name: name,
            description: description,
            price: price,
            externalUrl: externalUrl,
            imageUrl: imageUrl
        });
        return NFTDescriptor.constructTokenURI(params);
    }

    // ONLY FOR OWNER
    /**
     * @notice Toggle a boolean value which determines if `tokenURI` returns a data URI
     * or an HTTP URL.
     * @dev This can only be called by the owner.
     */
    function toggleDataURIEnabled() external override onlyOwner {
        bool enabled = !isDataURIEnabled;

        isDataURIEnabled = enabled;
        emit DataURIToggled(enabled);
    }

    /**
     * @notice Set the base URI for all token IDs. It is automatically
     * added as a prefix to the value returned in {tokenURI}, or to the
     * token ID if {tokenURI} is empty.
     * @dev This can only be called by the owner.
     */
    function setBaseURI(string calldata _baseURI) external override onlyOwner {
        baseURI = _baseURI;
        emit BaseURIUpdated(_baseURI);
    }

    function setContractURI(string memory _contractURI) external override onlyOwner {
        contractURI = _contractURI;
        emit ContractURIUpdated(contractURI);
    }

    function setUnrevealedImageURI(string memory _imageURI) external override onlyOwner {
        unrevealedImageURI = _imageURI;
        emit UnrevealedImageURIUpdated(_imageURI);
    }

    function setArtworkBaseURI(string memory _baseURI) external override onlyOwner {
        artworkBaseURI = _baseURI;
        emit ArtworkBaseURIUpdated(_baseURI);
    }

    /**
     * @notice Set the externalBaseURI
     * @dev Only callable by the contract owner.
     */
    function setExternalBaseURI(string memory _baseURI) external override onlyOwner {
        externalBaseURI = _baseURI;

        emit ExternalBaseURIUpdated(_baseURI);
    }

    // INTERNAL

    function artworkURL(uint256 tokenId) internal view returns (string memory) {
        return string(abi.encodePacked(artworkBaseURI, tokenId.toString()));
    }
}

