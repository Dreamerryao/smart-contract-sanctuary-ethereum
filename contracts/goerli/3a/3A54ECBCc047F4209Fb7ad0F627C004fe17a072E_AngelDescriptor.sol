    function isDataURIEnabled() external returns (bool);

    function contractURI() external returns (string memory);

    function unrevealedImageURI() external returns (string memory);

    function baseURI() external returns (string memory);

    function artworkBaseURI() external returns (string memory);

    function externalBaseURI() external returns (string memory);

    function setContractURI(string calldata contratURI) external;

    function setUnrevealedImageURI(string calldata imageURI) external;

    function setBaseURI(string calldata baseURI) external;

    function setArtworkBaseURI(string calldata artworkBaseURI) external;

