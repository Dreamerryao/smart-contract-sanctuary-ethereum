    pragma solidity ^0.8.0;




    contract Lender {
        using  SafeMath for uint256; // Safe math library for underflow/overflow value
        uint[] public listedNfts;    
        // ============ Immutable Variables ============
        uint256 public adminFeeInBasisPoints = 200; //Admin fee 2% of Owna
        uint256 public maximumExpiration = 72 hours;
        uint256 public oneDay = 180;
        address adminWallet;
        address secondWallet; // = 0x7B46F9a8A33849745E8D89D1621a5D0CD20A8d34;
        address tokenAddress;
        address nftAddress; // = 0x7B46F9a8A33849745E8D89D1621a5D0CD20A8d34;
        
        // ============ Mutable Variables ============
    
        // ============ Structs ============
    struct TimeDetail {
    uint256 durations;
    uint256 startTime;
    }
    struct LoanDetail {
            uint256 apr;
            uint256 minLoan;
            uint256 maxLoan; 
            uint256 loan; 
            uint256 acceptable_debt;
         
    }
        struct LoanOffer {
            uint256 offerID; 
            uint  offerType;  // 0 fixed ; 1 for flexible
            uint256 nftId;
            address lender;
            address borrower;
            string status;
            LoanDetail loanDetail;
            TimeDetail timeDetail;
        }
        // ============ Mappings ============
        // mapping (uint256=>Flexible) public flexibledLoanId;
    
        mapping(uint256=>bool) public isNftExists;
        mapping(uint256 => bool) public idToNft;
        
        // *********************
        mapping(address => mapping (uint256 => bool))public lenderOnNftId;
    
        mapping(uint256 => LoanOffer[]) public requestAgainstNft; 
        // *********************
    
        // ============ Events ============
        // event AdminFeeUpdated(uint256 newAdminFee);
        event fixedLoan(uint256 fixedId,uint256 durations,uint256 apr,uint256 minLoan,uint256 maxLoan,uint256 startTime,uint256 nftId,address lender);
        event flexibleLoan(uint256 flexibleId,uint256 apr,uint256 minLoan,uint256 maxLoan,uint256 acceptable_debt,uint256 startTime,uint256 nftId,address lender);
        event lenderRecivedFunds(address lender, uint256 nftID, uint256 offerID, uint256 maxLoan );
    
    
    function listedNft() public view returns( uint256[] memory) {
        return listedNfts;
    }
        function fixedLoanOffer( uint _durations ,uint _apr,uint _minLoan,uint _maxLoan,uint _nftId,address _borrower ) public  {
           
            require(IERC721(nftAddress).borrwerOf(_nftId)==_borrower, "please give valid borrower address");
            require(!idToNft[_nftId], "Borrower Already Accepted the offer at this Nft ID");
            require(!lenderOnNftId[msg.sender][_nftId], "Lender cannot again Offer on same nftId");
            require(_durations != 0,"Duration of loan zero no acceptable");
            require(_minLoan > 0,"Minimum loan should be greater 0");
            require(_maxLoan > _minLoan,"Maximum should be greater than minimum loan");
            require(_maxLoan.div(100).mul(_apr).div(365).mul(_durations.div(86400))<=_maxLoan.div(100).mul(25), 
            "your intrest is increasing 25% of total for selected duration. please select proper duration.");
            require(_apr > 0, "Apr Cannot be 0");
  
            LoanDetail memory loanDetail = LoanDetail({ apr:_apr,minLoan: _minLoan,maxLoan:_maxLoan,loan:0,acceptable_debt:0});  
            TimeDetail memory timeDetals = TimeDetail({durations:_durations,startTime:block.timestamp});  
        
            LoanOffer memory fix = LoanOffer(
                {
                offerID: requestAgainstNft[_nftId].length,
                offerType:0,
                nftId:_nftId,
                lender:msg.sender,
                borrower:_borrower,
                status:"Pending",
                timeDetail:timeDetals,loanDetail:loanDetail
            });   
       
            requestAgainstNft[_nftId].push(fix);
        
            lenderOnNftId[fix.lender][fix.nftId] = true;
        
            if(!isNftExists[fix.nftId]){
                // IERC721(nftAddress).transferFrom(_borrower, address(this), fix.nftId);
                isNftExists[fix.nftId] = true;   
                listedNfts.push(fix.nftId);
            }
        
            IERC20(tokenAddress).transferFrom(fix.lender,address(this),fix.loanDetail.maxLoan); 
            emit fixedLoan(fix.offerID, fix.timeDetail.durations, fix.loanDetail.apr, fix.loanDetail.minLoan, fix.loanDetail.maxLoan, fix.timeDetail.startTime, fix.nftId, fix.lender);
        }
    function flexibleLoanOffer( uint _durations,uint _apr,uint _minLoan,uint _maxLoan,uint _nftId,uint256 _acceptable_debt,address _borrower) public  {
     
            require(IERC721(nftAddress).borrwerOf(_nftId)==_borrower, "please give valid borrower address");
            require(!idToNft[_nftId], "Borrower Already Accepted the offer at this Nft ID");
            require(!lenderOnNftId[msg.sender][_nftId], "Lender cannot again Offer on same nftId");
            require(_acceptable_debt <= 25,"Maximum Acceptable Debt cannot be more than 23%" );
            require(_acceptable_debt >2,"Maximum Acceptable Debt cannot be 0%" );
            require(_durations != 0,"Duration of loan zero no acceptable");
            require(_minLoan> 0,"Minimum loan should be greater 0");
            require(_maxLoan> _minLoan,"Maximum should be greater than minimum loan");
            require(((_maxLoan.mul(_acceptable_debt.sub(2).mul(100)).div(10000)).mul(365))/(_maxLoan.mul(_apr.mul(100)).div(10000)) == _durations.div(86400), 
            "Please Select a valid duration for flexible offering ");  
            require(_apr > 0, "Apr Cannot be 0");
            LoanDetail memory loanDetail = LoanDetail({apr:_apr,minLoan: _minLoan,maxLoan:_maxLoan,loan:0,acceptable_debt:_acceptable_debt-2 });  
            TimeDetail memory timeDetals = TimeDetail({durations:_durations,startTime:block.timestamp});  
            LoanOffer memory fix = LoanOffer(
                {
                offerID: requestAgainstNft[_nftId].length,
                offerType: 1,
                nftId:_nftId,
                lender:msg.sender,
                borrower:_borrower,
                status:"Pending",
                timeDetail:timeDetals,loanDetail:loanDetail
                });    
     
            requestAgainstNft[_nftId].push(fix);
        
            lenderOnNftId[fix.lender][fix.nftId] = true;
        
            if(!isNftExists[fix.nftId]){
                // IERC721(nftAddress).transferFrom(_borrower, address(this), fix.nftId);
                isNftExists[fix.nftId] = true;  
                listedNfts.push(fix.nftId);  
            }
                    
            IERC20(tokenAddress).transferFrom(fix.lender,address(this),fix.loanDetail.maxLoan); 
            emit flexibleLoan(fix.offerID, fix.loanDetail.apr, fix.loanDetail.minLoan, fix.loanDetail.maxLoan, _acceptable_debt, fix.timeDetail.startTime, fix.nftId, fix.lender);
        }
    
    function lendersFunds (uint256 _nftID, uint256 _offerID) public {
            LoanOffer memory offer = requestAgainstNft[_nftID][_offerID];
            require(offer.lender!=address(0x0),"Offer not found");
            require(msg.sender == offer.lender,"Only Lender can Withdraw Funds");
            if(compareStrings(offer.status,"Rejected")){
                IERC20(tokenAddress).transfer(offer.lender,offer.loanDetail.maxLoan);
                delete requestAgainstNft[_nftID][_offerID];
            }
        
            else if(compareStrings(offer.status,"Pending")  && block.timestamp - offer.timeDetail.startTime >= 72 hours){
                IERC20(tokenAddress).transfer(offer.lender,offer.loanDetail.maxLoan);
                delete requestAgainstNft[_nftID][_offerID];
            }
        
            else{
                require(requestAgainstNft[_nftID][_offerID].loanDetail.maxLoan == 0, "Not eligible to with draw the fund");
            }
    emit lenderRecivedFunds(offer.lender, _nftID, _offerID, offer.loanDetail.maxLoan);
    }  
                
        function removeNftId(uint id ) internal {
        for(uint i =0;i<=listedNfts.length-1;i++){
            if(listedNfts[i]==id){
                listedNfts[i]=listedNfts[listedNfts.length-1];
                listedNfts.pop();
                break; 
            }
        }
        }

    function compareStrings(string memory a, string memory b) public pure returns (bool) {
            return keccak256(abi.encodePacked(a)) == keccak256(abi.encodePacked(b));
        }
 

    }

