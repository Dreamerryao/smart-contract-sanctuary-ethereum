pragma solidity ^0.8.0;






contract LSArtWorks721 is ERC721, ERC721Royalty, CantBeEvil {
  using Counters for Counters.Counter;
  Counters.Counter public counter;

  struct Sell {
    uint256 price;
    uint256 royalty;
    address receiver;
    address seller;
  }

  address public owner;

  uint256 public artWorkProjectId;

  uint96 public royaltyShare;

  IFundraising public fund;

  IDistributePool public distributePool;

  mapping(address => uint256) public mintableAmount;

  mapping(uint256 => Sell) public selling;

  constructor(
    address _owner,
    uint256 _projectId,
    uint96 _royaltyShare,
    address _fundContract,
    License.LicenseVersion license,
    address _distributePool
  ) ERC721("LSArtWorks", "LSA") CantBeEvil(license) {
    // factory = msg.sender;
    owner = _owner;
    artWorkProjectId = _projectId;
    fund = IFundraising(_fundContract);
    royaltyShare = _royaltyShare;
    distributePool = IDistributePool(_distributePool);
    _setDefaultRoyalty(_distributePool, royaltyShare);
  }

  /** ========== view functions ========== */
  function supportsInterface(bytes4 interfaceId)
    public
    view
    virtual
    override(CantBeEvil, ERC721Royalty, ERC721)
    returns (bool)
  {
    return interfaceId == type(IERC165).interfaceId;
  }

  function _burn(uint256 tokenId) internal virtual override(ERC721Royalty, ERC721) {
    super._burn(tokenId);
    _resetTokenRoyalty(tokenId);
  }

  function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    _requireMinted(tokenId);

    return "https://icloud.mypinata.cloud/ipfs/QmfC347aTxaUsQfjHeHNcAz6whs8d3R12PSAnmGr2Xt9fq";
  }

  function totalSupply() public view returns (uint256) {
    return counter.current();
  }

  /** ========== main functions ========== */

  function testMint() public {
      super._mint(msg.sender, counter.current());
      counter.increment();
  }

  function mint() external {
    uint256 mintAmount = mintableAmount[msg.sender];

    // check user can mint or not
    require(mintAmount != 0, "not available to mint");
    require(fund.checkProjectCompleted(artWorkProjectId), "project not completed");

    // update user whiteList
    mintableAmount[msg.sender] = 0;

    // mint works
    _mintArtWorks(msg.sender, mintAmount);
  }

  function recordForWhiteList(uint256 _mintAmount, address _receiver) external onlyFund {
    require(fund.checkProjectApproved(artWorkProjectId), "project need approval");
    mintableAmount[_receiver] += _mintAmount;

    emit WhiteListRecorded(artWorkProjectId, _receiver, _mintAmount);
  }

  function list(uint256 _tokenId, uint256 _price) external {
    require(!_exists(_tokenId), "ERC721: token already minted");
    require(ownerOf(_tokenId) == msg.sender, "do not belong to caller");

    // update
    Sell memory sell;
    sell.price = _price;
    sell.royalty = royaltyShare;
    sell.seller = msg.sender;
    sell.receiver = address(distributePool);
    selling[_tokenId] = sell;

    // transfer token to contract
    transferFrom(msg.sender, address(this), _tokenId);

    emit TokenListed(_tokenId, msg.sender, _price);
  }

  function unlist(uint256 _tokenId) external {
    Sell storage sell = selling[_tokenId];
    require(!_exists(_tokenId), "ERC721: token already minted");
    require(sell.price != 0, "token is not selling");

    // update
    delete selling[_tokenId];

    // return token to owner
    _transfer(address(this), msg.sender, _tokenId);
  }

  function buy(uint256 _tokenId) external payable {
    Sell memory sell = selling[_tokenId];
    require(!_exists(_tokenId), "ERC721: token already minted");
    require(sell.price != 0, "token price not set");
    require(msg.value >= sell.price, "not enough to purchase");

    // calculate royalty
    (address _distributePool, uint256 royalty) = royaltyInfo(_tokenId, sell.price);
    uint256 nftValue = msg.value - royalty;

    // transfer fund
    payable(_distributePool).transfer(royalty);
    payable(sell.receiver).transfer(nftValue);

    // clear price
    delete selling[_tokenId];

    // transfer token
    transferFrom(address(this), sell.receiver, _tokenId);
  }

  /** ========== internal functions ========== */
  function _mintArtWorks(address _receiver, uint256 _mintAmount) internal {
    for (uint256 i = 0; i < _mintAmount; i++) {
      counter.increment(); // incase token 0 utilized by others
      uint256 currentTokenId = counter.current();

      super._mint(_receiver, currentTokenId);

      emit ArtWorkMinted(_receiver, artWorkProjectId, currentTokenId);
    }
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal virtual override {
    require(selling[tokenId].price == 0, "token is listed");
    super._beforeTokenTransfer(from, to, tokenId);
  }

  /** =========== modifier ========== */
  modifier onlyFund() {
    require(msg.sender == address(fund), "only internal contract call");
    _;
  }

  /** ========== event ========== */
  event ArtWorkMinted(address indexed minter, uint256 indexed projectId, uint256 indexed tokenId);

  event WhiteListRecorded(uint256 indexed projectId, address indexed account, uint256 mintAmount);

  event TokenListed(uint256 indexed tokenId, address indexed seller, uint256 price);
}