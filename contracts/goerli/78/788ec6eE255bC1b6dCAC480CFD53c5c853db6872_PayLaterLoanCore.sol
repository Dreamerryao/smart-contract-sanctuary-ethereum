pragma solidity ^0.8.0;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
// import "./libraries/PayLaterLoanLibrary.sol";
// import "./libraries/RentalLibrary.sol";
import "./interfaces/IPayLaterFeeController.sol";
import "./interfaces/IPayLaterLoanCore.sol";
import "./interfaces/IGnosisSafe.sol";
import "hardhat/console.sol"; // remove or comment 

contract PayLaterLoanCore is AccessControl, Pausable, IPayLaterLoanCore {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    bytes32 public constant ORIGINATOR_ROLE = keccak256("ORIGINATOR_ROLE");
    bytes32 public constant REPAYER_ROLE = keccak256("REPAYER_ROLE");
    bytes32 public constant FEE_CLAIMER_ROLE = keccak256("FEE_CLAIMER_ROLE");

    Counters.Counter private loanIdTracker;
    Counters.Counter private rentalIdTracker;

    struct installment {
        uint256 amount;
        bool paid;
    }

    /** PayLater State */
    mapping(uint256 => PayLaterLoanLibrary.LoanData) private loans;
    // loandId => installment No => bool installmentPaymentStatus
    mapping(uint256 => mapping(uint256 => bool)) private installmentPaymentStatus;
    // loandId => installment No => price perInstallmentPrice
    mapping(uint256 => mapping(uint256 => uint256)) private perInstallmentPrice;
    // Under PayLater Loan nft address => bool status
    mapping(address => mapping(uint256 => bool)) private nftsUnderPayLaterLoan;

    /** Rental state */
    mapping(uint256 => RentalLibrary.RentalData) private rentals;
    // loandId => installment No => bool installmentPaymentStatus
    mapping(uint256 => mapping(uint256 => bool)) private rentalInstallmentPaymentStatus;
    // loandId => installment No => price perInstallmentPrice
    mapping(uint256 => mapping(uint256 => uint256)) private rentalPerInstallmentPrice;
    // Under PayLater Loan nft address => bool status
    mapping(address => mapping(uint256 => bool)) private nftsUnderRental;




    IPayLaterFeeController public  feeController; // add override

    // 10k bps per whole
    uint256 private constant BPS_DENOMINATOR = 10_000;

    constructor(IPayLaterFeeController _feeController) {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(FEE_CLAIMER_ROLE, _msgSender());
        // only those with FEE_CLAIMER_ROLE can update or grant FEE_CLAIMER_ROLE
        _setRoleAdmin(FEE_CLAIMER_ROLE, FEE_CLAIMER_ROLE);
        feeController = _feeController;

        // Avoid having loanId = 0
        loanIdTracker.increment();
        rentalIdTracker.increment();
    }

    /***
     * @inheritdoc ILoanCore
     */
    function getLoan(uint256 loanId) external view override returns (PayLaterLoanLibrary.LoanData memory loanData) {
        return loans[loanId];
    }

    function getFirstInstallmentOfPayLaterPurchase(uint256 loanId) external view override returns (uint256) {
        require(installmentPaymentStatus[loanId][0] == false, "First Installment already paid");
        return perInstallmentPrice[loanId][0];
    }

    function getInstallmentOfPayLaterPurchase(uint256 loanId) external view override returns (uint256, uint256) {
        PayLaterLoanLibrary.LoanData memory data = loans[loanId];
        for(uint256 i = 0; i< data.terms.parts; i++){
            if(installmentPaymentStatus[loanId][i] == false) {
                return (perInstallmentPrice[loanId][i], i);
            }
        }

        return (0,0);
    }

    function createPayLaterPurchase(
        PayLaterLoanLibrary.LoanTerms calldata loanTerms,
        address borrowerPrimaryAddress,
        address borrowerSecondaryAddress,
        address lenderPrimaryAddress,
        uint256 tokenType,
        address tokenAddress  
    ) external
      override
      whenNotPaused
      onlyRole(ORIGINATOR_ROLE)
      returns (uint256 payLaterLoanId) {

        require(loanTerms.durationSecs > 0, "PayLaterLoanCore::create: PayLaterPurchase is already expired");
        require(loanTerms.parts > 0, "PayLaterLoanCore::create: PayLaterPurchase payment cycle should be atleast 1");
        require(loanTerms.partduration > 0, "PayLaterLoanCore::create: PayLaterPurchase cycle duration should be greater than zero");

        payLaterLoanId = loanIdTracker.current();
        loanIdTracker.increment();

         

        loans[payLaterLoanId] = PayLaterLoanLibrary.LoanData(
            borrowerPrimaryAddress,
            borrowerSecondaryAddress,
            lenderPrimaryAddress,
            loanTerms,
            PayLaterLoanLibrary.LoanState.Created,
            block.timestamp + loanTerms.durationSecs,
            tokenAddress,
            tokenType
        );
        nftsUnderPayLaterLoan[tokenAddress][loanTerms.collateralTokenId] = true;
        
        for (uint i=0; i< loanTerms.parts; i++) {
            if(i == 0) {
                installmentPaymentStatus[payLaterLoanId][i] = false;
                perInstallmentPrice[payLaterLoanId][i] = getFirstInstallment(loanTerms.principal, loanTerms.interest, loanTerms.parts);
            } else {
                installmentPaymentStatus[payLaterLoanId][i] = false;
                perInstallmentPrice[payLaterLoanId][i] = getInstallment(loanTerms.principal, loanTerms.interest, loanTerms.parts);

            }
        }
        return payLaterLoanId;
      }

      function startPayLaterPurchase(uint256 loanId) external override whenNotPaused onlyRole(ORIGINATOR_ROLE) {
        
        PayLaterLoanLibrary.LoanData memory data = loans[loanId];
        
        require(data.state == PayLaterLoanLibrary.LoanState.Created, "PayLaterLoanCore::start: Invalid loan state");
        IERC20(data.terms.payableCurrency).safeTransferFrom(_msgSender(), address(this), perInstallmentPrice[loanId][0]);

        installmentPaymentStatus[loanId][0] = true;
        loans[loanId].state = PayLaterLoanLibrary.LoanState.Active;
        loans[loanId] = PayLaterLoanLibrary.LoanData(
            data.borrowerPrimaryWalletAddress,
            data.borrowerSecondaryWalletAddress,
            data.lenderPrimaryWalletAddress,
            data.terms,
            PayLaterLoanLibrary.LoanState.Active,
            data.dueDate,
            data.assetAddress, data.tokenType
        );

        address payable userSafeAddress = payable(data.borrowerSecondaryWalletAddress);
        IGnosisSafe(userSafeAddress).getApprovalForDefaultedNFTTransfer(data.assetAddress, data.terms.collateralTokenId, data.tokenType);
        IERC20(data.terms.payableCurrency).safeTransfer(data.lenderPrimaryWalletAddress, getPlatformLessFirstInstallment(loanId, data.terms.principal));
        emit LoanStarted(loanId, data.lenderPrimaryWalletAddress, data.borrowerPrimaryWalletAddress, data.borrowerSecondaryWalletAddress);

        if(data.terms.parts == 1) {
            loans[loanId].state = PayLaterLoanLibrary.LoanState.Repaid;
            nftsUnderPayLaterLoan[data.assetAddress][data.terms.collateralTokenId] = false;
            emit LoanRepaid(loanId);
        }
      }

    /**
     * Take a principal value and return the amount less protocol fees
     */
    function getPlatformFeesOnPrincipal(uint256 principal) internal view returns (uint256) {
        return principal.mul(feeController.getOriginationFee()).div(100);
    }

    function getPrincipalWithInterest(uint256 principal, uint256 interest) internal view returns (uint256) {
        return principal.add(principal.mul(interest).div(100));
    }

    function getFirstInstallment(uint256 principal, uint256 interest, uint256 parts) internal view returns (uint256) {
        uint256 totalPrincipalWithPlatformFee = getPlatformFeesOnPrincipal(principal).add(getPrincipalWithInterest(principal, interest).div(parts));
        console.log(totalPrincipalWithPlatformFee);
        return totalPrincipalWithPlatformFee;
    }

    function getInstallment(uint256 principal, uint256 interest, uint256 parts) internal view returns(uint256) {
        uint256 totalPrincipal = getPrincipalWithInterest(principal, interest);
        return totalPrincipal.div(parts);
    }

    function getPlatformLessFirstInstallment(uint256 loanId, uint256 principal) internal view returns(uint256) {

        return (perInstallmentPrice[loanId][0]).sub(getPlatformFeesOnPrincipal(principal));
    }

    function checkForNFTsPayLaterLoan(address assetAddress, uint256 token_id) external view override returns (bool) {
        return nftsUnderPayLaterLoan[assetAddress][token_id];
    } 

    function checkForNFTsUnderRental(address assetAddress, uint256 token_id) external view override returns (bool) {
       return nftsUnderRental[assetAddress][token_id];
    }

    function getFirstInstallmentWithoutLoan(uint256 amount, uint256 parts, uint256 interest) external view override returns (uint256) {
        return getFirstInstallment(amount, interest, parts);
    }

    /**
     * @dev Claim the protocol fees for the given token
     *
     * @param token The address of the ERC20 token to claim fees for
     *
     * Requirements:
     *
     * - Must be called by the owner of this contract
     */
    function claimFees(IERC20 token) external override onlyRole(FEE_CLAIMER_ROLE) {
        // any token balances remaining on this contract are fees owned by the protocol
        uint256 amount = token.balanceOf(address(this));
        token.safeTransfer(_msgSender(), amount);
        emit FeesClaimed(address(token), _msgSender(), amount);
    }

    /***
     * @inheritdoc ILoanCore
     */
    function repay(uint256 loanId, uint256 installmentAmount, uint256 index) external override onlyRole(REPAYER_ROLE) returns (bool) {
        PayLaterLoanLibrary.LoanData memory data = loans[loanId];
        // Ensure valid initial loan state
        require(data.state == PayLaterLoanLibrary.LoanState.Active, "PayLaterLoanCore::repay: Invalid loan state");

        // ensure repayment was valid
        IERC20(data.terms.payableCurrency).safeTransferFrom(_msgSender(), address(this), installmentAmount);

        installmentPaymentStatus[loanId][index] = true;
        IERC20(data.terms.payableCurrency).safeTransfer(data.lenderPrimaryWalletAddress, installmentAmount);

        for(uint256 i = 0; i< data.terms.parts; i++){
            if(installmentPaymentStatus[loanId][i] == false) {
                emit installmentRepaid(installmentAmount, loanId, index);
                return false;
            }
        }
        // state changes and cleanup
        // NOTE: these must be performed before assets are released to prevent reentrance
        loans[loanId].state = PayLaterLoanLibrary.LoanState.Repaid;
        nftsUnderPayLaterLoan[data.assetAddress][data.terms.collateralTokenId] = false;
        emit LoanRepaid(loanId);
        return true;
    }

    /***
     * @inheritdoc ILoanCore
     */
    function claim(uint256 loanId) external override whenNotPaused onlyRole(REPAYER_ROLE) {
        PayLaterLoanLibrary.LoanData memory data = loans[loanId];

        // Ensure valid initial loan state
        require(data.state == PayLaterLoanLibrary.LoanState.Active, "LoanCore::claim: Invalid loan state");

        require(data.dueDate < block.timestamp, "LoanCore::claim: Loan not expired");
        
        // NOTE: these must be performed before assets are released to prevent reentrance
        loans[loanId].state = PayLaterLoanLibrary.LoanState.Defaulted;

        // collateral redistribution
        // collateralToken.transferFrom(address(this), lender, data.terms.collateralTokenId);
        if(data.tokenType == 0) {
            IERC721(data.assetAddress).transferFrom(data.borrowerSecondaryWalletAddress, data.lenderPrimaryWalletAddress, data.terms.collateralTokenId);
        } else if(data.tokenType == 1){
            IERC1155(data.assetAddress).safeTransferFrom(data.borrowerSecondaryWalletAddress, data.lenderPrimaryWalletAddress, data.terms.collateralTokenId, 1, "0x0");
        }

        nftsUnderPayLaterLoan[data.assetAddress][data.terms.collateralTokenId] = false;
        emit LoanClaimed(loanId);
    }

    /** Renting */

    function getPlatformLessFirstRentalInstallment(uint256 loanId, uint256 principal) internal view returns(uint256) {
        return (rentalPerInstallmentPrice[loanId][0]).sub(getRentalPlatformFeesOnPrincipal(principal));
    }

    function getRentalInstallment(uint256 principal, uint256 parts) internal view returns(uint256) {
        return principal.div(parts);
    }

    function getRentalPlatformFeesOnPrincipal(uint256 principal) internal view returns (uint256) {
        return principal.mul(feeController.getRentalFee()).div(BPS_DENOMINATOR);
    }

    function getFirstRentalInstallment(uint256 principal, uint256 parts) internal view returns (uint256) {
        uint256 totalPrincipalWithPlatformFee = getRentalPlatformFeesOnPrincipal(principal).add(principal.div(parts));
        return totalPrincipalWithPlatformFee;
    }

    function getFirstRentalInstallmentWithoutLoan(uint256 amount, uint256 parts) external view override returns (uint256) {
        return getFirstRentalInstallment(amount, parts);
    }

    function getInstallmentOfRentalPurchase(uint256 loanId) external view override returns (uint256, uint256) {
        RentalLibrary.RentalData memory data = rentals[loanId];
        for(uint256 i = 0; i< data.terms.parts; i++){
            if(rentalInstallmentPaymentStatus[loanId][i] == false) {
                return (rentalPerInstallmentPrice[loanId][i], i);
            }
        }
        return (0,0);
    }

    function getFirstInstallmentOfRentalPurchase(uint256 loanId) external view override returns (uint256) {
        require(rentalInstallmentPaymentStatus[loanId][0] == false, "First Installment already paid");
        return rentalPerInstallmentPrice[loanId][0];
    }

    function getRental(uint256 loanId) external view override returns (RentalLibrary.RentalData memory loanData) {
        return rentals[loanId];
    }

    function createRentalRequest(
        RentalLibrary.RentalTerms calldata rentalTerms,
        address borrowerPrimaryAddress,
        address borrowerSecondaryAddress,
        address lenderPrimaryAddress,
        uint256 tokenType,
        address tokenAddress,
        uint256 rentalType
    ) external
      override
      whenNotPaused
      onlyRole(ORIGINATOR_ROLE)
      returns (uint256 rentalLoanId) {

        require(rentalTerms.durationSecs > 0, "RentalLoanCore::create: RentalPurchase is already expired");
        require(rentalTerms.parts > 0, "RentalLoanCore::create: RentalPurchase payment cycle should be atleast 1");
        require(rentalTerms.partduration > 0, "RentalLoanCore::create: RentalPurchase cycle duration should be greater than zero");

        rentalLoanId = rentalIdTracker.current();
        rentalIdTracker.increment();
        
        rentals[rentalLoanId] = RentalLibrary.RentalData(
            borrowerPrimaryAddress,
            borrowerSecondaryAddress,
            lenderPrimaryAddress,
            rentalTerms,
            RentalLibrary.LoanState.Created,
            block.timestamp + rentalTerms.durationSecs,
            tokenAddress,
            tokenType,
            rentalType
        );

        nftsUnderRental[tokenAddress][rentalTerms.collateralTokenId] = true;

        for (uint i=0; i< rentalTerms.parts; i++) {
            if(i == 0) {
                rentalInstallmentPaymentStatus[rentalLoanId][i] = false;
                rentalPerInstallmentPrice[rentalLoanId][i] = getFirstRentalInstallment(rentalTerms.principal, rentalTerms.parts);
            } else {
                rentalInstallmentPaymentStatus[rentalLoanId][i] = false;
                rentalPerInstallmentPrice[rentalLoanId][i] = getRentalInstallment(rentalTerms.principal, rentalTerms.parts);
            }
        }
        return rentalLoanId;
    }
    
    function startRentalPurchase(uint256 loanId) external override whenNotPaused onlyRole(ORIGINATOR_ROLE) {
        
        RentalLibrary.RentalData memory data = rentals[loanId];

        require(data.state == RentalLibrary.LoanState.Created, "RentalLoanCore::start: Invalid loan state");
        IERC20(data.terms.payableCurrency).safeTransferFrom(_msgSender(), address(this), rentalPerInstallmentPrice[loanId][0]);

        rentalInstallmentPaymentStatus[loanId][0] = true;
        rentals[loanId].state = RentalLibrary.LoanState.Active;
        rentals[loanId] = RentalLibrary.RentalData(
            data.borrowerPrimaryWalletAddress,
            data.borrowerSecondaryWalletAddress,
            data.lenderPrimaryWalletAddress,
            data.terms,
            RentalLibrary.LoanState.Active,
            data.dueDate,
            data.assetAddress, 
            data.tokenType,
            data.rentalType
        );

        address payable userSafeAddress = payable(data.borrowerSecondaryWalletAddress);
        IGnosisSafe(userSafeAddress).getApprovalForDefaultedNFTTransfer(data.assetAddress, data.terms.collateralTokenId, data.tokenType);
        IERC20(data.terms.payableCurrency).safeTransfer(data.lenderPrimaryWalletAddress, getPlatformLessFirstRentalInstallment(loanId, data.terms.principal));
        emit RentalStarted(loanId, data.lenderPrimaryWalletAddress, data.borrowerPrimaryWalletAddress, data.borrowerSecondaryWalletAddress);

        if(data.terms.parts == 1) {
            rentals[loanId].state = RentalLibrary.LoanState.Repaid;
            emit RentRepaid(loanId);
        }
    }

    function repayRent(uint256 loanId, uint256 installmentAmount, uint256 index) external override onlyRole(REPAYER_ROLE) returns (bool) {
        RentalLibrary.RentalData memory data = rentals[loanId];
        // Ensure valid initial loan state
        require(data.state == RentalLibrary.LoanState.Active, "RentalLoanCore::repay: Invalid loan state");
        require(data.terms.parts > 1, "RentalLoanCore:: Rent already paid");
        // ensure repayment was valid
        IERC20(data.terms.payableCurrency).safeTransferFrom(_msgSender(), address(this), installmentAmount);

        rentalInstallmentPaymentStatus[loanId][index] = true;
        IERC20(data.terms.payableCurrency).safeTransfer(data.lenderPrimaryWalletAddress, installmentAmount);

        for(uint256 i = 0; i< data.terms.parts; i++){
            if(rentalInstallmentPaymentStatus[loanId][i] == false) {
                emit rentalInstallmentRepaid(installmentAmount, loanId, index);
                return false;
            }
        }
        // state changes and cleanup
        // NOTE: these must be performed before assets are released to prevent reentrance
        rentals[loanId].state = RentalLibrary.LoanState.Repaid;
        emit RentRepaid(loanId);
        return true;
    }

    function claimRentalAsset(uint256 loanId) external override whenNotPaused onlyRole(REPAYER_ROLE) {
        RentalLibrary.RentalData memory data = rentals[loanId];

        // Ensure valid initial loan state
        require(data.state == RentalLibrary.LoanState.Active || data.state == RentalLibrary.LoanState.Repaid, "RentalLoanCore::claim: Invalid loan state");

        require(data.dueDate.add(feeController.getGracePeriod()) < block.timestamp, "RentalLoanCore::claim: Loan not expired");
        // NOTE: these must be performed before assets are released to prevent reentrance
        rentals[loanId].state = RentalLibrary.LoanState.Defaulted;

        // collateral redistribution
        // collateralToken.transferFrom(address(this), lender, data.terms.collateralTokenId);
        if(data.tokenType == 0) {
            IERC721(data.assetAddress).transferFrom(data.borrowerSecondaryWalletAddress, data.lenderPrimaryWalletAddress, data.terms.collateralTokenId);
        } else if(data.tokenType == 1){
            IERC1155(data.assetAddress).safeTransferFrom(data.borrowerSecondaryWalletAddress, data.lenderPrimaryWalletAddress, data.terms.collateralTokenId, 1, "0x0");
        }

        nftsUnderRental[data.assetAddress][data.terms.collateralTokenId] = false;
        emit RentalAssetClaimed(loanId);
    }



    // ADMIN FUNCTIONS

    /**
     * @dev Triggers stopped state.
     *
     * Requirements:
     *
     * - The contract must not be paused.
     */
    function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }

}

