pragma solidity ^0.8.8;
import "./IERC721_IERC2981.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

import "./IPaymentSplitter.sol";

import "hardhat/console.sol";




// interface IPaymentSplitter{
//     function release(address payable account) external; 
// }

// interface IERC2981{
//     function royaltyInfo(uint256 tokenId, uint256 salePrice) external returns (address, uint256);
// }

contract NFTMarketplace is ReentrancyGuard,Ownable {

    using Counters for Counters.Counter;
    Counters.Counter private itemCounter;
    // https://docs.chain.link/data-feeds/price-feeds/addresses
    // ETH/USD
    // AggregatorV3Interface internal priceFeed = AggregatorV3Interface(0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419);
    // MATIC/USD
    // AggregatorV3Interface internal priceFeed = AggregatorV3Interface(0x7bAC85A8a13A4BcD8abb3eB7d6b4d632c5a57676);

    AggregatorV3Interface internal priceFeed;

    // uint256 public priceDecimals = 2;
    uint256 public listingFeesBPS = 200; // 2%
    // uint256 public royaltyBPS = 1_000; // 10%
    // constructor(address _chainlink){
    //     priceFeed = AggregatorV3Interface(_chainlink);
    // }
    constructor(){}

    enum Status{Created, Release, Inactive}

    struct MarketItem{
        uint256 id;
        address nftContract;
        uint256 tokenId;
        address payable seller; // collection owner
        address payable buyer; // investor
        uint256 price; // price in USD, without decimal
        Status status;
    }
    mapping(uint256=>MarketItem) public marketItems;

    function getPriceFeedAddress()external view returns(address){
        return address(priceFeed);
    }

    function setPriceFeedAddress(address _chainlink)external{
        priceFeed = AggregatorV3Interface(_chainlink);
    }

    function createMarketItem(
        address _nftContract,
        uint256 _tokenId,
        uint256 price
    ) public nonReentrant{
        require(price>0,"Price must be at least 1 wei");
        require(IERC721_IERC2981(_nftContract).ownerOf(_tokenId) == msg.sender, "Only NFT owner can sell the NFT");
        require(IERC721_IERC2981(_nftContract).getApproved(_tokenId)==address(this),"NFT must be approved to market");
        itemCounter.increment();
        uint256 _id = itemCounter.current();
        marketItems[_id] = MarketItem(
            _id,
            _nftContract,
            _tokenId,
            payable(msg.sender),
            payable(address(0)),
            price,
            Status.Created
        );
        
    }

    function createMarketSale(
        uint256 _itemId
    ) public payable nonReentrant {
        MarketItem storage item = marketItems[_itemId];
        uint256 priceInWei = getRequiredPriceInWei(item.price);
        require(item.status == Status.Created,"marketItem alread sold");
        console.log("FUCK");
        console.log(msg.value,priceInWei);
        require(msg.value >= priceInWei, "Please submit the asking price");
        require(IERC721_IERC2981(item.nftContract).getApproved(item.tokenId) == address(this), "NFT must be approved to market");

        // Amount to Marketplace
        uint256 paymentToMarketplace = priceInWei*listingFeesBPS/10_000;

        // Amount to Royalty
        (address paymentSplitterAddress,uint256 royaltyAmount) = IERC721_IERC2981(item.nftContract).royaltyInfo(item.tokenId,priceInWei);
        
        // Amount to Collection Owner
        uint256 balance = priceInWei - paymentToMarketplace - royaltyAmount;

        // Release to paymentSplitter
        payable(paymentSplitterAddress).transfer(royaltyAmount);

        // Release royalty to Marketplace Owner and Collection Owner
        IPaymentSplitter(paymentSplitterAddress).release(payable(owner()));
        IPaymentSplitter(paymentSplitterAddress).release(item.seller);

        // Release balance to Collection Owner
        item.seller.transfer(balance);

        // Release balance to Marketplace Owner
        payable(owner()).transfer(paymentToMarketplace);

        // Transfer token to Investor
        IERC721_IERC2981(item.nftContract).transferFrom(item.seller,msg.sender,item.tokenId);

        // Update item 
        item.buyer = payable(msg.sender);
        item.status = Status.Release;

    }

    function getMarketItem(uint256 _itemId) public view returns(MarketItem memory){
        return marketItems[_itemId];
    }

    function deleteMarketItem(uint256 _itemId)public nonReentrant{
        require(_itemId<=itemCounter.current(),"itemId must <= itemCounter");
        require(marketItems[_itemId].status == Status.Created,"Item must be on market");
        MarketItem storage item = marketItems[_itemId];

        require(IERC721_IERC2981(item.nftContract).ownerOf(item.tokenId) == msg.sender,"must be the owner");
        require(IERC721_IERC2981(item.nftContract).getApproved(item.tokenId)==address(this),"NFT must be approved to market");

        item.status = Status.Inactive;
    }

    function getLatestPrice() public view returns (int256){
        (,int256 price,,,) = priceFeed.latestRoundData();
        return price;
    }

    function getDecimals() public view returns (uint8){
        return priceFeed.decimals();
    }
    // https://stackoverflow.com/questions/72992643/how-to-update-a-nft-mint-price-by-ethereum-price-in-usd
    function getRequiredPriceInWei(uint256 requiredPriceInUSD) public view returns (uint256){
        int256 eth_usd = getLatestPrice();
        uint256 eth_usd_price = uint256(eth_usd)*1e10;
        return (requiredPriceInUSD*1e36)/eth_usd_price;


    }
    /* ---------------------------------------------------------------------------------------------- */
    /* --------------------------------------- Helper Function -------------------------------------- */
    /* ---------------------------------------------------------------------------------------------- */
    // 1. Fetch all Listed items
    // 2. Fetch all my purchase items
    // 3. Fetch all my listed items
    function getAllListedItems()public view returns(MarketItem[] memory){
        MarketItem[] memory temp;
        uint256 j=0;
        for(uint256 i=0;i<itemCounter.current()+1;i++){
            if(marketItems[i].status == Status.Created){
                temp[j] = marketItems[i];
                j++;
            }
            
        }

        return temp;
    }

    function getAllMyPurchaseItems()public view returns(MarketItem[] memory){
       MarketItem[] memory temp;
        uint256 j=0;
        for(uint256 i=0;i<itemCounter.current()+1;i++){
            if(marketItems[i].buyer == msg.sender){
                temp[j] = marketItems[i];
                j++;
            }
            
        }

        return temp;
    }

    function getAllMyListedItems()public view returns(MarketItem[] memory){
       MarketItem[] memory temp;
        uint256 j=0;
        for(uint256 i=0;i<itemCounter.current()+1;i++){
            if(marketItems[i].seller == msg.sender && marketItems[i].status == Status.Created){
                temp[j] = marketItems[i];
                j++;
            }
            
        }

        return temp;
    }

    /* ---------------------------------------- selfdestruct ---------------------------------------- */
    function destroy()external onlyOwner{
        selfdestruct(payable(owner()));
    }



}

