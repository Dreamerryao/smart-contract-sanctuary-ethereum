                    if (
                        _traitsSplit[j] ==
                        traitIdToHash[_traitFiltersDecoded[i].traitId]
                    ) {
                        console.log("Excluded trait found on punk!");
                        return false;
                    }
                }
            }
        }

        return true;
    }
