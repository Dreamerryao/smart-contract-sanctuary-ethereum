/// @dev Thrown when the callback caller is not the host.
error Unauthorized();

/// @dev Thrown when the token being streamed to this contract is invalid
error InvalidToken();

/// @dev Thrown when the agreement is other than the Constant Flow Agreement V1
error InvalidAgreement();

/// @dev Thrown when the specified return token could not be transfered from the contract deployer to the contract
error ReturnTokenTransferFailure();

contract MockAqueductStream is SuperAppBase {
    /// @dev Super token that may be streamed to this contract
    ISuperToken internal immutable _acceptedToken;
    /// @dev Super token that may be streamed to this contract
    ISuperToken internal immutable _returnToken;

    ///@notice this is the superfluid host which is used in modifiers
    ISuperfluid immutable host;

    IConstantFlowAgreementV1 immutable cfa;

    /// @dev the deployer address of this contract
    address private immutable _owner;

    ///@dev ensures that only the owner can call functions where this is implemented
    modifier onlyOwner() {
        if (msg.sender != address(_owner)) revert Unauthorized();
        _;
    }

    ///@dev ensures that only the host can call functions where this is implemented
    //for usage in callbacks only
    modifier onlyHost() {
        if (msg.sender != address(host)) revert Unauthorized();
        _;
    }

    ///@dev checks whether the token and agreement class are the expected values
    ///@param superToken used when sending streams into contract to trigger callbacks
    ///@param agreementClass the address of the agreement which triggers callback
    modifier onlyExpected(ISuperToken superToken, address agreementClass) {
        if (superToken != _acceptedToken) revert InvalidToken();
        if (agreementClass != address(cfa)) revert InvalidAgreement();
        _;
    }

    constructor(
        ISuperToken acceptedToken,
        ISuperToken returnToken,
        ISuperfluid _host,
        IConstantFlowAgreementV1 _cfa
    ) {
        assert(address(_host) != address(0));
        assert(address(acceptedToken) != address(0));
        assert(address(returnToken) != address(0));

        _acceptedToken = acceptedToken;
        _returnToken = returnToken;
        host = _host;
        cfa = _cfa;
        _owner = msg.sender;

        // Registers Super App
        _host.registerApp(
            SuperAppDefinitions.TYPE_APP_FINAL |
                SuperAppDefinitions.BEFORE_AGREEMENT_CREATED_NOOP |
                SuperAppDefinitions.BEFORE_AGREEMENT_UPDATED_NOOP |
                SuperAppDefinitions.AFTER_AGREEMENT_UPDATED_NOOP |
                SuperAppDefinitions.BEFORE_AGREEMENT_TERMINATED_NOOP |
                SuperAppDefinitions.AFTER_AGREEMENT_TERMINATED_NOOP
        );
    }

    /// @dev withdraws the _returnToken contract balance tp to the owner
    function withdraw() external onlyOwner returns (bool) {
        uint256 amount = _returnToken.balanceOf(address(this));
        return _returnToken.transfer(_owner, amount);
    }

    /// @dev super app after agreement created callback
    function afterAgreementCreated(
        ISuperToken _superToken,
        bytes calldata _ctx,
        address _agreementClass,
        bytes32, //_agreementId,
        bytes calldata //_cbdata
    )
        external
        override
        onlyExpected(_superToken, _agreementClass)
        returns (bytes memory newCtx)
    {
        return _createMockSwap(_ctx);
    }

    /// @dev super app after agreement terminated callback
    function afterAgreementTerminated(
        ISuperToken _superToken,
        bytes calldata ctx,
        address _agreementClass,
        bytes32, // _agreementId,
        bytes calldata // _cbdata,
    ) external override onlyHost returns (bytes memory newCtx) {
        // According to the app basic law, we should never revert in a termination callback
        if (_superToken != _acceptedToken || _agreementClass != address(cfa)) {
            return ctx;
        }

        // get the current sender of the flow
        (, , address sender, , ) = host.decodeCtx(ctx);

        (newCtx, ) = host.callAgreementWithContext(
            cfa,
            abi.encodeCall(
                cfa.deleteFlow,
                (
                    _superToken,
                    sender,
                    address(this),
                    new bytes(0) // placeholder
                )
            ),
            ctx
        );
    }

    function _createMockSwap(bytes calldata ctx)
        private
        returns (bytes memory newCtx)
    {
        newCtx = ctx;

        // get the current sender of the flow
        (, , address sender, , ) = host.decodeCtx(ctx);

        (, int96 flowRate, , ) = cfa.getFlow(
            _acceptedToken,
            sender,
            address(this)
        );

        // return the the "opposite" token
        (newCtx, ) = host.callAgreementWithContext(
            cfa,
            abi.encodeCall(
                cfa.createFlow,
                (
                    _returnToken,
                    sender,
                    flowRate,
                    new bytes(0) // placeholder
                )
            ),
            ctx
        );
    }
}