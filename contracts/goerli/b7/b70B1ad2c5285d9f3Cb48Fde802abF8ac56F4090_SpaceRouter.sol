pragma solidity 0.8.17;
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./helpers.sol";

contract SpaceCoin is ERC20, Helpers {
    address public immutable owner;
    address public immutable treasury;

    /// Address of the ICO contract
    address private immutable icoAddress;
    bool public taxActive = true;

    constructor(
        string memory _name,
        string memory _symbol,
        address _owner,
        address _treasury,
        address _icoAddress
    ) ERC20(_name, _symbol) {
        owner = _owner;
        treasury = _treasury;
        icoAddress = _icoAddress;
        _mint(treasury, toDecimals(350_000));
        _mint(_icoAddress, toDecimals(150_000));
    }

    /// @notice Allows a user to transfer their tokens, collecting a 2% tax that is sent to the treasury
    /// @notice Taxes are not collected for the initial redemption, or if the contract owner has turned tax collection off
    /// @dev overwrites the OpenZeppelin ERC20 transfer function to allow for tax collection
    /// @param to The address being sent the tokens
    /// @param amount The number of tokens being transferred
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        uint256 tax;
        if (msg.sender == icoAddress || !taxActive) {
            tax = 0;
        } else {
            tax = (amount * 2) / 100;
        }
        super._transfer(from, to, amount - tax);
        super._transfer(from, treasury, tax);
    }

    // It might be possible to abstract this into helpers & combine with toggling fundraising and contributions
    /// Allows the owner to pause and resume tax collection at any time
    /// @param newState The boolean value representing the new state. false = off, true = on
    function taxOn(bool newState) external onlyOwner(owner) {
        if (newState == taxActive)
            revert Unauthorized("Cannot toggle to same state");
        taxActive = newState;
        if (taxActive) emit Resuming("Tax Collection");
        else emit Pausing("Tax Collection");
    }

    /// Utility function to convert given value to the correct decmial value
    /// @param amount Human readable amount
    function toDecimals(uint256 amount) private view returns (uint256) {
        return amount * 10 ** uint256(decimals());
    }
}

