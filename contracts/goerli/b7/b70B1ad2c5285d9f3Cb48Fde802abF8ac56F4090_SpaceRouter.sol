pragma solidity ^0.8.9;

import "./SpaceLP.sol";
import "./SpaceCoin.sol";

contract SpaceRouter {
    SpaceLP public immutable spaceLP;
    SpaceCoin public immutable spaceCoin;
    bool locked;

    // ------ EVENTS & ERRORS ------
    event LiquidityAdded(
        address indexed trader,
        uint256 amtEth,
        uint256 amtSpc
    );
    event EthRefundSent(address indexed trader, uint256 refundAmt);
    event LiquidityRemoved(address indexed trader, uint256 amtLP);
    event TokenExchanged(
        address indexed trader,
        bytes3 typeIn,
        uint256 amtIn,
        uint256 amtOut
    );

    error InsufficientLiquidity();
    error DepositFailed();
    error WithdrawalFailed();
    error RefundFailed();
    error MinNotReached();

    constructor(SpaceLP _spaceLP, SpaceCoin _spaceCoin) {
        spaceLP = _spaceLP;
        spaceCoin = _spaceCoin;
    }

    /// @notice Provides ETH-SPC liquidity to LP contract
    /// @param spc The amount of SPC to be deposited
    function addLiquidity(uint256 spc) external payable lock {
        uint256 eth = msg.value;
        if (spc == 0 || eth == 0) revert InsufficientLiquidity();
        (
            uint256 spcToDeposit,
            uint256 ethToDeposit
        ) = determineLiquidityToDeposit(spc, eth);

        bool spcTransferSuccess = spaceCoin.transferFrom(
            msg.sender,
            address(spaceLP),
            spcToDeposit
        );
        bool depositSuccess = spaceLP.deposit{value: ethToDeposit}(msg.sender);

        if (!spcTransferSuccess || !depositSuccess) revert DepositFailed();
        emit LiquidityAdded(msg.sender, ethToDeposit, spcToDeposit);
        /// @dev any ETH sent to the router in excess of that transferred to the liquidity pool should be refunded to the trader
        uint256 ethToRefund = eth - ethToDeposit;
        if (ethToRefund > 0) {
            (bool success, ) = payable(msg.sender).call{value: ethToRefund}("");
            if (!success) revert RefundFailed();
            emit EthRefundSent(msg.sender, ethToRefund);
        }
    }

    /// @notice Removes ETH-SPC liquidity from LP contract
    /// @param lpToken The amount of LP tokens being returned
    function removeLiquidity(uint256 lpToken) external lock {
        if (lpToken == 0) revert InsufficientLiquidity();
        bool lpTransferSuccess = spaceLP.transferFrom(
            msg.sender,
            address(spaceLP),
            lpToken
        );
        bool withdrawalSuccess = spaceLP.withdraw(msg.sender);
        if (!lpTransferSuccess || !withdrawalSuccess) revert WithdrawalFailed();
        emit LiquidityRemoved(msg.sender, lpToken);
    }

    /// @notice Swaps ETH for SPC in LP contract
    /// @param spcOutMin The minimum acceptable amout of SPC to be received
    function swapETHForSPC(uint256 spcOutMin) external payable lock {
        uint256 amtOut = spaceLP.swap{value: msg.value}(msg.sender, true);
        if (amtOut < spcOutMin) revert MinNotReached();
        emit TokenExchanged(msg.sender, "ETH", msg.value, amtOut);
    }

    /// @notice Swaps SPC for ETH in LP contract
    /// @param spcIn The amount of inbound SPC to be swapped
    /// @param ethOutMin The minimum acceptable amount of ETH to be received
    function swapSPCForETH(uint256 spcIn, uint256 ethOutMin) external lock {
        // transfer spc to spaceLP
        bool success = spaceCoin.transferFrom(
            msg.sender,
            address(spaceLP),
            spcIn
        );
        if (!success) revert DepositFailed();
        uint256 amtOut = spaceLP.swap(msg.sender, false);
        if (amtOut < ethOutMin) revert MinNotReached();
        emit TokenExchanged(msg.sender, "SPC", spcIn, amtOut);
    }

    // ------ PRIVATE & INTERNAL ------
    /// @notice Calculates the optimal (max) amount of SPC and ETH to deposit based on the users desired amounts and the current ratio of reserves in the liquidity pool.
    /// @dev Also accounts for the SPC transfer tax, relying on a hard-coded value of 2%
    function determineLiquidityToDeposit(
        uint256 spc,
        uint256 eth
    ) private view returns (uint256 spcToDeposit, uint256 ethToDeposit) {
        // Get the current reserves of SPC and ETH in the liquidity pool
        (uint256 spcReserves, uint256 ethReserves) = spaceLP.getReserves();

        /// @dev If the SPC tax is on, only 98% of the given SPC will be transferred to the pool
        uint8 spcTaxAdjustment = spaceCoin.taxActive() ? 98 : 100;

        /// @dev If either or both of the reserves are 0, this deposit will set the initial ratio, so send the full amount of both
        if (spcReserves == 0 || ethReserves == 0) {
            spcToDeposit = spc;
            ethToDeposit = eth;
            return (spcToDeposit, ethToDeposit);
        }
        /// @dev Otherwise, calculate how much of each to send
        uint256 spcForGivenEth = (eth * spcReserves * 100) /
            (ethReserves * spcTaxAdjustment);
        if (spcForGivenEth <= spc) {
            spcToDeposit = spcForGivenEth;
            ethToDeposit = eth;
        } else {
            ethToDeposit =
                (ethReserves * spc * spcTaxAdjustment) /
                (spcReserves * 100);
            spcToDeposit = spc;
            assert(ethToDeposit < eth);
        }
    }

    modifier lock() {
        locked = true;
        _;
        locked = false;
    }
}

