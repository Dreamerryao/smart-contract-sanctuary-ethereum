    /// @param amount The amount that was contributed
    event ContributionMade(address investor, uint256 amount);

    /// Event emitted once the total contribution limit has been reached for a given ICO phase
    /// Seed: 15000 ETH
    /// General & Open: 30000 ETH
    /// @param phase The current ICO phase
    /// @param limit The limit of the current ICO phase
    event PhaseLimitReached(Phases phase, uint256 limit);

    /// Event emitted when an investor redeems their tokens
    event TokensClaimed();

    /// Event emitted when the owner pauses fundraising, token redemption, or token transfer tax collection
    /// @param action The action that is being paused
    event Pausing(string action);

    /// Event emitted when the owner resumes fundraising, token redemption, or token transfer tax collection
    /// @param action The action that is being resumed
    event Resuming(string action);

    /// Event emitted when the owner withdraws all funds raised to date to the treasury address
    /// @param treasury the address of the treasury
