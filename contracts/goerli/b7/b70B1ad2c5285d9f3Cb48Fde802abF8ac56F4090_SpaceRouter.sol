contract SpaceLP is ERC20 {
    bool locked;
    SpaceCoin public immutable spaceCoin;
    Reserves private reserves;
    struct Reserves {
        uint256 spc;
        uint256 eth;
    }

    // ------ EVENTS & ERRORS ------
    event LiquidityDeposited(address indexed addr, uint256 newLiquidity);
    event LiquidityWithdrawn(address indexed addr, uint256 withdrawnLiquidity);
    event SwapMade(
        address indexed addr,
        bytes3 typeIn,
        uint256 amtIn,
        uint256 amtOut
    );
    error InsufficientLiquidity();
    error TransactionFailed();

    constructor(SpaceCoin _spaceCoin) ERC20("SpaceLP", "SLP") {
        spaceCoin = _spaceCoin;
    }

    // Allow direct contributions of ETH; primarily for testing

    /// @notice Adds ETH-SPC liquidity to LP contract
    /// @param to The address that will receive the LP tokens
    function deposit(address to) external payable lock returns (bool) {
        uint256 currLiquidity = totalSupply();
        (uint256 spcReserves, uint256 ethReserves) = getReserves();
        (uint256 spcBalance, uint256 ethBalance) = getBalances();

        /// @dev Assumes all additional tokens were added by the "to" address, even if they were force-fed by someone else
        uint256 newSpc = spcBalance - spcReserves;
        uint256 newEth = ethBalance - ethReserves;

        /// @dev Immediately revert to save gas if there was no increase in one of the tokens.
        if (newSpc == 0 || newEth == 0) revert InsufficientLiquidity();

        uint256 newLiquidity;
        if (currLiquidity == 0) {
            newLiquidity = (newSpc + newEth) / 2;
        } else {
            /// @dev If there is existing liqudity, there should be no way for either SPC or ETH reserves to be 0; just in case though, this assert prevents dividing by 0
            assert(spcReserves != 0 && ethReserves != 0);
            uint256 newSPCLiq = (newSpc * currLiquidity) / spcReserves;
            uint256 newEthLiq = (newEth * currLiquidity) / ethReserves;

            /// @dev Get the smaller in case tokens were not added in the correct proportions
            newLiquidity = newSPCLiq < newEthLiq ? newSPCLiq : newEthLiq;
        }

        // Updates reserves to equal current balance of eth & spc
        updateReserves(spcBalance, ethBalance);

        emit LiquidityDeposited(to, newLiquidity);
        _mint(to, newLiquidity);
        return true;
    }

    /// @notice Returns ETH-SPC liquidity to liquidity provider
    /// @param to The address that will receive the outbound token pair
    function withdraw(address to) external lock returns (bool) {
        (uint256 spcReserves, uint256 ethReserves) = getReserves();
        (uint256 spcBalance, uint256 ethBalance) = getBalances();

        /// @dev Assume any LP tokens owned by the liquidity pool were sent by the "to" address, even if they were force-fed by someone else
        uint256 liquidityRequested = balanceOf(address(this));

        /// @dev Immediately revert to save gas
        if (liquidityRequested == 0) revert InsufficientLiquidity();

        uint256 spcOut = (spcReserves * liquidityRequested) / totalSupply();
        uint256 ethOut = (ethReserves * liquidityRequested) / totalSupply();

        uint256 newSpcReserves = spcBalance - spcOut;
        uint256 newEthReserves = ethBalance - ethOut;
        updateReserves(newSpcReserves, newEthReserves);

        emit LiquidityWithdrawn(to, liquidityRequested);
        _burn(address(this), liquidityRequested);
        bool spcSuccess = spaceCoin.transfer(to, spcOut);
        (bool ethSuccess, ) = payable(to).call{value: ethOut}("");
        if (!spcSuccess || !ethSuccess) revert TransactionFailed();

        /// @dev Make sure no funny business happened during the call to spaceCoin.transfer
        // This is probably unnecessary (esp. with the lock), but I haven't convinced myself yet
        (uint256 updtSpcReserves, uint256 updtEthReserves) = getReserves();
        (uint256 updtSpcBalance, uint256 updtEthBalance) = getBalances();
        assert(
            updtSpcReserves == updtSpcBalance &&
                updtEthReserves == updtEthBalance
