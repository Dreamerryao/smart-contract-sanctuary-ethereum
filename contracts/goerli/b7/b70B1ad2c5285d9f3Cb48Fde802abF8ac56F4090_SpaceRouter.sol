        return true;
    }

    /// @notice Swaps ETH for SPC, or SPC for ETH
    /// @param to The address that will receive the outbound SPC or ETH
    /// @param isETHtoSPC Boolean indicating the direction of the trade
    function swap(
        address to,
        bool isETHtoSPC
    ) external payable lock returns (uint256 amtOut) {
        (uint256 spcReserves, uint256 ethReserves) = getReserves();
        uint256 k = spcReserves * ethReserves;

        (uint256 spcBalance, uint256 ethBalance) = getBalances();

        /// @dev Uses a helper function that uses the 1% fee to calculate the amount to transfer to the "to" address
        bool success;
        if (isETHtoSPC) {
            amtOut = calculateAmtOut(ethBalance, ethReserves, spcReserves, k);
            updateReserves(spcBalance - amtOut, ethBalance);

            emit SwapMade(to, "ETH", ethBalance - ethReserves, amtOut);
            success = spaceCoin.transfer(to, amtOut);
        } else {
            amtOut = calculateAmtOut(spcBalance, spcReserves, ethReserves, k);
            updateReserves(spcBalance, ethBalance - amtOut);

            emit SwapMade(to, "SPC", spcBalance - spcReserves, amtOut);
            (success, ) = payable(to).call{value: amtOut}("");
        }
        if (!success) revert TransactionFailed();

        /// @dev Make sure no funny business happened during the call to spaceCoin.transfer
        // This is probably unnecessary (esp. with the lock), but I haven't convinced myself yet
        (uint256 updtSpcReserves, uint256 updtEthReserves) = getReserves();
        (uint256 updtSpcBalance, uint256 updtEthBalance) = getBalances();
        assert(
            updtSpcReserves == updtSpcBalance &&
                updtEthReserves == updtEthBalance
        );

        /// @dev Return value is used by the spaceRouter to ensure the amount out meets their minimum
        return amtOut;
    }

    modifier lock() {
        locked = true;
        _;
        locked = false;
    }

    // ------ UTILITY FUNCTIONS ------

    /// @notice Updates the calculated reserves of SPC and ETH
    /// @dev Rather than just using the current balances for each
    function updateReserves(
        uint256 newSpcReserve,
        uint256 newEthReserve
    ) private {
        reserves.spc = newSpcReserve;
        reserves.eth = newEthReserve;
    }

    /// @notice Returns the current reserves of SPC and ETH
    /// @dev Getter function that returns the reserves of both tokens in one call
    function getReserves()
        public
        view
        returns (uint256 spcReserves, uint ethReserves)
    {
        spcReserves = reserves.spc;
        ethReserves = reserves.eth;
    }

    /// @notice Returns the current balances of SPC and ETH on the contract
    /// @dev Getter function that returns the balances of both tokens in one call
    function getBalances()
        private
        view
        returns (uint256 spcBalance, uint ethBalance)
    {
        spcBalance = spaceCoin.balanceOf(address(this));
        ethBalance = address(this).balance;
    }

    /// @notice Calculates how many ETH or SPC to send back to the user
    /// @dev Incorporates the 1% and current value of k
    function calculateAmtOut(
        uint256 balanceIn,
        uint256 reservesIn,
        uint256 reservesOut,
        uint256 k
    ) private pure returns (uint256 amtOut) {
        uint256 amtInLessFee = (((balanceIn - reservesIn)) * 99) / 100;
        amtOut = reservesOut - k / (reservesIn + amtInLessFee);
    }
}

