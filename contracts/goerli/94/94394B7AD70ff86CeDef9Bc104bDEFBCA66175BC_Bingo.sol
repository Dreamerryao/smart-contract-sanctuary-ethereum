pragma solidity >=0.8.4;

import "hardhat/console.sol";

contract Bingo {
    // protocol owner
    address public immutable owner;

    uint256 public gameCounter;
    uint256 public activeGameId;

    mapping(address => bool) public hosts;
    mapping(uint256 => Game) public games;

    mapping(uint256 => mapping(address => uint256)) public cards;
    mapping(uint256 => uint256[]) public numbers;
    mapping(uint256 => mapping(uint256 => bool)) public duplicateNumbers;

    struct Game {
        address host;
        uint256 cardPrice;
        uint256 maxCardsPerPlayer;
        uint256 totalPot;
        uint256 soldCards;
        GameState state;
        address winner;
    }

    enum GameState {
        INVALID,
        CREATED,
        STARTED,
        ENDED
    }

    modifier onlyOwner() {
        require(msg.sender == owner, "ONLY_OWNER");
        _;
    }

    modifier onlyHost(uint256 gameId) {
        require(hosts[msg.sender], "ONLY_HOSTS");
        require(gameId == 0 || games[gameId].host == msg.sender, "WRONG_HOST");
        _;
    }

    constructor() {
        owner = msg.sender;
    }

    function toggleHost(address addr, bool state) public onlyOwner {
        hosts[addr] = state;
    }

    function createGame(uint256 cardPrice, uint256 maxCards) public onlyHost(0) {
        gameCounter++;
        Game memory game;
        require(activeGameId == 0, "THERE_IS_GAME");

        game.host = msg.sender;
        game.cardPrice = cardPrice;
        game.maxCardsPerPlayer = maxCards;
        game.state = GameState.CREATED;

        activeGameId = gameCounter;

        games[gameCounter] = game;
    }

    function joinGame(uint256 gameId, uint256 cardsCount) public payable {
        Game memory game = games[gameId];

        require(game.state == GameState.CREATED, "INVALID_GAME");
        require(cardsCount > 0, "WRONG_COUNT");
        require(cardsCount <= game.maxCardsPerPlayer, "WRONG_CARD_NUMBER");
        require(msg.value == game.cardPrice * cardsCount, "WRONG_VALUE");
        require(cards[gameId][msg.sender] == 0, "ALREADY_REVEALED");

        game.totalPot += msg.value;
        game.soldCards += cardsCount;

        games[gameId] = game;

        cards[gameId][msg.sender] = _revealCards();
    }

    function startGame(uint256 gameId) public onlyHost(gameId) {
        Game storage game = games[gameId];
        require(game.state == GameState.CREATED, "INVALID_GAME");
        game.state = GameState.STARTED;
    }

    function pickNumber(uint256 gameId) public onlyHost(gameId) {
        Game memory game = games[gameId];
        require(game.state == GameState.STARTED, "INVALID_GAME");

        uint256 random = _mockRandom();
        uint256 number = (uint256(keccak256(abi.encode(random, numbers[gameId].length))) % 90) + 1;
        uint256 increment;
        while (duplicateNumbers[gameId][number]) {
            increment++;
            number = (uint256(keccak256(abi.encode(random, increment))) % 90) + 1;
        }
        numbers[gameId].push(number);
        duplicateNumbers[gameId][number] = true;
    }

    function assignWinner(uint256 gameId, address addr) public {
        Game memory game = games[gameId];
        require(game.state == GameState.STARTED, "INVALID_GAME");
        require(checkWinner(gameId, addr), "NOT_WINNER");

        game.state = GameState.ENDED;
        game.winner = addr;

        games[gameId] = game;
        activeGameId = 0;

        uint256 amount = (game.totalPot * 9) / 10;
        (bool sent, ) = addr.call{ value: amount }("");
        require(sent, "SEND_ERROR");
    }

    function checkWinner(uint256 gameId, address addr) public view returns (bool) {
        Game memory game = games[gameId];
        if (game.state != GameState.STARTED) {
            return false;
        }
        uint256[] memory _card = getCard(gameId, addr);
        uint256[] memory _numbers = numbers[gameId];
        uint256 hits;
        for (uint256 i = 0; i < _card.length; i++) {
            for (uint256 k = 0; k < _numbers.length; k++) {
                if (_card[i] == _numbers[k]) {
                    hits++;
                }
                if (hits == 15) {
                    return true;
                }
            }
        }
        return false;
    }

    function getCard(uint256 gameId, address addr) public view returns (uint256[] memory card) {
        if (cards[gameId][addr] > 0) {
            card = _expand(cards[gameId][addr], 15, 90);
        }
    }

    function getNumbers(uint256 gameId) public view returns (uint256[] memory _numbers) {
        _numbers = numbers[gameId];
    }

    function getCurrentGame() public view returns (Game memory game) {
        if (activeGameId == 0) {
            return game;
        }
        game = games[activeGameId];
    }

    function _revealCards() private view returns (uint256 cards) {
        cards = _mockRandom();
    }

    // This is a temp mock random function
    // TODO replace with VRF
    function _mockRandom() private view returns (uint256 random) {
        random = uint256(keccak256(abi.encode(msg.sender, block.timestamp)));
        // TODO: SAME NUMBERS SHOULD NOT PICKED TWICE
    }

    function _expand(
        uint256 randomNumber,
        uint256 count,
        uint256 limit
    ) private pure returns (uint256[] memory _randomArray) {
        _randomArray = new uint256[](count);
        for (uint256 i = 1; i <= count; i++) {
            uint256 newRN = uint256(keccak256(abi.encode(randomNumber, i)));
            _randomArray[i - 1] = ((newRN % limit) + 1);
        }
    }
}

