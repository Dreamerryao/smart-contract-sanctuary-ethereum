pragma solidity ^0.8.13;

import "../MockLandContract/MockIERC721Receiver.sol";
import "../utils/Address.sol";
import "../utils/Counters.sol";
import "../interfaces/IERC721LandVault.sol";
import {console2} from "../../lib/forge-std/src/console2.sol";

library LibVaultErc721 {
    using Address for address;
    using Counters for Counters.Counter;

    bytes32 constant VAULT_ERC721_STORAGE_POSITION =
        keccak256("wemeta.diamond.vault.ERC721.storage");

    struct VaultErc721Storage {
        // user address to history of staked tokenIds
        mapping(address => uint256[]) userTokenHistory;
        //dcl tokenId to array of staking instances
        mapping(uint256 => IERC721LandVault.StakeInstance[]) propertyStakingHistory;
        // array of tokenIds that have been staked
        uint256[] depositedTokenIds;
        //mapping of tokenIds to their current status
        mapping(uint256 => bool) currentStakeStatus;
        //tokenId to address eligible for unstaking
        mapping(uint256 => address) unstakeable;
        address dclLandAddress;
        Counters.Counter totalMinted;
        bool initialized;
        // Token name
        string _name;
        // Token symbol
        string _symbol;
        // Mapping from token ID to owner address
        mapping(uint256 => address) _owners;
        // Mapping owner address to token count
        mapping(address => uint256) _balances;
        // Mapping from token ID to approved address
        mapping(uint256 => address) _tokenApprovals;
        // Mapping from owner to operator approvals
        mapping(address => mapping(address => bool)) _operatorApprovals;
    }

    event Transfer(
        address indexed from,
        address indexed to,
        uint256 indexed tokenId
    );

    event Approval(
        address indexed owner,
        address indexed approved,
        uint256 indexed tokenId
    );

    event ApprovalForAll(
        address indexed owner,
        address indexed operator,
        bool approved
    );

    //!NOTE Must implement
    function getUsersStakingHistory(address _staker)
        external
        returns (uint256[] memory)
    {}

    //!note also get all current staked function

    function getAllCurrentlyStaked()
        internal
        view
        returns (uint256[] memory currentlyStaked)
    {
        VaultErc721Storage storage erc721 = vaultErc721Storage();
        uint256 j = 0;
        for (uint256 i = 0; i < erc721.depositedTokenIds.length; i++) {
            if (erc721.currentStakeStatus[erc721.depositedTokenIds[i]]) {
                j++;
            }
        }
        currentlyStaked = new uint256[](j);
        uint256 l = 0;
        for (uint256 k = 0; k < erc721.depositedTokenIds.length; k++) {
            if (erc721.currentStakeStatus[erc721.depositedTokenIds[k]]) {
                currentlyStaked[l] = erc721.depositedTokenIds[k];
                l++;
            }
        }
    }

    function vaultErc721Storage()
        internal
        pure
        returns (VaultErc721Storage storage vaultErc721)
    {
        bytes32 position = VAULT_ERC721_STORAGE_POSITION;
        assembly {
            vaultErc721.slot := position
        }
    }

    function safeTransfer(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) internal {
        transfer(from, to, tokenId);
        require(
            checkOnERC721Received(from, to, tokenId, _data),
            "ERC721: transfer to non ERC721Receiver implementer"
        );
    }

    function exists(uint256 tokenId) internal view returns (bool) {
        return
            LibVaultErc721.vaultErc721Storage()._owners[tokenId] != address(0);
    }

    function ownerOf(uint256 tokenId) internal view returns (address) {
        address owner = vaultErc721Storage()._owners[tokenId];
        require(
            owner != address(0),
            "ERC721: owner query for nonexistent token"
        );
        return owner;
    }

    function balanceOf(address owner) internal view returns (uint256) {
        require(
            owner != address(0),
            "ERC721: balance query for the zero address"
        );
        return vaultErc721Storage()._balances[owner];
    }

    function getApproved(uint256 tokenId) internal view returns (address) {
        require(
            exists(tokenId),
            "ERC721: approved query for nonexistent token"
        );

        return vaultErc721Storage()._tokenApprovals[tokenId];
    }

    function isApprovedForAll(address owner, address operator)
        internal
        view
        returns (bool)
    {
        return vaultErc721Storage()._operatorApprovals[owner][operator];
    }

    function isApprovedOrOwner(address spender, uint256 tokenId)
        internal
        view
        returns (bool)
    {
        require(
            exists(tokenId),
            "ERC721: operator query for nonexistent token"
        );
        address owner = ownerOf(tokenId);
        return (spender == owner ||
            getApproved(tokenId) == spender ||
            isApprovedForAll(owner, spender));
    }

    function _safeMint(address to, uint256 dclTokenId)
        internal
        returns (bool success)
    {
        VaultErc721Storage storage erc721 = vaultErc721Storage();
        erc721.totalMinted.increment();
        safeMint(to, dclTokenId, "");
        success = true;
    }

    function safeMint(
        address to,
        uint256 tokenId,
        bytes memory _data
    ) internal {
        mint(to, tokenId);
        require(
            checkOnERC721Received(address(0), to, tokenId, _data),
            "ERC721: transfer to non ERC721Receiver implementer"
        );
    }

    function setApprovalForAll(address operator, bool approved) internal {
        _setApprovalForAll(msg.sender, operator, approved);
    }

    function _setApprovalForAll(
        address owner,
        address operator,
        bool approved
    ) internal {
        VaultErc721Storage storage erc721 = vaultErc721Storage();

        require(owner != operator, "ERC721: approve to caller");
        erc721._operatorApprovals[owner][operator] = approved;
        emit ApprovalForAll(owner, operator, approved);
    }

    function mint(address to, uint256 tokenId) internal {
        require(to != address(0), "ERC721: mint to the zero address");
        require(!exists(tokenId), "ERC721: token already minted");

        beforeTokenTransfer(address(0), to, tokenId, 1);

        VaultErc721Storage storage erc721 = vaultErc721Storage();

        erc721._balances[to] += 1;
        erc721._owners[tokenId] = to;
        emit Transfer(address(0), to, tokenId);
    }

    function burn(uint256 tokenId) internal {
        address owner = ownerOf(tokenId);

        beforeTokenTransfer(owner, address(0), tokenId, 1);

        // Clear approvals
        approve(address(0), tokenId);

        VaultErc721Storage storage erc721 = LibVaultErc721.vaultErc721Storage();

        erc721._balances[owner] -= 1;
        delete erc721._owners[tokenId];
        delete erc721.unstakeable[tokenId];

        emit Transfer(owner, address(0), tokenId);
    }

    function transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal {
        require(
            ownerOf(tokenId) == from,
            "ERC721: transfer of token that is not own"
        );
        require(to != address(0), "ERC721: transfer to the zero address");

        beforeTokenTransfer(from, to, tokenId, 1);

        // Clear approvals from the previous owner
        approve(address(0), tokenId);

        VaultErc721Storage storage erc721 = LibVaultErc721.vaultErc721Storage();

        erc721._balances[from] -= 1;
        erc721._balances[to] += 1;
        erc721._owners[tokenId] = to;

        emit Transfer(from, to, tokenId);
    }

    function approve(address to, uint256 tokenId) internal {
        vaultErc721Storage()._tokenApprovals[tokenId] = to;
        emit Approval(ownerOf(tokenId), to, tokenId);
    }

    function beforeTokenTransfer(
        address from,
        address to,
        uint256, /* firstTokenId */
        uint256 batchSize
    ) internal {
        VaultErc721Storage storage erc721 = LibVaultErc721.vaultErc721Storage();
        if (batchSize > 1) {
            if (from != address(0)) {
                erc721._balances[from] -= batchSize;
            }
            if (to != address(0)) {
                erc721._balances[to] += batchSize;
            }
        }
    }

    function afterTokenTransfer(
        address from,
        address to,
        uint256 firstTokenId,
        uint256 batchSize
    ) internal {}

    function checkOnERC721Received(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) internal returns (bool) {
        if (to.isContract()) {
            try
                MockIERC721Receiver(to).onERC721Received(
                    msg.sender,
                    from,
                    tokenId,
                    data
                )
            returns (bytes4 retval) {
                return retval == MockIERC721Receiver.onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert(
                        "ERC721: transfer to non ERC721Receiver implementer"
                    );
                } else {
                    /// @solidity memory-safe-assembly
                    assembly {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } else {
            return true;
        }
    }

    function _getUserStakingHistory(address _staker)
        internal
        view
        returns (IERC721LandVault.StakeInstance[] memory userStakingHistory)
    {
        uint256[] storage userTokenHistory = LibVaultErc721
            .vaultErc721Storage()
            .userTokenHistory[_staker];
        uint256 lengthOfUserTokenHistory = userTokenHistory.length;

        uint256 sizeOfHistory = 0;
        for (uint256 i = 0; i < lengthOfUserTokenHistory; i++) {
            IERC721LandVault.StakeInstance[]
                memory stakingHistoryForToken = _getStakingHistoryForUserByTokenId(
                    _staker,
                    userTokenHistory[i]
                );
            sizeOfHistory += stakingHistoryForToken.length;
        }
        userStakingHistory = new IERC721LandVault.StakeInstance[](
            sizeOfHistory
        );
        uint256 historyIncrement = 0;
        for (uint256 i = 0; i < lengthOfUserTokenHistory; i++) {
            IERC721LandVault.StakeInstance[]
                memory stakingHistoryForToken = _getStakingHistoryForUserByTokenId(
                    _staker,
                    userTokenHistory[i]
                );
            for (uint256 j = 0; j < stakingHistoryForToken.length; j++) {
                userStakingHistory[historyIncrement] = stakingHistoryForToken[
                    j
                ];
                historyIncrement++;
            }
        }
    }

    // returns an array of timestamps.  Every even index is a deposit and odd is withdrawal time.
    // All zeros
    function _getStakingHistoryForUserByTokenId(
        address _staker,
        uint256 _tokenId
    )
        internal
        view
        returns (
            IERC721LandVault.StakeInstance[] memory userStakingHistoryByToken
        )
    {
        IERC721LandVault.StakeInstance[] storage stakeHistory = LibVaultErc721
            .vaultErc721Storage()
            .propertyStakingHistory[_tokenId];
        uint256 historyLength = stakeHistory.length;

        userStakingHistoryByToken = new IERC721LandVault.StakeInstance[](
            historyLength
        );
        for (uint256 i = 0; i < historyLength; i++) {
            IERC721LandVault.StakeInstance memory stakeInstance = stakeHistory[
                i
            ];
            if (stakeInstance.stakerAddress == _staker) {
                userStakingHistoryByToken[i] = stakeInstance;
            }
        }
    }

    function _recordStakeDeposit(address _staker, uint256 _dclLandTokenId)
        internal
        returns (bool success)
    {
        uint256[] storage userTokenHistory = LibVaultErc721
            .vaultErc721Storage()
            .userTokenHistory[_staker];
        uint256 lengthOfUserTokenHistory = userTokenHistory.length;

        bool found;
        for (uint256 i = 0; i < lengthOfUserTokenHistory; i++) {
            if (userTokenHistory[i] == _dclLandTokenId) {
                found = true;
            }
        }
        if (!found) {
            userTokenHistory.push(_dclLandTokenId);
        }

        IERC721LandVault.StakeInstance[]
            storage tokenStakingHistory = LibVaultErc721
                .vaultErc721Storage()
                .propertyStakingHistory[_dclLandTokenId];
        uint256 lengthOfStakingInstances = tokenStakingHistory.length;
        if (lengthOfStakingInstances > 0) {
            require(
                tokenStakingHistory[lengthOfStakingInstances - 1]
                    .timeWithdrawn != 0,
                "No withdrawal time listed for previous staking instance"
            );
        }
        tokenStakingHistory.push(
            IERC721LandVault.StakeInstance(block.timestamp, 0, _staker)
        );
        success = true;
    }

    function _recordStakeWithdrawal(address _staker, uint256 _dclLandTokenId)
        internal
        returns (bool success)
    {
        IERC721LandVault.StakeInstance[]
            storage tokenStakingHistory = LibVaultErc721
                .vaultErc721Storage()
                .propertyStakingHistory[_dclLandTokenId];
        uint256 lengthOfStakingInstances = tokenStakingHistory.length;
        //what about when length ==0?
        require(
            tokenStakingHistory[lengthOfStakingInstances - 1].timeWithdrawn ==
                0,
            "Last instance includes withdrawal time"
        );
        tokenStakingHistory[lengthOfStakingInstances - 1].timeWithdrawn = block
            .timestamp;
        success = true;
    }

    function _currentlyStaked(uint256 _tokenId)
        internal
        view
        returns (bool currentlyStaked)
    {
        currentlyStaked = LibVaultErc721
            .vaultErc721Storage()
            .currentStakeStatus[_tokenId];
    }

    function _previouslyStaked(uint256 _tokenId)
        internal
        view
        returns (bool previouslyStaked)
    {
        uint256 depositsLength = LibVaultErc721
            .vaultErc721Storage()
            .depositedTokenIds
            .length;

        for (uint256 i = 0; i < depositsLength; i++) {
            if (
                LibVaultErc721.vaultErc721Storage().depositedTokenIds[i] ==
                _tokenId
            ) {
                previouslyStaked = true;
            }
        }
    }
}

