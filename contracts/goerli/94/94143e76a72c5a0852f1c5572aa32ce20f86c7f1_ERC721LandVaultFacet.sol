contract ERC721LandVaultFacet is Context, IERC721LandVault {
    /// @notice Initialises the ERC721's name, symbol and mockDcllandAddress
    //  Implements ERC721 constructor data for the vault in the whole proxy pattern.
    function initERC721(
        string memory _name,
        string memory _symbol,
        address _dclLandContract
    ) external returns (bool success) {
        LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
            .vaultErc721Storage();
        require(!erc721.initialized, "ERC721 Storage already initialized");

        erc721.initialized = true;
        erc721._name = _name;
        erc721._symbol = _symbol;
        erc721.dclLandAddress = _dclLandContract;
        success = true;
    }

    //ERC721 implementation for checking Vault Name
    function getName() external view returns (string memory) {
        LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
            .vaultErc721Storage();
        return erc721._name;
    }

    function safeTransfer(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) external {
        return LibVaultErc721.safeTransfer(from, to, tokenId, _data);
    }

    function setApprovalForAll(address operator, bool _approved) external {
        return LibVaultErc721.setApprovalForAll(operator, _approved);
    }

    function ownerOf(uint256 tokenId) external view returns (address) {
        return LibVaultErc721.ownerOf(tokenId);
    }

    function balanceOf(address owner) external view returns (uint256) {
        return LibVaultErc721.balanceOf(owner);
    }

    function getApproved(uint256 tokenId) external view returns (address) {
        return LibVaultErc721.getApproved(tokenId);
    }

    function isApprovedForAll(address owner, address operator)
        external
        view
        returns (bool)
    {
        return LibVaultErc721.isApprovedForAll(owner, operator);
    }

    function isApprovedOrOwner(address spender, uint256 tokenId)
        external
        view
        returns (bool)
    {
        return LibVaultErc721.isApprovedOrOwner(spender, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external {
        return LibVaultErc721.transfer(from, to, tokenId);
    }

    //no admin currently
    function _safeMint(address to, uint256 tokenId) internal returns (bool) {
        return LibVaultErc721._safeMint(to, tokenId);
    }

    function burn(uint256 tokenId) internal {
        return LibVaultErc721.burn(tokenId);
    }

    function approve(address to, uint256 tokenId) external {
        return LibVaultErc721.approve(to, tokenId);
    }

    //! Might we need to allow approved users to stake, as well?
    function stake(uint256 _tokenId) external returns (uint256 _tokenMinted) {
        LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
            .vaultErc721Storage();
        require(
            MockIERC721(erc721.dclLandAddress).ownerOf(_tokenId) ==
                _msgSender(),
            "Msg.sender must be land owner to stake"
        );
        emit TestAddressThis(address(this));
        require(
            MockIERC721(erc721.dclLandAddress).isAuthorized(
                address(this),
                _tokenId
            ),
            "WeMeta Diamond Vault must be given approval permission of land to be staked"
        );
        MockIERC721(erc721.dclLandAddress).transferFrom(
            _msgSender(),
            address(this),
            _tokenId
        );
        erc721.unstakeable[_tokenId] = _msgSender();
        _safeMint(_msgSender(), _tokenId);
        bool previouslyStakedResult = LibVaultErc721._previouslyStaked(
            _tokenId
        );
        if (!previouslyStakedResult) {
            erc721.depositedTokenIds.push(_tokenId);
        }
        erc721.currentStakeStatus[_tokenId] = true;
        LibVaultErc721._recordStakeDeposit(_msgSender(), _tokenId);
        emit AddedStake(_msgSender(), _tokenId, block.timestamp);
    }

    function previouslyStaked(uint256 _tokenId)
        external
        view
        returns (bool previouslyStaked)
    {
        previouslyStaked = LibVaultErc721._previouslyStaked(_tokenId);
    }

    function getAllCurrentlyStaked()
        external
        view
        returns (uint256[] memory allCurrentlyStaked)
    {
        return LibVaultErc721.getAllCurrentlyStaked();
    }

    function currentlyStaked(uint256 _tokenId)
        external
        view
        returns (bool currentlyStaked)
    {
        currentlyStaked = LibVaultErc721._currentlyStaked(_tokenId);
    }

    //input the tokenID of your land, not the tokenID of your vault token
    function unstake(uint256 _tokenId) external {
        LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
            .vaultErc721Storage();

        require(
            erc721.unstakeable[_tokenId] == _msgSender(),
            "Only the original owner can reclaim that land"
        );
        MockIERC721(erc721.dclLandAddress).transferFrom(
            address(this),
            _msgSender(),
            _tokenId
        );
        erc721.unstakeable[_tokenId] = address(0);
        burn(_tokenId);
        erc721.currentStakeStatus[_tokenId] = false;
        LibVaultErc721._recordStakeWithdrawal(_msgSender(), _tokenId);
        emit RemovedStake(_msgSender(), _tokenId, block.timestamp);
    }

    function getStakingHistoryForUserByTokenId(
        address _staker,
        uint256 _tokenId
    ) external view returns (StakeInstance[] memory) {
        return
            LibVaultErc721._getStakingHistoryForUserByTokenId(
                _staker,
                _tokenId
            );
    }

    function getUserTokenHistory(address _staker)
        external
        view
        returns (uint256[] memory userTokenHistory)
    {
        return LibVaultErc721.vaultErc721Storage().userTokenHistory[_staker];
    }

    function getUserStakingHistory(address _staker)
        external
        view
        returns (StakeInstance[] memory)
    {
        return LibVaultErc721._getUserStakingHistory(_staker);
    }

    // function showCurrentlyStaked(address _staker) external returns(uint256[] memory currentlyStakedArray){
    //     LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
    //         .vaultErc721Storage();

    //     for( uin256t i=0; i<erc721.length; i++){

    //     }
    // }
    // input is DCL Land Address
    function showUnstakeableAddress(uint256 _tokenId)
        external
        view
        returns (address unstaker)
    {
        LibVaultErc721.VaultErc721Storage storage erc721 = LibVaultErc721
            .vaultErc721Storage();
        unstaker = erc721.unstakeable[_tokenId];
    }
}

