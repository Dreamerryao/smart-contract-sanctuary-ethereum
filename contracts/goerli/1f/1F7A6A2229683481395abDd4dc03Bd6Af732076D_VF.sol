import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/ClonesUpgradeable.sol";
import "@openzeppelin/contracts/proxy/ERC1967/ERC1967Proxy.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

//import "hardhat/console.sol";

import "./utils.sol";
import "./VF_Film_NFT.sol";
import "./VF_Festival.sol";
import "./film_metadata.sol";
import "./festival_metadata.sol";

//TODO dodelat komentare https://docs.soliditylang.org/en/latest/natspec-format.html
//TODO jazykova korekce

contract VF is UUPSUpgradeable, AccessControlEnumerableUpgradeable {
    //
    // Public items
    //

    // only OWNER can withdraw ETH
    //   0xb19546dff01e856fb3f010c267a7b1c60363cf8a4664e21cc89c26224620214e
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    // only DEPLOYER can upgrade VF contract and set new VF_Festival and VF_Film_NFT implementation contracts
    //   0xfc425f2263d0df187444b70e47283d622c70181c5baebb1306a01edba1ce184c
    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER_ROLE");

    // only VF_ADMIN can call the festival and film collection management functions
    //   0x8cbca34a65b016fecd3bbcca5d421c71ea2a741f5b6a9fca76d7dc42fefe797b
    bytes32 public constant VF_ADMIN_ROLE = keccak256("VF_ADMIN_ROLE");

    // block number where this contract is deployed
    uint256 public deploymentBlock;

    // festivals
    VF_Festival public VF_Festival_impl;  // current implementation VF_Festival contract
    VF_Festival[] public festivals;       // clones (proxies) of VF_Festival
    struct VF_Festival_params {
        bool exists;
    }
    mapping(VF_Festival => VF_Festival_params) private _festivalsMap;

    // film NFTs
    VF_Film_NFT public VF_Film_NFT_impl;   // current implementation VF_Film_NFT contract
    VF_Film_NFT[] public filmCollections;  // clones (proxies) of VF_Film_NFT
    struct VF_Film_NFT_params {
        bool exists;
    }
    mapping(VF_Film_NFT => VF_Film_NFT_params) private _filmCollectionsMap;

    //
    // Private items
    //
    // set of VF_Festival opened to mint
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    EnumerableSetUpgradeable.AddressSet private openFestivals;

    //
    // Events
    //
    event VF_Festival_implementationSet(VF_Festival implementationAddress);
    event FestivalCreated(VF_Festival festivalAddress, string festivalName);
    event VF_Film_NFT_implementationSet(VF_Film_NFT implementationAddress);
    event FilmCollectionCreated(VF_Film_NFT filmCollectionAddress, string symbol);

    //
    // Constructor
    //
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(VF_Festival _VF_Festival_contract, VF_Film_NFT _VF_Film_NFT_contract)
        public initializer
    {
        __UUPSUpgradeable_init();
        __AccessControlEnumerable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(OWNER_ROLE, msg.sender);
        _grantRole(DEPLOYER_ROLE, msg.sender);
        _grantRole(VF_ADMIN_ROLE, msg.sender);

        deploymentBlock = block.number;
        _set_VF_Festival_contract(_VF_Festival_contract);
        _set_VF_Film_NFT_contract(_VF_Film_NFT_contract);
    }

    //
    // Getters & Setters
    //
    function contractName() public pure returns(string memory) {
        return string(bytes.concat(
            bytes(type(VF).name),
            bytes(" (Vertifilms Main Contract)"))
        );
    }

    function contractVersion() public pure returns(uint) {
        return 1;
    }

    function contractIsProxy() public view returns(bool) {
        return deploymentBlock != 0;
    }

    function festivalsCount() public view returns (uint256) {
        return festivals.length;
    }

    function filmCollectionsCount() public view returns (uint256) {
        return filmCollections.length;
    }

    function isOpenFestival(VF_Festival _festival) public view returns(bool) {
        if (openFestivals.contains(address(_festival)))
            return true;
        return false;
    }

    function set_VF_Festival_contract(VF_Festival _VF_Festival_contract) public onlyRole(DEPLOYER_ROLE) {
        require(VF_Festival_impl != _VF_Festival_contract, "No change");
        _set_VF_Festival_contract(_VF_Festival_contract);
    }

    function _set_VF_Festival_contract(VF_Festival _VF_Festival_contract) private {
        VF_Festival_impl = _VF_Festival_contract;
        emit VF_Festival_implementationSet(VF_Festival_impl);
    }

    function set_VF_Film_NFT_contract(VF_Film_NFT _VF_Film_NFT_contract) public onlyRole(DEPLOYER_ROLE) {
        require(VF_Film_NFT_impl != _VF_Film_NFT_contract, "No change");
        _set_VF_Film_NFT_contract(_VF_Film_NFT_contract);
    }

    function _set_VF_Film_NFT_contract(VF_Film_NFT _VF_Film_NFT_contract) private {
        VF_Film_NFT_impl = _VF_Film_NFT_contract;
        emit VF_Film_NFT_implementationSet(VF_Film_NFT_impl);
    }

    //
    // Authenticity tests
    //
    function isValidFestival(VF_Festival festival) public view returns (bool) {
        return _festivalsMap[festival].exists;
    }

    function isValidFilmCollection(VF_Film_NFT filmCollection) public view returns (bool) {
        return _filmCollectionsMap[filmCollection].exists;
    }

    //
    // VF governance
    //
    function festival_setMetadata(VF_Festival _festival, VF_Festival_Metadata _metadata)
        public onlyRole(VF_ADMIN_ROLE)
    {
        _festival.set_metadata(_metadata);
    }

    function festival_setParams(VF_Festival _festival, uint64 _start, uint64 _end)
        public onlyRole(VF_ADMIN_ROLE)
    {
        _festival.set_openTimeInterval(_start, _end);
    }

    function festival_setFilmCollections(
            VF_Festival _festival,
            VF_Film_NFT[] calldata _filmCollections,
            uint32[] calldata _mintLimits
        )
        public onlyRole(VF_ADMIN_ROLE)
    {
        festival_clearFilmCollections(_festival);
        uint64 start = _festival.openStart();
        uint64 end = _festival.openEnd();
        for (uint i=0; i!=_filmCollections.length; i++) {
            VF_Film_NFT coll = _filmCollections[i];
            _festival.set_addFilmCollection(coll);
            coll.createRoyaltyReceiver(_festival);
            coll.set_listAtFestival(_festival, _mintLimits[i], start, end);
        }
    }

    function festival_open(VF_Festival _festival)
        public onlyRole(VF_ADMIN_ROLE)
    {
        openFestivals.add(address(_festival));
        _festival.set_freeze();
    }

    function festival_freezeParams(VF_Festival _festival)
        public onlyRole(VF_ADMIN_ROLE)
    {
        _festival.set_freeze();
    }

    function festival_close(VF_Festival _festival)
        public onlyRole(VF_ADMIN_ROLE)
    {
        openFestivals.remove(address(_festival));
        festival_clearFilmCollections(_festival);
    }

    function festival_closeAndSetFilmCollections(
            VF_Festival _festivalToClose,
            VF_Festival _festivalToSet,
            VF_Film_NFT[] calldata _filmCollections,
            uint32[] calldata _mintLimits
        )
        public onlyRole(VF_ADMIN_ROLE)
    {
        festival_close(_festivalToClose);
        festival_setFilmCollections(_festivalToSet, _filmCollections, _mintLimits);
    }

    function festival_clearFilmCollections(VF_Festival _festival)
        public onlyRole(VF_ADMIN_ROLE)
    {
        uint cnt = _festival.filmCollectionsToMintCount();
        for (uint i=0; i!=cnt; i++) {
            VF_Film_NFT collection = _festival.filmCollectionsToMint(i);
            collection.set_unlistFromFestival(_festival);
        }
        _festival.set_clearFilmCollectionsToMint();
    }

    function transferFestivalOwnership(VF_Festival festival, address newOwner)
        public onlyRole(VF_ADMIN_ROLE)
    {
        festival.transferOwnership(newOwner);
    }

    function transferFilmCollectionOwnership(VF_Film_NFT filmCollection, address newOwner)
        public onlyRole(VF_ADMIN_ROLE)
    {
        filmCollection.transferOwnership(newOwner);
    }

    function createFestival(
            string calldata name,
            uint32 shareBasisPoints
        ) public onlyRole(VF_ADMIN_ROLE) returns (VF_Festival)
    {
        VF_Festival newFestival = VF_Festival(payable(ClonesUpgradeable.clone(address(VF_Festival_impl))));
        VF_Festival(newFestival).initialize(
            this,
            name,
            shareBasisPoints
        );
        festivals.push(VF_Festival(newFestival));
        _festivalsMap[VF_Festival(newFestival)] = VF_Festival_params({exists:true});
        emit FestivalCreated(newFestival, name);
        return newFestival;
    }

    function createFilmCollection(
            string calldata name,
            string calldata symbol,
            VF_Film_Metadata metadata,
            uint32 hardCap,
            uint256 primarySalePrice,
            uint96 royaltyBasisPoints,
            uint32 shareBasisPoints
        ) public onlyRole(VF_ADMIN_ROLE) returns (VF_Film_NFT)
    {
        VF_Film_NFT newCollection = VF_Film_NFT(payable(ClonesUpgradeable.clone(address(VF_Film_NFT_impl))));
        VF_Film_NFT(newCollection).initialize(
            this,
            name,
            symbol,
            metadata,
            hardCap,
            primarySalePrice,
            royaltyBasisPoints,
            shareBasisPoints
        );
        filmCollections.push(VF_Film_NFT(newCollection));
        _filmCollectionsMap[VF_Film_NFT(newCollection)] = VF_Film_NFT_params({exists:true});
        emit FilmCollectionCreated(newCollection, symbol);
        return newCollection;
    }

    //
    // Payments
    //
    function withdraw() public onlyRole(OWNER_ROLE) {
        address payable to = payable(msg.sender);
        to.transfer(address(this).balance);
    }

    //
    // Upgradability functions
    //
    function _authorizeUpgrade(address /*newImplementation*/)
        internal
        view
        override
        onlyRole(DEPLOYER_ROLE)
    {}

    //
    // Fallback for proxy contracts only
    //
    // all received payments are considerd as
    //   - primary sale share from VF_Film_NFT
    //     OR
    //   - royalty share from VF_Film_NFT
    //
    // we don't allow receive ETH for implementation contract
    receive() external payable onlyProxy {
    }

}


