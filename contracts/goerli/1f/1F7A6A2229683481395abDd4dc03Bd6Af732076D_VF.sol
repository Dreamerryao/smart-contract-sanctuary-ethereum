pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

//import "hardhat/console.sol";

import "./utils.sol";
import "./VF.sol";
import "./VF_Film_NFT.sol";
import "./festival_metadata.sol";

contract VF_Festival is OwnableUpgradeable, OnlyDelegateCall {
    //
    // Public items
    //
    VF public VF_contract;

    // short version of festival name, e.g. 'Mexico 2022'
    // full festival name, e.g. 'Vertifilms Festival Mexico 2022' is stored in metadata
    string public shortName;

    // percentage from NFT sell intended for the festival
    // --- one memory slot ---
    uint32 public shareBasisPoints;
    uint224 private _dummy;
    // --- one memory slot ---

    // time interval start...end when film collection tokens can be minted
    // --- one memory slot ---
    uint64 public openStart;
    uint64 public openEnd;
    // --- one memory slot ---

    VF_Festival_Metadata public metadata;

    // if true no more change is allowed in FestivalParams
    bool public isFrozen;

    // film collections allowed to be minted at this festival
    VF_Film_NFT[] public filmCollectionsToMint;

    //
    // Private items
    //

    //
    // Events
    //

    //
    // Constructor
    //
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
            VF _vertifilmsContract,
            string memory _festivalName,
            uint32 _shareBasisPoints
        ) public initializer
    {
        require(address(_vertifilmsContract) != address(0), "VF must be nonzero");
        __Ownable_init();
        VF_contract = _vertifilmsContract;
        shortName = _festivalName;
        shareBasisPoints = (_shareBasisPoints > 10000) ? 10000 : _shareBasisPoints;
    }

    //
    // Modifiers
    //
    modifier modifyParamsAllowed() {
        requireSender();
        require( isFrozen == false, "Festival parameters are frozen" );
        _;
    }

    modifier onlyVF() {
        requireSender();
        _;
    }

    function requireSender() internal view {
        require( msg.sender == address(VF_contract), "Only VF can modify festival parameters" );
    }

    //
    // Getters & Setters
    //
    function contractName() public pure returns (string memory) {
        return string(bytes.concat(
            bytes(type(VF_Festival).name),
            bytes(" (Vertifilms Festival Contract)"))
        );
     }

    function contractVersion() public pure returns (uint) {
        return 1;
    }

    function contractIsProxy() public view returns(bool) {
        return address(VF_contract) != address(0);
    }

    function isOpenTime() public view returns (bool) {
        return block.timestamp > openStart && block.timestamp < openEnd;
    }

    function filmCollectionsToMintCount() public view returns (uint) {
        return filmCollectionsToMint.length;
    }

    //
    // Festival parameters
    //
    function set_metadata(VF_Festival_Metadata _metadata) external modifyParamsAllowed {
        metadata = _metadata;
    }

    function set_openTimeInterval(uint64 _start, uint64 _end) external modifyParamsAllowed {
        require(_start > 0, "Start must be nonzero");
        require(_start <= _end, "End must be after start");
        openStart = _start;
        openEnd = _end;
    }

    function set_addFilmCollection(VF_Film_NFT _filmCollection) external modifyParamsAllowed {
        filmCollectionsToMint.push(_filmCollection);
    }

    function set_freeze() external modifyParamsAllowed {
        isFrozen = true;
    }

    function set_clearFilmCollectionsToMint() external onlyVF {
        delete filmCollectionsToMint;
    }

    //
    // Payments
    //
    function withdraw() public onlyOwner {
        address payable to = payable(msg.sender);
        to.transfer(address(this).balance);
    }

    //
    // Fallback for proxy contracts only
    //
    // all received payments are considerd as
    //   - primary sale share from VF_Film_NFT
    //     OR
    //   - royalty share from VF_Film_NFT
    receive() external payable onlyDelegateCall {}
}

