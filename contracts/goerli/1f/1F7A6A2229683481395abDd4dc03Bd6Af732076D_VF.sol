contract RoyaltyReceiver {
    VF_Festival public festival;
    VF_Film_NFT public filmCollection;

    constructor(VF_Festival _festival, VF_Film_NFT _collection) {
        festival = _festival;
        filmCollection = _collection;
    }

    receive() external payable {
        filmCollection.handleRoyaltyPayment{value: msg.value}(festival);
    }
}

