pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721RoyaltyUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

//import "hardhat/console.sol";

import "./VF.sol";
import "./VF_Festival.sol";
import "./film_metadata.sol";

//TODO jak v OpenSea zajistit abychom byli well-known kontraktem ?  tj. kontraktem s fajfkou
//TODO contact OpenSea to mark an entire smart contract as frozen (https://docs.opensea.io/docs/metadata-standards)
//   prinejhorsim emitovat pro kazdy token: event PermanentURI(string _value, uint256 indexed _id);
//   to stoji 50k gasu na jeden mint

contract VF_Film_NFT is
    OwnableUpgradeable, // creator is owner of the film NFT collection
    ERC721Upgradeable,  // upgradability
    ERC721RoyaltyUpgradeable // royalties for creator
{
    //
    // Public items
    //
    VF public VF_contract;

    // --- one memory slot ---
    uint64 public primarySalePrice;
    uint32 public shareBasisPoints;
    uint96 public
    royaltyBasisPoints;
    uint32 public hardCap;
    uint32 private _nextTokenId;
    // --- one memory slot ---

    VF_Film_Metadata public metadata;

    //
    // Private items
    //
    // tokenId => festival where was minted
    mapping(uint256 => VF_Festival) private _mintedAtFestival;

    // minted at festival => where to send royalties
    mapping(VF_Festival => RoyaltyReceiver) private _royaltyReceivers;

    // festivals where it is listed to be minted
    struct MintParams {
        uint32 remainsToMint;
        uint64 openStart;
        uint64 openEnd;
    }
    mapping(VF_Festival => MintParams) private _listedAtFestivals;

    //
    // Events
    //
    event ReceivedRoyalty(uint256 value, VF_Festival mintedAtFestival);

    // OpenSea to mark token id as frozen
    event PermanentURI(string value, uint256 indexed id);

    //
    // Constructor
    //
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
            VF _vertifilmsContract,
            string calldata _name,
            string calldata _symbol,
            VF_Film_Metadata _metadata,
            uint32 _hardCap,
            uint256 _primarySalePrice,
            uint96 _royaltyBasisPoints,
            uint32 _shareBasisPoints
        ) public initializer
    {
        require(address(_vertifilmsContract) != address(0), "VF must be nonzero");
        __Ownable_init();
        __ERC721_init(_name, _symbol);

        VF_contract = _vertifilmsContract;
        metadata = _metadata;
        hardCap = _hardCap;
        primarySalePrice = uint64(_primarySalePrice);

        royaltyBasisPoints = _royaltyBasisPoints;

        shareBasisPoints = (_shareBasisPoints > 10000)
            ? 10000
            : _shareBasisPoints; // 22000 gas

        _nextTokenId = 1;
    }

    //
    // Modifiers
    //
    modifier onlyVF() {
        require( msg.sender == address(VF_contract), "Only VF can modify film collection parameters" );
        _;
    }

    //
    // Getters & Setters
    //
    function contractName() public pure returns (string memory) {
        return string(bytes.concat(
            bytes(type(VF_Film_NFT).name),
            bytes(" (Vertifilms Film NFT Collection Contract)"))
        );
     }

    function contractVersion() public pure returns (uint) {
        return 1;
    }

    function contractIsProxy() public view returns(bool) {
        return address(VF_contract) != address(0);
    }

    function totalSupply() public view returns (uint32) {
        return _nextTokenId-1;
    }

    function mintedAtFestival(uint256 tokenId) public view returns (VF_Festival) {
        return _mintedAtFestival[tokenId];
    }

    function royaltyReceivers(VF_Festival festivalAddr) public view returns (RoyaltyReceiver) {
        return _royaltyReceivers[festivalAddr];
    }

    function listedAtFestivals(VF_Festival festivalAddr) public view returns (MintParams memory) {
        return _listedAtFestivals[festivalAddr];
    }

    //
    // Minting parameters
    //
    function set_listAtFestival(
            VF_Festival festival, uint32 mintLimit,
            uint64 openStart, uint64 openEnd
        )
        external onlyVF
    {
        require(mintLimit > 0, "mintLimit cannot be 0");
        require(openStart > 0, "openStart cannot be 0");
        _listedAtFestivals[festival] = MintParams( {
            remainsToMint: mintLimit,
            openStart: openStart,
            openEnd: openEnd
        } );
    }

    function set_unlistFromFestival(VF_Festival festival) external onlyVF {
        delete _listedAtFestivals[festival];
    }

    //
    // NFT functions: Mint & Transfer & Burn & URI
    //
    // returns id of minted token
    function mintAtFestival(VF_Festival festival, address recipient)
        public payable returns (uint32)
    {
        //uint dbgGasOnStart = gasleft();

        // check if can be minted at the festival
        MintParams memory mintParams = _listedAtFestivals[festival]; // 2k gas first access _listedAtFestivals memory slot + 2k gas first access MintParams memory slot
        require(  // cca 200 gas
            mintParams.openStart > 0,
            "Film collection is not listed at the festival for mint"
        );
        require(  // cca 200 gas
            mintParams.remainsToMint > 0,
            "All festival supply already minted"
        );
        require(
            block.timestamp > mintParams.openStart && block.timestamp < mintParams.openEnd,
            "Festival is out of opening hours"
        );

        // check if the film NFT collection is mintable
        require(_nextTokenId <= hardCap, "Max supply reached"); // cca 250 gas

        require(  // cca 250 gas
            msg.value == primarySalePrice,
            "Transaction value did not equal the primary sale price"
        );

        //
        // do mint
        _listedAtFestivals[festival].remainsToMint--;
        _safeMint(recipient, _nextTokenId); // cca 47k gas
        _mintedAtFestival[_nextTokenId] = festival; // cca 22k gas   // <-- optimize? vyplnit dopredu pred mintem v set_listAtFestival() ?
        _handleIncomingPayment(festival); // cca 19k gas   // <-- optimize? neposilat hned ale nechat na pull pozdeji? podle me to za to nestoji

        //
        // set royalties
        address rr = address(_royaltyReceivers[festival]);
        if (rr != address(0))
            _setTokenRoyalty( _nextTokenId, rr, royaltyBasisPoints ); // cca 22k gas   // <-- optimize? vyplnit dopredu pred mintem v set_listAtFestival() ?

        //
        // OpenSea to mark minted token as frozen
        //emit PermanentURI(contractURI(), _currentTokenId); // gas cost 50k

        //console.log("DBG: gas usage: ", dbgGasOnStart-gasleft());

        return _nextTokenId++;
    }

    function createRoyaltyReceiver(VF_Festival fest) external {
        require( msg.sender == address(VF_contract), "Only VF can create RoyaltyReceiver" );
        if ( address(_royaltyReceivers[fest]) == address(0) ) {
            _royaltyReceivers[fest] = new RoyaltyReceiver(fest, this); //  ~150.000 gas
        }
    }

    function _burn(uint256 tokenId)
        internal override(ERC721Upgradeable, ERC721RoyaltyUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(tokenId>0 && tokenId<=_nextTokenId, "tokenId doesn't exist");

        bytes memory proofData = bytes.concat(
            '"proof_of_authenticity":{\n'
                '"blockchain":"Ethereum Mainnet",\n'
                '"chain_id":1,\n'

                '"VF":"' , bytes(StringsUpgradeable.toHexString(address(VF_contract))) , '",\n'
                '"VF_descr":"Vertifilms main contract",\n'
                '"VF_block":"' , bytes(StringsUpgradeable.toString(VF_contract.deploymentBlock())) , '",\n'
                '"VF_block_descr":"block number where VF was deployed",\n'

                '"VF_Festival":"' , bytes(StringsUpgradeable.toHexString(address(_mintedAtFestival[tokenId]))) , '",\n'
                '"VF_Festival_descr":"contract where this token was minted",\n'

                '"VF_Film_NFT":"' , bytes(StringsUpgradeable.toHexString(address(this))) , '",\n'
                '"VF_Film_NFT_descr":"this NFT contract"\n'

                '"sourceFiles":"ipfs://Qm............................................   todo ?"\n' //TODO
            '}\n'
        );

        VF_Festival mintedAt = _mintedAtFestival[tokenId];
        bytes memory festivalData;
        bytes memory festivalTokenData;
        if (address(mintedAt) != address(0)) {
            festivalData = bytes.concat(
                '"mintedAtFestival":{\n'
                '"name":"', bytes(mintedAt.shortName()) , '",\n'
                '"openStart":"', bytes(StringsUpgradeable.toString(mintedAt.openStart())) , '",\n'
                '"openEnd":"', bytes(StringsUpgradeable.toString(mintedAt.openEnd())) , '"\n'
                '}'
            );
            festivalTokenData = mintedAt.metadata().data(address(this));
        }

        return string.concat(
            'data:application/json;base64,' , Base64Upgradeable.encode(
                bytes.concat(
                    '{\n' ,
                        '"token_id":' , bytes(StringsUpgradeable.toString(tokenId)) , ',\n' ,
                        metadata.data() , ',\n' ,
                        festivalTokenData, ',\n' ,
                        festivalData , ',\n' ,
                        proofData ,
                    '}'
                )
            )
        );
    }

    //
    // Payments
    //
    function withdraw() public onlyOwner {
        address payable to = payable(msg.sender);
        to.transfer(address(this).balance);
    }

    // called from RoyaltyReceiver
    function handleRoyaltyPayment(VF_Festival fest) external payable {
        emit ReceivedRoyalty(msg.value, fest); // ~1300 gas
        _handleIncomingPayment(fest);
    }

    function _handleIncomingPayment(VF_Festival fest) internal {
        //
        // if VF is the owner of this collection then send all ETH to its address
        //
        if ( owner()==address(VF_contract) ) {
            (bool success, ) = address(VF_contract).call{value: msg.value}("");
            require(success, "ETH transfer to VF failed. (VF is the owner)");
            return;
        }

        //
        // Otherwise distribute shares
        //
        uint256 forMe = (shareBasisPoints * msg.value) / 10000;

        uint256 forFestival = 0;
        if ( fest.owner()!=address(VF_contract) ) {
            uint256 festivalShareBasisPoints = fest.shareBasisPoints();
            if (shareBasisPoints + festivalShareBasisPoints > 10000)
                festivalShareBasisPoints = 10000 - shareBasisPoints;
            forFestival = (festivalShareBasisPoints * msg.value) / 10000;
            (bool success2, ) = address(fest).call{value: forFestival}("");
            require(success2, "ETH transfer to VF_Festival failed.");
        }

        uint256 forVF = msg.value - forMe - forFestival;
        (bool success1, ) = address(VF_contract).call{value: forVF}("");
        require(success1, "ETH transfer to VF failed.");
    }

    //
    // ERC165 Standard Interface Detection
    //
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721RoyaltyUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    //
    // Fallback
    //
    // we don't allow receive ETH via fallback functions
    // for both proxy and implementation contracts
    receive() external payable {
        revert("VF_Film_NFT doesn't receive payments");
    }
}


