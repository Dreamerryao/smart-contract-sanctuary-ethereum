abstract contract NFTContract {
    function renderTokenById(uint256 id)
        external
        view
        virtual
        returns (string memory);

    function getDescription(uint256 id)
        external
        view
        virtual
        returns (string memory);

    function transferFrom(
        address from,
        address to,
        uint256 id
    ) external virtual;

    function name() external view virtual returns (string memory);
}

