// SPDX-License-Identifier: MIT
pragma solidity 0.8.19;

interface INftAuctionHouse {
    struct Auction {
        address auctioneer;
        address nftAddress;
        uint256 tokenId;
        uint256 auctionStartTime;
        uint256 auctionEndTime;
        uint256 startingBid;
        uint256 referrerNftFeeBps;
        uint256 lastBidId;
        bool hasResolved;
    }

    struct Bid {
        uint32 hlChainId;
        uint256 remoteBidId;
        address bidder;
        uint256 bidAmount;
        ReferrerNft referrerNft;
    }

    struct ReferrerNft {
        address addr;
        uint256 tokenId;
    }

    struct Bidder {
        address referrerNft;
        address invitee;
    }

    struct AuctionHouseInfo {
        uint256 version;
        uint256 feeBps;
        uint256 feeEarnings;
        uint256 minStartingPrice;
        uint256 minBidIncrementBps;
        uint256 auctionCooldown;
        uint256 auctionDuration;
    }

    function getAuctionInfo(uint32 hlChainId, uint256 auctionId) external returns (Auction memory);

    function getBidInfo(uint32 hlChainId, uint256 auctionId, uint256 bidId) external returns (Bid memory);

    function getAuctionHouseInfo() external returns (AuctionHouseInfo memory);

    function isAuctionLive(uint32 hlChainId, uint256 auctionId) external view returns (bool);

    event AuctionCreated(
        uint32 hlChainId,
        uint256 auctionId,
        address auctioneer,
        address nftAddress,
        uint256 tokenId,
        uint256 auctionStartTime,
        uint256 startingBid,
        uint256 referrerNftFeeBps
    );

    event CrossChainBidRefunded(uint32 hlChainId, uint256 remoteBidId, bytes32 messageId);

    function createAuction(
        address nftAddress,
        uint256 tokenId,
        uint256 auctionStartTime,
        uint256 startingBid,
        uint256 referrerNftFeeBps
    ) external returns (uint256);

    function bid(
        uint32 auctionHlChainId,
        uint256 auctionId,
        uint32 bidHlChainId,
        address bidder,
        uint256 bidAmount,
        ReferrerNft calldata referrerNft
    ) external payable;

    event BidPlaced(uint256 auctionId, address bidder, uint256 bidAmount);

    event AuctionResolved(uint256 auctionId, address winner, uint256 winningBidAmount);

    function resolveAuction(uint256 auctionId) external payable;

    function withdrawAuctionHouseFees() external;

    function withdrawReferrerNftFee(address referrerNft, uint256 tokenId) external;

    function remote_CreateAuction(
        uint32 hlChainId,
        uint256 remoteAuctionId,
        address auctioneer,
        address nftAddress,
        uint256 tokenId,
        uint256 auctionStartTime,
        uint256 startingBid,
        uint256 referrerNftFeeBps
    ) external;

    function remote_bid(
        uint32 auctionHlChainId,
        uint256 remoteAuctionId,
        uint32 bidHlChainId,
        uint256 remoteBidId,
        address bidder,
        uint256 bidAmount,
        ReferrerNft calldata referrerNft
    ) external;

    function remote_resolveAuction(uint32 hlChainId, uint256 auctionId) external payable;
}

abstract contract AuctionSetup is INftAuctionHouse {
    function _calculateBps(uint256 _amountInGwei, uint256 _bps) internal pure returns (uint256) {
        if (_amountInGwei == 0 || _bps == 0) {
            return 0;
        }
        require((_amountInGwei * _bps) >= 10_000, "CalculateBps error - amount too small");
        return (_amountInGwei * _bps) / 10_000;
    }
}

