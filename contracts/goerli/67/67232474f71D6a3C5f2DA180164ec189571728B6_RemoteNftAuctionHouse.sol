import {IMailbox} from "@hyperlane-xyz/core/contracts/interfaces/IMailbox.sol";
import {IMessageRecipient} from "@hyperlane-xyz/core/contracts/interfaces/IMessageRecipient.sol";
import {HyperlaneConnectionClient} from "@hyperlane-xyz/core/contracts/HyperlaneConnectionClient.sol";
import {ContextUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import {ERC2771Context} from "@openzeppelin/contracts/metatx/ERC2771Context.sol";
import {IERC20} from "@openzeppelin/contracts/interfaces/IERC20.sol";
import {INftAuctionHouse} from "./interfaces/INftAuctionHouse.sol";
import {IRemoteNftAuctionHouse} from "./interfaces/IRemoteNftAuctionHouse.sol";

contract RemoteNftAuctionHouse is Router, ERC2771Context, IRemoteNftAuctionHouse {
    IERC20 public immutable BASE_CURRENCY;
    uint32 immutable ORIGIN_HLCHAINID;
    uint32 destinationHlChainId;
    address receiverAddress;
    uint256 public auctionMinStartingPrice;
    uint256 auctionHouseFeeEarnings;

    uint256 public lastAuctionId;
    mapping(uint32 => mapping(uint256 => RemoteAuction)) remoteAuction;
    mapping(uint32 => mapping(uint256 => uint256)) lastRemoteBidId;

    mapping(uint32 => mapping(uint256 => mapping(uint256 => RemoteBid))) remoteBid;
    mapping(string => uint256) estGasAmount;
    mapping(address => mapping(uint256 => uint256)) public referrerNftEarnings;

    constructor(
        uint32 _hyperlaneChainId,
        uint32 _destinationHlChainId,
        address _receiverAddress,
        address trustedForwarder,
        address _BASE_CURRENCY
    ) ERC2771Context(trustedForwarder) {
        ORIGIN_HLCHAINID = _hyperlaneChainId;
        destinationHlChainId = _destinationHlChainId;
        receiverAddress = _receiverAddress;
        auctionMinStartingPrice = 0.0001 ether;
        BASE_CURRENCY = IERC20(_BASE_CURRENCY);
    }

    modifier onlySelf() {
        require(_msgSender() == address(this), "ONLY_SELF");
        _;
    }

    modifier onlyReceiver() {
        require(_msgSender() == receiverAddress, "ONLY_RECEIVER");
        _;
    }

    function setAuctionMinStartingPrice(uint256 _auctionMinStartingPrice) external onlyOwner {
        auctionMinStartingPrice = _auctionMinStartingPrice;
    }

    function initialize(address _mailbox, address _interchainGasPaymaster) external initializer {
        __Router_initialize(_mailbox, _interchainGasPaymaster);
    }

    function initialize(address _mailbox, address _interchainGasPaymaster, address _interchainSecurityModule)
        external
        initializer
    {
        __Router_initialize(_mailbox, _interchainGasPaymaster, _interchainSecurityModule);
    }

    function setDestinationHlChainId(uint32 _destinationHlChainId) external onlyOwner {
        destinationHlChainId = _destinationHlChainId;
    }

    function setAuctionHouseReceiver(address _auctionHouseReceiver) external onlyOwner {
        receiverAddress = _auctionHouseReceiver;
    }

    function setEstGasAmount(string memory _functionName, uint256 _estGasAmount) external {
        estGasAmount[_functionName] = _estGasAmount;
    }

    function quoteGasPayment(uint256 _estGasAmount) external view returns (uint256) {
        return interchainGasPaymaster.quoteGasPayment(destinationHlChainId, _estGasAmount);
    }

    function quoteGasPaymentByFunc(string memory _funcName) external view returns (uint256) {
        return interchainGasPaymaster.quoteGasPayment(destinationHlChainId, estGasAmount[_funcName]);
    }

    function _msgSender() internal view virtual override(ContextUpgradeable, ERC2771Context) returns (address sender) {
        if (isTrustedForwarder(msg.sender)) {
            // The assembly code is more direct than the Solidity version using `abi.decode`.
            /// @solidity memory-safe-assembly
            assembly {
                sender := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            return msg.sender;
        }
    }

    function _msgData() internal view virtual override(ContextUpgradeable, ERC2771Context) returns (bytes calldata) {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return msg.data;
        }
    }

    function createAuction(
        address nftAddress,
        uint256 tokenId,
        uint256 auctionStartTime,
        uint256 startingBid,
        uint256 referrerNftFeeBps
    ) external payable returns (bytes32 messageId, uint256 lastAuctionId) {
        require(startingBid >= auctionMinStartingPrice, "STARTING_BID_MUST_BE_GREATER_THAN_MIN_STARTING_PRICE");
        require(referrerNftFeeBps <= 10000, "REFERRER_FEE_BPS_MUST_BE_LESS_THAN_10000");
        IERC721(nftAddress).transferFrom(_msgSender(), address(this), tokenId);

        uint256 gasPayment = interchainGasPaymaster.quoteGasPayment(destinationHlChainId, estGasAmount["createAuction"]);
        require(msg.value >= gasPayment, "Not enough gas");
        lastAuctionId++;
        bytes memory message = abi.encodeWithSelector(
            INftAuctionHouse.remote_CreateAuction.selector,
            ORIGIN_HLCHAINID,
            lastAuctionId,
            _msgSender(),
            nftAddress,
            tokenId,
            auctionStartTime,
            startingBid,
            referrerNftFeeBps
        );
        messageId =
            _dispatchWithGas(destinationHlChainId, message, estGasAmount["createAuction"], gasPayment, _msgSender());
    }

    function bid(
        uint32 auctionHlChainId,
        uint256 auctionId,
        uint256 bidAmount,
        INftAuctionHouse.ReferrerNft calldata referrerNft
    ) external payable returns (bytes32 messageId) {
        require(bidAmount > auctionMinStartingPrice, "BID_AMOUNT_MUST_BE_GREATER_THAN_ZERO");

        BASE_CURRENCY.transferFrom(_msgSender(), address(this), bidAmount);

        uint256 gasPayment = interchainGasPaymaster.quoteGasPayment(destinationHlChainId, estGasAmount["bid"]);
        require(msg.value >= (gasPayment + bidAmount), "Not enough gas");
        // NEED TO FIX SINCE AUCTION ID IS NOT UNIQUE NEED TO INCLUDE CHAIN ID

        lastRemoteBidId[auctionHlChainId][auctionId]++;
        uint256 bidId = lastRemoteBidId[auctionHlChainId][auctionId];
        remoteBid[auctionHlChainId][auctionId][bidId] =
            RemoteBid(ORIGIN_HLCHAINID, _msgSender(), bidAmount, BidStatus.Accepted);

        bytes memory message = abi.encodeWithSelector(
            INftAuctionHouse.remote_bid.selector,
            auctionHlChainId,
            auctionId,
            ORIGIN_HLCHAINID,
            bidId,
            _msgSender(),
            bidAmount,
            referrerNft
        );
        messageId = _dispatchWithGas(destinationHlChainId, message, estGasAmount["bid"], gasPayment, _msgSender());
    }

    function resolveAuction(uint32 auctionHlChainId, uint256 auctionId) external payable returns (bytes32 messageId) {
        uint256 gasPayment =
            interchainGasPaymaster.quoteGasPayment(destinationHlChainId, estGasAmount["resolveAuction"]);
        require(msg.value >= gasPayment, "Not enough gas");

        bytes memory message =
            abi.encodeWithSelector(INftAuctionHouse.remote_resolveAuction.selector, auctionHlChainId, auctionId);
        messageId =
            _dispatchWithGas(destinationHlChainId, message, estGasAmount["resolveAuction"], gasPayment, _msgSender());
        remoteAuction[auctionHlChainId][auctionId].isResolved = true;
    }

    function _handle(uint32 _origin, bytes32 _sender, bytes calldata _message) internal override {
        console.log("RemoteAuctionHouse: _handle");
        (address to, bytes memory callMessage) = abi.decode(_message, (address, bytes));
        (bool success, bytes memory result) = address(to).call(callMessage);
        require(success, "Call failed");
    }

    function resolvePayout(
        address auctioneer,
        uint256 auctioneerAmount,
        address referrerNftAddr,
        uint256 referrerTokenId,
        uint256 referrerNftFee,
        uint256 auctionHouseFee
    ) public {
        console.log("RemoteAuctionHouse: resolvePayout");

        bool success = BASE_CURRENCY.transfer(auctioneer, auctioneerAmount);
        require(success, "AUCTIONEER_TRANSFER_FAILED");

        referrerNftEarnings[referrerNftAddr][referrerTokenId] += referrerNftFee;
        auctionHouseFeeEarnings += auctionHouseFee;
    }

    function withdrawAuctionHouseFees() external onlyOwner {
        BASE_CURRENCY.transfer(owner(), auctionHouseFeeEarnings);
        auctionHouseFeeEarnings = 0;
    }

    function withdrawReferrerNftFee(address referrerNft, uint256 tokenId) external {
        require(IERC721(referrerNft).ownerOf(tokenId) == _msgSender(), "SENDER_MUST_OWN_REFERRER_NFT");
        uint256 amount = referrerNftEarnings[referrerNft][tokenId];
        require(amount > 0, "NO_EARNINGS_FOR_REFERRER_NFT");

        BASE_CURRENCY.transfer(_msgSender(), amount);
        referrerNftEarnings[referrerNft][tokenId] = 0;
    }

    function refundBid(uint32 auctionHlChainId, uint256 auctionId, uint256 bidId, address bidder, uint256 bidAmount)
        external
        onlySelf
    {
        bool success = BASE_CURRENCY.transfer(bidder, bidAmount);
        require(success, "REFUND_FAILED");
        remoteBid[auctionHlChainId][auctionId][bidId].status = BidStatus.Refunded;
    }

    function refundBidForCompleteAuctions(
        uint32 auctionHlChainId,
        uint256 auctionId,
        uint256 bidId,
        address bidder,
        uint256 bidAmount
    ) external {
        require(bidder == _msgSender(), "SENDER_MUST_BE_BIDDER");
        require(remoteAuction[auctionHlChainId][auctionId].isResolved, "AUCTION_NOT_RESOLVED");
        if (remoteBid[auctionHlChainId][auctionId][bidId].status == BidStatus.Refunded) {
            revert("BID_ALREADY_REFUNDED");
        }
        bool success = BASE_CURRENCY.transfer(bidder, bidAmount);
        require(success, "REFUND_FAILED");
        remoteBid[auctionHlChainId][auctionId][bidId].status = BidStatus.Refunded;
    }
}

