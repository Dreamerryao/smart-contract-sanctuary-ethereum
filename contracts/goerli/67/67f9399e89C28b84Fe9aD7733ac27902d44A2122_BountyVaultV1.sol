// Bounty Vault stores bounties. It's a single contract which requires access to token by a user across all programs on a chain.
// Contract becomes owner of bounties and a single address will show all bounties locked up in the Attrace on a certain network.
// The amount of bounty a program has available to distribute is the sum of it's deposited bounties in all vaults across all supported networks.
// Ideally we deploy this as a CREATE2 proxy, so we have a predictable address and don't need the discovery bonanza.
contract BountyVaultV1 is Initializable, ContextUpgradeable, OwnableUpgradeable {
  // The DAO contract
  address public dao;

  // Program balances
  // refhash:token => balance
  mapping(bytes32 => mapping(address => uint256)) balancesERC20;
  mapping(bytes32 => uint256) balancesNative;

  // Valid claim roots
  // h(refHash,cpStart,cpEnd) => claimRoot
  // TODO: see if we need this mapping... might save bytes if we just store a "true value" (to test)
  mapping(bytes32 => bytes32) public claimRoots;

  // Mark payments done so no duplicate payments can happen
  mapping(bytes32 => bool) public bountiesClaimed;

  // Duplicate event next to erc20 so we can build an index on this alone.
  event ERC20BountyDeposited(bytes32 indexed refHash, address token, uint256 value, address from);

  event NativeBountyDeposited(bytes32 indexed refHash, uint256 value, address from);

  // Emitted after challenge period when a claim root is added
  event ClaimRootAdded(bytes32 indexed claimRoot, bytes32 indexed refHash, uint40 cpStart, uint40 cpEnd);

  // Emitted when bounty is distributed from the vault
  // Kind = ascii(B|W|F) (Bounty | Withdraw | Fees) ascii B=66 W=87 F=70
  event Claim(uint8 indexed kind, bytes32 indexed claimRoot, bytes32 indexed leafHash);

  // DEPRECATED: we keep this in the code for reference since events in test have been emitted.
  // event BountiesClaimed(bytes32 indexed claimRoot, address indexed holder, bytes32 payoutHash);

  // Constructor
  function initialize(address dao_) public initializer {
    __Context_init();
    __Ownable_init();

    require(address(dao_) != address(0x0));
    dao = dao_;
  }

  // -- DEPOSIT ERC20

  // Anybody can deposit erc20 bounty for a referral program
  // Requires pre-existing approval from the sender
  function depositERC20Bounty(bytes32 refHash, address tokenAddr, uint256 value, address sender) public onlyValidSender(sender) {
    require(tokenAddr != address(0) && value > 0, "400");

    IERC20(tokenAddr).transferFrom(sender, address(this), value);
    balancesERC20[refHash][tokenAddr] += value;
    emit ERC20BountyDeposited(refHash, tokenAddr, value, sender);
  }

  // -- Public getters
  function getERC20Balance(bytes32 refHash, address tokenAddr) public view returns (uint256) {
    return balancesERC20[refHash][tokenAddr];
  }

  function getNativeBalance(bytes32 refHash) public view returns (uint256) {
    return balancesNative[refHash];
  }

  // -- DEPOSIT NATIVE TOKEN

  // Anybody can deposit native token (ETH/MATIC/...) for a referral program
  function depositNativeBounty(bytes32 refHash, address sender) public payable onlyValidSender(sender) {
    require(msg.value > 0, "400");
    balancesNative[refHash] += msg.value;
    emit NativeBountyDeposited(refHash, msg.value, sender);
  }

  // -- EJECT ERC20
  function ejectERC20Bounty(bytes32 refHash, address tokenAddr) external onlyValidRefHashSenders(refHash) {
    require(tokenAddr != address(0), "400");
    require(balancesERC20[refHash][tokenAddr] > 0, "400");

    uint amout = balancesERC20[refHash][tokenAddr];
    balancesERC20[refHash][tokenAddr] = 0;
    IERC20(tokenAddr).transfer(msg.sender, amout);
  }

  // -- EJECT NATIVE TOKEN
  function ejectNativeBounty(bytes32 refHash) external onlyValidRefHashSenders(refHash) {
    require(balancesNative[refHash] > 0, "400");

    uint amout = balancesNative[refHash];
    balancesNative[refHash] = 0;
    payable(msg.sender).transfer(amout);
  }

  // -- CLAIMS

  function registerClaimRoot(bytes32 refHash, uint40 cpStart, uint40 cpEnd, bytes32 claimRoot) external onlyVerifier {
    claimRoots[keccak256(abi.encode(refHash, cpStart, cpEnd))] = claimRoot;
    emit ClaimRootAdded(claimRoot, refHash, cpStart, cpEnd);
  }

  // Generic release bounties
  function claim(
    uint8 kind, 
    bytes32 refHash, 
    uint40 cpStart, 
    uint40 cpEnd, 
    SenderBountyClaimRequest calldata q, 
    bytes32[] calldata proof
  ) public {
    bytes32 claimRoot = claimRoots[keccak256(abi.encode(refHash, cpStart, cpEnd))];
    require(claimRoot != bytes32(0), "412: claim root not found");

    // Calculate paymentLeaf hash based on input + msg.sender
    bytes32 leafHash = keccak256(abi.encode(kind, q.btHash, msg.sender, cpStart, cpEnd, q.bounties));

    // Verify no duplicate payment
    require(bountiesClaimed[leafHash] != true, "412: already claimed");
    bytes32 computedHash = leafHash;

    // Verify hash against tree
    for (uint256 i = 0; i < proof.length; i++) {
      bytes32 proofElement = proof[i];

      if (computedHash <= proofElement) {
        // Hash(current computed hash + current element of the proof)
        computedHash = keccak256(abi.encodePacked(computedHash, proofElement));
      } else {
        // Hash(current element of the proof + current computed hash)
        computedHash = keccak256(abi.encodePacked(proofElement, computedHash));
      }
    }

    // Check if the computed hash (root) is equal to the provided root
    require(computedHash == claimRoot, "401: invalid proof");

    // Mark payed to avoid re-entrancy
    bountiesClaimed[leafHash] = true;

    // Try paying, fails if insufficient bounty left
    for(uint256 i = 0; i < q.bounties.length; i++) {
      if(q.bounties[i].token == address(0)) {
        // TODO
        revert("500: native");
      } else {
        IERC20(q.bounties[i].token).transfer(msg.sender, q.bounties[i].value);
        balancesERC20[refHash][q.bounties[i].token] -= q.bounties[i].value;
      }
    }

    emit Claim(kind, claimRoot, leafHash);
  }

  // -- MODIFIERS

  modifier onlyVerifier {
    require(DAOStateUpgradeable(dao).addresses(VERIFIER_NETWORK_V2) == msg.sender, "401");
    _;
  }

  // TODO: test 0x0 sender optimization
  modifier onlyValidSender(address sender) {
    // Verify that the sender is depositing on his behalf, or it's the DAO-verified dapp setup helper contract.
    require(
      msg.sender == sender 
      || DAOStateUpgradeable(dao).addresses(DAPP_SETUP_HELPER_V2) == msg.sender
      || DAOStateUpgradeable(dao).addresses(DAPP_SETUP_HELPER) == msg.sender, 
      "401"
    );
    _;
  }

  modifier onlyValidRefHashSenders(bytes32 refHash) {
    require(
      verifyRefHashV1(refHash, msg.sender) == true
      , "401"
    );
    _;
  }
}

// -- ERC721 BOUNTY

// LATER: NFT support
// mapping(bytes32 => mapping(address => uint256[])) balancesERC721;

// // Anybody can transfer in bounty
// function receiveERC721Bounty(address token, uint256 value) public {
//   // TODO IERC721(token).transfer(this, value);
//   // TODO emit BountyDeposited event
// }

struct BountyValue {
  // 0x00..00 is used in token when the native token is used (ETH, MATIC, ...)
  address token;
  uint256 value;
}

struct SenderBountyClaimRequest {
  bytes32 btHash;
  BountyValue[] bounties;
}

