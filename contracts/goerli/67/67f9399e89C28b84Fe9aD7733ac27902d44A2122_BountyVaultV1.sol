pragma solidity ^0.8.6;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./Interfaces.sol";

// The DAOState reference contract
// 
// Lightweight state properties all contracts have access to, which are controlled by the DAO.
// This contract is expected to be evolved over time using upgrades or to be managed by an evolved parent.
//
// **Data structures and methods in this contract are under forward evolution rules**.
// **State storage should happen in native data types**.
//
// Attrace referral contracts are initialized with the DAO address and resolve settings through it.
// 
// Because of gas-efficient design with support for data evolution, 
// the changes applied here are only in effect once listener contracts execute IDAOConfigurable.pullDAOChanges. 
// To avoid listing complexity with gas limits over long-time,  it's deployer's responsibility to update 
// all older contracts (through secondary deploy job). pullDAOChanges should be a public method anybody can call.
contract DAOStateUpgradeable is 
  Initializable, 
  ContextUpgradeable, 
  OwnableUpgradeable
  {
  // Address of the attrace erc20 contract
  address public attr;

  // Helper to indicate this DAO contract is deprecated and has a replacement active.
  // This is just an indication, the replacement is expected to keep this contract in sync.
  // Default: 0x0
  address public replacedBy;

  // Discovery of the active registry
  address public registry;

  // Active verifier contract
  address public verifier;

  // Amount of stake in ATTR required to stake a verifier
  uint96 public verifierStake;

  // Amount of stake required in ATTR for being able to propose a verification (low-bound).
  // Soft slashing due to misbehavior might lower a node's skin in the game and get it removed.
  uint96 public verifierStakeSlashLowBound;

  // Default verifier fraud detection time window for optimistic conversions.
  // Limited to max 194 days.
  uint24 public verifierFraudDetectionMinTime;

  // Address of the tooling contract to allow a more efficient devflow in testnets.
  // Expected that code related to this is removed on mainnet deploys or set to 0x0.
  address public devtools;

  // Generic string-indexed address store, eg: usage dao.addresses("NetworkFeeOracleV1") -> addr
  mapping(string => address) public addresses;

  // Reserved to extend grants defined in code license
  string public licenseCoreAdditionalUseGrants;
  
  // Reserved to override license dates according to code license
  string public licenseCoreLicenseDate;

  event AddressChanged(string setting, address requestor, address verifier);
  event VerifierSettingsChanged(address requestor, uint96 verifierStake, uint96 verifierStakeSlashLowBound, uint24 verifierFraudDetectionMinTime);

  // Input and logging helper struct to pass verifier settings.
  // Should not be used for state storage.
  struct DAOVerifierSettings {
    uint96 verifierStake;
    uint96 verifierStakeSlashLowBound;
    uint24 verifierFraudDetectionMinTime;
  }

  // Initialize the registry
  function initialize(address _attr) public initializer {
    __Context_init();
    __Ownable_init();

    require(address(_attr) != address(0x0));
    attr = _attr;
  }

  // Method to update settings, can be used in develop as well to get faster settings.
  // Broadcasts changes
  function updateVerifierSettings(DAOVerifierSettings calldata settings) public onlyOwner {
    // Update settings if they are set and have changed
    uint changes;
    if(settings.verifierStake > 0 && settings.verifierStake != verifierStake) {
      verifierStake = settings.verifierStake;
      changes++;
    }
    if(settings.verifierStakeSlashLowBound > 0 && settings.verifierStakeSlashLowBound != verifierStakeSlashLowBound) {
      verifierStakeSlashLowBound = settings.verifierStakeSlashLowBound;
      changes++;
    }
    if(settings.verifierFraudDetectionMinTime > 0 && settings.verifierFraudDetectionMinTime != verifierFraudDetectionMinTime) {
      verifierFraudDetectionMinTime = settings.verifierFraudDetectionMinTime;
      changes++;
    }
    if(changes > 0) {
      emit VerifierSettingsChanged(msg.sender, verifierStake, verifierStakeSlashLowBound, verifierFraudDetectionMinTime);
    }
  }

  // Update the verifier contract
  function updateRegistry(address registryContract) public onlyOwner {
    require(registryContract != address(0x0));
    registry = registryContract;
    emit AddressChanged("registry", msg.sender, registryContract);
  }

  // Update the verifier contract
  function updateVerifier(address verifierContract) public onlyOwner {
    require(verifierContract != address(0x0));
    verifier = verifierContract;
    emit AddressChanged("verifier", msg.sender, verifierContract);
  }

  // Update the devtools contract
  function updateDevtools(address devtoolsContract) public onlyOwner {
    require(block.chainid != 1); // Don't allow this on mainnet eth
    devtools = devtoolsContract;
    emit AddressChanged("devtools", msg.sender, devtoolsContract);
  }

  function setAddress(string calldata key, address addr) public onlyOwner {
    addresses[key] = addr;
    emit AddressChanged(key, msg.sender, addr);
  }
}

