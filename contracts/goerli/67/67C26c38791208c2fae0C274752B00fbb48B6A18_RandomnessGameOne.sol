pragma solidity 0.8.13;

import "./QuestSubmissions.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Address.sol";

contract RandomnessGameOne is QuestSubmissions {
    // ================= Custom Errors =================
    error NoRegisteredContract();
    error NotAContract();
    error NotMEVArmyCollector();

    // ================= Custom Events =================

    event YouLived(uint256 score);
    event YouLost();

    address public constant MEVArmyNFTAddress =
        0x422CAe1256b7ADf771cef6Ba3Aebb3b1C63874F2;

    mapping(address => uint256) public score;
    mapping(address => address) public registeredContractToUser;

    address[] public normalWinners;
    address[] public smartContractWinners;

    constructor() {}

    function registerContractPlayer(address _contract) public {
        // ensure MEV Army Collector is only one who can register
        if (IERC721(MEVArmyNFTAddress).balanceOf(msg.sender) == 0)
            revert NotMEVArmyCollector();

        // check if actually registering a contract
        if (!Address.isContract(_contract)) revert NotAContract();

        registeredContractToUser[_contract] = msg.sender;
    }

    function enterDoor() public {
        address currentPlayer = msg.sender;
        bool senderIsContract = Address.isContract(currentPlayer);

        // if current player is a contract, make sure it is registred and replace current player
        if (senderIsContract) {
            if (registeredContractToUser[currentPlayer] == address(0))
                revert NoRegisteredContract();

            // make currentPlayer the EOA that registered the contract for score keeping
            currentPlayer = registeredContractToUser[currentPlayer];
        }

        // ensure current player owns a MEV Army NFT
        if (IERC721(MEVArmyNFTAddress).balanceOf(currentPlayer) == 0)
            revert NotMEVArmyCollector();

        // determine whether player is alive using block number.
        // alive = 0, dead = 1
        uint256 isAlive = block.number % 2;

        uint256 currentScore = score[currentPlayer];

        // reset score if player already won.
        if (currentScore == 3) {
            currentScore = 0;
        }

        // increment score or kill player by setting score to 0
        if (isAlive == 0) {
            currentScore += 1;
            emit YouLived(currentScore);
        } else {
            currentScore = 0;
            emit YouLost();
        }

        // check if player won.
        if (currentScore == 3) {
            // if current msg.sender is a smart contract add player to smart contract winner list
            // otherwise add to normal winner list
            if (senderIsContract) {
                smartContractWinners.push(currentPlayer);
            } else {
                normalWinners.push(currentPlayer);
            }
        }

        // store new user score
        score[currentPlayer] = currentScore;
        addQuestSubmitter(currentPlayer);
    }

    function getNormalWinners() public view returns (address[] memory) {
        return normalWinners;
    }

    function getSmartContractWinners() public view returns (address[] memory) {
        return smartContractWinners;
    }
}

