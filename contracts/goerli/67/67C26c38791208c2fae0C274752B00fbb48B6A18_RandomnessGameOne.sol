pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./IQuestSubmissions.sol";

contract QuestSubmissions is IQuestSubmissions {
    using EnumerableSet for EnumerableSet.AddressSet;

    // ================= Custom Structs =================
    struct FullSubmission {
        address user;
        address contractAddress;
    }

    EnumerableSet.AddressSet internal submitters;

    mapping(address => FullSubmission) public submissions;

    constructor(){}

    function getSubmitterAtIndex(uint256 _index) public view returns (address){
        return submitters.at(_index);
    }

    function addQuestSubmitter(address _submitter) internal {
        submitters.add(_submitter);
    }

    function getTotalQuestSubmitters() public view returns (uint256){
        return submitters.length();
    }

    function getQuestSubmitters()
        external
        view
        override
        returns (address[] memory)
    {
        address [] memory _submitters = new address[](submitters.length());

        for(uint256 i; i < submitters.length(); i++){
            _submitters[i] = submitters.at(i);
        }

        return _submitters;

    }


    function viewFullSubmissions()
        public
        view
        returns (FullSubmission[] memory)
    {
        uint256 totalSubmitters = getTotalQuestSubmitters();
        FullSubmission[] memory subs = new FullSubmission[](totalSubmitters);

        for (uint256 i; i < totalSubmitters; i++) {
            subs[i] = submissions[getSubmitterAtIndex(i)];
        }

        return subs;
    }
}

