pragma solidity ^0.8.7;

//imports
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./NFT/NFT.sol";
import "hardhat/console.sol";

/* Errors */
error nftError_pricMustBeAboveZero();
error nftError_NotApprovedForMarketplace();
error nftError_alreadyListedForSale(
    address nftAddress,
    uint256 tokenId,
    address ownerAddress,
    uint256 marketItemId
);
error nftError_notListed(
    address nftAddress,
    uint256 tokenId,
    uint256 price,
    bool onSale
);
error nftError_notOwner();

error nftError_noAmountAvailable();
error nftError_transferFailed();

/**
 * @title NFT
 * @author Patric&Irfan
 * @notice Decenterlized
 * @dev
 *
 */

contract NFTMarket is ReentrancyGuard {
    using Counters for Counters.Counter;
    Counters.Counter private _marketItemIds;
    Counters.Counter private _tokensSold;
    Counters.Counter private _tokensCanceled;

    struct MarketItem {
        uint256 marketItemId;
        address nftAddress;
        uint256 tokenId;
        address payable creator;
        address payable owner;
        address payable operator;
        uint256 price;
        bool onSale;
        bool canceled;
    }
    /*state variable */
    address payable private s_owner;

    uint256 public test;

    // "address" have many nfts, so catch NFT with the token ID, use double mapping and get the type with price and address of nft
    // mapping(address => mapping(uint => Listing)) private s_listing;
    mapping(uint256 => MarketItem) private s_marketItemIdToMarketItem;

    // nft seller address, and how much money he earned after selling the nft
    mapping(address => uint) private s_sellerAmount;

    /*NFT variable */

    /* Events */
    event nftPriceUpdate(
        address indexed owner,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event nftBought(
        address indexed buyer,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event MarketItemCreated(
        uint256 indexed marketItemId,
        address indexed nftAddress,
        uint256 indexed tokenId,
        address creator,
        address owner,
        address operator,
        uint256 price,
        bool onSale,
        bool canceled
    );

    event MarketItemCanceled(
        address nftAddress,
        uint indexed tokenId,
        address indexed owner,
        address indexed operator,
        bool canceled
    );

    constructor() {
        s_owner = payable(msg.sender);
    }

    /*modifiers*/
    modifier isListed(
        address nftAddress,
        uint256 tokenId,
        uint256 marketItemId
    ) {
        MarketItem memory listing = s_marketItemIdToMarketItem[marketItemId];
        if (listing.onSale == true) {
            revert nftError_alreadyListedForSale(
                nftAddress,
                tokenId,
                msg.sender,
                marketItemId
            );
        }
        _;
    }

    //this will check if the sender is the actual owner of this NFT or not;
    modifier isOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender != owner) {
            revert nftError_notOwner();
        }
        _;
    }

    /*main functions*/
    // function approveNft(address nftAddress, uint tokenId)
    //     public
    //     isOwner(nftAddress, tokenId, msg.sender)
    //     nonReentrant
    // {
    //     IERC721 nft = IERC721(nftAddress);
    //     nft.approve(address(this), tokenId);
    // }

    function marketItemListed(
        address nftAddress,
        uint256 tokenId,
        uint256 price
    ) external isOwner(nftAddress, tokenId, msg.sender) nonReentrant {
        if (price <= 0) {
            revert nftError_pricMustBeAboveZero();
        }
        IERC721 nft = IERC721(nftAddress);
        if (nft.getApproved(tokenId) != address(this)) {
            revert nftError_NotApprovedForMarketplace();
        }
        _marketItemIds.increment();

        uint256 marketItemId = _marketItemIds.current();
        address creator = NFT(nftAddress).getTokenCreatorById(tokenId);

        s_marketItemIdToMarketItem[marketItemId] = MarketItem(
            marketItemId,
            nftAddress,
            tokenId,
            payable(creator),
            payable(msg.sender),
            payable(address(this)),
            price,
            true,
            false
        );
        emit MarketItemCreated(
            marketItemId,
            nftAddress,
            tokenId,
            payable(creator),
            payable(msg.sender),
            payable(address(this)),
            price,
            true,
            false
        );
    }

    function marketItemCanceled(address nftAddress, uint256 marketItemId)
        external
        nonReentrant
    {
        uint256 tokenId = s_marketItemIdToMarketItem[marketItemId].tokenId;
        require(tokenId > 0, "This item is not exist");

        require(
            s_marketItemIdToMarketItem[marketItemId].owner == msg.sender,
            "You are not the Owner"
        );

        s_marketItemIdToMarketItem[marketItemId].owner = payable(msg.sender);
        s_marketItemIdToMarketItem[marketItemId].operator = payable(address(0));
        s_marketItemIdToMarketItem[marketItemId].canceled = true;
        s_marketItemIdToMarketItem[marketItemId].onSale = false;

        IERC721(nftAddress).safeTransferFrom(
            address(this),
            msg.sender,
            tokenId
        );

        _tokensCanceled.increment();

        emit MarketItemCanceled(
            nftAddress,
            tokenId,
            s_marketItemIdToMarketItem[marketItemId].owner,
            s_marketItemIdToMarketItem[marketItemId].operator,
            s_marketItemIdToMarketItem[marketItemId].canceled
        );
    }

    /**
     * @dev Creates a market sale by transfering msg.sender money to the owner and NFT token from the
     * marketplace to the msg.sender. It also sends the listingFee to the marketplace owner.
     */
    function marketItemBought(
        address nftAddress,
        uint256 tokenId,
        uint256 marketItemId
    )
        external
        payable
        nonReentrant
    // notListed(nftAddress, tokenId, marketItemId)
    {
        uint256 price = s_marketItemIdToMarketItem[marketItemId].price;

        require(
            msg.value == price,
            "Please submit the exact price in order to continue"
        );

        // address previousOwner = s_marketItemIdToMarketItem[marketItemId].owner;
        // s_sellerAmount[previousOwner] = s_sellerAmount[previousOwner] + price;

        s_marketItemIdToMarketItem[marketItemId].owner.transfer(msg.value);

        s_marketItemIdToMarketItem[marketItemId].operator = payable(
            (address(0))
        );
        s_marketItemIdToMarketItem[marketItemId].owner = payable(msg.sender);
        s_marketItemIdToMarketItem[marketItemId].onSale = false;

        // this will transfer the NFT value to SellerAmount verialbe, owner can withdraw the amount any time.
        // address previousOwner = s_marketItemIdToMarketItem[marketItemId].owner;
        // s_sellerAmount[previousOwner] = s_sellerAmount[previousOwner] + price;

        IERC721(nftAddress).safeTransferFrom(
            address(this),
            msg.sender,
            tokenId
        );

        emit nftBought(msg.sender, nftAddress, tokenId, price);
        _tokensSold.increment();
    }

    function getAddress() public view returns (address) {
        return address(this);
    }

    /**
     * @dev Get Latest Market Item by the token id
     */
    function getLatestMarketItemByTokenId(uint256 tokenId)
        external
        view
        returns (MarketItem memory, bool)
    {
        uint256 itemsCount = _marketItemIds.current();

        for (uint256 i = itemsCount; i > 0; i--) {
            MarketItem memory item = s_marketItemIdToMarketItem[i];
            if (item.tokenId != tokenId) continue;
            return (item, true);
        }

        MarketItem memory emptyMarketItem;
        return (emptyMarketItem, false);
    }

    /**
     * @dev Fetch non sold and non canceled market items
     */
    function fetchAvailableMarketItems()
        external
        view
        returns (MarketItem[] memory)
    {
        uint256 itemsCount = _marketItemIds.current();
        uint256 soldItemsCount = _tokensSold.current();
        uint256 canceledItemsCount = _tokensCanceled.current();
        uint256 availableItemsCount = itemsCount -
            soldItemsCount -
            canceledItemsCount;
        MarketItem[] memory marketItems = new MarketItem[](availableItemsCount);

        uint256 currentIndex = 0;
        for (uint256 i = 0; i < itemsCount; i++) {
            MarketItem memory item = s_marketItemIdToMarketItem[i + 1];
            if (item.operator != address(0)) {
                marketItems[currentIndex] = item;
                currentIndex += 1;
            }
        }
        return marketItems;
    }

    function updateItemPrice(
        address nftAddress,
        uint256 tokenId,
        uint256 marketItemId,
        uint256 newPrice
    ) external isOwner(nftAddress, tokenId, msg.sender) {
        uint256 NP = s_marketItemIdToMarketItem[marketItemId].price = newPrice;

        emit nftPriceUpdate(msg.sender, nftAddress, tokenId, NP);
    }

    function withdrawAmount() external {
        uint256 amount = s_sellerAmount[msg.sender];
        if (amount <= 0) {
            revert nftError_noAmountAvailable();
        }
        s_sellerAmount[msg.sender] = 0;
        (bool success, ) = payable(msg.sender).call{value: amount}("");

        if (!success) {
            revert nftError_transferFailed();
        }
    }

    function getSellerAmount(address seller) external view returns (uint256) {
        return s_sellerAmount[seller];
    }
}

