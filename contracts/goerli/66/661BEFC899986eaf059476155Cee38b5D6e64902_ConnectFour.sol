pragma solidity ^0.8.9;

/// @title ConnectFour
/// @author Bloomtech
/// @custom:revision 2.0: @zasnicoff, July 2022
/// changed pragma to 0.8.9, added BoardUpdated event to be used in future Sprints
/// @notice Allows any two players to place a 50-50 bet on who will win a game of Connect Four.
/// @notice For info on the rules of Connect Four, see https://en.wikipedia.org/wiki/Connect_Four
/// @dev See the {Game} struct for details on how the board is represented
/// @dev Some variables use the uint8 or uint16 type to make it easier for frontend apps to manipulate return values and events, avoiding BigNumbers type conversions. But this might limit the number of concurrent games or cause overflows.

import "hardhat/console.sol";

contract ConnectFour {
    /// @dev represents a single disc in the Connect Four board
    enum Disc {
        Empty,
        Player1,
        Player2
    }

    /// @dev status of an individual Game
    enum Status {
        NonExistent,
        Initialized,
        Started,
        BetWithdrawn
    }

    /// @dev indicates the direction to check the winning line of 4 discs
    // why is there no Left?
    enum WinningDirection {
        LeftDiagonal,
        Up,
        RightDiagonal,
        Right
    }

    /// @notice struct to represent a Connect Four game between 2 opponents. Each opponent
    /// enters the game by sending the betAmount, so that each game will have a pool of 2 * betAmount
    /// @dev player1 is the address who called ConnectFour.initializeGame, and player2
    /// is the player that called ConnectFour.startGame
    /// @dev each board is comprised of 7 columns, 6 rows, and starts out with each Disc initialized
    /// to Disc.Empty. The board is a single array, and to get the correct disc given a column and row
    /// ID (which are 0-indexed), see ConnectFour.boardIndex. We represent a position in the board as a tuple of
    /// (column, row). The disc (0, 0) is in the bottom left of the board and the disc in the top left of
    /// the board has the coordinates (0, 5) and exists at index 35 in the board array
    ///
    /// See this ASCII grid below for the board and the indexes of different slots
    ///
    /// I filled this in to help me do some of the logic
    /// -------------------------row
    /// |/35/36/37/38/39/40/41/| 5
    /// |/28/29/30/31/32/33/34/| 4
    /// |/21/22/23/24/25/26/27/| 3
    /// |/14/15/16/17/18/19/20/| 2
    /// |/7 /8 /9 /10/11/12/13/| 1
    /// |/0 /1 /2 /3 /4 /5 /6 /| 0
    /// col----------------------

    struct Game {
        address player1; // address of the player that first initialized the game and chose the betAmount
        address player2; // address of the player that started the previously initialized game
        Disc[42] board; // array representing the state of board's discs in a 7 column 6 row grid, at first all are empty
        uint256 betAmount; // number of wei each player bets on the game; the winner will receive 2 * betAmount
        Status status; // various states that denote the lifecycle of a game
        bool isPlayer1Turn; // true if it is player 1's turn, false if it is player 2's turn. Initially it is player 1's turn
    }

    event GameInitialized(uint256 gameId, address player1, uint256 betAmount);

    event GameStarted(uint256 gameId, address player2);

    event RewardClaimed(
        uint256 gameId,
        address winner,
        address recipient,
        uint256 rewardAmount
    );

    /// @dev only the last move idx is sent; frontend logic should keep a record of all the moves or query the blockchain (event filters) to update the local copy of the board. Maybe implement a getter method to return the full board.
    event BoardUpdated(uint256 gameId, address player, uint256 moveIdx); // boardIdx renamed to moveIdx

    /// @notice stores the Game structs for each game, identified by each uint256 game ID
    mapping(uint256 => Game) public games;

    /// @notice the minimum amount of wei that can be bet in a game. Setting a higher value (e.g. 1 ETH) indicates
    /// this is a contract meant for whales only. Set this lower if you want everyone to participate
    uint256 public minBetAmount;

    /// @notice the maximum amount of wei that can be bet in a game. Set this low to ensure people don't lose their shirts :D
    uint256 public maxBetAmount;

    /// @dev A monotonically incrementing counter used for new Game IDs. Starts out at 0, and increments by 1 with every new Game
    uint256 internal gameIdCounter = 0;

    /// @dev A monotonically incrementing counter used for new move IDs. Starts out at 0, and increments by 1 with every new move
    uint256 moveIdx = 0;

    /// @notice Set the minimum and maximum amounts that can be bet on any Games created through
    /// this contract
    /// @dev Increase _minBetAmount if you want to attract degenerates, lower _maxBetAmount to keep them away
    /// @param _minBetAmount the lowest amount a player will be able to bet
    /// @param _maxBetAmount the largest amount a player will be able to bet
    constructor(uint256 _minBetAmount, uint256 _maxBetAmount) {
        minBetAmount = _minBetAmount;
        maxBetAmount = _maxBetAmount;
    }

    /// @notice Create a Game that can be started by any other address. To start a game the caller
    /// must send an ETH amount between the min and max bet amounts
    /// @notice Each game is for a 50/50 bet, so when the caller of this functions sends, say, 1 ETH,
    /// the opponent must send in the same amount of ETH in ConnectFour.startGame
    /// @dev the returned gameId is a monotonically increasing ID used to interact with this new Game
    /// @return a game ID, which can be used by each player to interact with the new Game
    function initializeGame() external payable returns (uint256) {
        // all your require statements
        require(
            msg.value >= minBetAmount && msg.value <= maxBetAmount,
            "ConnectFour: msg.value must be above min and below max"
        );
        // perform any logic
        uint256 gameId = gameIdCounter;
        Disc[42] memory board;
        games[gameId] = Game(
            msg.sender, // player1
            address(0), // player2
            board, // board
            msg.value, // betAmount
            Status.Initialized,
            true
        );

        gameIdCounter++;
        // emit events and make returns
        emit GameInitialized(gameId, msg.sender, msg.value);

        return gameId;
    }

    /// @notice Start a game that has already been initialized by player1. The caller of this function (player2)
    /// must send in the same amount of ETH as player1 sent in. Afterwards the Game has started, and players
    /// may call ConnectFour.playMove to place their discs
    /// @param _gameId the game's ID, returned when player1 called ConnectFour.initializeGame
    function startGame(uint256 _gameId) external payable {
        // require
        require(
            games[_gameId].status == Status.Initialized,
            "ConnectFour: game hasn't started or already exists"
        );
        require(
            games[_gameId].player1 != msg.sender,
            "ConnectFour: can't start the same game you initialized"
        );
        require(
            games[_gameId].betAmount == msg.value,
            "ConnectFour: msg.value must equal game's betAmount"
        );
        // logic
        games[_gameId].status = Status.Started;
        games[_gameId].player2 = msg.sender;

        // emit
        emit GameStarted(_gameId, msg.sender);
    }

    /// @notice Place a disc in the given column with the given Game. player1 and player2 will take
    /// turns placing one of their discs in a column, where it will fall until it stays in the bottom-most
    /// slot or onto the bottom-most previously-placed disc. For more info on how to play Connect Four, see
    /// the wikipedia page https://en.wikipedia.org/wiki/Connect_Four
    /// @dev illegal moves will cause the transaction to revert, such as placing a disc out of bounds of the 7x6
    /// board, trying to place a disc in a column which is already full, or going out of turn
    /// @param _gameId the game's ID, returned when player1 called ConnectFour.initializeGame
    /// @param _col the index of the column to place a disc in, valid values are 0 through 6 inclusive
    function playMove(uint256 _gameId, uint256 _col) external payable {
        // require
        require(
            games[_gameId].status == Status.Started,
            "ConnectFour: game hasn't started, doesn't exist, or is already over"
        );
        require(
            _col >= 0 && _col <= 6,
            "ConnectFour: disc must be placed within the valid range of columns, inclusive (0 thru 6)"
        );

        uint256 topDisc = _col + 35; // get array index matching top disc of submitted _col
        require(
            games[_gameId].board[topDisc] == Disc.Empty,
            "ConnectFour: column submitted is already full. Please try another column"
        );

        // probably a more elegant / less gas intensive way to do this....
        // not sure I can add numbers to get an index within array brackets []
        if (games[_gameId].isPlayer1Turn == true) {
            require( // check isPlayer1Turn bool & revert if msg.sender doesn't match correct player
                games[_gameId].player1 == msg.sender,
                "ConnectFour: it is player1's turn"
            );
            // place disc
            if (games[_gameId].board[_col] == Disc.Empty) {
                games[_gameId].board[_col] = Disc.Player1;
            } else if (games[_gameId].board[7 + _col] == Disc.Empty) {
                games[_gameId].board[7 + _col] = Disc.Player1;
            } else if (games[_gameId].board[14 + _col] == Disc.Empty) {
                games[_gameId].board[14 + _col] = Disc.Player1;
            } else if (games[_gameId].board[21 + _col] == Disc.Empty) {
                games[_gameId].board[21 + _col] = Disc.Player1;
            } else if (games[_gameId].board[28 + _col] == Disc.Empty) {
                games[_gameId].board[28 + _col] = Disc.Player1;
            } else if (games[_gameId].board[35 + _col] == Disc.Empty) {
                games[_gameId].board[35 + _col] = Disc.Player1;
            } else {
                revert(
                    "ConnectFour: shouldn't ever get here, but all discs are full"
                );
            }
            games[_gameId].isPlayer1Turn = false; // flip isPlayer1Turn boolean
        } else {
            // do the same in above if statement, but for player 2
            require(
                games[_gameId].player2 == msg.sender,
                "ConnectFour: it is player2's turn"
            );
            if (games[_gameId].board[_col] == Disc.Empty) {
                games[_gameId].board[_col] = Disc.Player2;
            } else if (games[_gameId].board[7 + _col] == Disc.Empty) {
                games[_gameId].board[7 + _col] = Disc.Player2;
            } else if (games[_gameId].board[14 + _col] == Disc.Empty) {
                games[_gameId].board[14 + _col] = Disc.Player2;
            } else if (games[_gameId].board[21 + _col] == Disc.Empty) {
                games[_gameId].board[21 + _col] = Disc.Player2;
            } else if (games[_gameId].board[28 + _col] == Disc.Empty) {
                games[_gameId].board[28 + _col] = Disc.Player2;
            } else if (games[_gameId].board[35 + _col] == Disc.Empty) {
                games[_gameId].board[35 + _col] = Disc.Player2;
            } else {
                revert(
                    "ConnectFour: shouldn't ever get here, but all discs are full"
                );
            }
            games[_gameId].isPlayer1Turn = true;
        }

        moveIdx++; // increment moveIdx

        // emit events
        emit BoardUpdated(_gameId, msg.sender, moveIdx);
    }

    /// @notice Withdraws the bet amounts of both players to the recipient for the given game when there exists
    /// a winning four-in-a-row of the caller's discs. The caller specifies the four-in-a-row by providing
    /// starting column and row coordinates, as well as a direction in which to look for the 4 winning discs
    /// @dev As an example, imagine there is a winning four-in-a-row at coordinates (0,0), (0,1), (0,2), (0,3).
    /// Then the following function arguments will correctly claim the reward:
    /// _startingWinDiscCol = 0, _startingWinDiscRow = 0, _direction = Up
    /// @dev Note: there exists a vulnerability in this contract that we will exploit in a later Sprint :D
    /// @param _gameId the game's ID, returned when player1 called ConnectFour.initializeGame
    /// @param _recipient the address who will receive the bet's reward ETH
    /// @param _startingWinDiscCol the column index of one of the two end chips of the four-in-a-row
    /// @param _startingWinDiscRow the row index of one of the two end chips of the four-in-a-row
    /// @param _direction one of 4 possible directions in which to move when verifying the four-in-a-row
    function claimReward(
        uint256 _gameId,
        address payable _recipient,
        // address _winner, // initially added _winner because winner must be the only address able to claimReward
        // removing _winner because autograder doesn't like 6 arguments and needs 5. lol
        // now instead of winner calling and sending to an address of their choice,
        // caller of this function must also be the winner and also the recipient of the reward
        uint256 _startingWinDiscCol,
        uint256 _startingWinDiscRow,
        WinningDirection _direction
    ) external {
        address winner;
        // require
        require(
            games[_gameId].status == Status.Started,
            "ConnectFour: game hasn't started yet or doesn't exist"
        );
        require( // ensure startingWinDisc arguments are within bounds of play
            _startingWinDiscCol >= 0 && _startingWinDiscCol <= 6,
            "ConnectFour: _startingWinDiscCol is out of bounds"
        );
        require(
            _startingWinDiscRow >= 0 && _startingWinDiscRow <= 5,
            "ConnectFour: _startingWinDiscRow is out of bounds"
        );

        uint256 startIndex;
        startIndex = boardIndex(_startingWinDiscCol, _startingWinDiscRow);
        if (_direction == WinningDirection.LeftDiagonal) {
            require( // win direction must be in play for all 4 discs
                _startingWinDiscCol >= 3 && _startingWinDiscRow <= 2,
                "ConnectFour: win condition submitted is out of bounds"
            );
            // check win condition
            // I've read loops should perhaps be avoided due to gas so I'm doing it this way
            // loop seems like less code tho
            if (
                games[_gameId].board[startIndex] == Disc.Player1 &&
                games[_gameId].board[startIndex + 6] == Disc.Player1 &&
                games[_gameId].board[startIndex + 12] == Disc.Player1 &&
                games[_gameId].board[startIndex + 18] == Disc.Player1
            ) {
                winner = games[_gameId].player1;
            } else if (
                games[_gameId].board[startIndex] == Disc.Player2 &&
                games[_gameId].board[startIndex + 6] == Disc.Player2 &&
                games[_gameId].board[startIndex + 12] == Disc.Player2 &&
                games[_gameId].board[startIndex + 18] == Disc.Player2
            ) {
                winner = games[_gameId].player2;
            } else {
                revert("ConnectFour: win condition not met");
            }
        } else if (_direction == WinningDirection.Up) {
            require( // win direction must be in play for all 4 discs
                _startingWinDiscRow <= 2,
                "ConnectFour: win condition submitted is out of bounds"
            );
            if (
                games[_gameId].board[startIndex] == Disc.Player1 &&
                games[_gameId].board[startIndex + 7] == Disc.Player1 &&
                games[_gameId].board[startIndex + 14] == Disc.Player1 &&
                games[_gameId].board[startIndex + 21] == Disc.Player1
            ) {
                winner = games[_gameId].player1;
            } else if (
                games[_gameId].board[startIndex] == Disc.Player2 &&
                games[_gameId].board[startIndex + 7] == Disc.Player2 &&
                games[_gameId].board[startIndex + 14] == Disc.Player2 &&
                games[_gameId].board[startIndex + 21] == Disc.Player2
            ) {
                winner = games[_gameId].player2;
            } else {
                revert("ConnectFour: win condition not met");
            }
        } else if (_direction == WinningDirection.RightDiagonal) {
            require( // win direction must be in play for all 4 discs
                _startingWinDiscCol <= 3 && _startingWinDiscRow <= 2,
                "ConnectFour: win condition submitted is out of bounds"
            );
            if (
                games[_gameId].board[startIndex] == Disc.Player1 &&
                games[_gameId].board[startIndex + 8] == Disc.Player1 &&
                games[_gameId].board[startIndex + 16] == Disc.Player1 &&
                games[_gameId].board[startIndex + 24] == Disc.Player1
            ) {
                winner = games[_gameId].player1;
            } else if (
                games[_gameId].board[startIndex] == Disc.Player2 &&
                games[_gameId].board[startIndex + 8] == Disc.Player2 &&
                games[_gameId].board[startIndex + 16] == Disc.Player2 &&
                games[_gameId].board[startIndex + 24] == Disc.Player2
            ) {
                winner = games[_gameId].player2;
            } else {
                revert("ConnectFour: win condition not met");
            }
        } else if (_direction == WinningDirection.Right) {
            require( // win direction must be in play for all 4 discs
                _startingWinDiscCol <= 3,
                "ConnectFour: win condition submitted is out of bounds"
            );
            if (
                games[_gameId].board[startIndex] == Disc.Player1 &&
                games[_gameId].board[startIndex + 1] == Disc.Player1 &&
                games[_gameId].board[startIndex + 2] == Disc.Player1 &&
                games[_gameId].board[startIndex + 3] == Disc.Player1
            ) {
                winner = games[_gameId].player1;
            } else if (
                games[_gameId].board[startIndex] == Disc.Player2 &&
                games[_gameId].board[startIndex + 1] == Disc.Player2 &&
                games[_gameId].board[startIndex + 2] == Disc.Player2 &&
                games[_gameId].board[startIndex + 3] == Disc.Player2
            ) {
                winner = games[_gameId].player2;
            } else {
                revert("ConnectFour: win condition not met");
            }
        } else {
            revert(
                "ConnectFour: invalid direction submitted. Must be LeftDiagonal, Up, RightDiagonal, or Right"
            );
        }

        // require winner == msg.sender (otherwise bot can drain as soon as any game w/ win condition is detected)
        // (this may be the vulnerability alluded to?)
        require(
            winner == msg.sender && winner == _recipient,
            "ConnectFour: only the winner can call function & claim reward"
        );

        // logic
        uint256 rewardAmount = 2 * games[_gameId].betAmount; // calculate rewardAmount
        payable(_recipient).transfer(rewardAmount); // transfer rewardAmount to _recipient (is this all I need?)
        games[_gameId].status = Status.BetWithdrawn; // update game status

        // emits
        emit RewardClaimed(
            _gameId,
            winner, // winner
            _recipient, // recipient
            rewardAmount
        );
    }

    /// @notice Return the index of a disc in the board, given its column and row index (0-indexed)
    /// @dev this function will throw if the column or row are out of bounds
    /// @param _col the index of the column, valid values are 0 through 6 inclusive
    /// @param _row the index of the row, valid values are 0 through 5 inclusive
    /// @return the index of the board corresponding to these coordinates
    function boardIndex(uint256 _col, uint256 _row)
        public
        pure
        returns (uint256)
    {
        // require (may not need this since I'm only calling this function in claimReward and doing this same check there too)
        // if we don't need this, delete these below 2 requires to lower gas consumption
        require(
            _col >= 0 && _col <= 6,
            "ConnectFour: disc must be placed within the valid range of columns, inclusive (0 thru 6)"
        );
        require(
            _row >= 0 && _row <= 5,
            "ConnectFour: disc must be placed within the valid range of rows, inclusive (0 thru 5)"
        );

        // logic
        uint256 index;
        index = (_col * 1) + (_row * 7);

        // return
        return index;
    }
}

