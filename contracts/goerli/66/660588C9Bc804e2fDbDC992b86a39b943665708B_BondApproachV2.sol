pragma solidity ^0.8.0;

import "./BondBase.sol";

/// @title BondApproach2
/// @notice represents second approach to calculate bond amount
contract BondApproachV2 is BondBase {

    /// @dev TODO: include risk/term into calculation

    /// @inheritdoc IBond
    function takerBond(
        address token,
        uint256 amount,
        // solhint-disable-next-line no-unused-vars
        uint16 risk,
        // solhint-disable-next-line no-unused-vars
        uint16 term
    ) public view override returns (uint256 bondAmount) {
        amount = usdToBUMP(assetToUSD(token, amount)) * bondConfig[token].x;
    }

    /// @dev TODO: include risk/term into calculation
    /// @inheritdoc IBond
    function makerBond(
        address marketToken,
        uint256 amount,
        // solhint-disable-next-line no-unused-vars
        uint16 risk,
        // solhint-disable-next-line no-unused-vars
        uint16 term
    ) public view override returns (uint256 bondAmount) {
        uint bumpAmountInUSD = usdToBUMP(amount);
        bondAmount = bumpAmountInUSD * bondConfig[marketToken].y;
    }
}

