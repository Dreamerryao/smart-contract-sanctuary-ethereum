/// @title BondBase
/// @notice Abstract contract that implements storage of tokens
/// @dev Need to write calcAmountToLock function in your implementation
abstract contract BondBase is Initializable, BACUpgradable, IBond {
    using SafeERC20 for IERC20;

    /// @inheritdoc IBond
    address public override bondToken;

    /// @return address of the protocol's configuration
    address public protocolConfig;

    /// @return address of the swap router
    address public swapRouter;

    /// @dev record that represents deposited balance of particular user
    mapping(address => uint256) internal depositedBalances;

    /// @dev record that represents locked balance of particular user
    mapping(address => uint256) internal lockedBalances;

    // TODO: change to weth independent version
    address internal weth;
    uint24 internal wethUsdcFee;
    uint24 internal bumpWethFee;

    /// @notice record that represents bond config of particular market token
    mapping(address => BondConfig) public bondConfig;

    /// @notice set swap router to get BUMP token price
    function setSwapRouter(address router) external onlyGovernance {
        swapRouter = router;
    }

    /// @notice set amount of BUMP tokens for taker and maker positions calculation
    function setBondConfig(address token,  uint x, uint y) external onlyGovernance {
        bondConfig[token].x = x;
        bondConfig[token].y = y;
    }

    /// @notice initializer of this contract
    /// @param _bondToken -    
    /// @param _protocolConfig - protocol configuration address
    function initialize(
        address _bondToken,
        address _protocolConfig,
        address _weth,
        uint24 _wethUsdcFee,
        uint24 _bumpWethFee
    ) external virtual initializer {
        require(_bondToken != address(0), "Bad token");

        bondToken = _bondToken;
        protocolConfig = _protocolConfig;

        weth = _weth;
        wethUsdcFee = _wethUsdcFee;
        bumpWethFee = _bumpWethFee;

        _setGlobalAccessController( IProtocolConfig(protocolConfig).getGAC() );
    }

    /// @inheritdoc	IBond
    function deposit(uint256 amount) external virtual override {
        require(amount > 0, "Bad amount");

        depositedBalances[_msgSender()] += amount;

        IERC20(bondToken).safeTransferFrom(_msgSender(), address(this), amount);
    }

    /// @inheritdoc	IBond
    function withdraw(uint256 amount) external virtual override {
        require(
            amount > 0 && depositedBalances[_msgSender()] - amount >= 0,
            "Bad amount"
        );

        depositedBalances[_msgSender()] -= amount;

        IERC20(bondToken).safeTransfer(_msgSender(), amount);
    }

    /// @inheritdoc IBond
    function takerLock(
        address _owner,
        address token,
        uint256 amount,
        uint16 risk,
        uint16 term
    ) external virtual override onlyMarket(token) returns (uint256 bondAmount) {
        bondAmount = takerBond(token, amount, risk, term);
        require(depositedBalances[_owner] >= amount, "Not enough balance");

        depositedBalances[_owner] -= amount;
        lockedBalances[_owner] += amount;
    }

    /// @inheritdoc IBond
    function makerLock(
        address _owner,
        address marketToken,
        uint256 amount,
        uint16 risk,
        uint16 term
    )
        external
        virtual
        override
        onlyMarket(marketToken)
        returns (uint256 bondAmount)
    {
        bondAmount = amount = makerBond(marketToken, amount, risk, term);
        require(depositedBalances[_owner] >= amount, "Not enough balance");

        depositedBalances[_owner] -= amount;
        lockedBalances[_owner] += amount;
    }

    /// @inheritdoc IBond
    function unlock(
        address _owner,        
        address marketToken,
        uint256 bondAmount
    ) external virtual override onlyMarket(marketToken) {
        require(lockedBalances[_owner] >= bondAmount, "Bad amount");
        lockedBalances[_owner] -= bondAmount;
        depositedBalances[_owner] += bondAmount;
    }

    /// @inheritdoc IBond
    function takerBond(
        address token,
        uint256 amount,
        uint16 risk,
        uint16 term
    ) public view virtual override returns (uint256 bondAmount);

    /// @inheritdoc IBond
    function makerBond(
        address marketToken,
        uint256 amount,
        uint16 risk,
        uint16 term
    ) public view virtual override returns (uint256 bondAmount);

    /// @inheritdoc	IBond
    function balanceOf(address addr)
        external
        view
        virtual
        override
        returns (uint256 amount)
    {
        amount = depositedBalances[addr];
    }

    /// @inheritdoc	IBond
    function lockedOf(address addr)
        external
        view
        virtual
        override
        returns (uint256 amount)
    {
        amount = lockedBalances[addr];
    }

    /// @param token - address of asset token
    /// @param amount - amount of asset tokens to convert
    /// @return amountInUSD - amount of converted tokens
    function assetToUSD(address token, uint256 amount)
        internal
        view
        returns (uint256 amountInUSD)
    {
        (int256 price, ) = IMarket(
            IProtocolConfig(protocolConfig).getMarket(token)
        ).price();
        amountInUSD = uint256(price) * amount;
    }

    /// @param amount - amount of usd tokens to convert
    /// @return amountInBUMP - amount of converted tokens
    function usdToBUMP(uint256 amount)
        internal
        view
        returns (uint256 amountInBUMP)
    {

        return amount / 200; // TEST

        /*(bool success, bytes memory data) = swapRouter.staticcall(
            abi.encodeWithSelector(
                ISwap.swapExactTokensForTokensMult.selector,
                abi.encodePacked(
                    IProtocolConfig(protocolConfig).getStable(),
                    wethUsdcFee,
                    weth,
                    bumpWethFee,
                    bondToken
                ),
                amount,
                0
            )
        );

        require(success, "Price call failed");

        amountInBUMP = abi.decode(data, (uint256)); */
    }

    /// @notice requires msg.sender to be market contract
    /// @param token - address of market token
    modifier onlyMarket(address token) {
        require(
            IProtocolConfig(protocolConfig).getMarket(token) == msg.sender,
            "!market"
        );
        _;
    }
}

