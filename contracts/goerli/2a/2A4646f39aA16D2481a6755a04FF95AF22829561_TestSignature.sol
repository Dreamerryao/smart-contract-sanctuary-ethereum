error InvalidSignature();
error InvalidNonce(uint256 invalidNonce);
error InsufficientReward(uint256 currentRewardAmt);
error InvalidRewardTokenAddress(address invalidTokenAddress);

contract TestSignature is IRewardDistributor, Ownable, EIP712 {
    address public validator;

    bytes32 public TEST_TYPEHASH = keccak256("Test");

    constructor() {
        _initializeOwner(msg.sender);
    }

    /* -------------------------------------------------------------------------- */
    /*                               VIEW OPERATIONS                              */
    /* -------------------------------------------------------------------------- */
    function hashTypedData(bytes32 structHash) external view returns (bytes32) {
        return _hashTypedData(structHash);
    }

    function DOMAIN_SEPARATOR() external view returns (bytes32) {
        return _domainSeparator();
    }

    /* -------------------------------------------------------------------------- */
    /*                              WRITE OPERATIONS                              */
    /* -------------------------------------------------------------------------- */
    /**
     * @dev Set validator address
     */
    function setValidator(address _validator) external onlyOwner {
        validator = _validator;
    }

    function claim(bytes calldata signature) public {
        _validateClaimRequest(signature);
    }

    /* -------------------------------------------------------------------------- */
    /*                                  INTERNAL                                  */
    /* -------------------------------------------------------------------------- */
    function _domainNameAndVersion() internal pure override returns (string memory name, string memory version) {
        name = "MMLM";
        version = "1";
    }

    function _validateClaimRequest(bytes calldata signature) private {
        bytes32 structHash = keccak256(abi.encode(TEST_TYPEHASH));
        bytes32 digest = _hashTypedData(structHash);
        bool isValid = SignatureCheckerLib.isValidSignatureNowCalldata(validator, digest, signature);
        if (!isValid) {
            revert InvalidSignature();
        }
    }

    receive() external payable { }
}

