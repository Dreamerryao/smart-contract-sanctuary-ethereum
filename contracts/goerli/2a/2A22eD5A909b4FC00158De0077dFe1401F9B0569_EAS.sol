pragma solidity 0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";

import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";

import "./interfaces/IOwnable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/IERC1155MetadataURI.sol";

contract EASCollectionManager is Ownable {
    uint256 public collectionId = 1;

    mapping(uint256 => Collection) internal collections;

    mapping(address => address) internal setOwnerOfCollection;

    struct Collection {
        address contractAddress;
        string baseUri;
    }

    function isOwnerOfCollection(address contractAddress) internal view returns (bool) {
        if (setOwnerOfCollection[contractAddress] == msg.sender) return true;

        if (isOwnableCompatible(contractAddress)) {
            IOwnable ownable = IOwnable(contractAddress);
            if (ownable.owner() == msg.sender) return true;
        }

        return false;
    }

    function isOwnableCompatible(address contractAddress) internal view returns (bool) {
        if (
            ERC165Checker.supportsInterface(contractAddress, type(IERC721Metadata).interfaceId) ||
            ERC165Checker.supportsInterface(contractAddress, type(IERC721).interfaceId) ||
            ERC165Checker.supportsInterface(contractAddress, type(IERC1155).interfaceId)
        ) return true;
        else return false;
    }

    function registerCollection(address contractAddress, string memory _baseUri) external {
        require(isOwnerOfCollection(contractAddress), "You are not the owner of this collection");

        collections[collectionId].baseUri = _baseUri;
        collections[collectionId].contractAddress = contractAddress;

        collectionId++;

        emit CollectionRegistered(contractAddress, _baseUri);
    }

    function setCollectionOwner(address owner, address collectionAddress) external onlyOwner {
        setOwnerOfCollection[collectionAddress] = owner;
    }

    function revokeSetCollectionOwner(address collectionAddress) external onlyOwner {
        setOwnerOfCollection[collectionAddress] = address(0x0);
    }

    function getBaseUri(uint256 _collectionId) external view returns (string memory) {
        return collections[_collectionId].baseUri;
    }

    function updateBaseUri(string memory _baseUri, uint256 _collectionId) external onlyOwner {
        collections[_collectionId].baseUri = _baseUri;
    }

    event CollectionRegistered(address contractAddress, string baseUri);
}

