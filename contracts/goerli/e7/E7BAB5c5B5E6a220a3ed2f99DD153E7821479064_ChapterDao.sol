contract SchoolDao is ERC721PsiBurnable, ReentrancyGuard, Ownable {
    constructor(
        string memory name_,
        string memory symbol_,
        string memory uri_
    ) {
        _name = name_;
        _symbol = symbol_;
        _baseURILink = uri_;
    }

    mapping(uint256 => address) Ambassadors;
    mapping(address => uint256) AmbassadorsReverse;
    address public factory;

    function setFactory(address _factory) external onlyOwner {
        factory = _factory;
    }

    modifier onlyFactory() {
        require(
            msg.sender == factory || msg.sender == owner(),
            "SchoolDao: FORBIDDEN"
        );

        _;
    }

    function mint(address to, address chapterDao)
        external
        onlyFactory
        returns (uint256)
    {
        uint256 tokenId = _totalMinted() + 1;
        Ambassadors[tokenId] = chapterDao;
        AmbassadorsReverse[chapterDao] = tokenId;
        _safeMint(to, 1);
        return tokenId;
    }

    /**
