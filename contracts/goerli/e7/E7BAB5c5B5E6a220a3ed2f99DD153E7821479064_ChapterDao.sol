            revert UnAuthorizedBlackListed(_msgSender());
        if (!_exists(_tokenId)) revert NonexistentToken(_tokenId);
        if (!_isApprovedOrOwner(_msgSender(), _tokenId))
            revert IsNotApprovedOrOwner();

        _burn(_tokenId);
    }

    function adminBurn(uint256 _tokenId) public virtual onlyAmbassador {
        if (!_exists(_tokenId)) revert NonexistentToken(_tokenId);
        _burn(_tokenId);
        emit AdminBurn(_tokenId, block.number);
    }

    function emergancyWithdraw() external onlyAmbassador {
        payable(_msgSender()).sendValue(address(this).balance);
    }

    function recoverERC20(address tokenAddress, uint256 tokenAmount)
        public
        virtual
        onlyAmbassador
    {
        IERC20(tokenAddress).transfer(_msgSender(), tokenAmount);
    }

    function recoverERC721(address tokenAddress, uint256 tokenID)
        public
        virtual
        onlyAmbassador
    {
        IERC721(tokenAddress).safeTransferFrom(
            address(this),
            _msgSender(),
            tokenID
        );
    }
}

