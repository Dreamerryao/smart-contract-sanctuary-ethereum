        _baseURILink = baseUri_;
    }

    function burn(uint256 tokenId) external onlyOwner {
        _burn(tokenId);
    }

    function getDao(uint256 tokenId) external view returns (address) {
        return Ambassadors[tokenId];
    }

    function isAmbassador(address _ambassador, address _chapter)
        external
        view
        returns (bool)
    {
        uint256 chapterId = AmbassadorsReverse[_chapter];
        if (ownerOf(chapterId) == _ambassador) {
            return true;
        } else {
            return false;
        }
    }

    function getAmbassadorAddress(address chapterDao)
        external
        view
        returns (address)
    {
        return ownerOf(AmbassadorsReverse[chapterDao]);
    }

    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 tokenId,
        uint256 amount
    ) internal override {
        super._beforeTokenTransfers(from, to, tokenId, amount);
    }

    function _afterTokenTransfers(
        address from,
        address to,
        uint256 tokenId,
        uint256 amount
    ) internal override {
        super._afterTokenTransfers(from, to, tokenId, amount);
    }
}

