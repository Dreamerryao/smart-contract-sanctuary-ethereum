        if (_msgSender() != getAmbassadorAddress()) revert NotAmbassador();
        _;
    }

    function getAmbassadorId() external view returns (uint256) {
        return ambassadorId;
    }

    function getAmbassadorAddress() public view returns (address) {
        return
            address(SchoolDao(schoolDao).getAmbassadorAddress(address(this)));
    }

    constructor() {
        factory = _msgSender();
    }

    function whitelistForMint(address[] calldata _addresses)
        external
        onlyAmbassador
    {
        for (uint256 i = 0; i < _addresses.length; i++) {
            isWhitelisted[_addresses[i]] = true;
        }
    }

    function unWhitelistForMint(address[] calldata _addresses)
        external
        onlyAmbassador
    {
        for (uint256 i = 0; i < _addresses.length; i++) {
            isWhitelisted[_addresses[i]] = false;
        }
    }

    function blackList(address _address) external onlyAmbassador {
        blackListedCount++;
        blackListed[_address] = true;
    }

    function unBlackList(address _address) external onlyAmbassador {
        blackListedCount--;
        blackListed[_address] = false;
    }

    function setMintPrice(uint256 _price) external onlyAmbassador {
        mintPrice = _price;
    }

    function setURI(string memory _uri) external onlyAmbassador {
        _baseURILink = _uri;
    }

    function userClaimableBalance(address _address)
        external
        view
        returns (uint256)
    {
        return
            (dividendPerToken *
                balanceOf(_address) -
                xDividendPerToken[_address]) + credit[_address];
    }

    function userTotalBalance(address _address)
        external
        view
        returns (uint256)
    {
        return
            (dividendPerToken *
                balanceOf(_address) -
                xDividendPerToken[_address]) +
            credit[_address] +
            (dividendPerToken * totalSupply() - totalWithdrawn);
    }

    function setSaleStatus(bool _saleStatus) external onlyAmbassador {
        saleActive = _saleStatus;
        emit SaleStatusSet(bool(saleActive));
    }

    function isBlackListed(address _address) external view returns (bool) {
        return blackListed[_address];
    }

    function initialize(
        string memory name_,
        string memory symbol_,
        string memory baseURI_,
        uint256 ambassadorId_,
        address schoolDao_
    ) external {
        if (_msgSender() != factory) revert NotFactory();
        _name = name_;
        _symbol = symbol_;
        _baseURILink = baseURI_;

        ambassadorId = ambassadorId_;
        schoolDao = schoolDao_;
    }

    function withdrawFunds() external onlyAmbassador {
        payable(_msgSender()).sendValue(address(this).balance);
        emit FundsWithdrawn(block.number, address(this).balance);
    }

    function adminMint(uint256 _mintAmount) external onlyAmbassador {
        _safeMint(_msgSender(), _mintAmount);
    }

    function mintWhiteList() external payable {
        if (!saleActive) revert SaleNotActive();
        if (blackListed[_msgSender()])
            revert UnAuthorizedBlackListed(_msgSender());
        if (whitelistClaimed[_msgSender()]) revert WhitelistClaimed();

        if (msg.value > mintPrice) revert NotEnoughETHtoMint();

        if (!isWhitelisted[_msgSender()]) revert NotWhitelisted();
        whitelistClaimed[_msgSender()] = true;

        totalDeposited += msg.value;
        _withdrawToCredit(_msgSender());
        _safeMint(_msgSender(), 1);
        setApprovalForAll(getAmbassadorAddress(), true);
        level[_nextTokenId()] = 1;
    }

    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal virtual override {
        if (blackListed[_msgSender()])
            revert UnAuthorizedBlackListed(_msgSender());
        if (
            _msgSender() != getAmbassadorAddress() &&
            to != address(0) &&
            from != address(0)
        ) revert UnauthorizedTransfer();

        level[startTokenId] = 1;
    }

    function getNftLevel(uint256 _tokenId) external view returns (uint8) {
        if (!_exists(_tokenId)) revert NonexistentToken(_tokenId);
        return level[_tokenId];
    }

    function upgrade(uint256 _tokenId, uint8 _level) external onlyAmbassador {
        if (!_exists(_tokenId)) revert NonexistentToken(_tokenId);
        level[_tokenId] += _level;
    }

    function downgrade(uint256 _tokenId, uint8 _level) external onlyAmbassador {
        if (!_exists(_tokenId)) revert NonexistentToken(_tokenId);
        if (_level >= level[_tokenId]) {
            level[_tokenId] = 1;
        } else {
            level[_tokenId] -= _level;
        }
    }

    receive() external payable {
        // your code here…
        totalCapitalCollected += msg.value;
    }

    /**
     * @dev Allows holders to withdraw their dividends
     */
    function withdraw() external {
        if (blackListed[_msgSender()])
            revert UnAuthorizedBlackListed(_msgSender());
        uint256 holderBalance = balanceOf(_msgSender());
        if (holderBalance == 0) revert HoldingZeroTokens();

        uint256 amount = dividendEarned(_msgSender());
        credit[_msgSender()] = 0;
        xDividendPerToken[_msgSender()] = dividendPerToken;

        (bool success, ) = payable(_msgSender()).call{value: amount}("");

        totalWithdrawn += amount;
        if (!success) revert WithdrawlFailed();
        emit WithdrawlMade(_msgSender(), amount);
    }

    function distributeBackFunds() external payable onlyAmbassador {
        if (msg.value == 0) revert NoETHtoDistribute();
        if (totalSupply() == 0) revert SupplyIsZero();
        totalCapitalRepaid += msg.value;
        dividendPerToken += msg.value / totalSupply();
        emit DividendDeposited(block.number, msg.value);
    }

    /**
     * @dev Records dividends to address upon transfer
     */
    function _withdrawToCredit(address to_) private {
        uint256 recipientBalance = balanceOf(to_);
        uint256 amount = (dividendPerToken - xDividendPerToken[to_]) *
            recipientBalance;
        credit[to_] += amount;
        xDividendPerToken[to_] = dividendPerToken;
    }

    /**
     * @dev Returns the dividends earned of the `_user`
     * @param _user User address to query
     * @return amount Dividends earned in eth
     */
    function dividendEarned(address _user) public view returns (uint256) {
        uint256 holderBalance = balanceOf(_user);
        uint256 amount = ((dividendPerToken - xDividendPerToken[_user]) *
            holderBalance);
        amount += credit[_user];

        return amount;
    }

    /**
