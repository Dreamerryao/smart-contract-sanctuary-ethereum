import "hardhat/console.sol";
import { VaultInterface } from "contracts/interfaces/VaultInterface.sol";
import {
    ERC20Upgradeable
} from "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import {
    IERC20Upgradeable
} from "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";

import {
    SafeERC20Upgradeable
} from "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

import {
    ReentrancyGuardUpgradeable
} from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "../interfaces/VaultInterface.sol";

/**
 * @title Plastique Vault
 * @author Abderrahmen Hanafi, Arnab Ray
 */
contract PlastiqueVault is VaultInterface, ERC20Upgradeable, ReentrancyGuardUpgradeable {
    // underlying ERC20 asset
    IERC20Upgradeable private _underline;

    // emits currentPrice of the LP tokens when ever there is a deposit or redeem happening
    event UpdatedPrice (uint256 indexed _currentPrice);

    // Novel fee parameters
    uint256 private _entryFeeParameter;
    uint256 private _exitFeeParameter;

    // Protocol fee percentage denominated in ether (10*18)
    uint256 private _protocolEntryFeePercentage;
    uint256 private _protocolExitFeePercentage;

    // Protocol fee receiver
    address public treasury;

    /**
     * @notice Callable one only (initializer)
     * @dev    Initialize the vault
     * @param  underlineAsset ERC20 asset contract address
     * @param  vaultTokenName The vault name as per ERC20 standard
     * @param  vaultTokenSymbol The vault symbol as per ERC20 standard
     * @param  entryFeeParameterInEther The novel entry fee parameter denominated in ether (10**18)
     * @param  exitFeeParameterInEther The novel exit fee parameter denominated in ether (10**18)
     * @param  protocolEntryFeePercentageInEther The protocol entry fee percentage denominated in ether (10**18)
     * @param  protocolExitFeePercentageInEther The protocol exit fee percentage denominated in ether (10**18)
     * @param  protocolFeeRecipient The protocol fees receiver address (Fee is sent in vault ERC20 asset)
     * @param  underlyingSeedAmount The initial liquidity amount provided by the {creator}
     * @param  creator The vault initial liquidity provided
     */
    function setup(
        address underlineAsset,
        string memory vaultTokenName,
        string memory vaultTokenSymbol,
        uint256 entryFeeParameterInEther,
        uint256 exitFeeParameterInEther,
        uint256 protocolEntryFeePercentageInEther,
        uint256 protocolExitFeePercentageInEther,
        address protocolFeeRecipient,
        uint256 underlyingSeedAmount,
        address creator
    ) external initializer {
        require(underlyingSeedAmount > 0, "Vault: nullish seed");
        require(protocolFeeRecipient != address(0), "Vault: nullish treasury target");

        _underline = IERC20Upgradeable(underlineAsset);
        __ERC20_init(vaultTokenName, vaultTokenSymbol);
        __ReentrancyGuard_init();

        // Fee parameters
        _entryFeeParameter = entryFeeParameterInEther;
        _exitFeeParameter = exitFeeParameterInEther;

        // protocol Fee
        _protocolEntryFeePercentage = protocolEntryFeePercentageInEther;
        _protocolExitFeePercentage = protocolExitFeePercentageInEther;
        treasury = protocolFeeRecipient;

        // Seed initialization
        SafeERC20Upgradeable.safeTransferFrom(
            _underline,
            creator,
            address(this),
            underlyingSeedAmount
        );
        _mint(address(this), underlyingSeedAmount);
    }

    /**
     * @notice Return the protocol entry fee numerator and denominator
     * @return numerator The protocol entry fee numerator
     * @return denominator The protocol entry fee denominator
     */
    function getProtocolEntryFee() external view returns (uint256 numerator, uint256 denominator) {
        return (_protocolEntryFeePercentage, 1 ether);
    }

    /**
     * @notice Return the protocol exit fee numerator and denominator
     * @return numerator The protocol exit fee numerator
     * @return denominator The protocol exit fee denominator
     */
    function getProtocolExitFee() external view returns (uint256 numerator, uint256 denominator) {
        return (_protocolExitFeePercentage, 1 ether);
    }

    /**
     * @notice Return the entry fee parameter numerator and denominator
     * @return numerator The entry fee parameter numerator
     * @return denominator The entry fee parameter denominator
     */
    function getEntryFeeParameter() external view returns (uint256 numerator, uint256 denominator) {
        return (_entryFeeParameter, 1 ether);
    }

    /**
     * @notice Return the exit fee parameter numerator and denominator
     * @return numerator The exit fee parameter numerator
     * @return denominator The exit fee parameter denominator
     */
    function getExitFeeParameter() external view returns (uint256 numerator, uint256 denominator) {
        return (_exitFeeParameter, 1 ether);
    }

    /**
     * @notice Returns the address of the underlying token used for the Vault for accounting,
     *         depositing, and withdrawing.
     */
    function asset() external view returns (address assetTokenAddress) {
        return address(_underline);
    }

    /**
     * @notice Returns the total amount of the underlying asset that is “managed” by Vault.
     */
    function totalAssets() external view returns (uint256 totalManagedAssets) {
        return _totalAssets();
    }

    /**
     * @notice Return the vault current price (underlying Supply / Vault LP Supply ) denominated in ether
     */
    function currentPrice() external view returns (uint256) {
        return _currentPrice();
    }

    /**
     * @notice Returns the amount of shares that the Vault mint excluding fees.
     * @dev    Uses _convertToShares implementation to achieve the calculation
     *         - MUST exclude protocol fee
     *         - MUST include entry fee parameter as it's part of the novel vault system
     *         - MUST uses the current price
     * @param assets The underlying ERC20 asset amount
     * @return shares The expected vault Shares amount
     */
    function convertToShares(uint256 assets) external view returns (uint256 shares) {
        shares = _convertToShares(assets, _currentPrice(), _entryFeeParameter);
    }

    /**
     * @notice Returns the amount of shares that the Vault Redeem in a determined vault state
     * @dev    Forecast share amount with a custom vault state
     *         - MUST be pure calculation and does't rely on vault state
     *         - MUST exclude protocol fee
     *         - MUST include entry fee parameter as it's part of the novel vault system
     *         - MUST uses custom price in calculation
     *         - MUST uses custom entry fee parameter in calculation
     * @param assets The underlying ERC20 asset amount
     * @param price  The vault price numerated in ether
     * @param entryFeeParameter  The vault entry fee parameter numerated in ether
     * @return shares The expected vault Shares amount
     */
    function convertToSharesForecast(
        uint256 assets,
        uint256 price,
        uint256 entryFeeParameter
    ) external pure returns (uint256 shares) {
        shares = _convertToShares(assets, price, entryFeeParameter);
    }

    /**
     * @notice Returns the amount of assets that the Vault Redeem excluding fees.
     * @dev    Uses _convertToAssets implementation to achieve the calculation
     *         - MUST exclude protocol fee
     *         - MUST include entry fee parameter as it's part of the novel vault system
     *         - MUST uses the current price
     * @param shares The vault ERC20 shares amount
     * @return assets The expected underlying ERC20 assets amount
     */
    function convertToAssets(uint256 shares) external view returns (uint256 assets) {
        assets = _convertToAssets(shares, _currentPrice(), _totalAssets(), _exitFeeParameter);
    }

    /**
     * @notice Returns the amount of assets that the Vault Redeem in a determined vault state
     * @dev    Forecast assets amount with a custom vault state
     *         - MUST be pure calculation and does't rely on vault state
     *         - MUST exclude protocol fee
     *         - MUST include entry fee parameter as it's part of the novel vault system
     *         - MUST uses custom price in calculation
     *         - MUST uses custom entry fee parameter in calculation
     * @param shares The vault ERC20 shares amount
     * @param price  The vault price numerated in ether
     * @param totalAsset The vault balance of underlying ERC20 asset
     * @param exitFeeParameter  The vault exit fee parameter numerated in ether
     * @return assets The expected underlying ERC20 assets amount
     */
    function convertToAssetsForecast(
        uint256 shares,
        uint256 price,
        uint256 totalAsset,
        uint256 exitFeeParameter
    ) external pure returns (uint256 assets) {
        assets = _convertToAssets(shares, price, totalAsset, exitFeeParameter);
    }

    /**
     * @notice Return the maximum amount that can be redeemed
     * @dev    Return the user vault shares balance
     * @param  owner The owner
     * @return maxShares The owner balance
     */
    function maxRedeem(address owner) external view returns (uint256 maxShares) {
        return balanceOf(owner);
    }

    /**
     * @notice Return the maximum amount that can be deposited (no limits for this vault)
     * @dev    Return maximum value in uint256
     */

    function maxDeposit(
        address /*receiver*/
    ) external pure returns (uint256) {
        return type(uint256).max;
    }

    /**
     * @notice Preview the deposit transaction, returned shares is what depositor should receive
     * @dev    Allows an on-chain or off-chain user to simulate the effects of
     *         their deposit at the current block, given current on-chain conditions.
     *         - MUST include protocol fee deduction
     *         - MUST return as close to and no more than the exact amount of Vault shares that
     *           would be minted in a deposit call in the same transaction.
     *           I.e. {deposit} should return the same or more shares as {previewDeposit} if called
     *           in the same transaction.
     *         - MUST NOT account for deposit limits like those returned from maxDeposit and
     *           should always act as though the deposit would be accepted,
     *           regardless if the user has enough tokens approved, etc.
     *         - MUST NOT revert.
     * @param assets The underlying ERC20 assets amount
     * @return shares The expected vault Shares amount
     */
    function previewDeposit(uint256 assets) external view returns (uint256 shares) {
        uint256 protocolFeeAmount = (assets * _protocolEntryFeePercentage) / 1 ether;
        uint256 netAssetsAmount = assets - protocolFeeAmount;
        shares = _convertToShares(netAssetsAmount, _currentPrice(), _entryFeeParameter);
    }

    /**
     * @notice Deposit underlying ERC20 assets and mint Vault shares to a receiver
     * @dev    Mints Vault shares to receiver by depositing exactly amount of underlying tokens.
     *         - MUST emit the Deposit event.
     *         - MUST revert if all of assets cannot be deposited
     * @param assets The underlying ERC20 assets amount
     * @param receiver The minted Vault shares beneficiary
     * @return shares The expected vault Shares amount
     */
    function deposit(uint256 assets, address receiver)
        external
        nonReentrant
        returns (uint256 shares)
    {
        return _deposit(assets, receiver, 0);
    }

    /**
     * @notice Deposit underlying ERC20 assets and mint at least minimum amount
     *         of Vault shares to a receiver (EOA must use this in favor of regular deposit)
     * @dev    Mints Vault shares to receiver by depositing exactly amount of underlying tokens.
     *         - MUST emit the Deposit event.
     *         - MUST revert if all of assets cannot be deposited
     *         - MUST revert if shares are less than minOut (slippage)
     * @param assets The underlying ERC20 assets amount
     * @param receiver The minted Vault shares beneficiary
     * @param minOut The minimum shares amount the depositor accepting
     * @return shares The expected vault Shares amount
     */
    function deposit(
        uint256 assets,
        address receiver,
        uint256 minOut
    ) external nonReentrant returns (uint256 shares) {
        return _deposit(assets, receiver, minOut);
    }

    /**
     * @notice Preview the redeem transaction, returned assets amount is what redeemer should receive
     * @dev    Allows an on-chain or off-chain user to simulate the effects of their redemption
     *         at the current block, given current on-chain conditions.
     *         - MUST return as close to and no more than the exact amount of assets
     *           that would be withdrawn in a redeem call in the same transaction.
     *           I.e. redeem should return the same or more assets as previewRedeem if called in the
     *           same transaction.
     *         - MUST NOT account for redemption limits like those returned from maxRedeem and should
     *           always act as though the redemption would be accepted,
     *           regardless if the user has enough shares, etc.
     *         - MUST be inclusive of withdrawal fees. Integrators should be aware of the existence of withdrawal fees.
     *         - MUST NOT revert.
     * @param shares The vault ERC20 shares amount
     * @return netAssetsAmount The underlying ERC20 assets net amount
     */
    function previewRedeem(uint256 shares) external view returns (uint256 netAssetsAmount) {
        uint256 assets = _convertToAssets(
            shares,
            _currentPrice(),
            _totalAssets(),
            _exitFeeParameter
        );
        uint256 feeAmount = (assets * _protocolExitFeePercentage) / 1 ether;
        netAssetsAmount = assets - feeAmount;
    }

    /**
     * @notice Exchange shares owned by owner with underlying ERC20 assets sent to receiver
     * @dev    Burns exactly shares from owner and sends assets of underlying ERC20 assets to receiver.
     *         - MUST emit the Withdraw event.
     *         - MUST revert if all of shares cannot be redeemed
     * @param shares The vault ERC20 shares amount
     * @param receiver The underlying ERC20 assets beneficiary
     * @param owner The Vault shares owner
     * @return netAssetsAmount The underlying ERC20 assets net amount
     */
    function redeem(
        uint256 shares,
        address receiver,
        address owner
    ) external nonReentrant returns (uint256 netAssetsAmount) {
        return _redeem(shares, receiver, owner, 0);
    }

    /**
     * @notice Exchange shares owned by owner with underlying ERC20 assets
     *         sent to receiver, (EOA must use this in favor of regular redeem)
     * @dev    Burns exactly shares from owner and sends assets of underlying ERC20 assets to receiver.
     *         - MUST emit the Withdraw event.
     *         - MUST revert if all of shares cannot be redeemed
     * @param shares The vault ERC20 shares amount
     * @param receiver The underlying ERC20 assets beneficiary
     * @param owner The Vault shares owner
     * @param minOut The underlying ERC20 assets minimum amount the redeemer is accepting
     * @return netAssetsAmount The underlying ERC20 assets net amount
     */
    function redeem(
        uint256 shares,
        address receiver,
        address owner,
        uint256 minOut
    ) external nonReentrant returns (uint256 netAssetsAmount) {
        return _redeem(shares, receiver, owner, minOut);
    }

    /**
     * @notice Deposit underlying ERC20 asset without receiving any shares (distribute across vault shares holders)
     */
    function depositReward(address from, uint256 amount) external nonReentrant {
        SafeERC20Upgradeable.safeTransferFrom(_underline, from, address(this), amount);
    }

    /**
     * @notice NOT AVAILABLE, don't use this function as mint action is
     *         not handled in our vault and will revert, use this.deposit(assets, receiver); instead
     * @dev    Return zero always, deposit should be used instead of mint.
     * @return maxShares Static value Zero
     */
    function maxMint(
        address /*receiver*/
    ) external pure returns (uint256 maxShares) {
        return 0;
    }

    /**
     * @notice NOT AVAILABLE, mint is not handled, use this.previewDeposit(assets) instead
     * @dev    Return zero always, deposit should be used instead of mint.
     * @return assets Static value Zero
     */
    function previewMint(
        uint256 /*shares*/
    ) external pure returns (uint256 assets) {
        return 0;
    }

    /**
     * @notice NOT AVAILABLE, mint is not handled and will revert, use this.deposit(assets, receiver); instead
     * @dev    Revert the transaction if called
     */
    function mint(
        uint256, /*shares*/
        address /*receiver*/
    ) external pure returns (uint256) {
        revert("only deposit");
    }

    /**
     * @notice NOT AVAILABLE, don't use this function as withdraw action is not handled in our vault
     *         and will revert, use this.redeem(shares, receiver, owner); instead
     * @dev    Return zero always, redeem should be used instead of withdraw.
     * @return maxAssets Static value Zero
     */
    function maxWithdraw(
        address /* owner*/
    ) external pure returns (uint256 maxAssets) {
        return 0;
    }

    /**
     * @notice NOT AVAILABLE, withdraw is not handled, use this.redeem(shares, receiver, owner); instead
     * @dev    Return zero always, redeem should be used instead of withdraw.
     * @return shares Static value Zero
     */
    function previewWithdraw(
        uint256 /*assets*/
    ) external pure returns (uint256 shares) {
        return 0;
    }

    /**
     * @notice NOT AVAILABLE, withdraw is not handled and will revert, use this.redeem(shares, receiver, owner); instead
     * @dev    Revert the transaction if called
     */
    function withdraw(
        uint256, /*assets*/
        address, /*receiver*/
        address /*owner*/
    ) external pure returns (uint256) {
        revert("only redeem");
    }

    function _currentPrice() internal view returns (uint256) {
        return (_totalAssets() * 1 ether) / totalSupply();
    }

    function _totalAssets() internal view returns (uint256) {
        return _underline.balanceOf(address(this));
    }

    function _convertToShares(
        uint256 assets,
        uint256 price,
        uint256 entryFeeParameter
    ) internal pure returns (uint256) {
        uint256 entryFeeAmount = (price * entryFeeParameter) / 1 ether;

        /// @dev this should be divided by 1 ether but we keep for next calculation step for better precision
        uint256 denominator = price + entryFeeAmount;

        /// @dev A / (B/C) <==> (A * C) / B {1 ether is used the scalls-down currentPrice and entryFeeParameter}
        return (assets * 1 ether) / denominator;
    }

    function _convertToAssets(
        uint256 shares,
        uint256 price,
        uint256 totalAsset,
        uint256 exitFeeParameter
    ) internal pure returns (uint256) {
        /// @dev Scaled because price is expressed in ether and we avoid early division for better precision
        uint256 assetsScaled = (shares * price);

        /// @dev in ((exitFeeParameter * (assetsScaled / totalAsset)) / 1 ether)
        ///      we div by 1 ether because assetsScaled is expressed in ether
        uint256 effectiveExistFeeParameter = exitFeeParameter -
            ((exitFeeParameter * (assetsScaled / totalAsset)) / 1 ether);

        /// @dev 1 ether for price and 1 ether exitFeeParameter been expressed in ether
        uint256 DENOMINATOR = (1 ether * 1 ether);

        return (assetsScaled * (1 ether - effectiveExistFeeParameter)) / DENOMINATOR;
    }

    function _redeem(
        uint256 shares,
        address receiver,
        address owner,
        uint256 minOut
    ) private returns (uint256 netAssetsAmount) {
        // ensure caller have approval to spend owner shares
        if (msg.sender != owner) {
            _spendAllowance(owner, msg.sender, shares);
        }

        uint256 assets = _convertToAssets(
            shares,
            _currentPrice(),
            _totalAssets(),
            _exitFeeParameter
        );

        // calculate protocol-fee/net amount based on converted shares to assets
        uint256 protocolExitFeeAmount = (assets * _protocolExitFeePercentage) / 1 ether;
        netAssetsAmount = assets - protocolExitFeeAmount;

        // slippage
        require(netAssetsAmount >= minOut, "Vault: less than minimum");

        /// @dev burn shares before transfer underlying asset (external contract call) to prevent reentrancy
        // burn owner shares
        _burn(owner, shares);
        // safely transfer net amount to beneficiary
        SafeERC20Upgradeable.safeTransfer(_underline, receiver, netAssetsAmount);
        // safely transfer fee amount to treasury
        SafeERC20Upgradeable.safeTransfer(_underline, treasury, protocolExitFeeAmount);

        emit Withdraw(msg.sender, receiver, owner, assets, shares);
        uint256 _updatedPrice = _currentPrice();
        emit UpdatedPrice(_updatedPrice);
    }

    function _deposit(
        uint256 assets,
        address receiver,
        uint256 minOut
    ) private returns (uint256 shares) {
        // calculate fees/net amount, the _protocolEntryFeePercentage is denominated in ether
        uint256 protocolFeeAmount = (assets * _protocolEntryFeePercentage) / 1 ether;
        uint256 netAssetsAmount = assets - protocolFeeAmount;

        // calculate shares based on net asset amount (after deducting fees)
        shares = _convertToShares(netAssetsAmount, _currentPrice(), _entryFeeParameter);

        // slippage
        require(shares >= minOut, "Vault: less than minimum");
        /// @dev transfer underlying assets (external contract call) before minting shares to prevent reentrancy
        // safely transfer net amount to vault
        SafeERC20Upgradeable.safeTransferFrom(
            _underline,
            msg.sender,
            address(this),
            netAssetsAmount
        );
        // safely transfer fees amount to plastique treasury
        SafeERC20Upgradeable.safeTransferFrom(_underline, msg.sender, treasury, protocolFeeAmount);
        // finally mint shares after successful underlying asset transfer
        _mint(receiver, shares);

        emit Deposit(msg.sender, receiver, assets, shares);
        uint256 _updatedPrice = _currentPrice();
        emit UpdatedPrice(_updatedPrice);
    }
}

