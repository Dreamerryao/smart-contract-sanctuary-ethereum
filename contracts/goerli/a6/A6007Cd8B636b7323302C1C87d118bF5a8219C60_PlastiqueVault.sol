pragma solidity 0.8.14;

import {
    IERC4626Upgradeable
} from "@openzeppelin/contracts-upgradeable/interfaces/IERC4626Upgradeable.sol";

interface VaultInterface is IERC4626Upgradeable {
    function setup(
        address underlineAsset,
        string memory vaultTokenName,
        string memory vaultTokenSymbol,
        uint256 entryFeeParameterInEther,
        uint256 exitFeeParameterInEther,
        uint256 protocolEntryFeePercentageInEther,
        uint256 protocolExitFeePercentageInEther,
        address protocolFeeRecipient,
        uint256 underlyingSeedAmount,
        address creator
    ) external;

    function redeem(
        uint256 shares,
        address receiver,
        address owner,
        uint256 minOut
    ) external returns (uint256 netAssetsAmount);

    function deposit(
        uint256 assets,
        address receiver,
        uint256 minOut
    ) external returns (uint256 shares);

    function getProtocolEntryFee()
        external
        view
        returns (uint256 protocolEntryFee, uint256 denominator);

    function getProtocolExitFee()
        external
        view
        returns (uint256 protocolExitFee, uint256 denominator);

    function getEntryFeeParameter()
        external
        view
        returns (uint256 entryFeeParameter, uint256 denominator);

    function getExitFeeParameter()
        external
        view
        returns (uint256 entryFeeParameter, uint256 denominator);

    function depositReward(address from, uint256 amount) external;
}

