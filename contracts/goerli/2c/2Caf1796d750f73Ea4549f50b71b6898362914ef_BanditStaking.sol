pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "fx-portal-contracts/contracts/tunnel/FxBaseRootTunnel.sol";

contract BanditStaking is FxBaseRootTunnel, Ownable, Pausable {
    using EnumerableSet for EnumerableSet.UintSet;
    using Counters for Counters.Counter;

    Counters.Counter nonce;

    uint public DAILY_REWARD = 1000 ether;

    // Multipliers are divided by 1000, i.e. 1100 is 1.1x
    uint public MULTIPLIER_BASE = 1000;
    uint public MULTIPLIER_30_DAYS = 1100;
    uint public MULTIPLIER_60_DAYS = 1200;
    uint public MULTIPLIER_90_DAYS = 1300;

    IERC721 public token;

    struct StakeData {
        uint stakedAt;
        uint lastClaimedAt;
    }

    mapping(uint => StakeData) public tokenStakeData;
    mapping(address => EnumerableSet.UintSet) stakedTokens;

    struct BanditTypeConfig {
        string label;
        uint multiplier;
        uint globalMultiplier;
    }

    mapping(uint => BanditTypeConfig) public banditTypeConfigs;

    mapping(uint => uint) public banditTypes;

    struct MintMessage {
        address recipient;
        uint amount;
        uint nonce;
    }

    event Staked(address owner, uint256 tokenId, uint stakedAt);
    event Unstaked(address owner, uint256 tokenId, uint stakedAt, uint unstakedAt);
    event Claimed(address recipient, uint amount, uint nonce);

    constructor(address _checkpointManager, address _fxRoot) FxBaseRootTunnel(_checkpointManager, _fxRoot) {
        banditTypeConfigs[0] = BanditTypeConfig("Normal", 1000, 1000);
        banditTypeConfigs[1] = BanditTypeConfig("Legendary", 1250, 1000);
        banditTypeConfigs[2] = BanditTypeConfig("Royal", 3000, 1100);
    }

    // === MANAGEMENT ===
    function getCurrentNonce() public view returns (uint) {
        return nonce.current();
    }

    function setToken(address _token) public onlyOwner {
        token = IERC721(_token);
    }

    function setBanditTypeConfig(uint typeId, string calldata label, uint multiplier, uint globalMultiplier) external onlyOwner {
        banditTypeConfigs[typeId] = BanditTypeConfig(label, multiplier, globalMultiplier);
    }

    function setBanditType(uint tokenId, uint typeId) external onlyOwner {
        banditTypes[tokenId] = typeId;
    }

    function batchSetBanditType(uint[] calldata tokenIds, uint typeId) external onlyOwner {
        for (uint i = 0; i < tokenIds.length; i++) {
            banditTypes[tokenIds[i]] = typeId;
        }
    }

    function set30DaysMultiplier(uint _30DaysMultiplier) external onlyOwner {
        MULTIPLIER_30_DAYS = _30DaysMultiplier;
    }

    function set60DaysMultiplier(uint _60DaysMultiplier) external onlyOwner {
        MULTIPLIER_60_DAYS = _60DaysMultiplier;
    }

    function set90DaysMultiplier(uint _90DaysMultiplier) external onlyOwner {
        MULTIPLIER_90_DAYS = _90DaysMultiplier;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    // === STAKING ===
    function isStaked(address owner, uint tokenId) public view returns (bool) {
        return stakedTokens[owner].contains(tokenId);
    }

    function getStakedTokens() public view returns (uint[] memory) {
        return stakedTokens[msg.sender].values();
    }

    function stakeToken(uint tokenId) public whenNotPaused {
        require(!isStaked(msg.sender, tokenId), "Token is already staked");
        require(token.ownerOf(tokenId) == msg.sender, "Token is not owned by sender");
        require(token.isApprovedForAll(msg.sender, address(this)), "Approval required");

        require(stakedTokens[msg.sender].add(tokenId), "Failed to stake token");
        tokenStakeData[tokenId].stakedAt = block.timestamp;
        tokenStakeData[tokenId].lastClaimedAt = block.timestamp;

        token.transferFrom(msg.sender, address(this), tokenId);

        emit Staked(msg.sender, tokenId, block.timestamp);
    }

    function batchStakeTokens(uint[] calldata tokenIds) external whenNotPaused {
        for (uint i = 0; i < tokenIds.length; i++) {
            stakeToken(tokenIds[i]);
        }
    }

    function _unstakeToken(uint tokenId) internal {
        require(stakedTokens[msg.sender].remove(tokenId), "Failed to unstake token");
        uint stakedAt = tokenStakeData[tokenId].stakedAt;
        tokenStakeData[tokenId].stakedAt = 0;

        token.safeTransferFrom(address(this), msg.sender, tokenId);

        emit Unstaked(msg.sender, tokenId, stakedAt, block.timestamp);
    }

    function unstakeToken(uint tokenId) public whenNotPaused {
        require(isStaked(msg.sender, tokenId), "Token is not staked");

        _claimRewardsForToken(tokenId);

        _unstakeToken(tokenId);
    }

    /// @notice Unstake without claiming rewards even if contract is paused
    function forceUnstakeToken(uint tokenId) public {
        require(isStaked(msg.sender, tokenId), "Token is not staked");

        _unstakeToken(tokenId);
    }

    function batchUnstakeTokens(uint[] calldata tokenIds) external whenNotPaused {
        for (uint i = 0; i < tokenIds.length; i++) {
            unstakeToken(tokenIds[i]);
        }
    }

    // === REWARDS ===
    function calculateRewards(uint stakedAt, uint until, uint typeId) public view returns (uint) {
        require(until >= stakedAt, "Invalid time range");
        uint timeSinceStake = (until - stakedAt);

        uint typeMultiplier = banditTypeConfigs[typeId].multiplier;
        uint currentMultiplier = MULTIPLIER_BASE * typeMultiplier / 1000;
        uint totalRewards = 0;

        // First full 30 days
        if (timeSinceStake > 30 days) {
            totalRewards += (DAILY_REWARD / 1000) * currentMultiplier * 30;
            timeSinceStake -= 30 days;
            currentMultiplier = MULTIPLIER_30_DAYS * typeMultiplier / 1000;
        }

        // Second full 30 days (day 30 to 60)
        if (timeSinceStake > 30 days) {
            totalRewards += (DAILY_REWARD / 1000) * currentMultiplier * 30;
            timeSinceStake -= 30 days;
            currentMultiplier = MULTIPLIER_60_DAYS * typeMultiplier / 1000;
        }

        // Third full 30 days (day 60 to 90)
        if (timeSinceStake > 30 days) {
            totalRewards += (DAILY_REWARD / 1000) * currentMultiplier * 30;
            timeSinceStake -= 30 days;
            currentMultiplier = MULTIPLIER_90_DAYS * typeMultiplier / 1000;
        }

        // Remaining days
        totalRewards += (DAILY_REWARD / 1000) * currentMultiplier * timeSinceStake / 1 days;

        return totalRewards;
    }

    /// @notice Calculates token claimable (does not apply global multipliers)
    function calculateTokenClaimable(uint tokenId) public view returns (uint) {
        uint stakedAt = tokenStakeData[tokenId].stakedAt;
        uint lastClaimedAt = tokenStakeData[tokenId].lastClaimedAt;
        uint typeId = banditTypes[tokenId];

        uint previousRewards = calculateRewards(stakedAt, lastClaimedAt, typeId);
        uint currentRewards = calculateRewards(stakedAt, block.timestamp, typeId);

        if (previousRewards > currentRewards) {
            return 0;
        }

        return currentRewards - previousRewards;
    }

    function calculateTotalClaimable() public view returns (uint) {
        uint stakedTokenCount = stakedTokens[msg.sender].length();

        uint globalMultiplier = 1000;
        uint totalClaimable = 0;
        for (uint i = 0; i < stakedTokenCount; i++) {
            uint tokenId = stakedTokens[msg.sender].at(i);

            totalClaimable += calculateTokenClaimable(tokenId);

            globalMultiplier = globalMultiplier * banditTypeConfigs[banditTypes[tokenId]].globalMultiplier / 1000;
        }

        return totalClaimable * globalMultiplier / 1000;
    }

    function claimRewards() external whenNotPaused {
        uint stakedTokenCount = stakedTokens[msg.sender].length();
        require(stakedTokenCount > 0, "You must stake a token to claim rewards");

        uint globalMultiplier = 1000;
        uint totalRewards = 0;
        for (uint index = 0; index < stakedTokenCount; index++) {
            uint tokenId = stakedTokens[msg.sender].at(index);
            totalRewards += calculateTokenClaimable(tokenId);

            tokenStakeData[tokenId].lastClaimedAt = block.timestamp;

            globalMultiplier = globalMultiplier * banditTypeConfigs[banditTypes[tokenId]].globalMultiplier / 1000;
        }

        totalRewards = totalRewards * globalMultiplier / 1000;

        require(totalRewards > 0, "You have no rewards to claim");

        MintMessage memory mintMessage = MintMessage(msg.sender, totalRewards, nonce.current());
        _sendMessageToChild(abi.encode(mintMessage));

        emit Claimed(msg.sender, totalRewards, nonce.current());

        nonce.increment();
    }

    function _claimRewardsForToken(uint tokenId) internal {
        uint totalRewards = calculateTokenClaimable(tokenId);
        tokenStakeData[tokenId].lastClaimedAt = block.timestamp;

        uint globalMultiplier = 1000;
        uint stakedTokenCount = stakedTokens[msg.sender].length();
        for (uint index = 0; index < stakedTokenCount; index++) {
            uint tokenId = stakedTokens[msg.sender].at(index);
            globalMultiplier = globalMultiplier * banditTypeConfigs[banditTypes[tokenId]].globalMultiplier / 1000;
        }

        totalRewards = totalRewards * globalMultiplier / 1000;

        MintMessage memory mintMessage = MintMessage(msg.sender, totalRewards, nonce.current());
        _sendMessageToChild(abi.encode(mintMessage));

        emit Claimed(msg.sender, totalRewards, nonce.current());

        nonce.increment();
    }

    function claimRewardsForToken(uint tokenId) external whenNotPaused {
        require(isStaked(msg.sender, tokenId), "Token is not staked");

        _claimRewardsForToken(tokenId);
    }

    // === STATE SYNC ===
    function _processMessageFromChild(bytes memory data) internal override {
        // Intentionally empty as we don't expect any messages from the child
    }
}

