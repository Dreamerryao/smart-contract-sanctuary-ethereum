contract Ember is IEmber {
    uint256 public lendingId;
    mapping(uint256 => IEmber.LendingRenting) private lendingRenting;
    //mapping(uint256 => bytes) public half_sig;
    mapping(address => bool) private multisig;
    mapping(address => bool) private VerifiedEOA;
    //mapping(address => mapping(bytes=>bool)) private EOA_ENCKEY;
    mapping(address => string) public EOA_ENCKEY;
    uint256 public constant HUNDRED = 100 ether;
    uint256 public feePercentage = 5 ether;
    address owner = 0xFcce5CE96D091395bef9f9c19306983e4D812c10;
    event lend1(
        address nft,
        uint256 tokenid,
        bool isUpFront,
        uint256 amount,
        uint256 stakedTill,
        address lender
    );
    event Sublet(
        address nft,
        uint256 tokenid,
        bool isUpfront,
        uint256 amount,
        uint256 stakedTill,
        address lender
    );
    event Rent(
        address nft,
        uint256 tokenid,
        address EOA,
        uint256 duration,
        uint256 amount,
        uint256 charges
    );
    event exRent(address nft, uint256 tokenId, address borrower);

    //event Rent(IEmber.rentDetials);
    function addsigner(address signer) external {
        multisig[signer] = true;
    }

    function lend(lendDetails[] memory data, address adapter) external {
        lendDetails memory lendData;
        uint256 _lendingId = lendingId;
        for (uint256 i = 0; i < data.length; i++) {
            lendData = data[i];
            lendingRenting[_lendingId].lending.lenderAddress = msg.sender;
            lendingRenting[_lendingId].lending.adapter = adapter;
            lendingRenting[_lendingId].lending.amount = lendData.amount;
            lendingRenting[_lendingId].lending.stakedTill =
                block.timestamp +
                lendData.duration;
            lendingRenting[_lendingId].lending.tokenId = lendData.tokenId;
            lendingRenting[_lendingId].lending.nft = lendData.nft;
            lendingRenting[_lendingId].lending.isUpfront = lendData.isUpfront; //if not upfront then amount indicates the split % eg.60 shows 60% lender and 40% borrower
            if (!lendData.isUpfront) {
                require(lendData.borrower != address(0), "address(0)");
                lendingRenting[_lendingId].lending.borrower = lendData.borrower;
            }
            IERC721(lendData.nft).transferFrom(
                msg.sender,
                address(this),
                lendData.tokenId
            );
            _lendingId++;
            emit lend1(
                lendData.nft,
                lendData.tokenId,
                lendData.isUpfront,
                lendData.amount,
                block.timestamp + lendData.duration,
                msg.sender
            );
        }
        lendingId = _lendingId;
    }

    function SubLetting(subLetting[] memory data) external {
        //break subletting chain==> needs to be done
        subLetting memory lendData;
        for (uint256 i = 0; i < data.length; i++) {
            lendData = data[i];

            if (lendingRenting[lendData.Id].renting.sublet == true) {
                revert("already sublended");
            }
            //require(lendingRenting[lendData.Id].renting.sublet,"not allowed"); //skiping it for now need to cater it in future version
            lendingRenting[lendData.Id].sublet.lender = msg.sender;
            lendingRenting[lendData.Id].sublet.isUpfront = lendData.isUpfront;
            lendingRenting[lendData.Id].sublet.stakedTill =
                block.timestamp +
                lendData.duration;
            lendingRenting[lendData.Id].sublet.amount = lendData.amount;
            lendingRenting[lendData.Id].renting.sublet = true;
            require(lendingRenting[lendData.Id].lending.isUpfront, "BFY");
            require(
                lendingRenting[lendData.Id].sublet.stakedTill <=
                    lendingRenting[lendData.Id].renting.rentedTill,
                "SD>RD"
            );
            require(
                lendingRenting[lendData.Id].renting.renterAddress == msg.sender,
                "InValid lender"
            );
            if (!lendData.isUpfront) {
                require(lendData.borrower != address(0), "address(0)");
                lendingRenting[lendData.Id].sublet.borrower = lendData.borrower;
            }
            IERC721(lendingRenting[lendData.Id].lending.nft).transferFrom(
                msg.sender,
                address(this),
                lendingRenting[lendData.Id].lending.tokenId
            );
            emit Sublet(
                lendingRenting[lendData.Id].lending.nft,
                lendingRenting[lendData.Id].lending.tokenId,
                lendData.isUpfront,
                lendData.amount,
                block.timestamp + lendData.duration,
                msg.sender
            );
        }
    }

    // function verifyBorrower(bytes calldata encriptedKey, address EOA, bytes [] calldata signatures) external payable{

    //     verifySigner(EOA, signatures);
    //     EOA_ENCKEY[EOA] = encriptedKey;
    //     VerifiedEOA[EOA]=true;

    // }
    //can combine verifyBorrower & rentDetails

    function RentDetails(
        IEmber.rentDetials[] memory data,
        string calldata encriptedKey,
        address EOA,
        bytes[] calldata signatures
    ) external payable {
        if (!VerifiedEOA[EOA]) {
            verifySigner(EOA, signatures);
            EOA_ENCKEY[EOA] = encriptedKey;
            VerifiedEOA[EOA] = true;
        }
        rentDetials memory rentData;
        uint256 charges;
        uint256 totalCharges;
        uint256 id;
        for (uint256 i = 0; i < data.length; i++) {
            //renting subletted NFT
            rentData = data[i];
            id = rentData.Id;
            //sub-renting

            if (!lendingRenting[id].lending.isUpfront) {
                if (!lendingRenting[id].renting.sublet) {
                    require(
                        msg.sender == lendingRenting[id].lending.borrower,
                        "in-valid borrower"
                    );
                } else if (lendingRenting[id].renting.sublet) {
                    require(
                        msg.sender == lendingRenting[id].sublet.borrower,
                        "in-valid borrower"
                    );
                }
            }
            if (lendingRenting[id].renting.sublet) {
                if (
                    IERC721(lendingRenting[id].lending.nft).ownerOf(
                        lendingRenting[id].lending.tokenId
                    ) != address(this)
                ) {
                    require(
                        getSubRentedTill(id) < block.timestamp,
                        "Rent duration not Expired"
                    );
                    if (getRentedTill(id) < block.timestamp) {
                        //orignal borrower time expires then exexute normal rent
                        lendingRenting[id].renting.sublet = false; //issue:in that secanrio NFT can be relend
                        delete lendingRenting[id].sublet;
                        normalRent(
                            lendingRenting[id].lending.nft,
                            lendingRenting[id].lending.tokenId,
                            id,
                            EOA,
                            rentData.rentDuration,
                            lendingRenting[id].renting.subRenter
                        );
                    }
                }
                //nft in contract & subleted
                if (lendingRenting[id].renting.sublet) {
                    subletRent(id, EOA, rentData.rentDuration);
                    lendingRenting[id].renting.previousRenter = lendingRenting[
                        id
                    ].renting.subRenter;
                }
            } else {
                normalRent(
                    lendingRenting[id].lending.nft,
                    lendingRenting[id].lending.tokenId,
                    id,
                    EOA,
                    rentData.rentDuration,
                    lendingRenting[id].renting.renterAddress
                );
            }

            if (lendingRenting[id].sublet.isUpfront) {
                require(
                    (lendingRenting[rentData.Id].sublet.amount *
                        (rentData.rentDuration)) /
                        3600 ==
                        rentData.amount,
                    "Invalid Amount"
                );
                charges = (rentData.amount * feePercentage) / HUNDRED;
                totalCharges = totalCharges + charges;
                payable(lendingRenting[rentData.Id].lending.lenderAddress)
                    .transfer(rentData.amount - charges);
            }

            if (
                lendingRenting[id].lending.isUpfront &&
                !lendingRenting[id].renting.sublet
            ) {
                //temprary solution
                require(
                    lendingRenting[rentData.Id].lending.amount *
                        (rentData.rentDuration / 3600) ==
                        rentData.amount,
                    "Invalid Amount"
                );
                charges = (rentData.amount * feePercentage) / HUNDRED;
                totalCharges = totalCharges + charges;
                payable(lendingRenting[rentData.Id].lending.lenderAddress)
                    .transfer(rentData.amount - charges);
            }
            emit Rent(
                lendingRenting[id].lending.nft,
                lendingRenting[id].lending.tokenId,
                EOA,
                rentData.rentDuration,
                rentData.amount,
                charges
            );
        }

        if (totalCharges != 0) {
            payable(owner).transfer(totalCharges);
        }
        payable(EOA).transfer(address(this).balance);
    }

    function calculateFee(uint256 amount)
        external
        view
        returns (uint256 feeAmount)
    {
        feeAmount = (amount * feePercentage) / HUNDRED;
    }

    function normalRent(
        address nft,
        uint256 tokenId,
        uint256 id,
        address EOA,
        uint256 duration,
        address renter
    ) internal {
        if (IERC721(nft).ownerOf(tokenId) != address(this)) {
            require(
                getRentedTill(id) < block.timestamp,
                "Rent duration not Expired"
            );
            lendingRenting[id].renting.previousRenter = renter;
        }
        require(
            lendingRenting[id].lending.lenderAddress != msg.sender,
            "Lender can't be borrower for it's own NFT"
        );
        lendingRenting[id].renting.renterAddress = payable(EOA);
        lendingRenting[id].renting.rentedTill = block.timestamp + duration;
        require(
            lendingRenting[id].renting.rentedTill <=
                lendingRenting[id].lending.stakedTill,
            "RD>SD"
        );
    }

    function subletRent(
        uint256 id,
        address EOA,
        uint256 duration
    ) internal {
        require(
            lendingRenting[id].lending.lenderAddress != msg.sender,
            "Lender can't be borrower for it's own NFT"
        );
        require(
            lendingRenting[id].sublet.lender != msg.sender,
            "sublender-borrows NFT"
        );
        lendingRenting[id].renting.subRenter = payable(EOA); //sublented renter
        lendingRenting[id].renting.subRentedTill = block.timestamp + duration;
        require(
            lendingRenting[id].renting.subRentedTill <=
                lendingRenting[id].sublet.stakedTill,
            "RD>SD"
        );
    }

    function executeRents(IEmber.ExecuteRent[] calldata data) external {
        for (uint256 i = 0; i < data.length; i++) {
            if (lendingRenting[data[i].id].renting.sublet) {
                require(
                    lendingRenting[data[i].id].renting.subRenter == msg.sender,
                    "Invalid-subrenter"
                );
            } else {
                require(
                    lendingRenting[data[i].id].renting.renterAddress ==
                        msg.sender,
                    "Invalid-Renter"
                );
            }

            if (
                IERC721(lendingRenting[data[i].id].lending.nft).ownerOf(
                    lendingRenting[data[i].id].lending.tokenId
                ) == address(this)
            ) {
                IERC721(lendingRenting[data[i].id].lending.nft).transferFrom(
                    address(this),
                    msg.sender,
                    lendingRenting[data[i].id].lending.tokenId
                );
            } else {
                //get back nft from previous renter
                IERC721(lendingRenting[data[i].id].lending.nft).transferFrom(
                    lendingRenting[data[i].id].renting.previousRenter,
                    msg.sender,
                    lendingRenting[data[i].id].lending.tokenId
                );
            }
            emit exRent(
                lendingRenting[data[i].id].lending.nft,
                lendingRenting[data[i].id].lending.tokenId,
                msg.sender
            );
        }
    }

    function unLend(uint256[] calldata lendingIds) external {
        for (uint256 i = 0; i < lendingIds.length; i++) {
            //normal lender
            require(
                getRentedTill(lendingIds[i]) < block.timestamp,
                "Rent duration not Expired"
            );
            if (
                lendingRenting[lendingIds[i]].lending.lenderAddress ==
                msg.sender
            ) {
                if (
                    IERC721(lendingRenting[lendingIds[i]].lending.nft).ownerOf(
                        lendingRenting[lendingIds[i]].lending.tokenId
                    ) == address(this)
                ) {
                    IERC721(lendingRenting[lendingIds[i]].lending.nft)
                        .transferFrom(
                            address(this),
                            msg.sender,
                            lendingRenting[lendingIds[i]].lending.tokenId
                        );
                } else if (lendingRenting[lendingIds[i]].renting.sublet) {
                    IERC721(lendingRenting[lendingIds[i]].lending.nft)
                        .transferFrom(
                            lendingRenting[lendingIds[i]].renting.subRenter,
                            msg.sender,
                            lendingRenting[lendingIds[i]].lending.tokenId
                        );
                } else {
                    IERC721(lendingRenting[lendingIds[i]].lending.nft)
                        .transferFrom(
                            lendingRenting[lendingIds[i]].renting.renterAddress,
                            msg.sender,
                            lendingRenting[lendingIds[i]].lending.tokenId
                        );
                }
            }
            //sublender
            else if (
                lendingRenting[lendingIds[i]].sublet.lender == msg.sender
            ) {
                require(
                    lendingRenting[lendingIds[i]].renting.sublet,
                    "Sublet off"
                );
                require(
                    getSubRentedTill(lendingIds[i]) < block.timestamp,
                    "Rent duration not Expired"
                );
                if (
                    IERC721(lendingRenting[lendingIds[i]].lending.nft).ownerOf(
                        lendingRenting[lendingIds[i]].lending.tokenId
                    ) == address(this)
                ) {
                    require(
                        getRentedTill(lendingIds[i]) < block.timestamp,
                        "Rent duration not Expired"
                    );
                    IERC721(lendingRenting[lendingIds[i]].lending.nft)
                        .transferFrom(
                            address(this),
                            msg.sender,
                            lendingRenting[lendingIds[i]].lending.tokenId
                        );
                } else {
                    IERC721(lendingRenting[lendingIds[i]].lending.nft)
                        .transferFrom(
                            lendingRenting[lendingIds[i]].renting.subRenter,
                            msg.sender,
                            lendingRenting[lendingIds[i]].lending.tokenId
                        );
                }
            } else {
                revert("invalid call");
            }
        }
    }

    function forceClaim(uint256 _lendingId) external {
        if (
            IERC721(lendingRenting[_lendingId].lending.nft).ownerOf(
                lendingRenting[_lendingId].lending.tokenId
            ) != address(this)
        ) {
            if (!lendingRenting[_lendingId].lending.isUpfront) {
                require(
                    lendingRenting[_lendingId].lending.lenderAddress ==
                        msg.sender,
                    "Invalid-msg.sender"
                );
                IERC721(lendingRenting[_lendingId].lending.nft).transferFrom(
                    lendingRenting[_lendingId].renting.renterAddress,
                    msg.sender,
                    lendingRenting[_lendingId].lending.tokenId
                );
                delete lendingRenting[_lendingId];
            } else {
                require(
                    getRentedTill(_lendingId) > block.timestamp,
                    "Rent duration Expired"
                );
                require(
                    lendingRenting[_lendingId].sublet.lender == msg.sender,
                    "Invalid-msg.sender"
                );
                IERC721(lendingRenting[_lendingId].lending.nft).transferFrom(
                    lendingRenting[_lendingId].renting.subRenter,
                    msg.sender,
                    lendingRenting[_lendingId].lending.tokenId
                );
                delete lendingRenting[_lendingId].sublet;
            }
        }
    }

    function verifySigner(address EOA, bytes[] calldata signatures)
        internal
        view
    {
        bytes32 dataHASH1 = keccak256(abi.encodePacked(EOA));
        bytes32 datahash = keccak256(
            abi.encodePacked("\x19Ethereum Signed Message:\n32", dataHASH1)
        );
        address signer;

        for (uint256 i = 0; i < signatures.length; i++) {
            signer = recoverSigner(signatures[i], datahash);
            if (!multisig[signer]) {
                revert("Invalid Signer");
            }
        }
    }

    function recoverSigner(
        bytes memory sig,
        bytes32 _hash //signature, hash
    ) internal pure returns (address) {
        (uint8 v, bytes32 r, bytes32 s) = splitSignature(sig);

        return ecrecover(bytes32(_hash), v, r, s);
    }

    function splitSignature(bytes memory sig)
        internal
        pure
        returns (
            uint8 v,
            bytes32 r,
            bytes32 s
        )
    {
        require(sig.length == 65);

        assembly {
            // first 32 bytes, after the length prefix.
            r := mload(add(sig, 32))
            // second 32 bytes.
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 byt (es).
            v := byte(0, mload(add(sig, 96)))
        }

        return (v, r, s);
    }

    function getRentedTill(uint256 _lendingId) public view returns (uint256) {
        return lendingRenting[_lendingId].renting.rentedTill;
    }

    function getSubRentedTill(uint256 _lendingId)
        public
        view
        returns (uint256)
    {
        return lendingRenting[_lendingId].renting.subRentedTill;
    }
}

