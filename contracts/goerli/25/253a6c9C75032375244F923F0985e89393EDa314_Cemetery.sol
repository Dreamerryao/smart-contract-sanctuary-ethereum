//SPDX-License-Identifier: UNLICENSED
pragma solidity 0.8.4;

import "./ERC20.sol";
import "./Coffin.sol";
import "./StakeNFT.sol";
import "./Cemetery.sol";
import "./CemeterySettings.sol";
import "hardhat/console.sol";

contract Vampire {
    address public stakingTokenAddress;
    Coffin public coffin;
    address public coffinAddress;
    uint256 public allStakesNumber; // all stakes counter
    uint256 public activeStakesNumber; // active stakes counter
    uint256 public lastDistribution; // day of last rewards calculation and increasing Stake.awarded
    uint256 public idleDays; // idle time in days
    uint256 public stakesEndMax; // maximum end day of stakes
    uint256 public firstStakeStart; // start day of fist stake
    address public stakeNFTContractAddress;

    // data of one partucular stake
    struct Stake {
        address staker;
        uint256 start;
        uint256 end;
        uint256 amountDeposited;
        bool isActive; // active stake or ended
        uint256 awarded; // how much is awarded multiplied by 1000, this field is updated in the distributeRewards() function
    }

    mapping(uint256 => Stake) public stakes; // list of all active stakes

    event StakingStarted(
        uint256 stakeID,
        address stakerAddress,
        uint256 amountDeposited,
        uint256 startDay,
        uint256 endDay,
        uint256 tokenID,
        address stakeNFTContractAddress
    );

    event StakingEnded(uint256 stakeID, uint256 awarded);

    event FirstStakeStarted(uint256 firstStakeStart);

    event IdleDaysAdded(uint256 daysAdded);

    event StakesEndMaxUpdated(uint256 stakesEndMax);

    constructor(
        Coffin _coffinContract,
        address _stakingTokenAddress,
        address _stakeNFTContractAddress
    ) {
        stakingTokenAddress = _stakingTokenAddress;
        stakeNFTContractAddress = _stakeNFTContractAddress;
        coffin = _coffinContract;
    }

    function checkPeriod(uint256 daysLockedUp) private view returns (bool) {
        Cemetery cemetery = coffin.cemetery();
        CemeterySettings settings = cemetery.settings();
        if (
            (daysLockedUp < settings.minStakePeriod()) ||
            daysLockedUp > settings.maxStakePeriod()
        ) {
            return false;
        }

        uint256 coffinDaysLeft = getCoffinDaysLeft();
        return (daysLockedUp <= coffinDaysLeft);
    }

    function startStaking(uint256 amountDeposited, uint256 daysLockedUp)
        external
        returns (uint256 stakeID)
    {
        require(checkPeriod(daysLockedUp), "Wrong period");
        StakeNFT nft = StakeNFT(stakeNFTContractAddress);
        // require(
        //     nft.checkIPFS(
        //         ipfsHashVampire,
        //         amountDeposited,
        //         daysLockedUp,
        //         imageName,
        //         stakingTokenAddress,
        //         coffin.rewardsTokenAddress()
        //     ) == true,
        //     "Wrong IPFS hash"
        // );

        uint256 today = getToday();
        uint256 end = today + daysLockedUp - 1;
        allStakesNumber = allStakesNumber + 1;
        stakeID = allStakesNumber;
        activeStakesNumber = activeStakesNumber + 1;
        stakes[stakeID] = Stake({
            staker: msg.sender,
            start: today,
            end: end,
            amountDeposited: amountDeposited,
            isActive: true,
            awarded: 0
        });

        if (firstStakeStart > 0 && stakesEndMax < today) {
            // this is a first stake after idle time, count idle days:
            uint256 idleDaysToAdd = today - stakesEndMax;
            idleDaysToAdd -= 1;
            if (idleDaysToAdd > 0) {
                idleDays += idleDaysToAdd;
                emit IdleDaysAdded(idleDaysToAdd);
            }
        } else if (firstStakeStart == 0) {
            // this is a first stake of the coffin, save start day
            firstStakeStart = today;
            lastDistribution = today - 1; // yesterday to start to count from today
            emit FirstStakeStarted(firstStakeStart);
        }

        if (stakesEndMax < end) {
            stakesEndMax = end;
            emit StakesEndMaxUpdated(stakesEndMax);
        }

        distributeRewards();
        coffin.updateRewardsPerDayIfNewRewardsCome();

        // Remove staked amount from balance of staker
        ERC20 stakingToken = ERC20(stakingTokenAddress);
        require(
            stakingToken.transferFrom(
                msg.sender,
                address(this),
                amountDeposited
            ),
            "Tokens transfer failed"
        );

        // get an ipfs hash and Mint NFT for staker
        Cemetery cemetery = coffin.cemetery();

        StorageContract storageContract = StorageContract(
            cemetery.storageContractAddress()
        );

        string memory ipfsHash = storageContract.getStakerIpfsHash(
            address(this)
        );

        uint256 tokenID = nft.mint(msg.sender, stakeID, ipfsHash);
        // attach token to the vampire(staker) struct
        emit StakingStarted(
            stakeID,
            msg.sender,
            amountDeposited,
            today,
            end,
            tokenID,
            stakeNFTContractAddress
        );

        return stakeID;
    }

    function endStaking(uint256 stakeID) public {
        if (coffinAddress != msg.sender) {
            require(
                stakes[stakeID].staker == msg.sender,
                "Only staker can end"
            );
        }
        require(stakes[stakeID].isActive == true, "Stake is inactive");

        uint256 today = getToday();

        require(stakes[stakeID].end <= today, "Too early unlock"); // Check that time to unlock has come

        // Count final award and make stake inactive
        distributeRewards();
        activeStakesNumber--;
        Stake storage stake = stakes[stakeID];
        stake.isActive = false;

        // Send staking tokens back to staker
        ERC20 stakingToken = ERC20(stakingTokenAddress);
        require(
            stakingToken.transfer(
                stakes[stakeID].staker,
                stakes[stakeID].amountDeposited
            ),
            "Unlock failed"
        );

        // Send rewards tokens to staker
        coffin.sendRewards(stakeID);

        if (getCoffinDaysLeft() > 0) {
            coffin.updateRewardsPerDayIfNewRewardsCome();
        }

        // Burn NFT for this stake
        StakeNFT nft = StakeNFT(stakeNFTContractAddress);
        nft.burn(stakeID);

        emit StakingEnded(stakeID, stakes[stakeID].awarded);
    }

    // Make calculation and save current amount or rewards in the Stake.awarded field
    function distributeRewards() public {
        uint256 rewardsPerDay = coffin.rewardsPerDay();
        uint256 today = getToday();

        // We count rewards till yesterday only, because today we can receive new stakes
        if (
            rewardsPerDay > 0 &&
            activeStakesNumber > 0 &&
            lastDistribution < (today - 1)
        ) {
            for (uint256 day = lastDistribution + 1; day < today; day++) {
                uint256 dayTotal; // total sum for all stakes (amountDeposited * remaining days) for current day to get then share of this total

                // In this loop we calculate total sum for all stakes (amountDeposited * remaining days) to get then share of this total
                uint256 limit = allStakesNumber;
                for (uint256 stakeID = 1; stakeID <= limit; stakeID++) {
                    if (
                        stakes[stakeID].end >= day &&
                        stakes[stakeID].start <= day
                    ) {
                        uint256 daysCoeff = stakes[stakeID].end - day + 1;
                        uint256 dayStakeFactor = daysCoeff *
                            stakes[stakeID].amountDeposited;
                        dayTotal += dayStakeFactor;
                    }
                }

                if (dayTotal > 0) {
                    // In this loop we calculate share in this day rewards and add it to awarded field
                    for (uint256 stakeID = 1; stakeID <= limit; stakeID++) {
                        if (
                            stakes[stakeID].end >= day &&
                            stakes[stakeID].start <= day
                        ) {
                            uint256 daysCoeff = stakes[stakeID].end - day + 1;
                            uint256 dayStakeFactor = daysCoeff *
                                stakes[stakeID].amountDeposited;
                            uint256 share = preciseDiv(
                                dayStakeFactor,
                                dayTotal,
                                2
                            ); // Share in percents
                            uint256 todayStakeReward = rewardsPerDay * share;
                            todayStakeReward = todayStakeReward / 100; // Round down. Remaining rewards will be sent to fee receiver
                            stakes[stakeID].awarded += todayStakeReward;
                        }
                    }
                }
            }

            lastDistribution = today - 1;
        }
    }

    // Get amount of days remaining till the end of the coffin period, today is included
    function getCoffinDaysLeft() public view returns (uint256) {
        uint256 coffinPeriod = coffin.periodInDays();
        if (firstStakeStart == 0) {
            // No stakes were made yet, just return all coffinPeriod
            return coffinPeriod;
        } else {
            uint256 today = getToday();

            uint256 activeDaysPassed; // How many days coffin was active (had stakes), the current day is not included

            if (stakesEndMax < today) {
                // If last stake finished before present moment we have to count active days
                // till the end of last stake
                activeDaysPassed = stakesEndMax - firstStakeStart + 1;
            } else {
                // In case there are active stakes now we just count days till today as active days, the current day is not included
                activeDaysPassed = today - firstStakeStart;
            }

            activeDaysPassed = activeDaysPassed - idleDays;

            return coffinPeriod - activeDaysPassed;
        }
    }

    function isStakeActive(uint256 stakeID) external view returns (bool) {
        return stakes[stakeID].isActive;
    }

    function getStakerAddress(uint256 stakeID) external view returns (address) {
        return stakes[stakeID].staker;
    }

    function getAwardedAmount(uint256 stakeID) external view returns (uint256) {
        return stakes[stakeID].awarded / 1000; // We store awarded multiplied by 1000. Make division to get real amount of rewards
    }

    function getToday() public view returns (uint256) {
        return preciseDiv(block.timestamp, 1 days, 0);
        //return block.timestamp / 3 seconds; // uncomment this to run tests
    }

    // Makes division with correct rounding. Result is multiplied by 10**precision, e.g. (1, 2, 2) returns 50,
    // (5, 2, 0) returns 3.
    function preciseDiv(
        uint256 numerator,
        uint256 denominator,
        uint256 precision
    ) public pure returns (uint256 quotient) {
        uint256 _numerator = numerator * (10**(precision + 1));
        // with rounding of last digit
        uint256 _quotient = ((_numerator / denominator) + 5) / 10;
        return (_quotient);
    }

    // Check if not ended stakes exist or not, returns true if exists and false if not
    function notEndedStakesExist() external view returns (bool) {
        uint256 today = getToday();
        return (stakesEndMax >= today);
    }

    // End stakes which period is expired but their owners didn't end them
    function endExpiredStakes() external {
        uint256 today = getToday();
        uint256 limit = allStakesNumber;
        for (uint256 stakeID = 1; stakeID <= limit; stakeID++) {
            if (
                stakes[stakeID].end < today && stakes[stakeID].isActive == true
            ) {
                console.log(
                    "k------------------------------------------------------------------"
                );
                coffinAddress = coffin.getCoffinAddress();
                endStaking(stakeID);
            }
        }
    }
}

