pragma solidity 0.8.4;

import "./ERC721.sol";
import "./ERC721URIStorage.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

// This contract is Ownable to be able set storefront on OpenSea
contract KingVampire is ERC721URIStorage, Ownable, ReentrancyGuard {
    address payable previousOwner;
    uint256 public lastPrice; // last price of this token in wei
    address payable public creator; // who receive royalty 1% from each buying
    bool isBuyingAvailable; // Is buy() function avaliable to use

    constructor(uint256 _lastPrice) ERC721("King Vampire", "ANCIENT") {
        _safeMint(msg.sender, 1);
        _setTokenURI(1, "https://VampireStaking.crypto/metadata/ANCIENT/1");
        lastPrice = _lastPrice;
        creator = payable(msg.sender);
        isBuyingAvailable = true;
    }

    /**
     * @dev TODO: Added.
     */
    function setKingURI(string memory hash) external {
        _setTokenURI(1, hash);
    }

    /**
     * @dev TODO: Modified.
     */
    function buy() external payable nonReentrant {
        require(isBuyingAvailable, "This method is not available now.");
        uint256 minOffer = (lastPrice * 11) / 10;
        require(
            msg.value >= minOffer,
            "Your offer must be at least 10% higher than the last price"
        );

        lastPrice = msg.value;

        previousOwner = payable(ownerOf(1));

        // move token from old owner to new one
        if (previousOwner != msg.sender) {
            // Clear approvals from the previous owner
            _approve(address(0), 1);

            _balances[previousOwner] -= 1;
            _balances[msg.sender] += 1;
            _owners[1] = msg.sender;
            require(
                _checkOnERC721Received(previousOwner, msg.sender, 1, ""),
                "ERC721: transfer to non ERC721Receiver implementer"
            );
            emit Transfer(previousOwner, msg.sender, 1);
        }

        uint256 creatorFee = (5 * msg.value) / 100;
        creator.transfer(creatorFee); // TODO: https://consensys.net/diligence/blog/2019/09/stop-using-soliditys-transfer-now/
        previousOwner.transfer(msg.value - creatorFee);
    }

    // TODO: Consider making this a one-time on, one-time off thing instead.
    function updateBuyingAvailability(bool _isBuyingAvailable) external {
        if (creator != previousOwner) {
            require(
                _isBuyingAvailable == false,
                "Creator cannot make force buying available unless creator is owner"
            );
        }
        require(msg.sender == creator, "Only creator can call this method");
        isBuyingAvailable = _isBuyingAvailable;
    }
}

