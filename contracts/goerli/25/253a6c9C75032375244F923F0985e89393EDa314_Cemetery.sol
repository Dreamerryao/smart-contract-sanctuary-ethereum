pragma solidity 0.8.4;

import "./StorageContract.sol";
import "./Helpers.sol";
import "./verifyIPFS.sol";
import "./CemeterySettings.sol";
import "./ERC721.sol";
import "./ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";

contract StakeNFT is ERC721URIStorage {
    using Strings for uint256;
    using verifyIPFS for string;

    mapping(address => mapping(uint256 => uint256)) public issuedTokens; // vampireAddress => (stakeID => tokenID)
    uint256 public tokensCounter; // current token id
    address public storageContractAddress;
    string public baseImageURI;

    event BaseURIUpdated(string baseURI);
    event BaseImageURIUpdated(string baseImageURI);

    constructor(
        address _storageContractAddress,
        string memory _baseURI,
        string memory _baseImageURI
    ) ERC721("Vampire Token", "VMP") {
        storageContractAddress = _storageContractAddress;
        _setBaseURI(_baseURI);
        baseImageURI = _baseImageURI;
    }

    /**
     * For OpenSea, to be able to set up storefront
     * @return the address of the owner.
     */
    function owner() public view returns (address) {
        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        KingVampire kv = KingVampire(storageContract.kingVampireAddress());
        return kv.ownerOf(1);
    }

    /**
     * For OpenSea, to be able to set up storefront
     * @return true if `msg.sender` is the owner of the contract.
     */
    function isOwner() external view returns (bool) {
        return msg.sender == owner();
    }

    modifier onlyOwner() {
        require(msg.sender == owner(), "Only the owner can do this.");
        _;
    }

    function updateBaseURI(string memory baseURI) external onlyOwner {
        _setBaseURI(baseURI);
        emit BaseURIUpdated(baseURI);
    }

    function getBaseURI() external view returns (string memory) {
        return _baseURI();
    }

    function updateBaseImageURI(string memory _baseImageURI)
        external
        onlyOwner
    {
        baseImageURI = _baseImageURI;
        emit BaseImageURIUpdated(baseImageURI);
    }

    function mint(
        address account,
        uint256 stakeId,
        string memory ipfsHashVampire
    ) external returns (uint256) {
        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        require(
            storageContract.isVampireAlive(msg.sender),
            "Wrong caller address"
        );
        require(
            issuedTokens[msg.sender][stakeId] == 0,
            "Token is already issued"
        );

        tokensCounter++;
        issuedTokens[msg.sender][stakeId] = tokensCounter;
        _safeMint(account, tokensCounter);
        _setTokenURI(tokensCounter, ipfsHashVampire);
        return tokensCounter;
    }

    function burn(uint256 stakeId) external {
        require(issuedTokens[msg.sender][stakeId] != 0, "Token is not found");
        _burn(issuedTokens[msg.sender][stakeId]);
    }

    function generateMetadata(
        address stakingTokenAddress,
        address rewardsTokenAddress,
        uint256 amountDeposited,
        uint256 daysLockedUp,
        string memory imageName
    ) private view returns (string memory) {
        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        Helpers helpers = storageContract.helpers();

        string memory rewardsTokenSymbol = helpers.getTokenSymbol(
            rewardsTokenAddress
        );
        string memory stakingTokenSymbol = helpers.getTokenSymbol(
            stakingTokenAddress
        );
        uint8 stakingTokenDecimals = helpers.getTokenDecimals(
            stakingTokenAddress
        );
        string memory amountDepositedInTokens = fromCoinsToTokens(
            amountDeposited,
            stakingTokenDecimals
        );

        string memory metadata = concat('{"name":"Count ', stakingTokenSymbol);
        metadata = concat(metadata, " von ");
        metadata = concat(metadata, rewardsTokenSymbol);
        metadata = concat(metadata, '","description":"');
        metadata = concat(metadata, amountDepositedInTokens);
        metadata = concat(metadata, " ");
        metadata = concat(metadata, stakingTokenSymbol);
        metadata = concat(metadata, " staked for ");
        metadata = concat(metadata, daysLockedUp.toString());
        metadata = concat(metadata, " days. Reward: ");
        metadata = concat(metadata, rewardsTokenSymbol);
        metadata = concat(metadata, '.","image":"');
        metadata = concat(metadata, baseImageURI);
        metadata = concat(metadata, imageName);
        metadata = concat(metadata, '"}');
        return metadata;
    }

    function checkIPFS(
        string memory ipfsHashVampire,
        uint256 amountDeposited,
        uint256 daysLockedUp,
        string memory imageName,
        address stakingTokenAddress,
        address rewardsTokenAddress
    ) external view returns (bool) {
        string memory metadata = generateMetadata(
            stakingTokenAddress,
            rewardsTokenAddress,
            amountDeposited,
            daysLockedUp,
            imageName
        );
        return metadata.verifyHash(ipfsHashVampire);
    }

    // Fot tests
    function getHash(
        uint256 amountDeposited,
        uint256 daysLockedUp,
        string memory imageName,
        address stakingTokenAddress,
        address rewardsTokenAddress
    ) external view returns (bytes memory) {
        string memory metadata = generateMetadata(
            stakingTokenAddress,
            rewardsTokenAddress,
            amountDeposited,
            daysLockedUp,
            imageName
        );

        return metadata.generateHash();
    }

    // Concatenate two strings
    function concat(string memory _a, string memory _b)
        internal
        pure
        returns (string memory)
    {
        bytes memory bytes_a = bytes(_a);
        bytes memory bytes_b = bytes(_b);
        string memory length_ab = new string(bytes_a.length + bytes_b.length);
        bytes memory bytes_c = bytes(length_ab);
        uint256 k = 0;
        for (uint256 i = 0; i < bytes_a.length; i++) bytes_c[k++] = bytes_a[i];
        for (uint256 i = 0; i < bytes_b.length; i++) bytes_c[k++] = bytes_b[i];
        return string(bytes_c);
    }

    // Converting coins to tokens: coins/decimals with rounding.
    // If tokens num is integer return without decimal places.
    // If tokens num is float and more than 0.01 round to 2 decimal places.
    // If tokens num is less than 0.01 return '<0.01'
    function fromCoinsToTokens(uint256 coins, uint8 _decimals)
        public
        pure
        returns (string memory)
    {
        uint256 decimals = uint256(_decimals);
        uint256 quotient = preciseDiv(coins, 10**decimals, 2); // (coins/10^decimals)*100

        if (quotient == 0) {
            return "<0.01";
        }

        if (((quotient / 100) * 100) == quotient) {
            // result is integer
            return (quotient / 100).toString();
        }

        // result is float, add decimal point
        uint256 leftFromPoint = quotient / 100;
        uint256 rightFromPoint = quotient - leftFromPoint * 100;
        string memory result = concat(leftFromPoint.toString(), ".");
        if (rightFromPoint < 10) {
            result = concat(result, "0");
        }
        result = concat(result, rightFromPoint.toString());
        return result;
    }

    // Makes division with correct rounding. Result is multiplied by 10**precision, e.g. (1, 2, 2) returns 50,
    // (5, 2, 0) returns 3.
    function preciseDiv(
        uint256 numerator,
        uint256 denominator,
        uint256 precision
    ) public pure returns (uint256 quotient) {
        uint256 _numerator = numerator * (10**(precision + 1));
        // with rounding of last digit
        uint256 _quotient = ((_numerator / denominator) + 5) / 10;
        return (_quotient);
    }
}

