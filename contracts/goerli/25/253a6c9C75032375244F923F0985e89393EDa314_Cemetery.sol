pragma solidity 0.8.4;

import "./Vampire.sol";
import "./Coffin.sol";
import "./Helpers.sol";
import "./KingVampire.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract StorageContract {
    using Counters for Counters.Counter;
    Counters.Counter public vampireId;
    Counters.Counter public lastVampireId;

    Counters.Counter public stakerId;
    Counters.Counter public lastStakerId;

    address[] public vampires;
    uint256 public aliveVampiresCount;
    uint256 public killedVampiresCount;
    address public kingVampireAddress;
    address public cemeteryAddress;
    Helpers public helpers;

    /**
     * @dev TODO: Added.
     */
    enum ImageType {
        KING,
        STAKER,
        ENTOMB
    }

    ImageType public imageTypes;

    enum Status {
        PENDING,
        UNDEAD,
        KILLED
    }

    struct VampireIpfs {
        uint256 vampireId;
        string hash;
        bool isUsed;
        ImageType imageType;
    }

    struct StakerIpfs {
        address staker;
        string hash;
        bool isUsed;
        ImageType imageType;
    }

    struct TheVampire {
        uint256 id;
        address vampireAddress;
        address coffinAddress;
        VampireIpfs vampireHashObject;
        Status vampireStatus;
    }

    mapping(uint256 => VampireIpfs) ipfsByVampireId;
    mapping(uint256 => StakerIpfs) ipfsByStakerId;
    mapping(address => StakerIpfs) stakerHashObject; // every vampire could have many stakers

    mapping(uint256 => TheVampire) public vampireById;
    mapping(address => TheVampire) public vampireByAddress;

    mapping(address => bool) public vampireStatuses; // Vampire address => vampire status (true - alive, false - killed)
    mapping(address => address) public coffinsToVampires; // Coffin address => vampire address

    event CemeterySet(address cemeteryAddress);
    event VampireAdded(address vampireAddress);
    event CoffinAdded(address coffinAddress);
    event VampireKilled(address vampireAddress);
    event HelpersAddressUpdated(address helpersAddress);
    event PoolAdded(
        uint256 currentId,
        string hash,
        address vampireAddress,
        address coffinAddress
    );

    constructor(address _kingVampireAddress, address _helpersAddress) {
        kingVampireAddress = _kingVampireAddress;
        helpers = Helpers(_helpersAddress);
    }

    modifier onlyOwner() {
        KingVampire kv = KingVampire(kingVampireAddress);
        require(msg.sender == kv.ownerOf(1), "Only the owner can do this.");
        _;
    }

    // Can be called one time only
    function setCemetery(address _cemeteryAddress) external onlyOwner {
        require(
            _cemeteryAddress != address(0),
            "Cemetery address is zero address."
        );
        require(
            cemeteryAddress == address(0),
            "Cemetery address is already set."
        );
        cemeteryAddress = _cemeteryAddress;
        emit CemeterySet(cemeteryAddress);
    }

    function addVampireAndCoffin(address vampireAddress, address coffinAddress)
        external
    {
        require(cemeteryAddress == msg.sender, "Wrong caller.");
        require(vampireAddress != address(0), "Vampire address is zero.");
        require(coffinAddress != address(0), "Coffin address is zero.");

        /**
         * @dev TODO: Added/Modified.
         * empty Vampire is created by addIpfsHash() and
         * vampire id is incremented and shared with IPFS struct
         * here, we check if it exists then we add vampireAddress
         * and other attributes.
         * ex. add an array of hashes, increment vampire id by 1 for every element
         *     starting from 0 add to lastVampireId and see if an empty vampire with
         *     this id has been created
         */
        lastVampireId.increment();
        uint256 currentId = lastVampireId.current();
        require(
            vampireById[currentId].id != 0,
            "You need to add more vampire hashes!"
        );
        // attach hash and other attributes
        string memory ipfsHash = getVampireIpfsHash(
            currentId,
            ImageType.ENTOMB
        ); // entomb

        require(!ipfsByVampireId[currentId].isUsed, "this hash has been used!");

        ipfsByVampireId[currentId].isUsed = true;

        // VampireIpfs memory
        // vampireById[currentId].vampireHashObject = ipfsHash;

        TheVampire memory theVampire = vampireById[currentId];
        theVampire.vampireAddress = vampireAddress;
        theVampire.coffinAddress = coffinAddress;
        theVampire.vampireStatus = Status.UNDEAD; // activate (undead)

        vampireByAddress[vampireAddress] = theVampire;

        vampires.push(vampireAddress);
        vampireStatuses[vampireAddress] = true;
        aliveVampiresCount += 1;
        coffinsToVampires[coffinAddress] = vampireAddress;

        emit VampireAdded(vampireAddress);
        emit CoffinAdded(coffinAddress);
        emit PoolAdded(currentId, ipfsHash, vampireAddress, coffinAddress);
    }

    // Change vampire status
    function killVampire() external {
        address vampireAddress = coffinsToVampires[msg.sender];
        require(isVampireAlive(vampireAddress), "Wrong vampire address");
        /**
         * @dev TODO: Added.
         */
        TheVampire memory theVampire = vampireByAddress[vampireAddress];
        theVampire.vampireStatus = Status.KILLED;

        vampireStatuses[vampireAddress] = false;

        aliveVampiresCount -= 1;
        killedVampiresCount += 1;

        emit VampireKilled(vampireAddress);
    }

    function getAliveVampires() external view returns (address[] memory) {
        address[] memory aliveVampires = new address[](aliveVampiresCount);
        uint256 allVampiresCount = aliveVampiresCount + killedVampiresCount;
        uint256 count;
        for (uint256 i = 0; i < allVampiresCount; i++) {
            address vampireAddress = vampires[i];
            if (vampireStatuses[vampireAddress]) {
                aliveVampires[count] = vampireAddress;
                count += 1;
            }
        }
        return aliveVampires;
    }

    function getKilledVampires() external view returns (address[] memory) {
        address[] memory killedVampires = new address[](killedVampiresCount);
        uint256 allVampiresCount = aliveVampiresCount + killedVampiresCount;
        uint256 count;
        for (uint256 i = 0; i < allVampiresCount; i++) {
            address vampireAddress = vampires[i];
            if (vampireStatuses[vampireAddress] == false) {
                killedVampires[count] = vampireAddress;
                count += 1;
            }
        }
        return killedVampires;
    }

    function isVampireAlive(address vampireAddress) public view returns (bool) {
        return vampireStatuses[vampireAddress];
    }

    function updateHelpersAddress(address _helpersAddress) external onlyOwner {
        require(_helpersAddress != address(0), "Zero address");
        helpers = Helpers(_helpersAddress);
        emit HelpersAddressUpdated(_helpersAddress);
    }

    /**
     * @dev TODO: Added.
     * create ipfs objects/structs with no hash but share an id with vampire
     */
    function addVampireIpfsHash(string[] memory hashes) public onlyOwner {
        for (uint256 i; i < hashes.length; i++) {
            vampireId.increment();
            uint256 idVampire = vampireId.current();
            require(
                !ipfsByVampireId[idVampire].isUsed,
                "this hash is already used"
            );

            ipfsByVampireId[idVampire] = VampireIpfs(
                idVampire,
                hashes[i],
                false, // isUsed
                ImageType.ENTOMB
            );

            vampireById[idVampire] = TheVampire(
                idVampire,
                address(0),
                address(0),
                ipfsByVampireId[idVampire],
                Status.PENDING // prepare a struct for new vampire (Pending)
            );
            // Staker not attached to any vampires
        }
    }

    /**
     * @dev TODO: Added.
     * create ipfs objects/structs with no hash but share an id with vampire
     */
    function addStakerIpfsHash(string[] memory hashes) public onlyOwner {
        for (uint256 i; i < hashes.length; i++) {
            stakerId.increment();
            uint256 idStaker = stakerId.current();
            require(
                !ipfsByStakerId[idStaker].isUsed,
                "this hash is already used"
            );
            // empty staker ipfs object
            ipfsByStakerId[idStaker] = StakerIpfs(
                address(0),
                hashes[i],
                false, // isUsed
                ImageType.STAKER // Staker
            );
        }
    }

    /**
     * @dev TODO: Added.
     */
    function addKingIpfsHash(string memory hash) public onlyOwner {
        KingVampire kv = KingVampire(kingVampireAddress);
        kv.setKingURI(hash);
    }

    /**
     * @dev TODO: Added.
     */
    function getVampireIpfsHash(
        uint256 idVampire,
        ImageType imageType // 0,1,2 - king,staker,entomb
    ) public view returns (string memory) {
        // entomb
        require(idVampire != 0, "Id can not be zero!");
        require(imageType == ImageType.ENTOMB, "Only Entomb images!");
        VampireIpfs memory ipfsHash = ipfsByVampireId[idVampire];
        return ipfsHash.hash;
    }

    /**
     * @dev TODO: Added.
     */
    function getStakerIpfsHash(address vampireAddress)
        external
        returns (string memory)
    {
        // staker
        require(
            vampireByAddress[vampireAddress].vampireStatus == Status.UNDEAD,
            "we need an active vampire!"
        );
        lastStakerId.increment();
        uint256 currentId = lastStakerId.current();
        StakerIpfs memory ipfsHash = ipfsByStakerId[currentId];
        ipfsHash.isUsed = true;
        return ipfsHash.hash;
    }
}

