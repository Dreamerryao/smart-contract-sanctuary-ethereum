pragma solidity 0.8.4;

import "./ERC20.sol";
import "./Vampire.sol";
import "./Cemetery.sol";
import "./StorageContract.sol";
import "./CemeterySettings.sol";
import "hardhat/console.sol";

contract Coffin {
    address public rewardsTokenAddress;
    uint256 public periodInDays;
    address public owner; // owner of coffin, i.e. who created it
    address public vampireAddress;
    Cemetery public cemetery;
    uint16 public feeRate;
    string public ipfsHash;

    // Rewards per day multiplied by 1000. We cannot calculate this value from rewardsBalance because
    // rewardsBalance goes less after each rewards payment and rewardsPerDay is the same all the time,
    // it can only grow if more funds to coffin were added
    uint256 public rewardsPerDay;
    uint256 public rewardsBalance;

    mapping(uint256 => bool) public sentRewards; // List of stakeIDs at which we have already sent rewards

    event FeeUpdated(uint16 feeRate);

    event FundsAdded(uint256 rewardsAmount);

    event RewardsPerDayUpdated(uint256 rewardsPerDay);

    event RewardsBalanceIncreased(uint256 rewardsAmountAdded);

    event RewardsBalanceSubtracted(uint256 rewardsAmountSubtracted);

    event CoffinClosed(uint256 rewardsSentBackToOwner);

    event CoffinAutoClosed(uint256 rewardsRemainderSentToFeeReceiver);

    modifier isOwner() {
        require(msg.sender == owner, "Only the owner can do this.");
        _;
    }

    constructor(
        uint16 _currentFee, // current fee of Cemetery Settings
        address _stakingTokenAddress,
        address _rewardsTokenAddress,
        uint256 _periodInDays,
        uint256 _rewardsAmount,
        address _owner,
        address stakeNFTContractAddress
    ) {
        feeRate = _currentFee;
        rewardsTokenAddress = _rewardsTokenAddress;
        periodInDays = _periodInDays;
        owner = _owner;
        cemetery = Cemetery(msg.sender);
        rewardsBalance = _rewardsAmount;
        rewardsPerDay = (rewardsBalance * 1000) / periodInDays; // we store rewardsPerDay multiplied by 1000
        Vampire vampire = new Vampire(
            this,
            _stakingTokenAddress,
            stakeNFTContractAddress
        );
        vampireAddress = address(vampire);
    }

    function getCoffinAddress() external view returns (address) {
        return address(this);
    }

    // Add funds to rewards pool
    function addFunds(uint256 rewardsAmount) external {
        Vampire vampire = Vampire(vampireAddress);
        require(rewardsAmount > 0, "Wrong amount");
        require(vampire.getCoffinDaysLeft() > 0, "Coffin expired");

        // Calculate rewards with old rewardsPerDay till yesterday and save result in the Stake.awarded field.
        vampire.distributeRewards();

        // From today we will count rewardsPerDay with the added funds.
        updateRewardsPerDayAndRewardsBalance(rewardsAmount);

        // Transfer rewards tokens from caller to this contract.
        ERC20 rewardsToken = ERC20(rewardsTokenAddress);
        require(
            rewardsToken.transferFrom(msg.sender, address(this), rewardsAmount),
            "Transfer failed"
        );
        emit FundsAdded(rewardsAmount);
    }

    function updateRewardsPerDayAndRewardsBalance(uint256 rewardsAmountAdded)
        private
    {
        rewardsBalance += rewardsAmountAdded;
        emit RewardsBalanceIncreased(rewardsAmountAdded);

        Vampire vampire = Vampire(vampireAddress);
        uint256 daysLeft = vampire.getCoffinDaysLeft();
        if (daysLeft > 0) {
            rewardsPerDay += (rewardsAmountAdded * 1000) / daysLeft; // we store rewardsPerDay multiplied by 1000
            emit RewardsPerDayUpdated(rewardsPerDay);
        }
    }

    // Close the contract and send remaining rewards tokens to owner.
    // Contract cannot be closed if there are not ended stakes.
    function close() external isOwner {
        Vampire vampire = Vampire(vampireAddress);
        require(
            vampire.notEndedStakesExist() == false,
            "Not ended stakes exist"
        );

        if (vampire.activeStakesNumber() > 0) {
            vampire.endExpiredStakes(); // End stakes where the period is expired but their owners didn't end them
        }

        address storageAddress = cemetery.storageContractAddress();

        StorageContract storageContract = StorageContract(storageAddress);

        storageContract.killVampire();

        // If some rewards are remaining, send them back to the contract owner.
        ERC20 rewardsToken = ERC20(rewardsTokenAddress);
        uint256 remainingRewards = rewardsToken.balanceOf(address(this));
        if (remainingRewards > 0) {
            require(
                rewardsToken.transfer(msg.sender, remainingRewards),
                "Transfer failed"
            );
        }

        emit CoffinClosed(remainingRewards);
    }

    // Autoclose coffin when last stake is ended and coffin period is finished.
    // Send remaining rewards if any to fee receiver (the rewards may have remained due to rounding daily payout down).
    // The main purpose of this function is to send remainingRewards, which can left due to rounding down, to fee receiver.
    function autoClose() private {
        StorageContract storageContract = StorageContract(
            cemetery.storageContractAddress()
        );
        storageContract.killVampire();

        CemeterySettings settings = cemetery.settings();

        ERC20 rewardsToken = ERC20(rewardsTokenAddress);
        uint256 remainingRewards = rewardsToken.balanceOf(address(this));
        if (remainingRewards > 0) {
            address feeTo = settings.feeTo();
            if (feeTo != address(0)) {
                rewardsToken.transfer(feeTo, remainingRewards);
            } else {
                rewardsToken.transfer(owner, remainingRewards);
            }
        }

        emit CoffinAutoClosed(remainingRewards);
    }

    function sendRewards(uint256 stakeID) external {
        require(sentRewards[stakeID] == false, "Rewards already sent");
        Vampire vampire = Vampire(vampireAddress);
        require(vampire.isStakeActive(stakeID) == false, "Stake is active");

        sentRewards[stakeID] = true;
        uint256 awardedAmount = vampire.getAwardedAmount(stakeID);

        if (awardedAmount > 0) {
            uint256 feeAmount;
            CemeterySettings settings = cemetery.settings();

            address feeTo = settings.feeTo();

            /**
             * @dev TODO: Modified.
             * uint256 feeRate = settings.fee(); // only on initiliazation
             */
            if (feeTo != address(0) && feeRate > 0) {
                uint256 numerator = awardedAmount * feeRate;
                uint256 denominator = 10000;
                feeAmount = vampire.preciseDiv(numerator, denominator, 0);
            }

            rewardsBalance = rewardsBalance - awardedAmount;
            emit RewardsBalanceSubtracted(awardedAmount);

            ERC20 rewardsToken = ERC20(rewardsTokenAddress);
            if (feeTo != address(0) && feeAmount > 0) {
                require(
                    rewardsToken.transfer(feeTo, feeAmount),
                    "Fee transfer failed"
                );
            }

            require(
                rewardsToken.transfer(
                    vampire.getStakerAddress(stakeID),
                    awardedAmount - feeAmount
                ),
                "Rewards transfer failed"
            );
        }

        // Close coffin if this is a last stake and coffin period is finished
        if (
            vampire.activeStakesNumber() == 0 &&
            vampire.getCoffinDaysLeft() == 0
        ) {
            autoClose();
        }
    }

    function updateRewardsPerDayIfNewRewardsCome() external {
        ERC20 rewardsToken = ERC20(rewardsTokenAddress);
        uint256 rewardsAdded = rewardsToken.balanceOf(address(this)) -
            rewardsBalance;
        if (rewardsAdded > 0) {
            updateRewardsPerDayAndRewardsBalance(rewardsAdded);
        }
    }

    function updateCoffinIpfs(string memory newHash) public isOwner {
        ipfsHash = newHash;
    }
}

