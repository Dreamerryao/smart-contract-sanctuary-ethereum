pragma solidity 0.8.4;

import "./StorageContract.sol";

contract CemeterySettings {
    uint16 public fee; // fee taken from rewards in % multiplied by 100, i.e. 125 means 1.25%
    address public feeTo;

    // min and max period in days
    uint16 public minCoffinPeriod;
    uint16 public maxCoffinPeriod;
    uint16 public minStakePeriod;
    uint16 public maxStakePeriod;

    address public stakeNFTContractAddress;
    address public storageContractAddress;

    event FeeToUpdated(address feeTo);
    event FeeUpdated(uint16 fee);
    event NFTContractAddressUpdated(address stakeNFTContractAddress);
    event CoffinPeriodUpdated(uint16 minCoffinPeriod, uint16 maxCoffinPeriod);
    event StakePeriodUpdated(uint16 minStakePeriod, uint16 maxStakePeriod);

    constructor(
        address _storageContractAddress,
        address _feeTo,
        uint16 _minCoffinPeriod,
        uint16 _maxCoffinPeriod,
        uint16 _minStakePeriod,
        uint16 _maxStakePeriod,
        address _stakeNFTContractAddress
    ) {
        storageContractAddress = _storageContractAddress;
        feeTo = _feeTo;
        minCoffinPeriod = _minCoffinPeriod;
        maxCoffinPeriod = _maxCoffinPeriod;
        minStakePeriod = _minStakePeriod;
        maxStakePeriod = _maxStakePeriod;
        stakeNFTContractAddress = _stakeNFTContractAddress;
    }

    modifier onlyOwner() {
        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        KingVampire kv = KingVampire(storageContract.kingVampireAddress());
        require(msg.sender == kv.ownerOf(1), "Only the owner can do this.");
        _;
    }

    // Update min and max coffin period in days
    function updateMinAndMaxCoffinPeriod(
        uint16 _minCoffinPeriod,
        uint16 _maxCoffinPeriod
    ) external onlyOwner {
        minCoffinPeriod = _minCoffinPeriod;
        maxCoffinPeriod = _maxCoffinPeriod;
        emit CoffinPeriodUpdated(minCoffinPeriod, maxCoffinPeriod);
    }

    // Update min and max stake period in days
    function updateMinAndMaxStakePeriod(
        uint16 _minStakePeriod,
        uint16 _maxStakePeriod
    ) external onlyOwner {
        minStakePeriod = _minStakePeriod;
        maxStakePeriod = _maxStakePeriod;
        emit StakePeriodUpdated(minStakePeriod, maxStakePeriod);
    }

    function updateFeeTo(address newFeeTo) external onlyOwner {
        require(newFeeTo != address(0), "Zero address");
        feeTo = newFeeTo;
        emit FeeToUpdated(feeTo);
    }

    function updateFee(uint16 newFee) external onlyOwner {
        fee = newFee;
        emit FeeUpdated(fee);
    }

    function updateNFTContractAddress(address _stakeNFTContractAddress)
        external
        onlyOwner
    {
        require(_stakeNFTContractAddress != address(0), "Zero address");
        stakeNFTContractAddress = _stakeNFTContractAddress;
        emit NFTContractAddressUpdated(stakeNFTContractAddress);
    }

    function getMinMaxPeriods()
        external
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return (
            minCoffinPeriod,
            maxCoffinPeriod,
            minStakePeriod,
            maxStakePeriod
        );
    }
}

