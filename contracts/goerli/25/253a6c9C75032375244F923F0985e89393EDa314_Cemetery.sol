pragma solidity 0.8.4;

import "./ERC20.sol";
import "./Coffin.sol";
import "./StorageContract.sol";
import "./KingVampire.sol";
import "./Helpers.sol";
import "./CemeterySettings.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

contract Cemetery is Initializable, UUPSUpgradeable {
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    address public storageContractAddress;
    address public settingsAddress;
    CemeterySettings public settings;

    event NewEntryCreated(
        address coffinAddress,
        address vampireAddress,
        uint256 periodInDays,
        uint256 rewardsAmount,
        address owner
    );

    event NewEntryTokensData(
        address vampireAddress,
        address stakingTokenAddress,
        address rewardsTokenAddress,
        string stakingTokenSymbol,
        string rewardsTokenSymbol,
        uint8 stakingTokenDecimals,
        uint8 rewardsTokenDecimals
    );

    event SettingsAddressUpdated(address settingsAddress);

    modifier onlyOwner() {
        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        KingVampire kv = KingVampire(storageContract.kingVampireAddress());
        require(msg.sender == kv.ownerOf(1), "Only the owner can do this.");
        _;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    // Replacement of constructor for an upgradable contract.
    // initializer modifier takes care that this function is called only one time
    function initialize(
        address _storageContractAddress,
        address _settingsAddress
    ) external initializer {
        storageContractAddress = _storageContractAddress;
        settings = CemeterySettings(_settingsAddress);
        __UUPSUpgradeable_init();
    }

    function createNewEntry(
        address _stakingTokenAddress,
        address _rewardsTokenAddress,
        uint256 _periodInDays,
        uint256 _rewardsAmount
    ) external {
        require(_stakingTokenAddress != address(0), "Staking token is zero");
        require(_rewardsTokenAddress != address(0), "Rewards token is zero");
        require(_rewardsAmount > 0, "Wrong rewards amount");
        require(
            _periodInDays >= settings.minCoffinPeriod(),
            "The period is too short"
        );
        require(
            _periodInDays <= settings.maxCoffinPeriod(),
            "The period is too long"
        );

        /**
         * @dev TODO: Added.
         */
        uint16 currentFee = settings.fee();

        Coffin coffin = new Coffin(
            currentFee,
            _stakingTokenAddress,
            _rewardsTokenAddress,
            _periodInDays,
            _rewardsAmount,
            msg.sender,
            settings.stakeNFTContractAddress()
        );
        address vampireAddress = coffin.vampireAddress();

        StorageContract storageContract = StorageContract(
            storageContractAddress
        );
        storageContract.addVampireAndCoffin(vampireAddress, address(coffin));

        Helpers helpers = storageContract.helpers();
        string memory rewardsTokenSymbol = helpers.getTokenSymbol(
            _rewardsTokenAddress
        );
        string memory stakingTokenSymbol = helpers.getTokenSymbol(
            _stakingTokenAddress
        );
        uint8 rewardsTokenDecimals = helpers.getTokenDecimals(
            _rewardsTokenAddress
        );
        uint8 stakingTokenDecimals = helpers.getTokenDecimals(
            _stakingTokenAddress
        );

        // Transfer rewards tokens from msg.sender to coffin
        ERC20 rewardsToken = ERC20(_rewardsTokenAddress);
        require(
            rewardsToken.transferFrom(
                msg.sender,
                address(coffin),
                _rewardsAmount
            ),
            "Rewards transfer failed"
        );

        emit NewEntryCreated(
            address(coffin),
            vampireAddress,
            _periodInDays,
            _rewardsAmount,
            msg.sender
        );

        emit NewEntryTokensData(
            vampireAddress,
            _stakingTokenAddress,
            _rewardsTokenAddress,
            stakingTokenSymbol,
            rewardsTokenSymbol,
            stakingTokenDecimals,
            rewardsTokenDecimals
        );
    }

    function updateSettingsAddress(address _settingsAddress)
        external
        onlyOwner
    {
        require(_settingsAddress != address(0), "Zero address");
        settings = CemeterySettings(_settingsAddress);
        emit SettingsAddressUpdated(_settingsAddress);
    }

    function getMinMaxPeriods()
        external
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return settings.getMinMaxPeriods();
    }
}

