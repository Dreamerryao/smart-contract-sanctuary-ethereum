/******************************************************************************\
/// @title Library Status Control
/// @author Ather Labs
/******************************************************************************/

import {LibAppStorage, AppStorage} from "./AppStorage.sol";

library LibStatusControl {
    /// @notice Checks if deposit features are enabled
    function _requireDepositEnable() internal view {
        AppStorage storage s = LibAppStorage.diamondStorage();
        require(s.status == 1, "LSta:deposit disabled");
    }

    /// @notice Checks if operations features are enabled
    function _requireOperationsEnable() internal view {
        AppStorage storage s = LibAppStorage.diamondStorage();
        require(s.status == 1 || s.status == 2, "LSta:operations disabled");
    }

    /// @notice Checks if withdraw features are enabled
    function _requireWithdrawEnable() internal view {
        AppStorage storage s = LibAppStorage.diamondStorage();
        require(
            s.status == 1 || (s.status == 0 && (block.timestamp < s.maintenanceExpirationTime / 2)),
            "LSta:withdraw disabled"
        );
    }

    /// @notice Checks that current state is Maintenance
    function _requireExitEnable() internal view {
        AppStorage storage s = LibAppStorage.diamondStorage();
        require(s.status == 3, "LSta: Exis disabled. Contract is not in mass exit mode");
    }
}

