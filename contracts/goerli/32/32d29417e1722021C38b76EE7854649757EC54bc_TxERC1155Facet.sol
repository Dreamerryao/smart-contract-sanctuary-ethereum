/******************************************************************************\
/// @title Contract Facet for ERC1155 Token Transaction
/// @author Ather Labs
/******************************************************************************/

import {AppStorage} from "../libraries/AppStorage.sol";
import {LibTokenManagement} from "../libraries/LibTokenManagement.sol";
import {LibSafeCast} from "../libraries/LibSafeCast.sol";
import {LibOperations} from "../libraries/LibOperations.sol";
import {LibStatusControl} from "../libraries/LibStatusControl.sol";
import {IOperations} from "../interfaces/IOperations.sol";
import {IERC1155} from "../interfaces/IERC1155.sol";

contract TxERC1155Facet is IOperations {
    /* ------ TYPE & VARIABLES ------ */
    AppStorage internal s;

    /* ------ EVENTS ------ */
    /// @notice
    event NewDeposit(
        address from,
        uint8 listingId,
        uint88 tokenId,
        uint88 amount,
        uint32 currentBatchId,
        uint256 newRequestId
    );

    /// @notice
    event NewDepositBatch(
        address from,
        uint8 listingId,
        uint88[] tokenIds,
        uint88[] amounts,
        uint32 currentBatchId,
        uint256 newRequestId
    );

    /* ------ EXTERNAL FUNTIONS ------ */
    /// @notice
    function depositERC1155(
        uint88 _tokenId,
        uint88 _amount,
        IERC1155 _tokenAddress
    ) external {
        LibStatusControl._requireDepositEnable();

        uint8 listingId_ = LibTokenManagement._validateTokenAddress(address(_tokenAddress));
        LibTokenManagement._requireUnpaused(listingId_);

        uint256 balanceBefore = _tokenAddress.balanceOf(address(this), _tokenId);
        _tokenAddress.safeTransferFrom(msg.sender, address(this), _tokenId, _amount, "");
        uint256 balanceAfter = _tokenAddress.balanceOf(address(this), _tokenId);
        uint88 depositAmount = LibSafeCast.toUint88(balanceAfter - balanceBefore);
        require(
            depositAmount > 0 && depositAmount <= LibOperations.MAX_DEPOSIT_NFT_AMOUNT,
            "TE1155:invalid deposit amount"
        );

        Deposit memory deposit = Deposit({
            from: msg.sender,
            listingId: listingId_,
            tokenId: _tokenId,
            amount: depositAmount
        });
        bytes32 hashDepositRequest = keccak256(
            abi.encode(s.domainSeparator, LibOperations._hashDepositRequest(deposit))
        );
        uint256 newRequestId = LibOperations._addPendingRequest(hashDepositRequest);

        emit NewDeposit(msg.sender, deposit.listingId, deposit.tokenId, deposit.amount, s.currentBatchId, newRequestId);
    }

    /// @notice
    function depositBatchERC1155(
        IERC1155 _tokenAddress,
        uint88[] calldata _tokenIds,
        uint88[] calldata _amounts
    ) external {
        require(_tokenIds.length == _amounts.length, "TE1155:invalid data length");
        require(_tokenIds.length > 1 && _tokenIds.length <= 10, "TE1155:invalid array length");
        LibStatusControl._requireDepositEnable();

        uint8 listingId_ = LibTokenManagement._validateTokenAddress(address(_tokenAddress));
        LibTokenManagement._requireUnpaused(listingId_);

        uint256[] memory tokenIds = new uint256[](_tokenIds.length);
        uint256[] memory amounts = new uint256[](_tokenIds.length);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            tokenIds[i] = _tokenIds[i];
            amounts[i] = _amounts[i];
            uint256 balanceBefore = _tokenAddress.balanceOf(address(this), tokenIds[i]);
            _tokenAddress.safeTransferFrom(msg.sender, address(this), tokenIds[i], amounts[i], "");
            uint256 balanceAfter = _tokenAddress.balanceOf(address(this), tokenIds[i]);
            uint128 depositAmount = LibSafeCast.toUint88(balanceAfter - balanceBefore);
            require(
                depositAmount > 0 && depositAmount <= LibOperations.MAX_DEPOSIT_NFT_AMOUNT,
                "TE1155:invalid deposit amount"
            );
        }

        DepositBatch memory depositBatch = DepositBatch({
            from: msg.sender,
            listingId: listingId_,
            arrayLength: LibSafeCast.toUint8(_tokenIds.length),
            tokenIds: _tokenIds,
            amounts: _amounts
        });

        bytes32 hashDepositBatchRequest = keccak256(
            abi.encode(s.domainSeparator, LibOperations._hashDepositBatchRequest(depositBatch))
        );
        uint256 newRequestId = LibOperations._addPendingRequest(hashDepositBatchRequest);

        emit NewDepositBatch(
            msg.sender,
            depositBatch.listingId,
            depositBatch.tokenIds,
            depositBatch.amounts,
            s.currentBatchId,
            newRequestId
        );
    }

    /* ------ INTERNAL FUNTIONS ------ */
}

