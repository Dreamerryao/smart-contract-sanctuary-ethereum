pragma solidity 0.8.17;
import "hardhat/console.sol";
import "./SpaceCoin.sol";
import "./helpers.sol";

contract ICO is Helpers {
    SpaceCoin public spaceCoin;

    // ------ ICO STATE ------
    address public immutable owner;
    address public immutable treasury;

    /// Total contributions made to the ICO
    uint256 public totalContributions;

    /// Tracks the current phase the ICO is in
    Phases public phase;

    /// "Activity" is a bad name; this is just tracking whether fundraising and token redemption are toggled on or off
    ActivityStatus private status = ActivityStatus(true, true);

    /// Mapping of addresses that are allowed to contribute during the seed phase
    mapping(address => bool) public allowList;

    /// Mapping of addresses to the total amount they have contributed to the ICO
    mapping(address => uint256) public contributors;

    /// Mapping of addresses the the number of unclaimed (or unredeemed) tokens they have been awarded
    mapping(address => uint256) public unclaimedTokens;

    constructor(
        address _owner,
        address _treasury,
        address[] memory _allowList
    ) {
        spaceCoin = new SpaceCoin(
            "SpaceCoin",
            "SPC",
            _owner,
            _treasury,
            address(this)
        );
        owner = _owner;
        treasury = _treasury;
        createAllowListMapping(_allowList);
    }

    /**  @notice Allows an address to contribute to the ICO
     * Only addresses on the allowList are allowed to contribute during the seed phase.
     * The sum of the new contribution and any previous contributions made by that investor cannot exceed the contribution limit
     * Tokens are awarded to each investor at a rate of 5 SPC: 1 ETH
     */
    /// @dev the contribution limit is dependent upon the current phase, as well as any previous contributions the investor may have made
    function contribute() external payable {
        if (!status.fundraising) revert Unauthorized("Fundraising paused");
        if (phase == Phases.SEED && !allowList[msg.sender])
            revert Unauthorized("Not on Allow List");
        uint256 totalLimit = phase == Phases.SEED ? 15_000 ether : 30_000 ether;
        uint256 individualLimit = getContributionLimit(totalLimit);

        if (msg.value > individualLimit)
            revert ContributionLimitExceeded(individualLimit, msg.value);
        contributors[msg.sender] += msg.value;
        unclaimedTokens[msg.sender] += msg.value * 5;
        totalContributions += msg.value;

        if (totalContributions == totalLimit) {
            emit PhaseLimitReached(phase, totalLimit);
        }
        emit ContributionMade(msg.sender, msg.value);
    }

    /// @notice Moves invested funds from the ICO contract
    /// @dev Withdraws the total amount of ether on the contract, rather than the totalContributionsMade. This means the value of totalContributions ca nbe used to track fundraising limits.
    /// @param to The address to move the invested funds to
    function withdraw(address to) external onlyOwner(owner) {
        if (to != treasury)
            revert Unauthorized("Funds must be sent to treasury");
        uint256 withdrawalAmount = address(this).balance;
        (bool success, ) = address(to).call{value: withdrawalAmount}("");
        require(success, "Withdrawal tx failed");
        emit WithdrawalMade(to, withdrawalAmount);
    }

    /**
     * Allows an investor to claim any tokens they have been awarded once the ICO has reached the Open phase
     * The investor can make additional contributions after claiming previously awarded tokens and then claim their new tokens
     */
    function claimTokens() external {
        if (phase != Phases.OPEN)
            revert Unauthorized(
                "Tokens can only be redeemed in the Open phase"
            );
        if (unclaimedTokens[msg.sender] == 0)
            revert Unauthorized("You have no unclaimed tokens");
        if (!status.tokenRedemption)
            revert Unauthorized("Token redemption paused");

        uint256 numTokens = unclaimedTokens[msg.sender];
        unclaimedTokens[msg.sender] = 0;
        spaceCoin.transfer(msg.sender, numTokens);
        emit TokensClaimed();
    }

    // ------ OWNER FUNCTIONS ------
    /// Allows the contract owner to advance the phase of the ICO
    /// @dev uses an enum to track the phases
    function advancePhase(Phases nextPhase) external onlyOwner(owner) {
        if (Phases(uint256(nextPhase) - 1) != phase)
            revert IncorrectNextPhase();
        if (phase == Phases.OPEN)
            revert Unauthorized("Open phase already active");
        phase = Phases(uint256(phase) + 1);
    }

    //ASIDE: there's probably a way to abstract these two functions + toggling the tax into helpers; I just haven't figured out the nuance of how to best deal with the different scopes yet
    /// Allows the owner to pause and resume fundraising at any time
    /// @param newState The boolean value representing the new state. false = off, true = on
    function fundraisingOn(bool newState) external onlyOwner(owner) {
        if (newState == status.fundraising)
            revert Unauthorized("Cannot toggle to same state");
        status.fundraising = newState;
        if (status.fundraising) emit Resuming("Fundraising");
        else emit Pausing("Fundraising");
    }

    /// Allows the owner to pause and resume token redemption at any time
    /// @param newState The boolean value representing the new state. false = off, true = on
    function tokenRedemptionOn(bool newState) external onlyOwner(owner) {
        if (newState == status.tokenRedemption)
            revert Unauthorized("Cannot toggle to same state");
        status.tokenRedemption = newState;
        if (status.tokenRedemption) emit Resuming("Token Redemption");
        else emit Pausing("Token Redemption");
    }

    // ------ PRIVATE FUNCTIONS ------

    /// Sets the individual and total contribution limits for the current phase
    /// @return The individual contribution limit for the current ICO phase
    function getContributionLimit(
        uint256 totalLimit
    ) private view returns (uint256) {
        uint256 totalRemaining = totalLimit - totalContributions;
        uint256 individualRemaining;
        if (phase == Phases.SEED)
            individualRemaining = 1_500 ether - contributors[msg.sender];
        else if (phase == Phases.GENERAL) {
            individualRemaining = 1_000 ether < contributors[msg.sender]
                ? 0
                : 1_000 ether - contributors[msg.sender];
        } else individualRemaining = totalRemaining;
        return
            individualRemaining < totalRemaining
                ? individualRemaining
                : totalRemaining;
    }

    /// Creates the mapping of addresses on the allow list given an array of addresses
    function createAllowListMapping(address[] memory _allowList) private {
        address investor;
        for (uint256 i = 0; i < _allowList.length; i++) {
            investor = _allowList[i];
            allowList[investor] = true;
        }
        allowList[owner] = true;
    }
}

