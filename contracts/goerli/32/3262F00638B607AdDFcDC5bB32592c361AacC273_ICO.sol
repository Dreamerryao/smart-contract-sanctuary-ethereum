/// Types, events, errors and modifiers used by the ICO and SpaceCoin contracts
contract Helpers {
    // ------ Types ------
    /// @notice List of ICO phases
    /// @dev Using an enum makes it easier to advance the phase
    enum Phases {
        SEED,
        GENERAL,
        OPEN
    }

    /// @notice Communicates whether or not fundrasing and/or token redemption are on or off
