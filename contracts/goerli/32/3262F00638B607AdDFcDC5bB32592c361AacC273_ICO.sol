        bool fundraising;
        bool tokenRedemption;
    }

    // ------ EVENTS ------

    /// Event emitted any time a new contribution is made
    /// @param investor Address that made the contribution
    /// @param amount The amount that was contributed
    event ContributionMade(address investor, uint256 amount);

    /// Event emitted once the total contribution limit has been reached for a given ICO phase
    /// Seed: 15000 ETH
    /// General & Open: 30000 ETH
    /// @param phase The current ICO phase
    /// @param limit The limit of the current ICO phase
    event PhaseLimitReached(Phases phase, uint256 limit);

    /// Event emitted when an investor redeems their tokens
    event TokensClaimed();

    /// Event emitted when the owner pauses fundraising, token redemption, or token transfer tax collection
    /// @param action The action that is being paused
    event Pausing(string action);

    /// Event emitted when the owner resumes fundraising, token redemption, or token transfer tax collection
    /// @param action The action that is being resumed
    event Resuming(string action);

    /// Event emitted when the owner withdraws all funds raised to date to the treasury address
    /// @param treasury the address of the treasury
    /// @param withdrawalAmount the amount sent to the treasury
    event WithdrawalMade(address indexed treasury, uint256 withdrawalAmount);

    // ------ CUSTOM ERRORS ------
    /// Error emmited if a user tries to do something that is disallowed
    /// @param message Custom message explaining why the action was unauthorized
    error Unauthorized(string message);

    /// Error emitted if an investor tries to make a contribution that exceeds their current limit
    /// @param limit The maximum amount of ether they are currently allowed to contribute
    /// @param amount The amount they they tried to contribute
    error ContributionLimitExceeded(uint256 limit, uint256 amount);

    /// Error emitted to prevent accidental phase advancement
    error IncorrectNextPhase();

    // ------ Modifiers ------
    modifier onlyOwner(address owner) {
        if (msg.sender != owner) revert Unauthorized("Only Owner");
        _;
    }
}

