pragma solidity ^0.7.0;







/// @author Philippe Dumonet
contract Compounder is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20 public hakToken;
    IBank public bank1;
    IBank public bank2;

    // solhint-disable-next-line no-empty-blocks
    receive() external payable {}

    constructor(IERC20 _hakToken, IBank _bank1, IBank _bank2) Ownable() {
        hakToken = _hakToken;
        bank1 = _bank1;
        bank2 = _bank2;
        hakToken.safeApprove(address(_bank1), type(uint256).max);
        hakToken.safeApprove(address(_bank2), type(uint256).max);
    }

    function doArbTx(address _dest, bytes memory _payload, uint256 _value) external onlyOwner {
        (bool success, ) = _dest.call{ value: _value }(_payload);
        require(success, "Arb tx failed");
    }

    function doFirstDeposit() external onlyOwner {
        _splitDepositAll();
    }


    function doCompound() external onlyOwner {
        _withdrawFromBanks();
        _splitDepositAll();
    }

    function withdrawAll() external onlyOwner {
        _withdrawFromBanks();
        hakToken.safeTransfer(msg.sender, hakToken.balanceOf(address(this)));
    }

    function _withdrawFromBanks() internal {
        bank1.withdraw(address(hakToken), 0);
        bank2.withdraw(address(hakToken), 0);
    }

    function _splitDepositAll() internal {
        uint256 totalBalance = hakToken.balanceOf(address(this));
        uint256 half1 = totalBalance.div(3);
        bank1.deposit(address(hakToken), half1);
        uint256 half2 = totalBalance.sub(half1);
        bank2.deposit(address(hakToken), half2);
    }
}