pragma solidity ^0.8.9;

import "../library/Operation.sol";

interface IORMakerDeposit {
    enum lpState {
        ACTION,
        UPDATE,
        PAUSE,
        STOP,
        USERSTOP
    }
    enum chanllengeState {
        ACTION,
        RESPONSED,
        WITHDRAWED
    }

    event MakerContract(address indexed maker, address indexed mdc);
