            intensity: _intensity,
            progress: _progress
        });
        tokenImage[tokenId] = createArtwork( hue, _progress, _intensity);        
        tokenHueToId[hue] = tokenId;
        _safeMint(msg.sender, 1);
    }

    /**
     * @dev Public method to get token attributes
     */
    function getAttributes(uint256 tokenId) external view returns (Attributes memory){
        return tokenAttributes[tokenId];
    }
    /**
     * @dev Returns the base uri if exists, else the compiled URI
     */
    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {

        // if (!_exists(_tokenId)) revert URIQueryForNonexistentToken();
        return getURI(_tokenId);
    }
    
    /**
     * @dev Returns the image string
     */
    function getImage(uint256 tokenId) public view returns (string memory ){
        return tokenImage[tokenId];
    }
    
    /**
     * @dev Returns the extended string if available
     */
    function getTokenExtended(uint256 tokenId) public view returns (string memory ){
        return tokenIdToExtended[tokenId];
    }
    /**
     * @dev Returns the extended string if available
     */
    function setTokenExtended(uint256 tokenId, string calldata extended ) external  {
        tokenIdToExtended[tokenId] = extended;
    }

    /**
     * @dev Allows owner to update the token attributes
     */
    function setAttributes(uint256 tokenId,  uint256 _duration,  uint256 _progress, uint256 _intensity  ) external {
    
        tokenAttributes[tokenId] = Attributes({
            hue: tokenAttributes[tokenId].hue,
            duration: _duration, 
            intensity: _intensity,
            progress: _progress
        });
    }

    
    /**
     * @dev Compiles the URI string
     */
    function getURI(uint256 tokenId) internal view returns (string memory){
        return string.concat('data:application/json;base64,', 
            Base64.encode(
                abi.encodePacked(
                    '{"description": "Dynamic On-chain NFT", "name": "Origin #', 
                    Strings.toString(tokenId), 
                    '", "attributes": [{"trait_type": "hue", "value": "', 
                            Strings.toString(tokenAttributes[tokenId].hue),
                        '"}],',
                    ' "image": "data:image/svg+xml;base64,', 
                    Base64.encode(
                        bytes(getImage(tokenId))
                    ),
                    '"}'
                )
            )
        );
    }
    /**
     *  Withdraw Funds
     */
    function withdraw(address payable recipientAddress) public onlyOwner {
        recipientAddress.transfer(address(this).balance);
    }
}