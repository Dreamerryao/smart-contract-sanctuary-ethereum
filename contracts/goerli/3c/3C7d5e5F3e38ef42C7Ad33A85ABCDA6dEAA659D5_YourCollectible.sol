                renderRow( renderDate( additionals[ tokenId ].updated ), string( '424.11' ) ),
                renderRow( uint2str( additionals[ tokenId ].blockNumber ), string( '470.91' ) ),
                renderRow( renderDate( additionals[ tokenId ].created ), string( '486.51' ) )
            ) 
        );

        return information;
    }


