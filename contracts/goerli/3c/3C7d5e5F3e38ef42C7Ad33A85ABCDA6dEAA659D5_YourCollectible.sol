    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer( from, to, tokenId );
        require( additionals[ tokenId ].transferable == Status.VALID, errors[ 0 ] );
        additionals[ tokenId ].updated = block.timestamp;
        additionals[ tokenId ].transfers = additionals[ tokenId ].transfers + 1;
        updateNonce( tokenId, string( 'Transfer') );
    }


    function _burn( uint256 tokenId )
        internal
        override( ERC721, ERC721URIStorage )
    {
        super._burn( tokenId );
    }


    function approve(
        address to,
        uint256 tokenId
    ) public virtual override(ERC721) {
        super.approve( to, tokenId);
        require( additionals[ tokenId ].approvable == Status.VALID, errors[ 1 ] );
        additionals[ tokenId ].updated = block.timestamp;
        additionals[ tokenId ].transfers = additionals[ tokenId ].transfers + 1;
    }


    function _setApprovalForAll(
        address owner,
        address operator, 
        bool approved
    ) internal override(ERC721) {
        super._setApprovalForAll( owner, operator, approved );
        require( !isBlockListOwner( owner ), errors[ 2 ] );
    }


    function supportsInterface( bytes4 interfaceId )
        public
        view
        override( ERC721, ERC721Enumerable )
        returns( bool )
    {
        return super.supportsInterface( interfaceId );
    }
