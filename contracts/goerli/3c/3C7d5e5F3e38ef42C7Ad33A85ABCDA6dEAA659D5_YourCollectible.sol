pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "base64-sol/base64.sol";
import "hardhat/console.sol";
import "./DateTime.sol";


contract YourCollectible is
    ERC721,
    ERC721Enumerable,
    ERC721URIStorage,
    ERC721Burnable,
    Ownable
{
    using Counters for Counters.Counter;
    using BokkyPooBahsDateTimeLibrary for uint;

    enum Status { VALID, ON_HOLD, INVALID, OTHER }
    struct Additional { 
        uint256 created;
        uint256 updated;
        uint256 blockNumber;
        uint256 transfers;
        uint256 nonce;
        Status approvable;
        Status transferable;
        Status overall;
    }


    string[] private symbols = [ "&#9989;", "&#128992;", "&#10060;" ];
    string[] private errors = [
        "_____ Your tokenId is not eligible for 'transfer' or 'burn'. _____",
        "_____ Your tokenId is not eligible for 'approve'. _____",
        "_____ Your Address is blocked for 'setApprovalForAll'. _____"
    ];
    mapping( uint256 => Additional ) public additionals;
    mapping (address => bool) public blockListOwners;  
    Counters.Counter private _tokenIdCounter;

    event newState( uint tokenId, uint nonce, string purpose );


    constructor() ERC721( 'Approvable v.10 (Goerli)', 'TRS' ) {}


    function renderDate( uint256 timestamp ) internal view returns( string memory ) {
        ( uint year, uint month, uint day, uint hour, uint minute, uint second ) = BokkyPooBahsDateTimeLibrary
            .timestampToDateTime( timestamp );

        string memory zero = '0';
        string memory mm = uint2str( month );
        string memory dd = uint2str( day );
        string memory hh = uint2str( hour );
        string memory m = uint2str( minute );
        string memory s = uint2str( second );
        string memory ts = string( abi.encodePacked( '(', uint2str( timestamp ), ')' ) );

        if( bytes( mm ).length == 1) { mm = string( abi.encodePacked( zero, mm ) ); }
        if( bytes( dd ).length == 1) { dd = string( abi.encodePacked( zero, dd ) ); }
        if( bytes( hh ).length == 1) { hh = string( abi.encodePacked( zero, hh ) ); }
        if( bytes( m ).length == 1) { m = string( abi.encodePacked( zero, m ) ); }
        if( bytes( s ).length == 1) { s = string( abi.encodePacked( zero, s ) ); }

        string memory str = string( abi.encodePacked( 
            uint2str( year ), '/', mm, '/', dd, ' ', hh, ':', m, ':', s, ' GMT ', ts
        ) );

        return str;
    }


    function _baseURI() internal pure override returns( string memory ) {
        return "data:application/json;base64,";
    }


    function randomStatus( uint256 tokenId ) private view returns( Status ) {
        uint result = tokenId % 2;
        if( result == 0 ) { return Status.VALID; }
        if( result == 1 ) { return Status.INVALID; }

        return Status.OTHER;
    }


    function mintItem( address to, string memory uri ) public returns( uint256 ) {
        uint256 tokenId = _tokenIdCounter.current();
        _safeMint( to, tokenId );
        _setTokenURI( tokenId, uri );

        additionals[ tokenId ].created = block.timestamp;
        additionals[ tokenId ].updated = block.timestamp;
        additionals[ tokenId ].blockNumber = block.number;
        additionals[ tokenId ].transfers = 0;
        additionals[ tokenId ].nonce = 0;

        uint plusOne = tokenId + 1;
        additionals[ tokenId ].approvable = randomStatus( plusOne );
        additionals[ tokenId ].transferable = randomStatus( tokenId );
        setOverall( tokenId );

        _tokenIdCounter.increment();
        return tokenId;
    }


    function updateNonce( uint tokenId, string memory purpose ) internal {
        additionals[ tokenId ].nonce = additionals[ tokenId ].nonce  + 1;
        emit newState( tokenId, additionals[ tokenId ].nonce, purpose);
    }


    function toggleTransferable( uint tokenId ) public {
        require( _msgSender() == ownerOf( tokenId ), 'Function is only available for NFT Holder.' );
        additionals[ tokenId ].updated = block.timestamp;

        if( additionals[ tokenId ].transferable == Status.VALID ) { 
            additionals[ tokenId ].transferable = Status.INVALID; 
        } else { 
            additionals[ tokenId ].transferable = Status.VALID; 
        }

        setOverall( tokenId );
        updateNonce( tokenId, string( 'toggleTransferable' ) );
    }


    function toggleApprovable( uint tokenId ) public {
        require( _msgSender() == ownerOf( tokenId ), 'Function is only available for NFT Holder.' );
        additionals[ tokenId ].updated = block.timestamp;

        if( additionals[ tokenId ].approvable == Status.VALID ) { 
            additionals[ tokenId ].approvable = Status.INVALID; 
        } else { 
            additionals[ tokenId ].approvable = Status.VALID; 
        }

        setOverall( tokenId );
        updateNonce( tokenId, string( 'toggleApprovable' ) );
    }


    function toggleBlockListOwner( address _owner ) public {
        require( _msgSender() == _owner, 'Owner is not msg.sender' );
        if( isBlockListOwner( _owner ) ) {
            delete blockListOwners[ _owner ];
        } else {
            blockListOwners[ _owner ] = true;
        }
    }


    function isBlockListOwner( address _owner ) public view returns( bool ) {
        return blockListOwners[ _owner ];
    }


    function setOverall( uint tokenId ) private {
        bool approvable = ( additionals[ tokenId ].approvable == Status.VALID );
        bool transferable = ( additionals[ tokenId ].transferable == Status.VALID );

        if( approvable && transferable ) {
            additionals[ tokenId ].overall = Status.VALID;
        } else {
            additionals[ tokenId ].overall = Status.INVALID;
        }
    }


    function tokenURI( uint256 tokenId )
        public
        view
        override( ERC721, ERC721URIStorage )
        returns( string memory )
    {
        require( _exists( tokenId ), 'not exist' );

        string memory name = string( 
            abi.encodePacked(
                'Transfer Test #',
                uint2str( tokenId )
            ) 
        );

        string[ 3 ] memory ss = [ ' valid', ' on hold', ' invalid' ];
        string[ 3 ] memory tt = [ 'transferable', 'not transferable', 'not transferable' ];

        string memory description = string( 
            abi.encodePacked(
                'The Certificate #',
                uint2str( tokenId ),
                ' is',
                ss[ uint( additionals[ tokenId ].approvable ) ],
                ' and ',
                tt[ uint( additionals[ tokenId ].transferable ) ],
                '. The owner ',
                Strings.toHexString( uint256( uint160( ownerOf( tokenId ) ) ), 20 ),
                ( additionals[ tokenId ].transfers == 0 ) ? string( ' is also the minter.' ) : string( ' is not the minter.' )
            ) 
        );

        string memory image = Base64.encode( bytes( generateSVGofTokenById( tokenId ) ) );

        return
            string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                name,
                                '", "description":"',
                                description,
                                '", "owner":"',
                                Strings.toHexString( uint256( uint160( ownerOf( tokenId ) ) ), 20 ),
                                '", "image_data": "',
                                'data:image/svg+xml;base64,',
                                image,
                                '"}'
                            )
                            )
                        )
                )
            );
    }

  
    function generateSVGofTokenById( uint256 tokenId ) internal view returns( string memory ) {
    
        string memory svg = string(
            abi.encodePacked( 
                string( 
                   // "<!DOCTYPE html><html><body>"
                    "<svg id=\"Layer_1\" data-name=\"Layer 1\" xmlns=\"http://www.w3.org/2000/svg\" "
                    "xmlns:xlink=\"http://www.w3.org/1999/xlink\" viewBox=\"0 0 512 512\"> "
                ),
                renderTokenById(tokenId),
                "</svg>"
                // </body></html>"
            )
        );

        return svg;
    }


    function returnState( Status status ) internal view returns( string memory ) {
/*
        string[ 3 ] memory a = [ '', '', '' ];

        string memory str = string( 
            abi.encodePacked( 
                symbols[ uint( status ) ],
                a[ uint( status ) ]
            ) 
        );
*/
        return symbols[ uint( status ) ];
    }


    function renderOwner( uint tokenId ) internal view returns( string memory ) {
        string memory emoji = isBlockListOwner( ownerOf( tokenId ) ) ? string( symbols[ uint( 2 ) ]) : symbols[ uint( 0 ) ];

        string memory owner = string( 
            abi.encodePacked(
                Strings.toHexString( uint256( uint160( ownerOf( tokenId ) ) ), 20 ),
                string( ' (' ),
                uint2str( balanceOf( ownerOf( tokenId ) ) ),
                string( ') ' ),
                emoji
            ) 
        );

        return owner;
    }


    function renderOverall( uint tokenId ) internal view returns( string memory ) {
        string memory t = '';
        if( uint( additionals[ tokenId ].overall ) == 0 && !isBlockListOwner( ownerOf( tokenId ) ) ) {
            t = symbols[ uint( 0 ) ];
        } else {
            t = symbols[ uint( 2 ) ];
        }

        string memory overall = string( 
            abi.encodePacked(
                t,
                string( " &#128064;" )
            ) 
        );

        return overall;
    }


    function renderRow( string memory middle, string memory positionY ) internal view returns (string memory ) {
        string memory begin = "<tspan x=\"121.15\" y=\"";
        string memory begin2 = "\">";
        string memory end = "</tspan>";
        string memory result = string( 
            abi.encodePacked( 
                begin, 
                positionY, 
                begin2, 
                middle, 
                end
            )
        );

        return result;
    }


    function renderInformation( uint tokenId ) internal view returns( string memory ) {
        string memory tn = string( 
            abi.encodePacked(
                uint2str( additionals[ tokenId  ].transfers ),
                ' / ',
                uint2str( additionals[ tokenId  ].nonce )
            ) 
        );

        string memory owner = renderOwner( tokenId );

        string memory information = string( 
            abi.encodePacked(
                renderRow( returnState( additionals[ tokenId ].transferable ), string( '314.91' ) ),
