contract TokenReceiver is Ownable {

    // Kai Token Adress: 0xa1d9256D3B12d70CbBB6B8580ad63B8da5507145
    // 合約部署上去，要去token那allowanceIncrease此合約


    IERC20 private _token;
    address _banker;
    mapping(address=>uint256) lastTimeFreeRecive;
    mapping(address=>int256) winOrLoseTable;
    address _contract_address;




    // address owner;


    event DoneReceiveFree(address from);
    event pickResultLog(string text);
    event pickWinnerLog(string text);
    event log(string text);

    /**
     * @dev Constructor sets token that can be received
     */
    
    constructor (IERC20 token) public {
        // _token = address(0xa1d9256D3B12d70CbBB6B8580ad63B8da5507145);
        _token = token;
        // _banker = address(0x956D7672fd666F2F57E3E753dF80FAa03103F175);
        // _banker = msg.sender;
        _banker = address(this);
        // console.log(address(this));
        _contract_address = address(this);
        // _token.approve(address(this),16000000000000000000000);
        // _token.approve(address(this),_token.balanceOf(msg.sender));
        _token.approve(address(this),1600000000000000000000000);

        // token.increaseAllowance(address(this),150000000000000000000);
        // token.increaseAllowance(0x956D7672fd666F2F57E3E753dF80FAa03103F175,150000000000000000000);
        // token.approve(0xa1d9256D3B12d70CbBB6B8580ad63B8da5507145,150000000000000000000);

    }

    function showAddress() view public returns(address,address,address,address,string memory){
        return(_banker,_contract_address,address(_token),msg.sender,"_banker | _contract_address | _token | _my_address");
    }

    /**
     * @dev Do stuff, requires tokens
     */
    function receiveFree() payable external {
        address from = msg.sender;
        // require((block.timestamp-1 hours)>lastTimeFreeRecive[from]);

        // console.log(address(this));

        // _token.transfer(address(this),msg.value );

        // _token.approve(address(0x956D7672fd666F2F57E3E753dF80FAa03103F175), 1000);

        // _token.approve(address(_banker),7*(10**18));
        _token.transferFrom(_banker, from , 7*(10**18));


        lastTimeFreeRecive[from] = block.timestamp;

        emit DoneReceiveFree(from);
    }

    // function increaseAllowanceFromBanker() payable external {
    //     _token.approve(bankerAdress,150000000000000000000);
    // } 

    function setBankerAccount(address bankerAdress) onlyOwner public {
        _banker = bankerAdress;
        _token.approve(bankerAdress,150000000000000000000);
    }

    function getAllMoneyBackFromBanker() onlyOwner public {
        _token.approve(address(this),_token.balanceOf(_banker));
        _token.transferFrom(_banker, msg.sender , _token.balanceOf(_banker));
    }

    function showBalance() public view returns(uint256) {
        return _token.balanceOf(msg.sender);
    }

    function showBankBalance() public view returns(uint256) {
        return _token.balanceOf(_banker);
    }

    function showAllowance() public view returns(uint256) {
        return _token.allowance(_banker, msg.sender);
    }

    function random() public view returns (uint256) {
        // sha3 and now have been deprecated
        return uint(keccak256(abi.encodePacked(block.timestamp, msg.sender)));
        // convert hash to integer
        // players is an array of entrants
    }

    function pickResult() public view returns(uint256){
        uint256 index = random()%2;
        return index;
    }

    function gamerApprove() external{
        _token.approve(_contract_address,_token.balanceOf(msg.sender));
        _token.approve(_banker,_token.balanceOf(msg.sender));
    }

    function bankerApprove() onlyOwner external returns (bool){
        // _token.approve(_contract_address,_token.balanceOf(_banker));
        _token.approve(msg.sender,16000000000000000000000);
        return true;
    }

    function pickWinner(uint256 playerPick,uint256 playerAmount) payable external{
        uint256 result = pickResult();
        emit pickResultLog(Strings.toString(result));
        // _token.approve(address(msg.sender),playerAmount*(10**18));
        if(result==playerPick){
            _token.transferFrom(_banker, msg.sender , playerAmount*(10**18));
            winOrLoseTable[msg.sender]+=int(playerAmount);
            // return "win";
            emit pickWinnerLog("win");

        }else{

            _token.transferFrom(msg.sender, _banker , playerAmount*(10**18));
            winOrLoseTable[msg.sender]-=int(playerAmount);
            // return "lose";
            emit pickWinnerLog("lose");

        }
    }

    function showProfit() public view returns(int256){
        return winOrLoseTable[msg.sender];
    }
}

