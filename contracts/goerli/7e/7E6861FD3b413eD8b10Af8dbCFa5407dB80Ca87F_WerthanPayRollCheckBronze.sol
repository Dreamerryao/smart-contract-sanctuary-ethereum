        hoc = IHOC(_hoc);
        mintSplitter = _mintSplitter;
    }

    function setHODL(address _hodl) external onlyOwner {
        hodl = IHODL(_hodl);
    }

    function setHOC(address _hoc) external onlyOwner {
        hoc = IHOC(_hoc);
    }

    function setMintSplitter(address _mintSplitter) external onlyOwner {
        mintSplitter = _mintSplitter;
    }

    function setMaxTotalSupply(uint256 supply) 
        external 
        onlyOwner
    {
        MAX_TOTAL_SUPPLY = supply;
    }

    function _baseURI() 
        internal 
        view
        override 
        returns (string memory) 
    {
        return baseURI;
    }

    function _baseURIRevealed() 
        internal 
        view
        returns (string memory) 
    {
        return baseURIRevealed;
    }

