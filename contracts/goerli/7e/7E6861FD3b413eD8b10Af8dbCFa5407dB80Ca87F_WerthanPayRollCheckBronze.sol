    mapping (uint256 => uint256) public tokenNumbers;
    // token ID of already minted with token number and level: token number => token ID
    mapping (uint256 => uint256) public isMintedTokenNumber;
    mapping (uint256 => bool) public revealed;

    // Prices
    uint256 public tokenPrice = 100 * 10**18;           // 100 HODL
    uint256 public tokenPriceLocked = 50 * 10**18;      // 50 HODL

