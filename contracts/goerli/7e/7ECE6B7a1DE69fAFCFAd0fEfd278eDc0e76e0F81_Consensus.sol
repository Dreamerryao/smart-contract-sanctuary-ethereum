// SPDX-License-Identifier: MIT
pragma solidity >=0.8.0 <0.9.0;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "./INfor.sol";
import "./IConsensus.sol";
import "hardhat/console.sol";

contract Consensus is IConsensus, ERC1155Holder, Ownable {
    struct EventData {
        address user;
        uint256 tokenId;
        bytes32 dataHash;
        // string dataId;
        bytes32 stepId;
        MeasurementAction actionType;
    }

    struct StepDefinition {
        uint256[] tokenIds;
        bytes32[] stepIdsDependencies;
        bytes32[] stepIdsReprovalDependencies;
        bytes32 processId;
    }

    bytes32 public orgHash;

    INfor public nforAddr;

    // processId -> stepIds[]
    mapping(bytes32 => bytes32[]) private _consensusFlow;

    // stepId -> StepDefinition
    mapping(bytes32 => StepDefinition) private _stepDefinition;

    // measurementId -> stepId -> eventDataId
    mapping(bytes32 => mapping(bytes32 => bytes32[])) private _measurements;

    // eventDataId -> measurementData
    mapping(bytes32 => EventData) private _eventData;

    // functionName -> policyName
    mapping(string => string) public functionPolicy;

    modifier authenticate(string memory _policyName) {
        require(
            nforAddr.authenticate(orgHash, _policyName, _msgSender()) == true,
            "NOT_CONSENSUS_ADMIN"
        );
        _;
    }

    modifier authenticateH3avenAdmin() {
        require(
            nforAddr.authenticate(nforAddr.adminOrg(), "h3avenManagement", _msgSender()) == true,
            "NOT_H3AVEN_ADMIN"
        );
        _;
    }

    modifier isTokenIdInStep(bytes32 stepId, uint256 tokenId) {
        require(isTokenIdRegisteredInStep(stepId, tokenId) == true, "TOKEN_ID_NOT_IN_STEP");
        require(hasOrgRole(tokenId), "USER_DONT_HAVE_ORG_ROLE");
        _;
    }

    modifier dependenciesMet(bytes32 measurementId, bytes32 stepId) {
        for (uint256 i = 0; i < _stepDefinition[stepId].stepIdsDependencies.length; i++) {
            bool __isStepAgreedUpon = isStepAgreedUpon(
                measurementId,
                _stepDefinition[stepId].stepIdsDependencies[i]
            );
            require(__isStepAgreedUpon, "STEP_DEPENDENCIES_UNDER_APPROVAL");

            delete __isStepAgreedUpon;
        }
        _;
    }

    modifier dependenciesReproved(bytes32 measurementId, bytes32 stepId) {
        for (uint256 i = 0; i < _stepDefinition[stepId].stepIdsReprovalDependencies.length; i++) {
            bool _isStepReproved = isStepReproved(
                measurementId,
                _stepDefinition[stepId].stepIdsReprovalDependencies[i]
            );
            require(_isStepReproved, "STEP_DEPENDENCIES_NOT_REPROVED");

            delete _isStepReproved;
        }
        _;
    }

    modifier stepIdNotTaken(bytes32 processId, bytes32 stepId) {
        require(_stepDefinition[stepId].processId != processId, "STEP_NAME_ALREADY_TAKEN");
        _;
    }

    modifier stepExists(bytes32 stepId) {
        require(_stepDefinition[stepId].processId != bytes32(0), "STEP_DOESNT_EXIST");
        _;
    }

    constructor(INfor _nforAddr, bytes32 _orgHash) {
        nforAddr = _nforAddr;
        orgHash = _orgHash;
    }

    function setBasePolicies() public authenticate("toManage") {
        setFunctionPolicy("createStep", "toManage");
        setFunctionPolicy("updateStepIdsDependencies", "toManage");
        setFunctionPolicy("updateStepIdsReprovalDependencies", "toManage");
        setFunctionPolicy("setStepTokenIds", "toManage");
        setFunctionPolicy("addTokenIdToStep", "toManage");
        setFunctionPolicy("addTokenIdsToStep", "toManage");
        setFunctionPolicy("removeTokenIdFromStep", "toManage");
        emit NewFunctionPolicy("setNforAddress", "h3avenManagement");
        emit NewFunctionPolicy("setFunctionPolicy", "toPolicy");
    }

    function setFunctionPolicy(
        string memory functionName,
        string memory _policy
    ) public authenticate("toPolicy") {
        require(bytes(_policy).length > 0, "Consensus: Empty Policy");

        functionPolicy[functionName] = _policy;

        emit NewFunctionPolicy(functionName, _policy);
    }

    function setNforAddress(INfor _nforAddr) public authenticateH3avenAdmin {
        nforAddr = _nforAddr;
    }

    function consensusFlow(bytes32 processId) public view returns (bytes32[] memory stepIds) {
        return _consensusFlow[processId];
    }

    function isStepAgreedUpon(bytes32 measurementId, bytes32 stepId) public view returns (bool) {
        uint256 total = 0;

        for (uint256 i = 0; i < _measurements[measurementId][stepId].length; i++) {
            // se tiver o approval
            if (
                _eventData[_measurements[measurementId][stepId][i]].actionType ==
                MeasurementAction.WRITE_DATA ||
                _eventData[_measurements[measurementId][stepId][i]].actionType ==
                MeasurementAction.APPROVAL
            ) {
                total += 1;
            }
            if (
                _eventData[_measurements[measurementId][stepId][i]].actionType ==
                MeasurementAction.REJECTION
            ) {
                total = 0;
            }
        }

        if (total == _stepDefinition[stepId].tokenIds.length) {
            return true;
        }

        delete total;

        return false;
    }

    function isStepReproved(bytes32 measurementId, bytes32 stepId) public view returns (bool) {
        for (uint256 i = 0; i < _measurements[measurementId][stepId].length; i++) {
            if (
                _eventData[_measurements[measurementId][stepId][i]].actionType ==
                MeasurementAction.REJECTION
            ) {
                return true;
            }
        }

        return false;
    }

    function stepDefinition(
        bytes32 stepId
    )
        public
        view
        returns (
            uint256[] memory tokenIds,
            bytes32[] memory stepIdsDependencies,
            bytes32[] memory stepIdsReprovalDependencies
        )
    {
        return (
            _stepDefinition[stepId].tokenIds,
            _stepDefinition[stepId].stepIdsDependencies,
            _stepDefinition[stepId].stepIdsReprovalDependencies
        );
    }

    function hasOrgRole(uint256 tokenId) public view returns (bool) {
        return IERC1155(address(nforAddr)).balanceOf(_msgSender(), tokenId) > 0;
    }

    function _stepDependenciesOnSameProcessId(
        bytes32 processId,
        bytes32[] memory stepDependencies
    ) internal view returns (bool) {
        for (uint256 i = 0; i < stepDependencies.length; i++) {
            if (_stepDefinition[stepDependencies[i]].processId != processId) {
                return false;
            }
        }
        return true;
    }

    function canRegisterAction(
        bytes32 measurementId,
        bytes32 stepId,
        MeasurementAction actionType,
        bytes32 dataHash
    ) internal view returns (bool) {
        uint32 hashAppearance = 0;
        bytes32 lastHash;

        for (uint256 i = 0; i < _measurements[measurementId][stepId].length; i++) {
            if (
                _eventData[_measurements[measurementId][stepId][i]].user == _msgSender() &&
                _eventData[_measurements[measurementId][stepId][i]].actionType == actionType &&
                _eventData[_measurements[measurementId][stepId][i]].dataHash == dataHash
            ) {
                revert("ALREADY_MEASURED_IN_STEP");
            }

            if (
                actionType != MeasurementAction.WRITE_DATA &&
                _eventData[_measurements[measurementId][stepId][i]].dataHash != dataHash
            ) {
                hashAppearance += 1;
            }

            if (
                _eventData[_measurements[measurementId][stepId][i]].actionType ==
                MeasurementAction.WRITE_DATA
            ) {
                lastHash = _eventData[_measurements[measurementId][stepId][i]].dataHash;
            }
        }

        if (
            actionType != MeasurementAction.WRITE_DATA &&
            (hashAppearance == _measurements[measurementId][stepId].length || dataHash != lastHash)
        ) {
            revert("INVALID_HASH");
        }

        return true;

        // _;
    }

    function isTokenIdRegisteredInStep(bytes32 stepId, uint256 tokenId) public view returns (bool) {
        for (uint256 i = 0; i < _stepDefinition[stepId].tokenIds.length; i++) {
            if (_stepDefinition[stepId].tokenIds[i] == tokenId) {
                return true;
            }
        }
        return false;
    }

    function isMeasurementFullyConsensed(
        bytes32 processId,
        bytes32 measurementId
    ) public view returns (bool) {
        for (uint256 i = 0; i < _consensusFlow[processId].length; i++) {
            if (isStepAgreedUpon(measurementId, _consensusFlow[processId][i]) == false) {
                return false;
            }
        }

        return true;
    }

    function eventData(
        bytes32 eventDataId
    )
        public
        view
        returns (
            address user,
            MeasurementAction actionType,
            bytes32 dataHash,
            // string memory dataId,
            bytes32 stepId
        )
    {
        return (
            _eventData[eventDataId].user,
            _eventData[eventDataId].actionType,
            _eventData[eventDataId].dataHash,
            // _eventData[eventDataId].dataId,
            _eventData[eventDataId].stepId
        );
    }

    function areDependenciesMet(bytes32 measurementId, bytes32 stepId) public view returns (bool) {
        for (uint256 i = 0; i < _stepDefinition[stepId].stepIdsDependencies.length; i++) {
            if (!isStepAgreedUpon(measurementId, _stepDefinition[stepId].stepIdsDependencies[i])) {
                return false;
            }
        }
        return true;
    }

    function createStep(
        bytes32 processId,
        bytes32 stepId,
        uint256[] memory tokenIds,
        bytes32[] memory stepIdsDependencies,
        bytes32[] memory stepIdsReprovalDependencies
    ) public stepIdNotTaken(processId, stepId) authenticate(functionPolicy["createStep"]) {
        require(
            _stepDependenciesOnSameProcessId(processId, stepIdsDependencies) == true,
            "INVALID_DEPENDENCY"
        );

        _stepDefinition[stepId] = StepDefinition(
            tokenIds,
            stepIdsDependencies,
            stepIdsReprovalDependencies,
            processId
        );

        _consensusFlow[processId].push(stepId);

        emit StepCreated(
            processId,
            stepId,
            tokenIds,
            stepIdsDependencies,
            stepIdsReprovalDependencies
        );
    }

    function updateStepIdsDependencies(
        bytes32 stepId,
        bytes32[] memory stepIdsDependencies
    ) public stepExists(stepId) authenticate(functionPolicy["updateStepIdsDependencies"]) {
        require(
            _stepDependenciesOnSameProcessId(
                _stepDefinition[stepId].processId,
                stepIdsDependencies
            ) == true,
            "INVALID_DEPENDENCY"
        );

        _stepDefinition[stepId].stepIdsDependencies = stepIdsDependencies;

        emit StepDependenciesUpdated(stepId, stepIdsDependencies);
    }

    function updateStepIdsReprovalDependencies(
        bytes32 stepId,
        bytes32[] memory stepIdsReprovalDependencies
    ) public stepExists(stepId) authenticate(functionPolicy["updateStepIdsReprovalDependencies"]) {
        require(
            _stepDependenciesOnSameProcessId(
                _stepDefinition[stepId].processId,
                stepIdsReprovalDependencies
            ) == true,
            "INVALID_DEPENDENCY"
        );

        _stepDefinition[stepId].stepIdsReprovalDependencies = stepIdsReprovalDependencies;

        emit StepReprovalDependenciesUpdated(stepId, stepIdsReprovalDependencies);
    }

    function setStepTokenIds(
        bytes32 stepId,
        uint256[] memory tokenIds
    ) public stepExists(stepId) authenticate(functionPolicy["setStepTokenIds"]) {
        _stepDefinition[stepId].tokenIds = tokenIds;

        emit StepTokenIdsUpdated(stepId, _stepDefinition[stepId].tokenIds);
    }

    function addTokenIdToStep(
        bytes32 stepId,
        uint256 tokenId
    ) public stepExists(stepId) authenticate(functionPolicy["addTokenIdToStep"]) {
        require(isTokenIdRegisteredInStep(stepId, tokenId) == false, "NFOR_ID_ALREADY_REGISTERED");

        _stepDefinition[stepId].tokenIds.push(tokenId);

        emit StepTokenIdsUpdated(stepId, _stepDefinition[stepId].tokenIds);
    }

    function addTokenIdsToStep(
        bytes32 stepId,
        uint256[] memory tokenIds
    ) public stepExists(stepId) authenticate(functionPolicy["addTokenIdsToStep"]) {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                isTokenIdRegisteredInStep(stepId, tokenIds[i]) == false,
                "NFOR_ID_ALREADY_REGISTERED"
            );

            _stepDefinition[stepId].tokenIds.push(tokenIds[i]);
        }
        emit StepTokenIdsUpdated(stepId, _stepDefinition[stepId].tokenIds);
    }

    function removeTokenIdFromStep(
        bytes32 stepId,
        uint256 tokenId
    ) public stepExists(stepId) authenticate(functionPolicy["removeTokenIdFromStep"]) {
        require(isTokenIdRegisteredInStep(stepId, tokenId) == true, "TOKEN_ID_NOT_IN_STEP");

        uint256 length = _stepDefinition[stepId].tokenIds.length;

        for (uint256 i = 0; i < length; i++) {
            if (_stepDefinition[stepId].tokenIds[i] == tokenId) {
                // org role que deve ser removido recebe os valores da última definição de org role
                _stepDefinition[stepId].tokenIds[i] = _stepDefinition[stepId].tokenIds[length - 1];
                // última definição de org role é removida do array
                _stepDefinition[stepId].tokenIds.pop();
                emit StepTokenIdsUpdated(stepId, _stepDefinition[stepId].tokenIds);
                return;
            }
        }
    }

    function finishConsensusAndMintOrgRole(
        bytes32 measurementId, // hash("documentacao")
        bytes32 stepId, // hash("documentacao")
        uint256 tokenId, // token id do Nfor que o usuário vai usar hash("org_admin")
        bytes32 dataHash, // hash dos dados| TODO: migrar pra merkle tree // string memory dataId, // ID do banco. validar tamanho máximo desse dataId.,
        bytes calldata data
    ) public {
        // msg.sender validations on registerAction
        registerAction(measurementId, stepId, tokenId, dataHash, MeasurementAction.APPROVAL);

        require(isStepAgreedUpon(measurementId, stepId), "NOT_FINAL_ACTION_ON_STEP");

        nforAddr.grantOrgRole(orgHash, "role", msg.sender);

        // (bool success, ) = address(nforAddr).call(data);
        // require(success, "call failed");
    }

    // reprovei etapa 1, tenho que ir pra 2

    function handleReproval(
        bytes32 measurementId, // hash("medicao de outubro")
        bytes32 stepId, // hash("etapa que vai pra quando etapa anterior é reprovada") // etapa 2
        uint256 tokenId, // token id do Nfor que o usuário vai usar
        bytes32 dataHash, // hash dos dados| TODO: migrar pra merkle tree
        // string memory dataId, // ID do banco. validar tamanho máximo desse dataId.
        MeasurementAction actionType // tipo de ação
    ) public dependenciesReproved(measurementId, stepId) isTokenIdInStep(stepId, tokenId) {
        require(canRegisterAction(measurementId, stepId, actionType, dataHash) == true, "REVERT");

        bytes32 eventId = keccak256(
            abi.encodePacked(
                measurementId,
                stepId,
                dataHash,
                /* dataId, */
                uint8(actionType)
            )
        );

        _measurements[measurementId][stepId].push(eventId);

        _eventData[eventId] = EventData(
            _msgSender(),
            tokenId,
            dataHash,
            // dataId,
            stepId,
            actionType
        );

        emit NewEvent(
            _msgSender(),
            measurementId,
            stepId,
            eventId,
            tokenId,
            dataHash,
            // dataId,
            actionType
        );
    }

    function registerAction(
        bytes32 measurementId, // hash("medicao de outubro")
        bytes32 stepId, // hash("Etapa 1")
        uint256 tokenId, // token id do Nfor que o usuário vai usar
        bytes32 dataHash, // hash dos dados| TODO: migrar pra merkle tree
        // string memory dataId, // ID do banco. validar tamanho máximo desse dataId.
        MeasurementAction actionType // tipo de ação
    ) public isTokenIdInStep(stepId, tokenId) dependenciesMet(measurementId, stepId) {
        require(canRegisterAction(measurementId, stepId, actionType, dataHash) == true, "REVERT");
        require(isStepAgreedUpon(measurementId, stepId) == false, "STEP_ALREADY_CONSENSED");

        bytes32 eventId = keccak256(
            abi.encodePacked(
                measurementId,
                stepId,
                dataHash,
                /* dataId, */
                uint8(actionType)
            )
        );

        _measurements[measurementId][stepId].push(eventId);

        _eventData[eventId] = EventData(
            _msgSender(),
            tokenId,
            dataHash,
            // dataId,
            stepId,
            actionType
        );

        emit NewEvent(
            _msgSender(),
            measurementId,
            stepId,
            eventId,
            tokenId,
            dataHash,
            // dataId,
            actionType
        );
    }
}

