pragma solidity 0.6.12;








contract BotoAaveRepayV2 is Ownable, AccessControl {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    event LogDepositReceived(address from, uint256 amount);
    event LoanRepayed(address user, uint256 amountRepayed);
    event Error(string);

    bytes32 public constant USER_ROLE = keccak256("USER");

    /// @dev Restricted to members of the admin role.
    modifier onlyAdmin() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, msg.sender),
            "Restricted to admins."
        );
        _;
    }
    modifier stopInEmergency() {
        require(!stopped, "Contract has ben stopped, please contact support.");
        _;
        // if (!stopped) _;
    }
    modifier onlyInEmergency() {
        if (stopped) _;
    }

    mapping(address => uint256) private _balances;
    uint256 public totalSupply;
    address public lendingPoolAddress;
    address public dataProviderAddress;
    address public botoWalletAddress;

    bool private stopped = false;
    uint256 private stableDebt;
    uint256 private variableDebt;

    constructor(address pool, address dataProvider, address botoWallet) public {
        // _setRoleAdmin(USER_ROLE, DEFAULT_ADMIN_ROLE);
        require(pool != address(0), "pool address cannot be 0");
        require(dataProvider != address(0), "dataProvider address cannot be 0");
        require(botoWallet != address(0), "botoWallet address cannot be 0");
        lendingPoolAddress = pool;
        dataProviderAddress = dataProvider;
        botoWalletAddress = botoWallet;
        // initial whitelist when deployed
        // whitelist can stopContract functionality.
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        // whitelist onlyOwner can add (?)
    }

    // consider permanent inactivation
    // multi sig for owner account, 2/3 signatures needed to transact, hacking would require 2 accounts to be able to use our smart contract
    // gnosis safe, research
    function makeContractInactive() public onlyAdmin {
        // You can add an additional modifier that restricts stopping a contract to be based on another action, such as a vote of users
        stopped = true;
    }

    function depositFunds() public payable {
        _balances[msg.sender] += msg.value;
        totalSupply += msg.value;
        assert(address(this).balance >= totalSupply);
        emit LogDepositReceived(msg.sender, msg.value);
    }

    function checkBalance() public view returns (uint256) {
        return _balances[msg.sender];
    }

    // in case of emergency we want user to be able to withdraw funds
    function withdrawFunds(uint256 _amount) public {
        require(_balances[msg.sender] >= _amount);
        _balances[msg.sender] -= _amount; // Reverts if transfer fails
        // This is what we want separated from other app logic
        msg.sender.transfer(_amount);
    }

    // in case of emergency we don't want users to be able to send funds to other addresses
    function sendFunds(
        address _receiver,
        uint256 _amount
    ) public stopInEmergency {
        /* Check if sender has balance and for overflows */
        require(
            _balances[msg.sender] >= _amount &&
                _balances[_receiver] + _amount >= _balances[_receiver]
        );
        // ensure msg.sender can only modify their own balance
        _balances[msg.sender] -= _amount;
        _balances[_receiver] += _amount;
    }

    receive() external payable {
        _balances[msg.sender] += msg.value;
        totalSupply += msg.value;
        assert(address(this).balance >= totalSupply);
        emit LogDepositReceived(msg.sender, msg.value);
    }

    function transfer(
        address tokenAddress,
        address destination,
        uint256 amountToTransfer
    ) public onlyOwner stopInEmergency {
        IERC20 token = IERC20(tokenAddress);
        token.transfer(destination, amountToTransfer);
    }

    // who calls this function? If its public anybody could call it... only the contract owner should be able to call.
    function repay(
        address userAddress,
        address tokenAddress,
        uint256 debtType,
        uint256 repayAmount,
        uint256 gasFee,
        uint256 botoFee
    ) external onlyOwner stopInEmergency {
        ILendingPool lendingPool = ILendingPool(lendingPoolAddress);
        IProtocolDataProvider dataProvider = IProtocolDataProvider(
            dataProviderAddress
        );

        IERC20 token = IERC20(tokenAddress);
        uint256 debtPlusALittleExtra = repayAmount;
        // only pay total if no amount was specified by user
        if (repayAmount == 0) {
            (, stableDebt, variableDebt, , , , , , ) = dataProvider
                .getUserReserveData(tokenAddress, userAddress);
            /**
             * Aave recommends when paying the total of the loan on behalf of somebody else
             * to add a little extra:
             * "In case of repayments on behalf of another user, it's recommended to send an
             * _amount slightly higher than the current borrowed amount."
             *  */
            if (debtType == 2) {
                // multiply by 1.01
                debtPlusALittleExtra = variableDebt.div(100).mul(101);
            } else {
                // multiply by 1.01
                debtPlusALittleExtra = stableDebt.div(100).mul(101);
            }
        }

        // allow lendingPool to transfer from SC balance.
        token.approve(
            lendingPoolAddress,
            debtPlusALittleExtra.add(gasFee).add(botoFee)
        );
        // we should also transfer the gas fee
        token.transferFrom(
            userAddress,
            address(this),
            debtPlusALittleExtra.add(gasFee).add(botoFee)
        ); // +converted gasfee +botofee
        uint256 amountRepaid = lendingPool.repay(
            tokenAddress,
            debtPlusALittleExtra,
            debtType,
            userAddress
        );
        // transfer leftover balance from loan repayment back to user
        // since values are calculated and used in this one function, I don't believe it's
        // necessary to keep a separate balance.

        token.transfer(userAddress, debtPlusALittleExtra.sub(amountRepaid));

        // token transfer gasfee and botofee to our wallet address
        // fees are already provided in the specific token to repay with
        token.transfer(botoWalletAddress, gasFee.add(botoFee));
        // TODO: swap and then transfer eth directly
        // TODO: look into corporate solution for private key storage.


        assert(amountRepaid!=0);
        emit LoanRepayed(userAddress, amountRepaid);
    }
}

