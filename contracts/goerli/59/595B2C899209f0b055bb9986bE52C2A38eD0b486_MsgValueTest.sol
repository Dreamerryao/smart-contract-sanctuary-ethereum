pragma solidity ^0.8.9;

import "@opengsn/contracts/src/ERC2771Recipient.sol";
//import "@openzeppelin/contracts/GSN/GSNRecipient.sol"; might need this
// Uncomment this line to use console.log
import "hardhat/console.sol";

contract MsgValueTest is ERC2771Recipient{

    mapping(address => uint256) public stakeAmount;
    mapping(address => bool) public staked;
    uint256 public counter=0;
    address private _trustedForwarder;

    event WithdrawAll(address who, uint256 amount);
    event Withdraw(address who, uint256 amount);
    event Stake(address who, uint256 amount);
    event Donate(address who, uint256 amount);

     constructor(address forwarder) // Initialize trusted forwarder
     {
        _setTrustedForwarder(forwarder);
     }


    function setTrustedForwarder(address forwarder) public {
        _setTrustedForwarder(forwarder);
    }

    function stake() external payable {
        stakeAmount[_msgSender()] = msg.value;
        staked[_msgSender()] = true;

        emit Stake(_msgSender(), msg.value);
    }

    function withdraw(uint256 amount) public {
        require(stakeAmount[_msgSender()] > amount, "Balance is 0");
        payable(_msgSender()).transfer(amount);
        stakeAmount[_msgSender()] -= amount;
        if(stakeAmount[_msgSender()]==0)
            staked[_msgSender()] = false;

        emit Withdraw(_msgSender(), amount);
    }

    function withdrawAll() public {
        require(stakeAmount[_msgSender()] > 0, "Balance is 0");
        payable(_msgSender()).transfer(stakeAmount[_msgSender()]);
        stakeAmount[_msgSender()] = 0;
        staked[_msgSender()] = false;

        emit WithdrawAll(_msgSender(), stakeAmount[_msgSender()]);
    }

    function donateWithoutEvent() external payable {

    }

    function donateWithEvent() external payable {
        emit Donate(_msgSender(), msg.value);
    }

    function addCounter() external {
        counter++;
    }

    function getStakeAmount(address who) view external returns(uint256) {
        return stakeAmount[who];
    }

    function getStakeStatus(address who) view external returns(bool) {
        return staked[who];
    }

    // function _msgSender() internal view override(ERC2771Recipient)
    //   returns (address sender) {
    //   sender = ERC2771Recipient._msgSender();
    // }

    // function _msgData() internal view override(Context, ERC2771Recipient)
    //   returns (bytes calldata) {
    //   return ERC2771Recipient._msgData();
    // }

    // function isTrustedForwarder(address _who) public view override( ERC2771Recipient)
    // returns (bool) {
    //     return _trustedForwarder == _who;
    // }
}

