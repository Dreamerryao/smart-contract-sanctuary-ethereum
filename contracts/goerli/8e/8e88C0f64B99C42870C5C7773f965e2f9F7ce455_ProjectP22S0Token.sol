    function checkGoldenIndexIsAvailbale(uint256 randomNumber)
        internal
        returns (uint256)
    {
        uint256 j = 0;
        for (
            j = randomNumber;
            j <= goldenTicketTokenMaxAvailableIndex;
            j = j + 1
        ) {
            if (isGoldenTicketTokenIndexPicked[uint256(j)] != true) {
                return j;
            }
        }
        if (randomNumber == goldenTicketTokenMinAvailableIndex) {
            isAllGoldenTicketRedeem = true;
            return 0;
        }

        if (randomNumber > 0)
            goldenTicketTokenMaxAvailableIndex = uint256(randomNumber - 1);
        return
            checkGoldenIndexIsAvailbale(
                randomNumber != 1 ? (randomNumber / 2) : 0
            );
    }

