        external
        onlyOwner
    {
        VestedUtilsAddress = _VestedUtilsAddress;
    }

    // ***************************** onlyOwner : End *****************************

    // ***************************** public view : Start *************************

    function Mint(
        uint256 numberOfTokens,
        address toAddress,
        uint256 tokenStartIndex
    ) external {
        require(PackContractAddress == msg.sender, "Invalid pack address");
        require(numberOfTokens < 51, "Can only mint 20 cards at a time");
        _safeMint(toAddress, numberOfTokens, tokenStartIndex);
        // Don’t forget what happened to the man who suddenly got everything he wanted... He lived happily ever after.
    }

    function numberMinted(address owner) external view returns (uint256) {
        return _numberMinted(owner);
    }

    function getOwnershipData(uint256 cardId)
        external
        view
        returns (TokenOwnership memory)
    {
        return _ownershipOf(cardId);
    }

    function GetProvenance(string memory sheet)
        external
        view
        returns (string memory)
    {
        string memory data = "";
        for (uint256 i = 0; i < provenanceRecord[sheet].length; i++) {
            data = string(
                abi.encodePacked(
                    data,
                    provenanceRecord[sheet][i].cardRang,
                    ": ",
                    provenanceRecord[sheet][i].Provenance,
                    "  ||  "
                )
            );
        }
        return data;
    }

    function GetTokenImageMapping(uint256 cardId)
        public
        view
        returns (uint256)
    {
        uint256 curr = cardId;
        unchecked {
            if (0 <= curr && curr < _nextTokenId()) {
                uint256 tokenImageIndex = tokenImageMapping[curr];
                if (tokenImageIndex > 0) {
                    return tokenImageIndex - 1;
                }
                while (true) {
                    curr--;
                    tokenImageIndex = tokenImageMapping[curr];
                    if (tokenImageIndex > 0) {
                        return tokenImageIndex + (cardId - curr) - 1;
                    }
                }
            }
        }
        revert QueryForNonexistentToken();
    }

    // Little surprises around every corner, but nothing dangerous.
    function GetSheet(uint256 cardId) external view returns (string memory) {
        uint256 imageMapping = GetTokenImageMapping(cardId);
        imageMapping = imageMapping * 15485863123;
        imageMapping =
            imageMapping *
            imageMapping *
            imageMapping *
            2038074743123;
        uint256 sheetIndex = (cardId + imageMapping) % 9;

        // She Was A Bad Egg.
        if (sheetIndex > 5) {
            return "C";
        } else if (sheetIndex > 2) {
            return "B";
        }
        return "A";
    }

    function RedeemGoldenTicket(uint256 cardId) external nonReentrant {
        // Stop. Don’t. Come back.
        require(
            GoldenTicketRedeemActive,
            "Golden Ticket redemption is not active"
        );
        GoldenTicketRedeemRequest
            memory goldenTicketRedeemRequestInstance = goldenTicketRedeemRequestMapping[
                cardId
            ];
        if (goldenTicketRedeemRequestInstance.isBurned == false) {
            address owner = ERC721AUpgradeable.ownerOf(cardId);
            if (msg.sender != owner) {
                // You Lose. Good Day, Sir.
                revert("You are not the owner of the entered card");
            }
        } else if (
            goldenTicketRedeemRequestInstance.userAddress != msg.sender
        ) {
            revert("You are not the owner of the entered card");
        } else if (goldenTicketRedeemRequestInstance.isNftTransferred == true) {
            revert("You are not the owner of the entered card");
        } else if (
            goldenTicketRedeemRequestInstance.lastTriedBlockNumber >
            block.number - 20
        ) {
            revert("You are not the owner of the entered card");
        }

        require(
            isAllGoldenTicketRedeem == false,
            "All Golden ticket tokens already claimed"
        );
        require(
            IsGoldenTicket[cardId] == true,
            "Selected card is not a Golden Ticket"
        );
        if (goldenTicketRedeemRequestInstance.isBurned == false) {
            // I Think That Furnace Is Only Lit Every Other Day, So They Have A Good Sporting Chance, Haven't They?
            _burn(cardId);
            goldenTicketRedeemRequestMapping[
                cardId
            ] = GoldenTicketRedeemRequest(
                msg.sender,
                true,
                false,
                block.number,
                0
            );
        }
        uint256 requestId = COORDINATOR.requestRandomWords(
            chainLinkConfig.chainLinkKeyHash,
            chainLinkConfig.chainLinkSubscriptionId,
            chainLinkConfig.chainLinkRequestConfirmations,
            chainLinkConfig.chainLinkCallbackGasLimit,
            1
        );
        linkRequestMapping[requestId] = LinkRequest(cardId, msg.sender);
        goldenTicketRedeemRequestMapping[cardId].lastTriedBlockNumber = block
            .number;
        goldenTicketRedeemRequestMapping[cardId].linkRequestId = requestId;
    }

    function totalMinted() external view returns (uint256) {
        return _totalMinted();
    }

    function burn(uint256 cardId) external {
        address owner = ERC721AUpgradeable.ownerOf(cardId);
        if (owner == msg.sender || cardBurnerAddress == msg.sender) {
            _burn(cardId);
        }
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        payable
        override(ERC721AUpgradeable, IERC721AUpgradeable)
        onlyAllowedOperator(from)
    {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        payable
        override(ERC721AUpgradeable, IERC721AUpgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    )
        public
        payable
        override(ERC721AUpgradeable, IERC721AUpgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, data);
    }
}

