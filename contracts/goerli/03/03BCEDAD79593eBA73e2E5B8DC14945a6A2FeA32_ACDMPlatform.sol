pragma solidity ^0.8.0;

import "hardhat/console.sol";

interface IACDMPlatform {
    /// Only available for sale round
    error AvailableOnlyForSaleRound();
    /// Only available for trade round
    error AvailableOnlyForTradeRound();
    /// Can't buy tokens balance is too low
    error CantBuyTokensBalanceIsLow(
        address _buyer,
        uint256 _ethAmount,
        uint256 _tokensAmount,
        uint256 _tokenBalance
    );
    /// Head address provided by sender doesn't register
    error HeadIsNotRegistered(address _head);
    /// Amount has to be greater than zero
    error AmountHasToBeGreaterThanZero();
    /// Your ACDMToken balance is low
    error YourAcdmTokenBalanceIsLow(uint256 _amount, uint256 balance);
    /// You are not the saller
    error YouAreNotTheSeller();
    /// Order balance is zero
    error OrderBalanceIsZero();
    /// Ether amount has to be greater than zero
    error EtherAmountHasToBeGreaterThanZero();
    /// Order token balance is low
    error OrderTokenBalanceIsLow(
        uint256 _orderId,
        uint256 _pricePerToken,
        uint256 _sumTokenToBuy
    );
    /// Only DAO can call the method
    error OnlyDaoCanCallTheMethod();
    /// You can't be you head
    error YouCantBeYouHead();

    /// @notice New user register
    /// @dev New user register
    /// @param _user address
    /// @param _head (lead) of the user
    event NewUserInit(address _user, address _head);

    /// @notice Sale round was started
    /// @dev Sale round was started
    /// @param _roundNumber new number of a round
    /// @param _roundFinishTime when the round will finish
    event SaleRoundStart(uint256 _roundNumber, uint256 _roundFinishTime);

    /// @notice Sale round can't start
    /// @dev Sale round can't start, so some requirements is not available
    event SaleRoundCantStart();

    /// @notice ACDMTokens was sold
    /// @dev ACDMTokens was sold
    /// @param _buyer who buy ACDMTokens
    /// @param _ethAmount spend to buy ACDMTokens
    /// @param _tokensAmount acquire by buyer =)
    event BuyAcdmToken(
        address _buyer,
        uint256 _ethAmount,
        uint256 _tokensAmount
    );

    /// @notice ACDMTokens was sold and first level get reward
    /// @dev ACDMTokens was sold and first level get reward
    /// @param _buyer who buy ACDMTokens
    /// @param _head lead address of buyer
    /// @param _amount of reward
    /// @param currentPercent of first level at this time
    event BuyAcdm1LevelAward(
        address _buyer,
        address _head,
        uint256 _amount,
        uint256 currentPercent
    );

    /// @notice ACDMTokens was sold and second level get reward
    /// @dev ACDMTokens was sold and second level get reward
    /// @param _head level 1 lead
    /// @param _amount of reward
    /// @param currentPercent of second level at this time
    event BuyAcdm2LevelAward(
        address _head,
        uint256 _amount,
        uint256 currentPercent
    );

    /// @notice ACDMTokens were minted
    /// @dev ACDMTokens were minted
    /// @param _roundNumber current numbber of a round
    /// @param _amount of minted tokens
    /// @param _acdmTokenPrice price for current round
    event AcdmTokenMinted(
        uint256 _roundNumber,
        uint256 _amount,
        uint256 _acdmTokenPrice
    );

    /// @notice Trade round started
    /// @dev Trade round started
    /// @param _roundNumber current numbber of a round
    /// @param _roundFinishTime when the round will finish
    event TradeRoundStart(uint256 _roundNumber, uint256 _roundFinishTime);

    /// @notice Trade round can't start
    /// @dev Trade round can't start, so some requirements is not available
    event TradeRoundCantStart();

    /// @notice Order was added
    /// @dev Order was added
    /// @param _orderId simple just order identifier
    /// @param _saller who add the order
    /// @param _amount token to trade
    /// @param _pricePerToken as is
    event OrderWasAdded(
        uint256 _orderId,
        address _saller,
        uint256 _amount,
        uint256 _pricePerToken
    );

    /// @notice Order was removed
    /// @dev Order was removed
    /// @param _orderId simple just order identifier
    /// @param _saller who add the order
    /// @param _amount token was claimed back
    event OrderWasRemoved(
        uint256 _orderId, 
        address _saller, 
        uint256 _amount
    );

    /// @notice Explain to an end user what this does
    /// @dev Explain to a developer any extra details
    /// @param _orderId simple just order identifier
    /// @param _buyer person who buy an order
    /// @param _eth amount spend on order
    /// @param _sumTokenToBuy Sum of tokens was sold
    event OrderRedeemed(
        uint256 _orderId,
        address _buyer,
        uint256 _eth,
        uint256 _sumTokenToBuy
    );

    /// @notice ACDMTokens was sold on trade round and first level get reward
    /// @dev ACDMTokens was sold on trade round and first level get reward
    /// @param _buyer who buy ACDMTokens
    /// @param _head lead address of buyer
    /// @param _amount of reward
    /// @param currentPercent of first level at this time
    event RedeemAcdm1LevelAward(
        address _buyer,
        address _head,
        uint256 _amount,
        uint256 currentPercent
    );

    /// @notice ACDMTokens was sold on trade round and second level get reward
    /// @dev ACDMTokens was sold on trade round and second level get reward
    /// @param _head level 1 lead
    /// @param _amount of reward
    /// @param currentPercent of second level at this time
    event RedeemAcdm2LevelAward(
        address _head,
        uint256 _amount,
        uint256 currentPercent
    );

    /// @notice Register user in refferal system
    /// @dev Register user in refferal system
    /// @param _head an address who lead the caller
    function register(address _head) external;

    /// @notice Start sale round
    /// @dev Sale round, setup new round
    function startSaleRound() external;

    /// @notice Buy ACDMToken while sale round
    /// @dev Buy ACDMToken while sale round
    function buyACDM() external payable;

    /// @notice Start trade round
    /// @dev Trade round, setup new round
    function startTradeRound() external;

    /// @notice Add order to sale ACDMToken
    /// @dev Add order to sale ACDMToken
    /// @param _amount amount ACDMToken to sale
    /// @param _pricePerToken price per one ACDMToken
    function addOrder(uint256 _amount, uint256 _pricePerToken) external;

    /// @notice Remove order, claim residue of tokens
    /// @dev Remove order, claim residue of tokens
    /// @param _orderId id of order to remove
    function removeOrder(uint256 _orderId) external;

    /// @notice Redeem ACDMTokens in an order
    /// @dev Redeem ACDMTokens in an order
    /// @param _orderId id of order to buy
    function redeemOrder(uint256 _orderId) external payable;

    /// @notice Only DAO can call
    /// @dev Set all collected rewards to the owner, only DAO can call
    function sendRewardToOwner() external;

    /// @notice Only DAO can call
    /// @dev Buy XXXTokens on collected reward and burn them.
    function increaseXXXTokenPrice() external;
}

