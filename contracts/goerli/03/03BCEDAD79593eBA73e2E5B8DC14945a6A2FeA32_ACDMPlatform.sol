pragma solidity ^0.8.0;

import "./interfaces/ierc20.sol";
import "./interfaces/IACDMPlatform.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

import "hardhat/console.sol";

/// @title ACDMToken
/// @author nashgc
/// @notice Platform to buy and trade ACDM Tokens
/// @dev Platform to buy and trade ACDM Tokens
contract ACDMPlatform is IACDMPlatform, ReentrancyGuard {

    using Counters for Counters.Counter;

    address public acdmToken;
    address public dao;
    address public owner;
    address public uniV2Router;
    address public xxxToken;
    uint public roundDuration;
    uint public acdmTokenPrice;
    uint public totalOwnerComission;
    uint public saleRefferLevelOnePercentBP = 500;
    uint public saleRefferLevelTwoPercentBP = 300;
    uint public tradeRefferLevelOnePercentBP = 250;
    uint public tradeRefferLevelTwoPercentBP = 250;
    uint internal roundFinishTime;
    uint totalTradeEther;

    Round public currentRound;
    Counters.Counter public roundNumber;
    Counters.Counter private _orderCounter;

    enum Round {
        Sale,
        Trade
    }

    struct Order {
        address seller;
        uint amount;
        uint pricePerToken;
    }

    struct User {
        bool register;
        address head;
    }

    mapping(uint => Order) public orderBook;
    mapping(address => User) public refferals;

    modifier onlySaleRound () {
        if (currentRound != Round.Sale)
            revert AvailableOnlyForSaleRound(); 
        _;
    }

    modifier onlyTradeRound () {
        if (currentRound != Round.Trade)
            revert AvailableOnlyForTradeRound(); 
        _;
    }

    modifier onlyDao() {
        if (dao != msg.sender)
            revert OnlyDaoCanCallTheMethod();
        _;
    }

    /// @notice Check user registration
    /// @dev Check an address to be participant refferal system
    /// @param _address an address for check
    /// @return bool, true if participant else false
    function isRegistered(address _address) internal view returns(bool) {
        return refferals[_address].register;
    }

    /// @notice Check if user a refferal
    /// @dev Check an address in refferals, then get the head
    /// @param _address an address for check
    /// @return bool, true if has refferal else false
    function hasHead(address _address) internal view returns(bool) {
        if (refferals[_address].head == address(0))
            return false;
        return true;
    }

    /// @notice Initial function for setup contract
    /// @dev Setup initial platform parametrs
    /// @param _acdmToken an address of ACDMToken contract
    /// @param _roundDuration round duration, default 3 days
    /// @param _acdmTokenPrice ACDMToken price for the first sale round
    constructor(
        address _acdmToken,
        address _dao,
        address _uniV2Router,
        address _xxxToken,
        uint _roundDuration, 
        uint _acdmTokenPrice
    ) {
        acdmToken = _acdmToken;
        dao = _dao;
        owner = msg.sender;
        uniV2Router = _uniV2Router;
        xxxToken = _xxxToken;
        roundDuration = _roundDuration;
        acdmTokenPrice = _acdmTokenPrice;
    }

    /// @notice Register user in refferal system
    /// @dev Register user in refferal system
    /// @param _head an address who lead the caller
    function register(address _head) external override {
        if (msg.sender == _head)
            revert YouCantBeYouHead();
        if (!isRegistered(_head) && _head != address(0))
            revert HeadIsNotRegistered(_head);

        refferals[msg.sender].register = true;
        refferals[msg.sender].head = _head;

        emit NewUserInit(msg.sender, _head);
    }

    /// @notice Start sale round
    /// @dev Sale round, setup new round
    function startSaleRound() external override {
        uint _roundNumber = roundNumber.current();

        if (
            _roundNumber == 0 &&
            block.timestamp > roundFinishTime
        ) {
            mintAcdmToken();
            roundFinishTime = block.timestamp + roundDuration;
            emit SaleRoundStart(_roundNumber, roundFinishTime);
        } else if (
            _roundNumber != 0 && 
            currentRound != Round.Sale && 
            block.timestamp > roundFinishTime
        ) {
            roundNumber.increment();
            currentRound = Round.Sale;
            mintAcdmToken();
            roundFinishTime = block.timestamp + roundDuration;
            emit SaleRoundStart(roundNumber.current(), roundFinishTime);
        } else {
            emit SaleRoundCantStart();
        }
    }

    /// @notice Caclulate how many tokens to buy
    /// @dev Calcelation based on (eth / tokenPrice) * 10 * 6
    /// @param _eth sum of ether to buy tokens
    /// @param _acdmTokenPrice price for a token
    /// @return uint, amount tokens to buy
    function tokenToBuy(uint _eth, uint _acdmTokenPrice) 
        internal 
        pure 
        returns(uint) 
    {
        return (_eth / _acdmTokenPrice) * 10 ** 6;
    }

    /// @notice Buy ACDMToken while sale round
    /// @dev Buy ACDMToken while sale round
    function buyACDM() 
        external 
        payable
        override
        nonReentrant
        onlySaleRound 
    {
        uint sumTokenToBuy = tokenToBuy(msg.value, acdmTokenPrice);
        uint acdmBalance = IERC20(acdmToken).balanceOf(address(this));
        
        if (sumTokenToBuy > acdmBalance)
            revert CantBuyTokensBalanceIsLow(msg.sender, msg.value, sumTokenToBuy, acdmBalance);

        IERC20(acdmToken).transfer(msg.sender, sumTokenToBuy);

        emit BuyAcdmToken(msg.sender, msg.value, sumTokenToBuy);

        uint award = msg.value * saleRefferLevelOnePercentBP / 10000;
        uint awardLevel2 = msg.value * saleRefferLevelTwoPercentBP / 10000;
        
        if (isRegistered(msg.sender) && hasHead(msg.sender)) {
            address head = refferals[msg.sender].head;
            payable(head).transfer(award);
            emit BuyAcdm1LevelAward(msg.sender, head, award, saleRefferLevelOnePercentBP);
            if (hasHead(head)) {
                address headLevel2 = refferals[head].head;
                payable(headLevel2).transfer(awardLevel2);
                emit BuyAcdm2LevelAward(headLevel2, awardLevel2, saleRefferLevelTwoPercentBP);
            } else {
                totalOwnerComission += awardLevel2;
            }
        } else {
            totalOwnerComission += award;
        }
    }

    /// @notice Start trade round
    /// @dev Trade round, setup new round
    function startTradeRound() external override {
        uint acdmBalance = IERC20(acdmToken).balanceOf(address(this));

        if (
            (currentRound != Round.Trade && 
            block.timestamp > roundFinishTime) ||
            acdmBalance == 0 &&
            currentRound != Round.Trade
        ) {
            IERC20(acdmToken).burnFrom(address(this), acdmBalance);

            currentRound = Round.Trade;
            roundFinishTime = block.timestamp + roundDuration;
            roundNumber.increment();
            totalTradeEther = 0;

            emit TradeRoundStart(roundNumber.current(), roundFinishTime);
        } else {
            emit TradeRoundCantStart();
        }
    }

    /// @notice Add order to sale ACDMToken
    /// @dev Add order to sale ACDMToken
    /// @param _amount amount ACDMToken to sale
    /// @param _pricePerToken price per one ACDMToken
    function addOrder(uint _amount, uint _pricePerToken) 
        external 
        override 
        onlyTradeRound
    {
        if (_amount == 0)
            revert AmountHasToBeGreaterThanZero();

        uint acdmSellerBalance = IERC20(acdmToken).balanceOf(msg.sender);

        if (_amount > acdmSellerBalance)
            revert YourAcdmTokenBalanceIsLow(_amount, acdmSellerBalance);
        
        IERC20(acdmToken).transferFrom(msg.sender, address(this), _amount);

        uint orderId = _orderCounter.current();
        orderBook[orderId].seller = msg.sender;
        orderBook[orderId].amount = _amount;
        orderBook[orderId].pricePerToken = _pricePerToken;
        _orderCounter.increment();

        emit OrderWasAdded(orderId, msg.sender, _amount, _pricePerToken);
    }

    /// @notice Remove order, claim residue of tokens
    /// @dev Remove order, claim residue of tokens
    /// @param _orderId id of order to remove
    function removeOrder(uint _orderId) 
        external 
        override 
        onlyTradeRound 
    {
        Order storage order = orderBook[_orderId];

        if (order.seller != msg.sender)
            revert YouAreNotTheSeller();
        if (order.amount == 0)
            revert OrderBalanceIsZero();
        
        IERC20(acdmToken).transfer(msg.sender, order.amount);

        emit OrderWasRemoved(_orderId, order.seller, order.amount);
    }

    /// @notice Redeem ACDMTokens in an order
    /// @dev Redeem ACDMTokens in an order
    /// @param _orderId id of order to buy
    function redeemOrder(uint _orderId) 
        external
        payable
        override
        nonReentrant
        onlyTradeRound 
    {
        if (msg.value == 0)
            revert EtherAmountHasToBeGreaterThanZero();

        Order storage order = orderBook[_orderId];
        uint sumTokenToBuy = tokenToBuy(msg.value, order.pricePerToken);

        if (sumTokenToBuy > order.amount)
            revert OrderTokenBalanceIsLow(_orderId, order.pricePerToken, sumTokenToBuy);

        IERC20(acdmToken).transfer(msg.sender, sumTokenToBuy);
        order.amount -= sumTokenToBuy;

        emit OrderRedeemed(_orderId, msg.sender, msg.value, sumTokenToBuy);

        uint award = msg.value * tradeRefferLevelOnePercentBP / 10000;
        uint awardLevel2 = msg.value * tradeRefferLevelTwoPercentBP / 10000;
        uint residueToSeller = msg.value - (award + awardLevel2);

        payable(order.seller).transfer(residueToSeller);

        totalTradeEther += msg.value;
    
        if (isRegistered(msg.sender) && hasHead(msg.sender)) {
            address head = refferals[msg.sender].head;
            payable(head).transfer(award);
            emit RedeemAcdm1LevelAward(msg.sender, head, award, tradeRefferLevelOnePercentBP);
            if (hasHead(head)) {
                address headLevel2 = refferals[head].head;
                payable(headLevel2).transfer(awardLevel2);
                emit RedeemAcdm2LevelAward(headLevel2, awardLevel2, tradeRefferLevelTwoPercentBP);
            } else {
                totalOwnerComission += awardLevel2;
            }
        } else {
            totalOwnerComission += award;
        }
    }

    /// @notice Mint ACDMToken for a sale round
    /// @dev Mint ACDMToken, different algorithms depends of round.
    function mintAcdmToken() internal onlySaleRound {
        uint amount;
        uint _roundNumber = roundNumber.current();

        if (_roundNumber == 0) {
            amount = (1 ether / acdmTokenPrice) * 10 ** 6;
        } else {
            acdmTokenPrice = acdmTokenPrice * 103 / 100 + 4000000000000;
            amount = (totalTradeEther / acdmTokenPrice) * 10 ** 6;
        }

        IERC20(acdmToken).mint(address(this), amount);

        emit AcdmTokenMinted(_roundNumber, amount, acdmTokenPrice);
    }

    /// @notice Only DAO can call
    /// @dev Set all collected rewards to the owner, only DAO can call
    function sendRewardToOwner() 
        external 
        override 
        onlyDao 
    {
        payable(owner).transfer(totalOwnerComission);
        totalOwnerComission = 0;
    }

    /// @notice Only DAO can call
    /// @dev Buy XXXTokens on collected reward and burn them.
    function increaseXXXTokenPrice() 
        external 
        override 
        onlyDao 
    {
        IUniswapV2Router01 router = IUniswapV2Router01(uniV2Router);

        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = address(xxxToken);

        uint deadline = block.timestamp + 360;
        router.swapExactETHForTokens{value:totalOwnerComission}(
            0,
            path, 
            address(this), 
            deadline
        );

        totalOwnerComission = 0;

        IERC20(xxxToken).burnFrom(
            address(this),
            IERC20(xxxToken).balanceOf(address(this))
        );
    }

}

