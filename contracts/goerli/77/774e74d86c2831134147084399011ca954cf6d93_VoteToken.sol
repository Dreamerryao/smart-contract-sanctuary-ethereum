enum OrderStatus {
    // 0
    Unknown,
    // 1-Orders can continue to be traded while they are still pending
    Listing,
    // 2-The order has been canceled by the seller
    Canceled,
    // 3-The order has expired and failed to sell
    Unsold,
    // 4-The order has been filled
    Sold
}

enum OrderKind {
    // 0-
    FixedPrice,
    // 1-
    AuctionWithoutReserve,
    // 2-
    AuctionWithReserve
}

enum ItemType {
    Unknown,
    ERC721,
    ERC1155
}

