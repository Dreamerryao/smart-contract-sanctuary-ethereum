pragma solidity 0.8.16;

import "oz/token/ERC20/IERC20.sol";
import "oz/token/ERC721/ERC721.sol";
import {Ownable} from "oz/access/Ownable.sol";
import "./interfaces/VoteTokenInterface.sol";
import "./interfaces/NFTVoteErrors.sol";
import "./lib/Structs.sol";

contract VoteToken is VoteTokenInterface, ERC721, Ownable {
    /**
     * @dev Staked asset address
     * If the asset is ETH ,the address is zero.
     */
    address private immutable _ASSET;
    string private _baseuri;
    mapping(uint256 => VoteMeta) private _metas;
    /**
     * @dev token id flag.
     */
    uint256 public nonce;
    mapping(address => bool) public operators;

    constructor(string memory _name, string memory _symbol, address _asset, string memory baseURI_)
        ERC721(_name, _symbol)
    {
        //slither-disable-next-line missing-zero-check
        _ASSET = _asset;
        _baseuri = baseURI_;
    }

    modifier onlyOperator() {
        if (!operators[msg.sender]) {
            revert OnlyCallByVotePool();
        }
        _;
    }

    function _baseURI() internal view override returns (string memory) {
        return _baseuri;
    }

    function setBaseURI(string memory uri) external onlyOwner {
        _baseuri = uri;
    }

    function addOperator(address op) external onlyOwner {
        operators[op] = true;
        emit OperatorGranted(op);
    }

    function removeOperator(address op) external onlyOwner {
        delete operators[op];
        emit OperatorRevoked(op);
    }

    function asset() external view override returns (address) {
        return _ASSET;
    }

    /**
     * @notice Mint an NFT for voter after voted.
     *
     * Requirements:
     *
     * - `voter` must not be zero address.
     * - `msg.sender` must be `votePool` address.
     *
     * The `orderHash` and `share` values are not checked here.
     * If they are set incorrectly, the `voter` will lose ETH.
     *
     * @param voter he is nft recipient.
     * @param orderHash the hash of order.
     * @param share it is the profit weigth of this vote.
     */
    function mintWithETH(address voter, bytes32 orderHash, uint120 share) external payable override onlyOperator {
        assert(_ASSET == address(0));
        assert(msg.value < type(uint120).max);
        _mintTo(voter, orderHash, uint120(msg.value), share);
    }

    /**
     * @notice Mint an NFT for voter after voted.
     * @dev see `mintWithETH` method.
     */
    function mintWithERC20(address voter, bytes32 orderHash, uint256 amount, uint120 share)
        external
        override
        onlyOperator
    {
        assert(_ASSET != address(0));
        assert(amount < type(uint120).max);

        bool success = IERC20(_ASSET).transferFrom(msg.sender, address(this), amount);
        if (!success) {
            revert ERC20TransferGenericFailure();
        }
        _mintTo(voter, orderHash, uint120(amount), share);
    }

    /**
     * @notice Burn the given NFT item.
     * Destory the NFT and return staked assets after canceling the vote.
     * There may be a burning fee to pay. Like canceling a vote before it's over.
     *
     * Requirements:
     *
     * - `feeReceiver` must not be zero address.
     * - `msg.sender` must be `votePool` address..
     *
     * @param tokenId the id of token item.
     * @param fee a burning fee.
     * @param feeCollector the fee collector.
     */
    function burn(uint256 tokenId, uint256 fee, address feeCollector) external override onlyOperator {
        _withdraw(tokenId, fee, feeCollector);
        _burn(tokenId);
    }

    /**
     * @notice burn the `tokenId` nft
     * @dev Return the staked assets but keep NFT.
     *
     * Requirements:
     *
     * - `msg.sender` must be `votePool` address.
     */
    function withdraw(uint256 tokenId) external override onlyOperator {
        _withdraw(tokenId, 0, address(0));
    }

    function tokenMeta(uint256 tokenId) external view override returns (VoteMeta memory) {
        return _metas[tokenId];
    }

    function _mintTo(address to, bytes32 orderHash, uint120 amount, uint120 share) private {
        uint256 tokenId = ++nonce;
        _mint(to, tokenId);

        _metas[tokenId] = VoteMeta({
            orderHash: orderHash,
            amount: amount,
            share: share,
            mintTime: uint64(block.timestamp), //slither-disable timestamp
            withdrawn: false
        });
    }

    function _withdraw(uint256 tokenId, uint256 fee, address feeCollector) private {
        if (_metas[tokenId].withdrawn) {
            revert RepeatWithdrawal();
        }
        address voter = ownerOf(tokenId);
        uint256 amount = _metas[tokenId].amount;
        _metas[tokenId].withdrawn = true;

        _transferAsset(voter, amount - fee);
        _transferAsset(feeCollector, fee);

        emit Withdraw(tokenId, fee);
    }

    function _transferAsset(address to, uint256 amount) private {
        if (amount == 0) {
            return;
        }
        if (to == address(0)) {
            revert MissingAddress();
        }
        if (_ASSET == address(0)) {
            //ETH
            // slither-disable-next-line all,arbitrary-send-eth,low-level-calls
            (bool success,) = to.call{value: amount}("");
            if (!success) {
                revert EtherTransferGenericFailure();
            }
        } else {
            // ERC20
            bool success = IERC20(_ASSET).transfer(to, amount);
            if (!success) {
                revert ERC20TransferGenericFailure();
            }
        }
    }
}

