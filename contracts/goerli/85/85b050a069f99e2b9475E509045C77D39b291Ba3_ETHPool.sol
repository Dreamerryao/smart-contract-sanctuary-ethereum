pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

contract ETHPool is AccessControl, ReentrancyGuard {
    using SafeMath for uint256;

    bytes32 public constant STAFF_ROLE = keccak256("STAFF_ROLE");
    uint256 public constant PRECISION_PADDING = 1000000000000000000;

    mapping(uint256 => mapping(address => uint256)) public balances;

    mapping(uint256 => uint256) public periodTotalStaked;
    mapping(address => uint256) public lastPeriodWithdrawn;
    mapping(uint256 => uint256) public rewardsPerPeriod;

    address public owner;
    uint256 public totalStaked;
    uint256 public currentPeriod;
    bool public increasePeriodOnStake;

    event RewardsDeposited(address indexed sender, uint256 amount, uint256 period);
    event StaffAdded(address indexed added, address indexed newStaff);
    event StakeDeposited(address indexed sender, uint256 amount);
    event StakeWithdrawn(address indexed sender, uint256 stakesWithdrawn, uint256 rewardAmount);

    constructor() {
        _setupRole(STAFF_ROLE, msg.sender);
        _setRoleAdmin(STAFF_ROLE, STAFF_ROLE);
    }

    /// @notice gets the cumulative balances a user has staked across staking periods
    /// @dev only queries the sender of the query
    /// @return currentBalance uint256
    function getUserBalance() public view returns (uint256 currentBalance) {
        for (uint256 i = lastPeriodWithdrawn[msg.sender]; i <= currentPeriod; i++) {
            currentBalance += balances[i][msg.sender];
        }
    }

    /// @notice queries the total amount of funds defined for rewards
    /// @return rewardsFunds uint256
    function getTotalRewards() public view returns (uint256 rewardsFunds) {
        for (uint256 i = 0; i <= currentPeriod; i++) {
            rewardsFunds += rewardsPerPeriod[i];
        }
    }

    /// @notice calculates the reward amount a user has earned in a specific period
    /// @param user address of the user to be queried
    /// @param period uint256 staking period number
    /// @return reward uint256
    function getPeriodRewardByUser(address user, uint256 period)
        public
        view
        returns (uint256 reward)
    {
        return
            calculateRewards(
                periodTotalStaked[period],
                balances[period][user],
                rewardsPerPeriod[period]
            );
    }

    /// @notice calculates the proportional reward for a staking period
    /// @param totalStake uint256 total stakes of the staking period
    /// @param stakeAmount uint256 stake amount to calculate the rewards from
    /// @param periodRewards uint256 reward amount to be distributed to the staker
    /// @return rewardAmount
    function calculateRewards(
        uint256 totalStake,
        uint256 stakeAmount,
        uint256 periodRewards
    ) public pure returns (uint256 rewardAmount) {
        if (totalStake > 0 && stakeAmount > 0) {
            uint256 stakeShare = (totalStake * PRECISION_PADDING) / stakeAmount;
            rewardAmount =
                (((periodRewards * PRECISION_PADDING) / stakeShare) * PRECISION_PADDING) /
                PRECISION_PADDING;
        }
    }

    /// @notice gets the rewards amount owed to the sender
    /// @return rewardBalance
    function getRewardAmount() public view returns (uint256 rewardBalance) {
        for (uint256 i = lastPeriodWithdrawn[msg.sender]; i < currentPeriod; i++) {
            rewardBalance += getPeriodRewardByUser(msg.sender, i);
        }
    }

    /// @notice adds staff role to a given wallet address
    /// @dev requires sender to be member from the staff role
    /// @param newStaff address new staff
    function addStaff(address newStaff) public {
        require(hasRole(STAFF_ROLE, msg.sender), "ETHPool: sender is not staff");

        grantRole(STAFF_ROLE, newStaff);

        emit StaffAdded(msg.sender, newStaff);
    }

    /// @notice deposits rewards funds at the end of a staking period
    /// @dev requires the sender to be staff
    /// @dev cant deposit rewards for periods without stake
    /// @dev prevents new stakes for entering the same period
    function depositRewards() external payable nonReentrant {
        require(hasRole(STAFF_ROLE, msg.sender), "ETHPool: sender is not staff");
        require(msg.value > 0, "ETHPool: Reward deposit is 0");

        require(periodTotalStaked[currentPeriod] > 0, "ETHPool: adding rewards with no stakers");

        rewardsPerPeriod[currentPeriod] += msg.value;

        emit RewardsDeposited(msg.sender, msg.value, currentPeriod);
        increasePeriodOnStake = true;
    }

    /// @notice stakes funds in ether for the submitting user
    function depositStake() external payable nonReentrant {
        require(msg.value > 0, "ETHPool: amount deposit is 0");

        if (increasePeriodOnStake) {
            increasePeriodOnStake = false;
            currentPeriod++;
        }

        balances[currentPeriod][msg.sender] += msg.value;
        periodTotalStaked[currentPeriod] += msg.value;
        totalStaked += msg.value;

        emit StakeDeposited(msg.sender, msg.value);
    }

    /// @notice withdraws the given amount form stakes plus their proportional reward
    /// @dev requested amount is debited only from stakes
    /// @param amount uint256 amount requested to be withdrawn
    function withdraw(uint256 amount) external nonReentrant {
        uint256 stakesWithdrawn;
        uint256 rewardToClaim;
        uint256 newWithdrawnPeriod = 0;

        for (
            uint256 i = lastPeriodWithdrawn[msg.sender];
            stakesWithdrawn <= amount && i <= currentPeriod;
            i++
        ) {
            uint256 remanentStakeAmount = amount - stakesWithdrawn;
            uint256 periodBalance = balances[i][msg.sender];
            uint256 periodRewards;
            uint256 periodDebitAmount;

            // Partial period withdrawal
            if (periodBalance > remanentStakeAmount) {
                periodRewards = calculateRewards(
                    periodTotalStaked[i],
                    remanentStakeAmount,
                    rewardsPerPeriod[i]
                );
                periodDebitAmount = remanentStakeAmount;
            } else {
                // Full Period Withdrawal
                periodRewards = getPeriodRewardByUser(msg.sender, i);
                periodDebitAmount = periodBalance;
            }

            balances[i][msg.sender] -= periodDebitAmount;
            periodTotalStaked[i] -= periodDebitAmount;
            stakesWithdrawn += periodDebitAmount;

            rewardToClaim += periodRewards;
            rewardsPerPeriod[i] -= periodRewards;

            newWithdrawnPeriod = i;
        }

        lastPeriodWithdrawn[msg.sender] = newWithdrawnPeriod;
        totalStaked -= stakesWithdrawn;

        emit StakeWithdrawn(msg.sender, stakesWithdrawn, rewardToClaim);

        (bool sent, ) = msg.sender.call{value: stakesWithdrawn + rewardToClaim}("");
        require(sent, "Failed to withdraw");
    }
}

