pragma solidity 0.8.9;

import "./interfaces/ILimitOrdersController.sol";
import "./interfaces/IMarketOrdersController.sol";
import "./interfaces/ITheTradeWETH.sol";
import "./interfaces/IController.sol";
import "./interfaces/ITFactory.sol";
import "./interfaces/IUniSwapV3Router.sol";

contract PositionRouterStorage {
    enum CreationType {
        LONG_LIMIT,
        SHORT_LIMIT,
        LONG_MARKET,
        SHORT_MARKET
    }

    enum PositionType {
        OPENED_LONG_IN_PROGRESS,
        OPENED_SHORT_IN_PROGRESS,
        OPENED_LONG_COMPLITE,
        OPENED_SHORT_COMPLITE,
        CLOSED_LONG_IN_PROGRESS,
        CLOSED_SHORT_IN_PROGRESS,
        CLOSED_LONG_COMPLITE,
        CLOSED_SHORT_COMPLITE,
        LIQUIDATED_LONG,
        LIQUIDATED_SHORT,
        CANCELLED_LONG,
        CANCELLED_SHORT
    }

    struct Position {
        uint256 positionId;
        PositionType positionType;
        address owner;
        address collateralToken;
        address poolAddress;
        Order openOrder;
        address closeToken;
        Order closeOrder;
        uint256 collateralAmount;
        uint256 borrowAmount;
        uint256 blockNumber;
        uint256 executionCost;
        SignatureData signature;
    }

    struct Order {
        bool marketType;
        uint256 orderId;
        uint256 receivedAmountOfToken0;
        uint256 receivedAmountOfToken1;
    }

    struct SignatureData {
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    struct CreateParamsLimit {
        CreationType positionType;
        address poolAddress;
        address collateralToken;
        int24 tickLower;
        int24 tickUpper;
        uint256 collateralAmount;
        uint256 leverage;
        uint256 deadline;
        SignatureData signature;
    }

    struct CreateParamsMarket {
        CreationType positionType;
        address poolAddress;
        address collateralToken;
        uint256 collateralAmount;
        uint256 minAmountOut;
        uint256 leverage;
        uint256 deadline;
        uint160 sqrtPriceLimitX96;
        SignatureData signature;
    }

    struct CloseMarketParams {
        uint256 positionId;
        address receivingToken;
        uint256 deadline;
    }

    struct CloseLimitParams {
        uint256 positionId;
        int24 tickLower;
        int24 tickUpper;
        uint256 deadline;
        SignatureData signature;
    }

    ILimitOrdersController public limitOrdersController;
    IMarketOrdersController public marketOrdersController;
    IController public controller;
    ITFactory public tTokenFactory;
    address public creatorDelegator;
    address public closerDelegator;
    address public cancellerDelegator;
    uint256 public maxLeverage;
    uint256 public leverageAccuracy;
    uint256 public positionCounter;
    IUniSwapV3Router public swapRouter;

    uint256 public WITHDRAW_GAS;
    uint256 public TRANSFER_GAS;
    uint256 public SWAP_GAS;
    uint256 public SEND_GAS;

    uint256 public SLIPPAGE;
    uint256 public SLIPPAGE_ACCURACY;
    uint256 public GAS_SLIPPAGE;
    uint256 public GAS_SLIPPAGE_ACCURACY;

    /// @notice Address of wrapped ether
    ITheTradeWETH public weth;
    // positionId => Position data
    mapping(uint256 => Position) public positions;

    // closeOrderId => openOrderId
    mapping(uint256 => uint256) internal marketToPositionId;
    mapping(uint256 => uint256) internal limitToPositionId;

    event CreatePosition(
        uint256 id,
        CreationType positionType,
        address owner,
        address collateralToken,
        address poolAddress,
        Order openOrder,
        uint256 collateralAmount,
        uint256 leverage,
        uint256 blockNumber
    );

    event ClosePosition(
        uint256 id,
        PositionType positionType,
        address owner,
        address collateralToken,
        address poolAddress,
        Order openOrder,
        address receivingToken,
        Order closeOrder,
        uint256 blockNumber
    );

    event CancelPosition(
        uint256 id,
        PositionType positionType,
        address owner,
        address collateralToken,
        uint256 amountOfCollateralToken,
        address secondToken,
        uint256 amountOfSecondToken,
        address poolAddress,
        Order openOrder,
        Order closeOrder,
        uint256 blockNumber
    );

    event PositionStateChanged(uint256 id, PositionType positionType);
}

