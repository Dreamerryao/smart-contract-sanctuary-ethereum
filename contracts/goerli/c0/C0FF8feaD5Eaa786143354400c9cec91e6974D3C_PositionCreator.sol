pragma solidity 0.8.9;

import "./PositionRouterStorage.sol";
import "./interfaces/ITErc20.sol";
import "@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";

contract PositionCreator is
    PositionRouterStorage,
    Initializable,
    UUPSUpgradeable,
    AccessControlUpgradeable
{
    function createPositionUsingMarket(CreateParamsMarket memory _params)
        external
        returns (uint256 orderId)
    {
        require(
            _msgSender() ==
                ecrecover(
                    keccak256(
                        abi.encodePacked(
                            "\x19Ethereum Signed Message:\n32",
                            weth.getPermitMessageHash(
                                _msgSender(),
                                address(this)
                            )
                        )
                    ),
                    _params.signature.v,
                    _params.signature.r,
                    _params.signature.s
                ),
            "Wrong sign."
        );
        require(_params.poolAddress != address(0), "Pool address cannot be 0");
        require(
            IUniswapV3Pool(_params.poolAddress).token0() ==
                _params.collateralToken ||
                IUniswapV3Pool(_params.poolAddress).token1() ==
                _params.collateralToken,
            "Wrong pool address"
        );
        require(_params.deadline >= block.timestamp, "Deadline");
        require(_params.leverage <= maxLeverage, "Leverage is too big");
        require(_params.leverage >= leverageAccuracy, "Leverage is too small");
        require(
            _params.positionType == CreationType.LONG_MARKET ||
                _params.positionType == CreationType.SHORT_MARKET,
            "Wrong position type"
        );
        positionCounter++;
        uint256 borrowAmount = ((_params.collateralAmount * _params.leverage) /
            leverageAccuracy) - _params.collateralAmount;
        ITErc20 tToken = ITErc20(
            tTokenFactory.getPool(_params.collateralToken)
        );
        // Borrow
        tToken.borrow(borrowAmount, positionCounter);
        IERC20Upgradeable(_params.collateralToken).transferFrom(
            _msgSender(),
            address(this),
            _params.collateralAmount
        );
        address secondToken = _params.collateralToken ==
            IUniswapV3Pool(_params.poolAddress).token0()
            ? IUniswapV3Pool(_params.poolAddress).token1()
            : IUniswapV3Pool(_params.poolAddress).token0();

        IERC20Upgradeable(_params.collateralToken).approve(
            address(marketOrdersController),
            _params.collateralAmount + borrowAmount
        );

        orderId = marketOrdersController.createMarketOrder(
            IMarketOrdersController.CreateMarketOrderParams({
                receiver: address(this),
                tokenIn: _params.collateralToken,
                tokenOut: secondToken,
                fee: IUniswapV3Pool(_params.poolAddress).fee(),
                amountIn: _params.collateralAmount + borrowAmount,
                minAmountOut: _params.minAmountOut,
                deadline: _params.deadline,
                sqrtPriceLimitX96: _params.sqrtPriceLimitX96
            })
        );
        IMarketOrdersController.MarketOrder
            memory order = marketOrdersController.marketOrders(
                _params.poolAddress,
                orderId
            );
        positions[positionCounter] = Position({
            positionId: positionCounter,
            positionType: _params.positionType == CreationType.LONG_MARKET
                ? PositionType.OPENED_LONG_IN_PROGRESS
                : PositionType.OPENED_SHORT_IN_PROGRESS,
            owner: _msgSender(),
            collateralToken: _params.collateralToken,
            poolAddress: _params.poolAddress,
            openOrder: Order({
                marketType: true,
                orderId: orderId,
                receivedAmountOfToken0: order.tokenIn ==
                    IUniswapV3Pool(_params.poolAddress).token0()
                    ? order.amountIn
                    : order.amountOut,
                receivedAmountOfToken1: order.tokenOut ==
                    IUniswapV3Pool(_params.poolAddress).token1()
                    ? order.amountOut
                    : order.amountIn
            }),
            closeToken: address(0),
            closeOrder: Order({
                marketType: false,
                orderId: 0,
                receivedAmountOfToken0: 0,
                receivedAmountOfToken1: 0
            }),
            collateralAmount: _params.collateralAmount,
            borrowAmount: borrowAmount,
            blockNumber: block.number,
            executionCost: 0,
            signature: _params.signature
        });
        marketToPositionId[orderId] = positionCounter;
        emit CreatePosition(
            positionCounter,
            _params.positionType,
            _msgSender(),
            _params.collateralToken,
            _params.poolAddress,
            positions[positionCounter].openOrder,
            _params.collateralAmount,
            borrowAmount,
            block.number
        );
    }

    function createPositionUsingLimit(CreateParamsLimit memory _params)
        external
        payable
        returns (uint256 orderId)
    {
        require(
            _msgSender() ==
                ecrecover(
                    keccak256(
                        abi.encodePacked(
                            "\x19Ethereum Signed Message:\n32",
                            weth.getPermitMessageHash(
                                _msgSender(),
                                address(this)
                            )
                        )
                    ),
                    _params.signature.v,
                    _params.signature.r,
                    _params.signature.s
                ),
            "Wrong sign."
        );
        require(_params.poolAddress != address(0), "Pool address cannot be 0");
        require(
            IUniswapV3Pool(_params.poolAddress).token0() ==
                _params.collateralToken ||
                IUniswapV3Pool(_params.poolAddress).token1() ==
                _params.collateralToken,
            "Wrong pool address"
        );
        require(_params.deadline >= block.timestamp, "Deadline");
        require(_params.leverage <= maxLeverage, "Leverage is too big");
        require(_params.leverage >= leverageAccuracy, "Leverage is too small");
        require(
            _params.positionType == CreationType.LONG_LIMIT ||
                _params.positionType == CreationType.SHORT_LIMIT,
            "Wrong position type"
        );
        positionCounter++;
        uint256 borrowAmount = ((_params.collateralAmount * _params.leverage) /
            leverageAccuracy) - _params.collateralAmount;
        ITErc20 tToken = ITErc20(
            tTokenFactory.getPool(_params.collateralToken)
        );
        tToken.borrow(borrowAmount, positionCounter);
        IERC20Upgradeable(_params.collateralToken).transferFrom(
            _msgSender(),
            address(this),
            _params.collateralAmount
        );
        ILimitOrdersController.OrderType orderType = _params.collateralToken ==
            IUniswapV3Pool(_params.poolAddress).token0()
            ? ILimitOrdersController.OrderType.TAKE_PROFIT
            : ILimitOrdersController.OrderType.BUY_LIMIT;
        IERC20Upgradeable(_params.collateralToken).approve(
            address(limitOrdersController),
            _params.collateralAmount + borrowAmount
        );
        orderId = limitOrdersController.createOrder(
            ILimitOrdersController.CreateOrderParams({
                fee: IUniswapV3Pool(_params.poolAddress).fee(),
                token0: IUniswapV3Pool(_params.poolAddress).token0(),
                token1: IUniswapV3Pool(_params.poolAddress).token1(),
                tickLower: _params.tickLower,
                tickUpper: _params.tickUpper,
                amountOfToken0: orderType ==
                    ILimitOrdersController.OrderType.TAKE_PROFIT
                    ? _params.collateralAmount + borrowAmount
                    : 0,
                amountOfToken1: orderType ==
                    ILimitOrdersController.OrderType.BUY_LIMIT
                    ? _params.collateralAmount + borrowAmount
                    : 0,
                receivedAmountOfToken0: orderType ==
                    ILimitOrdersController.OrderType.BUY_LIMIT
                    ? limitOrdersController.getAmount0FromAmount1(
                        _params.collateralAmount + borrowAmount,
                        _params.tickUpper -
                            IUniswapV3Pool(_params.poolAddress).tickSpacing(),
                        _params.tickUpper
                    )
                    : 0,
                receivedAmountOfToken1: orderType ==
                    ILimitOrdersController.OrderType.TAKE_PROFIT
                    ? limitOrdersController.getAmount1FromAmount0(
                        _params.collateralAmount + borrowAmount,
                        _params.tickLower,
                        _params.tickLower +
                            IUniswapV3Pool(_params.poolAddress).tickSpacing()
                    )
                    : 0,
                deadline: _params.deadline,
                orderType: orderType,
                signature: ILimitOrdersController.SignatureData(
                    _params.signature.v,
                    _params.signature.r,
                    _params.signature.s
                )
            })
        );

        positions[positionCounter] = Position({
            positionId: positionCounter,
            positionType: _params.positionType == CreationType.LONG_LIMIT
                ? PositionType.OPENED_LONG_IN_PROGRESS
                : PositionType.OPENED_SHORT_IN_PROGRESS,
            owner: _msgSender(),
            collateralToken: _params.collateralToken,
            poolAddress: _params.poolAddress,
            openOrder: Order({
                marketType: false,
                orderId: orderId,
                receivedAmountOfToken0: 0,
                receivedAmountOfToken1: 0
            }),
            closeToken: address(0),
            closeOrder: Order({
                marketType: false,
                orderId: 0,
                receivedAmountOfToken0: 0,
                receivedAmountOfToken1: 0
            }),
            collateralAmount: _params.collateralAmount,
            borrowAmount: borrowAmount,
            blockNumber: block.number,
            executionCost: 0,
            signature: _params.signature
        });
        limitToPositionId[orderId] = positionCounter;

        emit CreatePosition(
            positionCounter,
            _params.positionType,
            _msgSender(),
            _params.collateralToken,
            _params.poolAddress,
            Order({
                marketType: false,
                orderId: orderId,
                receivedAmountOfToken0: 0,
                receivedAmountOfToken1: 0
            }),
            _params.collateralAmount,
            borrowAmount,
            block.number
        );
    }

    /// @notice interface support
    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}

    fallback() external payable {}

    receive() external payable {}
}

