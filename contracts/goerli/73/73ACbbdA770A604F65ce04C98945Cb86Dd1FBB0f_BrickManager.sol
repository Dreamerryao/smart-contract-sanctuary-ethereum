pragma solidity ^0.8.9;

import "./RBAC.sol";
import "./BrickExchange.sol";
import "./IBrickToken.sol";
import "./IBrickNft.sol";
import "./IBrickStaking.sol";

contract BrickManager is RBAC, BrickExchange {

    struct nftInfo{
        uint256 id;
        uint8 _type;
        uint256 stakedAmount;
        string uri;
    }

    //not are actual prises
    uint256 constant BROKER_DEPOSIT = 1; //10000
    uint256 constant DEVELOPER_DEPOSIT = 9; //90000
    uint256 constant INVESTOR_DEPOSIT = 36; //360000

    uint256 constant BROKER_PRICE = 1; //1200
    uint256 constant DEVELOPER_PRICE = 10; //10800
    uint256 constant INVESTOR_PRICE = 43; //43200
    
    uint256 constant BROKER_POWER = 1;
    uint256 constant DEVELOPER_POWER = 4;
    uint256 constant INVESTOR_POWER = 36;

    address payable private treasury;
    IBrickToken private brickToken;
    IBrickNft private brickNft;
    IBrickStaking private brickStaking;

    mapping (uint256 => uint8) _nftType;
    mapping (address => uint8) _userType;
    mapping (address => uint256) _nftId; 
    mapping (uint8 => uint256) _typeLeft;

    event TokensAquired(address buyer, address withToken, uint256 tokenAmount, uint256 brickAmount);
    event NftAquired(address buyer, address withToken, uint256 tokenAmount, uint256 tokenId, uint8 nftType, uint256 addedDeposit);
    event NftUpgraded(address upgrader, address withToken, uint256 tokenAmount, uint256 tokenId, uint256 addedDeposit);

    event TokensDelivered(address buyer, uint256 brickAmount, uint256 chainId);
    event NftDelivered(address buyer, uint8 nftType, uint256 chainId, uint256 addedDeposit);
    event NftUpgradeDelivered(uint256 nftId, uint8 nftType, uint8 nftTypeTo, uint256 chainId, uint256 addedDeposit);

    constructor(address treasury_) {
        treasury = payable(treasury_);
        _typeLeft[1] = 10000;
        _typeLeft[2] = 2; //should be 1000, left "2" for testing proposes
        _typeLeft[3] = 300;
    }
    
    function setOracle(address token_, address oracle) public onlyAdmin {
        _setOracle(token_, oracle);
    }

    function removeOracle(address token_) public onlyAdmin {
        _removeOracle(token_);
    }

    function getNftType(uint256 id) public view returns (uint8) {
        return _nftType[id];
    }

    function getUserType(address user) public view returns (uint8) {
        return _userType[user];
    }

    function getNftsLeft(uint8 nftType) public view returns (uint256) {
        return _typeLeft[nftType];
    }

    function getNftInfo(address owner) public view returns (nftInfo memory){
        nftInfo memory nft;
        nft.id = _nftId[owner];
        nft._type = _userType[owner];
        nft.uri = brickNft.getUri(_nftId[owner]);
        nft.stakedAmount = brickStaking.getStakedAmount(owner);
        return nft;
    }

    function setBrick(address brickToken_, address brickNft_, address brickStaking_) external onlyAdmin {
        brickToken = IBrickToken(brickToken_);
        brickNft = IBrickNft(brickNft_);
        brickStaking = IBrickStaking(brickStaking_);
    }

    function setTreasury(address _treasury) public onlyAdmin {
        treasury = payable(_treasury);
    }

    function getNftPrice(uint8 nftType) public pure returns (uint256) {
        if (nftType == 1) {
            return BROKER_PRICE;
        } else if (nftType == 2) {
            return DEVELOPER_PRICE;
        } else if (nftType == 3) {
            return INVESTOR_PRICE;
        } else {
            revert("BrickManager: Nft type is not supported");
        }
    }

    function getNftDeposit(uint8 nftType) public pure returns (uint256) {
        if (nftType == 1) {
            return BROKER_DEPOSIT;
        } else if (nftType == 2) {
            return DEVELOPER_DEPOSIT;
        } else if (nftType == 3) {
            return INVESTOR_DEPOSIT;
        } else {
            revert("BrickManager: Nft type is not supported");
        }
    }

    function getVotingPower(uint8 nftType) public pure returns (uint256) {
        if (nftType == 1) {
            return BROKER_POWER;
        } else if (nftType == 2) {
            return DEVELOPER_POWER;
        } else if (nftType == 3) {
            return INVESTOR_POWER;
        } else {
            revert("BrickManager: Nft type is not supported");
        }
    }

    function calculateRequiredDeposit(address user, uint8 nftType) public view returns (uint256){
        uint256 fullDeposit = getNftDeposit(nftType);
        uint256 userBrickBalance = brickToken.balanceOf(user);
        uint256 requiredDeposit = userBrickBalance < fullDeposit ? fullDeposit - userBrickBalance : 0;

        return requiredDeposit;
    }

    function _mintNft(uint8 nftType, address reciever, string memory uri, uint256 addedDeposit) internal returns (uint256) {
        require(_typeLeft[nftType] > 0, "No Nft`s of this type left");
        uint256 tokenId = brickNft.mint(reciever, uri);
        _nftType[tokenId] = nftType;
        _userType[reciever] = nftType;
        _typeLeft[nftType]--;

        if(addedDeposit > 0)
            brickToken.mint(reciever, addedDeposit);

        return tokenId;
    }

    function _upgradeNft(uint256 nftId, uint8 typeTo, uint256 addedDeposit) internal {
        require(_typeLeft[typeTo] > 0, "No Nft`s of this type left");
        brickToken.mint(brickNft.ownerOf(nftId), addedDeposit);

        address owner = brickNft.ownerOf(nftId);
        uint8 currentType = _nftType[nftId];
        _typeLeft[currentType]++;
        _typeLeft[typeTo]--;
        _userType[owner] = typeTo;
        _nftType[nftId] = typeTo;
    }

    function buyToken(address tokenAddress, uint256 amount, uint256 minBrickAllowed) public onlyRegistered {        
        uint256 bricAmount = calculateBricAmount(tokenAddress, amount);
        require (bricAmount > 0, "Cant buy zero bricks");
        require(bricAmount >= minBrickAllowed, "Resulting BRIC amount is below the slippage level");
        IERC20Metadata token = IERC20Metadata(tokenAddress);
        token.transferFrom(msg.sender, treasury, amount);
        brickToken.mint(msg.sender, bricAmount);
        emit TokensAquired(msg.sender, tokenAddress, amount, bricAmount);
    }

    function buyTokenWithEther(uint256 minBrickAllowed) public payable onlyRegistered {
        uint256 value = msg.value;
        uint256 bricAmount = calculateBricAmount(address(0), value);
        require (bricAmount > 0, "Cant buy zero bricks");
        require(bricAmount >= minBrickAllowed, "Resulting BRIC amount is below the slippage level");
        treasury.transfer((value));
        brickToken.mint(msg.sender, bricAmount);
        emit TokensAquired(msg.sender, address(0), value, bricAmount);
    }

    function buyNft(address tokenAddress, uint8 nftType, string memory uri) public onlyRegistered {

        uint256 requiredDeposit = calculateRequiredDeposit(msg.sender, nftType);
        uint256 nftTotalPrice = getNftPrice(nftType) + requiredDeposit;

        (uint256 tokenAmount,) = calculateTokenAmount(tokenAddress, nftTotalPrice);
        IERC20Metadata token = IERC20Metadata(tokenAddress);
        token.transferFrom(msg.sender, treasury, tokenAmount);
        uint256 tokenId = _mintNft(nftType, msg.sender, uri, requiredDeposit);

        emit NftAquired(msg.sender, tokenAddress, tokenAmount, tokenId, nftType, requiredDeposit);
    }

    function buyNftWithEther(uint8 nftType, string memory uri) public payable onlyRegistered {

        uint256 requiredDeposit = calculateRequiredDeposit(msg.sender, nftType);
        uint256 nftTotalPrice = getNftPrice(nftType) + requiredDeposit;

        (uint256 etherAmount,) = calculateTokenAmount(address(0), nftTotalPrice);
        require(msg.value >= etherAmount, "BrickNft: Not enought Ether to buy this NFT");
        treasury.transfer((etherAmount));
        uint256 tokenId = _mintNft(nftType, msg.sender, uri, requiredDeposit);
        payable(msg.sender).transfer(msg.value - etherAmount);

        emit NftAquired(msg.sender, address(0), etherAmount, tokenId, nftType, requiredDeposit);
    }

    function upgradeNft(address tokenAddress, uint256 tokenId, uint8 typeTo) public onlyRegistered {
        require(typeTo > _nftType[tokenId], "Final Nft Type should be bigger than current");

        uint256 requiredDeposit = calculateRequiredDeposit(msg.sender, typeTo);
        uint256 upgradeTotalPrice = getNftPrice(typeTo) - getNftPrice(_nftType[tokenId]) + requiredDeposit;
        (uint256 tokenAmount,) = calculateTokenAmount(tokenAddress, upgradeTotalPrice);
        IERC20Metadata token = IERC20Metadata(tokenAddress);
        token.transferFrom(msg.sender, treasury, tokenAmount);
        _upgradeNft(tokenId, typeTo, requiredDeposit);
        emit NftUpgraded(msg.sender, tokenAddress, tokenAmount, tokenId, requiredDeposit);
    }

    function upgradeNftWithEther(uint256 tokenId, uint8 typeTo) public payable onlyRegistered {
        require(typeTo > _nftType[tokenId], "Final Nft Type should be bigger than current");

        uint256 requiredDeposit = calculateRequiredDeposit(msg.sender, typeTo);
        uint256 upgradeTotalPrice = (getNftPrice(typeTo) - getNftPrice(_nftType[tokenId])) + requiredDeposit;
        (uint256 etherAmount,) = calculateTokenAmount(address(0), upgradeTotalPrice);
        require(etherAmount <= msg.value, "BrickNft: Not enought Ether to upgrade this NFT");
        treasury.transfer((etherAmount));
        _upgradeNft(tokenId, typeTo, requiredDeposit);
        payable(msg.sender).transfer(msg.value - etherAmount);
        emit NftUpgraded(msg.sender, address(0), etherAmount, tokenId, requiredDeposit);
    }

    //delivering tokens bougth on other chains
    function confirmTokenBuy(address buyer, uint256 brickAmount, uint256 chainId) external onlyAdmin {
        brickToken.mint(buyer, brickAmount);
        emit TokensDelivered(buyer, brickAmount, chainId);
    }

    function confirmNftBuy(uint8 nftType, address buyer, string memory uri, uint256 chainId, uint256 addedDeposit) external onlyAdmin {
        require(addedDeposit + brickToken.balanceOf(buyer) >= getNftDeposit(nftType), "Deposit is too small for purchase of this NFT");
        _mintNft(nftType, buyer, uri, addedDeposit);
        emit NftDelivered(buyer, nftType, chainId, addedDeposit);
    }

    function confirmNftUpgrade(uint256 nftId, uint8 typeFrom, uint8 typeTo, uint256 chainId, uint256 addedDeposit) external onlyAdmin {
        address nftOwner = brickNft.ownerOf(nftId);
        require(addedDeposit + brickToken.balanceOf(nftOwner) >= getNftDeposit(typeTo), "Deposit is too small for upgrade this NFT");
        require(typeFrom == _nftType[nftId], "User paid for upgrading from another nft type");
        _upgradeNft(nftId, typeTo, addedDeposit);
        emit NftUpgradeDelivered(nftId, typeFrom, typeTo, chainId, addedDeposit);
    }
}

