contract BakeShop {
    uint256 pinoyTasty = 20;
    uint256 pandesal = 100;
    uint256 cheeseBread = 50;
    uint256 spanishBread = 50;
    uint256 mamon = 50;
    uint256 ensaymada = 30;
    uint256 pianonoRoll = 50;
    uint closeTime;
    uint openTime;
    bool bakeryAccess;

    mapping (address => bool) whiteListedAddresses; 
    mapping (address => bool) accessValid;

    address public owner;
    constructor() {
        owner = msg.sender;
        whiteListedAddresses[msg.sender] = true;
        console.log("The owner of this addres is : " , msg.sender);
    }

    modifier onlyOwner {
        if(msg.sender != owner) {
            revert NotOwner();
        }
        _;
    }
    modifier outOfStock {
        if(
            pinoyTasty == 0 || 
            pandesal == 0 || 
            cheeseBread == 0 ||
            spanishBread == 0 ||
            mamon == 0 ||
            ensaymada == 0 ||
            pianonoRoll == 0
            ) {
            revert OutOfStock(
                pinoyTasty, 
                pandesal,
                cheeseBread,
                spanishBread,
                mamon,
                ensaymada,
                pianonoRoll
                );
        }
        _;
    }
    modifier storeClose {
        if( block.timestamp < closeTime) {
            revert storeStillClosed(closeTime);
        }
        _;
    }
    modifier storeOpen {
        if( block.timestamp > openTime) {
            revert storeIsOpen(openTime);
        }
        _;
    }

    modifier GrantAccess {
        if(!whiteListedAddresses[msg.sender] ) {   
            revert accessError(bakeryAccess);
        }
        _;
    }

    function getOwner() public view returns(address) {
        return owner;
    }

    // Function to see available stocks
    function PinoyTasty() public view returns(uint256) {
        return pinoyTasty;
    }  
    function Pandesal() public view returns(uint256) {
        return pandesal;
    }
    function CheeseBread() public view returns(uint256) {
        return cheeseBread;
    }
    function SpanishBread() public view returns(uint256) {
        return spanishBread;
    } 
    function Mamon() public view returns(uint256) {
        return mamon;
    }
    function Ensaymada() public view returns(uint256) {
        return ensaymada;
    }
    function PianonoRoll() public view returns(uint256) {
        return pianonoRoll;
    }

    // Buy functions
    function buyPinoyTasty(uint256 _pinoyTasty) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _pinoyTasty * 5 gwei, "Insufficient payment.");
        require(pinoyTasty >= _pinoyTasty, "Available stocks are not enough.");
        pinoyTasty -= _pinoyTasty;
    }
    function buyPandesal(uint256 _pandesal) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _pandesal * 1 gwei, "Insufficient payment.");
        require(pandesal >= _pandesal, "Available stocks are not enough.");
        pandesal -= _pandesal;
    }
    function buyCheeseBread(uint256 _cheeseBread) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _cheeseBread * 2 gwei, "Insufficient payment.");
        require(cheeseBread >= _cheeseBread, "Available stocks are not enough.");
        cheeseBread -= _cheeseBread;
    }
    function buySpanishBread(uint256 _spanishBread) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _spanishBread * 2 gwei, "Insufficient payment.");
        require(spanishBread >= _spanishBread, "Available stocks are not enough.");
        spanishBread -= _spanishBread;
    }
    function buyMamon(uint256 _mamon) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _mamon * 2 gwei, "Insufficient payment.");
        require(mamon >= _mamon, "Available stocks are not enough.");
        mamon -= _mamon;
    }
    function buyEnsaymada(uint256 _ensaymada) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _ensaymada * 3 gwei, "Insufficient payment.");
        require(ensaymada >= _ensaymada, "Available stocks are not enough.");
        ensaymada -= _ensaymada;
    }
    function buyPianonoRoll(uint256 _pianonoRoll) public outOfStock storeClose storeOpen payable {
        require(msg.value >= _pianonoRoll * 2 gwei, "Insufficient payment.");
        require(pianonoRoll >= _pianonoRoll, "Available stocks are not enough.");
        pianonoRoll -= _pianonoRoll;
    }

    // Restock functions
    function restockPinoyTasty(uint256 quantity) public onlyOwner {
        if(pinoyTasty == 0 || pinoyTasty < 20) {
            pinoyTasty += quantity;
        }
        require(pinoyTasty <= 20, "Total quantity should be less than or equal to 20");
    }
    function restockPandesal(uint256 quantity) public onlyOwner {
        if(pandesal == 0 || pandesal < 100) {
            pandesal += quantity;
        }
        require(pandesal <= 100, "Total quantity should be less than or equal to 100");
    }
    function restockCheeseBread(uint256 quantity) public onlyOwner {
        if(cheeseBread == 0 || cheeseBread < 50) {
            cheeseBread += quantity;
        }
        require(cheeseBread <= 50, "Total quantity should be less than or equal to 50");
    }
    function restockSpanishBread(uint256 quantity) public onlyOwner {
        if(spanishBread == 0 || spanishBread < 50) {
            spanishBread += quantity;
        }
        require(spanishBread <= 50, "Total quantity should be less than or equal to 50");
    }
    function restockMamon(uint256 quantity) public onlyOwner {
        if(mamon == 0 || mamon < 50) {
            mamon += quantity;
        }
        require(mamon <= 50, "Total quantity should be less than or equal to 50");
    }
    function restockEnsaymada(uint256 quantity) public onlyOwner {
        if(ensaymada == 0 || ensaymada < 30) {
            ensaymada += quantity;
        }
        require(mamon <= 30, "Total quantity should be less than or equal to 30");
    }
    function restockPianonoRoll(uint256 quantity) public onlyOwner {
        if(pianonoRoll == 0 || pianonoRoll < 50) {
            pianonoRoll += quantity;
        }
        require(pianonoRoll <= 50, "Total quantity should be less than or equal to 50");
    }

    // Close store
    function closeStore(uint256 time) public onlyOwner {
        closeTime = block.timestamp + time;
    }

    function getCloseTimeLeft() public view returns(uint256) {
        require(closeTime >= block.timestamp, "Time is over! Store is now open.");
        return  closeTime - block.timestamp;
    }

    // Open store
    function openStore(uint256 time) public onlyOwner {
        openTime = block.timestamp + time;
    }
    function getOpenTimeLeft() public view returns(uint256) {
        require(openTime >= block.timestamp, "Time is over! Store is now close.");
        return openTime - block.timestamp;
    }

    // withdraw
    function withdraw() public onlyOwner {
        (bool callSuccess, ) = payable(msg.sender).call{value: address(this).balance}("");
        require(callSuccess, "Call Failed");
    }

    // whitelist
    function addUserToWhitelist (address _addressToWhitelist) public {   
        whiteListedAddresses[_addressToWhitelist] = true;
    }

    function verifyUserIfWhitelisted(address _address) public view returns (bool) {  
        bool IsUserWhitelisted = whiteListedAddresses[_address];
        return IsUserWhitelisted;
    }

    function AccessBakeryInAndOut() public GrantAccess { 
        if( accessValid[msg.sender] == true ) {
            accessValid[msg.sender] = false;
        } else accessValid[msg.sender] = true;
    }

    function isLoggedIn(address _address) public view returns (bool) {   
        return  accessValid[_address]; 
    }
}