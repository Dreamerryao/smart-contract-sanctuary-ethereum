pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777Recipient.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777.sol";
import "@openzeppelin/contracts/utils/introspection/IERC1820Registry.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";

// Errors
error InitialInvestorsVesting__TokensStillVesting();
error InitialInvestorsVesting__InsufficientBalance();
error InitialInvestorsVesting__AddressMustNotBeZero();
error InitialInvestorsVesting__AmountMustNotBeZero();
error InitialInvestorsVesting__VestingPeriodNotPassed();
error InitialInvestorsVesting__AllVestedTokensReleased();
error InitialInvestorsVesting__TransferFailed();
error InitialInvestorsVesting__InvalidToken();

contract InitialInvestorsVesting is
    Ownable,
    Pausable,
    AccessControl,
    IERC777Recipient,
    ReentrancyGuard
{
    // Variables
    uint256 public allocatedTokens = 0;
    IERC777 public token;
    uint256 public startVesting;
    //uint256[] public monthsInSec; //= convertMonthsToSec(monthsInMonth);

    uint256 public immutable start;

    // Events
    event Deposited(address indexed payee, uint256 weiAmount);
    event Withdrawn(address indexed payee, uint256 weiAmount);
    event NewWhitelistedAccountAdded(address indexed payee, uint256 weiAmount);
    event vestedTokensReleased(uint256 cumReleased, uint256 releaseTime);

    //Mapping
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryVestedTokens;
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryAmountAvailable;
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryAllocation;
    mapping(address => mapping(IERC777 => uint256))
        public beneficiaryCumReceived;
    mapping(address => mapping(IERC777 => uint256)) public balance;
    bytes public testStore;

    //Array
    address[] public earlyAdopters;
    // Vesting Schedule
    uint256[] public monthsInMonth = [18, 28, 32, 38, 44, 50, 56, 59, 60];
    uint256[] public releasePercentBy100 = [
        2000,
        3000,
        4503,
        6000,
        7000,
        8000,
        9000,
        9503,
        10000
    ];

    // keccak256('ERC777TokensRecipient')
    bytes32 private constant TOKENS_RECIPIENT_INTERFACE_HASH =
        0xb281fc8c12954d22544db45de3159a39272895b169a852b314f9cc762e44c53b;
    // Shared registry address
    IERC1820Registry private registry =
        IERC1820Registry(0x1820a4B7618BdE71Dce8cdc73aAB6C95905faD24);

    /**@dev Initialise the contract
     * @param _token: M2 ERC777 token address
     * @param  _start: time when to start counting vesting period
     */
    constructor(address _token, uint256 _start) {
        token = IERC777(_token);
        // set the interval and start time
        start = _start;
        startVesting = block.timestamp + start;
    }

    /////////////////////
    // Main Functions //
    /////////////////////

    /**@dev convert months to seconds
     */
    function convertMonthsToSec(uint256[] memory _months)
        internal
        view
        returns (uint256[] memory _monthsInSec)
    {
        for (uint256 i = 0; i < _months.length; i++) {
            _monthsInSec[i] =
                (monthsInMonth[i] * 30 * 24 * 60 * 60) +
                startVesting;
        }
        return _monthsInSec;
    }

    /**@dev sets the ERC777 interface
     */
    function setInterfaceImplementerInRegistry() external {
        // Registers itself as having the implementation of an ERC777TokensRecipient
        registry.setInterfaceImplementer(
            address(this),
            TOKENS_RECIPIENT_INTERFACE_HASH,
            address(this)
        );
    }

    /**@dev Receives ERC777 M2-Token
     * @param amount in wei
     */
    function tokensReceived(
        address, /*operator*/
        address, /*from*/
        address, /*to*/
        uint256 amount,
        bytes calldata userData,
        bytes calldata /*operatorData*/
    ) external override {
        if (msg.sender != address(token)) {
            revert InitialInvestorsVesting__InvalidToken();
        }
        // update balance of token for record keeping
        balance[address(this)][token] += amount;
        testStore = userData;
    }

    /**@dev Allows whitelisted addresses to withdraw all their
     * available tokens
     */
    function withdraw() public whenNotPaused nonReentrant {
        uint256 _amount = getBalanceAfterVesting(msg.sender) -
            beneficiaryCumReceived[msg.sender][token];

        if (_amount <= 0) {
            revert InitialInvestorsVesting__InsufficientBalance();
        }
        if (
            beneficiaryCumReceived[msg.sender][token] >=
            beneficiaryAllocation[msg.sender][token]
        ) {
            revert InitialInvestorsVesting__AllVestedTokensReleased();
        }

        beneficiaryCumReceived[msg.sender][token] += _amount;
        // beneficiaryVestedTokens[msg.sender][token] =
        //     beneficiaryAllocation[msg.sender][token] -
        //     getBalanceAfterVesting(msg.sender);
        token.send(payable(msg.sender), _amount, "");
        // Withdrawn event is emitted after every withdrawal
        emit Withdrawn(msg.sender, _amount);
    }

    /**@dev Allows only the contract owner to add whitelisted accounts
     * and their allocations
     * @param _whiteListedAddress: address of the new accounts
     * @param _totalAllocation: total token allocations in wei
     */
    function addwhiteListedAccounts(
        address _whiteListedAddress,
        uint256 _totalAllocation
    ) external onlyOwner whenNotPaused {
        //IERC777 _token = IERC777(_tokenAddress);
        if (_whiteListedAddress == address(0)) {
            revert InitialInvestorsVesting__AddressMustNotBeZero();
        }
        if (_totalAllocation <= 0) {
            revert InitialInvestorsVesting__AmountMustNotBeZero();
        }
        earlyAdopters.push(_whiteListedAddress);
        beneficiaryAllocation[_whiteListedAddress][token] =
            beneficiaryAllocation[_whiteListedAddress][token] +
            _totalAllocation;
        beneficiaryVestedTokens[_whiteListedAddress][
            token
        ] = beneficiaryAllocation[_whiteListedAddress][token];
        // NewWhitelistedAccountAdded event is emmitted after a new account is added
        emit NewWhitelistedAccountAdded(_whiteListedAddress, _totalAllocation);
    }

    /**@dev internal function to calculate beneficiary's tokens available for
     * withdrawal according to the vesting schedule
     * @param _account: address of the new accounts
     */
    function getBalanceAfterVesting(address _account)
        internal
        returns (uint256)
    {
        uint256[] memory monthsInSec = convertMonthsToSec(monthsInMonth);
        uint256[] memory releasePer = releasePercentBy100;
        if (block.timestamp < monthsInSec[0]) {
            return (beneficiaryAmountAvailable[_account][token] = 0);
        }
        if (
            block.timestamp >= monthsInSec[0] &&
            block.timestamp < monthsInSec[1]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[0]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[1] &&
            block.timestamp < monthsInSec[2]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[1]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[2] &&
            block.timestamp < monthsInSec[3]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[2]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[3] &&
            block.timestamp < monthsInSec[4]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[3]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[4] &&
            block.timestamp < monthsInSec[5]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[4]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[5] &&
            block.timestamp < monthsInSec[6]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[5]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[6] &&
            block.timestamp < monthsInSec[7]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[6]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
        if (
            block.timestamp >= monthsInSec[7] &&
            block.timestamp < monthsInSec[8]
        ) {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[7]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        } else {
            beneficiaryAmountAvailable[_account][token] =
                (beneficiaryAllocation[_account][token] * releasePer[8]) /
                10000;
            return beneficiaryAmountAvailable[_account][token];
        }
    }

    /**@dev Spend tokens (as operator) on behalf of _address
     * @param _tokenAddress ERC777 token address
     * @param _address address of the ERC777 tokens owner
     * @param _amount amount of tokens to be transferred from
     * _address to this contract(address(this))
     */
    function spendTokens(
        address _tokenAddress,
        address _address,
        uint256 _amount
    ) external {
        // Try taking 1 unit of a given token from given address
        IERC777(_tokenAddress).operatorSend(
            _address,
            address(this),
            _amount,
            "",
            ""
        );
    }

    /////////////////////
    // Getter Functions //
    /////////////////////

    /**@dev Getting added whitelisted accounts
     * @return array of whitelisted addresses
     */
    function getWhitelistedAddresses()
        external
        view
        returns (address[] memory)
    {
        return earlyAdopters;
    }

    /**@dev Returns the quantity of tokens of a whitlisted account available
     * for withdrawal
     * @param _account address of the whitelisted account
     * @return token balance in wei
     */
    function getAvailableTokens(address _account) external returns (uint256) {
        beneficiaryAmountAvailable[_account][token] =
            getBalanceAfterVesting(_account) -
            beneficiaryCumReceived[msg.sender][token];
        return beneficiaryAmountAvailable[_account][token];
    }

    /**@dev Returns the quantity of tokens of a whitlisted account still vesting
     * @param _account address of the whitelisted account
     * @return token balance in wei
     */
    function getVestedTokens(address _account) external returns (uint256) {
        beneficiaryVestedTokens[_account][token] =
            beneficiaryAllocation[_account][token] -
            getBalanceAfterVesting(_account);
        return beneficiaryVestedTokens[_account][token];
    }

    /**@dev Token balance of this contract
     * @return contract balance in wei
     */
    function getcontractBalance() external view returns (uint256) {
        return balance[address(this)][token];
    }
}

