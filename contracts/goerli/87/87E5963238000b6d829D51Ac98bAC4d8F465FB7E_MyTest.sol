contract MyTest {

    mapping(uint256 => string) public seeds;
    
    function generateSeed(uint256 tokenId) public returns (uint256) {
        uint256 r = random(tokenId);
        uint256 headSeed = 100 * (r % 7 + 10) + ((r >> 48) % 20 + 10);
        uint256 faceSeed = 100 * ((r >> 96) % 6 + 10) + ((r >> 96) % 20 + 10);
        uint256 bodySeed = 100 * ((r >> 144) % 7 + 10) + ((r >> 144) % 20 + 10);
        uint256 legsSeed = 100 * ((r >> 192) % 2 + 10) + ((r >> 192) % 20 + 10);

        seeds[tokenId] = string(abi.encodePacked(
            "r: ", Strings.toString(r), "-",
            "headSeed: ", Strings.toString(headSeed), "-",
            "faceSeed: ", Strings.toString(faceSeed), "-",
            "bodySeed: ", Strings.toString(bodySeed), "-",
            "legsSeed: ", Strings.toString(legsSeed), "-",
            "seed: ", Strings.toString(10000 * (10000 * (10000 * headSeed + faceSeed) + bodySeed) + legsSeed)
            ));

        return 10000 * (10000 * (10000 * headSeed + faceSeed) + bodySeed) + legsSeed;
    }

    function random(uint256 tokenId) private view returns (uint256 pseudoRandomness) {
        pseudoRandomness = uint256(
            keccak256(abi.encodePacked(blockhash(block.number - 1), tokenId))
        );

        return pseudoRandomness;
    }
}