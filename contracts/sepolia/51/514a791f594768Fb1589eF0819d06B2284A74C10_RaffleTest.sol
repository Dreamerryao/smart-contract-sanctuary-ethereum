    }


    function requestRandomWords() public returns (uint256) {
        return
            COORDINATOR.requestRandomWords(keyHash, subscriptionId, requestConfirmations, callbackGasLimit, numWords);
    }

    function getRandomNumber(
        uint256 _id,
        uint256 _entriesSize,
        uint256 _requestId
    ) internal {
        chainlinkRaffleInfo[_requestId] = RaffleInfo({
            id: _id,
            size: _entriesSize
        });
    }





    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        uint256 randomResult = _randomWords[0] % 2;

        RaffleInfo memory raffleInfo = chainlinkRaffleInfo[_requestId];
        uint256 normalizedRandomNumber = (randomResult % raffleInfo.size) + 1;
          RandomResult memory result = RandomResult({
            randomNumber: randomResult,
            nomalizedRandomNumber: normalizedRandomNumber
        });

        requests[raffleInfo.id] = result;

        emit RandomNumberCreated(
            raffleInfo.id,
            randomResult,
            normalizedRandomNumber
        );
        
        transferNFTAndFunds(raffleInfo.id, normalizedRandomNumber);
    }

    function createRaffle(
        uint256 _endTime,
        uint256 _desiredFundsInWeis,
        uint256 _maxEntriesPerUser,
        address[] memory _collateralAddress,
        uint256[] memory _collateralId,
        uint256[] memory _tokenAmount, // 0 -> erc721, != 0 -> ERC1155
        uint256 _minimumFundsInWeis,
        PriceStructure[] memory _prices,
      //  uint256 _commissionInBasicPoints,
        bool _requireCollection,
        address[] memory _collectionWhitelist
            ) external onlyRole(OPERATOR_ROLE) returns (uint256) {
        require(_maxEntriesPerUser > 0, "maxEntries is 0"); // max entries
        //collateral sc nft
        for (uint256 i = 0; i < _collateralAddress.length; i++) {
        require(_collateralAddress[i] != address(0), "NFT is null");
        }
        require(_endTime > getCurrentTime(), "End time can't be < as current time.");

        //transfer nfts from msg sender to sc
        safeMultipleTransfersFrom(
            msg.sender,
            address(this),
            _collateralAddress,
            _collateralId,
            _tokenAmount
        );



        RaffleStruct memory raffle = RaffleStruct({
            status: STATUS.CREATED,
            endTime: _endTime,
            maxEntries: _maxEntriesPerUser,
            collateralAddress: _collateralAddress,
            collateralId: _collateralId,
            tokenAmount: _tokenAmount,
            winner: address(0),
            randomNumber: 0,
            amountRaised: 0,
            creator: msg.sender,
            entriesLength: 0,
            cancellingDate: 0,
            requireCollection: _requireCollection,
            collectionWhitelist: _collectionWhitelist});

        raffles.push(raffle);

        //prices
        require(_prices.length > 0, "No prices");
        for (uint256 i = 0; i < _prices.length; i++) {
            require(_prices[i].numEntries > 0, "numEntries is 0");

            PriceStructure memory p = PriceStructure({
                id: _prices[i].id,
                numEntries: _prices[i].numEntries,
                price: _prices[i].price
            });

            prices[raffles.length - 1][i] = p;
        }

        uint256 idRaffle = raffles.length - 1;
        for (uint256 i = 0; i < _prices.length; i++) {
            require(_prices[i].numEntries > 0, "numEntries is 0");

            PriceStructure memory p = PriceStructure({
                id: _prices[i].id,
                numEntries: _prices[i].numEntries,
                price: _prices[i].price
            });

            prices[idRaffle][i] = p;
        }

        fundingList[idRaffle] = FundingStructure({
            minimumFundsInWeis: _minimumFundsInWeis,
            desiredFundsInWeis: _desiredFundsInWeis
        });

        emit RaffleCreated(
            idRaffle,
            _collateralAddress,
            _collateralId
        );

        return idRaffle;
    }



