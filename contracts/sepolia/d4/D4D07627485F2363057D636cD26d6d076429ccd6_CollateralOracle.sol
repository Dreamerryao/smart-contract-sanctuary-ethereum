pragma solidity ^0.8.17;

import "./CollateralOracleStorage.sol";
import "../interface/ICollateralOracle.sol";
import "../interface/IPriceOracle.sol";
import "../interface/IAddressMap.sol";
import "../library/AddressHandler.sol";
import "../library/Constant.sol";

import "hardhat/console.sol";

contract CollateralOracle is ICollateralOracle, CollateralOracleStorage, AddressHandler {
  constructor(address addressMap) AddressHandler(IAddressMap(addressMap)) {}

  function registerCollateral(address asset, uint32 multiplier) external onlyRole(ROLE_FACTORY) {
    require(_collateralInfos[asset].asset == address(0), "Collateral was already registered");
    require(multiplier >= 1000, "Multiplier must be above 1000");
    _storeCollateralInfo(CollateralAssetInfo(asset, multiplier, 0, false));
    emit RegisterCollateral(asset, multiplier);
  }

  function revokeCollateralAsset(address asset) external onlyRole(ROLE_FACTORY) {
    CollateralAssetInfo memory collateralAssetInfo = _collateralInfos[asset];
    if (collateralAssetInfo.asset != address(0)) {
      collateralAssetInfo.isRevoked = true;
      _collateralInfos[asset] = collateralAssetInfo;
      emit RevokeCollateralAsset(asset);
    }
  }

  function fixCollateralPrice(address asset, uint fixedPrice) external onlyRole(ROLE_FACTORY) {
    CollateralAssetInfo memory collateralAssetInfo = _collateralInfos[asset];
    require(collateralAssetInfo.asset != address(0), "Collateral not found");
    collateralAssetInfo.fixedPrice = fixedPrice;
    _collateralInfos[asset] = collateralAssetInfo;
    emit FixCollateralPrice(asset, fixedPrice);
  }

  function updateCollateralMultiplier(
    address asset,
    uint32 multiplier
  ) external onlyRole(ROLE_FACTORY) {
    CollateralAssetInfo memory collateralAssetInfo = _collateralInfos[asset];
    require(collateralAssetInfo.asset != address(0), "Collateral not found");
    require(multiplier >= 1000, "Multiplier must be above 1000");
    collateralAssetInfo.multiplier = multiplier;
    _collateralInfos[asset] = collateralAssetInfo;
    emit UpdateCollateralMultiplier(asset, multiplier);
  }

  function queryCollateralPrice(
    address asset,
    uint timeFrame
  ) external view returns (CollateralPriceInfo memory) {
    CollateralAssetInfo memory assetInfo = queryCollateralAssetInfo(asset);
    require(assetInfo.asset != address(0) && !assetInfo.isRevoked, "Collateral is not available");

    (uint price, uint lastUpdate) = assetInfo.fixedPrice == 0
      ? IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(asset, timeFrame)
      : (assetInfo.fixedPrice, MAX_INT);

    return
      CollateralPriceInfo({
        asset: assetInfo.asset,
        price: price,
        lastUpdate: lastUpdate,
        multiplier: assetInfo.multiplier,
        isRevoked: assetInfo.isRevoked
      });
  }

  function queryCollateralAssetInfo(
    address asset
  ) public view returns (CollateralAssetInfo memory) {
    return _collateralInfos[asset];
  }

  function queryCollateralAssetInfos() external view returns (CollateralAssetInfo[] memory) {
    return _readCollateralInfos();
  }
}

// SPDX-License-Identifier: AGPL-3.0
pragma solidity ^0.8.17;

import "../library/Struct.sol";

contract CollateralOracleStorage {
  address[] _assets;
  mapping(address => CollateralAssetInfo) _collateralInfos;

  function _storeCollateralInfo(CollateralAssetInfo memory collateralAssetInfo) internal {
    _assets.push(collateralAssetInfo.asset);
    _collateralInfos[collateralAssetInfo.asset] = collateralAssetInfo;
  }

  function _readCollateralInfos() internal view returns (CollateralAssetInfo[] memory) {
    CollateralAssetInfo[] memory infos = new CollateralAssetInfo[](_assets.length);
    for (uint i = 0; i < _assets.length; i++) {
      infos[i] = _collateralInfos[_assets[i]];
    }
    return infos;
  }
}

