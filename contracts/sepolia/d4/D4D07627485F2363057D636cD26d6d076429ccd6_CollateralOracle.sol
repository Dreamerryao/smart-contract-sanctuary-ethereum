pragma solidity ^0.8.17;

import "../interface/IAddressMap.sol";

contract AddressHandler {
  IAddressMap public _addressMap;

  constructor(IAddressMap addressMap) {
    _addressMap = addressMap;
  }

  modifier onlyRole(uint256 role) {
    require(msg.sender == lookup(role), "Only Role");
    _;
  }

  function lookup(uint256 role) internal view returns (address contractAddress) {
    contractAddress = _addressMap.readAddressList(role);
    require(contractAddress != address(0), "lookup error");
  }
}

