pragma solidity ^0.8.20; contract SOKOCOIN { mapping(address => uint) public balances; uint public totalSupply; constructor(uint _supply, address _initialHolder) { totalSupply = _supply; balances[_initialHolder] = 25000; } function transfer(address _to, uint _value) public returns (bool) { require(balances[msg.sender] >= _value); balances[msg.sender] -= _value; balances[_to] += _value; return true; } }