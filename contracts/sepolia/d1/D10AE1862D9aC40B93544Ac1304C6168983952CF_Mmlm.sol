import "@chainlink/v0.8/automation/AutomationCompatible.sol";

contract Mmlm is Context, IERC20, Ownable, AutomationCompatibleInterface {
    using SafeMath for uint256;
    using Address for address;

    struct BlockInfo {
        mapping(address => uint256) _rOwned;
        mapping(address => uint256) _tOwned;
        uint256 _rTotal;
        uint256 _tTotal;
        uint256 _tFee;
    }

    mapping(address => uint256) private _rOwned;
    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(uint256 => BlockInfo) blockInfos;
    mapping(address => uint256) userBlockIds;

    mapping(address => bool) private _isExcluded;
    address[] private _excluded;

    uint256 public latestBlockId = 0;
    uint256 private constant MAX = ~uint256(0);
    uint256 private constant _tTotal = 10 * 10 ** 6 * 10 ** 9; // 10000000000000000
    uint256 private _rTotal = (MAX - (MAX % _tTotal)); // 115792089237316195423570985008687907853269984665640564039457580000000000000000
    uint256 private _tFeeTotal;

    string private _name = "mmlm";
    string private _symbol = "MMLM";
    uint8 private _decimals = 9;

    constructor() public {
        userBlockIds[_msgSender()] = latestBlockId;
        blockInfos[latestBlockId]._rTotal = _rTotal;
        blockInfos[latestBlockId]._tTotal = _tTotal;
        blockInfos[latestBlockId]._rOwned[_msgSender()] = _rTotal;
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        uint256 blockNumber = userBlockIds[account];
        // if (_isExcluded[account]) return blockInfos[blockNumber]._tOwned[account];
        // console.log("blockInfos[blockNumber]._rOwned[account] = ", blockInfos[blockNumber]._rOwned[account]);
        return tokenFromReflection(blockInfos[blockNumber]._rOwned[account], blockNumber);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance")
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero")
        );
        return true;
    }

    function isExcluded(address account) public view returns (bool) {
        return _isExcluded[account];
    }

    function totalFees() public view returns (uint256) {
        return _tFeeTotal;
    }

    // function reflect(uint256 tAmount) public {
    //     address sender = _msgSender();
    //     require(!_isExcluded[sender], "Excluded addresses cannot call this function");
    //     (uint256 rAmount,,,,) = _getValues(tAmount);
    //     _rOwned[sender] = _rOwned[sender].sub(rAmount);
    //     _rTotal = _rTotal.sub(rAmount);
    //     _tFeeTotal = _tFeeTotal.add(tAmount);
    // }

    // function reflectionFromToken(uint256 tAmount, bool deductTransferFee) public view returns (uint256) {
    //     require(tAmount <= _tTotal, "Amount must be less than supply");
    //     if (!deductTransferFee) {
    //         (uint256 rAmount,,,,) = _getValues(tAmount);
    //         return rAmount;
    //     } else {
    //         (, uint256 rTransferAmount,,,) = _getValues(tAmount);
    //         return rTransferAmount;
    //     }
    // }

    function tokenFromReflection(uint256 rAmount, uint256 blockNumber) public view returns (uint256) {
        // require(rAmount < = _rTotal, "Amount must be less than total reflections");
        uint256 currentRate = _getRate(blockNumber);
        if (currentRate == 0) {
            return 0;
        }
        return rAmount.div(currentRate);
    }

    function excludeAccount(address account, uint256 blockNumber) external onlyOwner {
        require(!_isExcluded[account], "Account is already excluded");
        if (_rOwned[account] > 0) {
            _tOwned[account] = tokenFromReflection(_rOwned[account], blockNumber);
        }
        _isExcluded[account] = true;
        _excluded.push(account);
    }

    function includeAccount(address account) external onlyOwner {
        require(_isExcluded[account], "Account is already excluded");
        for (uint256 i = 0; i < _excluded.length; i++) {
            if (_excluded[i] == account) {
                _excluded[i] = _excluded[_excluded.length - 1];
                _tOwned[account] = 0;
                _isExcluded[account] = false;
                _excluded.pop();
                break;
            }
        }
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(address sender, address recipient, uint256 amount) private {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        _transferStandard(sender, recipient, amount);

        // if (_isExcluded[sender] && !_isExcluded[recipient]) {
        //     _transferFromExcluded(sender, recipient, amount);
        // } else if (!_isExcluded[sender] && _isExcluded[recipient]) {
        //     _transferToExcluded(sender, recipient, amount);
        // } else if (!_isExcluded[sender] && !_isExcluded[recipient]) {
        //     _transferStandard(sender, recipient, amount);
        // } else if (_isExcluded[sender] && _isExcluded[recipient]) {
        //     _transferBothExcluded(sender, recipient, amount);
        // } else {
        //     _transferStandard(sender, recipient, amount);
        // }
    }

    function _transferStandard(address sender, address recipient, uint256 tAmount) private {
        uint256 senderBlockId = userBlockIds[sender];
        uint256 recipientBlockId = userBlockIds[recipient];
        uint256 rAmount = tAmount.mul(_getRate(senderBlockId));
        uint256 tFee = tAmount / 100; // 1%
        uint256 tTransferAmount = tAmount - tFee;
        if (userBlockIds[recipient] == 0) {
            latestBlockId++;
            uint256 __tTotal = tTransferAmount;
            uint256 __rTotal = (MAX - (MAX % __tTotal));
            userBlockIds[recipient] = latestBlockId;
            blockInfos[latestBlockId]._tTotal = __tTotal;
            blockInfos[latestBlockId]._rTotal = __rTotal;
            blockInfos[latestBlockId]._rOwned[recipient] = __rTotal;
        } else {
            uint256 rTransferAmount = tTransferAmount.mul(_getRate(recipientBlockId));
            blockInfos[recipientBlockId]._rOwned[recipient] =
                blockInfos[recipientBlockId]._rOwned[recipient].add(rTransferAmount);
            blockInfos[recipientBlockId]._tTotal = blockInfos[recipientBlockId]._tTotal.add(tTransferAmount);
            blockInfos[recipientBlockId]._rTotal = blockInfos[recipientBlockId]._rTotal.add(rTransferAmount);
        }

        blockInfos[senderBlockId]._rOwned[sender] = blockInfos[senderBlockId]._rOwned[sender].sub(rAmount);
        blockInfos[senderBlockId]._rTotal = blockInfos[senderBlockId]._rTotal.sub(rAmount);
        blockInfos[senderBlockId]._tTotal = blockInfos[senderBlockId]._tTotal.sub(tAmount);

        blockInfos[senderBlockId]._tFee = blockInfos[senderBlockId]._tFee.add(tFee);
        emit Transfer(sender, recipient, tTransferAmount);
    }

    function _getRate(uint256 blockNumber) private view returns (uint256) {
        (uint256 rSupply, uint256 tSupply) = _getCurrentSupply(blockNumber);
        if (tSupply == 0) return 0;
        return rSupply.div(tSupply);
    }

    function _getCurrentSupply(uint256 blockNumber) private view returns (uint256, uint256) {
        uint256 rSupply = blockInfos[blockNumber]._rTotal;
        uint256 tSupply = blockInfos[blockNumber]._tTotal;
        // for (uint256 i = 0; i < _excluded.length; i++) {
        //     if (_rOwned[_excluded[i]] > rSupply || _tOwned[_excluded[i]] > tSupply) return (_rTotal, _tTotal);
        //     rSupply = rSupply.sub(_rOwned[_excluded[i]]);
        //     tSupply = tSupply.sub(_tOwned[_excluded[i]]);
        // }
        // if (rSupply < _rTotal.div(_tTotal)) return (_rTotal, _tTotal);
        return (rSupply, tSupply);
    }

    function _updateROwner() public {
        for (uint256 i = 0; i <= latestBlockId; i++) {
            if (blockInfos[i]._tFee > 0) {
                for (uint256 j = 0; j < i; j++) {
                    BlockInfo storage currentBlock = blockInfos[j];
                    if (currentBlock._tTotal > 0) {
                        currentBlock._rTotal = currentBlock._rTotal.sub(
                            blockInfos[i]._tFee.mul(currentBlock._tTotal).div(_tTotal).mul(_getRate(j))
                        );
                    }
                }
                blockInfos[i]._tFee = 0;
            }
        }
    }

    function getROwner(uint256 blockId, address account) public view returns (uint256) {
        return blockInfos[blockId]._rOwned[account];
    }

    function getTOwner(uint256 blockId, address account) public view returns (uint256) {
        return blockInfos[blockId]._tOwned[account];
    }

    function getBlockInfo(uint256 blockId) public view returns (uint256, uint256, uint256) {
        return (blockInfos[blockId]._rTotal, blockInfos[blockId]._tTotal, blockInfos[blockId]._tFee);
    }

    function getUserInfo(address account) public view returns (uint256) {
        return userBlockIds[account];
    }

    function checkUpkeep(bytes calldata checkData)
        external
        override
        returns (bool upkeepNeeded, bytes memory performData)
    {
        for (uint256 i = 0; i <= latestBlockId; i++) {
            if (blockInfos[i]._tFee > 0) {
                return (true, "0x");
            }
        }
    }

    function performUpkeep(bytes calldata performData) external override {
        _updateROwner();
    }
}

