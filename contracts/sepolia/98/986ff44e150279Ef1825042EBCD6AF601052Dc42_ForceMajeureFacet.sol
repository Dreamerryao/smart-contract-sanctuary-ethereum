// Author: @Linkchar
contract ForceMajeureFacet{

    function getForceMajeure(string memory _serviceId, uint _startDate, uint _endDate)public view returns (forceMajeureLib.ForceMajeure[] memory) {
        uint length = 0;
        uint position = 0;

        forceMajeureLib.ForceMajeure[] storage storageForceMajeure = forceMajeureLib.getStorage().forceMajeureMap[_serviceId];

        for(uint i = 0; i < storageForceMajeure.length; i++){
            if(
                storageForceMajeure[i].startDate >= _startDate && 
                storageForceMajeure[i].endDate <= _endDate || //In this case the maintenance is all inside the ranges

                storageForceMajeure[i].startDate <= _startDate && 
                storageForceMajeure[i].endDate >= _endDate || //In this the case the maintenance starts before but ends in the requested range
                
                storageForceMajeure[i].startDate >= _startDate && 
                storageForceMajeure[i].startDate <= _endDate || //In this the case the maintenance starts after the start of requested time 
                                                                //but ends before the requested range
                storageForceMajeure[i].endDate >= _startDate &&
                storageForceMajeure[i].endDate <= _endDate      //In this case the maintenance ends after the start frequested time
                                                                // but ends before the requested end time
                ){
                length++;
            }
        }

        forceMajeureLib.ForceMajeure[] memory returnedForceMajeure = new forceMajeureLib.ForceMajeure[](length);
        for(uint i = 0; i < storageForceMajeure.length; i++){
            if(
                storageForceMajeure[i].startDate >= _startDate && 
                storageForceMajeure[i].endDate <= _endDate || //In this case the maintenance is all inside the ranges

                storageForceMajeure[i].startDate <= _startDate && 
                storageForceMajeure[i].endDate >= _endDate || //In this the case the maintenance starts before but ends in the requested range
                
                storageForceMajeure[i].startDate >= _startDate && 
                storageForceMajeure[i].startDate <= _endDate|| //In this the case the maintenance starts after the start of requested time 
                                                                //but ends before the requested range
                storageForceMajeure[i].endDate >= _startDate &&
                storageForceMajeure[i].endDate <= _endDate      //In this case the maintenance ends after the start frequested time
                                                                // but ends before the requested end time
                ){
                returnedForceMajeure[position]=storageForceMajeure[i];
                position ++;
                // console.log(
                //     "Im returnig the force majeure in the position %s with the id %s",
                //     i,
                //     storageForceMajeure[i].forceMajeureId
                // );

                // console.log("and the start date %s and the end date %s", 
                //     storageForceMajeure[i].endDate, 
                //     storageForceMajeure[i].startDate
                // );
            }
        }
        return returnedForceMajeure;
    }

    function getForceMajeureTimesIDs(string memory _serviceId) external view returns (string[] memory){
        return forceMajeureLib.getForceMajeureIDs(_serviceId);
    }

    function createForceMajeureTime(
        string[] memory _serviceIds, 
        string memory _forceMajeureId,
        uint[] memory _startDates, 
        uint[] memory _endDates)
        public {

        require(
            _serviceIds.length == _startDates.length &&
            _serviceIds.length == _endDates.length);

        for(uint recordNumber = 0; recordNumber < _serviceIds.length; recordNumber++){

            forceMajeureLib.ForceMajeure memory newForceMajeure = forceMajeureLib.ForceMajeure({
                                                            forceMajeureId:_forceMajeureId,
                                                            startDate:_startDates[recordNumber], 
                                                            endDate:_endDates[recordNumber]
                                                            });

            forceMajeureLib.addForceMajeure(_serviceIds[recordNumber], newForceMajeure);
        }
    }

    function updateForceMajeureTime(
        string[] memory _serviceIds, 
        string memory _forceMajeureId,
        uint _startDate, 
        uint _endDate)
        public{

        for(uint i = 0; i < _serviceIds.length; i++){
            string memory actualService =  _serviceIds[i];
            forceMajeureLib.ForceMajeure[] storage storageForceMajeure = forceMajeureLib.getStorage().forceMajeureMap[actualService];

            for(uint j = 0; j < storageForceMajeure.length; j++){
                if(keccak256(abi.encodePacked(storageForceMajeure[j].forceMajeureId)) == keccak256(abi.encodePacked(_forceMajeureId))){
                    forceMajeureLib.updateForceMajeureTime(actualService, j, _startDate, _endDate);
                    break;
                }
            }   
        }
    }

    function deleteForceMajeureTime(
        string[] memory _serviceIds,
        string memory _forceMajeureId)
        public{

        for(uint i = 0; i < _serviceIds.length; i++){
            string memory actualService =  _serviceIds[i];

            forceMajeureLib.ForceMajeure[] storage storageForceMajeure = forceMajeureLib.getStorage().forceMajeureMap[actualService];
        
            for(uint j = 0; j < storageForceMajeure.length; j++){
                if(keccak256(abi.encodePacked(storageForceMajeure[j].forceMajeureId)) == keccak256(abi.encodePacked(_forceMajeureId))){
                    forceMajeureLib.deleteForceMajeureTime(actualService, j);
                    break;
                }
            }
        }   
    }
}

