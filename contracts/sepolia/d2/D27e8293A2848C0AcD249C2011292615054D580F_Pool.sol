pragma solidity ^0.8.19;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IPoolManager.sol";
import "./interfaces/IPool.sol";
import "./interfaces/ILoan.sol";
import "./interfaces/IOracle.sol";
import "./interfaces/IStaking.sol";
import "./interfaces/IKonaGlobals.sol";
import "./interfaces/IOracleManager.sol";
import "./KonaMath.sol";
import "./KonaToken.sol";
import "./ArrayUtils.sol";
import "hardhat/console.sol";

contract Pool is Ownable, ERC20, IPool {
    using ArrayUtils for address[];

    address public override konaGlobals;

    uint256 constant CYCLE_DURATION = 90;

    address[] private oracles;
    mapping(address=>bool) public loans;

    IERC20 public brzToken;
    KonaToken public konaToken;
    address private konaMath;

    struct Request {
        bool isDeposit;
        uint total;
        bool processed;
        uint date;
    }

    struct CapitalAssigned {
        bool confirmed;
        uint256 totalOriginal;
        uint256 totalAccepted;
        uint256 fundsAvailableToSpend;
    }

    //Cycle x Oracle x Stand By Capital
    mapping(uint256=>mapping(address=>uint256)) public standByCapitals;

    //Cycle x Oracle Address x Capital Assigned
    mapping(uint256=>mapping(address=>CapitalAssigned)) public capitalsAssigned;

    //Cycle x Oracle Address x Cost Of Capital
    mapping(uint256=>mapping(address=>uint256)) public costsOfCapital;

    //Cycle x Oracle Address x Default Percentage for Voting
    mapping(uint256=>mapping(address=>uint256)) public defaultVotings;

    //Cycle x Oracle Address x Total Votes
    mapping(uint256=>mapping(address=>uint256)) public totalVotesToOracles;

    //Cycle x Oracle Address x List of Voters
    mapping(uint256=>mapping(address=>Vote[])) public votesToOracles;

    //Cycle x Staker Address x Total Votes
    mapping(uint256=>mapping(address=>uint256)) internal votesByStakers;

    //Cycle x Total Votes
    mapping(uint256=>uint256) internal cycleVotes;

    //LPs
    uint256 public totalCapital;
    mapping(address => Request[]) public requests;
    mapping(address => uint) public lastProcessedRequestIndex; 

    modifier onlyOracle() {
        require(oracles.search(msg.sender), "Only Oracles can call this");
        _;
    }

    modifier onlyPoolManager() {
        address poolManager = IKonaGlobals(konaGlobals).poolManager();
        require(msg.sender == poolManager, "Only the Pool Manager can call this");
        _;
    }

    constructor(address _konaGlobals, IERC20 _brzToken, KonaToken _konaToken) ERC20("VaultToken", "VT") {
        require((konaGlobals = _konaGlobals) != address(0), "P:ZERO_ADDRESS");

        brzToken = _brzToken;
        konaToken = _konaToken;
    }

    function decimals() public view virtual override returns (uint8) {
        return 4;
    }

    function changeOracleStatus(address _oracle, bool _status) public onlyOwner {
        require(!oracles.search(_oracle) || !_status, "Can set the same status");
        
        if (_status) {
            address oracleManager = IKonaGlobals(konaGlobals).oracleManager();
            bool isOracleEnabled = IOracleManager(oracleManager).isOracleEnabled(_oracle);
            require(isOracleEnabled, "Oracle not enabled by Manager");

            oracles.add(_oracle);
        } else {
            oracles.remove(_oracle);
        }

        emit OracleStatusChanged(_oracle, _status);
    }

    function changeLoanStatus(address _loan, bool _status) public onlyOwner {
        require(loans[_loan] != _status, "Cannot set the same status"); 

        if (_status) { 
            uint256 maturity = ILoan(_loan).getMaturity();
            address poolManager = IKonaGlobals(konaGlobals).poolManager();
            bool isInsideCurrentCycle = IPoolManager(poolManager).isDateInsideCurrentCycle(maturity);
            require(isInsideCurrentCycle, "Maturity outside current cycle");
        } else {
            require(!ILoan(_loan).getIsFunded(), "Loan already funded");
        }

        loans[_loan] = _status;

        emit LoanStatusChanged(_loan, _status);
    }

    function isDateInsideCurrentCycle(uint256 _date) public view returns(bool) {
        address poolManager = IKonaGlobals(konaGlobals).poolManager();
        return IPoolManager(poolManager).isDateInsideCurrentCycle(_date);
    }

    function getCurrentCycleNumber() public view returns(uint256) {
        address poolManager = IKonaGlobals(konaGlobals).poolManager();
        (uint256 currentCycle, , ) = IPoolManager(poolManager).getCycleStatus();
        return currentCycle;
    }

    function isJoinOrLeavePeriod() public view returns(bool) {
        address poolManager = IKonaGlobals(konaGlobals).poolManager();
        (uint256 currentCycle, , IPoolManager.Status status) = IPoolManager(poolManager).getCycleStatus();
        return status == IPoolManager.Status.JOIN_LEAVE;
    }

    //Admin
    function requestAddOracle(address _oracle) public {
        //TODO
    }

    function requestRemoveOracle(address _oracle) public {
        //TODO
    }

    function processOraclesRequests() public {
        //TODO
    }

    function addFundsAfterDistribution(uint256 _cycleNumber, address _oracle, uint256 _total) external {
        //TODO: Add back this check before going to Mainnet
        //require(msg.sender == IKonaGlobals(konaGlobals).poolManager(), "Only callable by the Pool Manager");

        totalCapital += _total;

        emit FundsAddedAfterDistribution(_cycleNumber, _oracle, _total);
    }

    function processLPsRequests(address user) external onlyOwner {
        require(isJoinOrLeavePeriod(), "Not available yet");

        Request[] storage userRequests = requests[user];
        uint start = lastProcessedRequestIndex[user];

        for(uint i = start; i < userRequests.length; i++) {
            Request storage request = userRequests[i];
            if(request.processed == false) {
                if(request.isDeposit) {
                    uint shares = getShares(request.total);
                    _mint(user, shares); 
                    totalCapital += request.total;
                } else {
                    uint sharesToBurn = request.total;
                    require(balanceOf(user) >= sharesToBurn, "Insufficient balance");
                    _burn(user, sharesToBurn); 
                    uint brzToTransfer = getBRZForShares(sharesToBurn);
                    brzToken.transfer(user, brzToTransfer);
                    totalCapital -= brzToTransfer;
                }
                request.processed = true;
                lastProcessedRequestIndex[user] = i;
            }
        }
    }

    function calculateCostsOfCapital(address _oracle) public onlyOwner {
        require(oracles.search(_oracle), "Not an oracle");

        uint currentCycle = getCurrentCycleNumber();

        //TODO: Require that the stand by cand capital assigned are already set
        //Once the cycle started, we can't set it again

        (uint256 referenceRate, uint256 s, uint256 c) = getCostOfCapitalParams(_oracle, currentCycle);
        uint256 calculatedCost = KonaMath.calculateCostOfCapital(referenceRate, s, c, CYCLE_DURATION);

        costsOfCapital[currentCycle][_oracle] = calculatedCost;

        emit OracleCostOfCapitalSet(_oracle, currentCycle, calculatedCost);
    }

    //TODO: Make internal
    function getCostOfCapitalParams(address _oracle, uint256 _cycle) public view returns(uint256, uint256, uint256) {
        CapitalAssigned memory capitalAssigned = capitalsAssigned[_cycle][_oracle];
        uint256 totalAssigned = capitalAssigned.confirmed? capitalAssigned.totalAccepted : capitalAssigned.totalOriginal;
        uint256 referenceRate = IKonaGlobals(konaGlobals).referenceRate();
        address staking = IKonaGlobals(konaGlobals).staking();
        uint256 totalStaked = IStaking(staking).getTotalStaked();
        uint256 totalTokens = konaToken.totalSupply();
        uint256 s = totalStaked * 100 / totalTokens;
        uint256 c = standByCapitals[_cycle][_oracle] * 100 / totalAssigned;
        return (referenceRate, s, c);
    }

    function assignCapitalAfterVoting(address _oracle) public onlyOwner {
        require(oracles.search(_oracle), "Not an oracle");

        //TODO: Require not assigned before

        uint currentCycle = getCurrentCycleNumber();

        uint256 weight = 100 * totalVotesToOracles[currentCycle][_oracle] / cycleVotes[currentCycle];
        uint256 totalOriginal = totalCapital * weight / 100;
        
        capitalsAssigned[currentCycle][_oracle].totalOriginal = totalOriginal;

        emit OracleCapitalAssignedSet(_oracle, currentCycle, totalOriginal);
    }

    //ORACLES

    function commitStandByCapital(uint256 _total) external onlyOracle {
        uint currentCycle = getCurrentCycleNumber();

        standByCapitals[currentCycle][msg.sender] += _total;

        emit OracleStandByCapitalCommitted(msg.sender, currentCycle, _total);
    }

    function acceptCapitalAssigned(address _oracle, uint256 _totalAccepted) external {
        uint currentCycle = getCurrentCycleNumber();

        require(_totalAccepted <= capitalsAssigned[currentCycle][_oracle].totalOriginal, "You cannot accept more than what you were assigned");

        capitalsAssigned[currentCycle][_oracle].confirmed = true;
        capitalsAssigned[currentCycle][_oracle].totalAccepted = _totalAccepted;
        capitalsAssigned[currentCycle][_oracle].fundsAvailableToSpend = _totalAccepted;

        totalCapital -= _totalAccepted;

        emit OracleCapitalAssignedAccepted(_oracle, currentCycle, _totalAccepted);
    }

    function requestRemove() external onlyOracle {
        return requestRemoveOracle(msg.sender);
    }

    function requestFunding(address _loan) external onlyOracle {
        require(loans[_loan], "Loan not approved"); 
        require(!ILoan(_loan).getIsFunded(), "Loan already funded");
        
        uint currentCycle = getCurrentCycleNumber();
        uint256 loanAmount = ILoan(_loan).getAmount();
        uint256 fundsAvailableToSpend = capitalsAssigned[currentCycle][msg.sender].fundsAvailableToSpend; 
        
        require(fundsAvailableToSpend >= loanAmount, "Insufficent funds");
        capitalsAssigned[currentCycle][msg.sender].fundsAvailableToSpend -= loanAmount; 

        ILoan(_loan).setIsFunded();

        address rampAddress = ILoan(_loan).getRampAddress();
        brzToken.transfer(rampAddress, loanAmount);

        emit LoanFunded(currentCycle, msg.sender, _loan, loanAmount);
    }

    function repay(address _loan, uint256 _total) external onlyOracle {
        brzToken.transferFrom(_loan, address(this), _total);

        uint currentCycle = getCurrentCycleNumber();

        capitalsAssigned[currentCycle][msg.sender].fundsAvailableToSpend += _total; 

        emit LoanRepaid(_loan, msg.sender, currentCycle, _total);
    }

    function spend_TESTING_ONLY(uint256 _total, address _oracle) public {
        uint currentCycle = getCurrentCycleNumber();
        
        //console.log(
        //    "fundsAvailableToSpend %s BRZ. - I want to substract %s",
        //    capitalsAssigned[currentCycle][_oracle].fundsAvailableToSpend,
        //    _total
        //);

        capitalsAssigned[currentCycle][_oracle].fundsAvailableToSpend -= _total; 
        brzToken.transfer(msg.sender, _total);
    }

    function repay_TESTING_ONLY(uint256 _total, address _oracle) public {
        brzToken.transferFrom(msg.sender, address(this), _total);
        uint currentCycle = getCurrentCycleNumber();
        capitalsAssigned[currentCycle][_oracle].fundsAvailableToSpend += _total; 
    }

    //STAKERS

    function setDefaultVoting(address _oracle, uint256 _percentage) public {
        //TODO
        defaultVotings[getCurrentCycleNumber()][_oracle] = _percentage;
    }

    function vote(address _oracle, uint256 _totalVotes) public {
        address staking = IKonaGlobals(konaGlobals).staking();
        uint256 totalVotes = IStaking(staking).getTotalStakedByUser(msg.sender);

        uint256 currentCycle = getCurrentCycleNumber();

        uint256 remainingVotes = totalVotes - votesByStakers[currentCycle][msg.sender];

        require(_totalVotes <= remainingVotes, "Not enough votes");

        addOrCreateVote(currentCycle, _oracle, msg.sender, _totalVotes);
        totalVotesToOracles[currentCycle][_oracle] += _totalVotes;
        votesByStakers[currentCycle][msg.sender] += _totalVotes;
        cycleVotes[currentCycle] += _totalVotes;

        emit StakerVoted(msg.sender, _oracle, currentCycle, _totalVotes);
    }

    function addOrCreateVote(uint256 _cycle, address _oracle, address _staker, uint256 _totalVotes) internal {
        for(uint i=0; i<votesToOracles[_cycle][_oracle].length;i++) {
            if (votesToOracles[_cycle][_oracle][i].staker == _staker) {
                votesToOracles[_cycle][_oracle][i].totalVotes += _totalVotes;
                return;
            }
        }

        votesToOracles[_cycle][_oracle].push(Vote(_staker, _totalVotes));
    }

    //LPS
    function requestDeposit(uint256 _total) external {
        brzToken.transferFrom(msg.sender, address(this), _total);
        Request memory newRequest = Request({
            isDeposit: true,
            total: _total,
            processed: false,
            date: block.timestamp
        });
        requests[msg.sender].push(newRequest);

        emit NewDepositRequest(msg.sender, _total);
    }

    function requestWithdraw(uint256 _total) external {
        require(balanceOf(msg.sender) >= _total, "Insufficient balance");
        Request memory newRequest = Request({
            isDeposit: false,
            total: _total,
            processed: false,
            date: block.timestamp
        });
        requests[msg.sender].push(newRequest);

        emit NewWithdrawRequest(msg.sender, _total);
    }

    function requestCreateRewardDeposit(address _beneficiary, uint256 _total) external onlyPoolManager {
        Request memory newRequest = Request({
            isDeposit: true,
            total: _total,
            processed: false,
            date: block.timestamp
        });
        requests[_beneficiary].push(newRequest);

        emit NewRewardDepositRequest(msg.sender, _total);
    }

    //GET

    function getShares(uint _brzAmount) public view returns(uint) {
        if (totalSupply() == 0) return _brzAmount; 
        return _brzAmount * totalSupply() / totalCapital;
    }

    function getBRZForShares(uint shares) public view returns(uint) {
        if (totalSupply() == 0) return 0; 
        return shares * totalCapital / totalSupply();
    }

    function getBRZForMyShares() public view returns(uint) {
        uint256 shares = balanceOf(msg.sender);
        return shares * totalCapital / totalSupply();
    }

    function getFundsAvailableToSpend(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return capitalsAssigned[_cycleNumber][_oracle].fundsAvailableToSpend;
    }

    function getCommittedStandByCapital(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return standByCapitals[_cycleNumber][_oracle];
    }

    function getOriginalCapitalAssigned(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return capitalsAssigned[_cycleNumber][_oracle].totalOriginal;
    }

    function getAcceptedCapitalAssigned(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return capitalsAssigned[_cycleNumber][_oracle].totalAccepted;
    }

    function getCostOfCapital(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return costsOfCapital[_cycleNumber][_oracle];
    }

    function getTotalVotesToOracles(address _oracle, uint256 _cycleNumber) external view returns(uint256) {
        return totalVotesToOracles[_cycleNumber][_oracle];
    }

    function getVotesToOracles(address _oracle, uint256 _cycleNumber) external view returns(Vote[] memory) {
        return votesToOracles[_cycleNumber][_oracle];
    }

    function getCycleVotes(uint256 _cycleNumber) external view returns(uint256) {
        return cycleVotes[_cycleNumber];
    }

    function getOracles() external view returns(address[] memory) {
        return oracles;
    }

    function hasOracles() external view returns(bool) {
        return oracles.length > 0;
    }

    //TODO
    //Function to reallocate funds between oracles
}

