library KonaMath {
    function max(uint256 a, uint256 b) internal pure returns (uint256) {
        return a >= b ? a : b;
    }

    //The result is a percentage and the two last digits are decimals
    function calculateCostOfCapital(uint256 _referenceRate, uint256 _s, uint256 _c, uint256 _cycleDuration) public pure returns(uint256) {
        return (_referenceRate * 100 + max(100 - _s - _c, 0) ** 2 / 2) * _cycleDuration / 360;
	}

    //TODO: Change to pure after removing the console log
    function calculateExcessForStakers(uint256 _alpha, uint256 _s) public view returns(uint256) {
        console.log("_alpha %s", _alpha);
        uint x = (100 - _alpha) * 10 ** 16;
        uint256 e = exp(x);
        console.log("e %s", e);
        e = e / 1e27 + 1e9;
        console.log("e %s", e);
        uint256 excess = _alpha ** 2 * _s * e / 2 / 1e9;
        console.log("excess %s", excess);
        return excess;
    }

    uint256 private constant SCALE = 1e18;

    function exp(uint256 x) internal pure returns (uint256) {
        uint256 xi = x;
        uint256 res = SCALE;
        uint256 i = 1;
        while (xi > 0) {
            res = res + (xi * SCALE) / factorial(i);
            xi = (xi * x) / SCALE;
            i++;
        }
        return res;
    }

    function factorial(uint256 n) private pure returns (uint256) {
        if (n == 0) {
            return 1;
        } else {
            return n * factorial(n - 1);
        }
    }

    //TODO: Change to pure after removing the console log
    function calculateSlashRate(uint256 _alpha) public view returns(uint256) {
        if (_alpha == 100 || _alpha == 0) {
            return 0;
        }

        return 9797; //DUMMY VALUE FOR NOW

        console.log("_alpha %s", _alpha);

        uint256 tan = uint256(tanh((100 - _alpha), _alpha));
        console.log("tan %s", tan);

        uint256 slashRate = _alpha / (100 - _alpha) * tan;
        console.log("slashRate %s", slashRate);

        return  slashRate;
    }

    uint256 constant k = 19;
    uint256 constant c = 147221949; //0.147221949
    uint256 constant M = 1e27; //KonaToken total supply

    //TODO: Change to pure after removing the console log
    function calculateInflation(uint256 _winningOraclesWeight, uint256 _s, uint256 _cycleNumber, uint256 _currentSupply) public view returns(uint256) {
        uint256 t = _winningOraclesWeight * _s * _cycleNumber;
        uint256 e = reciprocal(expApproximation(c * t / 1e14));
        uint256 div = 1 + k * e;
        uint256 totalNewSupply = M * 1e18 / div;

        console.log("t %s", t);
        console.log("e %s", e);
        console.log("div %s", div);
        console.log("totalNewSupply %s", totalNewSupply);

        return totalNewSupply - _currentSupply;
    }

    function reciprocal(uint256 _x) internal pure returns(uint256) {
        return 1e36 / _x; // Adjusted to maintain decimal precision
    }

    function expApproximation(uint256 _x) internal pure returns(uint256) {
        return 1e18 + (_x * 1e18) + (_x**2 * 1e18) / 2 + (_x**3 * 1e18) / 6 + (_x**4 * 1e18) / 24; // Adjusted to maintain decimal precision
    }

    int256 private constant FIXED_POINT_DECIMALS = 1e18;

    function exp(int256 x) internal pure returns (int256) {
        int256 term = FIXED_POINT_DECIMALS;
        int256 result = term;
        int256 i = 1;

        while (true) {
            term = (term * x) / (i * FIXED_POINT_DECIMALS);

            if (term == 0) {
                break;
            }

            result += term;
            i++;
        }

        return result;
    }

    function tanh(uint256 numerator, uint256 denominator) public pure returns (int256) {
        int256 scaledNumerator = int256(numerator) * FIXED_POINT_DECIMALS;
        int256 scaledDenominator = int256(denominator);

        int256 x = (scaledNumerator * FIXED_POINT_DECIMALS) / scaledDenominator;
        int256 exp2x = exp(2 * x);
        int256 numeratorResult = exp2x - FIXED_POINT_DECIMALS;
        int256 denominatorResult = exp2x + FIXED_POINT_DECIMALS;

        return (numeratorResult * FIXED_POINT_DECIMALS) / denominatorResult;
    }
}

