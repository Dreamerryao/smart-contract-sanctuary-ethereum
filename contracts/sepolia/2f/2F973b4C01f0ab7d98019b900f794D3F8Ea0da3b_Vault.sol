pragma solidity ^0.8.4;

// Import necessary contracts and libraries
import 'hardhat/console.sol';
import './interfaces/IERC20.sol';
import './interfaces/IATON.sol';
import './libraries/AStructs.sol';
import '@openzeppelin/contracts/access/AccessControl.sol';

contract Vault is AccessControl {
    // Define constants and variables
    uint256 public Premium = 10000000; // The premium percentage (1%)
    uint256 constant pct_denom = 1000000000; // Denominator for percentage calculations
    bytes32 public constant MANAGER_ROLE = keccak256('MANAGER_ROLE');

    // Addresses for USDT and ATON tokens
    IERC20 USDT;
    IATON ATON;
    address owner;

    // Constructor of the contract, initializing the USDT and ATON token references
    constructor(address _USDT, address _ATON) {
        USDT = IERC20(_USDT); // Initialize the USDT token contract reference
        ATON = IATON(_ATON); // Initialize the ATON token contract reference
        owner = msg.sender;
        //TODO Remove on production release
        // Grant DEFAULT_ADMIN_ROLE to the contract deployer
        _grantRole(DEFAULT_ADMIN_ROLE, owner);
        _mockReferalGroup(); // Initialize a mock referral group for testing purposes; should be removed in production
    }

    // Event emitted when a new stake is added to a Event
    event StakeAdded(
        string indexed eventId, // Event ID for which the stake is added
        address indexed player, // Address of the player who added the stake
        uint256 amountUSDT, // Amount of USDT staked
        uint256 amountATON, // Amount of ATON staked
        uint8 team // Team on which the stake is placed (0 for Team A, 1 for Team B)
    );

    // Event emitted when a new Event is added
    event EventAdded(
        string indexed eventId, // Unique identifier for the Event
        uint8 sport, // Sport type (represented as an integer)
        uint64 startTime, // Start time of the Event in UNIX timestamp
        address indexed creator // Address of the creator (user) who added the Event
    );

    // Mappings and counters to store Event and player related information
    mapping(bytes8 => AStructs.Event) public events;
    uint256 public eventCount;

    mapping(address => AStructs.Player) players;
    uint256 playerCount;

    mapping(uint256 => AStructs.Associate[]) referalGroups;

    // Dynamic array to store active Event IDs and the corresponding counter
    bytes8[] public activeEvents;
    event ATONAddressSet(address indexed newATONAddress);

    // Function to add an authorized address with the MANAGER_ROLE
    function addAuthorizedAddress(address authorizedAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(MANAGER_ROLE, authorizedAddress);
    }

    // Function to remove an authorized address with the MANAGER_ROLE
    function removeAuthorizedAddress(address authorizedAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        revokeRole(MANAGER_ROLE, authorizedAddress);
    }

    // Function to retrieve a specified amount of ATON tokens from a player's account
    function retrieveUSDT(address _player, uint256 _amountUSDT) external onlyRole(MANAGER_ROLE) {
        // Ensure the player has enough USDT tokens in their balance
        require(USDT.balanceOf(_player) >= _amountUSDT, 'Player has Not enough USDT balance');

        // Ensure the contract has approval to transfer the specified amount of USDT tokens from the player's account
        require(USDT.allowance(_player, address(this)) >= _amountUSDT, 'Need to approve USDT');

        // Transfer the specified amount of USDT tokens from the player's account to the contract's account
        require(USDT.transferFrom(_player, address(this), _amountUSDT), 'USDT transfer not possible');
    }

    // Function to retrieve a specified amount of ATON tokens from a player's account
    function retrieveATON(address _player, uint256 _amountATON) external onlyRole(MANAGER_ROLE) {
        // Ensure the player has enough ATON tokens in their balance
        require(ATON.balanceOf(_player) >= _amountATON, 'Not enough ATON balance');

        // Ensure the contract has approval to transfer the specified amount of ATON tokens from the player's account
        require(ATON.allowance(_player, address(this)) >= _amountATON, 'Need to approve ATON');

        // Transfer the specified amount of ATON tokens from the player's account to the contract's account
        require(ATON.transferFrom(_player, address(this), _amountATON), 'ATON transfer not possible');
    }

    // Function to burn a specified amount of ATON tokens held by the contract
    function burnATON(uint256 _burnAmount) external onlyRole(MANAGER_ROLE) {
        // Ensure the contract has enough ATON tokens in its balance to burn the specified amount
        require(ATON.balanceOf(address(this)) >= _burnAmount, 'Not enough ATON balance');

        // Burn the specified amount of ATON tokens from the contract's account
        require(ATON.burnFrom(_burnAmount), 'ATON transfer not possible');
    }

    function ifEventExists(bytes8 _eventIdBytes) external view returns (bool) {
        // bytes8 _eventIdBytes = stringToBytes8(_eventId);
        return _ifEventExists(_eventIdBytes);
    }

    function ifEventExistsAndActive(bytes8 _eventIdBytes) external view returns (bool) {
        // bytes8 _eventIdBytes = stringToBytes8(_eventId);
        return (_ifEventExists(_eventIdBytes) && events[_eventIdBytes].active);
    }

    function _ifEventExists(bytes8 _eventIdBytes) internal view returns (bool) {
        if (events[_eventIdBytes].stakeCount == 0) {
            return false;
        } else {
            return true;
        }
    }

    // Function to add a new Event
    function addEvent(bytes8 _eventIdBytes, uint64 _startDate, uint8 _sport) external onlyRole(MANAGER_ROLE) {
        // Validate the input
        // bytes8 _eventIdBytes = stringToBytes8(_eventId);
        require(_startDate > block.timestamp, 'Event already Started');

        require(events[_eventIdBytes].active == false, 'Event already Active');
        require(!_ifEventExists(_eventIdBytes), 'Event already has Stakes, cant add Event');
        // Create a new Event struct and set its properties
        // AStructs.Event storage newEvent = events[_eventId];
        events[_eventIdBytes].eventIdBytes = _eventIdBytes;
        events[_eventIdBytes].startDate = _startDate;
        events[_eventIdBytes].stakeCount = 0;
        events[_eventIdBytes].active = true;
        events[_eventIdBytes].winner = -1;
        events[_eventIdBytes].sport = _sport;
        events[_eventIdBytes].factorATON = 0;

        // Increment eventCount
        eventCount += 1;

        // Add the Event ID to the activeEvents array and update the mapping
        activeEvents.push(_eventIdBytes);

        emit EventAdded(_bytes8ToString(_eventIdBytes), _sport, _startDate, msg.sender);
    }

    // Function to retrieve Event details in a DTO (Data Transfer Object) format
    function getEventDTO(bytes8 _eventIdBytes) external view returns (AStructs.EventDTO memory) {
        return (_getEventDTO(_eventIdBytes));
    }

    function _getEventDTO(bytes8 _eventIdBytes) internal view returns (AStructs.EventDTO memory) {
        return (
            AStructs.EventDTO(
                _bytes8ToString(events[_eventIdBytes].eventIdBytes),
                events[_eventIdBytes].startDate,
                events[_eventIdBytes].sport,
                events[_eventIdBytes].totalUSDT[0],
                events[_eventIdBytes].totalUSDT[1],
                events[_eventIdBytes].totalATON[0],
                events[_eventIdBytes].totalATON[1],
                events[_eventIdBytes].active,
                events[_eventIdBytes].scoreA,
                events[_eventIdBytes].scoreB,
                events[_eventIdBytes].winner,
                events[_eventIdBytes].factorATON
            )
        );
    }

    // Function to remove a Event from the active events list
    function _removeEventFromActiveEvents(bytes8 _eventIdBytes) internal {
        // Check if the Event is active
        // require(events[_eventId].active, 'Event is not active');
        // bytes8 _eventIdBytes = stringToBytes8(_eventId);

        // Find the index of the Event in the activeEvents array
        uint256 indexToRemove = activeEvents.length;
        for (uint256 i = 0; i < activeEvents.length; i++) {
            if (activeEvents[i] == _eventIdBytes) {
                indexToRemove = i;
                break;
            }
        }

        // If the Event is found in the activeEvents array, remove it
        require(indexToRemove < activeEvents.length, 'Event not found in activeEvents');

        // Swap the Event to remove with the last Event in the array and then pop (remove) the last element
        activeEvents[indexToRemove] = activeEvents[activeEvents.length - 1];
        activeEvents.pop();

        // Update the Event's active status and decrement the activeEventsCount
        events[_eventIdBytes].active = false;
    }

    function getActiveEvents(int8 _sport) external view returns (AStructs.EventDTO[] memory) {
        uint256 count = 0;

        // Count events that belong to the specified sport
        for (uint i = 0; i < activeEvents.length; i++) {
            AStructs.EventDTO memory currentEvent = _getEventDTO(activeEvents[i]);
            if (currentEvent.sport == uint8(_sport) || _sport < 0) {
                count++;
            }
        }

        AStructs.EventDTO[] memory activeEventsDTO = new AStructs.EventDTO[](count);

        // Add events that belong to the specified sport to the result array
        uint256 index = 0;
        for (uint i = 0; i < activeEvents.length; i++) {
            AStructs.EventDTO memory currentEvent = _getEventDTO(activeEvents[i]);
            if (currentEvent.sport == uint8(_sport) || _sport < 0) {
                activeEventsDTO[index] = currentEvent;
                index++;
            }
        }

        return activeEventsDTO;
    }

    // Function to get the list of players for a specific Event
    function EventPlayers(bytes8 _eventIdBytes) public view returns (address[] memory) {
        return events[_eventIdBytes].players;
    }

    // Function to get the USDT balance of the calling player
    function getPlayerActiveEvents(address _player) external view returns (bytes8[] memory) {
        return players[_player].activeEvents;
    }

    // Function to get the USDT balance of the calling player
    function getPlayerUSDT(address _player) external view returns (uint256) {
        return players[_player].USDT;
    }

    // Function to get the ATON balance of the calling player
    function getPlayerATON(address _player) external view returns (uint256) {
        return players[_player].ATON;
    }

    // Function to get the address of the USDT token contract
    function USDTtoken() public view returns (address) {
        return address(USDT);
    }

    // Function to get the address of the ATON token contract
    function ATONtoken() public view returns (address) {
        return address(ATON);
    }

    // Function to get the length of the active events array
    function ActiveEventsLength() public view returns (uint256) {
        return activeEvents.length;
    }

    // Function to add a stake to a Event for a specific player and team
    function addStake(
        bytes8 _eventIdBytes,
        uint256 _amountUSDT,
        uint256 _amountATON,
        uint8 _team,
        address _player
    ) external onlyRole(MANAGER_ROLE) {
        // Increment the stake count for the Event

        events[_eventIdBytes].stakeCount++;

        require(events[_eventIdBytes].startDate > block.timestamp, 'Event already Started');
        require(events[_eventIdBytes].active, 'Event Closed');

        // If the player has not staked on this Event yet, create a new stake for the player
        if (events[_eventIdBytes].stakes[_player].amountATON == 0 && events[_eventIdBytes].stakes[_player].amountUSDT == 0) {
            events[_eventIdBytes].stakes[_player] = AStructs.Stake(_amountUSDT, _amountATON, _team);
            events[_eventIdBytes].totalUSDT[_team] += _amountUSDT;
            events[_eventIdBytes].totalATON[_team] += _amountATON;
        }
        // If the player has already staked on the same team, update the stake with the new amounts
        else if (events[_eventIdBytes].stakes[_player].team == _team) {
            events[_eventIdBytes].stakes[_player] = AStructs.Stake(
                _amountUSDT + events[_eventIdBytes].stakes[_player].amountUSDT,
                _amountATON + events[_eventIdBytes].stakes[_player].amountATON,
                _team
            );

            events[_eventIdBytes].totalUSDT[_team] += _amountUSDT;
            events[_eventIdBytes].totalATON[_team] += _amountATON;
        }
        // If the player tries to stake on a different team, revert the transaction with an error message
        else {
            require(false, "Can't Add Stake to other team");
        }

        // Check if the Event has already been added to the player's events and add it if not
        bool EventAlreadyAddedToPlayer = false;
        for (uint256 i = 0; i < players[_player].activeEvents.length; i++) {
            if (players[_player].activeEvents[i] == _eventIdBytes) {
                EventAlreadyAddedToPlayer = true;
            }
        }
        if (!EventAlreadyAddedToPlayer) {
            players[_player].activeEvents.push(_eventIdBytes);
            events[_eventIdBytes].players.push(_player);
        }

        // Emit an event to record the stake addition
        emit StakeAdded(_bytes8ToString(_eventIdBytes), _player, _amountUSDT, _amountATON, _team);
    }

    // Function to update Event details with the final result and scores
    function updateEvent(bytes8 _eventIdBytes, int8 _winner, uint8 _scoreA, uint8 _scoreB) external onlyRole(MANAGER_ROLE) {
        events[_eventIdBytes].active = false;
        events[_eventIdBytes].winner = _winner;
        events[_eventIdBytes].scoreA = _scoreA;
        events[_eventIdBytes].scoreB = _scoreB;
        uint256 factorATON = IATON(ATON).calculateFactorATON();
        events[_eventIdBytes].factorATON = factorATON;

        _removeEventFromActiveEvents(_eventIdBytes);
    }

    // Function to get the Event escrow amount, which is the total USDT staked minus the premium
    function getEventEscrow(bytes8 _eventIdBytes) external view returns (uint256) {
        uint256 escrow;
        for (uint i = 0; i < events[_eventIdBytes].totalUSDT.length; i++) {
            escrow = escrow + events[_eventIdBytes].totalUSDT[i];
        }

        // escrow = escrow - (Premium * escrow) / pct_denom;

        return escrow;
    }

    // Function to get the premium value and the percentage denominator
    function getPremium() external view returns (uint256, uint256) {
        return (Premium, pct_denom);
    }

    // Function to get the effective USDT for a team based on the Event ID and the result
    function getEfectiveUSDTForTeam(bytes8 _eventIdBytes, uint256 _team) external view returns (uint256) {
        uint256 factorATON = events[_eventIdBytes].factorATON;

        if (factorATON == 0) {
            factorATON = IATON(ATON).calculateFactorATON();
        }
        uint256 leverageATONInUSDT = (events[_eventIdBytes].totalATON[_team] * factorATON) / pct_denom;

        uint256 efectiveUSDTForTeam = events[_eventIdBytes].totalUSDT[_team] + leverageATONInUSDT;

        return efectiveUSDTForTeam;
    }

    // Function to get the effective USDT for a team based on the Event ID and the result
    function getUSDTForTeam(bytes8 _eventIdBytes, uint256 _team) external view returns (uint256) {
        uint256 efectiveUSDTForTeam = events[_eventIdBytes].totalUSDT[_team];

        return efectiveUSDTForTeam;
    }

    // Function to get the effective USDT for a Event
    function getEfectiveUSDT(bytes8 _eventIdBytes) external view returns (uint256) {
        uint256 factorATON = events[_eventIdBytes].factorATON;

        if (factorATON == 0) {
            factorATON = IATON(ATON).calculateFactorATON();
        }
        uint256 efectiveEventUSDT;
        for (uint i = 0; i < events[_eventIdBytes].totalUSDT.length; i++) {
            efectiveEventUSDT += (events[_eventIdBytes].totalUSDT[i] + (events[_eventIdBytes].totalATON[i] * factorATON) / pct_denom);
        }
        return efectiveEventUSDT;
    }

    // Function to get the players of a Event
    function getEventPlayers(bytes8 _eventIdBytes) external view returns (address[] memory) {
        return events[_eventIdBytes].players;
    }

    // Function to get a player's stake for a specific Event
    function getPlayerStake(bytes8 _eventIdBytes, address _player) external view returns (AStructs.StakeDTO memory) {
        return _getPlayerStake(_eventIdBytes, _player);
    }

    // Function to get a player's stake for a specific Event
    function _getPlayerStake(bytes8 _eventIdBytes, address _player) internal view returns (AStructs.StakeDTO memory) {
        uint256 factorATON = events[_eventIdBytes].factorATON;

        if (factorATON == 0) {
            factorATON = IATON(ATON).calculateFactorATON();
        }

        AStructs.Stake memory stake = events[_eventIdBytes].stakes[_player];
        uint256 efectivePlayerStake = stake.amountUSDT + (stake.amountATON * factorATON) / pct_denom;
        AStructs.StakeDTO memory stakeDTO = AStructs.StakeDTO(stake.amountUSDT, stake.amountATON, stake.team, efectivePlayerStake);
        return stakeDTO;
    }

    function isPlayerFinalizedEvent(bytes8 _eventIdBytes, address _player) external view returns (bool) {
        return events[_eventIdBytes].stakeFinalized[_player];
    }

    function setPlayerFinalizedEvent(bytes8 _eventIdBytes, address _player) external onlyRole(MANAGER_ROLE) returns (bool) {
        events[_eventIdBytes].stakeFinalized[_player] = true;
        _removePlayerActiveEvent(_eventIdBytes, _player);
        players[_player].closedEvents.push(_eventIdBytes);
        return true;
    }

    // Function to add earnings to a player's balances
    function addEarningsToPlayer(address _player, uint256 _amountUSDT, uint256 _amountATON) external {
        _addEarningsToPlayer(_player, _amountUSDT, _amountATON);
    }

    // Function to add earnings to a player's balances
    function _addEarningsToPlayer(address _player, uint256 _amountUSDT, uint256 _amountATON) internal {
        players[_player].USDT += _amountUSDT;
        players[_player].ATON += _amountATON;
    }

    // Function to calculate the ATON equivalent of a given USDT amount
    function getUSDTtoATON(uint256 _amountUSDT) external view returns (uint256) {
        uint256 factorATON = IATON(ATON).calculateFactorATON();

        uint256 amountATON = (_amountUSDT * pct_denom) / factorATON;

        return amountATON;
    }

    function getATONtoUSDT(uint256 _amountATON) external view returns (uint256) {
        uint256 factorATON = IATON(ATON).calculateFactorATON();

        uint256 amountUSDT = (_amountATON * factorATON) / pct_denom;

        return amountUSDT;
    }

    // This function allows the contract manager to withdraw USDT tokens on behalf of a player.
    // It can only be called by someone with the MANAGER_ROLE.
    function withdrawUSDT(address _player, uint256 _amountUSDT) external onlyRole(MANAGER_ROLE) {
        // Check if the contract has a sufficient balance of USDT tokens to fulfill the withdrawal request.
        require(IERC20(USDT).balanceOf(address(this)) >= _amountUSDT, 'USDT No More');

        // Check if the player has a sufficient USDT balance in their account to withdraw the requested amount.
        require(players[_player].USDT >= _amountUSDT, 'Player has not enough USDT Vault balance');

        // Transfer the requested amount of USDT tokens from the contract to the player's address.
        require(IERC20(USDT).transfer(_player, _amountUSDT), 'USDT transfer Vault to Player failed');

        players[_player].USDT -= _amountUSDT;
    }

    // This function allows the contract manager to withdraw ATON tokens on behalf of a player.
    // It can only be called by someone with the MANAGER_ROLE.
    function withdrawATON(address _player, uint256 _amountATON) external onlyRole(MANAGER_ROLE) {
        // Check if the contract has a sufficient balance of ATON tokens to fulfill the withdrawal request.
        require(IATON(ATON).balanceOf(address(this)) >= _amountATON, 'ATON No More');

        // Check if the player has a sufficient ATON balance in their account to withdraw the requested amount.
        require(players[_player].ATON >= _amountATON, 'Player has not enough ATON Vault balance');

        // Transfer the requested amount of ATON tokens from the contract to the player's address.
        require(IATON(ATON).transfer(_player, _amountATON), 'ATON transfer Vault to Player failed');

        players[_player].ATON -= _amountATON;
    }

    function substractUSDT(address _player, uint256 _amountUSDT) external onlyRole(MANAGER_ROLE) returns (bool) {
        // Check if the player has a sufficient USDT balance in their account to withdraw the requested amount.
        require(players[_player].USDT >= _amountUSDT, 'Player has not enough USDT balance');

        players[_player].USDT -= _amountUSDT;

        return true;
    }

    function substractATON(address _player, uint256 _amountATON) external onlyRole(MANAGER_ROLE) returns (bool) {
        // Check if the player has a sufficient ATON balance in their account to withdraw the requested amount.
        require(players[_player].ATON >= _amountATON, 'Player has not enough ATON balance');

        players[_player].ATON -= _amountATON;

        return true;
    }

    // This function is used to distribute referral earnings to the associates of a referral group.
    // It takes the player address and the player's USDT earnings as input parameters.
    function payReferals(address _player, uint256 _playerUSDTearnings) external returns (uint256 expensesUSDT) {
        // Get the referral group code for the player
        uint256 referalCode = players[_player].referalGroupCode;

        // Retrieve the list of associates belonging to the player's referral group
        AStructs.Associate[] memory associatesList = referalGroups[referalCode];

        // Iterate through the list of associates
        for (uint i = 0; i < associatesList.length; i++) {
            // Get the wallet address and share percentage of the current associate
            address wallet = associatesList[i].wallet;
            uint256 share = associatesList[i].share;

            // Calculate the referral earning for the current associate based on their share percentage and the player's USDT earnings
            uint256 expense = ((_playerUSDTearnings * share * Premium) / (pct_denom * pct_denom));

            // Add the calculated referral earning to the associate's wallet
            players[wallet].USDT += expense;

            // Keep track of the total referral expenses paid out in USDT
            expensesUSDT += expense;
        }
    }

