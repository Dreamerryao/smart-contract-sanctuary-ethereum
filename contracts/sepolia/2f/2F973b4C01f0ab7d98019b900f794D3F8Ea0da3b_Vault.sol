    function cancelPlayerStake(bytes8 _eventIdBytes, address _player, uint256 _cancelationPctCost) external onlyRole(MANAGER_ROLE) {
        // Ensure the Event has not started yet and is still active

        require(events[_eventIdBytes].startDate > block.timestamp, 'Event already started');
        require(events[_eventIdBytes].active, 'Event already closed');

        // Retrieve the player's stake details for the Event
        uint256 amountUSDT = events[_eventIdBytes].stakes[_player].amountUSDT;
        uint256 amountATON = events[_eventIdBytes].stakes[_player].amountATON;
        uint8 team = events[_eventIdBytes].stakes[_player].team;

        // Calculate the amount of USDT to return after deducting the cancelation fee
        uint256 amountUSDTtoReturn = amountUSDT - (amountUSDT * _cancelationPctCost) / pct_denom;

        // Update the total staked amounts of USDT and ATON for the team
        events[_eventIdBytes].totalUSDT[team] -= amountUSDTtoReturn;
        events[_eventIdBytes].totalATON[team] -= amountATON;
        events[_eventIdBytes].stakeCount -= 1;

        // Reset the player's stake amounts for the Event
        events[_eventIdBytes].stakes[_player].amountUSDT = 0;
        events[_eventIdBytes].stakes[_player].amountATON = 0;

        // Transfer the calculated USDT amount back to the player
        require(USDT.transfer(_player, amountUSDTtoReturn), 'Transfer failed');

        // Remove the player's address from the Event's players array
        uint256 indexToRemove = events[_eventIdBytes].players.length;
        for (uint256 i = 0; i < events[_eventIdBytes].players.length; i++) {
            if (events[_eventIdBytes].players[i] == _player) {
                indexToRemove = i;
                break;
            }
        }
        require(indexToRemove < events[_eventIdBytes].players.length, 'Player not found in players array');
        events[_eventIdBytes].players[indexToRemove] = events[_eventIdBytes].players[events[_eventIdBytes].players.length - 1];
        events[_eventIdBytes].players.pop();

        // Remove the Event _eventIdBytes from the player's events array
        _removePlayerActiveEvent(_eventIdBytes, _player);
    }

    function _removePlayerActiveEvent(bytes8 _eventIdBytes, address _player) internal {
        uint256 indexToRemove = players[_player].activeEvents.length;
        for (uint256 i = 0; i < players[_player].activeEvents.length; i++) {
            if (players[_player].activeEvents[i] == _eventIdBytes) {
                indexToRemove = i;
                break;
            }
        }
        require(indexToRemove < players[_player].activeEvents.length, 'Event not found in players.events array');
        players[_player].activeEvents[indexToRemove] = players[_player].activeEvents[players[_player].activeEvents.length - 1];
        players[_player].activeEvents.pop();
    }

    function _stringToBytes8(string memory source) internal pure returns (bytes8 result) {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            result := mload(add(source, 32))
        }
    }

    function _bytes8ToString(bytes8 x) internal pure returns (string memory) {
        bytes memory bytesString = new bytes(8);
        for (uint256 i = 0; i < 8; i++) {
            bytesString[i] = x[i];
        }
        return string(bytesString);
    }
}

