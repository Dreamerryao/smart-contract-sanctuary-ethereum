  function editBuyNowSellingAgreement(
    address nftContractAddress,
    uint256 tokenId,
    uint96 newPrice
  ) external override nonReentrant {
    BuyNowSellingAgreement memory sellingAgreement = s_buyNowSellingAgreements[
      nftContractAddress
    ][tokenId];

    sellingAgreement.mustExist();
    sellingAgreement.mustBeOwnedBy(msg.sender);
    newPrice.mustBeAboveZero();

    s_buyNowSellingAgreements[nftContractAddress][tokenId].price = newPrice;
    emit BuyNowSellingAgreementEdited(
      nftContractAddress,
      tokenId,
      msg.sender,
      newPrice
    );
  }

  // todo: why do we need this if we have events?
  // todo: if we need it, should we move it to the interface?
  /**
   * @notice Returns the buy now sale details for an NFT if one is available.
   * @dev If no price is found, seller will be address(0) and price will be max uint256.
   * @param nftContract The address of the NFT contract.
   * @param tokenId The id of the NFT.
   * @return seller The address of the owner that listed a buy price for this NFT.
   * Returns `address(0)` if there is no buy price set for this NFT.
   * @return price The price of the NFT.
   * Returns max uint256 if there is no buy price set for this NFT (since a price of 0 is supported).
   */
  function getBuyNowSaleDetails(
    address nftContract,
    uint256 tokenId
  )
    external
    view
    returns (
      address seller,
      uint256 price,
      uint256 startTime,
      bool isPrimarySale
    )
  {
    seller = s_buyNowSellingAgreements[nftContract][tokenId].seller;
    if (seller == address(0)) {
      return (seller, 0, 0, false);
    }
    price = s_buyNowSellingAgreements[nftContract][tokenId].price;
    startTime = s_buyNowSellingAgreements[nftContract][tokenId].startTime;
    isPrimarySale = s_buyNowSellingAgreements[nftContract][tokenId]
      .isPrimarySale;
  }
}

