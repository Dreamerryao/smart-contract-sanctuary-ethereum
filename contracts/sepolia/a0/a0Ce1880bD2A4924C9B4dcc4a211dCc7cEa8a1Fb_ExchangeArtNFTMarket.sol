library AuctionChecks {
  function mustNotExist(uint256 auctionId) internal pure {
    if (auctionId != 0) {
      revert NFTMarketAuction_Already_Exists(auctionId);
    }
  }

  function mustExist(AuctionState memory auction) internal pure {
    if (auction.reservePriceOrHighestBid == 0) {
      revert NFTMarketAuction_Inexistent();
    }
  }

  function mustHaveEnded(
    AuctionState memory auction,
    uint256 currentTime
  ) internal pure {
    if (auction.end > currentTime) {
      revert NFTMarketAuction__Ongoing();
    }
  }

  function mustBeOngoing(
    AuctionState memory auction,
    uint256 currentTime
  ) internal pure {
    if (auction.end < currentTime) {
      revert NFTMarketAuction__Ended();
    }
  }

  function callerMustBeSeller(
    AuctionState memory auction,
    address caller
  ) internal pure {
    if (auction.seller != caller) {
      revert NFTMarketAuction_Inexistent();
    }
  }

  function callerMustNotBeSeller(
    AuctionState memory auction,
    address caller
  ) internal pure {
    if (auction.seller == caller) {
      revert NFTMarketAuction__callerMustNotBeSeller();
    }
  }

  function mustHaveAtLeastOneBid(AuctionState memory auction) internal pure {
    if (auction.highestBidder == address(0)) {
      revert NFTMarketAuction_DoesNotHave_Bids();
    }
  }

  function mustNotHaveBids(AuctionState memory auction) internal pure {
    if (auction.highestBidder != address(0)) {
      revert NFTMarketAuction_DoesNotHave_Bids();
    }
  }

  function mustBeGreaterThanOrEqual(
    uint256 value,
    uint256 compareTo
  ) internal pure {
    if (compareTo > value) {
      revert NFTMarketAuction__SellingAgreement__PriceNotMet();
    }
  }

  function mustBeReedemedByOwnerOrHighestBidder(
    AuctionState memory auction,
    address caller
  ) internal pure {
    if (caller != auction.seller && caller != auction.highestBidder) {
      revert NFTMarketAuction_Invalid_Caller();
    }
  }

  function callerCannotBeHighestBidder(
    AuctionState memory auction,
    address caller
  ) internal pure {
    if (caller == auction.highestBidder) {
      revert NFTMarketAuction_Invalid_Caller();
    }
  }
}

