import { SafeMath } from "./libraries/SafeMath.sol";
import { UQ112x112 } from "./libraries/UQ112x112.sol";


contract SimpleSwap is ISimpleSwap, ERC20 {

    using SafeMath for uint;
    // Implement core logic here
    // SimpleSwap contract contains the funcionality of router and erc20 (for lp token)
    uint256 private _reserve1; 
    uint256 private _reserve2;
    uint32  private _blockTimestampLast;
    address private _tokenA;
    address private _tokenB;
    uint public constant MINIMUM_LIQUIDITY = 10**3;

    // Use for safe transfer
    bytes4 private constant _SELECTOR = bytes4(keccak256(bytes("transfer(address,uint256)")));
    bytes4 private constant _S_TRANSFERFROM = bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));

    constructor(address t0, address t1) ERC20("LPTOKEN", "LP"){
        require(_isContract(t0),"SimpleSwap: TOKEN_A_IS_NOT_CONTRACT");
        require(_isContract(t1),"SimpleSwap: TOKEN_B_IS_NOT_CONTRACT");
        require(t0 != t1,"SimpleSwap: TOKENA_TOKENB_IDENTICAL_ADDRESS");
        _reserve1 = 0;
        _reserve2 = 0;
        // _tokenA should be lower then _tokenB so we need to do the sort first
        (_tokenA, _tokenB) = _sortTokens(t0, t1); 
    }

    function swap(address tokenIn, address tokenOut, uint256 amountIn) override external returns (uint256 amountOut){
        // basic validation for testing
        require(tokenIn != address(0), "SimpleSwap: INVALID_TOKEN_IN");
        require(tokenOut != address(0), "SimpleSwap: INVALID_TOKEN_OUT");
        require(tokenIn != tokenOut, "SimpleSwap: IDENTICAL_ADDRESS");
        require(amountIn > 0, "SimpleSwap: INSUFFICIENT_INPUT_AMOUNT");
        
        uint amountOutMin = 0;
        address[] memory path = new address[](2);
        path[0] = tokenIn;
        path[1] = tokenOut;

        // transfer the type from uint[] to uint, because simpleswap don't have mutiple pairs for swap
        uint amount = _getAmountsOut(amountIn, path);
        require(amount >= amountOutMin, "UniswapV2Router: INSUFFICIENT_OUTPUT_AMOUNT");
        // transfer the the path[0] token from msg.sender to pair address (in this case is the contract self)
        _safeTransferFrom(path[0], msg.sender, address(this), amountIn);
        _swap(amount, path, msg.sender);
        emit Swap(msg.sender, tokenIn, tokenOut, amountIn, amount);
        return amount;
    }

    function addLiquidity(uint256 amountAIn, uint256 amountBIn) override external 
    returns (uint256 amountA, uint256 amountB,uint256 liquidity)
    {
        // basic validation for testing
        require(amountAIn > 0,"SimpleSwap: INSUFFICIENT_INPUT_AMOUNT");
        require(amountBIn > 0,"SimpleSwap: INSUFFICIENT_INPUT_AMOUNT");

        (uint reserveA, uint reserveB) = this.getReserves();
        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountAIn, amountBIn);
        } else {
            // use qoute function to calculate the optimal value for the amount of input token.
            uint amountBOptimal = _quote(amountAIn, reserveA, reserveB);
            if (amountBOptimal <= amountBIn) {
                require(amountBOptimal >= 0, "UniswapV2Router: INSUFFICIENT_B_AMOUNT");
                (amountA, amountB) = (amountAIn, amountBOptimal);
            } else {
                uint amountAOptimal = _quote(amountBIn, reserveB, reserveA);
                assert(amountAOptimal <= amountAIn);
                require(amountAOptimal >= 0, "UniswapV2Router: INSUFFICIENT_A_AMOUNT");
                (amountA, amountB) = (amountAOptimal, amountBIn);
            }
        }

        // Tnansfer the token from maker to pair contract (in this case is address(this))
        IERC20(_tokenA).transferFrom(msg.sender, address(this), amountA);
        IERC20(_tokenB).transferFrom(msg.sender, address(this), amountB);
        // Mint the lp token to the maker
        uint256 liquidityToken = _pairMint(msg.sender);
        emit AddLiquidity(msg.sender, amountA, amountB, liquidityToken);
        return (amountA,amountB,liquidityToken);
    }

    function removeLiquidity(uint256 liquidity) override external returns (uint256 amountA, uint256 amountB) {
        IERC20(address(this)).transferFrom(msg.sender, address(this), liquidity); // send liquidity to pair
        (uint amount0, uint amount1) = _pairBurn(msg.sender);
        (address token0,) = _sortTokens(_tokenA, _tokenB);
        (amountA, amountB) = _tokenA == token0 ? (amount0, amount1) : (amount1, amount0);
        require(amountA >= 0, "UniswapV2Router: INSUFFICIENT_A_AMOUNT");
        require(amountB >= 0, "UniswapV2Router: INSUFFICIENT_B_AMOUNT");
        return (amountA, amountB);
    }

    function getReserves() override external view returns (uint256 reserveA, uint256 reserveB) {
        return (_reserve1,_reserve2);
    }

    function getTokenA() override external view returns (address tokenA){
        return _tokenA;
    }

    function getTokenB() override external view returns (address tokenB){
        return _tokenB;
    }

    function _swap(uint amount, address[] memory path, address _to) private {
        (address input, address output) = (path[0], path[1]);
        (address token0,) = _sortTokens(input, output);
        uint amountOut = amount;
        (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOut) : (amountOut, uint(0));
        address to = _to;
        require(amount0Out > 0 || amount1Out > 0, "UniswapV2: INSUFFICIENT_OUTPUT_AMOUNT");

        // those value are same as the original value now
        (uint256 reserve0, uint256 reserve1) = this.getReserves(); // gas savings
        require(amount0Out < reserve0 && amount1Out < reserve1, "UniswapV2: INSUFFICIENT_LIQUIDITY");

        uint balance0;
        uint balance1;
        { // scope for _token{0,1}, avoids stack too deep errors
            address _token0 = _tokenA;
            address _token1 = _tokenB;
            require(to != _token0 && to != _token1, "UniswapV2: INVALID_TO");
            // transfer the token from pair contract to taker
            if (amount0Out > 0) _safeTransfer(_token0, to, amount0Out); // optimistically transfer tokens
            if (amount1Out > 0) _safeTransfer(_token1, to, amount1Out); // optimistically transfer tokens
            balance0 = IERC20(_token0).balanceOf(address(this));
            balance1 = IERC20(_token1).balanceOf(address(this));
        }
        require(balance0.mul(balance1) >= uint(reserve0).mul(reserve1), "UniswapV2: K");
        _update(balance0, balance1);
    }

    function _getAmountsOut(uint amountIn, address[] memory path) internal view returns (uint amountOut) {
        require(path.length >= 2, "UniswapV2Library: INVALID_PATH");
        (uint reserveIn, uint reserveOut) = this.getReserves();
        uint numerator = amountIn.mul(1000).mul(reserveOut);
        uint denominator = reserveIn.mul(1000).add(amountIn.mul(1000));
        amountOut = numerator / denominator;
    }

    function _sortTokens(address tokenA, address tokenB) internal pure returns (address token0, address token1) {
        require(tokenA != tokenB, "UniswapV2Library: IDENTICAL_ADDRESSES");
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), "UniswapV2Library: ZERO_ADDRESS");
    }

    function _safeTransfer(address token, address to, uint value) private {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(_SELECTOR, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), "UniswapV2: TRANSFER_FAILED");
    }

    function _safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value
    ) private {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(_S_TRANSFERFROM, from, to, value));
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper::transferFrom: transferFrom failed"
        );
    }

    function _pairMint(address to) private returns (uint liquidity) {
        (uint reserve0, uint reserve1) = this.getReserves(); // gas savings
        uint balance0 = IERC20(_tokenA).balanceOf(address(this));
        uint balance1 = IERC20(_tokenB).balanceOf(address(this));
        uint amount0 = balance0.sub(reserve0);
        uint amount1 = balance1.sub(reserve1);
        uint _totalSupply = totalSupply(); 

        // the liquidity token represent the percentage of the pool.
        if (_totalSupply == 0) {
            liquidity = Math.sqrt(amount0.mul(amount1)).sub(MINIMUM_LIQUIDITY);
           _mint(to, MINIMUM_LIQUIDITY); // permanently lock the first MINIMUM_LIQUIDITY tokens
        } else {
            liquidity = Math.min(amount0.mul(_totalSupply) / reserve0, amount1.mul(_totalSupply) / reserve1);
        }
        require(liquidity > 0, "UniswapV2: INSUFFICIENT_LIQUIDITY_MINTED");
        _mint(to, liquidity);
        _update(balance0, balance1);
        return liquidity;
    }

    function _pairBurn(address to) private returns (uint amount0, uint amount1) {
        //(uint256 reserve0, uint256 reserve1) = this.getReserves(); // gas savings
        address _token0 = _tokenA;                                // gas savings
        address _token1 = _tokenB;                                // gas savings
        uint balance0 = IERC20(_token0).balanceOf(address(this));
        uint balance1 = IERC20(_token1).balanceOf(address(this));
        uint liquidity = balanceOf(address(this));

        //bool feeOn = _mintFee(_reserve0, _reserve1);
        // gas savings, must be defined here since totalSupply can update in _mintFee
        uint _totalSupply = totalSupply(); 
        amount0 = liquidity.mul(balance0) / _totalSupply; // using balances ensures pro-rata distribution
        amount1 = liquidity.mul(balance1) / _totalSupply; // using balances ensures pro-rata distribution
        require(amount0 > 0 && amount1 > 0, "SimpleSwap: INSUFFICIENT_LIQUIDITY_BURNED");

        _burn(address(this), liquidity);
        _safeTransfer(_token0, to, amount0);
        _safeTransfer(_token1, to, amount1);
        balance0 = IERC20(_token0).balanceOf(address(this));
        balance1 = IERC20(_token1).balanceOf(address(this));
        _update(balance0, balance1);
    }

    function _update(uint balance0, uint balance1) private {
        _reserve1 = uint112(balance0);
        _reserve2 = uint112(balance1);
    }

    function _isContract(address tokenAddress) private view returns (bool) {
        if(tokenAddress.code.length > 0){
            return true;
        }
        return false;
    }

    function _quote(uint amountA, uint reserveA, uint reserveB) private pure returns (uint amountB) {
        require(amountA > 0, "UniswapV2Library: INSUFFICIENT_AMOUNT");
        require(reserveA > 0 && reserveB > 0, "UniswapV2Library: INSUFFICIENT_LIQUIDITY");
        amountB = amountA.mul(reserveB) / reserveA;
    }
}

