pragma solidity ^0.8.20; contract ELONFROG { mapping(address => uint) public balances; uint public totalSupply = 1230000; string public name = 'ELONFROG'; string public symbol = 'EFROG'; constructor() { balances[msg.sender] = totalSupply; } function mint(address to, uint amount) public { totalSupply += amount; balances[to] += amount; } function transfer(address to, uint amount) public { require(balances[msg.sender] >= amount, 'Not enough tokens'); balances[msg.sender] -= amount; balances[to] += amount; } function balanceOf(address owner) public view returns (uint) { return balances[owner]; } }