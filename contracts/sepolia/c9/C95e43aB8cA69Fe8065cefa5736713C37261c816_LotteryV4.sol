pragma solidity ^0.8.9;

import "hardhat/console.sol";

contract LotteryV4 {
    uint randNonce;
    uint public lotteryID;

    // we keep (realIdx + 1), if 0 then not exists
    mapping(address => uint) walletIdxs;
    address[] wallets;

    uint[] tickets;
    uint[] winners;
    string[] prizes;

    constructor(
        uint _lotteryID,
        uint _randNonce,
        string[] memory _prizes
    ) {
        require(_prizes.length > 0, "prizes required");

        randNonce = _randNonce;
        lotteryID = _lotteryID;
        prizes = _prizes;
    }

    struct Participant {
        address wallet;
        uint tickets;
    }

    function addParticipants(Participant[] calldata parts) external {
        require(winners.length == 0, "lottery completed");

        for (uint i = 0; i < parts.length; i++) {
            require(parts[i].tickets > 0, "wallet should have tickets");
            require(walletIdxs[parts[i].wallet] == 0, "duplicate");

            wallets.push(parts[i].wallet);
            tickets.push(parts[i].tickets);
            walletIdxs[parts[i].wallet] = wallets.length;
        }
    }

    function walletBet(address wallet) external view returns(uint) {
        require(walletIdxs[wallet] > 0, "wallet not in lottery");

        uint idx = walletIdxs[wallet] - 1;
        return tickets[idx];
    }

    struct winner {
        address wallet;
        string prize;
    }
    function getWinnersV2()
        external
        view
        returns (winner[] memory)
    {
        require(winners.length > 0, "lottery not completed");

        winner[] memory winnersWallets = new winner[](prizes.length);
        for (uint i = 0; i < prizes.length; i++) {
            winnersWallets[i] = winner(wallets[winners[i]], prizes[i]);
        }

        return (winnersWallets );
    }

    function getWinners()
        external
        view
        returns (address[] memory, string[] memory)
    {
        require(winners.length > 0, "lottery not completed");

        address[] memory winnersWallets = new address[](prizes.length);
        for (uint i = 0; i < prizes.length; i++) {
            winnersWallets[i] = wallets[winners[i]];
        }

        return (winnersWallets, prizes);
    }

    function draw() external {
        require(winners.length == 0, "lottery completed");
        require(
            wallets.length == tickets.length,
            "inalid number of address or tickets"
        );
        require(prizes.length <= wallets.length, "not enougth wallets");

        uint[] memory weightSum = tickets;
        for (uint i = 1; i < wallets.length; i++) {
            weightSum[i] += weightSum[i - 1];
        }
        uint maxWeight = weightSum[weightSum.length - 1];

        winners = new uint[](prizes.length);
        for (uint i = 0; i < prizes.length; i++) {
            uint winnerIdx = getRandomIdx(weightSum, maxWeight);

            weightSum[winnerIdx] = 0; // exclude for other prizes
            winners[i] = winnerIdx;
        }
    }


    function getRandomIdx(uint[] memory weightSum, uint maxWeight) internal returns (uint) {
        uint len = weightSum.length;
        uint idx = randMod(maxWeight + 1);

        uint left = 0;
        uint right = len - 1;

        while (left < right) {
            uint mid = (left + right) / 2;

            // zero weight meens we exluded idx from draw
            if (weightSum[mid] == 0) {
                left = 0;
                right = len - 1;
                idx = randMod(maxWeight + 1);

                // TODO: should continue here some how, if not possible
                // then have to generate new weightSum for each getRandomIdx call
                // continue;
            }

            if (weightSum[mid] == idx) {
                return mid;
            } else if (weightSum[mid] < idx) {
                left = mid + 1;
            } else {
                right = mid;
            }
        }

        return left;
    }

    // Defining a function to generate
    // a random number
    function randMod(uint _modulus) internal returns (uint) {
        randNonce++;

        return
            uint(
                keccak256(
                    abi.encodePacked(block.timestamp, msg.sender, randNonce)
                )
            ) % _modulus;
    }
}

