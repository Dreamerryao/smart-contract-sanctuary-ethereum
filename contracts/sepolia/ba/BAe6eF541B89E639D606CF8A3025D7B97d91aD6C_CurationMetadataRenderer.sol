import {IERC721AUpgradeable} from "erc721a-upgradeable/IERC721AUpgradeable.sol";

import {IERC721Press} from "./core/interfaces/IERC721Press.sol";
import {IERC721PressLogic} from "./core/interfaces/IERC721PressLogic.sol";
