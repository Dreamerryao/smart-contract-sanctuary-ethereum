import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../interface/IAddressMap.sol";
import "../library/Struct.sol";

contract LendingStorage {
  using SafeERC20 for IERC20;

  uint32 internal _protocolFeeRate;
  uint internal _positionIdx;

  // asset => AssetConfig
  mapping(address => AssetConfig) internal _assetConfigs;
  // idx => Position
  mapping(uint => Position) internal _positions;
  mapping(address => uint[]) internal _userPositionList;
  mapping(address => mapping(address => mapping(address => bool))) internal _isPositionExist;
  mapping(address => mapping(address => mapping(address => bool))) internal _isShortPositionExist;

  function _createPosition(uint idx, Position memory position) internal {
    _positions[idx] = position;
    _userPositionList[position.owner].push(idx);
    if (position.isShort)
      _isShortPositionExist[position.owner][position.asset.token][position.collateral.token] = true;
    else _isPositionExist[position.owner][position.asset.token][position.collateral.token] = true;
  }

  function _removePosition(uint idx) internal {
    Position memory position = _positions[idx];

    if (position.isShort)
      delete _isShortPositionExist[position.owner][position.asset.token][position.collateral.token];
    else delete _isPositionExist[position.owner][position.asset.token][position.collateral.token];

    _removeUserPositionList(position.owner, idx);

    delete _positions[idx];
  }

  function _removeUserPositionList(address user, uint idx) internal {
    uint[] storage list = _userPositionList[user];
    bool isExist = false;
    for (uint i = 0; i < list.length; i++) {
      if (list[i] == idx) isExist = true;
      else if (isExist) list[i - 1] = list[i];
    }
    require(isExist, "Not own specified position");
    if (isExist) list.pop();
  }
}

