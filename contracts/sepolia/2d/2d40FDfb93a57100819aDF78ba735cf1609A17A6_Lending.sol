pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./LendingStorage.sol";
import "../interface/IZAsset.sol";
import "../interface/ILending.sol";
import "../interface/IStaking.sol";
import "../interface/ICollateralManager.sol";
import "../interface/IPriceOracle.sol";
import "../interface/ICollateralOracle.sol";
import "../interface/IDexManager.sol";
import "../interface/IAddressMap.sol";
import "../library/AddressHandler.sol";
import "../library/Decimal.sol";
import "../library/MathUtils.sol";
import "../library/Constant.sol";

import "hardhat/console.sol";

contract Lending is ILending, LendingStorage, AddressHandler, Ownable {
  using SafeERC20 for IERC20;
  using MathUtils for uint;
  using Decimal for decimal;

  constructor(address addressMap, uint32 protocolFee) AddressHandler(IAddressMap(addressMap)) {
    _positionIdx = 1;
    _protocolFeeRate = protocolFee;
  }

  function updateProtocolFee(uint32 protocolFee) external onlyOwner {
    require(protocolFee < 50, "ProtocolFee must be less than 5%");

    _protocolFeeRate = protocolFee;

    emit UpdateProtocolFee(protocolFee);
  }

  function registerAsset(
    address asset,
    uint32 liquidationDiscount,
    uint32 minCollateralRatio
  ) external onlyRole(ROLE_FACTORY) {
    require(liquidationDiscount < 1000, "liquidationDiscount must be smaller than 1000");
    require(minCollateralRatio > 1100, "minCollateralRatio must be larger or equal than 1100");
    require(_assetConfigs[asset].asset == address(0), "Asset was already registered");

    _assetConfigs[asset] = AssetConfig({
      asset: asset,
      liquidationDiscount: liquidationDiscount,
      minCollateralRatio: minCollateralRatio,
      endPrice: 0
    });

    emit RegisterAsset(asset, liquidationDiscount, minCollateralRatio);
  }

  function updateAsset(
    address asset,
    uint32 liquidationDiscount,
    uint32 minCollateralRatio
  ) external onlyOwner {
    require(liquidationDiscount < 1000, "liquidationDiscount must be smaller than 1000");
    require(minCollateralRatio > 1100, "minCollateralRatio must be larger or equal than 1100");

    AssetConfig memory assetConfig = _assetConfigs[asset];
    require(assetConfig.asset != address(0), "No asset data stored");

    assetConfig.liquidationDiscount = liquidationDiscount;
    assetConfig.minCollateralRatio = minCollateralRatio;

    _assetConfigs[asset] = assetConfig;

    emit UpdateAsset(asset, liquidationDiscount, minCollateralRatio);
  }

  function registerMigration(address asset, uint endPrice) external onlyRole(ROLE_FACTORY) {
    AssetConfig memory assetConfig = _assetConfigs[asset];
    if (assetConfig.asset != address(0)) {
      assetConfig.minCollateralRatio = PERMILLE;
      assetConfig.endPrice = endPrice;
      _assetConfigs[asset] = assetConfig;
    }
  }

  function openPosition(
    Asset memory collateral,
    address asset,
    uint32 collateralRatio,
    ShortParams memory shortParams
  ) external {
    require(collateral.amount > 0, "Wrong collateral");
    require(
      shortParams.isShort
        ? !_isShortPositionExist[msg.sender][asset][collateral.token]
        : !_isPositionExist[msg.sender][asset][collateral.token],
      "Same pair of CDP already exists"
    );

    uint collateralPrice;
    uint32 multiplier;
    {
      CollateralPriceInfo memory info = ICollateralOracle(lookup(ROLE_COLLATERAL_ORACLE))
        .queryCollateralPrice(collateral.token, DEFAULT_TIME_FRAME);
      collateralPrice = info.price;
      multiplier = info.multiplier;
    }

    {
      AssetConfig memory assetConfig = _assetConfigs[asset];
      require(assetConfig.asset != address(0), "No asset data stored");

      require(
        collateralRatio * PERMILLE >= assetConfig.minCollateralRatio * multiplier,
        "Can not open a position with low collateral ratio than minimum"
      );
    }

    (uint assetPrice, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      asset,
      DEFAULT_TIME_FRAME
    );

    uint8 assetDecimals = IERC20Metadata(asset).decimals();
    uint8 collateralDecimals = IERC20Metadata(collateral.token).decimals();
    uint mintAmount = uint(collateral.amount)
      .calcAmount(collateralPrice, assetDecimals, assetPrice, collateralDecimals)
      .divPermille(collateralRatio);

    uint idx = _positionIdx;

    ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).increaseCollateral(
      idx,
      collateral,
      msg.sender
    );

    {
      Position memory position = Position({
        idx: idx,
        owner: msg.sender,
        collateral: collateral,
        asset: Asset({ token: asset, amount: mintAmount }),
        isShort: shortParams.isShort
      });
      _createPosition(idx, position);
    }

    if (shortParams.isShort) {
      address dm = lookup(ROLE_DEX_MANAGER);
      IZAsset(asset).mint(dm, mintAmount);
      IDexManager(dm).shortSwap(asset, msg.sender, mintAmount, shortParams);
      IStaking(lookup(ROLE_STAKING)).increaseShortToken(msg.sender, asset, mintAmount);
    } else {
      IZAsset(asset).mint(msg.sender, mintAmount);
    }

    _positionIdx = idx + 1;

    emit OpenPosition(
      idx,
      collateral.token,
      collateral.amount,
      asset,
      collateralRatio,
      shortParams.isShort
    );
  }

  function deposit(uint idx, Asset memory collateral) public {
    Position memory position = _positions[idx];
    require(msg.sender == position.owner, "unauthorized");
    require(position.collateral.token == collateral.token, "Wrong collateral");
    require(collateral.amount > 0, "Amount dose not allow 0");

    position.collateral.amount += collateral.amount;
    _positions[idx] = position;
    ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).increaseCollateral(
      idx,
      collateral,
      msg.sender
    );

    emit Deposit(idx, collateral.token, collateral.amount);
  }

  function withdraw(uint idx, Asset memory collateral) public {
    Position memory position = _positions[idx];
    require(msg.sender == position.owner, "unauthorized");
    require(position.collateral.token == collateral.token, "Wrong collateral");
    require(collateral.amount > 0, "Amount dose not allow 0");
    require(
      position.collateral.amount >= collateral.amount,
      "Cannot withdraw more than you provide"
    );

    AssetConfig memory assetConfig = _assetConfigs[position.asset.token];

    (uint assetPrice, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      position.asset.token,
      DEFAULT_TIME_FRAME
    );

    uint collateralPrice;
    uint32 multiplier;
    {
      CollateralPriceInfo memory info = ICollateralOracle(lookup(ROLE_COLLATERAL_ORACLE))
        .queryCollateralPrice(collateral.token, DEFAULT_TIME_FRAME);
      collateralPrice = info.price;
      multiplier = info.multiplier;
    }
    if (assetConfig.endPrice > 0) multiplier = PERMILLE;

    uint8 assetDecimals = IERC20Metadata(position.asset.token).decimals();
    uint8 collateralDecimals = IERC20Metadata(collateral.token).decimals();
    uint collateralAmount = position.collateral.amount - collateral.amount;
    {
      uint minCollateralAmount = uint(position.asset.amount)
        .calcAmount(assetPrice, collateralDecimals, collateralPrice, assetDecimals)
        .mulPerMillion(multiplier, assetConfig.minCollateralRatio);

      require(
        minCollateralAmount <= collateralAmount,
        "Cannot withdraw collateral over than minimum collateral ratio"
      );
    }

    if (collateralAmount == 0 && position.asset.amount == 0) {
      _removePosition(idx);
    } else {
      position.collateral.amount = collateralAmount;
      _positions[idx] = position;
    }

    ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
      idx,
      collateral.amount,
      msg.sender
    );

    emit Withdraw(idx, collateral.token, collateral.amount);
  }

  function mint(uint idx, Asset memory asset, ShortParams memory shortParams) public {
    uint mintAmount = asset.amount;
    Position memory position = _positions[idx];
    require(msg.sender == position.owner, "unauthorized");
    require(position.asset.token == asset.token, "Wrong asset");
    require(asset.amount > 0, "Amount dose not allow 0");
    require(position.isShort == shortParams.isShort, "ShortParams is invalid");

    // assert the asset migrated
    AssetConfig memory assetConfig = _assetConfigs[position.asset.token];

    uint collateralPrice;
    uint32 multiplier;
    {
      CollateralPriceInfo memory info = ICollateralOracle(lookup(ROLE_COLLATERAL_ORACLE))
        .queryCollateralPrice(position.collateral.token, DEFAULT_TIME_FRAME);
      collateralPrice = info.price;
      multiplier = info.multiplier;
    }
    (uint assetPrice, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      asset.token,
      DEFAULT_TIME_FRAME
    );
    uint8 assetDecimals = IERC20Metadata(asset.token).decimals();
    uint8 collateralDecimals = IERC20Metadata(position.collateral.token).decimals();

    require(
      assetPrice *
        (mintAmount + position.asset.amount) *
        10 ** collateralDecimals *
        multiplier *
        assetConfig.minCollateralRatio <=
        collateralPrice * position.collateral.amount * 10 ** assetDecimals * PER_MILLION,
      "Cannot mint asset over than min collateral ratio"
    );

    position.asset.amount += mintAmount;
    _positions[idx] = position;

    if (shortParams.isShort) {
      address dm = lookup(ROLE_DEX_MANAGER);
      IZAsset(position.asset.token).mint(dm, mintAmount);
      IDexManager(dm).shortSwap(asset.token, msg.sender, mintAmount, shortParams);

      IStaking(lookup(ROLE_STAKING)).increaseShortToken(msg.sender, asset.token, mintAmount);
    } else {
      IZAsset(asset.token).mint(msg.sender, mintAmount);
    }

    emit Mint(idx, asset.token, asset.amount);
  }

  function burn(uint idx, Asset memory asset) public {
    uint burnAmount = asset.amount;
    Position memory position = _positions[idx];
    require(position.asset.token == asset.token, "Wrong asset");
    require(burnAmount > 0, "Amount dose not allow 0");

    AssetConfig memory assetConfig = _assetConfigs[position.asset.token];
    require(position.asset.amount >= burnAmount, "Cannot burn asset more than you mint");

    uint collateralPrice;
    {
      CollateralPriceInfo memory info = ICollateralOracle(lookup(ROLE_COLLATERAL_ORACLE))
        .queryCollateralPrice(position.collateral.token, DEFAULT_TIME_FRAME);
      collateralPrice = info.price;
    }

    uint8 assetDecimals = IERC20Metadata(asset.token).decimals();
    uint8 collateralDecimals = IERC20Metadata(position.collateral.token).decimals();
    if (assetConfig.endPrice > 0) {
      decimal memory conversionRate = Decimal.min(
        Decimal.fromRatio(assetConfig.endPrice, collateralPrice),
        Decimal.fromRatio(
          position.collateral.amount * 10 ** assetDecimals,
          position.asset.amount * 10 ** collateralDecimals
        )
      );
      uint refundCollateral = Decimal.toUint(
        conversionRate.mulDivUint(burnAmount * 10 ** collateralDecimals, 10 ** assetDecimals)
      );

      position.asset.amount = position.asset.amount - burnAmount;
      position.collateral.amount -= refundCollateral;

      // NOTE: collateral does not complately be 0.
      // REVIEW: If collateral become lower than $1 and all assets is burned, position is closed.
      if (
        collateralPrice * position.collateral.amount < 100_000_000 * 10 ** collateralDecimals &&
        position.asset.amount == 0
      ) {
        _removePosition(idx);
      } else {
        _positions[idx] = position;
      }

      uint protocolFee = burnAmount
        .calcAmount(assetConfig.endPrice, collateralDecimals, collateralPrice, assetDecimals)
        .mulPermille(_protocolFeeRate);
      if (protocolFee > 0) {
        uint userRefund = refundCollateral - protocolFee;
        ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
          idx,
          refundCollateral,
          address(this)
        );
        IERC20(position.collateral.token).safeTransfer(lookup(ROLE_COLLECTOR), protocolFee);
        IERC20(position.collateral.token).safeTransfer(msg.sender, userRefund);
      }
    } else {
      require(msg.sender == position.owner, "unauthorized");
      (uint assetPrice, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
        asset.token,
        DEFAULT_TIME_FRAME
      );
      uint protocolFee = burnAmount
        .calcAmount(assetPrice, collateralDecimals, collateralPrice, assetDecimals)
        .mulPermille(_protocolFeeRate);
      if (protocolFee > 0) {
        ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
          idx,
          protocolFee,
          address(this)
        );

        IERC20(position.collateral.token).safeTransfer(lookup(ROLE_COLLECTOR), protocolFee);
        position.collateral.amount = position.collateral.amount - protocolFee;
        position.asset.amount = position.asset.amount - burnAmount;
        _positions[idx] = position;
      }
    }

    if (position.isShort)
      IStaking(lookup(ROLE_STAKING)).decreaseShortToken(msg.sender, asset.token, burnAmount);

    IZAsset(position.asset.token).burn(msg.sender, burnAmount);
    emit Burn(idx, asset.token, asset.amount);
  }

  function liquidation(Asset memory asset, uint idx) external {
    Position memory position = _positions[idx];
    require(position.asset.token == asset.token, "Wrong asset");
    require(asset.amount > 0, "Amount dose not allow 0");

    AssetConfig memory assetConfig = _assetConfigs[asset.token];
    require(assetConfig.asset != address(0), "No asset data stored");

    require(
      position.asset.amount >= asset.amount,
      "Cannot liquidate more than the position amount"
    );

    uint8 assetDecimals = IERC20Metadata(asset.token).decimals();
    uint8 collateralDecimals = IERC20Metadata(position.collateral.token).decimals();
    (uint assetPrice, ) = IPriceOracle(lookup(ROLE_PRICE_ORACLE)).queryAssetPrice(
      asset.token,
      DEFAULT_TIME_FRAME
    );
    uint collateralPrice;
    {
      CollateralPriceInfo memory info = ICollateralOracle(lookup(ROLE_COLLATERAL_ORACLE))
        .queryCollateralPrice(position.collateral.token, DEFAULT_TIME_FRAME);
      require(!info.isRevoked, "The collateral asset provided is no longer valid");
      collateralPrice = info.price;
      require(
        assetPrice *
          position.asset.amount *
          10 ** collateralDecimals *
          assetConfig.minCollateralRatio *
          info.multiplier >=
          collateralPrice * position.collateral.amount * 10 ** assetDecimals * PER_MILLION,
        "Cannot liquidate a safely collateralized position"
      );
    }

    uint returnCollateralAmount;
    uint refundAssetAmount;
    {
      uint32 discount = uint32(
        MathUtils.max(
          PERMILLE - assetConfig.liquidationDiscount,
          2 * PERMILLE - assetConfig.minCollateralRatio
        )
      );

      // Convert asset value in discounted collateral unit
      uint assetValueInDiscountedCollateralAsset = uint(asset.amount)
        .calcAmount(assetPrice, collateralDecimals, collateralPrice, assetDecimals)
        .divPermille(discount);

      // Cap return collateral amount to position collateral amount
      // If the given asset amount exceeds the amount required to liquidate position,
      // left asset amount will be refunded to the executor.
      if (assetValueInDiscountedCollateralAsset > position.collateral.amount) {
        refundAssetAmount =
          asset.amount -
          uint(position.collateral.amount)
            .calcAmount(collateralPrice, assetDecimals, assetPrice, collateralDecimals)
            .mulPermille(discount);
        if (refundAssetAmount > 0) {
          IERC20(asset.token).safeTransfer(msg.sender, refundAssetAmount);
        }
        returnCollateralAmount = position.collateral.amount;
      } else {
        refundAssetAmount = 0;
        returnCollateralAmount = assetValueInDiscountedCollateralAsset;
      }
    }

    uint liquidatedAssetAmount = asset.amount - refundAssetAmount;
    {
      uint leftAssetAmount = position.asset.amount - liquidatedAssetAmount;
      uint leftCollateralAmount = position.collateral.amount - returnCollateralAmount;
      if (leftCollateralAmount == 0) {
        _removePosition(idx);
      } else if (leftAssetAmount == 0) {
        _removePosition(idx);
        ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
          idx,
          leftCollateralAmount,
          position.owner
        );
      } else {
        position.collateral.amount = leftCollateralAmount;
        position.asset.amount = leftAssetAmount;
        _positions[idx] = position;
      }
    }

    if (liquidatedAssetAmount > 0) IZAsset(asset.token).burn(msg.sender, liquidatedAssetAmount);

    uint protocolFee = liquidatedAssetAmount
      .calcAmount(assetPrice, collateralDecimals, collateralPrice, assetDecimals)
      .mulPermille(_protocolFeeRate);
    uint returnFeeCollectedCollateralAmount = returnCollateralAmount - protocolFee;

    // return collateral to liquidator
    ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
      idx,
      returnFeeCollectedCollateralAmount,
      msg.sender
    );

    if (protocolFee > 0) {
      ICollateralManager(lookup(ROLE_COLLATERAL_MANAGER)).decreaseCollateral(
        idx,
        protocolFee,
        lookup(ROLE_COLLECTOR)
      );
    }

    if (position.isShort) {
      // To prevent deep stack
      IStaking(lookup(ROLE_STAKING)).decreaseShortToken(
        position.owner,
        asset.token,
        liquidatedAssetAmount
      );
    }

    emit Liquidation(idx, liquidatedAssetAmount);
  }

  function depositAndMint(
    uint idx,
    Asset memory collateral,
    Asset memory asset,
    ShortParams memory shortParams
  ) external {
    deposit(idx, collateral);
    mint(idx, asset, shortParams);
  }

  function burnAndWithdraw(uint idx, Asset memory collateral, Asset memory asset) external {
    burn(idx, asset);
    withdraw(idx, collateral);
  }

  function queryAssetConfig(address assetToken) external view returns (AssetConfig memory) {
    return _assetConfigs[assetToken];
  }

  function queryPosition(uint idx) external view returns (Position memory res) {
    Position memory position = _positions[idx];
    if (position.owner == msg.sender) res = position;
  }

  function queryUserPositions(address owner) external view returns (Position[] memory) {
    uint[] memory idxs = _userPositionList[owner];
    Position[] memory res = new Position[](idxs.length);
    for (uint i = 0; i < idxs.length; i++) {
      Position memory position = _positions[idxs[i]];
      if (position.owner != msg.sender) return new Position[](0);
      res[i] = position;
    }
    return res;
  }
}

