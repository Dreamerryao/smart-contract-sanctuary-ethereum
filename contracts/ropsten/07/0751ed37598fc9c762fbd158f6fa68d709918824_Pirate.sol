pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";
import "./TraitSet.sol";

contract PirateMetadataBuilder is Ownable {
    struct Artwork {
        address[] addresses;
        uint256 map;
    }

    enum TraitType {
        Beard,
        Body,
        Clothing,
        Face,
        Hairs,
        Hat,
        Special,
        Unique,
        Wing
    }

    using Address for address;

    Artwork private _artwork;

    function build(uint256 tokenId, TraitSet calldata traits) external view returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        abi.encodePacked(
                            '{"image":',
                            '"data:image/svg+xml;base64,',
                            Base64.encode(_svgFrom(traits)),
                            '","description":"TODO","name":"',
                            _nameOf(tokenId, traits),
                            '","attributes":[',
                            _attributeList(traits),
                            "]}"
                        )
                    )
                )
            );
    }

    function setArtwork(Artwork calldata artwork) external onlyOwner {
        _artwork = artwork;
    }

    function _artworkOf(TraitType traitType) private view returns (address) {
        return _artwork.addresses[(_artwork.map >> ((8 - uint256(traitType)) * 3)) & 0x07];
    }

    function _attributeList(TraitSet calldata traits) private pure returns (bytes memory) {
        if (traits.unique != Unique.None) {
            return hex"";
        }

        bool isMale = traits.gender == Gender.Male;

        return
            abi.encodePacked(
                _wrapWing(traits.wing),
                isMale ? _wrapMaleBody(traits.body) : _wrapFemaleBody(traits.body),
                isMale ? _wrapMaleClothing(traits.clothing) : _wrapFemaleClothing(traits.clothing),
                isMale ? _wrapMaleHairs(traits.hairs) : _wrapFemaleHairs(traits.hairs),
                _wrapBeard(traits.beard),
                // Nest encodePacked as workaround for
                //  CompilerError: Stack too deep when compiling inline assembly:
                //  Variable value0 is 1 slot(s) too deep inside the stack.
                abi.encodePacked(
                    _wrapHairColor(traits.hairColor),
                    isMale ? _wrapMaleHat(traits.hat) : _wrapFemaleHat(traits.hat),
                    isMale ? _wrapMaleFace(traits.face) : _wrapFemaleFace(traits.face),
                    _wrapSpecial(traits.special),
                    _wrapGender(traits.gender)
                )
            );
    }

    function _bitmapFor(
        TraitType traitType,
        uint8 traitId,
        TraitSet calldata traits
    ) private view returns (bytes memory) {
        if (traitType != TraitType.Unique && traits.unique != Unique.None) {
            return hex"";
        }

        string memory typeName;

        if (traitType == TraitType.Beard) {
            if (traitId == uint8(Beard.None)) {
                return hex"";
            }

            typeName = traitId == uint8(Beard.Dreaded)
                ? string(abi.encodePacked("beard", Strings.toString(uint256(traits.hairColor))))
                : "beard";
        } else if (traitType == TraitType.Body) {
            if (traits.gender == Gender.Male) {
                if (traitId == uint8(MaleBody.None)) {
                    return hex"";
                }

                typeName = "maleBody";
            } else {
                typeName = "femaleBody";
            }
        } else if (traitType == TraitType.Clothing) {
            if (traits.special != Special.None) {
                return hex"";
            }

            typeName = traits.gender == Gender.Male ? "maleClothing" : "femaleClothing";
        } else if (traitType == TraitType.Face) {
            if (traitId == uint8(MaleFace.None) || traitId == uint8(FemaleFace.None)) {
                return hex"";
            }

            typeName = traits.gender == Gender.Male ? "maleFace" : "femaleFace";
        } else if (traitType == TraitType.Hairs) {
            if (traits.gender == Gender.Male) {
                if (traitId == uint8(MaleHairs.None)) {
                    return hex"";
                }

                typeName = traitId == uint8(MaleHairs.Dreaded)
                    ? string(abi.encodePacked("maleHairs", Strings.toString(uint256(traits.hairColor))))
                    : "maleHairs";
            } else {
                typeName = "femaleHairs";
            }
        } else if (traitType == TraitType.Hat) {
            if (traitId == uint8(MaleHat.None) || traitId == uint8(FemaleHat.None)) {
                return hex"";
            }

            typeName = traits.gender == Gender.Male ? "maleHat" : "femaleHat";
        } else if (traitType == TraitType.Special) {
            if (traitId == uint8(Special.None)) {
                return hex"";
            }

            typeName = "special";
        } else if (traitType == TraitType.Unique) {
            if (traitId == uint8(Unique.None)) {
                return hex"";
            }

            typeName = "unique";
        } else if (traitType == TraitType.Wing) {
            if (traitId == uint8(Wing.None)) {
                return hex"";
            }

            typeName = "wing";
        } else {
            revert("Unexpected TraitType value");
        }

        return
            abi.encodePacked(
                hex"89504E470D0A1A0A0000000D494844520000005400000054",
                abi.decode(
                    _artworkOf(traitType).functionStaticCall(
                        abi.encodeWithSignature(string(abi.encodePacked(typeName, Strings.toString(traitId), "()")))
                    ),
                    (bytes)
                ),
                hex"0000000049454E44AE426082"
            );
    }

    function _colorFilter(TraitType traitType, TraitSet calldata traits) private pure returns (string memory) {
        if (
            (traitType != TraitType.Beard && traitType != TraitType.Hairs) ||
            traits.hairColor == HairColor.Blue ||
            (traits.gender == Gender.Male &&
                ((traitType == TraitType.Beard && (traits.beard == Beard.None || traits.beard == Beard.Dreaded)) ||
                    (traitType == TraitType.Hairs &&
                        (traits.hairs == uint8(MaleHairs.None) || traits.hairs == uint8(MaleHairs.Dreaded)))))
        ) {
            return "";
        }

        uint256 filters = 0x00_00_21_bd_3b_38_ca_8e_8e_a8_a3_5b_00_00_00_00_00_ab;
        uint256 step = uint8(traits.hairColor) * 3;

        return
            string(
                abi.encodePacked(
                    ' style="filter:hue-rotate(',
                    Strings.toString(uint8(filters >> ((17 - step) << 3))),
                    "deg) saturate(",
                    Strings.toString(uint8(filters >> ((16 - step) << 3))),
                    "%) brightness(",
                    Strings.toString(uint8(filters >> ((15 - step) << 3))),
                    '%);"'
                )
            );
    }

    function _jsonTrait(
        string memory name,
        string memory value,
        bool isLast
    ) private pure returns (bytes memory) {
        return abi.encodePacked('{"trait_type":"', name, '","value":"', value, '"}', isLast ? "" : ",");
    }

    function _nameOf(uint256 tokenId, TraitSet calldata traits) private pure returns (string memory) {
        if (traits.unique == Unique.None) {
            return string(abi.encodePacked("Pirate #", Strings.toString(tokenId)));
        }

        string[11] memory names = [
            "Angel of Death",
            "Captain Jack",
            "Cthulhu",
            "Davy Jones",
            "Flame Demon",
            "Golden Bones",
            "Gold Robot",
            "Medusa",
            "Warlord",
            "Young Jack",
            "Zombie"
        ];

        return names[uint8(traits.unique) - 1];
    }

    function _svgFrom(TraitSet calldata traits) private view returns (bytes memory) {
        return
            abi.encodePacked(
                '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 84 84" width="100%" height="100%">',
                _svgImage(TraitType.Wing, uint8(traits.wing), traits),
                _svgImage(TraitType.Body, traits.body, traits),
                _svgImage(TraitType.Clothing, traits.clothing, traits),
                _svgImage(TraitType.Hairs, traits.hairs, traits),
                _svgImage(TraitType.Beard, uint8(traits.beard), traits),
                _svgImage(TraitType.Face, traits.face, traits),
                _svgImage(TraitType.Special, uint8(traits.special), traits),
                _svgImage(TraitType.Hat, traits.hat, traits),
                _svgImage(TraitType.Unique, uint8(traits.unique), traits),
                "<style>image{image-rendering:pixelated;}</style></svg>"
            );
    }

    function _svgImage(
        TraitType traitType,
        uint8 traitId,
        TraitSet calldata traits
    ) private view returns (bytes memory) {
        bytes memory data = _bitmapFor(traitType, traitId, traits);

        if (data.length == 0) {
            return hex"";
        }

        return
            abi.encodePacked(
                '<image href="data:image/png;base64,',
                Base64.encode(data),
                '" width="100%" height="100%"',
                _colorFilter(traitType, traits),
                "/>"
            );
    }

    function _wrapBeard(Beard value) private pure returns (bytes memory) {
        if (value == Beard.None) {
            return hex"";
        }

        string[6] memory names = ["Long", "Short", "Handlebar", "Braid", "Goatee", "Dreaded"];

        return _jsonTrait("Beard", names[uint8(value) - 1], false);
    }

    function _wrapFemaleBody(uint8 value) private pure returns (bytes memory) {
        string[7] memory names = ["Natural", "Tan", "Dark", "Dark Tan", "Deep Tan", "Pale", "Shadow"];

        return _jsonTrait("Body", names[value], false);
    }

    function _wrapFemaleClothing(uint8 value) private pure returns (bytes memory) {
        string[9] memory names = [
            "Black Shirt",
            "White Shirt",
            "Blue Blouse",
            "White Blouse",
            "Bodysuit",
            "Deckhand",
            "Leaves",
            "Steel Plate Armor",
            "Gown"
        ];

        return _jsonTrait("Clothing", names[value], false);
    }

    function _wrapFemaleFace(uint8 value) private pure returns (bytes memory) {
        if (value == uint8(FemaleFace.None)) {
