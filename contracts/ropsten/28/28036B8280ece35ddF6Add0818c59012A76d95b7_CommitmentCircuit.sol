pragma solidity =0.8.4;

import "./RP/MathRP.sol";
import "./EC/EllipticCurveInterface.sol";

contract SchnorrSignature is MathRP {

    function SchnorrSignatureVerify
    (
        string memory message, 
        PointEC memory publicKey,
        PointEC memory ecR,
        uint256 s
    ) 
    public view returns (bool)
    {
        uint256 messageHash;
        PointEC memory ecG;
        PointEC memory ecLeft;
        PointEC memory ecRight;

        require(
            EllipticCurveInterface.eIsOnCurve(publicKey.x, publicKey.y) && 
            EllipticCurveInterface.eIsOnCurve(ecR.x, ecR.y),
            "Invalid input parametrs to verify the Schnorr signature"
        );

        // c = H (X, R, m)
        messageHash = uint256(sha256(abi.encodePacked(
            publicKey.x, publicKey.y,
            ecR.x, ecR.y,
            message        
        )));
        console.log("c", messageHash);
        //s*G
        ecG.x = EllipticCurveInterface.gx;
        ecG.y = EllipticCurveInterface.gy;
        
        (ecLeft.x, ecLeft.y) = EllipticCurveInterface.eMul(s, ecG.x, ecG.y);
        console.log("ecLeft:", ecLeft.x, ecLeft.y);
        //R + c*X
        (ecRight.x, ecRight.y) = EllipticCurveInterface.eMul(messageHash, publicKey.x, publicKey.y);
        (ecRight.x, ecRight.y) = EllipticCurveInterface.eAdd(ecRight.x, ecRight.y, ecR.x, ecR.y);
        console.log("ecRight:", ecRight.x, ecRight.y);
        return _equalPointEC(ecLeft, ecRight);
    }
}

