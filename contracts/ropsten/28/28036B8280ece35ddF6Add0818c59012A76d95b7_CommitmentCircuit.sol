pragma solidity =0.8.4;

import "../EC/EllipticCurveInterface.sol";
import "hardhat/console.sol";

contract MathRP is EllipticCurveInterface {
    struct PointEC {
        uint256 x;
        uint256 y;
    }

    function _hashPPToChallange (PointEC memory _pEC_1, PointEC memory _pEC_2) internal pure returns (uint256) {
        uint256 hashVal = uint256( sha256(abi.encode(_pEC_1.x, _pEC_1.y, _pEC_2.x, _pEC_2.y)));
        return hashVal;
    }

    function _count_bit_number (uint256 _arg) internal pure returns (uint256 _n)
    {
        uint16 r;
        if (_arg >= 2**128) { _arg >>= 128; r += 128; }
        if (_arg >= 2**64) { _arg >>= 64; r += 64; }
        if (_arg >= 2**32) { _arg >>= 32; r += 32; }
        if (_arg >= 2**16) { _arg >>= 16; r += 16; }
        if (_arg >= 2**8) { _arg >>= 8; r += 8; }
        if (_arg >= 2**4) { _arg >>= 4; r += 4; }
        if (_arg >= 2**2) { _arg >>= 2; r += 2; }
        if (_arg >= 2**1) { _arg >>= 1; r += 1; }
        _n = ++r;        
    }

    function _matrixOfProductMod (uint256 _v, uint256 _n, uint256 _pp) internal pure returns (uint256[] memory) {
        uint256[] memory _matr = new uint256[](_n);
        uint256 _prod =1;
        _matr[0] = _prod;
        for (uint i = 1; i < _n; i++ ){
            _prod = (mulmod(_prod, _v, _pp));
            _matr[i] = _prod;
        }
        return _matr;
    }

    function _genMatrixNegProd(uint256[] memory _matrixArg, uint256 _n) internal pure returns (uint256[] memory){
        uint256 uV;
        require(_n >0, "_genMatrixNegProd._matrixArg _genMatrixNegProd must not be empty");
        require(_n == _matrixArg.length, "_matrixArg is false");
        uint256[] memory _matrixRez = new uint256[](_n);
        for (uint8 i = 0; i < _n; i++) {
            uV = _negProdMod(_matrixArg[i], pp);
            _matrixRez[i] = uV;
        }
        return _matrixRez;
    }

    function _productScalarMatrix
    (
        uint256 [] memory _matrixOne, 
        uint256 [] memory _matrixTwo
    ) internal pure returns (uint256[] memory _matrixScalar){
        uint256 _n = _matrixOne.length;
        require(_n > 0, "_productScalarMatrix ._matrixScalar must not be empty");
        require (_n == _matrixTwo.length, "rang of _matrixOne and _matrixTwo are different");        
        _matrixScalar = new uint256[](_n);
        for(uint8 i = 0; i < _n; i++) {
            _matrixScalar[i] = mulmod(_matrixOne[i], _matrixTwo[i], pp);
        }
    }

    function _productMatrixOnScalar 
    (
        uint256 _argScalar,
        uint256[] memory _argMatrix
    ) 
    internal pure returns (uint256[] memory) 
    {
        uint256[] memory _matrixRez;
        uint256 uV;
        uint256 _n = _argMatrix.length;
        require(_n > 0, "_productMatrixScalar._matixArg must not be empty");
        _matrixRez = new uint256[](_n);
        for(uint8 i = 0; i < _n; i++){
            uV = mulmod(_argScalar, _argMatrix[i], pp);
            _matrixRez[i] = uV;
        }
        return _matrixRez;
    }

    function _scalarProductScalarMatrToPointECMatr (
        uint256[] memory _matrixScalar,
        PointEC[] memory _matrixPEC
    ) internal pure returns (PointEC memory _pECRez){
        PointEC memory _pEC;
        uint256 _n = _matrixScalar.length;
        require(_n > 0, "_scalarProductScalarMatrToPointECMatr._matrixScalar must not be empty");
        require (_n == _matrixPEC.length, "rang of scalar matrix and point matrix are different");
        (_pECRez.x, _pECRez.y) = (0, 0);
        for (uint8 i = 0; i < _n; i++) {
            (_pEC.x, _pEC.y) = eMul(_matrixScalar[i], _matrixPEC[i].x, _matrixPEC[i].y);
            (_pECRez.x, _pECRez.y) = eAdd(_pECRez.x, _pECRez.y, _pEC.x, _pEC.y);
        }
    }
    

    function _scalarProductScalarMatrToScalarMatr 
    (
        uint256 [] memory _matrixOne, 
        uint256 [] memory _matrixTwo
    )
    internal pure returns (uint256)
    {
        uint256 _len = _matrixOne.length;
        require(_len == _matrixTwo.length, "rang of scalar matrices are different");
        uint256 _product = 0;
        for (uint8 i = 0; i < _len; i++) {
            _product = addmod(_product, mulmod(_matrixOne[i],_matrixTwo[i], pp), pp);
        }
        return _product;
    }

    function _negMod (uint256 _v, uint256 _pp) internal pure returns (uint256 _negM) {
        _negM = mulmod(1, _v, _pp);
        _negM = _pp - _negM;
    }

    function _negProdMod (uint256 _v, uint256 _pp) internal pure returns (uint256) {
        return invMod(_v, _pp);
    }

    function _genPointEC (bytes memory _paramSHA) 
    internal 
    pure 
    returns (PointEC memory _pEC)
    {
        uint256 _x;
        uint256 _y;
        uint8 _i;
        while (_i<256){
            _x = addmod(uint256(0), uint256(sha256(abi.encodePacked(_i, _paramSHA))), pp);
            _y = eDeriveY(2, _x);
            if (eIsOnCurve(_x, _y) == true) {
                _pEC.x = _x;
                _pEC.y = _y;
                return _pEC;
            }
            _i++;                
        }
    }

    function _genMatrixECPoints (bytes memory _paramSHA, uint256 _n) 
    internal 
    view
    returns (PointEC[] memory ) 
    {
        PointEC[] memory _matrixECPoints = new PointEC[](_n);
        console.log("_genMatrixECPoints");
        for (uint8 i = 0; i < _n; i++){
            // console.logBytes(abi.encodePacked(_paramSHA, i));
            _matrixECPoints[i] = _genPointEC(abi.encodePacked(_paramSHA, i));
        }
        return _matrixECPoints;
    }

    function _equalPointEC
    (
        PointEC memory _pEC1, 
        PointEC memory _pEC2
    ) internal pure returns (bool _isEq)
    {
        _isEq = (_pEC1.x == _pEC2.x) && (_pEC1.y == _pEC2.y);
    }
}

