import "../interfaces/external/IERC20Extended.sol";

contract FundraisingVault is IFundraisingVault {
	using SafeERC20 for IERC20Extended;
	mapping(address => uint256) public userInfo;
	VaultInfo info;
	address public immutable usdcAddress;
	address public immutable priceFeed;
	address public immutable manager;
	bool public userClaimBack;
	bool public vaultStatus;

	uint256 public constant DEADLINE = 72 hours;
	uint256 public immutable startTime;
	uint256 public totalRaised;
	uint256 public endTime;

	constructor(
		VaultInfo memory _info,
		address _usdcAddress,
		address _priceFeed,
		address _manager
	) {
		endTime = block.timestamp + _info.fundraisingPeriod * 24 hours;
		startTime = block.timestamp;
		usdcAddress = _usdcAddress;
		priceFeed = _priceFeed;
		manager = _manager;
		_info.capacityHit = 50000;
		info = _info;
	}

	function deposit(uint256 amount) external override {
		uint256 usdcDecimal = IERC20Extended(usdcAddress).decimals();
		require(startTime < block.timestamp, "deposit is not started");
		require(endTime > block.timestamp, "deposit ended");
		require(
			totalRaised / 10**usdcDecimal < info.capacityHit,
			"deposit failed due to tpHit"
		);
		IERC20Extended(usdcAddress).safeTransferFrom(
			msg.sender,
			address(this),
			amount
		);
		totalRaised += amount;
		userInfo[msg.sender] += amount;
		emit Deposit(msg.sender, amount);
	}

	function refund() external override {
		require(userInfo[msg.sender] > 0, "Nothing to refund");
		require(endTime + DEADLINE < block.timestamp, "Not ready to refund");
		userClaimBack = true;
		userInfo[msg.sender] = 0;
		IERC20(usdcAddress).transfer(msg.sender, userInfo[msg.sender]);
	}

	// /**
	//  * @dev check if able to open position
	//  * @return 0 => current price is higher, 1 => able to open position, 2 => should be liquidated
	//  */

	// function isOpenPosition() external returns (uint256) {
	// 	require(
	// 		block.timestamp > endTime && block.timestamp < endTime + DEADLINE,
	// 		"Unable to trade!"
	// 	);
	// 	require(userClaimBack == false, "vault is closed!");

	// 	(, int256 currentPrice, , , ) = IAggregatorV3Interface(priceFeed)
	// 		.latestRoundData();
	// 	uint256 decimal = IAggregatorV3Interface(priceFeed).decimals();
	// 	uint256 currentPriceWithoutDecimal = uint256(currentPrice) /
	// 		10**decimal;

	// 	if (currentPriceWithoutDecimal > info.entryPrice) {
	// 		return 0;
	// 	} else if (
	// 		(info.entryPrice * 98) / 100 < currentPriceWithoutDecimal &&
	// 		currentPriceWithoutDecimal < info.entryPrice
	// 	) {
	// 		return 1;
	// 	} else if (currentPriceWithoutDecimal < info.liquidationPrice) {
	// 		return 2;
	// 	}
	// }

	/**
	 * @dev should calculate quantity of virtual token by following formula
	 * vAsset amount = AccountBalance / (Index price - (1- 0.0625) * Liquidation price)
	 */
	function withdrawToLeverageModule() external override {
		require(
			block.timestamp > endTime && block.timestamp < endTime + DEADLINE,
			"Unable to trade!"
		);
		require(userClaimBack == false, "vault is closed!");

		(, int256 currentPrice, , , ) = IAggregatorV3Interface(priceFeed)
			.latestRoundData();
		uint256 decimal = IAggregatorV3Interface(priceFeed).decimals();
		uint256 currentPriceWithoutDecimal = uint256(currentPrice) /
			10**decimal;
		if (currentPriceWithoutDecimal > info.entryPrice) {
			// current price > entry price
			// then wait
			emit WithdrawToLeverageModule("wait", currentPriceWithoutDecimal);
		} else if (
			(info.entryPrice * 98) / 100 < currentPriceWithoutDecimal &&
			currentPriceWithoutDecimal < info.entryPrice
		) {
			// entry * 0.98 < price < entry
			// execute trade

			/**
			 * open Long position
			 * calculate quantity of virtual token
			 * IPerpLeverageModule(tradingVault).trade();
			 */
			if (info.positionStatus == true) {
				// calculate vBalance using liquidity price
				uint256 vBalance = (totalRaised *
					currentPriceWithoutDecimal *
					10000) /
					(currentPriceWithoutDecimal *
						10000 -
						9375 *
						info.liquidationPrice);
				IERC20(usdcAddress).transfer(info.tradingVault, totalRaised);
				IPerpLeverageModule(info.tradingVault).deposit();
				uint256 vTokenToTrade = ((totalRaised + 1000 * 1e6) /
					(10**IERC20Extended(usdcAddress).decimals())) * 1e18;

				(uint256 deltaBase, uint256 deltaQuote) = IPerpLeverageModule(
					info.tradingVault
				).trade(vTokenToTrade, 0, info.positionStatus);

				emit WithdrawToLeverageModule(
					"long",
					currentPriceWithoutDecimal
				);
				// emit OpenPosition(deltaBase, deltaQuote, true);
			} else {
				// calculate vBalance using liquidity price
				// accValue / (indexP * (2 *  marginRatio + 1) - liqP*(marginRation + 1)) * indexP
				uint256 vBalance = (totalRaised *
					currentPriceWithoutDecimal *
					10000) /
					(currentPriceWithoutDecimal *
						11250 -
						10625 *
						info.liquidationPrice);

				IERC20(usdcAddress).transfer(info.tradingVault, totalRaised);
				IPerpLeverageModule(info.tradingVault).deposit();
				uint256 vTokenToTrade = (totalRaised /
					(10**IERC20Extended(usdcAddress).decimals())) * 1e18;

				(uint256 deltaBase, uint256 deltaQuote) = IPerpLeverageModule(
					info.tradingVault
				).trade(vTokenToTrade, 0, info.positionStatus);
				console.log("totalRaised", totalRaised);
				emit WithdrawToLeverageModule(
					"short",
					currentPriceWithoutDecimal
				);
			}
		} else if (currentPriceWithoutDecimal < info.liquidationPrice) {
			// let users claim back
			userClaimBack = true;
			emit WithdrawToLeverageModule(
				"liquidated",
				currentPriceWithoutDecimal
			);
		}
	}

	modifier onlyManager() {
		require(msg.sender == manager, "only manager");
		_;
	}
}

