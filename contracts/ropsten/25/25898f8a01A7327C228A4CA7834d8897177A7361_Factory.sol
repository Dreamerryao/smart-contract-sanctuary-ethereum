pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SignedSafeMath.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { ERC20 } from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";
import "./interfaces/external/perp-v2/IVault.sol";
import "./interfaces/external/perp-v2/IQuoter.sol";
import "./interfaces/external/perp-v2/IMarketRegistry.sol";
import "./interfaces/IOneVToken.sol";
import "./interfaces/external/perp-v2/IClearingHouse.sol";
import "./interfaces/external/perp-v2/IClearingHouseConfig.sol";
import "./interfaces/external/perp-v2/IAccountBalance.sol";

import { PreciseUnitMath } from "./lib/PreciseUnitMath.sol";

/**
 * @title PerpV2LeverageModule
 * @author 1v1t Protocol
 * @notice Smart contract that enables leveraged trading using the PerpV2 protocol.
 */
contract PerpV2LeverageModule is ReentrancyGuard {
	IERC20 public immutable collateralToken;
	mapping(address => uint256) userInfo;

	// Decimals of collateral token. We set this in the constructor for later reading
	uint8 internal immutable collateralDecimals;

	IVault public immutable perpVault;

	// PerpV2 contract which provides a trading API
	IClearingHouse public immutable perpClearingHouse;

	// PerpV2 contract which makes it possible to simulate a trade before it occurs
	IQuoter public immutable perpQuoter;

	// PerpV2 contract which provides a getter for baseToken UniswapV3 pools
	IMarketRegistry public immutable perpMarketRegistry;

	// PerpV2 contract which provides getters for base, quote, and owedRealizedPnl balances
	IAccountBalance public immutable perpAccountBalance;

	address public baseToken;
	address public manager;
	address public usdcAddress;
	address public fundraisingVault;
	address public immutable factoryAddress;

	struct ActionInfo {
		address baseToken; // Virtual token minted by the Perp protocol
		bool isLong; // When true, long position, when false, short position
		uint256 baseTokenAmount; // Base token quantity in 10**18 decimals
		uint256 oppositeAmountBound; // vUSDC pay or receive quantity bound (see `_createActionInfoNotional` for details)
	}

	/**
	 * @dev Emitted on deposit (not issue or redeem)
	 * @param _collateralToken      Token being deposited as collateral (USDC)
	 * @param _amountDeposited      Amount of collateral being deposited into Perp
	 */
	event CollateralDeposited(
		IERC20 indexed _collateralToken,
		uint256 indexed _amountDeposited
	);

	/* ============ Constructor ============ */

	/**
	 * @dev Sets external PerpV2 Protocol contract addresses. Sets `collateralToken` and `collateralDecimals`
	 * to the Perp vault's settlement token (USDC) and its decimals, respectively.
	 *
	 * @param _perpVault                Address of Perp Vault contract
	 * @param _perpQuoter               Address of Perp Quoter contract
	 * @param _perpMarketRegistry       Address of Perp MarketRegistry contract
	 * @param _baseToken       Address of Perp base Token (i.e, vBTC)
	 */

	constructor(
		address _perpVault,
		address _perpQuoter,
		address _perpMarketRegistry,
		address _baseToken,
		address _usdcAddress,
		address _manager
	) {
		perpVault = IVault(_perpVault);
		address tempCollateralToken = IVault(_perpVault).getSettlementToken();
		collateralToken = IERC20(tempCollateralToken);
		collateralDecimals = ERC20(tempCollateralToken).decimals();
		perpClearingHouse = IClearingHouse(
			IVault(_perpVault).getClearingHouse()
		);
		perpAccountBalance = IAccountBalance(
			IVault(_perpVault).getAccountBalance()
		);
		perpMarketRegistry = IMarketRegistry(_perpMarketRegistry);
		perpQuoter = IQuoter(_perpQuoter);
		baseToken = _baseToken;
		manager = _manager;
		usdcAddress = _usdcAddress;
		factoryAddress = msg.sender;
	}

	/**
	 * @dev MANAGER ONLY: Deposits total amount of collateral token into the PerpV2 Vault
	 *
	 */
	function deposit() external nonReentrant {
		uint256 totalBalance = collateralToken.balanceOf(address(this));
		require(totalBalance > 0, "Deposit amount is 0");
		collateralToken.approve(address(perpVault), totalBalance);
		perpVault.deposit(address(collateralToken), totalBalance);
		emit CollateralDeposited(collateralToken, totalBalance);
	}

	function withdraw(address token, uint256 amountX10_D)
		external
		nonReentrant
	{
		perpVault.withdraw(token, amountX10_D);
		uint256 totalAmount = IERC20(usdcAddress).balanceOf(address(this));
		IERC20(usdcAddress).transfer(fundraisingVault, totalAmount);
	}

	/**
	 * @dev MANAGER ONLY: Allows manager to buy or sell perps to change exposure to the underlying baseToken.
	 * @param _baseQuantityUnits            Quantity of virtual token to trade in position units
	 * @param _quoteBoundQuantityUnits      Max/min of vQuote asset to pay/receive when buying or selling
	 * @param _isLong                        State of buy or sell // Quote to Base
	 */
	function trade(
		uint256 _baseQuantityUnits,
		uint256 _quoteBoundQuantityUnits,
		bool _isLong
	) external nonReentrant returns (uint256 base, uint256 quote) {
		ActionInfo memory actionInfo = _createAndValidateActionInfo(
			_baseQuantityUnits,
			_quoteBoundQuantityUnits,
			_isLong
		);

		(base, quote) = _executeTrade(actionInfo);
	}

	/**
	 * @dev Construct the ActionInfo struct for trading. This method takes POSITION UNIT amounts and passes to
	 *  _createActionInfoNotional to create the struct. If the _baseTokenQuantity is zero then revert. If
	 * the _baseTokenQuantity = -(baseBalance/setSupply) then close the position entirely. This method is
	 * only called from `trade` - the issue/redeem flow uses createActionInfoNotional directly.
	 *
	 * @param _baseQuantityUnits    Quantity of baseToken to trade in PositionUnits
	 * @param _quoteReceiveUnits    Quantity of quote to receive if selling base and pay if buying, in PositionUnits
	 * @param _isLong                State of buy or sell
	 *
	 * @return ActionInfo           Instance of constructed ActionInfo struct
	 */
	function _createAndValidateActionInfo(
		uint256 _baseQuantityUnits,
		uint256 _quoteReceiveUnits,
		bool _isLong
	) internal view returns (ActionInfo memory) {
		require(_baseQuantityUnits != 0, "Amount is 0");
		require(
			perpMarketRegistry.hasPool(baseToken),
			"Base token does not exist"
		);

		return
			ActionInfo({
				baseToken: baseToken,
				isLong: _isLong,
				baseTokenAmount: _baseQuantityUnits,
				oppositeAmountBound: _quoteReceiveUnits
			});
	}

	function _executeTrade(ActionInfo memory _actionInfo)
		internal
		returns (uint256 deltaBase, uint256 deltaQuote)
	{
		// When isBaseToQuote is true => short, false => long
		// When isExactInput is true => long false => short
		IClearingHouse.OpenPositionParams memory params = IClearingHouse
			.OpenPositionParams({
				baseToken: _actionInfo.baseToken,
				isBaseToQuote: !_actionInfo.isLong,
				isExactInput: _actionInfo.isLong,
				amount: _actionInfo.baseTokenAmount,
				oppositeAmountBound: 0,
				deadline: PreciseUnitMath.maxUint256(),
				sqrtPriceLimitX96: 0,
				referralCode: bytes32(0)
			});

		(deltaBase, deltaQuote) = IClearingHouse(perpClearingHouse)
			.openPosition(params);
	}

	/// @notice Close position on Perp
	function closePosition() external returns (uint256 base, uint256 quote) {
		IClearingHouse.ClosePositionParams memory params = IClearingHouse
			.ClosePositionParams({
				baseToken: baseToken,
				sqrtPriceLimitX96: 0,
				oppositeAmountBound: 0,
				deadline: PreciseUnitMath.maxUint256(),
				referralCode: bytes32(0)
			});

		(base, quote) = IClearingHouse(perpClearingHouse).closePosition(params);
	}

	function setFundraisingVault(address _fundRaisingVault) external {
		require(factoryAddress == msg.sender, "Only Factory allowed");
		fundraisingVault = _fundRaisingVault;
	}
}

