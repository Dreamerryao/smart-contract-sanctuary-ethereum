pragma solidity ^0.8.0;

import "./PerpV2LeverageModule.sol";
import "./protocol/FundraisingVault.sol";
import "./interfaces/IFactory.sol";

contract Factory is IFactory {
	VaultInfo[] public vaultInfos;
	// mapping(address => VaultInfo[]) public vaultsByUser;
	address public immutable perpVault;
	address public immutable perpQuoter;
	address public immutable perpMarketRegistry;
	address public immutable usdcAddress;
	uint256 public totalVaults;

	constructor(
		address _perpVault,
		address _perpQuoter,
		address _perpMarketRegistry,
		address _usdcAddress
	) {
		perpVault = _perpVault;
		perpQuoter = _perpQuoter;
		perpMarketRegistry = _perpMarketRegistry;
		usdcAddress = _usdcAddress;
	}

	/*
	 */
	function createNewVault(VaultInfo memory info, address _priceFeed)
		external
	{
		// check if vBalance is not over maximum value

		require(info.fundraisingPeriod <= 3, "invalid fundraising period");
		PerpV2LeverageModule tradingVault = _createTradingVault(info.baseToken);
		info.creator = msg.sender;
		info.tradingVault = address(tradingVault);

		FundraisingVault fundRaisingVault = _createFundraisingVault(
			info,
			_priceFeed,
			msg.sender
		);

		info.fundRaisingVault = address(fundRaisingVault);
		tradingVault.setFundraisingVault(address(fundRaisingVault));
		vaultInfos.push(info);
		// vaultsByUser[msg.sender].push(info);
		totalVaults++;
	}

	function _createTradingVault(address _baseToken)
		internal
		returns (PerpV2LeverageModule)
	{
		PerpV2LeverageModule tradingVault = new PerpV2LeverageModule(
			perpVault,
			perpQuoter,
			perpMarketRegistry,
			_baseToken,
			usdcAddress,
			msg.sender
		);
		return tradingVault;
	}

	function _createFundraisingVault(
		VaultInfo memory info,
		address _priceFeed,
		address creator
	) internal returns (FundraisingVault) {
		return new FundraisingVault(info, usdcAddress, _priceFeed, creator);
	}

	function getVaultInfos() external view returns (VaultInfo[] memory) {
		return vaultInfos;
	}

	// function getVaultByUser(address user)
	// 	external
	// 	view
	// 	returns (VaultInfo[] memory)
	// {
	// 	return vaultsByUser[user];
	// }
}

