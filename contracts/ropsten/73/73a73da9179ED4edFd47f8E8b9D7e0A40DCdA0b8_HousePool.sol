    // --- Start of Public Functions related to Farms and Virtual Token ---

    function calculateNewEVValue(uint256 _newAmount, int256 _newEV, address _toUser) public view returns(int256) {
        int256 newEVForUser = (int256(_newAmount) * _newEV) + (int256(deposits[_toUser]) * userEVTracker[_toUser]) / 
                              int256((_newAmount + deposits[_toUser]));
        return newEVForUser;
    }
    
    ///@notice getNextCooldownTimestamp function to get the next cooldown start timestamp of the user
    ///@param userCooldownTimestamp current timestamp
    ///@param amountToReceive amount to stake
    ///@param toAddress receiver of the stake benefits
    ///@param toBalance LpToken balance
    function getNextCooldownTimestamp(
        uint256 userCooldownTimestamp,
        uint256 amountToReceive,
        address toAddress,
        uint256 toBalance
    ) public view returns (uint256) {
        uint256 toCooldownTimestamp;
        if(cooldownActive == true) {
            toCooldownTimestamp = cooldownStartTimes[toAddress];
            if (toCooldownTimestamp == 0) {
                return 0;
            }
            uint256 minimalValidCooldownTimestamp = ((block.timestamp -
            coolDownSeconds) - (unstakeWindow));

            if (minimalValidCooldownTimestamp > toCooldownTimestamp) {
                toCooldownTimestamp = 0;
            }   else {
                    uint256 fromCooldownTimestamp = (minimalValidCooldownTimestamp >
                    userCooldownTimestamp)
                    ? block.timestamp
                    : userCooldownTimestamp;

            if (fromCooldownTimestamp < toCooldownTimestamp) {
                return toCooldownTimestamp;
            }   else {
                    toCooldownTimestamp =
                        (amountToReceive *
                            (block.timestamp)) + // block.timestamp
                            (toBalance * (toCooldownTimestamp)) / 
                        (amountToReceive + (toBalance));
                }
            }
        } 
        return toCooldownTimestamp;
    }

    // --- End of Public Functions related to  Farms and Virtual Token ---

    // --- Start of Public Functions related to Betting and pools ---

    ///@notice storeBetWithEVME function to place bets along with ME and EV values
    function storeBetWithEVME(
        bytes memory sig_,
        ValuesOfInterest memory voi_,
        BetInfo[] memory info
    ) public onlyValid(voi_, sig_) onlyRole(HOUSE_POOL_DATA_PROVIDER) {
        _setVoi(voi_);
        storeBets(info);
    }

    ///@notice storeBets function to store the bets placed
    ///@param betinformation information of the bets to be placed
    function storeBets(BetInfo[] memory betinformation)
        public
        onlyRole(HOUSE_POOL_DATA_PROVIDER)
    {
        for (uint256 i = 0; i < betinformation.length; i++) {
            pendingStakes += int256(betinformation[i].amount);
            updateAttributes();
            // TODO pending Commission
            betInfoWithId[betinformation[i].Id].parlay = betinformation[i].parlay;
            betInfoWithId[betinformation[i].Id].user = betinformation[i].user;
            betInfoWithId[betinformation[i].Id].Id = betinformation[i].Id;
            betInfoWithId[betinformation[i].Id].amount = betinformation[i].amount;
            betInfoWithId[betinformation[i].Id].result = betinformation[i].result;
            betInfoWithId[betinformation[i].Id].payout = betinformation[i].payout;
            betInfoWithId[betinformation[i].Id].commission = betinformation[i].commission;
            userBets[betinformation[i].user].push(betinformation[i].Id);
        }
    } 

    ///@notice updateBets function to update the bet information
    ///@param _Id, bet ids
    ///@param _payout payout amount of the respective betIDs
    function updateBets(
        uint256[] memory _Id,
        //int256[] memory _odds,
        //bool[] memory _parley,
        uint256[] memory _payout
    ) public onlyRole(HOUSE_POOL_DATA_PROVIDER) {
        for (uint256 i = 0; i < _Id.length; i++) {
            if(betInfoWithId[i].parlay == true) {
                betInfoWithId[_Id[i]].payout = _payout[i];
            }
        }
    }
    
    ///@notice settleBets function to settle bets
    ///@param _Id, bet Ids
    ///@param _result result of the respective betIDs
    function settleBets(uint256[] memory _Id, uint256[] memory _result)
        public
        onlyRole(HOUSE_POOL_DATA_PROVIDER)
    {
        for (uint256 i = 0; i < _Id.length; i++) {
            // Zero means house has lost.
            if (_result[i] == 1) {
                betInfoWithId[_Id[i]].result = 1;
                liquidity -= (betInfoWithId[_Id[i]].payout + betInfoWithId[_Id[i]].amount);
                stakedToken.transfer(
                    sportsBookContract,
                    betInfoWithId[_Id[i]].payout
                );
                pendingStakes -=
                    int256(betInfoWithId[_Id[i]].amount);
                    // TODO pending commission
            } else if (_result[i] == 3) {
                betInfoWithId[_Id[i]].result = 3;
                stakedToken.transfer(
                    sportsBookContract,
                    betInfoWithId[_Id[i]].amount
                );
                pendingStakes -=
                    int256(betInfoWithId[_Id[i]].amount);
                    // TODO pending commission
            } else if(_result[i] == 2) {
                betInfoWithId[_Id[i]].result = 2;
                liquidity += betInfoWithId[_Id[i]].amount ;
                pendingStakes -=
                    int256(betInfoWithId[_Id[i]].amount );
                    // TODO pending commission
            }
        }
    } 

    // --- End of Public Functions related to Betting and Pools ---

    // --- Start of Internal Functions related to Farms and Virtual Token ---

    ///@notice internal transfer function used by transfer function of ERC20
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        uint256 balanceOfFrom = balanceOf(from);
        FarmInfo memory farm = updateFarm();
        // Sender
        UserInfo storage sender = userInfo[from];
        sender.rewardDebt -= int256(
            (amount * farm.accRewardsPerShare) / ACC_REWARD_PRECISION
        );
        sender.amount -= amount;

        // Recipient
        if (from != to) {
            uint256 balanceOfTo = balanceOf(to);
            UserInfo storage receiver = userInfo[to];
            receiver.rewardDebt += int256(
                (amount * farm.accRewardsPerShare) / ACC_REWARD_PRECISION
            );
            receiver.amount += amount;
            userEVTracker[to] = calculateNewEVValue(amount, userEVTracker[from], to);
            uint256 previousSenderCooldown = cooldownStartTimes[from];
            cooldownStartTimes[to] = getNextCooldownTimestamp(
                previousSenderCooldown,
                amount,
                to,
                balanceOfTo
            );
            // if cooldown was set and whole balance of sender was transferred - clear cooldown
            if (balanceOfFrom == amount && previousSenderCooldown != 0) {
                cooldownStartTimes[from] = 0;
            }
        }
        super._transfer(from, to, amount);
    }

    ///@notice _afterTokenTransfer
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override(ERC20Upgradeable) {
        super._afterTokenTransfer(from, to, amount);
    }

    ///@notice _mint internal function to mint the tokens
    function _mint(address to, uint256 amount)
        internal
        override(ERC20Upgradeable)
    {
        super._mint(to, amount);
    }

    ///@notice _burn internal function  to burn the tokens
    function _burn(address account, uint256 amount)
        internal
        override(ERC20Upgradeable)
    {
        super._burn(account, amount);
    }

    // --- End of Internal Functiona related to Farms and Virtual Token ---

    // --- Start of Internal Functions related to Betting and Pools ---

    ///@notice updateAttributed internal function to update the bet attributes once the bet is placed
    function updateAttributes() internal {
        totalValueLocked = uint256(int256(liquidity) +(voi.expectedValue) - pendingStakes);
        lpTokenPrice = (totalValueLocked * 10**MAX_PRECISION) / totalSupply();    
        liquidityWithAdjustedME = int256(totalValueLocked) - (voi.maxExposure);
    }

    ///@notice _setTokenPrice internal function to set the tokenPrice. 
    function _setTokenPrice() internal {
        if (totalSupply() != 0) {
            lpTokenPrice =
                (uint256(totalValueLocked) * 10**MAX_PRECISION) /
                totalSupply();
        }
    }
    
    ///@notice internal function to set the token withdrawal price
    function _setTokenWithdrawalPrice() internal {
        if (totalSupply() != 0) {
            lpTokenWithdrawalPrice =
                (liquidity * 10**MAX_PRECISION) /
                totalSupply();
        }
    }
    ///@notice internal function to update TVL whenever new ev is updated
    ///@param _expectedValue new ev value 
    function _updateTVL(int256 _expectedValue) internal {
        //TVL += expectedValue; // TVL also should be in base units and currency dependent
        totalValueLocked = uint256(
            int256(liquidity) +
                _expectedValue -
                pendingStakes         
        );
    }

    ///@notice internal function to set VOI
    ///@param _voi me and ev are passed
    function _setVoi(ValuesOfInterest memory _voi) internal {
        if (_voi.expectedValue != voi.expectedValue) {
            _setEV(_voi.expectedValue);
        }
        if (_voi.maxExposure != voi.maxExposure) {
            _setME(_voi.maxExposure);
        }
    }
    ///@notice internal function to set Maximum exposure
    ///@param _exposure new exposure value to set
    function _setME(int256 _exposure) internal {
        voi.maxExposure = _exposure;
    }
    ///@notice internal function to set EV
    ///@param _newEV new ev value
    function _setEV(int256 _newEV) internal {
        _updateTVL(_newEV);
        voi.expectedValue = _newEV;
        _setTokenPrice();
    }

    // --- End of Internal Functions related to Betting and Pools ---

    // *********************************************************************************************************************************

}

