    ///@notice function to unstake the tokens
    ///@param to address to which the unstaked tokens are returned
    ///@param amount amount of tokens to unstake
    function unStake(address to, uint256 amount) public {
        require(
            amount != 0 && amount <= stakedToken.balanceOf(msg.sender),
            "HOUSEPOOL:INVALID_AMOUNT"
        );
        // if cooldown is active then do the rest otherwise don't
        uint256 cooldownStartTimestamp = cooldownStartTimes[msg.sender];
        require(
            (block.timestamp) > (cooldownStartTimestamp + (coolDownSeconds)),
            "HOUSEPOOL:COOLDOWN_NOT_COMPLETE"
        );
        require(
            block.timestamp - (cooldownStartTimestamp + (coolDownSeconds)) <=
                unstakeWindow,
            "HOUSEPOOL:UNSTAKE_WINDOW_FINISHED"
        );

        // Pool Logic
        liquidity -= amount;
        liquidityWithAdjustedME = int256(totalValueLocked) - (voi.maxExposure);
        deposits[msg.sender] -= amount;

        int256 EVWithdrawalAmount;
        int256 EVMEWithdrawalAmount;
       
        if (EVValuePerUserShare[msg.sender] < 0) {
            EVWithdrawalAmount = tvlLPUserShare[msg.sender];//  write getTVLLP and TVLLPEV
        } else if (
            (EVValuePerUserShare[msg.sender] >= 0) &&
            (EVValuePerUserShare[msg.sender] <
            userEVTracker[msg.sender]) &&
            (userEVTracker[msg.sender]) >= 0
        ) {
            EVWithdrawalAmount =
                tvlLPUserShareExcludingEV[msg.sender] +
                (EVValuePerUserShare[msg.sender]); // write getEVLP
        } else if (
            EVValuePerUserShare[msg.sender] >=
            (userEVTracker[msg.sender])
        ) {
            EVWithdrawalAmount =
                tvlLPUserShareExcludingEV[msg.sender] +
                (userEVTracker[msg.sender]);
        }

        // refactor
        if (EVWithdrawalAmount <= liquidityWithAdjustedME) {
            EVMEWithdrawalAmount = EVWithdrawalAmount;
        } else {
            liquidityWithAdjustedME;
        }

        _burn(
            msg.sender,
            (amount * 10**MAX_PRECISION) / lpTokenPrice
        );
        withdrawalTokenPriceEVAdjusted[msg.sender] =
            (uint256(EVWithdrawalAmount) * 10**MAX_PRECISION) /
            balanceOf(msg.sender);
        userRequestedWithdrawalPoolTokens[msg.sender] =
            (amount * 10**precisionDifference) /
            lpTokenPrice;
        userRequestedWithdrawalIndepositTokens[msg.sender] =
            userRequestedWithdrawalPoolTokens[msg.sender] *
            lpTokenPrice;
        userRequestedWithdrawalInDepositTokensWithEVME[msg.sender] =
            userRequestedWithdrawalIndepositTokens[msg.sender] *
            withdrawalTokenPriceEVAdjusted[msg.sender];
        evRelatedWithdrawalLoss[msg.sender] = int256(userRequestedWithdrawalIndepositTokens[msg.sender]) -
                int256(EVMEWithdrawalAmount); // overflow check this
        userActualWithdrawalFromPool[msg.sender] = uint256(
            int256(userRequestedWithdrawalIndepositTokens[msg.sender]) -
                int256(evRelatedWithdrawalLoss[msg.sender])
        );

        require(
            amount <= uint256(userActualWithdrawalFromPool[msg.sender]),
            "HousePool: Can't withdraw"
        );

        // Farm Related Logic
        uint256 balanceOfMessageSender = balanceOf(msg.sender);
        FarmInfo memory farm = updateFarm();
        UserInfo storage user = userInfo[msg.sender];
        user.rewardDebt -= int256(
            ((((amount * 10**18) * farm.accRewardsPerShare) / lpTokenPrice)) /
                ACC_REWARD_PRECISION
        );
        user.amount -= (amount * 10**18) / lpTokenPrice;
        
        if (balanceOfMessageSender - ((amount * 10**18) / lpTokenPrice) == 0) {
            cooldownStartTimes[msg.sender] = 0;
        }
        IStakedToken(stakedToken).transfer(to, amount);
        emit UnStaked(msg.sender, msg.sender, amount);
    }

