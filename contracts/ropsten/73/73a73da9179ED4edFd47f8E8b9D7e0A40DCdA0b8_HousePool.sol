    ///@notice function to stake the amount of tokens
    ///@param onBehalfOf address of the user to send the tokens to
    ///@param amount amount of tokens to be staked
    function stake(address onBehalfOf, uint256 amount) public {
        require(amount != 0, "HOUSEPOOL:INVALID_AMOUNT");
        require(
            amount <= stakedToken.balanceOf(msg.sender),
            "House Pool:Check your balance"
        );
        
        // Pool and betting Related Logic
        deposits[msg.sender] += amount;
        runningTotalDeposits += amount;
        //liquidity  += runningTotalDeposits + bettingStakes - totalPayouts - pendingCommission - 
        liquidity += amount ; // TBD
        totalValueLocked = uint256(
            int256(liquidity) +
                (voi.expectedValue) -
                pendingStakes 
        );
        uint256 lpTokensToMint = (amount  * 10**MAX_PRECISION) / lpTokenPrice;
        
        if(deposits[msg.sender] == 0 && voi.expectedValue > 0) {
            userEVTracker[msg.sender] = voi.expectedValue; // TBD  When the LP token balance is zero, reset this value to zero.
        } else {
             userEVTracker[msg.sender] = calculateNewEVValue(lpTokensToMint, voi.expectedValue, msg.sender); // for deposit  - This is for VLFI
        }
        _mint(
            onBehalfOf,
            lpTokensToMint
        );
        EVValuePerUserShare[msg.sender] = (int256(balanceOf(msg.sender)) / int256(totalSupply())) * voi.expectedValue;
        tvlLPUserShare[msg.sender] =
            (int256(balanceOf(msg.sender)) / int256(totalSupply())) *
            int256(totalValueLocked);
        tvlLPUserShareExcludingEV[msg.sender] = int256((balanceOf(msg.sender) / totalSupply())) * 
                                                (int256(totalValueLocked) - voi.expectedValue);
                                               
        lpTokenPrice = (totalValueLocked * 10**MAX_PRECISION) / totalSupply();

        // Farming related logic
        //uint256 balanceOfUser = balanceOf(onBehalfOf); // commented becasue of new implementation of getNextCooldownTimestamp
        FarmInfo memory farm = updateFarm();
        UserInfo storage user = userInfo[msg.sender];
        user.amount += balanceOf(msg.sender);
        user.rewardDebt += int256(
            ((balanceOf(msg.sender)) * farm.accRewardsPerShare) /
                ACC_REWARD_PRECISION
        );
        if(cooldownActive == true) {
            cooldownStartTimes[onBehalfOf] = getNextCooldownTimestamp(
            0,
            amount,
            onBehalfOf,
            //balanceOfUser
            deposits[msg.sender]
        );
        }
        IStakedToken(stakedToken).transferFrom(msg.sender, address(this), amount);
        emit Staked(msg.sender, onBehalfOf, amount);
    }

    //Let's add ane global cooldown active true or false -- in vlfi its true, in USDC it's false
