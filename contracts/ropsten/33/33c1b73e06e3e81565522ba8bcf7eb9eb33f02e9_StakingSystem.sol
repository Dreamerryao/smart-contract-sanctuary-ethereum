contract StakingSystem is Ownable{
    IERC721 public nft;
    using SafeMath for uint256;
    uint256 points;
    constructor(IERC721 _nft) {
        nft = _nft;
    }
    mapping (uint256 => uint256) public stake_point;
    mapping(uint256 => mapping (address => uint256)) public stakinginfo; 
    mapping(address => bool) public staking;
    mapping(uint256 => uint256) public amountlevel;
    function StakeNFT(uint256 level,uint256 amount) public {       
        require(staking[msg.sender] == false,"staking already processed");
         for(uint256 i = 0; i < amount; i++){      
         nft.transferFrom(msg.sender,address(this),amount);  
        }      
        staking[msg.sender] = true;
        stakinginfo[level][msg.sender] = amount;
        amountlevel[level] = amount;

    }
    function unstake(uint256 level,uint256 amount) public {
        require(staking[msg.sender] == true,"user not a staker");
        uint256 _amount  = amountlevel[amount];
        points = level * _amount;
        stake_point[level] = stake_point[points].add(points); 
        nft.transferFrom((address(this)),msg.sender,_amount);
        staking[msg.sender] = false;
    }
}