pragma solidity 0.8.11;






interface INftania {
    function snapshot() external view;
    function balanceOfAt (address, uint256) external view returns (uint256);
    function balanceOf (address) external view returns (uint256);
}

contract rotateRandomizer  is VRFConsumerBaseV2 {
    // Random Number Variables
    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId = 4472;
    address vrfCoordinator = 0x6168499c0cFfCaCD319c818142124B7A15E857ab;
    bytes32 keyHash = 0xd89b2bf150e3b9e13446986e571fb9cab24b13cea0a43ea20a6049a85cc807cc;
    uint32 callbackGasLimit =25000000;
    uint16 requestConfirmations = 3;
    uint32 numWords = 1;
    uint256[] public s_randomWords;
    uint256 public s_requestId;
    address s_owner;

    // other variables
    uint256 public heroesPrice;
    uint256 public champsPrice;
    uint256 public heroTicketsCount;
    uint256 public champTicketsCount;
    uint256 public winningHeroIdsCount = 1;
    uint256 public winningChampIdsCount = 1;
    uint256 public maxHeroesCount;
    uint256 public maxChampsCount;
    uint256 k;//number of rotations to be applied
    uint256 maxK=77;//Max number of rotations 
    uint256 randomNumber;//remove later
    bool public raffleOpen;
    bool public heroRaffle = true;
    bool public champRaffle = false;
    bool public firstTimeHero = true;
    bool public firstTimeChamp = true;

    uint256[] public userHeroWinningTicketsList;
    uint256[] public userChampWinningTicketsList;
    mapping (address => bool) public applied;
    mapping (address => uint256[]) userHeroWinningTickets;
    mapping (address => uint256[]) userChampWinningTickets;
    mapping (address => uint256) public snapshot;
    mapping (uint256 => address) internal usersOfHeroIds;
    mapping (uint256 => address) internal usersOfChampIds;
    mapping (address => uint256) internal seatsOfHeroUser;
    mapping (address => uint256) internal seatsOfChampUser;
    mapping (uint256 => uint256) public splittedNumbersDigits;
    mapping (uint256 => mapping(uint256 => uint256)) public rotatedNumbersMap;
    mapping (uint256 => uint256) public mergedRotatedNumbers;
    mapping (uint256 => mapping(uint256 => uint256)) public numbersList;
    mapping (uint256 => uint256) public winningHeroIdsList;
    mapping (uint256 => uint256) public winningChampIdsList;
    address token;
    

    event NewRandomNumber (uint256 k,uint256 rotatedNumber);
    event NewHeroWinner(uint256 winningHeroIdsCount,uint256 winningNumber) ;
    event NewChampWinner(uint256 winningChampIdsCount,uint256 winningNumber) ;
    event UserSeats (uint256 heroSeats,uint256 champSeats);
    event HeroWinner (address heroWinner);
    event ChampWinner (address champWinner);
    event UserWinningTickets (uint256[] userHeroWinningList,uint256[] userChampWinningList);

    // constructor (uint256 _maxHeroesCount, uint256 _maxChampsCount, uint256 _heroesPrice,uint256 _champsPrice, address tokenAddress) VRFConsumerBaseV2(vrfCoordinator) {
    //     COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
    //     s_owner = msg.sender;        
    //     heroesPrice = _heroesPrice;
    //     champsPrice = _champsPrice;
    //     maxHeroesCount = _maxHeroesCount;
    //     maxChampsCount = _maxChampsCount;
    //     token = tokenAddress;
    // }

    constructor () VRFConsumerBaseV2(vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_owner = msg.sender;        
        heroesPrice = 66000;
        champsPrice = 21000;
        maxHeroesCount = 5000;
        maxChampsCount = 30000;
        token = 0xaD4E31B5D74d556903FAd0409C0270015e00cf19;
    }

    // Open Raffle
    function openRaffleAppliction() public onlyOwner {
        raffleOpen = true;
        // INftania(token).snapshot();
    }

    // Close Raffle
    function closeRaffleAppliction() public onlyOwner {
        raffleOpen = false;
    }
    
    // Apply for raffle
    // function applyForRaffle (uint256 numberOfHeroesTickets, uint256 numberOfChampsTickets) public raffleIsOpen {
    function applyForRaffle (uint256 numberOfHeroesTickets, uint256 numberOfChampsTickets,uint256 snapshotId) public raffleIsOpen {
        uint256 currentBalance = INftania(token).balanceOf(msg.sender);   
        // uint256 snapshotbalance = INftania(token).balanceOfAt(msg.sender,1);
        uint256 snapshotbalance = INftania(token).balanceOfAt(msg.sender,snapshotId);
        uint256 orderAmount = numberOfHeroesTickets * heroesPrice + numberOfChampsTickets * champsPrice;

        require (currentBalance > 0,"address has zero balance");
        require (snapshotbalance > 0,"address did not join at the snapshot stage");
        require (orderAmount <= snapshotbalance,"amount of $NFT2 tokens qualified for raffle should be less than or equal to the snapshot date tokens amount");
        require (currentBalance >= orderAmount,"$NFT2 tokens balance is below the cost of the total requested ticket.");
        require (applied[msg.sender] == false,"user has already applied for the raffle");
        
        //Flag that wallet has laready applied for raffle tikets
        applied[msg.sender] = true;

        // add hero tickets for user in the raffle
        
        for (uint i=1; i<=numberOfHeroesTickets; i++){
            if (numberOfHeroesTickets>0){
                heroTicketsCount++;
                usersOfHeroIds[heroTicketsCount]=msg.sender;
            } 
        }
    
        // add champ tickets for user in the raffle
        for (uint j=1; j<=numberOfChampsTickets; j++){
            if (numberOfChampsTickets>0){
                champTicketsCount++;
                usersOfChampIds[champTicketsCount]=msg.sender;
            }
        }
        
    }
    
    // find  User by Hero ticket ID
    function getHeroUser(uint256 _heroTiketId) public view returns (address user){
        console.log("HeroId:", _heroTiketId,usersOfHeroIds[_heroTiketId]);
        return usersOfHeroIds[_heroTiketId];
    }

    // find User by Champ ticket ID
    function getChampUser(uint256 _champTicketId) public view returns (address user){
        console.log("ChampId:", _champTicketId,usersOfChampIds[_champTicketId]);
        return usersOfChampIds[_champTicketId];
    }

    function makeHeroWinner (uint256 winningNumber) internal {
        //get address of winning hero ticket
        address _winner = usersOfHeroIds[winningNumber]; 
        //add seat to Hero winning address
        seatsOfHeroUser[_winner]++; 
        // add winning numbers to user's  winning heroes list
        userHeroWinningTickets[_winner].push(winningNumber);
    }

    function makeChampWinner (uint256 winningNumber) internal {
        //get address of winning champ ticket Id
        address _winner = usersOfChampIds[winningNumber];
        //add seat to cahmp winning address Id
        seatsOfChampUser[_winner]++; 
        // add winning numbers to user's  winning champs list
        userChampWinningTickets[_winner].push(winningNumber);
    }

    function getUserWinningTickets (address user) public returns (uint256[] memory userHeroWinningList, uint256[] memory userChampWinningList){
        userHeroWinningList = userHeroWinningTickets[user];
        userChampWinningList = userChampWinningTickets[user];
        emit UserWinningTickets (userHeroWinningList,userChampWinningList);
        return (userHeroWinningList,userChampWinningList);
    }


    function getUserSeats (address userWallet) public returns (uint256 heroSeats, uint256 champSeats){
        heroSeats = seatsOfHeroUser[userWallet]; 
        champSeats = seatsOfChampUser[userWallet]; 
        emit UserSeats (heroSeats,champSeats);
        return (heroSeats,champSeats);
    }

    function getHeroWinner (uint256 winningHeroId) public returns (address heroWinner){
        heroWinner = usersOfHeroIds[winningHeroId];
        emit HeroWinner (heroWinner);
        return heroWinner;
    }

    function getChampWinner (uint256 winningChampId) public returns (address champWinner){
        champWinner = usersOfChampIds[winningChampId];
        emit ChampWinner (champWinner);
        return champWinner;
    }

    function makeHeroRaffle() public raffleIsClosed onlyOwner {
        if (firstTimeHero) {
            getRandomNumber(heroRaffle); // for hero raffle
            firstTimeHero = false;
        }
        else {
            rotateNumberOnce (heroRaffle);
        }
    }

    function makeChampRaffle() public raffleIsClosed onlyOwner {
        if (firstTimeHero) {
            getRandomNumber(champRaffle); // for champ raffle
            firstTimeChamp = false;
        }
        else {  
            rotateNumberOnce (champRaffle);
        }
    }

    function getRandomNumber(bool raffleType) internal {
        // requestRandomWords (callbackGasLimit,numWords,requestConfirmations);
        // randomNumber = s_randomWords[0];  
        randomNumber = uint(keccak256(abi.encodePacked(s_owner, block.timestamp)));   // to be replaced with VRF above
        emit NewRandomNumber (k,randomNumber);
        getFifteenNumbers(randomNumber,raffleType); 
        k++;       
        splitNumberDigits(randomNumber);
    }

    function splitNumberDigits (uint256 Number) internal {
        uint256 tempNumber;
        uint256 splittedNumberDigits;
        tempNumber = Number;
        for (uint256 i=0; i<77; i++) {
            splittedNumberDigits = uint256(tempNumber % 10);
            splittedNumbersDigits[i]=splittedNumberDigits;
            tempNumber = tempNumber / 10;    
        }
    }

    function rotateNumberOnce (bool raffleType)  internal {
        if (raffleType == heroRaffle) {
            require (winningHeroIdsCount<=maxHeroesCount,"Target Winning Hero Ids Count Is Already Achieved");
        }
        else {
            require (winningChampIdsCount<=maxChampsCount,"Target Winning Champ Ids Count Is Already Achieved");
        }
        if (k < maxK) {            
            for (int i=0; i<77; i++) {
                int j = i-int(k);
                if (j < 0){
                    j += 77;
                }
                uint256 a = uint256(i);//digit index
                uint256 b = uint256(j);//Shift from refernce digit index
                uint256 c = uint256(k);//Rotated number Index
                rotatedNumbersMap[c][a]=splittedNumbersDigits[b];
            }
            uint256 rotatedNumber;
            rotatedNumber = mergeRotatedNumber();
            emit NewRandomNumber (k,rotatedNumber);
            getFifteenNumbers(rotatedNumber,raffleType);
            k++;            
        }    
        else{
            k=0;
            getRandomNumber(raffleType);
        }      
    }

    // function merge Rotated Number//
    // this function turns 77 seperated digits stored in mapping into one single 77 digits integer
    function mergeRotatedNumber() internal returns (uint256 rotatedNumber){
        for (uint256 i=0; i<77; i++) {
            rotatedNumber = rotatedNumber + (10**i) * rotatedNumbersMap[k][i];
        }
        mergedRotatedNumbers[k]=rotatedNumber;
        return (rotatedNumber);
    }

    // function get fifteen numbers//
    // this function splits 77 digits random number into a 15 five digits random number
    // each of which has a max value equals to max number of applied tickets
    function getFifteenNumbers(uint256 number, bool raffleType) internal {

        uint256 numbersCount=1;
        while (number > 0 && numbersCount<16 ) {
            uint256 partialNumber = uint256(number % 100000);
            uint256 winningNumber;
            if (raffleType == heroRaffle) {
                // make random winner Ids value range within one upto max number of hero tickets 
                winningNumber= partialNumber % (heroTicketsCount+1); 
            }
            else{
                // make random winner Ids value range within one upto max number of champ tickets
                winningNumber= partialNumber % (champTicketsCount+1); 
            }
            number = number / 100000;
            numbersList[k][numbersCount]=winningNumber; 
            numbersCount++;    
            // if hero raffle make hero winners
            if (raffleType == heroRaffle) {
                winningHeroIdsList[winningHeroIdsCount] = winningNumber; // store new winning number in the list of winning heros ids
                makeHeroWinner (winningNumber); // Get winning wallet and increase its heroes seats
                emit NewHeroWinner(winningHeroIdsCount,winningNumber) ; // announce new winning Hero ticket ID
                winningHeroIdsCount++;
            }
            // if champ raffle make champ winners
            else {
                winningChampIdsList[winningChampIdsCount] = winningNumber; // store new winning number in the list of winning champs ids
                makeChampWinner (winningNumber); // Get winning wallet and increase its champs seats
                emit NewChampWinner(winningChampIdsCount,winningNumber) ; // announce new winning Champ ticket ID
                winningChampIdsCount++;
            }
        }
    }  

    function requestRandomWords(uint32 _callbackGasLimit,uint32 _numWords,uint16 _requestConfirmations) internal {
        s_requestId = COORDINATOR.requestRandomWords(keyHash, s_subscriptionId, _requestConfirmations, _callbackGasLimit,_numWords);
    }
    
    function fulfillRandomWords(uint256, /* requestId */ uint256[] memory randomWords) internal override {
        s_randomWords = randomWords;
    }

    modifier raffleIsOpen {
      require(raffleOpen == true,"raffle is closed");
        _;  
   }  

    modifier raffleIsClosed {
    require(raffleOpen == false,"raffle is open");
        _;
    }  
    
    modifier onlyOwner() {
    require(msg.sender == s_owner,"its not owner");
    _;
    }
}

