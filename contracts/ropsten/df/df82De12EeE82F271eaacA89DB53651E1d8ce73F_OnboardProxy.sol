// File: contracts/OnboardProxy.sol


pragma solidity ^0.8.4;



contract OnboardProxy is Ownable {

	using Address for address payable;

	address public feeGetter;
	uint256 public fee = 100; // 1%
	IUniswapV2Router02 public router;

	event Swap(address indexed token, uint256 value, uint256 indexed created);

	constructor(IUniswapV2Router02 _router) {
		feeGetter = msg.sender;
		router = _router;
	}

	function setFeeGetter(uint256 _fee) external onlyOwner {
		fee = _fee;
	}

	function setFee(address wallet) external onlyOwner {
		feeGetter = wallet;
	}

	function buy(uint256 amountOutMin, address[] calldata path, address to, uint256 deadline) external payable {
		uint256 feeAmount = msg.value * fee / 10000;
		router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:msg.value - feeAmount}(amountOutMin, path, to, deadline);
		payable(feeGetter).sendValue(feeAmount);
		emit Swap(path[path.length-1], feeAmount, block.timestamp);
	}

	function sell(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline) external {
		router.swapExactTokensForETHSupportingFeeOnTransferTokens(amountIn, amountOutMin, path, address(this), deadline);

		uint256 balance = address(this).balance;
		uint256 feeAmount = balance * fee / 10000;

		payable(feeGetter).sendValue(feeAmount);
		payable(to).sendValue(balance - feeAmount);
		emit Swap(path[0], feeAmount, block.timestamp);
	}
	
    receive() external payable {}
}