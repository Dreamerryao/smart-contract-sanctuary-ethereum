// SPDX-License-Identifier: MIT
pragma solidity 0.8.9;

import "../../Shared/libraries/Strings.sol";
import "../../Shared/libraries/SafeMath.sol";
import "../../Shared/libraries/LibDiamond.sol";
import "./LibBeepBoopRaffleStorage.sol";
import "../../NFT/libraries/LibERC721Storage.sol";

contract LibRaffleModifiers {
    using SafeMath for uint256;
    using Strings for uint256;

    modifier onlyOwner() {
        LibDiamond.enforceIsContractOwner();
        _;
    }

    modifier onlyNFTOwner() {
        ERC721Storage storage s = LibERC721Storage.diamondStorage();

        require(s._balances[msg.sender] > 0, "Only NFT owners can call this function");
        _;
    }

    modifier onlyNotNFTOwner() {
        ERC721Storage storage s = LibERC721Storage.diamondStorage();

        require(!(s._balances[msg.sender] > 0), "Only people without NFT's yet can call this function");
        _;
    }

}

