pragma solidity ^0.8.13;

import 'erc721a/contracts/ERC721A.sol';

error CallerNotOwner();
error PresaleNotActive();
error ExceededPresaleLimit();
error WrongAmount();
error ProofNotProvided();
error AddressNotVerified();
error PublicSaleNotActive();
error ExceededPublicSaleLimit();
error ExceededMaxSupply();
error ExceededDevLimit();
error ContractFrozen();
error URIDoesNotMatch();
error WithdrawFailed();

contract QOTest is ERC721A {
    string private constant _NAME = '[QO]Test';
    string private constant _SYMBOL = '[QO]';

    uint256 private constant _SHARES = 1000;
    address[] private _payees = [0x9A0c66926ae19246D312c3Be6af6EEF1edE9D26E,
                                 0xdC12E1964a8f9ac6BB9e029FCD17744f9e4BBC5E];
    uint256[] private _shares = [975, 25];

    struct Config {
        uint16 maxSupply;
        uint16 maxTokensForDevs;
        uint16 maxTokensInPresale;
        uint16 maxTokensInPublicSale;
        uint32 preSaleStartTime;
        uint32 publicSaleStartTime;
        uint64 priceWL;
        uint64 price;
    }

    Config public config;
    address private _owner;
    string private _baseTokenURI;
    bytes32 public immutable revealURIHash;
    bytes32 public merkleRoot;
    bool public frozen;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    constructor(
        uint16 maxSupply_,
        uint16 maxTokensInPreSale_,
        uint16 maxTokensInPublicSale_,
        uint16 maxTokensForDevs_,
        bytes32 revealURIHash_,
        uint32 preSaleStartTime_,
        uint32 publicSaleStartTime_,
        uint64 price_,
        bytes32 merkleRoot_,
        string memory baseURI_
    ) ERC721A(_NAME, _SYMBOL) {
        config.maxSupply = maxSupply_;
        config.maxTokensInPresale = maxTokensInPreSale_;
        config.maxTokensInPublicSale = maxTokensInPublicSale_;
        config.maxTokensForDevs = maxTokensForDevs_;
        revealURIHash = revealURIHash_;
        config.preSaleStartTime = preSaleStartTime_;
        config.publicSaleStartTime = publicSaleStartTime_;
        config.price = price_;
        merkleRoot = merkleRoot_;
        _baseTokenURI = baseURI_;
        _transferOwnership(_msgSenderERC721A());
    }

    modifier onlyOwner() {
        if (_msgSenderERC721A() != _owner) revert CallerNotOwner();
        _;
    }

    modifier isFrozen() {
        if (frozen) revert ContractFrozen();
        _;
    }

    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function _efficientHash(bytes32 a, bytes32 b) private pure returns (bytes32 value) {
        assembly {
            mstore(0x00, a)
            mstore(0x20, b)
            value := keccak256(0x00, 0x40)
        }
    }

    function _verifyProof(bytes32[] memory proof) internal view {
        bytes32 hash = keccak256(abi.encodePacked(_msgSenderERC721A()));
        for (uint256 i; i < proof.length; i++)
            hash = hash <= proof[i] ? _efficientHash(hash, proof[i]) : _efficientHash(proof[i], hash);
        if (hash != merkleRoot) revert AddressNotVerified();
    }

    function _mint(uint256 startTime, uint16 maxTokens, uint256 quantity, bytes32[] memory proof) internal {
        unchecked {
            if (_totalMinted() + quantity > config.maxSupply) revert ExceededMaxSupply();
            uint256 mints = _getAux(_msgSenderERC721A());
            uint256 proofLength = proof.length;
            if ((proofLength == 0 ? mints / 100 : mints % 100) + quantity > maxTokens) {
                if (proof.length > 0) revert ExceededPresaleLimit();
                else revert ExceededPublicSaleLimit();
            }
            if (startTime == 0 || block.timestamp < startTime) {
                if (proof.length > 0) revert PresaleNotActive();
                else revert PublicSaleNotActive();
            }
            if (msg.value != config.price * quantity) revert WrongAmount();
            if (proofLength > 0) _verifyProof(proof);
            _setAux(_msgSenderERC721A(), uint64(mints + (proofLength == 0 ? quantity * 100 : quantity)));
        }
        if (_msgSenderERC721A().code.length > 0) _safeMint(_msgSenderERC721A(), quantity);
        else _mint(_msgSenderERC721A(), quantity);
    }

    function rawMint(uint256 quantity) external {
        _mint(_msgSenderERC721A(), quantity);
    }

    function rawSafeMint(uint256 quantity) external {
        _safeMint(_msgSenderERC721A(), quantity);
    }

    function devMint(address to, uint256 quantity) external onlyOwner {
        if (_totalMinted() + quantity > config.maxTokensForDevs) revert ExceededDevLimit();
        _mint(to, quantity);
    }

    function whitelistMint(uint256 quantity, bytes32[] calldata proof) external payable {
        if (proof.length == 0) revert ProofNotProvided();
        _mint(config.preSaleStartTime, config.maxTokensInPresale, quantity, proof);
    }

    function publicMint(uint256 quantity) external payable {
        bytes32[] memory proof;
        _mint(config.publicSaleStartTime, config.maxTokensInPublicSale, quantity, proof);
    }

    function setMaxSupply(uint16 quantity) external onlyOwner isFrozen {
        config.maxSupply = quantity;
    }

    function setPublicSaleStartTime(uint32 timestamp) external onlyOwner {
        config.publicSaleStartTime = timestamp;
    }

    function setPreSaleStartTime(uint32 timestamp) external onlyOwner {
        config.preSaleStartTime = timestamp;
    }

    function setPrice(uint64 price) external onlyOwner {
        config.price = price;
    }

    function setMerkleRoot(bytes32 root) external onlyOwner {
        merkleRoot = root;
    }

    function _baseURI() internal view override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string calldata baseURI) external onlyOwner isFrozen {
        _baseTokenURI = baseURI;
    }

    function reveal(string calldata baseURI) external onlyOwner isFrozen {
        if (keccak256(abi.encodePacked(baseURI)) != revealURIHash) revert URIDoesNotMatch();
        _baseTokenURI = baseURI;
    }

    function freeze() external onlyOwner {
        frozen = true;
    }

    function withdraw(uint256 amount) external onlyOwner {
        for (uint256 i; i < _payees.length; i++) {
            (bool success,) = payable(_payees[i]).call{value: amount * _shares[i] / _SHARES}('');
            if (!success) revert WithdrawFailed();
        }
    }

    function _transferOwnership(address newOwner) internal {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }

    function transferOwnership(address newOwner) external onlyOwner {
        _transferOwnership(newOwner);
    }

    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }
}

