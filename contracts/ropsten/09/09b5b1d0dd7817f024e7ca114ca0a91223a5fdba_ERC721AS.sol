                block.timestamp >= _stakingPolicy.stakingBegin &&
                _tokenStatus[tokenId].stakingTimestamp >=
                _stakingPolicy.stakingBegin &&
                ((_tokenStatus[tokenId].stakingTimestamp +
                    _stakingPolicy.breaktime) > block.timestamp);
        }
    }

