// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "hardhat/console.sol";

import "./interfaces/IRewarder.sol";
import "./interfaces/ISTOA.sol";
import "./interfaces/IStoaToken.sol";

// Import and refactor of Synthetix StakingRewarder
// https://github.com/Synthetixio/synthetix/blob/develop/contracts/StakingRewards.sol
contract Rewarder is ReentrancyGuard, Pausable, IRewarder, Ownable {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public token;
    address public stoa;
    address public treasury;
    uint256 public rewardPerTokenStored;
    uint256 public sagePathThreshold;
    uint256 public lastUpdateTime;

    mapping(address => uint256) public userStakeRewardPerTokenPaid;
    mapping(address => Staker) private _stakers;
    uint256 private _totalStaked;

    // uint256[][] public commitmentThresholds = [[0 days, 30 days],
    // [30 days, 90 days], [90 days, 180 days], [180 days, 360 days]]; //TODO on deploy
    Counters.Counter public sagepathUsers;
    Counters.Counter public low;
    Counters.Counter public mid;
    Counters.Counter public high;
    Counters.Counter public stoic;

    struct Staker {
        bool exists;
        uint256 balance;
        uint256 rewards;
        bool sagePathUser;
        uint256 sagepathTimestamp;
    }

    struct Rewards {
        uint256 baseRewards;
        uint256 stakingRewards;
        uint256 accomplishmentRewards;
        uint256 commitmentRewards;
    }

    constructor(
        address _owner,
        address _token,
        address _stoaData,
        address _treasury,
        uint256 _sagePathThreshold
    ) {
        token = _token;
        stoa = _stoaData;
        treasury = _treasury;
        sagePathThreshold = _sagePathThreshold;

        if (msg.sender != _owner) {
            transferOwnership(_owner);
        }
    }

    // Event
    event AvailableRewardsUpdated(uint256 added, uint256 totalSupply);
    event RewardPaid(string rewardType, address account, uint256 amount);
    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event Recovered(address token, uint256 amount);
    event SagePathUpdated(uint256 threshold);

    // Views
    function rewardsToken() external view returns (address _rewardsToken) {
        return token;
    }

    // Base rewards
    function getBaseRewardRatePerUser() public view returns (uint256 baseRewardRatePerUser) {
        uint256 userCount = _stoa().getUserCount();
        if (userCount == 0) {
            return 0;
        }
        baseRewardRatePerUser = _stoa().getBaseRate(_stoa().getPeriodIndex()).div(userCount);
    }

    function getBaseReward(address user) public view returns (uint256 rewardAmount) {
        uint256 timeSinceLastClaim = block.timestamp.sub(_stoa().getLastClaimed(user));
        if (timeSinceLastClaim <= 0) {
            return 0;
        }
        return getBaseRewardRatePerUser().mul(timeSinceLastClaim);
    }

    // Staking
    function totalStaked() external view returns (uint256) {
        return _totalStaked;
    }

    function getStakedBalance(address account) public view returns (uint256) {
        return _stakers[account].balance;
    }

    function stakingRewardPerToken() public view override returns (uint256) {
        if (_totalStaked == 0) {
            return rewardPerTokenStored;
        }
        return
            rewardPerTokenStored.add(
                lastTimeRewardApplicable()
                    .sub(lastUpdateTime)
                    .mul(_stoa().getStakingRate(_stoa().getPeriodIndex()))
                    .mul(1e18)
                    .div(_totalStaked)
            );
    }

    function getStakingReward(address user) public view override returns (uint256 rewardAmount) {
        return
            getStakedBalance(user).mul(stakingRewardPerToken().sub(userStakeRewardPerTokenPaid[user])).div(1e18).add(
                _stakers[user].rewards
            );
    }

    // Accomplishment
    // TODO no magic numbers
    function getAccomplishmentReward(address user, uint256 periodIndex) public view returns (uint256 rewardAmount) {
        uint256 lvl = _stoa().getAccomplishmentLevel(user, periodIndex);
        console.log("Rewarder::lvl: ", lvl);
        require(lvl != 0, "Nothing accomplished");
        if (lvl == 1) {
            // TODO should this be called from _stoa?
            rewardAmount = _stoa().getAccomplishmentRewards(periodIndex).mul(15).div(100);
        }
        if (lvl == 2) {
            rewardAmount = _stoa().getAccomplishmentRewards(periodIndex).mul(30).div(100);
        }
        if (lvl == 3) {
            rewardAmount = _stoa().getAccomplishmentRewards(periodIndex).mul(55).div(100);
        }
    }

    // Commitment
    function getCommitmentReward(address user) public view returns (uint256 rewardAmount) {
        console.log("Rewarder::stakedBalance: ", _stakers[user].balance);
        console.log("Rewarder::threshold: ", sagePathThreshold);

        return getCommitmentRewardSince(_stakers[user].sagepathTimestamp);
    }

    function getCommitmentRewardSince(uint256 stakingTimestamp) public view returns (uint256 rewardAmount) {
        uint256 stakeDuration = block.timestamp - stakingTimestamp;
        console.log("Rewarder::stakeDuration: ", stakeDuration);
        rewardAmount = _calculateCommitmentReward(stakeDuration);
    }

    function _calculateCommitmentReward(uint256 stakeDuration) internal view returns (uint256 rewardAmount) {
        // TODO smart period index parsing
        uint256 periodIndex = 0;
        uint256 commitmentRewardsForPeriod = _stoa().getCommitmentRewardForPeriod(periodIndex);
        console.log("Rewarder::commitmentRewardsForPeriod: ", commitmentRewardsForPeriod); // CHECK
        if (stakeDuration >= 30 days && stakeDuration < 90 days) {
            uint256 ratio = (1 / 10) * 1e18;
            return _calculatePortion(commitmentRewardsForPeriod, ratio);
        }
        if (stakeDuration >= 90 days && stakeDuration < 180 days) {
            uint256 ratio = (2 / 10) * 1e18;
            return _calculatePortion(commitmentRewardsForPeriod, ratio);
        }
        if (stakeDuration >= 180 && stakeDuration < 360 days) {
            uint256 ratio = (3 / 10) * 1e18;
            return _calculatePortion(commitmentRewardsForPeriod, ratio);
        }
        if (stakeDuration >= 360) {
            uint256 ratio = (4 / 10) * 1e18;
            return _calculatePortion(commitmentRewardsForPeriod, ratio);
        }
        return 0;
    }

    // Sage path

    function isSagePathUser(address user) public view returns (bool isIndeed) {
        // TODO check on last meditated
        return _meetsSagePathThreshold(user);
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return block.timestamp < _stoa().getPeriodEnd() ? block.timestamp : _stoa().getPeriodEnd();
    }

    function getSagepathCount() public view returns (uint256 sagepathCount) {
        return sagepathUsers.current();
    }

    // Mutative functions
    // Claims
    // TODO check if notifyRewardsPaid can be used as modifier
    function claimBaseReward() external nonReentrant {
        uint256 rewardAmount = getBaseReward(msg.sender);
        _stoa().notifyRewardsPaid(msg.sender, rewardAmount);
        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("BaseReward", msg.sender, rewardAmount);
    }

    function claimStakingReward() external nonReentrant onlySagePath {
        uint256 rewardAmount = getStakingReward(msg.sender);
        _stoa().notifyRewardsPaid(msg.sender, rewardAmount);
        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("StakingReward", msg.sender, rewardAmount);
    }

    function claimCommitmentReward() external nonReentrant onlySagePath {
        uint256 rewardAmount = getCommitmentReward(msg.sender);
        require(rewardAmount > 0, "No commitment reward available");
        _stoa().notifyRewardsPaid(msg.sender, rewardAmount);
        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("CommitmentReward", msg.sender, rewardAmount);
    }

    function claimAccomplishmentReward(uint256 periodIndex) external nonReentrant onlySagePath {
        require(_stoa().getAccomplishmentLevel(msg.sender, periodIndex) != 0, "Not enough meditation");
        require(_stoa().getPeriodIndex() > periodIndex, "Period out of range");

        uint256 rewardAmount = getAccomplishmentReward(msg.sender, periodIndex);
        _stoa().notifyRewardsPaid(msg.sender, rewardAmount);
        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("AccomplishmentReward", msg.sender, rewardAmount);
    }

    // Staking
    function stake(uint256 amount) external nonReentrant whenNotPaused updateReward(msg.sender) {
        require(amount > 0, "Cannot stake 0");
        console.log("Rewarder::stake::amount: ", amount);
        Staker storage user = _stakers[msg.sender];
        if (!user.exists) {
            user.exists = true;
        }
        _totalStaked = _totalStaked.add(amount);
        user.balance = user.balance.add(amount);

        if (user.sagePathUser == false && _meetsSagePathThreshold(msg.sender)) {
            user.sagePathUser = true;
            user.sagepathTimestamp = block.timestamp;
            sagepathUsers.increment();
        }
        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);
        emit Staked(msg.sender, amount);
    }

    function withdraw(uint256 amount) public nonReentrant updateReward(msg.sender) {
        require(amount > 0, "Cannot withdraw 0");
        Staker storage user = _stakers[msg.sender];

        _totalStaked = _totalStaked.sub(amount);
        user.balance = user.balance.sub(amount);

        if (user.sagePathUser == true && !_meetsSagePathThreshold(msg.sender)) {
            user.sagePathUser = false;
            user.sagepathTimestamp = 0;
            sagepathUsers.decrement();
        }

        IERC20(token).safeTransfer(msg.sender, amount);
        emit Withdrawn(msg.sender, amount);
    }

    // function getReward() public nonReentrant updateReward(msg.sender) {
    //     uint256 reward = rewards[msg.sender];
    //     if (reward > 0) {
    //         rewards[msg.sender] = 0;
    //         IERC20(token).safeTransfer(msg.sender, reward);
    //         emit RewardPaid("Reward", msg.sender, reward);
    //     }
    // }

    function exit() external {
        withdraw(_stakers[msg.sender].balance);
        uint256 rewardAmount = getBaseReward(msg.sender) +
            getStakingReward(msg.sender) +
            getAccomplishmentReward(msg.sender, _stoa().getPeriodIndex()) +
            getCommitmentReward(msg.sender);

        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("Exit", msg.sender, rewardAmount);
    }

    // Internal
    function _meetsSagePathThreshold(address user) internal view returns (bool passed) {
        return getStakedBalance(user) >= sagePathThreshold;
    }

    function _calculatePortion(uint256 amount, uint256 ratio) internal pure returns (uint256 portion) {
        return amount.mul(ratio).div(1e18);
    }

    // Config
    function setSagePathThreshold(uint256 _value) public onlyOwner {
        sagePathThreshold = _value;
    }

    // Modifiers
    //TODO maybe not pretty as modifier
    modifier updateReward(address account) {
        rewardPerTokenStored = stakingRewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            _stakers[account].rewards = getStakingReward(account);
            userStakeRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }

    modifier onlySagePath() {
        require(isSagePathUser(msg.sender), "Not a sage path user");
        _;
    }

    // System
    function _stoa() internal view returns (ISTOA) {
        return ISTOA(stoa);
    }

    function _treasury() internal view returns (address) {
        return treasury;
    }

    function _token() internal view returns (IStoaToken) {
        return IStoaToken(token);
    }
}

