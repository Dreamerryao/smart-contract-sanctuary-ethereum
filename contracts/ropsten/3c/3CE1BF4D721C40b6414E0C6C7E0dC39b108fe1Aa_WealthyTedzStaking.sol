/// @title Wealthy Tedz Staking Contract
/// @author 0xSimon_
/// @notice This contract allows Wealthy Tedz holders to stake their ERC721 in return for ETH Rewards. Rewards are earned on a per-round basis and users must register for the newest round.
/// @dev to do: Explain to a developer any extra details


contract WealthyTedzStaking is Ownable {
    //@dev Wealthy Tedz Main Collection
    address public wealthy_teds = 0xd9145CCE52D386f254917e481eB44e9943F39138;

    //@dev Reward Aggregator Just In Case
    address public reward_aggregator;

    //@dev set this to the max supply of your collection. Used to calculate rewards per second
    uint public numNfts = 10;
    //@dev reward Per Second which is set when starting a new round
    uint public rewardPerSecond;
    //@ dev Initializer Timestamp
    uint blockTimestamp;

    //@dev keeps track of the round we are on
    uint16 public roundCounter;

    
    //@dev tracks address -> stakers
    mapping(address => Staker) public stakers;

    //@dev Tracks ownership of tokenId -> address
    mapping(uint => address) public owners;
    
    //@dev Tracks information for each relative staking round
    mapping(uint => stakingRound) public stakingRounds;

    //@dev used to check if round zero. Prevents malicious action upon creation of the contract.
    bool private roundZeroSet;


    struct Staker {
        uint256 accumulatedRewards;
        uint256[2] lastUpdateTimestamp;
        uint16 nfts_staked;
        uint256[] tokenIds;
        uint lastRate;
        uint32 lastRound;
    }

    struct stakingRound {
        uint startTimestamp;
        uint endTimestamp;
        uint duration;
        uint maxReward;
    }

    
    constructor(){
        blockTimestamp = block.timestamp;
    }
    
    //@dev event emitted when a user starts stake
    event STARTED_STAKE(uint indexed stakingRound, address indexed staker, uint timestamp);

    //@dev sets the wealthyTedz address in case of emergency
    function setWealthyTedz(address _address) public onlyOwner {
        wealthy_teds = _address;
    }



    
    /*/////////////////////////////////////////////////
           UPDATE REWARD UNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/
    modifier updateReward(address account) {
        //Saves timestamp pre-update
        stakers[account].lastUpdateTimestamp[0] = stakers[account].lastUpdateTimestamp[1] == 0? block.timestamp : stakers[account].lastUpdateTimestamp[1];

        //Find the currentRate that's been set for the staker
        uint currentStakerRate = stakers[account].lastRate;
        uint currentStakerRound = stakers[account].lastRound;

        //Makes sure to set the rate for a user if they've never staked before::This also indicates they haven't start a stake for this round
        if(currentStakerRate == 0){
            stakers[account].lastRate = rewardPerSecond;
        }
      
        //Let's Update the total time staked per round;


        //If round hasn't ended
      

        //Calculate reward to give
        uint256 reward = nextReward(account);

        //Update Timestamp
        stakers[account].lastUpdateTimestamp[1] = block.timestamp;


        //Update the reward per second
        stakers[account].lastRate = rewardPerSecond;

        //Update Reward
        stakers[account].accumulatedRewards += reward;

        //Update the round that the user is earning rewards on
        stakers[account].lastRound = roundCounter;
        _;
    }


      
    /*/////////////////////////////////////////////////
           CACULATE NEXT REWARD:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

function nextReward(address account) public view returns(uint256) {
    console.log("Calling Next Reward");
    Staker storage staker =  stakers[account];
    if(staker.lastUpdateTimestamp[1] == 0){
        return 0;
    }

    uint user_current_round = staker.lastRound;
    uint endRoundTimestamp = stakingRounds[user_current_round].endTimestamp;

    uint timeDelta;
    //If Round Has Ended Because This Runs After updateReward 
    if(block.timestamp >  endRoundTimestamp)
    {
        timeDelta = endRoundTimestamp - staker.lastUpdateTimestamp[0];
        
    }
    //If Round Hasn't Ended
    else{
        timeDelta = block.timestamp - staker.lastUpdateTimestamp[1];
        
    }
    
    uint num_staked = staker.nfts_staked;
    uint reward = timeDelta * num_staked * staker.lastRate;
    return reward;
}




 /*/////////////////////////////////////////////////
           ROUND SETTERS FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

    function setNewStakingRound(uint durationInDays) external payable onlyOwner{
        require(roundZeroSet,"Must Have Initialized Round 0");
        //Prevents Underflow
        require(block.timestamp > stakingRounds[roundCounter].endTimestamp,"Last Round Must End Before Starting New Round");
    
        roundCounter++;
        //@dev in days

        uint maxDuration = durationInDays * uint(1 days);
        stakingRounds[roundCounter].endTimestamp = block.timestamp + maxDuration;
        stakingRounds[roundCounter].duration = maxDuration;
        rewardPerSecond = (msg.value / numNfts) / (durationInDays  * uint (1 days));
        stakingRounds[roundCounter].startTimestamp = block.timestamp;
        stakingRounds[roundCounter].maxReward = msg.value / numNfts;

        /*
        //@dev in seconds:: used for testing
        */


        // uint maxDuration = durationInDays;
        // stakingRounds[roundCounter].endTimestamp = block.timestamp + maxDuration;
        // stakingRounds[roundCounter].duration = maxDuration;
        // rewardPerSecond = (msg.value / numNfts) / (durationInDays);
        // stakingRounds[roundCounter].startTimestamp = block.timestamp;
        
    }

  

    function setRoundZero(uint durationInDays) external payable onlyOwner{
        require(!roundZeroSet,"Can Only Be Used On Round Zero");
        //@dev in days
        uint maxDuration = durationInDays * uint(1 days);
        stakingRounds[roundCounter].endTimestamp = block.timestamp + maxDuration;
        stakingRounds[roundCounter].duration = maxDuration;
        rewardPerSecond = (msg.value / numNfts) / (durationInDays  * uint (1 days));
        stakingRounds[roundCounter].startTimestamp = block.timestamp;
        
        /*
        // @dev in seconds::used for testing
        */

        // uint maxDuration = durationInDays;
        // stakingRounds[roundCounter].endTimestamp = block.timestamp + maxDuration;
        // stakingRounds[roundCounter].duration = maxDuration;
        // rewardPerSecond = (msg.value / numNfts) / (durationInDays);
        // stakingRounds[roundCounter].startTimestamp = block.timestamp;


        roundZeroSet =true;
        
    }

    function setNumNfts(uint _numNfts) external onlyOwner{
        numNfts = _numNfts;
    }

    function viewContractBalance() external view returns(uint){
        return address(this).balance;
    }



    function viewRatePerAccount(address account) public view returns(uint) {
        return stakers[account].lastRate;
    }

   
  

    

 /*/////////////////////////////////////////////////
           ARBITRARY GETTER :::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

    function getTimestampPair(address account) public view returns(uint ts1,uint ts2){
        ts1 = stakers[account].lastUpdateTimestamp[0];
        ts2 = stakers[account].lastUpdateTimestamp[1];
    }

 
  
 /*/////////////////////////////////////////////////
           VIEW REWARD FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

    function viewReward() public view returns(uint) {
        return stakers[msg.sender].accumulatedRewards + nextReward(msg.sender);
    }
    function viewWalletReward(address account) public view returns(uint) {
        return stakers[account].accumulatedRewards + nextReward(account);
    }


         
    /*/////////////////////////////////////////////////
           WITHDRAW REWARD FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/
    function withdrawReward() external payable updateReward(msg.sender){
        if(stakers[msg.sender].lastRound != roundCounter){
            stakers[msg.sender].accumulatedRewards = 0;
        }
        uint256 reward = stakers[msg.sender].accumulatedRewards;
        stakers[msg.sender].accumulatedRewards = 0;
        (bool os, ) = payable(msg.sender).call{value: reward}("");
        require(os);
    }

    

    function withdrawReward(address account) external payable {
        require(msg.sender == reward_aggregator,"Unauthorized");
        uint256 reward = stakers[account].accumulatedRewards;
        stakers[account].accumulatedRewards = 0;
        (bool os, ) = payable(account).call{value: reward}("");
        require(os);
    }
 

 /*/////////////////////////////////////////////////
           HELPER FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

    function removeElement(uint element, uint[] storage tokenIds) internal {
        for(uint i; i<tokenIds.length;i++){
            if(element == tokenIds[i]){
                tokenIds[i] = tokenIds[tokenIds.length -1];
                tokenIds.pop();
                return;
            }
        }
    }


       
    /*/////////////////////////////////////////////////
           SINGLE STAKE FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/

    function startStake(uint256 tokenId) external updateReward(msg.sender) {
        require(roundZeroSet,"Staking Not Active");
        ERC721(wealthy_teds).transferFrom(msg.sender,address(this),tokenId);
        owners[tokenId] = msg.sender;
        stakers[msg.sender].tokenIds.push(tokenId);
        stakers[msg.sender].nfts_staked++;
        emit STARTED_STAKE(roundCounter, msg.sender,block.timestamp);
    }

    function endStake(uint256 tokenId) external updateReward(msg.sender) {
        require(roundZeroSet,"Staking Not Active");
        require(owners[tokenId] != address(0),"Cannot Be Zero Address");
        require(msg.sender == owners[tokenId],"Invalid Owner");
        ERC721(wealthy_teds).transferFrom(address(this),msg.sender,tokenId);
        stakers[msg.sender].nfts_staked--;
        removeElement(tokenId,stakers[msg.sender].tokenIds);
        delete owners[tokenId];
    }



         
    /*/////////////////////////////////////////////////
           BATCH STAKE FUNCTIONS:::::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/
    function batchStake(uint256[] calldata tokenIds) external updateReward(msg.sender) {
        require(roundZeroSet,"Staking Not Active");
        for(uint256 i; i<tokenIds.length; i++){
            ERC721(wealthy_teds).transferFrom(msg.sender,address(this),tokenIds[i]);
            owners[tokenIds[i]] = msg.sender;
            stakers[msg.sender].tokenIds.push(tokenIds[i]);
            stakers[msg.sender].nfts_staked++;
        }
    }

    function batchUnstake (uint256[] calldata tokenIds) external updateReward(msg.sender) {
        require(roundZeroSet,"Staking Not Active");
        for(uint256 i; i<tokenIds.length;i++) {
            require(owners[tokenIds[i]] != address(0),"Cannot Be Zero Address");
            require(msg.sender == owners[tokenIds[i]],"Invalid Owner");
            ERC721(wealthy_teds).transferFrom(address(this),msg.sender,tokenIds[i]);
            stakers[msg.sender].nfts_staked--;
            removeElement(tokenIds[i],stakers[msg.sender].tokenIds);
            delete owners[tokenIds[i]];
        }
    }
   



    /*/////////////////////////////////////////////////
            GETTER FUNCTIONS FOR FRONTEND:::
    \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\*/
    function getTedzStaked(address user) public view returns(uint256[] memory){
        return stakers[user].tokenIds;
    }
    function getTedzStakedByIndex(address user, uint index) public view returns(uint){
        return stakers[user].tokenIds[index];
    }
    function getTedzLength(address account) public view returns(uint){
        return stakers[account].tokenIds.length;
    }
    //---------END STAKE FUNCTIONS


   
}





interface ERC721 {
    function transferFrom(address from, address to,uint256 tokenId) external;
}

