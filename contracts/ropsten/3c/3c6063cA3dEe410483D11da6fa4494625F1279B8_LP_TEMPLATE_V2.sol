pragma solidity ^0.8.3;

import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

interface IERC20 {
    function totalSupply() external view returns (uint256);

    function increaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool);

    function decreaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);

    function balanceOf(address account) external view returns (uint256);
}

contract ALT_TOKEN is IERC20 {
    using SafeMath for IERC20;

    IERC20 public alt_token;

    event Invest(address user);

    mapping(address => uint256) public _balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _allowance_lps;

    bool _locked;
    bool _paused;

    uint256 private _totalSupply;
    uint256 private _decimals;

    string private _name;
    string private _symbol;

    address public contract_lp_1y;
    address public contract_owner;
    uint256 public decimals;
    uint256 public total_supply;

    bytes32 public BURNER_ROLE;
    bytes32 public MINTER_ROLE;

    uint256 rate_period;
    uint256 lockup_days;

    struct AccountPosition {
        uint256 datetime_init;
        uint256 datetime_end;
        uint256 volume;
        uint256 rate_period;
    }

    mapping(address => AccountPosition[]) public _investments;

    modifier noReentrancy() {
        require(!_locked);
        _locked = true;
        _;
        _locked = false;
    }

    modifier onlyRole(address owner) {
        require(owner == contract_owner, "ONLY_ROLE");
        _;
    }

    modifier onlyLP() {
        require(_allowance_lps[msg.sender], "ONLY_LP");
        _;
    }

    modifier whenPaused() {
        require(!paused(), "PAUSED");
        _;
    }

    modifier whenNotPaused() {
        require(paused(), "NOT_PAUSED");
        _;
    }

    receive() external payable {}

    fallback() external payable {}

    constructor() {
        _name = "ALT_TOKEN";
        _symbol = "ALT_TOKEN";
        _decimals = 6;
        _totalSupply = (2 * 10**6) * 10**_decimals;

        contract_owner = msg.sender;
        total_supply = _totalSupply;
        decimals = _decimals;

        lockup_days = 360;
        rate_period = 10;

        contract_lp_1y = 0xe7f1725E7734CE288F8367e1Bb143E90bb3F0512;

        mint(contract_owner, total_supply);
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function changeLP(address contract_lp, bool enable)
        public
        onlyRole(contract_owner)
    {
        _allowance_lps[contract_lp] = enable;
    }

    function pause() public onlyRole(contract_owner) whenNotPaused {
        _paused = true;
    }

    function unpause() public onlyRole(contract_owner) whenPaused {
        _paused = false;
    }

    function mint(address to, uint256 amount) public onlyRole(contract_owner) {
        _totalSupply += amount;
        _balances[to] += amount;
    }

    function paused() public view virtual returns (bool) {
        return _paused;
    }

    function burn(uint256 amount) public onlyRole(contract_owner) {
        uint256 accountBalance = _balances[contract_owner];
        require(accountBalance >= amount, "BURN_AMOUNT_EXCEED");
        unchecked {
            _balances[contract_owner] = accountBalance - amount;
        }
        _totalSupply -= amount;
    }

    function transfer(address to, uint256 amount) public {
        require(_balances[msg.sender] >= amount, "USER_NO_BALANCE");

        _balances[msg.sender] -= amount;
        _balances[to] += amount;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public override onlyRole(contract_owner) returns (bool) {
        require(_balances[from] >= amount, "USER_NO_BALANCE");

        _balances[from] -= amount;
        _balances[to] += amount;

        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        override
        returns (bool)
    {
        require(spender != address(0), "NO_ZERO_ADDRESS");

        _allowances[spender][msg.sender] += addedValue;
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtracted_value)
        public
        virtual
        override
        returns (bool)
    {
        uint256 currenta_allowance = _allowances[spender][msg.sender];
        require(currenta_allowance >= subtracted_value, "ALLOW_EXCEEDS");

        _allowances[spender][msg.sender] -= subtracted_value;
        return true;
    }

    function transferFromTreasury(
        address to,
        uint256 amount,
        uint256 amount_interest
    ) public onlyLP {
        require(_allowances[msg.sender][to] >= amount, "NOT_ALLOWED");
        require(_balances[msg.sender] >= amount, "USER_NO_BALANCE");
        require(_balances[msg.sender] >= amount_interest, "NO_BALANCE");

        _allowances[msg.sender][to] -= amount;
        _balances[msg.sender] -= amount_interest;
        _balances[to] += amount_interest;
    }

    function transferToTreasury(address to, uint256 amount) public onlyLP {
        require(_balances[to] >= amount, "USER_NO_BALANCE");

        _allowances[msg.sender][to] += amount;
        _balances[msg.sender] += amount;
        _balances[to] -= amount;
    }
}

