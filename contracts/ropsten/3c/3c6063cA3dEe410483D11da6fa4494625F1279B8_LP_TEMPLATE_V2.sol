pragma solidity ^0.8.3;

import "./ALT_TOKEN.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "hardhat/console.sol";

interface IERC20_LP {
    function totalSupply() external view returns (uint256);

    function increaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool);

    function decreaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);

    function balanceOf(address account) external view returns (uint256);
}

contract LP_TEMPLATE_V2 is IERC20_LP {
    using SafeMath for IERC20_LP;

    ALT_TOKEN public alt_token;

    AggregatorV3Interface internal price_feed;

    event Invest(address user);

    mapping(address => uint256) public _balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _fee_payed;

    bool _locked;
    bool _paused;

    uint256 private _totalSupply;
    uint256 private _decimals;

    string private _name;
    string private _symbol;

    address payable public contract_owner;
    address payable public altstreet_treasury;
    uint256 public decimals;
    uint256 public total_supply;

    bytes32 public BURNER_ROLE;
    bytes32 public MINTER_ROLE;

    uint256 rate_period;
    uint256 lockup_days;

    struct AccountPosition {
        uint256 datetime_init;
        uint256 datetime_end;
        uint256 volume;
        uint256 rate_period;
    }

    mapping(address => AccountPosition[]) public _investments;

    modifier noReentrancy() {
        require(!_locked);
        _locked = true;
        _;
        _locked = false;
    }

    modifier onlyRole(address owner) {
        require(owner == contract_owner);
        _;
    }

    modifier whenPaused() {
        require(!paused(), "PAUSED");
        _;
    }

    modifier whenNotPaused() {
        require(paused(), "NOT_PAUSED");
        _;
    }

    receive() external payable {}

    constructor() {
        _name = "LP_TEMPLATE_V2";
        _symbol = "LP_TEMPLATE_V2";
        _decimals = 6;
        _totalSupply = (2 * 10**6) * 10**_decimals;

        contract_owner = payable(msg.sender);
        decimals = _decimals;
        total_supply = _totalSupply;
        lockup_days = 360;
        rate_period = uint256(10.00 * 10**decimals);

        altstreet_treasury = payable(
            0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266
        );

        alt_token = ALT_TOKEN(
            payable(0x5FbDB2315678afecb367f032d93F642f64180aa3)
        );

        price_feed = AggregatorV3Interface(
            0x9326BFA02ADD2366b30bacB125260Af641031331
        );

        mint(contract_owner, _totalSupply);
    }

    function pause() public onlyRole(contract_owner) whenNotPaused {
        _paused = true;
    }

    function unpause() public onlyRole(contract_owner) whenPaused {
        _paused = false;
    }

    function mint(address to, uint256 amount) public onlyRole(contract_owner) {
        _totalSupply += amount;
        _balances[to] += amount;
    }

    function burn(address from, uint256 amount)
        public
        onlyRole(contract_owner)
    {
        uint256 accountBalance = _balances[from];
        require(accountBalance >= amount, "BURN_EXCEEDS_ALLOWANCE");
        unchecked {
            _balances[from] -= amount;
        }
        _totalSupply -= amount;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public override onlyRole(contract_owner) returns (bool) {
        require(_balances[from] >= amount, "USER_NO_BALANCE");

        _balances[from] -= amount;
        _balances[to] += amount;

        return true;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function paused() public view virtual returns (bool) {
        return _paused;
    }

    function transfer(address to, uint256 amount) public {
        require(_balances[msg.sender] >= amount, "USER_NO_BALANCE");

        _balances[msg.sender] -= amount;
        _balances[to] += amount;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        override
        returns (bool)
    {
        require(spender != address(0), "NO_ZERO_ADDRESS");

        _allowances[spender][msg.sender] += addedValue;
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        override
        returns (bool)
    {
        uint256 currentAllowance = _allowances[spender][msg.sender];
        require(currentAllowance >= subtractedValue, "ALLOW_EXCEEDS");

        _allowances[spender][msg.sender] -= subtractedValue;
        return true;
    }

    function getTransactions() public view returns (AccountPosition[] memory) {
        return _investments[msg.sender];
    }

    function changeAltstreetTreasury(address account)
        public
        payable
        onlyRole(contract_owner)
        returns (bool)
    {
        altstreet_treasury = payable(account);

        return true;
    }

    function changeprice_feed(address contract_feed)
        public
        onlyRole(contract_owner)
        returns (bool)
    {
        price_feed = AggregatorV3Interface(contract_feed);

        return true;
    }

    function changeRatePeriod(uint256 new_rate_period)
        public
        onlyRole(contract_owner)
        returns (bool)
    {
        rate_period = new_rate_period;

        return true;
    }

    function hasInvestment() public view {
        require(_investments[msg.sender].length > 0, "USER_NO_INVESTMENT");
    }

    function canReclaim() public view returns (uint256) {
        hasInvestment();

        uint256 amount_reclaimable = 0;
        for (uint256 idx = 0; idx < _investments[msg.sender].length; idx += 1) {
            if (_investments[msg.sender][idx].datetime_end > block.timestamp) {
                break;
            }
            amount_reclaimable += _investments[msg.sender][idx].volume;
        }

        return amount_reclaimable;
    }

    function activatePool() public payable {
        require(msg.value == 0.01 ether, "USER_NO_BALANCE_ETH");
        require(!_fee_payed[msg.sender], "USER_POOL_OPEN");

        _fee_payed[msg.sender] = true;

        (bool success, ) = altstreet_treasury.call{value: msg.value}("");
        require(success, "FAIL_ETH");
    }

    function deactivatePool() public {
        require(_fee_payed[msg.sender], "USER_POOL_CLOSE");

        _fee_payed[msg.sender] = false;
    }

    function invest(uint256 amount) public payable noReentrancy {
        //uint256 day_in_seconds = 86400;
        uint256 day_in_seconds = 5;

        (bool success, uint256 due_date) = SafeMath.tryAdd(
            block.timestamp,
            //day_in_seconds * lockup_days
            day_in_seconds
        );
        require(success, "SAFEMATH.TRYADD");

        require(_balances[contract_owner] >= amount, "LP_NO_BALANCE");
        require(
            alt_token.balanceOf(msg.sender) >= amount,
            "USER_NO_BALANCE_ALT"
        );
        require(msg.sender.balance >= 100, "USER_NO_BALANCE_ETH");

        _balances[contract_owner] -= amount;
        _balances[msg.sender] += amount;

        // add investment to user
        _investments[msg.sender].push(
            AccountPosition(
                block.timestamp,
                due_date, // 86400000 = 86400 * 1000 (1 day in seconds)
                amount,
                rate_period
            )
        );

        _fee_payed[msg.sender] = false;

        alt_token.transferToTreasury(msg.sender, amount);
    }

    function reclaim(uint256 amount) public payable noReentrancy {
        uint256 reclaim_amount = canReclaim();
        require(reclaim_amount > 0, "CANT_RECLAIM");
        require(reclaim_amount >= amount, "CANT_RECLAIM_AMOUNT");

        uint256 idx = 0;
        uint256 amount_interest = 0;
        uint256 amount_left = amount;
        uint256 amount_min;
        uint256 counter = 0;

        while (amount_left > 0) {
            if (_investments[msg.sender][idx].datetime_end > block.timestamp) {
                break;
            }

            amount_min = Math.min(
                amount_left,
                _investments[msg.sender][idx].volume
            );
            amount_left -= amount_min;
            _investments[msg.sender][idx].volume -= amount_min;

            console.log("starting...");
            console.log(amount_min);
            amount_interest +=
                (amount_min *
                    ((1 * 10**_decimals) +
                        _investments[msg.sender][idx].rate_period /
                        100)) /
                (10**_decimals);

            int256 last_price = 0; //this.getLatestPrice();

            console.log(amount_left);
            console.log(amount_min);
            console.log(_investments[msg.sender][idx].volume);
            console.log(_investments[msg.sender][idx].rate_period);
            console.log(amount_interest);
            //console.log(last_price);
            console.log("=====");

            if (_investments[msg.sender][idx].volume == 0) {
                rearrangeInvestments(idx, counter);
                counter += 1;
            }
        }

        for (idx = 0; idx < counter; idx += 1) {
            _investments[msg.sender].pop();
        }

        // removes LP from user
        _balances[msg.sender] -= amount;
        _balances[contract_owner] += amount;

        // transfer shares with interest to user
        alt_token.transferFromTreasury(msg.sender, amount, amount_interest);
    }

    function rearrangeInvestments(uint256 idx, uint256 counter) private {
        for (
            uint256 innerIdx = idx;
            innerIdx < _investments[msg.sender].length - counter - 1;
            innerIdx += 1
        ) {
            _investments[msg.sender][innerIdx] = _investments[msg.sender][
                innerIdx + 1
            ];
        }
    }

    function getLatestPrice() public view returns (int256) {
        (
            ,
            /*uint80 roundID*/
            int256 price, /*uint startedAt*/ /*uint timeStamp*/ /*uint80 answeredInRound*/
            ,
            ,

        ) = price_feed.latestRoundData();
        return price;
    }
}

