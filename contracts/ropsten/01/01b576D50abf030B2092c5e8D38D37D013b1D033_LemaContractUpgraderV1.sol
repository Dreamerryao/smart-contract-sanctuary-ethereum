// import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import { IConversionPool } from "../interfaces/IConversionPool.sol";
import { IRouterV2 } from "../interfaces/IRouterV2.sol";
import { IExchangeRateFeeder } from "../interfaces/IExchangeRateFeeder.sol";
import { ICurve } from "../interfaces/ICurve.sol";
import { ICurveSwapper } from "../interfaces/ICurveSwapper.sol";
import { IStorage } from "../interfaces/IStorage.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

contract LemaContractUpgraderV1 is IStorage, Initializable {
    struct PoolInfo {
        address outputToken;
        address conversionPool;
    }

    using SafeMath for uint256;
    // using SafeERC20 for IERC20;

    address public owner;
    address public withdrawalOperator;
    address public constant UST = address(0xa47c8bf37f92aBed4A126BDA807A7b7498661acD);
    address public constant exchangeRate = address(0xB12B8247bD1749CC271c55Bb93f6BD2B485C94A7);

    // address public UST;
    // address public exchangeRate;

    mapping(address => mapping(address => uint256)) public inputAmount;
    mapping(address => mapping(address => uint256)) public outputAmount;
    mapping(address => PoolInfo) public poolInfo;

    event Deposit(
        address indexed owner,
        address indexed tokenAddress,
        uint256 amount,
        uint256 createdAt 
    );
    event Withdraw(
        address indexed owner,
        address indexed tokenAddress,
        uint256 amount,
        uint256 createdAt 
    );
    event SendToken(
        address indexed owner,
        address indexed tokenAddress,
        uint256 amount,
        uint256 createdAt 
    );

    function initialize(
        address _withdrawalOperator,
        address[] memory _inputToken,
        PoolInfo[] memory _info
    ) public initializer {
        require(_withdrawalOperator != address(0), "operator is the zero address");
        require(_inputToken.length == _info.length, "Not match");

        for (uint256 i = 0; i < _inputToken.length; i++) {
            poolInfo[_inputToken[i]] = PoolInfo({
               outputToken: _info[i].outputToken,
               conversionPool: _info[i].conversionPool
            });
        }

        owner = msg.sender;
        withdrawalOperator = _withdrawalOperator;

        // UST = address(0xa47c8bf37f92aBed4A126BDA807A7b7498661acD);
        // exchangeRate = address(0xB12B8247bD1749CC271c55Bb93f6BD2B485C94A7);
    }

    function changeOwner(address _newOwner) external onlyOwner {
        require(_newOwner != address(0), "new owner is the zero address");
        owner = _newOwner;
    }

    function changeWithdrawalOperator(address _newWithdrawalOperator) external onlyOwner {
        require(_newWithdrawalOperator != address(0), "new operator is the zero address");
        withdrawalOperator = _newWithdrawalOperator;
    }

    function updatePools(address[] memory _inputToken, PoolInfo[] memory _info) external onlyOwner {
        require(_inputToken.length == _info.length, "Not match");

        for (uint256 i = 0; i < _inputToken.length; i++) {
            poolInfo[_inputToken[i]] = PoolInfo({
               outputToken: _info[i].outputToken,
               conversionPool: _info[i].conversionPool
            });
        }
    }

    function deletePools(address[] memory _inputToken) external onlyOwner {
        for (uint256 i = 0; i < _inputToken.length; i++) {
            delete poolInfo[_inputToken[i]];
        }
    }

    function deposit(address _tokenAddress, uint256 _amount) external supportedToken(_tokenAddress) {
        address outputToken = poolInfo[_tokenAddress].outputToken; // For gas optimization
        address conversionPool = poolInfo[_tokenAddress].conversionPool; // For gas optimization

        uint256 originalOutputAmount = IERC20(outputToken).balanceOf(address(this));
        IERC20(_tokenAddress).transferFrom(msg.sender, address(this), _amount);

        IERC20(_tokenAddress).approve(conversionPool, _amount);
        if (_tokenAddress == UST) {
            address operator = IRouterV2(conversionPool).initDepositStable(_amount);
            console.log('operator', operator);
            IRouterV2(conversionPool).finishDepositStable(operator);
        } else {
            IConversionPool(conversionPool).deposit(_amount);
        }

        inputAmount[_tokenAddress][msg.sender] = inputAmount[_tokenAddress][msg.sender].add(_amount);
        outputAmount[_tokenAddress][msg.sender] = outputAmount[_tokenAddress][msg.sender].add(IERC20(outputToken).balanceOf(address(this))).sub(originalOutputAmount);

        emit Deposit(msg.sender, _tokenAddress, _amount, block.timestamp);
    }

    // TODO: need check inputAmount and outputAmount
    function withdraw(address _tokenAddress, uint256 _amount) external supportedToken(_tokenAddress) {
        address outputToken = poolInfo[_tokenAddress].outputToken; // For gas optimization
        address conversionPool = poolInfo[_tokenAddress].conversionPool; // For gas optimization
        uint256 inputAmountByUSer = inputAmount[_tokenAddress][msg.sender]; // For gas optimization
        uint256 outputAmountByUser = outputAmount[_tokenAddress][msg.sender]; // For gas optimization

        if (_tokenAddress == UST) {
            uint256 pER = IExchangeRateFeeder(exchangeRate).exchangeRateOf(_tokenAddress, false);
            uint256 out = _amount.mul(1e18).div(pER);
            require(out <= outputAmountByUser, "amount should be smaller than output amount");
            IERC20(outputToken).approve(conversionPool, out);
            address operator = IRouterV2(conversionPool).initRedeemStable(_amount);
            console.log('operator', operator);
            IRouterV2(conversionPool).finishRedeemStable(operator);
            if (_amount >= inputAmountByUSer) {
                inputAmountByUSer = 0;
            } else {
                inputAmountByUSer = inputAmountByUSer.sub(_amount);
            }
            inputAmount[_tokenAddress][msg.sender] = inputAmountByUSer;
            outputAmount[_tokenAddress][msg.sender] = outputAmountByUser.sub(out);
        } else {
            address swapper = IConversionPool(conversionPool).swapper();
            Route memory route = ICurveSwapper(swapper).getRoute(UST, _tokenAddress);
            uint256 amount = _amount;
            for (uint256 i = 0; i < route.pools.length; i++) {
                amount = ICurve(route.pools[i]).get_dy(
                    route.indexes[i.mul(2)],
                    route.indexes[i.mul(2).add(1)],
                    amount
                );
            }
            uint256 pER = IExchangeRateFeeder(exchangeRate).exchangeRateOf(_tokenAddress, false);
            uint256 out = amount.mul(1e18).div(pER);
            require(out <= outputAmountByUser, "amount should be smaller than output amount");

            IERC20(outputToken).approve(conversionPool, out);
            IConversionPool(conversionPool).redeem(out);
            if (_amount >= inputAmountByUSer) {
                inputAmountByUSer = 0;
            } else {
                inputAmountByUSer = inputAmountByUSer.sub(_amount);
            }
            inputAmount[_tokenAddress][msg.sender] = inputAmountByUSer;
            outputAmount[_tokenAddress][msg.sender] = outputAmountByUser.sub(out);
        }

        emit Withdraw(msg.sender, _tokenAddress, _amount, block.timestamp);
    }

    function sendToken(address _user, address _tokenAddress, uint256 _amount) external onlyOperator {
        IERC20(_tokenAddress).transferFrom(address(this), _user, _amount);

        emit SendToken(_user, _tokenAddress, _amount, block.timestamp);
    }

    function totalDepositedAmount(address _depositTokenAddress) external view returns (uint256) {
        return inputAmount[_depositTokenAddress][msg.sender];
    }

    function totalOutputAmount(address _depositTokenAddress) external view returns (uint256) {
        return outputAmount[_depositTokenAddress][msg.sender];
    }

    function earnedAmount(address _tokenAddress) public supportedToken(_tokenAddress) view returns (uint256) {
        address conversionPool = poolInfo[_tokenAddress].conversionPool; // For gas optimization
        uint256 returnAmount = 0;

        if (_tokenAddress == UST) {

        } else {
            uint256 pER = IExchangeRateFeeder(exchangeRate).exchangeRateOf(_tokenAddress, false);
            uint256 out = outputAmount[_tokenAddress][msg.sender].mul(pER).div(1e18);

            address swapper = IConversionPool(conversionPool).swapper();
            Route memory route = ICurveSwapper(swapper).getRoute(_tokenAddress, UST);
            uint256 amount = out;
            for (uint256 i = 0; i < route.pools.length; i++) {
                amount = ICurve(route.pools[i]).get_dy(
                    route.indexes[i.mul(2)],
                    route.indexes[i.mul(2).add(1)],
                    amount
                );
            }
            
            returnAmount = amount.sub(inputAmount[_tokenAddress][msg.sender]);
        }

        return returnAmount;
    }

    /**
     * @notice Modifier to check caller is owner or not
     */
    modifier onlyOwner() {
        require(msg.sender == owner, "caller is not the owner");
        _;
    }

    /**
     * @notice Modifier to check caller is operator or not
     */
    modifier onlyOperator() {
        require(msg.sender == withdrawalOperator, "caller is not the operator");
        _;
    }

    /**
     * @notice Modifier to check token is supported token or not
     */
    modifier supportedToken(address _tokenAddress) {
        require(_tokenAddress == UST || poolInfo[_tokenAddress].outputToken != address(0), "Unsupported token");
        _;
    }
}

