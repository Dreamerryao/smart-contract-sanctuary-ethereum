pragma solidity ^0.8.11;

library StrategyTypes {

    struct StrategyStep {
        address module;

        //any encoded extra arguments to pass to module
        bytes extra;

        //step offset in the broader strategy. Used to avoid replay attacks
        uint index;
    }

    struct Authorization {

        //strategy authority that signed off on the execution
        address authority;

        //the root of the entire strategy tree to validate against
        bytes32 merkleRoot;

        //signature of the root hash
        bytes signature;

        //node hashes up to the root of the tree provind that nothing was tampered with
        bytes32[][] stepProof;
    }

    struct FeeDetails {
         //bps fee to apply to relevant steps in the strategy
        uint8 bps;

        //affiliate referral if any
        address affiliate;

        //max allowed gas price in gwei
        uint112 maxGasPrice;

        //portion given to affiliate as a percentage in 18dec format
        uint affiliatePortion;
    }

    struct StrategyRequest {
        //vault to execute strategy through
        address vault;

        //fee details
        FeeDetails fees;

        //individual steps to execute in this request
        StrategyStep[] steps;

        //authorization for all the details of the strategy
        Authorization auth;
    }
}

