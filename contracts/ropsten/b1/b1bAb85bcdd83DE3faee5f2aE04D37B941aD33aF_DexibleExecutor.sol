contract DexibleExecutor is BaseConfig { 

    uint constant POST_EXEC_OP_GAS = 70_000;



    function verifyAuthority(IStrategyVault vault, address authority, bytes32 rootHash, bytes calldata sig) external view returns (bool) {
        return _verifyAuth(vault, authority, rootHash, sig);
    }

    function feeForModule(address module) external view returns (uint8) {
        return LibStorage.getExecutorStorage().moduleBps[module];
    }

    function initialize(ConfigTypes.Config calldata config) public initializer {
        BaseConfig.initConfig(config);
    }


    function execute(StrategyTypes.StrategyRequest calldata request) external onlyRelay {
        uint startGas = gasleft();
        IStrategyVault vault = IStrategyVault(request.vault);
        require(request.fees.maxGasPrice >= tx.gasprice, "Gas price exceeds user-authorized max price");
        //first, let's verify the signature of the root auth hash
        require(_verifyAuth(vault, request.auth.authority, request.auth.merkleRoot, request.auth.signature), "Unauthorized strategy execution");
     
        
        try vault.execute{gas: gasleft() - POST_EXEC_OP_GAS}(request) returns (bool success, string memory fail) {
            if(!success) {
                console.log("Failed to execute strategy", fail);
            }
        } catch Error(string memory e) {
            console.log("Fail reason", e);
        } catch  {
            console.log("Vault execution failed for unknown reason");
        }
        _payGas(request, startGas);
    }


    function _payGas(StrategyTypes.StrategyRequest calldata request, uint startGas) internal {
        uint gasUsed = (startGas - gasleft()) + POST_EXEC_OP_GAS;
        uint gasCost = gasUsed * tx.gasprice;
        (bool s1, string memory e) = IStrategyVault(request.vault).payGas(_msgSender(), gasCost);
        if(!s1) {
            console.log("PAY GAS FAIL REASON:", e);
            //vault failed to pay so we have to revert. Means Dexible didn't check native balance
            //before submission
            revert(e);
        }
    }

    function _verifyAuth(IStrategyVault vault, address authority, bytes32 rootHash, bytes calldata sig) internal view returns (bool) {
        bytes32 messageHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", rootHash));
        (uint8 v, bytes32 r, bytes32 s) = LibSig.splitSignature(sig);
        address actualSigner = ecrecover(messageHash, v, r, s);
        return actualSigner == authority && vault.hasRole(AccessTypes.ADMIN_ROLE, authority);
    }

    function createVault(bytes calldata data) public returns (address vault) {
        return LibVaultFactory.createVault(data);
    }

    function vaultsForAccount(address creator) external view returns(address[] memory) {
        return LibStorage.getExecutorStorage().vaults[creator];
    }

