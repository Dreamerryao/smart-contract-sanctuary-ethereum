pragma solidity ^0.8.11;

import "../types/AccessTypes.sol";

library LibAccess {

    event AddedRole(address indexed wallet, bytes32 role);
    event RevokedRole(address indexed wallet, bytes32 role);

   
    function addAdmin(AccessTypes.AccessControl storage ac, address admin) external {
        _addRole(ac, AccessTypes.ADMIN_ROLE, admin);
    }

    function removeAdmin(AccessTypes.AccessControl storage ac, address admin) external {
        _revokeRole(ac, AccessTypes.ADMIN_ROLE, admin);
    }

    function addDelegate(AccessTypes.AccessControl storage ac, address delegate) external {
        _addRole(ac, AccessTypes.DELEGATE_ROLE, delegate);
    }

    function removeDelegate(AccessTypes.AccessControl storage ac, address delegate) external {
        _revokeRole(ac, AccessTypes.DELEGATE_ROLE, delegate);
    }

    function addModule(AccessTypes.AccessControl storage ac, address a) external {
        _addRole(ac, AccessTypes.MODULE_ROLE, a);
    }

    function removeModule(AccessTypes.AccessControl storage ac, address a) external {
        _revokeRole(ac, AccessTypes.MODULE_ROLE, a);
    }

    function addRelay(AccessTypes.AccessControl storage ac, address actor) external {
        _addRole(ac, AccessTypes.RELAY_ROLE, actor);
    }

    function removeRelay(AccessTypes.AccessControl storage ac, address actor) external {
        _revokeRole(ac, AccessTypes.RELAY_ROLE, actor);
    }

    function hasRole(AccessTypes.AccessControl storage ac, bytes32 role, address actor) external view returns (bool) {
        return ac.roles[role][actor];
    }

    function _initRoles(AccessTypes.AccessControl storage ac, bytes32[] calldata roles, address[] calldata actors) internal {
        require(roles.length == actors.length, "Must be a role for each actor");
        for(uint i=0;i<roles.length;++i) {
            ac.roles[roles[i]][actors[i]] = true;
        }
    }

    function _addRole(AccessTypes.AccessControl storage ac, bytes32 role, address actor) internal  {
        require(actor != address(0), "Invalid address");
        ac.roles[role][actor] = true;
        emit AddedRole(actor, role);

    }

    function _revokeRole(AccessTypes.AccessControl storage ac, bytes32 role, address actor) internal  {
        delete ac.roles[role][actor];
        emit RevokedRole(actor, role);
    }
}

