pragma solidity ^0.8.11;

import "./LibStorage.sol";
import "./LibConfig.sol";
import "../proxy/TraderVaultProxy.sol";
import "../types/ExecutorTypes.sol";

library LibVaultFactory {

    event VaultCreated(address indexed creator, address indexed vault);

    function createVault(bytes calldata data) public returns (address vault) {
        ConfigTypes.Config storage cs = LibStorage.getConfigStorage();

         //we cannot use a clone pattern with beacon because the delegate calls 
         //will fail. Must create a new instance of the vault beacon proxy so that 
         //it properly uses the underlying beacon to make delegate calls to impl.
        TraderVaultProxy proxy = new TraderVaultProxy(cs.masterTradeVaultBeacon, data);
        
        vault = address(proxy);
        ExecutorTypes.ExecutorStorage storage es = LibStorage.getExecutorStorage();
        es.vaults[msg.sender].push(vault);
        emit VaultCreated(msg.sender, vault);
    }
}

