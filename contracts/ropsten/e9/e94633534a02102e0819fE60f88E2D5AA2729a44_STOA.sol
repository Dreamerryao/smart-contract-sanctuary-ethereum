// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "./interfaces/IStoaToken.sol";
import "./interfaces/ISTOA.sol";
import "./interfaces/IRewarder.sol";

import "hardhat/console.sol";

contract STOA is ISTOA, Ownable {
    using SafeMath for uint256;
    using Counters for Counters.Counter;

    uint256 public genesis;

    address public token;
    address public rewarder;
    address public treasury;

    Counters.Counter public userCount;
    mapping(address => User) public users;
    mapping(address => mapping(uint256 => UserSeason)) public meditations;

    uint256 public quarterlyDecay;
    uint256 public yearlyDecay;

    //TODO period duration as deploy var
    uint256 public periodDuration = 90 days;

    uint256 public baseRatio;
    uint256 public stakeRatio;
    uint256 public commitmentRatio;
    uint256 public accomplishmentRatio;

    Period public currentPeriod;
    mapping(uint256 => Period) public periodHistory;

    struct UserSeason {
        uint256 secondsMeditated;
        uint accomplishmentLevel;
        Meditation[] meditations;
    }

    struct Period {
        uint256 index;
        uint256 start;
        uint256 end;
        uint256 totalSupply;
        uint256 rewardsPaid;
        uint256 baseRate;
        uint256 stakeRate;
        uint256 commitmentRewards;
        uint256 accomplishmentRewards;
        Counters.Counter bronze;
        Counters.Counter silver;
        Counters.Counter gold;
    }

    constructor(
        address _owner,
        address _stoaToken,
        address _treasury,
        uint256 _quarterlyDecay,
        uint256 _yearlyDecay,
        uint256 _baseRatio,
        uint256 _stakeRatio,
        uint256 _commitmentRatio,
        uint256 _accomplishmentRatio
    ) {
        token = _stoaToken;
        treasury = _treasury;
        quarterlyDecay = _quarterlyDecay;
        yearlyDecay = _yearlyDecay;
        baseRatio = _baseRatio;
        stakeRatio = _stakeRatio;
        commitmentRatio = _commitmentRatio;
        accomplishmentRatio = _accomplishmentRatio;
        genesis = block.timestamp;
        uint256 rewardsForNextPeriod = _calculateRewardForPeriod();
        Counters.Counter memory bronze;
        Counters.Counter memory silver;
        Counters.Counter memory gold;
        currentPeriod = Period({
            index: 0,
            start: block.timestamp,
            end: block.timestamp + periodDuration,
            totalSupply: rewardsForNextPeriod, //TODO no hardcoded magic number
            rewardsPaid: 0,
            baseRate: _calculateRewardsRateForPeriod(rewardsForNextPeriod, baseRatio, periodDuration),
            stakeRate: _calculateRewardsRateForPeriod(rewardsForNextPeriod, stakeRatio, periodDuration),
            commitmentRewards: rewardsForNextPeriod.mul(commitmentRatio).div(1e18),
            accomplishmentRewards: rewardsForNextPeriod.mul(accomplishmentRatio).div(1e18),
            bronze: bronze,
            silver: silver,
            gold: gold
        });
        if (msg.sender != _owner) {
            transferOwnership(_owner);
        }
    }

    // Events
    event UserCreated(uint256 userCount);
    event UserDeleted(uint256 userCount);
    event MeditationStored(address user, uint256 duration);
    event AccomplishmentAchieved(address user, uint level);
    event PeriodUpdated(uint256 periodCounter, uint256 start, uint256 end);
    event PeriodDurationUpdated(uint256 newDuration);
    event RewardRatioUpdated(string rewardType, uint256 oldRatio, uint256 newRatio);

    // Views

    // External
    function getRewardForPeriod() external view returns (uint256 totalRewards) {
        return _calculateRewardForPeriod();
    }

    function getPeriodIndex() external view returns (uint256 currentIndex) {
        return currentPeriod.index;
    }

    function storeMeditation(uint256 _duration) external updatePeriod {
        if (!_isUser(msg.sender)) {
            users[msg.sender] = User(block.timestamp, false, _duration, block.timestamp, block.timestamp, true);
            userCount.increment();
            emit UserCreated(userCount.current());
        } else {
            users[msg.sender].totalMeditation += _duration;
            users[msg.sender].lastMeditated = block.timestamp;
        }
        uint256 totalMeditatedSeason = meditations[msg.sender][currentPeriod.index].secondsMeditated + _duration;
        meditations[msg.sender][currentPeriod.index].secondsMeditated = totalMeditatedSeason;
        meditations[msg.sender][currentPeriod.index].meditations.push(
            Meditation({ timestamp: block.timestamp, duration: _duration })
        );

        uint accomplishment = getAccomplishmentLevel(totalMeditatedSeason);
        if (accomplishment != meditations[msg.sender][currentPeriod.index].accomplishmentLevel) {
            _updateAccomplishmentLevel(accomplishment);
        }

        emit MeditationStored(msg.sender, _duration);
    }

    function getUserCount() external view returns (uint256) {
        return userCount.current();
    }

    function getMinutesMeditated(address user) external view isUser(user) returns (uint256) {
        return users[user].totalMeditation / 60;
    }

    function getMinutesMeditatedForPeriod(address user, uint256 periodIndex)
        external
        view
        isUser(user)
        returns (uint256)
    {
        return meditations[user][periodIndex].secondsMeditated;
    }

    function getAccomplishmentLevel(address user, uint256 periodIndex)
        public
        view
        isUser(user)
        returns (uint256 accomplishmentLevel)
    {
        return getAccomplishmentLevel(meditations[user][periodIndex].secondsMeditated);
    }

    function getAccomplishmentLevel(uint256 secondsMeditated) public pure returns (uint256 accomplishmentLevel) {
        if (secondsMeditated >= 5000) {
            return 3;
        }
        if (secondsMeditated >= 1800) {
            return 2;
        }
        if (secondsMeditated >= 600) {
            return 1;
        }
        return 0;
    }

    function getAccomplishments()
        external
        view
        returns (
            uint256 bronze,
            uint256 silver,
            uint256 gold
        )
    {
        bronze = currentPeriod.bronze.current();
        silver = currentPeriod.silver.current();
        gold = currentPeriod.gold.current();
    }

    function getLastClaimed(address account) external view isUser(account) returns (uint256) {
        return users[account].lastClaimed;
    }

    function getPeriodDuration() external view returns (uint256) {
        return periodDuration;
    }

    function getPeriodEnd() external view returns (uint256) {
        return currentPeriod.end;
    }

    // Rates
    function getBaseRate(uint256 periodIndex) external view periodExists(periodIndex) returns (uint256) {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.baseRate;
        }
        return periodHistory[periodIndex].baseRate;
    }

    function getStakingRate(uint256 periodIndex) external view periodExists(periodIndex) returns (uint256) {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.stakeRate;
        }
        return periodHistory[periodIndex].stakeRate;
    }

    function getAccomplishmentRewards(uint256 periodIndex) external view periodExists(periodIndex) returns (uint256) {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.accomplishmentRewards;
        }
        return periodHistory[periodIndex].accomplishmentRewards;
    }

    function getCommitmentRewards(uint256 periodIndex) external view periodExists(periodIndex) returns (uint256) {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.commitmentRewards;
        }
        return periodHistory[periodIndex].commitmentRewards;
    }

    // TODO keep user reward balances
    // TODO only from rewarder for user
    function notifyRewardsPaid(address user, uint256 amount) external {
        currentPeriod.rewardsPaid += amount;
        if (block.timestamp >= currentPeriod.end) {
            _updatePeriodExpires();
        }
    }

    // Public
    function createUser() public {
        require(!_isUser(msg.sender), "User already exists");
        users[msg.sender] = User(block.timestamp, false, 0, block.timestamp, 0, true);
        userCount.increment();
        emit UserCreated(userCount.current());
    }

    function deleteUser() public isUser(msg.sender) {
        delete users[msg.sender];
        userCount.decrement();
        emit UserDeleted(userCount.current());
    }

    function getUser(address account) external view isUser(account) returns (User memory) {
        return users[account];
    }

    function getMeditation(address user, uint256 index) external view isUser(user) returns (Meditation memory) {
        require(meditations[user][currentPeriod.index].meditations.length >= index, "Meditation not found");
        return meditations[user][currentPeriod.index].meditations[index];
    }

    function getLatestMeditation(address user) external view isUser(user) returns (Meditation memory) {
        require(meditations[user][currentPeriod.index].meditations.length > 0, "User has not meditated yet");
        uint256 lastIndex = meditations[user][currentPeriod.index].meditations.length - 1;
        return meditations[user][currentPeriod.index].meditations[lastIndex];
    }

    function getMeditationCount(address user) external view isUser(user) returns (uint256 meditationCount) {
        return meditations[user][currentPeriod.index].meditations.length;
    }

    // TODO go over periods
    function getSecondsMeditatedSince(address user, uint256 timestamp)
        public
        view
        isUser(user)
        returns (uint256 sumDuration)
    {
        sumDuration = 0;
        for (uint256 i = meditations[user][currentPeriod.index].meditations.length; i > 0; i--) {
            Meditation memory med = meditations[user][currentPeriod.index].meditations[i - 1];
            if (med.timestamp >= timestamp) {
                sumDuration += med.duration;
            } else {
                return sumDuration;
            }
        }
    }

    // Amounts
    //TODO period duration to period history
    function getBaseRewardForPeriod(uint256 periodIndex)
        external
        view
        periodExists(periodIndex)
        returns (uint256 rewardAmount)
    {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.baseRate.mul(periodDuration);
        }
        return periodHistory[periodIndex].baseRate.mul(periodDuration);
    }

    function getStakingRewardForPeriod(uint256 periodIndex)
        external
        view
        periodExists(periodIndex)
        returns (uint256 rewardAmount)
    {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.stakeRate.mul(periodDuration);
        }
        return periodHistory[periodIndex].stakeRate.mul(periodDuration);
    }

    function getAccomplishmentRewardForPeriod(uint256 periodIndex)
        external
        view
        periodExists(periodIndex)
        returns (uint256 rewardAmount)
    {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.accomplishmentRewards;
        }
        return periodHistory[periodIndex].accomplishmentRewards;
    }

    function getCommitmentRewardForPeriod(uint256 periodIndex)
        external
        view
        periodExists(periodIndex)
        returns (uint256 rewardAmount)
    {
        if (periodIndex == currentPeriod.index) {
            return currentPeriod.commitmentRewards;
        }
        return periodHistory[periodIndex].commitmentRewards;
    }

    // Internal

    function _isUser(address userAddress) internal view returns (bool isIndeed) {
        return users[userAddress].exists;
    }

    //TODO to 0 at end of Y10 Q4
    //TODO update leftover reward accounting
    function _updatePeriodExpires() internal {
        periodHistory[currentPeriod.index] = currentPeriod;

        uint256 leftoverRewards = currentPeriod.totalSupply - currentPeriod.rewardsPaid;
        uint256 rewardsForReserves = leftoverRewards.mul(commitmentRatio.add(accomplishmentRatio)).div(1e18);
        uint256 rewardsForNextPeriod = _calculateRewardForPeriod();
        Counters.Counter memory bronze;
        Counters.Counter memory silver;
        Counters.Counter memory gold;
        Period memory newPeriod = Period({
            index: currentPeriod.index + 1,
            start: currentPeriod.end,
            end: currentPeriod.end + periodDuration,
            totalSupply: rewardsForNextPeriod,
            rewardsPaid: 0,
            baseRate: _calculateRewardsRateForPeriod(rewardsForNextPeriod, baseRatio, periodDuration),
            stakeRate: _calculateRewardsRateForPeriod(rewardsForNextPeriod, stakeRatio, periodDuration),
            commitmentRewards: rewardsForNextPeriod.mul(commitmentRatio).div(1e18),
            accomplishmentRewards: rewardsForNextPeriod.mul(accomplishmentRatio).div(1e18),
            bronze: bronze,
            silver: silver,
            gold: gold
        });
        currentPeriod = newPeriod;
        _token().mintReward(_treasury(), rewardsForReserves.div(10));
        emit PeriodUpdated(currentPeriod.index, currentPeriod.start, currentPeriod.end);
    }

    function _updateAccomplishmentLevel(uint level) internal {
        uint oldLvl = meditations[msg.sender][currentPeriod.index].accomplishmentLevel;
        if (level == 1) {
            currentPeriod.bronze.increment();
            meditations[msg.sender][currentPeriod.index].accomplishmentLevel = 1;
        }
        if (level == 2) {
            currentPeriod.silver.increment();
            meditations[msg.sender][currentPeriod.index].accomplishmentLevel = 2;
        }
        if (level == 3) {
            currentPeriod.gold.increment();
            meditations[msg.sender][currentPeriod.index].accomplishmentLevel = 3;
        }

        if (oldLvl == 1) {
            currentPeriod.bronze.decrement();
        }
        if (oldLvl == 2) {
            currentPeriod.silver.decrement();
        }
        if (oldLvl == 3) {
            currentPeriod.gold.decrement();
        }
        emit AccomplishmentAchieved(msg.sender, level);
    }

    function _getYearIndex() internal view returns (uint256 yearIndex) {
        return (block.timestamp - genesis).div(90 days * 4);
    }

    function _getQuarterIndex() internal view returns (uint256 quarterIndex) {
        return (block.timestamp - genesis).div(90 days).mod(4);
    }

    //TODO no hardcoded magic number
    function _calculateRewardForPeriod() internal view returns (uint256 rewardForPeriod) {
        uint256 rewardForYear = _token().getRewardCap().mul(yearlyDecay.div(2**_getYearIndex())).div(1e18);

        uint256 currentQuarterFraction = quarterlyDecay.mul(840**_getQuarterIndex()).div(1000**_getQuarterIndex());
        rewardForPeriod = rewardForYear.mul(currentQuarterFraction).div(1e18);
    }

    function _calculateRewardsRateForPeriod(
        uint256 rewardForPeriod,
        uint256 rewardRatio,
        uint256 periodDuration
    ) internal pure returns (uint256 rewardRate) {
        return rewardForPeriod.mul(rewardRatio).div(1e18).div(periodDuration);
    }

    // Config
    function setPeriodDuration(uint256 _rewardsDuration) external onlyOwner {
        periodDuration = _rewardsDuration;
        emit PeriodDurationUpdated(periodDuration);
    }

    // TODO validate updated values
    function setBaseRatio(uint256 _baseRatio) external onlyOwner {
        uint256 oldRatio = baseRatio;
        baseRatio = _baseRatio;
        emit RewardRatioUpdated("BaseRewards", oldRatio, baseRatio);
    }

    function setStakingRatio(uint256 _stakeRatio) external onlyOwner {
        uint256 oldRatio = stakeRatio;
        stakeRatio = _stakeRatio;
        emit RewardRatioUpdated("StakingRewards", oldRatio, stakeRatio);
    }

    function setAccomplishmentRatio(uint256 _accomplishmentRatio) external onlyOwner {
        uint256 oldRatio = accomplishmentRatio;
        accomplishmentRatio = _accomplishmentRatio;
        emit RewardRatioUpdated("AccomplishmentRewards", oldRatio, accomplishmentRatio);
    }

    function setCommitmentRatio(uint256 _commitmentRatio) external onlyOwner {
        uint256 oldRatio = commitmentRatio;
        commitmentRatio = _commitmentRatio;
        emit RewardRatioUpdated("CommitmentRewards", oldRatio, commitmentRatio);
    }

    // Modifiers
    modifier isUser(address account) {
        require(_isUser(account), "User does not exist");
        _;
    }

    modifier periodExists(uint256 periodIndex) {
        require(periodIndex <= currentPeriod.index, "Period out of range");
        _;
    }

    modifier updatePeriod() {
        if (block.timestamp >= currentPeriod.end) {
            _updatePeriodExpires();
        }
        _;
    }

    // System
    function _token() internal view returns (IStoaToken) {
        return IStoaToken(token);
    }

    function _rewarder() internal view returns (IRewarder) {
        return IRewarder(rewarder);
    }

    function _treasury() internal view returns (address) {
        return treasury;
    }
}

