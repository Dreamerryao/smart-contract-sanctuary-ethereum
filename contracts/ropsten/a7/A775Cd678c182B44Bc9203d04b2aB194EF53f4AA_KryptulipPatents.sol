pragma solidity ^0.8.0;

import {ERC721} from "@rari-capital/solmate/src/tokens/ERC721.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

contract KryptulipPatents is ERC721, Ownable {
    mapping(uint256 => string) private _uris;
    string private _uri;

    /// @dev Store the validity in number and not boolean to save gas
    mapping(uint256 => uint8) private _validityOf;

    uint256 public totalSupply;

    constructor() ERC721("Kryptulip Patents Token", "KPT") {}

    function tokenURI(uint256 id) public view override returns (string memory) {
        string memory uri_ = _uris[id];
        if (bytes(uri_).length > 0) return uri_;
        return _uri;
    }

    function setUri(uint256 id, string memory uri_) external onlyOwner {
        _uris[id] = uri_;
    }

    function setUri(string memory uri_) external onlyOwner {
        _uri = uri_;
    }

    function setPatentValidity(uint256 patent, bool isValid)
        external
        onlyOwner
    {
        if (isValid) {
            _validityOf[patent] = 1;
        } else {
            _validityOf[patent] = 2;
        }
    }

    function burn(uint256 patent) external onlyOwner {
        _burn(patent);
    }

    function validityOf(uint256 patent) external view returns (bool) {
        return _validityOf[patent] == 1;
    }

    function mint(uint256 patentNumber) external {
        _mint(msg.sender, patentNumber);
    }

    function destroy(uint256 patent) external {
        require(ownerOf[patent] == msg.sender, "NOT_OWNER");
        _burn(patent);
    }
}

