pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "hardhat/console.sol";

import "./LogReporting.sol";
import "./IERC721Deposit.sol";

contract ERC721Deposit is IERC721Deposit, Context, AccessControl, LogReporting {
    bytes32 public constant GOVERNOR_ROLE = keccak256("GOVERNOR_ROLE");
    uint64 private unlockPeriod;

    address[] private _contractWhitelistArray;
    mapping(address => bool) private _contractWhitelist;
    mapping(address => ContractDepositState[]) private _allTokenDepositByContract;
    mapping(address => UserDepositState[]) private _allTokenDepositByUser;
    mapping(address => mapping(uint256 => DepositState)) private _tokenDepositByContract;

    constructor(address _governor) LogReporting("ERC721Deposit") {
        _setupRole(GOVERNOR_ROLE, _governor);
        unlockPeriod = 2 weeks;
    }

    modifier _onlyGovernor() {
        require(
            hasRole(GOVERNOR_ROLE, _msgSender()),
            reportError("You don't have the governor role to perform this operation")
        );
        _;
    }

    modifier _notWhitelistedContract(address contractAddr) {
        require(
            _isContractWhitelisted(contractAddr) == false,
            reportError("Contract address already whitelisted")
        );
        _;
    }

    modifier _allContractWhitelisted(DepositRequest[] memory tokenDatas) {
        bool allContractWhitelisted = true;
        address notWhitelisted = address(0);
        for (uint256 i = 0; i < tokenDatas.length; i++) {
            if (_isContractWhitelisted(tokenDatas[i].contractAddr) == false) {
                allContractWhitelisted = false;
                notWhitelisted = tokenDatas[i].contractAddr;
            }
        }
        require(
            allContractWhitelisted == true,
            reportError("Contract address not whitelisted", addressToString(notWhitelisted))
        );
        _;
    }

    function getWhitelist() public view _onlyGovernor returns (address[] memory) {
        return _contractWhitelistArray;
    }

    function isWhitelist(address contractAddr) public view _onlyGovernor returns (bool) {
        return _isContractWhitelisted(contractAddr);
    }

    function addContract(address contractAddr)
        public
        _onlyGovernor
        _notWhitelistedContract(contractAddr)
    {
        _contractWhitelist[contractAddr] = true;
        _contractWhitelistArray.push(contractAddr);
    }

    function removeContract(address contractAddr) public _onlyGovernor {
        require(contractAddr != address(0), reportError("Contract address can't be 0x"));
        require(
            _isContractWhitelisted(contractAddr) != false,
            reportError("Contract address not whitelisted")
        );
        _contractWhitelist[contractAddr] = false;
        for (uint64 i = 0; i < _contractWhitelistArray.length; i++) {
            if (_contractWhitelistArray[i] == contractAddr) {
                _contractWhitelistArray[i] = _contractWhitelistArray[
                    _contractWhitelistArray.length - 1
                ];
                _contractWhitelistArray.pop();
                break;
            }
        }
    }

    function getTokenDepositedByContract(address contractAddr)
        public
        view
        _onlyGovernor
        returns (ContractDepositState[] memory)
    {
        return _allTokenDepositByContract[contractAddr];
    }

    function getTokenDepositedByUser(address userAddr)
        public
        view
        returns (UserDepositState[] memory)
    {
        return _allTokenDepositByUser[userAddr];
    }

    function getStandardUnlockPeriod() public view returns (uint64) {
        return unlockPeriod;
    }

    function _isContractWhitelisted(address contractAddr) private view returns (bool) {
        if (_contractWhitelist[contractAddr] == true) {
            return true;
        }
        return false;
    }

    function _depositToken(uint256 tokenId, address contractAddr) internal {
        IERC721 tokenInstance = IERC721(contractAddr);
        DepositState storage ds = _tokenDepositByContract[contractAddr][tokenId];
        require(
            ds.owner == address(0),
            reportError(
                "you already deposited token: ",
                string(
                    abi.encodePacked(
                        Strings.toString(tokenId),
                        " & ",
                        addressToString(contractAddr)
                    )
                )
            )
        );
        tokenInstance.transferFrom(_msgSender(), address(this), tokenId);
        ds.owner = _msgSender();
        ds.unlockPeriod = unlockPeriod;
        _allTokenDepositByContract[contractAddr].push(ContractDepositState(tokenId, ds));
        _allTokenDepositByUser[_msgSender()].push(UserDepositState(tokenId, ds, contractAddr));
    }

    function depositTokens(DepositRequest[] memory tokenDatas)
        public
        _allContractWhitelisted(tokenDatas)
    {
        for (uint256 i = 0; i < tokenDatas.length; i++) {
            _depositToken(tokenDatas[i].tokenId, tokenDatas[i].contractAddr);
        }
        emit TokenDeposited(_msgSender(), tokenDatas);
    }

    function _requestWithdraw(uint256 tokenId, address contractAddr)
        internal
        returns (
            uint256,
            DepositState memory,
            address
        )
    {
        DepositState storage ds = _tokenDepositByContract[contractAddr][tokenId];
        require(
            ds.owner == _msgSender(),
            reportError(
                "Token is not deposited : ",
                string(
                    abi.encodePacked(
                        Strings.toString(tokenId),
                        " & ",
                        addressToString(contractAddr)
                    )
                )
            )
        );
        require(
            ds.lockedUntil == 0,
            reportError(
                "You already requested to unlock token:",
                string(
                    abi.encodePacked(
                        Strings.toString(tokenId),
                        " & ",
                        addressToString(contractAddr)
                    )
                )
            )
        );
        ds.lockedUntil = uint64(block.timestamp + ds.unlockPeriod);
        updateTokenFromStorage(tokenId, contractAddr, ds);
        return (tokenId, ds, contractAddr);
    }

    function requestWithdraws(DepositRequest[] memory tokenDatas) public {
        UserDepositState[] memory tokenRequested = new UserDepositState[](tokenDatas.length);
        for (uint256 i = 0; i < tokenDatas.length; i++) {
            (uint256 tokenId, DepositState memory ds, address contractAddr) = _requestWithdraw(
                tokenDatas[i].tokenId,
                tokenDatas[i].contractAddr
            );
            tokenRequested[i] = UserDepositState(tokenId, ds, contractAddr);
        }
        emit TokenWithdrawalRequest(_msgSender(), tokenRequested);
    }

    function _withdraw(uint256 tokenId, address contractAddr) internal {
        DepositState storage ds = _tokenDepositByContract[contractAddr][tokenId];
        require(
            ds.owner == _msgSender(),
            reportError(
                "Token is not deposited : ",
                string(
                    abi.encodePacked(
                        Strings.toString(tokenId),
                        " & ",
                        addressToString(contractAddr)
                    )
                )
            )
        );
        require(
            ds.lockedUntil != 0 && uint64(block.timestamp) > ds.lockedUntil,
            reportError(
                "Time of withdrawal did not come yet for token",
                string(
                    abi.encodePacked(
                        Strings.toString(tokenId),
                        " & ",
                        addressToString(contractAddr)
                    )
                )
            )
        );
        IERC721 tokenInstance = IERC721(contractAddr);
        tokenInstance.transferFrom(address(this), _msgSender(), tokenId);
        ds.owner = address(0);
        ds.unlockPeriod = 0;
        ds.lockedUntil = 0;
        removeTokenFromStorage(tokenId, contractAddr);
    }

    function updateTokenFromStorage(
        uint256 tokenId,
        address contractAddr,
        DepositState memory ds
    ) internal {
        for (uint256 i = 0; i < _allTokenDepositByContract[contractAddr].length; i++) {
            if (_allTokenDepositByContract[contractAddr][i].tokenId == tokenId) {
                _allTokenDepositByContract[contractAddr][i].ds = ds;
                break;
            }
        }
        for (uint256 i = 0; i < _allTokenDepositByUser[_msgSender()].length; i++) {
            if (_allTokenDepositByUser[_msgSender()][i].tokenId == tokenId) {
                _allTokenDepositByUser[_msgSender()][i].ds = ds;
                break;
            }
        }
    }

    function removeTokenFromStorage(uint256 tokenId, address contractAddr) internal {
        for (uint256 i = 0; i < _allTokenDepositByContract[contractAddr].length; i++) {
            if (_allTokenDepositByContract[contractAddr][i].tokenId == tokenId) {
                _allTokenDepositByContract[contractAddr][i] = _allTokenDepositByContract[
                    contractAddr
                ][_allTokenDepositByContract[contractAddr].length - 1];
                _allTokenDepositByContract[contractAddr].pop();
                break;
            }
        }
        for (uint256 i = 0; i < _allTokenDepositByUser[_msgSender()].length; i++) {
            if (_allTokenDepositByUser[_msgSender()][i].tokenId == tokenId) {
                _allTokenDepositByUser[_msgSender()][i] = _allTokenDepositByUser[_msgSender()][
                    _allTokenDepositByUser[_msgSender()].length - 1
                ];
                _allTokenDepositByUser[_msgSender()].pop();
                break;
            }
        }
    }

    function withdraws(DepositRequest[] memory tokenDatas) public {
        for (uint256 i = 0; i < tokenDatas.length; i++) {
            _withdraw(tokenDatas[i].tokenId, tokenDatas[i].contractAddr);
        }
        emit TokenWithdrawal(_msgSender(), tokenDatas);
    }
}

