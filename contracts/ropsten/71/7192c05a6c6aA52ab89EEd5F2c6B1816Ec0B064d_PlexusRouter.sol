pragma solidity >=0.6.12;

import "../interfaces/IPlexusPair.sol";
import "./SafeMath.sol";
import "../interfaces/IPlexusFactory.sol";
import "hardhat/console.sol";

library PlexusLibrary {
    using SafeMath for uint256;

    // returns sorted token addresses, used to handle return values from pairs sorted in this order
    function sortTokens(address tokenA, address tokenB) internal pure returns (address token0, address token1) {
        require(tokenA != tokenB, "PlexusLibrary: IDENTICAL_ADDRESSES");
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), "PlexusLibrary: ZERO_ADDRESS");
    }

    // calculates the CREATE2 address for a pair without making any external calls
    function pairFor(
        address factory,
        address tokenA,
        address tokenB
    ) internal pure returns (address pair) {
        (address token0, address token1) = sortTokens(tokenA, tokenB);
        pair = address(
            uint256(
                keccak256(
                    abi.encodePacked(
                        hex"ff",
                        factory,
                        keccak256(abi.encodePacked(token0, token1)),
                        hex"6a1f7d39e47739deb43175f23dc05f667cebfc9a92eaed7b7b982f765cecbb31" // init code hash
                    )
                )
            )
        );
    }

    // fetches and sorts the reserves for a pair
    function getReserves(
        address factory,
        address tokenA,
        address tokenB
    ) internal view returns (uint256 reserveA, uint256 reserveB) {
        (address token0, ) = sortTokens(tokenA, tokenB);
        (uint256 reserve0, uint256 reserve1, ) = IPlexusPair(pairFor(factory, tokenA, tokenB)).getReserves();
        (reserveA, reserveB) = tokenA == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
    }

    // given some amount of an asset and pair reserves, returns an equivalent amount of the other asset
    function quote(
        uint256 amountA,
        uint256 reserveA,
        uint256 reserveB
    ) internal pure returns (uint256 amountB) {
        require(amountA > 0, "PlexusLibrary: INSUFFICIENT_AMOUNT");
        require(reserveA > 0 && reserveB > 0, "PlexusLibrary: INSUFFICIENT_LIQUIDITY");
        amountB = amountA.mul(reserveB) / reserveA;
    }

    // given an input amount of an asset and pair reserves, returns the maximum output amount of the other asset
    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountOut) {
        require(amountIn > 0, "PlexusLibrary: INSUFFICIENT_INPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "PlexusLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 amountInWithFee = amountIn.mul(997);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        uint256 denominator = reserveIn.mul(1000).add(amountInWithFee);
        amountOut = numerator / denominator;
    }

    // given an output amount of an asset and pair reserves, returns a required input amount of the other asset
    function getAmountIn(
        uint256 amountOut,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountIn) {
        require(amountOut > 0, "PlexusLibrary: INSUFFICIENT_OUTPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "PlexusLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 numerator = reserveIn.mul(amountOut).mul(1000);
        uint256 denominator = reserveOut.sub(amountOut).mul(997);
        amountIn = (numerator / denominator).add(1);
    }

    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut,
        uint256 fee
    ) internal pure returns (uint256 amountOut) {
        require(amountIn > 0, "PlexusLibrary: INSUFFICIENT_INPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "PlexusLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 numerator = amountIn.mul(10000 - fee).mul(reserveOut);
        uint256 denominator = reserveIn.mul(10000).add(amountIn.mul(10000 - fee));
        amountOut = numerator / denominator;
    }

    // given an output amount of an asset and pair reserves, returns a required input amount of the other asset
    function getAmountIn(
        uint256 amountOut,
        uint256 reserveIn,
        uint256 reserveOut,
        uint256 fee
    ) internal pure returns (uint256 amountIn) {
        require(amountOut > 0, "PlexusLibrary: INSUFFICIENT_OUTPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "PlexusLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 numerator = reserveIn.mul(amountOut).mul(10000);
        uint256 denominator = reserveOut.sub(amountOut).mul(10000 - fee);

        amountIn = (numerator / denominator).add(1);
    }

    // performs chained getAmountOut calculations on any number of pairs
    function getAmountsOut(
        address factory,
        uint256 amountIn,
        address[] memory path
    ) internal view returns (uint256[] memory amounts) {
        require(path.length >= 2, "PlexusLibrary: INVALID_PATH");
        amounts = new uint256[](path.length);
        amounts[0] = amountIn;
        for (uint256 i; i < path.length - 1; i++) {
            uint256 fee = IPlexusFactory(factory).getPairFee(IPlexusFactory(factory).getPair(path[i], path[i + 1]));
            (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, path[i], path[i + 1]);
            amounts[i + 1] = getAmountOut(amounts[i], reserveIn, reserveOut, fee);
        }
    }

    // performs chained getAmountIn calculations on any number of pairs
    function getAmountsIn(
        address factory,
        uint256 amountOut,
        address[] memory path
    ) internal view returns (uint256[] memory amounts) {
        require(path.length >= 2, "PlexusLibrary: INVALID_PATH");
        amounts = new uint256[](path.length);
        amounts[amounts.length - 1] = amountOut;
        for (uint256 i = path.length - 1; i > 0; i--) {
            uint256 fee = IPlexusFactory(factory).getPairFee(IPlexusFactory(factory).getPair(path[i - 1], path[i]));

            (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, path[i - 1], path[i]);
            amounts[i - 1] = getAmountIn(amounts[i], reserveIn, reserveOut, fee);
        }
    }

    // amount / neu1 / wklay /  nr / wklay
    function getPaybackAmountsOut(
        address factory,
        uint256 amountOut,
        address tokenA,
        address tokenB,
        address NR,
        address WKLAY
    ) internal view returns (uint256 amount) {
        uint256 fee = IPlexusFactory(factory).getPairFee(IPlexusFactory(factory).getPair(tokenA, tokenB));
        if (tokenB == NR) {
            (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, tokenA, NR);
            amount = getAmountOut(amountOut, reserveIn, reserveOut);
            amount = amount.mul(fee) / 100000;
        } else if (tokenB == WKLAY) {
            address[] memory addresses = new address[](3);
            addresses[0] = tokenA;
            addresses[1] = WKLAY;
            addresses[2] = NR;
            uint256[] memory amounts = new uint256[](3);
            amounts[0] = amountOut;
            for (uint256 i = 0; i < amounts.length - 1; i++) {
                (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, addresses[i], addresses[i + 1]);
                amounts[i + 1] = getAmountOut(amounts[i], reserveIn, reserveOut);
            }
            amount = amounts[amounts.length - 1].mul(fee) / 100000;
        }
    }

    function getPaybackAmountsOut(
        address factory,
        uint256 amountOut,
        address tokenA,
        address NR
    ) internal view returns (uint256 amount) {
        uint256 fee = IPlexusFactory(factory).getPairFee(IPlexusFactory(factory).getPair(tokenA, NR));
        (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, tokenA, NR);
        amount = getAmountOut(amountOut, reserveIn, reserveOut);
        amount = amount.mul(fee) / 100000;
    }

    // calculates the CREATE2 address for a pair without making any external calls
    function getPair(
        address factory,
        address token0,
        address token1
    ) internal view returns (bool) {
        if (IPlexusFactory(factory).getPair(token0, token1) == address(0)) {
            return false;
        } else {
            return true;
        }
    }

    function getRony(
        uint256 _feeTokenAmount,
        uint256 _buybackPercent,
        uint256 _ronyPercent
    ) internal view returns (uint256 buybackAmount, uint256 ronyAmount) {
        buybackAmount = _feeTokenAmount.mul(_buybackPercent.sub(_ronyPercent)) / 10000;
        ronyAmount = _feeTokenAmount.mul(_ronyPercent) / 10000;
    }

    function getPayback(
        address _factory,
        address _tokenAddress,
        address _tokenBAddress,
        address _neuron,
        address _wklay,
        uint256 _tokenAmount,
        uint256 _tokenBAmount
    ) internal view returns (uint256 paybackAmount) {
        uint256 fee = IPlexusFactory(_factory).getPairFee(
            IPlexusFactory(_factory).getPair(_tokenAddress, _tokenBAddress)
        );
        if (_tokenAddress == _neuron) {
            paybackAmount = _tokenAmount.mul(fee) / 10000;
        } else if (_tokenBAddress == _neuron) {
            paybackAmount = _tokenBAmount.mul(fee) / 10000;
        } else if (getPair(_factory, _tokenAddress, _neuron)) {
            paybackAmount = getPaybackAmountsOut(_factory, _tokenAmount, _tokenAddress, _neuron, _neuron, _wklay);
        } else if (getPair(_factory, _tokenAddress, _wklay)) {
            paybackAmount = getPaybackAmountsOut(_factory, _tokenAmount, _tokenAddress, _wklay, _neuron, _wklay);
        } else if (_tokenBAddress == _wklay) {
            paybackAmount = getPaybackAmountsOut(_factory, _tokenBAmount, _wklay, _neuron);
        } else if (getPair(_factory, _tokenBAddress, _neuron)) {
            paybackAmount = getPaybackAmountsOut(_factory, _tokenBAmount, _tokenBAddress, _wklay, _neuron, _wklay);
        } else if (getPair(_factory, _tokenBAddress, _wklay)) {
            paybackAmount = getPaybackAmountsOut(_factory, _tokenBAmount, _tokenBAddress, _wklay, _neuron, _wklay);
        } else {
            paybackAmount = 0;
        }
    }
}

