pragma solidity =0.6.12;

import "../libraries/PlexusLibrary.sol";
import "../libraries/SafeMath.sol";
import "../libraries/TransferHelper.sol";
import "../interfaces/IPlexusRouter.sol";
import "../interfaces/IPlexusFactory.sol";
import "../interfaces/IERC20.sol";
import "../interfaces/IWETH.sol";
import "../interfaces/IPlexusPair.sol";
import "../Ownable.sol";

interface IFeeDistributor {
    function updateFeeToken(address feeToken_, uint256 amount) external;
}

interface IPayback {
    function paybackClaim(address _user, uint256 amount) external;
}

contract PlexusRouter is IPlexusRouter, Ownable {
    using SafeMath for uint256;

    address public immutable override factory;
    address public immutable override WETH;
    uint256 public createPairFee = 100 * 1e18;
    address public payback;
    address public buyBackWallet;
    address public plexus;
    uint256 public swapStartBlock;
    bool public ronyOpen;
    uint256 public ronyPercent;
    uint256 public buybackPercent = 24;
    IFeeDistributor public ronyClub;
    address public airdrop;
    // Payback public payback;

    mapping(address => address) public referralAddress;
    mapping(address => mapping(address => uint256)) public referralTokenamount;
    event referralSet(address invitedAddress, address referralAddress);
    event swap(address pairAddress, address fromToken, uint256 fromTokenAmount, address toToken, uint256 toTokenAmount);
    event liquidityadd(address aToken, uint256 aTokenAmount, address bToken, uint256 bTokenAmount);
    event liquidityremove(uint256 lpAmount, address aToken, uint256 aTokenAmount, address bToken, uint256 bTokenAmount);

    address private deadAddress = 0x000000000000000000000000000000000000dEaD;

    function setCreatePairFee(uint256 _createPairFee) public onlyOwner {
        createPairFee = _createPairFee;
    }

    function setReferralAddress(address _user) external {
        require(_user != msg.sender && _user != deadAddress);
        require(referralAddress[msg.sender] == address(0));
        referralAddress[msg.sender] = _user;
        emit referralSet(msg.sender, _user);
    }

    modifier ensure(uint256 deadline) {
        require(deadline >= block.timestamp, "PlexusRouter: EXPIRED");
        _;
    }

    constructor(
        address _factory,
        address _WETH,
        address payable _buyBackWallet,
        address payable _payback,
        address _plexus
    ) public {
        factory = _factory;
        WETH = _WETH;
        buyBackWallet = _buyBackWallet;
        payback = _payback;
        plexus = _plexus;
    }

    receive() external payable {
        assert(msg.sender == WETH); // only accept ETH via fallback from the WETH contract
    }

    // **** ADD LIQUIDITY ****
    function _addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin
    ) internal virtual returns (uint256 amountA, uint256 amountB) {
        // create the pair if it doesn't exist yet
        require(IPlexusFactory(factory).getPair(tokenA, tokenB) != address(0));
        (uint256 reserveA, uint256 reserveB) = PlexusLibrary.getReserves(factory, tokenA, tokenB);

        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountADesired, amountBDesired);
        } else {
            uint256 amountBOptimal = PlexusLibrary.quote(amountADesired, reserveA, reserveB);
            if (amountBOptimal <= amountBDesired) {
                require(amountBOptimal >= amountBMin, "PlexusRouter: INSUFFICIENT_B_AMOUNT");
                (amountA, amountB) = (amountADesired, amountBOptimal);
            } else {
                uint256 amountAOptimal = PlexusLibrary.quote(amountBDesired, reserveB, reserveA);
                assert(amountAOptimal <= amountADesired);
                require(amountAOptimal >= amountAMin, "PlexusRouter: INSUFFICIENT_A_AMOUNT");
                (amountA, amountB) = (amountAOptimal, amountBDesired);
            }
        }
    }

    function addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    )
        external
        virtual
        override
        ensure(deadline)
        returns (
            uint256 amountA,
            uint256 amountB,
            uint256 liquidity
        )
    {
        (amountA, amountB) = _addLiquidity(tokenA, tokenB, amountADesired, amountBDesired, amountAMin, amountBMin);
        address pair = PlexusLibrary.pairFor(factory, tokenA, tokenB);
        TransferHelper.safeTransferFrom(tokenA, msg.sender, pair, amountA);
        TransferHelper.safeTransferFrom(tokenB, msg.sender, pair, amountB);
        liquidity = IPlexusPair(pair).mint(to);
        emit liquidityadd(tokenA, amountA, tokenB, amountB);
    }

    function addLiquidityETH(
        address token,
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountETHMin,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (
            uint256 amountToken,
            uint256 amountETH,
            uint256 liquidity
        )
    {
        (amountToken, amountETH) = _addLiquidity(
            token,
            WETH,
            amountTokenDesired,
            msg.value,
            amountTokenMin,
            amountETHMin
        );
        address pair = PlexusLibrary.pairFor(factory, token, WETH);
        TransferHelper.safeTransferFrom(token, msg.sender, pair, amountToken);
        IWETH(WETH).deposit{ value: amountETH }();
        assert(IWETH(WETH).transfer(pair, amountETH));
        liquidity = IPlexusPair(pair).mint(to);
        emit liquidityadd(token, amountToken, WETH, amountETH);
        // refund dust eth, if any
        if (msg.value > amountETH) TransferHelper.safeTransferETH(msg.sender, msg.value - amountETH);
    }

    // **** REMOVE LIQUIDITY ****
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    ) public virtual override ensure(deadline) returns (uint256 amountA, uint256 amountB) {
        address pair = PlexusLibrary.pairFor(factory, tokenA, tokenB);
        IPlexusPair(pair).transferFrom(msg.sender, pair, liquidity); // send liquidity to pair
        (uint256 amount0, uint256 amount1) = IPlexusPair(pair).burn(to);
        (address token0, address token1) = PlexusLibrary.sortTokens(tokenA, tokenB);
        (amountA, amountB) = tokenA == token0 ? (amount0, amount1) : (amount1, amount0);
        require(amountA >= amountAMin, "PlexusRouter: INSUFFICIENT_A_AMOUNT");
        require(amountB >= amountBMin, "PlexusRouter: INSUFFICIENT_B_AMOUNT");
        emit liquidityremove(liquidity, token0, amountA, token1, amountB);
    }

    function removeLiquidityETH(
        address token,
        uint256 liquidity,
        uint256 amountTokenMin,
        uint256 amountETHMin,
        address to,
        uint256 deadline
    ) public virtual override ensure(deadline) returns (uint256 amountToken, uint256 amountETH) {
        (amountToken, amountETH) = removeLiquidity(
            token,
            WETH,
            liquidity,
            amountTokenMin,
            amountETHMin,
            address(this),
            deadline
        );
        TransferHelper.safeTransfer(token, to, amountToken);
        IWETH(WETH).withdraw(amountETH);
        TransferHelper.safeTransferETH(to, amountETH);
        emit liquidityremove(liquidity, token, amountToken, WETH, amountETH);
    }

    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(
        uint256[] memory amounts,
        address[] memory path,
        address _to
    ) internal virtual {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = PlexusLibrary.sortTokens(input, output);
            uint256 amountOut = amounts[i + 1];
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOut)
                : (amountOut, uint256(0));
            address to = i < path.length - 2 ? PlexusLibrary.pairFor(factory, output, path[i + 2]) : _to;
            IPlexusPair(PlexusLibrary.pairFor(factory, input, output)).swap(amount0Out, amount1Out, to, new bytes(0));
            emit swap(PlexusLibrary.pairFor(factory, input, output), path[i], amounts[i], path[i + 1], amounts[i + 1]);
        }
    }

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address payable to,
        uint256 deadline
    ) external virtual override ensure(deadline) returns (uint256[] memory amounts) {
        amounts = PlexusLibrary.getAmountsOut(factory, amountIn, path);
        require(amounts[amounts.length - 1] >= amountOutMin, "PlexusRouter: INSUFFICIENT_OUTPUT_AMOUNT");
        uint256 referralPaybackAmount = PlexusLibrary.getPayback(
            factory,
            path[0],
            path[path.length - 1],
            plexus,
            WETH,
            amounts[0],
            amounts[amounts.length - 1]
        );

        // 레퍼럴 등록을 했을경우 레퍼럴 보상 지급

        if (IERC20(plexus).balanceOf(payback) > referralPaybackAmount && referralPaybackAmount > 0) {
            if (referralAddress[msg.sender] != address(0)) {
                address referralAddress = referralAddress[msg.sender];
                // TransferHelper.safeTransferFrom(path[0], msg.sender, referralAddress[msg.sender], referralReward);
                // 레퍼럴주소에게 레퍼럴 지급
                IPayback(payback).paybackClaim(payable(referralAddress), referralPaybackAmount);
                referralTokenamount[referralAddress][plexus] = referralTokenamount[referralAddress][plexus].add(
                    referralPaybackAmount
                );
                referralPaybackAmount = referralPaybackAmount.mul(2);
            }
            IPayback(payback).paybackClaim(to, referralPaybackAmount);
        }
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            PlexusLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, to);
    }

    function swapTokensForExactTokens(
        uint256 amountOut,
        uint256 amountInMax,
        address[] calldata path,
        address payable to,
        uint256 deadline
    ) external virtual override ensure(deadline) returns (uint256[] memory amounts) {
        amounts = PlexusLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= amountInMax, "PlexusRouter: EXCESSIVE_INPUT_AMOUNT");
        uint256 referralPaybackAmount = PlexusLibrary.getPayback(
            factory,
            path[0],
            path[path.length - 1],
            plexus,
            WETH,
            amounts[0],
            amounts[amounts.length - 1]
        );

        // 레퍼럴 등록을 했을경우 레퍼럴 보상 지급
        if (payback != address(0)) {
            if (IERC20(plexus).balanceOf(payback) > referralPaybackAmount && referralPaybackAmount > 0) {
                if (referralAddress[msg.sender] != address(0)) {
                    address referralAddress = referralAddress[msg.sender];
                    // TransferHelper.safeTransferFrom(path[0], msg.sender, referralAddress[msg.sender], referralReward);
                    // 레퍼럴주소에게 레퍼럴 지급
                    IPayback(payback).paybackClaim(payable(referralAddress), referralPaybackAmount);
                    referralTokenamount[referralAddress][plexus] = referralTokenamount[referralAddress][plexus].add(
                        referralPaybackAmount
                    );
                    referralPaybackAmount = referralPaybackAmount.mul(2);
                }
                IPayback(payback).paybackClaim(to, referralPaybackAmount);
            }
        }
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            PlexusLibrary.pairFor(factory, path[0], path[1]),
            amounts[0]
        );
        _swap(amounts, path, to);
    }

    function swapExactETHForTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address payable to,
        uint256 deadline
    ) external payable virtual override ensure(deadline) returns (uint256[] memory amounts) {
        require(path[0] == WETH, "PlexusRouter: INVALID_PATH");
        amounts = PlexusLibrary.getAmountsOut(factory, msg.value, path);
        require(amounts[amounts.length - 1] >= amountOutMin, "PlexusRouter: INSUFFICIENT_OUTPUT_AMOUNT");
        uint256 referralPaybackAmount = PlexusLibrary.getPaybackAmountsOut(factory, amounts[0], path[0], plexus);
        if (IERC20(plexus).balanceOf(payback) > referralPaybackAmount && referralPaybackAmount > 0) {
            if (referralAddress[msg.sender] != address(0)) {
                address referralAddress = referralAddress[msg.sender];
                // 레퍼럴주소에게 레퍼럴 지급
                IPayback(payback).paybackClaim(payable(referralAddress), referralPaybackAmount);
                referralTokenamount[referralAddress][plexus] = referralTokenamount[referralAddress][plexus].add(
                    referralPaybackAmount
                );
                referralPaybackAmount = referralPaybackAmount.mul(2);
            }
            IPayback(payback).paybackClaim(to, referralPaybackAmount);
        }
        IWETH(WETH).deposit{ value: amounts[0] }();
        assert(IWETH(WETH).transfer(PlexusLibrary.pairFor(factory, path[0], path[1]), amounts[0]));
        _swap(amounts, path, to);
    }

