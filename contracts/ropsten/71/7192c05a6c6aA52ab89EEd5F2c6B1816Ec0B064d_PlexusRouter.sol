        );
        // 0.24%의 바이백풀로 이동하는 값
        if (IERC20(plexus).balanceOf(payback) > referralPaybackAmount && referralPaybackAmount > 0) {
            if (referralAddress[msg.sender] != address(0)) {
                address referralAddress = referralAddress[msg.sender];
                // 레퍼럴주소에게 레퍼럴 지급
                IPayback(payback).paybackClaim(payable(referralAddress), referralPaybackAmount);
                referralTokenamount[referralAddress][plexus] = referralTokenamount[referralAddress][plexus].add(
                    referralPaybackAmount
                );
                referralPaybackAmount = referralPaybackAmount.mul(2);
            }
