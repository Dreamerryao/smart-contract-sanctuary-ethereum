    ) external virtual override ensure(deadline) returns (uint256[] memory amounts) {
        require(path[path.length - 1] == WETH, "PlexusRouter: INVALID_PATH");
        amounts = PlexusLibrary.getAmountsIn(factory, amountOut, path);
        require(amounts[0] <= amountInMax, "PlexusRouter: EXCESSIVE_INPUT_AMOUNT");
        uint256 referralPaybackAmount = PlexusLibrary.getPayback(
