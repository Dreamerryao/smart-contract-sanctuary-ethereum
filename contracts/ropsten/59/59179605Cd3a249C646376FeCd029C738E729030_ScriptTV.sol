import "./ScriptPay/IScriptPay.sol";
import "./ScriptGlasses/IScriptGlasses.sol";
import "./ScriptGem/IScriptGem.sol";

// enums
import "./enums/NFTType.sol";

/// Amount cannot be zero
error ZeroAmount();

/// Balance cannot be zero
error ZeroBalance();

/// User has already claimed freemium glasses or Collateral
error AlreadyClaimed();

/// Collateral is not unlocked yet
error StillLocked();

/// User does not own asset
error AssetNotOwned();

/// The provided signature is invalid
error InvalidSignature();

/// Transaction has already been executed
error TransactionAlreadyExecuted();

/// Insufficient asset supply, current `supply`
error InsufficientSupply(uint24 supply);

/// User has insufficient balance, Needed `required` but has `balance`
error InsufficientFunds(uint256 balance, uint256 required);

/// Gem to be equipped is not compatible with the glasses, Needed `glassesType` but has `gemType`
error GemNotCompatible(NFTType gemType, NFTType glassesType);

/// Provided type is out of range
error TypeOutOfRange(NFTType _type);

/// @title Script TV
/// @author @n4beel
/// @notice Contract for Script TV - watch to earn platform
contract ScriptTV is Ownable {
    // bytes32 type for storing transaction hashes
    using ECDSA for bytes32;

    // Address of the SPAY Token
    IScriptPay public immutable spay;

    // Address of Glasses contract
    IScriptGlasses public immutable scriptGlasses;

    // Address of Gem contract
    IScriptGem public immutable gem;

    // 30 Days (30 * 24 * 60 * 60)
    uint256 public constant LOCK_DURATION = 2592000;

    // Glass struct for storing data about different glasses types
    // maxSupply - max number of glasses that can be minted
    // mintedSupply - number of glasses already minted
    // collateral - amount of SPAY to be locked when minting a pair of glasses
    // cost - amount of SPAY burnt when minting a pair of glasses
    struct Glass {
        uint24 maxSupply;
        uint24 mintedSupply;
        uint256 collateral;
        uint256 glassCost;
        uint256 gemCost;
    }

    // Glass struct for storing data about different glasses types
    // maxSupply - max number of glasses that can be minted
    // mintedSupply - number of glasses already minted
    // collateral - amount of SPAY to be locked when minting a pair of glasses
    // cost - amount of SPAY burnt when minting a pair of glasses
    struct Lock {
        uint256 amount;
        uint256 lockedTill;
        bool unlocked;
    }

    // mapping to store glasses data
    mapping(uint256 => Glass) public glasses;

    // has a transaction been executed
    mapping(bytes32 => bool) public executed;

    // does a user own a pair of freemium glasses
    mapping(address => bool) public freemiumOwner;

    // Amount of SPAY tokens locked by an address
    // <user address> =>
    //      <token ID> => <Collateral info of a pair of glasses>
    mapping(address => mapping(uint256 => Lock)) public lockedBalances;

    /**
     * @notice Emitted when user receives reward for watching content
     * @param to address of the receiver
     * @param value amount of SPAY to be paid out
     * @param nonce transaction ID
     */
    event EarningPayout(address indexed to, uint256 value, uint256 nonce);

    /**
     * @notice Emitted when user receives reward for referring
     * @param to address of the receiver
     * @param value amount of SPAY to be paid out
     * @param referral id of the referral
     * @param nonce transaction ID
     */
    event ReferralPayout(
        address indexed to,
        uint256 value,
        string indexed referral,
        uint256 indexed nonce
    );

    /**
     * @notice Emitted when the user burns SPAY for recharging their glasses
     * @param from address of the spender
     * @param value amount of SPAY paid
     * @param glassId id of glasses being recharged
     * @param nonce transaction ID
     */
    event RechargeGlasses(
        address indexed from,
        uint256 value,
        uint256 indexed glassId,
        uint256 indexed nonce
    );

    /**
     * @notice Emitted when the user burns SPAY for opening a loot box
     * @param from address of the spender
     * @param value amount of SPAY paid
     * @param lootBox id of loot box
     * @param nonce transaction ID
     */
    event OpenLootBox(
        address indexed from,
        uint256 value,
        string indexed lootBox,
        uint256 indexed nonce
    );

    /**
     * @notice Emitted when the user locks SPAY
     * @param user address of the user locking SPAY
     * @param value amount of SPAY locked
     */
    event LockSPAY(address indexed user, uint256 indexed value);

    /**
     * @notice Emitted when the user unlocks SPAY
     * @param user address of the user unlocking SPAY
     * @param value amount of SPAY unlocked
     */
    event UnlockSPAY(address indexed user, uint256 indexed value);

    /**
     * @notice Emitted when the user mints a pair of glasses
     * @param user address of the user minting the glasses
     * @param tokenID ID of glass being minted
     * @param glassType type of glasses being minted
     */
    event GlassMinted(
        address indexed user,
        uint256 indexed tokenID,
        NFTType indexed glassType
    );

    /**
     * @notice Emitted when a pair of glasses are confiscated
     * @param user address of the user
     * @param tokenID ID of glass being confiscated
     */
    event GlassConfiscated(address indexed user, uint256 indexed tokenID);

    /**
     * @notice Emitted when the user mints gems
     * @param user address of the user minting the gems
     * @param tokenID ID of gem being minted
     * @param gemType type of gem minted
     */
    event GemMinted(
        address indexed user,
        uint256 indexed tokenID,
        NFTType indexed gemType
    );

    /**
     * @notice Emitted when the user equips a gem into a glasses' socket
     * @param glassID ID of the glasses
     * @param gemID ID of the gem
     * @param gemType type of gem equipped
     */
    event GemEquipped(
        uint256 indexed glassID,
        uint256 indexed gemID,
        NFTType gemType
    );

    /**
     * @notice Constructor
     * @param _spay SPAY contract address
     * @param _scriptGlasses glasses contract address
     * @param _scriptGem gem contract address
     */
    constructor(
        IScriptPay _spay,
        IScriptGlasses _scriptGlasses,
        IScriptGem _scriptGem
    ) {
        spay = _spay;
        scriptGlasses = _scriptGlasses;
        gem = _scriptGem;

        // data of freemium glasses
        glasses[0] = Glass(15000, 0, 50e18, 0, 0);

        // data of common glasses
        glasses[1] = Glass(750000, 0, 20e18, 10e18, 10e18);

        // data of rare glasses
        glasses[2] = Glass(200000, 0, 30e18, 20e18, 20e18);

        // data of superscript glasses
        glasses[3] = Glass(50000, 0, 40e18, 30e18, 30e18);
    }

    /**
     * @notice Locks caller's tokens as collateral
     * @param _amount amount of SPAY being locked
     */
    function _lockTokens(uint256 _tokenId, uint256 _amount) private {
        console.log("Locked amount for token %s => %s", _tokenId, _amount);
        spay.transferFrom(msg.sender, address(this), _amount);
        lockedBalances[msg.sender][_tokenId] = Lock(
            _amount,
            block.timestamp + LOCK_DURATION,
            false
        );
        emit LockSPAY(msg.sender, _amount);
    }

    /**
     * @notice Unlocks the collateral of a glasses
     * @dev needs to be updated for timelock
     */
    function unlockTokens(uint256 _tokenId) external {
        Lock storage lockedBalance = lockedBalances[msg.sender][_tokenId];

        if (lockedBalance.unlocked) {
            revert AlreadyClaimed();
        }
        if (lockedBalance.lockedTill > block.timestamp) {
            revert StillLocked();
        }

        lockedBalance.unlocked = true;
        spay.transfer(msg.sender, lockedBalance.amount);
        emit UnlockSPAY(msg.sender, lockedBalance.amount);
    }

    /**
     * @notice Mints a pair of Freemium glasses, locks caller's collateral
     */
    function mintFreemiumGlasses() external {
        Glass storage glassType = glasses[uint256(NFTType.Freemium)];
        uint256 collateral = glassType.collateral;

        if (freemiumOwner[msg.sender]) {
            revert AlreadyClaimed();
        }
        if (glassType.mintedSupply >= glassType.maxSupply) {
            revert InsufficientSupply(glassType.mintedSupply);
        }
        if (spay.balanceOf(msg.sender) < collateral) {
            revert InsufficientFunds(spay.balanceOf(msg.sender), collateral);
        }

        glassType.mintedSupply++;
        uint256 tokenID = scriptGlasses.safeMint(msg.sender, NFTType.Freemium);
        _lockTokens(tokenID, collateral);
        freemiumOwner[msg.sender] = true;

        emit GlassMinted(msg.sender, tokenID, NFTType.Freemium);
    }

    /**
     * @notice Mints a pair of common, rare or superscript glasses, burns caller's SPAY and locks their collateral
     * @param _type type of glasses to be minted
     */
    function mintGlasses(NFTType _type) external {
        if (uint256(_type) < 1 || uint256(_type) > 3) {
            revert TypeOutOfRange(_type);
        }
        Glass storage glassType = glasses[uint256(_type)];
        uint256 cost = glassType.glassCost;
        uint256 collateral = glassType.collateral;
        if (glassType.mintedSupply >= glassType.maxSupply) {
            revert InsufficientSupply(glassType.mintedSupply);
        }
        if (spay.balanceOf(msg.sender) < cost + collateral) {
            revert InsufficientFunds(
                spay.balanceOf(msg.sender),
                cost + collateral
            );
        }

        spay.burnFrom(msg.sender, cost);
        glassType.mintedSupply++;
        uint256 tokenID = scriptGlasses.safeMint(msg.sender, _type);
        _lockTokens(tokenID, collateral);

        emit GlassMinted(msg.sender, tokenID, _type);
    }

    /**
     * @notice Confiscates user's glasses and transfers to Script TV
     * @param _tokenID ID of glasses to be confiscated
     */
    function confiscateGlasses(uint256 _tokenID) external onlyOwner {
        address glassesOwner = scriptGlasses.ownerOf(_tokenID);
        scriptGlasses.transferFrom(glassesOwner, address(this), _tokenID);
        emit GlassConfiscated(glassesOwner, _tokenID);
    }

    /**
     * @notice Mints specified gem and burns caller's SPAY
     * @param _type type of gem to be minted
     */
    function mintGem(NFTType _type) external {
        uint256 cost = glasses[uint256(_type)].gemCost;
        if (spay.balanceOf(msg.sender) < cost) {
            revert InsufficientFunds(spay.balanceOf(msg.sender), cost);
        }

        spay.burnFrom(msg.sender, cost);
        uint256 tokenID = gem.safeMint(msg.sender, _type);

        emit GemMinted(msg.sender, tokenID, _type);
    }

    /**
     * @notice Equips specified gem into the socket of specified glasses and burns the gem
     * @param _glassID id of glasses to equip the gem into
     * @param _gemID id of gem to be equipped
     */
    function equipGem(uint256 _glassID, uint256 _gemID) external {
        address glassOwner = scriptGlasses.ownerOf(_glassID);
        address gemOwner = gem.ownerOf(_gemID);
        NFTType glassType = scriptGlasses.glassType(_glassID);
        NFTType gemType = gem.gemType(_gemID);

        if (glassOwner != msg.sender || gemOwner != msg.sender) {
            revert AssetNotOwned();
        }
        if (glassType != gemType) {
            revert GemNotCompatible(gemType, glassType);
        }

        gem.burn(_gemID);
        emit GemEquipped(_glassID, _gemID, gemType);
    }

    /**
     * @notice Creates signature for verification
     * @param _to address of the receiver
     * @param _amount amount of SPAY to be sent
     * @param _nonce unique integer
     * @return bytes32 transaction signature
     */
    function getTxHash(
        address _to,
        uint256 _amount,
        uint256 _nonce
    ) public view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n",
                    address(this),
                    _to,
                    _amount,
                    _nonce
                )
            );
    }

    /**
     * @notice Modifier to check if the signature is created by the owner
     * @param _amount amount of SPAY to be sent
     * @param _nonce unique integer
     * @param _sig transaction signature
     */
    modifier verifySignature(
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig
    ) {
        if (_amount == 0) {
            revert ZeroAmount();
        }

        bytes32 txHash = getTxHash(msg.sender, _amount, _nonce);
        address signer = txHash.recover(_sig);
        bool valid = signer == owner();

        if (executed[txHash]) {
            revert TransactionAlreadyExecuted();
        }

        if (!valid) {
            revert InvalidSignature();
        }

        executed[txHash] = true;

        _;
    }

    /**
     * @notice Rewards SPAY earned from watching content
     * @param _amount amount of spay to be rewarded
     * @param _nonce unique nonce of the transaction
     * @param _sig signature signed by the owner
     * @dev Signature replay prevention
     */
    function earningPayout(
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig
    ) external verifySignature(_amount, _nonce, _sig) {
        spay.mint(msg.sender, _amount);
        emit EarningPayout(msg.sender, _amount, _nonce);
    }

    /**
     * @notice Rewards SPAY earned through referrals
     * @param _amount amount of spay to be rewarded
     * @param _nonce unique nonce of the transaction
     * @param _sig signature signed by the owner
     * @param _referral id of referral
     * @dev Signature replay prevention
     */
    function referralPayout(
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        string memory _referral
    ) external verifySignature(_amount, _nonce, _sig) {
        spay.mint(msg.sender, _amount);
        emit ReferralPayout(msg.sender, _amount, _referral, _nonce);
    }

    /**
     * @notice Burns SPAY for recharging the glass
     * @param _amount amount of SPAY to be burnt
     * @param _nonce unique nonce of the transaction
     * @param _sig signature signed by the owner
     * @param _glassId glasses being recharged
     */
    function rechargeGlasses(
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        uint256 _glassId
    ) external verifySignature(_amount, _nonce, _sig) {
        spay.burnFrom(msg.sender, _amount);
        emit RechargeGlasses(msg.sender, _amount, _glassId, _nonce);
    }

    /**
     * @notice Burns SPAY for recharging the glass
     * @param _amount amount of SPAY to be burnt
     * @param _nonce unique nonce of the transaction
     * @param _sig signature signed by the owner
     * @param _lootBox id of loot box
     */
    function openLootBox(
        uint256 _amount,
        uint256 _nonce,
        bytes memory _sig,
        string memory _lootBox
    ) external verifySignature(_amount, _nonce, _sig) {
        spay.burnFrom(msg.sender, _amount);
        emit OpenLootBox(msg.sender, _amount, _lootBox, _nonce);
    }
}

