  function _increaseReflectionVaultCollectionAccount(address _id, uint256 _rewardPerItem) internal isCollectionAccountInitialized(_id) {
    for (uint256 i = 1; i <= COLLECTION_ACCOUNTS[_id].supply; i++) {
