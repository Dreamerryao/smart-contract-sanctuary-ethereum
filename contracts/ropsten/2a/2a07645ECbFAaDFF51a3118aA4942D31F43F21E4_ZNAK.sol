pragma solidity 0.8.0;

interface IZNAK {
  function mint(address _to) external returns(uint);
  function burn(uint _share, address _to) external;

  // view functions
  function underlyingAsset() external returns (address);
  function leftAmount(address _minter) external view returns (uint);
  function mintEpochDuration() external view returns(uint);
  function getMintAllowances(address _minter) external view returns (uint lastMintEpoch,uint left, uint allowancePerEpoch);

  function quantityToShare(uint _quantity) external view returns (uint);
  function shareToQuantity(uint _share) external view returns (uint);

  function borrow(uint _collaterialAmount, uint _borrowAmount, address _to) external;
  function repay(uint _debtId) external;
  function liquidate(uint _debtId, address _to) external;
}

interface IZNAKManagement {
  function setPenaltyFee(uint _penaltyNominator, uint _penaltyDenominator) external;
  function setPermamentFee(uint _permanentFeeNominator, uint _permanentFeeDenominator) external;
  function setLTV(uint _LTVNominator, uint _LTVDenominator) external;
  function setAllowance(address _minter, uint _amount ) external;
  function setFoundationFeeReceiver(address _foundationFeeReceiver) external;
  function setFoundationFee(uint _foundationFeeNominator, uint _foundationFeeDenominator) external;
  function setInterest(uint _interestNominator, uint _interestDenominator) external;
}

