import "../interfaces/IZNAK.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../libraries/persentMath.sol";
import "hardhat/console.sol";

contract ZNAK is IZNAK, IZNAKManagement, ERC20, Ownable {
    constructor(
        string memory _name,
        string memory _symbol,
        address _underlyingAsset,
        uint _mintEpochLen
    ) ERC20(_name, _symbol) {
        underlyingAsset = _underlyingAsset;
        mintEpochLen = _mintEpochLen;
    }
    
    address public override underlyingAsset;
    uint public trackingBalance;
    // ZNAK that locked in the contract
    uint public lockedZNAK;
    // underlying asset that was taken via REPO
    uint public virtualBalance;

    // Loan To Value %
    // if loanToValue = (N: 50, D: 100), then the loan will be 50% of the underlying asset value
    uint LTVNominator;
    uint LTVDenominator;

    // Penalty -- when LTV < 100%, debt should be liqudated, liqudator will burn loan body and get penalty
    // fee, for (N: 1, D: 100), penalty fee = 1% of the loan body
    uint penaltyNominator;
    uint penaltyDenominator;

    // Debt rate
    uint interestNominator;
    uint interestDenominator;

    // Payment for the debt creation service
    uint permanentFeeNominator;
    uint permanentFeeDenominator;

    uint foundationFeeNominator;
    uint foundationFeeDenominator;

    address foundationFeeReceiver;
    // calculations are made of current block number / epochlen = current epoch
    uint mintEpochLen;
    using Counters for Counters.Counter;
    Counters.Counter private lastDebtId;

    struct MintInfo {
        uint lastMintEpoch;
        // lefted quantity in current epoch
        uint left;
        uint allowancePerEpoch;
    }

    struct Debt {
        // ZNAK amount
        uint lendAmount;
        // how much USDC was taken
        uint borrowAmount;
        address borrower;
        uint interestNominator;
        uint interestDenominator;
        // in what block user took borow
        uint borrowBlockNumber;
        bool isReturned;
        bool isLiquidated;
    }

    mapping(address => MintInfo) mintAllowances;

    mapping(uint => Debt) debts;

    function isDebtActual(uint _debtId) public view returns (bool) {
        return !debts[_debtId].isReturned && !debts[_debtId].isLiquidated;
    }

    function setFoundationFeeReceiver(address _foundationFeeReceiver) 
        public 
        override
        onlyOwner 
    {
        require(_foundationFeeReceiver != address(0));
        foundationFeeReceiver = _foundationFeeReceiver;
    }

    function setFoundationFee(uint _foundationFeeNominator, uint _foundationFeeDenominator) 
        public 
        override
        onlyOwner 
    {
        require(_foundationFeeNominator > 0 && _foundationFeeDenominator > 0, "ZNAK: invalid parameters");
        require(_foundationFeeNominator < _foundationFeeDenominator, "ZNAK: Nominator must be less than Denominator");
        foundationFeeNominator = _foundationFeeNominator;
        foundationFeeDenominator = _foundationFeeDenominator;
    }

    function getMintAllowances(address _minter) public view override returns (uint lastMintEpoch,uint left, uint allowancePerEpoch) {
        MintInfo memory info = mintAllowances[_minter];
        return (info.lastMintEpoch, info.left, info.allowancePerEpoch);
    }

      function mintEpochDuration() external view override returns(uint) {
          return mintEpochLen;
      }

    function debtLTV(uint _debtId)
        public
        view
        returns (uint _currentBorrowsAmount, uint _LTV)
    {
        Debt memory debt = debts[_debtId];
        require(!debt.isReturned, "ZNAK REPO: debt is returned");
        require(!debt.isLiquidated, "ZNAK REPO: debt is liquidated");

        uint term = block.number - debt.borrowBlockNumber;
        uint _interest = (term * debt.interestNominator);

        _currentBorrowsAmount = debt.borrowAmount + ((debt.borrowAmount * _interest) / debt.interestDenominator);
        _LTV = (quantityToShare(debt.lendAmount) * 100 / _currentBorrowsAmount);
    }

    function setInterest(uint _interestNominator, uint _interestDenominator)
        public
        override
        onlyOwner
    {
        require(_interestNominator > 0 && _interestDenominator > 0, "ZNAK: invalid parameters");
        require(
            _interestNominator <= _interestDenominator,
            "ZNAK: Nominator must be less than Denominator"
        );
        interestNominator = _interestNominator;
        interestDenominator = _interestDenominator;
    }

    function setPenaltyFee(uint _penaltyNominator, uint _penaltyDenominator)
        public
        override
        onlyOwner
    {
        require(
            _penaltyNominator > 0 && _penaltyDenominator > 0,
            "ZNAK: invalid params"
        );
        require(
            _penaltyNominator < _penaltyDenominator,
            "ZNAK: invalid params"
        );
        penaltyNominator = _penaltyNominator;
        penaltyDenominator = _penaltyDenominator;
    }

    function setPermamentFee(
        uint _permanentFeeNominator,
        uint _permanentFeeDenominator
    ) public override onlyOwner {
        require(_permanentFeeNominator > 0 && _permanentFeeDenominator > 0);
        require(_permanentFeeNominator < _permanentFeeDenominator);
        permanentFeeNominator = _permanentFeeNominator;
        permanentFeeDenominator = _permanentFeeDenominator;
    }

    function setLTV(uint _LTVNominator, uint _LTVDenominator)
        public
        override
        onlyOwner
    {
        require(
            _LTVNominator > 0 && _LTVDenominator > 0,
            "ZNAK: invalid params"
        );
        require(_LTVNominator < _LTVDenominator, "ZNAK: invalid params");
        LTVNominator = _LTVNominator;
        LTVDenominator = _LTVDenominator;
    }

    function setAllowance(address _minter, uint _amount)
        public
        override
        onlyOwner
    {
        mintAllowances[_minter].lastMintEpoch = block.number / mintEpochLen;
        mintAllowances[_minter].allowancePerEpoch = _amount;
    }

    // helper functions

    function shareToQuantity(uint share)
        public
        view
        override
        returns (uint quantity)
    {
        quantity = (share * (trackingBalance + virtualBalance)) / totalSupply();
    }

    function quantityToShare(uint quantity)
        public
        view
        override
        returns (uint share)
    {
        if (totalSupply() == 0) {
            return quantity;
        } else {
            share = (quantity * totalSupply()) / (trackingBalance + virtualBalance);
        }
    }

    function leftAmount(address _minter) public view override returns (uint) {
        uint currentEpoch = block.number / mintEpochLen;
        if (mintAllowances[_minter].lastMintEpoch < currentEpoch) {
            return mintAllowances[_minter].allowancePerEpoch;
        } else {
            return mintAllowances[_minter].left;
        }
    }

    function updateMintAllowance(MintInfo storage minter) private {
        if (
            mintAllowances[msg.sender].lastMintEpoch <
            block.number / mintEpochLen
        ) {
            mintAllowances[msg.sender].left = minter.allowancePerEpoch;
            mintAllowances[msg.sender].lastMintEpoch =
                block.number /
                mintEpochLen;
        }
    }

    // behaviour functions

    function mint(address _to) public override returns(uint) {
        MintInfo storage minter = mintAllowances[msg.sender];
        updateMintAllowance(minter);

        uint amount = quantityToShare(IERC20(underlyingAsset).balanceOf(address(this)) - trackingBalance);
        require(amount > 0, "ZNAK: no tokens to mint");

        // if amount to mint is less than left amount in current epoch, mint left balance
        uint ZNAKAmount = amount > minter.left ? minter.left : amount;
        uint foundationFee  = (ZNAKAmount * foundationFeeNominator / foundationFeeDenominator);
        require(foundationFee > 0, "ZNAK: amount is too low");

        trackingBalance = IERC20(underlyingAsset).balanceOf(address(this));
        minter.left -= ZNAKAmount;

        _mint(foundationFeeReceiver, foundationFee);
        _mint(_to, ZNAKAmount - foundationFee);
        return amount;
    }

    function burn(uint _share, address _to) public override {
        require(_share > 0, "ZNAK: invalid params");
        uint burnAmount = shareToQuantity(_share);
        IERC20(underlyingAsset).transfer(_to, burnAmount);
        _burn(msg.sender, _share);
    }

    function borrow(
        uint _collaterialAmount,
        uint _amountBorrow,
        address _to
    ) public override {
        require(_amountBorrow > 0, "ZNAK: invalid params");
        address borrower = msg.sender;

        // calculate amount of underlying asset that will go to the borrower
        // permament fee will be stay on contract and added to ZNAK pool
        // example:
        //      user1: 100 ZNAK, user2: 100 ZNAK, ZNAKreserve = 200 USDC
        //          ZNAK = 1 USDC
        //          LTV = 80%, permament fee = 1%
        //          user1 deposit 100 ZNAK, and can take
        //          100 * 1 (znak price) * 80% (LTV) =
        //          80 USDC - 1% = 79,2 USDC go to the borrower
        //          and he need to return 80 USDC
        //      user1: 0 ZNAK, user2: 100 ZNAK, ZNAKreserve = 120,8 USDC
        //      where 20 is risk LTV, and 0.8 will stay from ZNAK holders
        _transfer(borrower, address(this), _collaterialAmount);

        // calc permament fee
        uint permamentFee = _amountBorrow * permanentFeeNominator / permanentFeeDenominator;

        // check LTV
        uint maxAmountBorrow = (shareToQuantity(_collaterialAmount) *
            LTVNominator) / LTVDenominator;
        require(
            maxAmountBorrow >= _amountBorrow,
            "ZNAK REPO: not enough collateral"
        );

        uint256 id = lastDebtId.current();

        debts[id] = Debt({
            lendAmount: _collaterialAmount,
            borrowAmount: _amountBorrow,
            borrower: msg.sender,
            interestNominator: interestNominator,
            interestDenominator: interestDenominator,
            borrowBlockNumber: block.number,
            isReturned: false,
            isLiquidated: false
        });
        lastDebtId.increment();
        lockedZNAK += _collaterialAmount;
        virtualBalance += _amountBorrow;
        IERC20(underlyingAsset).transfer(_to, _amountBorrow - permamentFee);
        trackingBalance = IERC20(underlyingAsset).balanceOf(address(this));
    }

    function repay(uint _debtId) public override {
        require(_debtId <= lastDebtId.current(), "ZNAK REPO: invalid params");
        Debt storage debt = debts[_debtId];

        require(!debt.isReturned, "ZNAK REPO: debt is already returned");
        require(!debt.isLiquidated, "ZNAK REPO: debt is already liquidated");

        // calculate Loan to Value
        (uint currentBorrowAmount, uint LTV) = debtLTV(_debtId);

        // calculate blocks APY on debt
        uint _interest = ((block.number - debt.borrowBlockNumber) *
            debt.interestNominator) / debt.interestDenominator;
        debt.borrowAmount += _interest;
        debt.borrowBlockNumber = block.number;
        
        uint UADelta = IERC20(underlyingAsset).balanceOf(address(this)) - trackingBalance;

        // if collaterial ZNAK value minus % from debt, lend should be liqudated
        require(LTV > 100, "ZNAK REPO: return is not actual");

        if (UADelta >= currentBorrowAmount) {
            lockedZNAK -= debt.lendAmount;
            UADelta > virtualBalance 
                ? virtualBalance = 0 
                : virtualBalance -= currentBorrowAmount;
            trackingBalance = IERC20(underlyingAsset).balanceOf(address(this));
            debt.isReturned = true;
            _transfer(address(this), debt.borrower, debt.lendAmount);
        } else {
            UADelta > virtualBalance
                ? virtualBalance = 0
                : virtualBalance -= UADelta;
            debt.borrowAmount = currentBorrowAmount;
            debt.borrowBlockNumber = block.number;
            debt.borrowAmount -= UADelta;

            uint persent = utils.PercentYofX(currentBorrowAmount, UADelta);
            uint repayAmount = (debt.lendAmount * persent) / 10**18;

            _transfer(address(this), debt.borrower, repayAmount);
        }
        trackingBalance = IERC20(underlyingAsset).balanceOf(address(this));
    }

    function liquidate(uint _debtId, address _to) public override {
        require(_debtId <= lastDebtId.current(), "ZNAK REPO: invalid params");
        Debt storage debt = debts[_debtId];

        require(!debt.isReturned, "ZNAK REPO: debt is already returned");
        require(!debt.isLiquidated, "ZNAK REPO: debt is already liquidated");

        // calculate Loan to Value
        (, uint LTV) = debtLTV(_debtId);

        // if collaterial ZNAK value minus % from debt, lend can be liqudated
        require(LTV <= 100, "ZNAK REPO: lend is not liqudable");

        debt.isLiquidated = true;
        uint liquidatorFee = debt.lendAmount * penaltyNominator /
            penaltyDenominator;

        _transfer(address(this), _to, liquidatorFee);
        trackingBalance = IERC20(underlyingAsset).balanceOf(address(this));

        lockedZNAK -= debt.lendAmount;
        virtualBalance -= debt.borrowAmount;

        _burn(address(this), debt.lendAmount - liquidatorFee);
    }
}

