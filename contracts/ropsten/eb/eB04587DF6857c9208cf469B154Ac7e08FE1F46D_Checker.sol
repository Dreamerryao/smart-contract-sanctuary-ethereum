pragma solidity ^0.8.0;

import 'hardhat/console.sol';
import "@openzeppelin/contracts/access/Ownable.sol";
import {IWhitelist} from './Store.sol';



contract Checker is Ownable {
    mapping(string => uint) public delays;
    IWhitelist public whitelist;

    constructor(address _whitelist_addr) {
        set_delay('avatar', 7 days);
        set_delay('patron', 24 hours);
        set_delay('vault', 48 hours);
        set_whitelist(_whitelist_addr);
    }

    function get_window(uint _base_timestamp, string memory _kind, bool is_dev) external view returns (uint) {
        uint rn = block.timestamp;
        uint delay = is_dev ? 1 seconds : delays[_kind];
        uint release_time = _base_timestamp + delay;
        return rn >= release_time ? 0 : release_time - rn;
    }

    function set_delay(string memory _kind, uint _delay) public onlyOwner {
        delays[_kind] = _delay;
    }

    function set_whitelist(address _addr) public onlyOwner {
        require(_addr != address(0), 'Cannot add null address');
        whitelist = IWhitelist(_addr);
    }

    function get_delay(string memory _kind) external view returns (uint) {
        return delays[_kind];
    }
}

