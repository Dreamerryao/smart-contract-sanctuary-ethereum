//SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import 'hardhat/console.sol';
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PullPaymentUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
//import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {Status, Item} from './Inventory.sol';


interface IWhitelist {
    function has_access(string memory _name, address _address) external view returns (bool);
    function grant_access(string memory _name, address _address) external;
}

interface IInventory {
    function get_item(uint32 _item_id) external view returns (Item memory);
    function update_custodian(uint32 _item_id, address _custodian) external;
}

interface IChecker {
    function get_window(uint _base_timestamp, string memory _kind, bool is_dev) external view returns (uint);
    function get_delay(string memory _kind) external view returns (uint);
}

// TODO: Resell item: transfer_item()
// TODO: Add another avatar
// TODO: Turn off revert strings

contract Store is Initializable, OwnableUpgradeable, PausableUpgradeable, PullPaymentUpgradeable, ReentrancyGuardUpgradeable {
    uint16 constant BP = 10000;
    uint16 private _ivallotment;
    uint16 private _totalshares;
    uint public locker;

    mapping(address => uint) private _patrons;          // ETH
    mapping(address => uint16) private _avatars;        // Shares
    mapping(address => uint) private _locktimes;
    address[] private _active_avatars;
    address private _release_addr;
    IWhitelist public whitelist;
    IInventory public inventory;
    IChecker public checker;

    event BuyItem(address indexed _address, uint _amount, uint32 _item_id);
    event Receive(address indexed _address, uint _amount);
    event Release(address indexed _address, uint _amount);
    event Avatar(bool indexed _add, address _address, uint16 _shares);

    function init(
        address[] memory _payees, uint16[] memory _shares,
        address _whitelist_addr, address _inventory_addr, address _checker_addr, address _release
    ) external initializer {
        __Ownable_init();
        __Pausable_init();
        __PullPayment_init();
        __ReentrancyGuard_init();

        // Deployed contracts
        set_whitelist(_whitelist_addr);
        set_inventory(_inventory_addr);
        set_checker(_checker_addr);

        _avatars[_payees[0]] = _shares[0];   // P
        _avatars[_payees[1]] = _shares[1];   // M
        _avatars[_payees[2]] = _shares[2];   // J
        _active_avatars = _payees;
        _release_addr = _release;

        _totalshares = _shares[0] + _shares[1] + _shares[2];
        _ivallotment = 3000;
    }

    modifier onlyStaff {
        require(
            whitelist.has_access('STAFF', _msgSender()) ||
            whitelist.has_access('DAYZERO', _msgSender()),
            'Staff: You shall not pass!'
        );
        _;
    }

    modifier onlyAdmin {
        require(whitelist.has_access('DAYZERO', _msgSender()), 'Admin: You shall not pass!');
        _;
    }

    modifier onlyAvatar {
        require(
            whitelist.has_access('AANG', _msgSender()) ||
            whitelist.has_access('DAYZERO', _msgSender()),
            'Avatar: You shall not pass!'
        );
        _;
    }

    modifier notNull(address _addr) {
        require(_addr != address(0), 'Cannot add null address');
        _;
    }



    function buy_item(uint32 _item_id, uint _count) external whenNotPaused payable {
        Item memory itm = inventory.get_item(_item_id);
        uint total;
        uint to_split;
        uint for_patron;
        uint deduct;
        address[] memory addrlist = _active_avatars;
        uint len = addrlist.length;

        if(keccak256(abi.encodePacked(itm.kind)) == keccak256(abi.encodePacked('land'))) {
            require(
                itm.id > 0 && itm.custodian == address(0) &&
                itm.status == Status.ACTIVE && !itm.deleted,
                'Invalid ID'
            );
        }
        else {
            require(itm.id > 0 && itm.status == Status.ACTIVE && !itm.deleted, 'Invalid ID');
        }

        require(_count >= 1, 'Minimum amount needed');
        total = itm.price * _count;
        require(msg.value == total, 'Exact amount only');

        to_split = payment_split(msg.value, _ivallotment);
        for_patron = msg.value - to_split;
        _patrons[_msgSender()] += for_patron;

        // Escrow
        for(uint i; i < len; i++) {
            address addr = addrlist[i];
            uint for_avatar = payment_split(to_split, _avatars[addr]);
            _asyncTransfer(addr, for_avatar);
            deduct += for_avatar;
        }
        to_split -= deduct;
        if(to_split > 0) locker += to_split;

        inventory.update_custodian(_item_id, _msgSender());
        emit BuyItem(_msgSender(), for_patron, _item_id);
    }

    function withdrawPayments(address payable payee) public whenNotPaused onlyAvatar virtual override {
        // TODO: Change is_dev before deployment
        bool is_dev = true;

        require(_avatars[payee] > 0, 'Invalid avatar');
        require(checker.get_window(_locktimes[payee], 'avatar', is_dev) == 0, 'Window is closed');

        _locktimes[payee] = block.timestamp;
        super.withdrawPayments(payee);
    }

    function release_funds(uint _amount) external whenNotPaused onlyAdmin nonReentrant {
        // TODO: Change is_dev before deployment
        bool is_dev = true;
        address payee = address(this);

        require(payee.balance >= _amount, 'Insufficient balance');
        require(checker.get_window(_locktimes[payee], 'vault', is_dev) == 0, 'Window is closed');

        _locktimes[payee] = block.timestamp;
        (bool success, ) = _release_addr.call{value: _amount}("");
        require(success, 'Unable to send value');
        emit Release(_release_addr, _amount);
    }

//    // TEST: For testing
//    function transfer_item(uint32 _item_id, address _recipient, uint _value) external {
//    }

    /**
     * Get the amount based on their percentage.
     *
     * Requirements:
     * @param _amount:   > BP
     * @param _shares:   Percentage * _bp (e.g. 30% is 3000, 12.25% is 1225)
     */
    function payment_split(uint _amount, uint _shares) internal virtual pure returns (uint) {
        require(_amount >= BP, "Too small to compute");
        return _amount * _shares / BP;
    }

    function update_avatar_wallet(address _currentaddr, address _newaddr) external onlyOwner virtual {
        require(_avatars[_currentaddr] > 0, 'Invalid avatar');
        uint16 shares = _avatars[_currentaddr];

        delete _avatars[_currentaddr];
        _remove_from_array(_active_avatars, _currentaddr);

        _avatars[_newaddr] = shares;
        _active_avatars.push(_newaddr);
        emit Avatar(false, _newaddr, shares);
    }

    /*
     * Create or update an avatar. All shares must be <= BP.
     * @param _addr:    Address to add/update.
     * @param _shares:  Setting this to 0 deletes the avatar.
     * @param _add:     [True] adding new avatar. [False] Updating an existing avatar's shares.
     */
    function set_avatar(address _addr, uint16 _shares, bool _add) external onlyOwner virtual {
        if(_shares == 0) {
            _remove_from_array(_active_avatars, _addr);
            delete _avatars[_addr];
            emit Avatar(false, _addr, 0);
        }
        else {
            uint16 addrshares = _avatars[_addr];
            uint16 sharecount = (_add ? _totalshares : _totalshares - addrshares) + _shares;

            require(sharecount <= BP, 'Amount of shares exceeds 100%');

            if(_add) _active_avatars.push(_addr);
            _totalshares = sharecount;
            addrshares = _shares;
            _avatars[_addr] = addrshares;
            emit Avatar(true, _addr, _shares);
        }
    }



    receive() external payable virtual {
        locker += msg.value;
        emit Receive(_msgSender(), msg.value);
    }

    fallback() external virtual {
        require(msg.data.length == 0);
    }

    function pause() external whenNotPaused onlyOwner virtual {
        _pause();
    }

    function unpause() external whenPaused onlyOwner virtual {
        _unpause();
    }



    function get_totalshares() public onlyStaff virtual view returns (uint) {
        return _totalshares;
    }

    function set_totalshares(uint16 _shares) public onlyOwner virtual {
        require(_shares > 0 && _shares <= BP, 'Invalid shares');
        _totalshares = _shares;
    }

    function set_locker(uint _amount) public onlyAdmin virtual {
        require(_amount >= 1 ether && _amount < locker, 'Invalid locker');
        locker = _amount;
    }

    function _remove_from_array(address[] storage _addrs, address to_remove) private {
        address[] memory datalist = _addrs;
        uint len = _addrs.length;
        uint lastIdx = len - 1;

        if(datalist[lastIdx] == to_remove) {
            _addrs.pop();
        }
        else {
            for(uint i; i < len; i++) {
                if(datalist[i] == to_remove) {
                    _addrs[i] = _addrs[lastIdx];
                    _addrs.pop();
                    break;
                }
            }
        }
    }



    function set_whitelist(address _addr) public onlyOwner notNull(_addr) virtual {
        whitelist = IWhitelist(_addr);
    }

    function set_inventory(address _addr) public onlyOwner notNull(_addr) virtual {
        inventory = IInventory(_addr);
    }

    function set_checker(address _addr) public onlyOwner notNull(_addr) virtual {
        checker = IChecker(_addr);
    }

    function set_release(address _addr) public whenNotPaused onlyOwner notNull(_addr) virtual {
        _release_addr = _addr;
    }



    function verify_avatar_shares(address _address) external onlyOwner virtual view returns (uint) {
        return _avatars[_address];
    }

    function verify_contract_balance() external onlyOwner virtual view returns (uint) {
        return address(this).balance;
    }

    function verify_active_avatars() external onlyOwner virtual view returns (address[] memory) {
        return _active_avatars;
    }
}

