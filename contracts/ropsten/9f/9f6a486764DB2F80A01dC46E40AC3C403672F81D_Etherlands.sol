pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/governance/GovernorUpgradeable.sol";

//import "./openzeppelin/OwnableUpgradeable.sol";
//import "./openzeppelin/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";


import "./maticnetwork/NativeMetaTransaction.sol";

import "./IERC20.sol";
import "./IEtherlands.sol";

import "hardhat/console.sol";

contract Etherlands is
    IEtherlands,
    ERC721Upgradeable,
    OwnableUpgradeable,
    NativeMetaTransaction
{
    /*** variables ***/

    uint256[] public plotPrices; // array of prices, in wei, which correspond to the below
    uint32[] public plotPriceDistances; // array of distances [x1,x2..] which correspond to the above

    uint256 public districtPrice; // price to mint a district without any deeds

    uint256 public totalPlots; // total number of minted plots
    uint256 public totalSupply; // total currently minted districts

    bool public claimable; // whether or not it is possible to mint an district or claim deeds
    uint32 public worldSize; // maximum value of any plot coordinate
    address public trustedForwarder; // this should be set to the current contract addr
    //56 bits left in word

    IERC20 public underlyingCurrency; // currency to accept for payment

    /*** mappings ***/
    // holds plot information
    struct plotInfo {
        int32 plot_x;
        int32 plot_z;
        uint128 plot_district;
    }

    mapping(uint256 => plotInfo) public plotInfoOf; // mapping from plotId to plot coordinate;
    mapping(uint64 => uint256) public plotIdOf; // mapping from {x,z} to plotId

    // holds name information
    mapping(bytes24 => uint256) public nameDistrictOf; // mapping from bytes24 to uint256
    mapping(uint256 => bytes24) public districtNameOf; // mapping from uint128 to bytes24
    

    /*** proxy logic ***/
    function initialize(string memory _name, string memory _symbol)
        public
        override
        initializer
    {
        __ERC721_init(_name, _symbol);
        __EIP712Base_init(_name);
        __Ownable_init();

        claimable = false;
        worldSize = 1000_000;
    }

    /*** metadata ***/
    function _baseURI() internal pure override returns (string memory) {
        return "https://etherlands.com/metalink/district/";
    }

    /*** admin functions ***/

    function eminentDomainDistrict(uint256 _districtId)
        external
        override
        onlyOwner
    {
        address from = ERC721Upgradeable.ownerOf(_districtId);
        address to = address(this);
        _approve(address(0), _districtId);
        _transfer(from, to, _districtId);
    }

    /*** admin setters ***/
    function setClaimable(bool _claimable) external override onlyOwner {
        claimable = _claimable;
    }

    function setPlotPrices(uint256[] memory _prices, uint32[] memory _distances)
        external
        override
        onlyOwner
    {
        require(_prices.length == _distances.length, "length doesn't match");
        plotPrices = _prices;
        plotPriceDistances = _distances;
    }

    function setWorldSize(uint32 _worldSize) external override onlyOwner {
        worldSize = _worldSize;
    }

    function setDistrictPrice(uint256 _districtPrice)
        external
        override
        onlyOwner
    {
        districtPrice = _districtPrice;
    }

    function setUnderlyingCurrency(address _erc20Address)
        external
        override
        onlyOwner
    {
        underlyingCurrency = IERC20(_erc20Address);
    }

    function setTrustedForwarder(address _address) external onlyOwner {
        trustedForwarder = _address;
    }

    /*** plot logic ***/
    function transferPlot(
        uint128 _originId,
        uint128 _targetId,
        uint256[] calldata _plotIds
    ) external override {
        require(
            _isApprovedOrOwner(_msgSender(), _originId),
            "transfer caller not owner or approved"
        );
        for (uint256 i = 0; i < _plotIds.length; i++) {
            _transferPlot(_originId, _targetId, _plotIds[i]);
        }
    }

    function _transferPlot(
        uint128 _originId,
        uint128 _targetId,
        uint256 _plotId
    ) internal {
        require(
            plotInfoOf[_plotId].plot_district == _originId,
            "plot not in origin district"
        );
        plotInfoOf[_plotId].plot_district = _targetId;
        emit PlotTransfer(_originId, _targetId, _plotId);
    }

    /*** district logic ***/
    // name and string handling
    // district is the actual ERC721. it is a collection of plotIds
    function claimDistrictLands(
        int32[] calldata _xs,
        int32[] calldata _zs,
        uint256 _districtId,
        string memory _nickname
    ) external override {
        require(claimable, "claiming is currently disabled");

        uint128 id = safe128(_districtId);

        if (_districtId == 0) {
            totalSupply = totalSupply + 1; // id 0 is reserved for the null district
            _safeMint(_msgSender(), totalSupply);
            id = safe128(totalSupply);
            if (bytes(_nickname).length > 0) {
                setDistrictName(id, _nickname); // nickname only matters if districtId == 0 && _nickname != 0
            }
        } else {
            require(
                ERC721Upgradeable.ownerOf(_districtId) != address(0),
                "district does not exist"
            );
        }
        require(_xs.length == _zs.length, "xs and zs length mismatch");
        // calculate total cost
        uint256 total_cost = 0;
        for (uint256 i = 0; i < _xs.length; i++) {
            total_cost = total_cost + _calculateLandCost(_xs[i], _zs[i]);
        }
        if (_xs.length == 0) {
            total_cost = total_cost + districtPrice;
        }
        underlyingCurrency.transferFrom(
            _msgSender(),
            address(this),
            total_cost
        );
        for (uint256 i = 0; i < _xs.length; i++) {
            _claimPlot(id, _xs[i], _zs[i]);
        }
    }

    function _calculateLandCost(int32 _x, int32 _z)
        internal
        view
        returns (uint256)
    {
        uint32 xA = uint32(_x >= 0 ? _x : -_x);
        uint32 zA = uint32(_z >= 0 ? _z : -_z);

        uint32 min = (xA < zA ? xA : zA);

        uint256 price = 0;
        for (uint256 i = 0; i < plotPrices.length; i++) {
            if (min >= plotPriceDistances[i]) {
                price = plotPrices[i];
            } else {
                break;
            }
        }
        return price;
    }

    function setDistrictName(uint256 _districtId, string memory _districtName)
        public
        override
    {
        require(
            _isApprovedOrOwner(_msgSender(), _districtId),
            "set name not approved"
        );
        uint128 districtId128 = safe128(_districtId);
        bytes24 name24 = create24Name(_districtName);
        require(
            nameDistrictOf[name24] == 0 ||
                nameDistrictOf[name24] == districtId128,
            "name taken"
        );

        _setDistrictName(districtId128, name24);
    }

    function _setDistrictName(uint128 _districtId, bytes24 _districtName)
        internal
    {
        nameDistrictOf[
            districtNameOf[_districtId]
        ] = 0x000000000000000000000000000000000000000000000000;
        nameDistrictOf[_districtName] = _districtId;
        districtNameOf[uint256(_districtId)] = _districtName;

        emit DistrictName(_districtId, _districtName);
    }

    /*** claim logic ***/
    // we claim deeds by adding them to an existing estate.
    function _claimPlot(
        uint128 _districtId,
        int32 _x,
        int32 _z
    ) internal returns (uint256 tokenId) {
        uint64 coords_packed = pack_coordinates(_x, _z);
        require(plotIdOf[coords_packed] == 0, "already claimed");
        uint32 xA = uint32(_x >= 0 ? _x : -_x);
        uint32 zA = uint32(_z >= 0 ? _z : -_z);
        require((worldSize > xA) && (worldSize > zA), "claim too far");
        require((xA > 2) && (zA > 2), "too close to axis");
        return _mintPlot(_districtId, _x, _z);
    }

    function _mintPlot(
        uint128 _districtId,
        int32 _x,
        int32 _z
    ) internal returns (uint256 tokenId) {
        uint64 coords_packed = pack_coordinates(_x, _z);
        // the first plot has id = 1;
        totalPlots = totalPlots + 1;
        plotInfoOf[totalPlots] = plotInfo(_x, _z, _districtId);
        plotIdOf[coords_packed] = totalPlots;
        emit PlotCreation(_x, _z, _districtId, totalPlots);
        return totalPlots;
    }

    /*** admin functions ***/
    // the owner of the contract may claim any unclaimed plot and assign it to any district id
    function adminClaim(
        int32[] calldata _xs,
        int32[] calldata _zs,
        uint256 _districtId
    ) external override onlyOwner {
        require(_xs.length == _zs.length, "xs and zs length mismatch");
        require(_districtId != 0, "id 0 is reserved for the null district");
        uint128 districtId128 = safe128(_districtId);
        for (uint256 i = 0; i < _xs.length; i++) {
            _adminClaimPlot(districtId128, _xs[i], _zs[i]);
        }
    }

    function _adminClaimPlot(
        uint128 _districtId,
        int32 _x,
        int32 _z
    ) internal returns (uint256) {
        uint64 coords_packed = pack_coordinates(_x, _z);
        uint256 plotId = plotIdOf[coords_packed];
        if (plotId == 0) {
            return _mintPlot(_districtId, _x, _z);
        } else {
            uint128 targetDistrictID = plotInfoOf[plotId].plot_district;
            _transferPlot(targetDistrictID, _districtId, plotId);
            return plotId;
        }
    }

    /*** opensea integration ***/
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override(ERC721Upgradeable, IERC721Upgradeable)
        returns (bool isOperator)
    {
        // always approve OZ proxy 0x58807baD0B376efc12F5AD86aAc70E78ed67deaE;
        if (_operator == address(0x58807baD0B376efc12F5AD86aAc70E78ed67deaE)) {
            return true;
        }
        return ERC721Upgradeable.isApprovedForAll(_owner, _operator);
    }

    /*** metatransaction logic ***/
    function isTrustedForwarder(address _forwarder)
        public
        view
        override
        returns (bool)
    {
        return _forwarder == trustedForwarder;
    }

    function _msgSender() internal view override returns (address signer) {
        signer = msg.sender;
        if (msg.data.length >= 20 && isTrustedForwarder(signer)) {
            assembly {
                signer := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        }
    }

    /*** pure functions ***/
    function safe128(uint256 n) internal pure returns (uint128) {
        require(n < 2**128, "uint128 overflow");
        return uint128(n);
    }

    function pack_coordinates(int32 _x, int32 _z)
        public
        pure
        returns (uint64 answer)
    {
        bytes memory zz = abi.encodePacked(_x, _z);
        assembly {
            answer := mload(add(zz, 8))
        }
    }

    // this takes up to the first 24 valid characters in a string and returns them as a bytes24
    function create24Name(string memory name)
        public
        pure
        returns (bytes24 result)
    {
        bytes memory namebytes = bytes(name);
        uint256 output;
        assembly {
            output := mload(0x40)
        }
        uint256 j = 0;
        for (uint256 i = 0; i < namebytes.length; i++) {
            if (j == 24) {
                break;
            }
            assembly {
                let yes := 0
                let char := byte(i, mload(add(namebytes, 32)))
                let idx := add(output, j)
                if and(gt(char, 0x40), lt(char, 0x5B)) {
                    //A-Z
                    mstore8(idx, char)
                    yes := 1
                }
                if iszero(yes) {
                    if and(gt(char, 0x60), lt(char, 0x7B)) {
                        // a-z
                        mstore8(idx, char)
                        yes := 1
                    }
                }
                if iszero(yes) {
                    if and(gt(char, 0x2f), lt(char, 0x3a)) {
                        // 0-9
                        mstore8(idx, char)
                        yes := 1
                    }
                }
                if iszero(yes) {
                    yes := 1
                    switch char
                    case 0x21 {
                        //exclaimation
                        mstore8(idx, char)
                    }
                    case 0x2D {
                        //hyphen
                        mstore8(idx, char)
                    }
                    case 0x5F {
                        //underscore
                        mstore8(idx, char)
                    }
                    default {
                        yes := 0
                    }
                }
                j := add(j, yes)
            }
        }
        assembly {
            result := mload(output)
        }
    }
}

