pragma solidity ^0.8.0;

// Cargo Residues Contracts
import "./waste/cargo_residues/CargoResiduesOthersERC20.sol";
import "./waste/cargo_residues/TankWashingIERC20.sol";
import "./waste/cargo_residues/TankWashingIIXERC20.sol";
import "./waste/cargo_residues/TankWashingIIYERC20.sol";
import "./waste/cargo_residues/TankWashingIIZERC20.sol";

// Garbage Contracts
import "./waste/garbage/AnimalCarcassERC20.sol";
import "./waste/garbage/CookingOilERC20.sol";
import "./waste/garbage/DomesticWastesERC20.sol";
import "./waste/garbage/FoodWastesERC20.sol";
import "./waste/garbage/IncineratorAshesERC20.sol";
import "./waste/garbage/OperationalWastesERC20.sol";
import "./waste/garbage/PlasticERC20.sol";

// Related Substances
import "./waste/related_substances/ExhaustGasCleaningResiduesERC20.sol";
import "./waste/related_substances/OzoneDepletingSubstancesERC20.sol";

// Sewage
import "./waste/sewage/SewageERC20.sol";

// Waste Oils
import "./waste/waste_oils/OilyBilgeWaterERC20.sol";
import "./waste/waste_oils/OilyResiduesERC20.sol";
import "./waste/waste_oils/OthersSpecifyERC20.sol";

import "./PortNetNFT.sol";
import "./WastePassport.sol";

contract PortNetDApp {
    // WastePassport
    WastePassport _wastePassport;

    // PortNet NFT
    PortNetNFT _portNetNFT;

    // Cargo Residues Contracts
    CargoResiduesOthersERC20 _cargoResiduesOthersERC20;
    TankWashingIERC20 _tankWashingIERC20;
    TankWashingIIXERC20 _tankWashingIIXERC20;
    TankWashingIIYERC20 _tankWashingIIYERC20;
    TankWashingIIZERC20 _tankWashingIIZERC20;

    // Garbage Contracts
    AnimalCarcassERC20 _animalCarcassERC20;
    CookingOilERC20 _cookingOilERC20;
    DomesticWastesERC20 _domesticWastesERC20;
    FoodWastesERC20 _foodWastesERC20;
    IncineratorAshesERC20 _incineratorAshesERC20;
    OperationalWastesERC20 _operationalWastesERC20;
    PlasticERC20 _plasticERC20;

    // Related Substances
    ExhaustGasCleaningResiduesERC20 _exhaustGasCleaningResiduesERC20;
    OzoneDepletingSubstancesERC20 _ozoneDepletingSubstancesERC20;

    //Sewage
    SewageERC20 _sewageERC20;

    // Waste Oils
    OilyBilgeWaterERC20 _oilyBilgeWaterERC20;
    OilyResiduesERC20 _oilyResiduesERC20;
    OthersSpecifyERC20 _othersSpecifyERC20;

    constructor(address portNetNftAddress, address wastePassportAddress) public {
        _portNetNFT = PortNetNFT(portNetNftAddress);
        _wastePassport = WastePassport(wastePassportAddress);
    }

    function mint(address address998, uint256 tokenId, bytes memory tokenIdBytes, string memory tokenMetadata, address[] memory listWasteAddress, uint256[] memory listWaste) external {
        //Essential!! The params has to come in in order to work correctly
        require(listWaste.length == 18);
        require(listWasteAddress.length == 18);
        address thisContract = address(this);

        //1. Waste Oils
        if (listWaste[0] > 0) {
            _oilyBilgeWaterERC20 = OilyBilgeWaterERC20(listWasteAddress[0]);
            _oilyBilgeWaterERC20.mint(thisContract, listWaste[0]);
            _oilyBilgeWaterERC20.approve(address998, listWaste[0]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[0], listWaste[0]);
        }

        if (listWaste[1] > 0) {
            _oilyResiduesERC20 = OilyResiduesERC20(listWasteAddress[1]);
            _oilyResiduesERC20.mint(thisContract, listWaste[1]);
            _oilyResiduesERC20.approve(address998, listWaste[1]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[1], listWaste[1]);
        }

        if (listWaste[2] > 0) {
            _othersSpecifyERC20 = OthersSpecifyERC20(listWasteAddress[2]);
            _othersSpecifyERC20.mint(thisContract, listWaste[2]);
            _othersSpecifyERC20.approve(address998, listWaste[2]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[2], listWaste[2]);
        }

        //2. Sewage
        if (listWaste[3] > 0) {
             _sewageERC20 = SewageERC20(listWasteAddress[3]);
            _sewageERC20.mint(thisContract, listWaste[3]);
            _sewageERC20.approve(address998, listWaste[3]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[3], listWaste[3]);
        }

        //3. Garbage Contracts
        if (listWaste[4] > 0) {
            _animalCarcassERC20 = AnimalCarcassERC20(listWasteAddress[4]);
            _animalCarcassERC20.mint(thisContract, listWaste[4]);
            _animalCarcassERC20.approve(address998, listWaste[4]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[4], listWaste[4]);
        }

        if (listWaste[5] > 0) {
            _cookingOilERC20 = CookingOilERC20(listWasteAddress[5]);
            _cookingOilERC20.mint(thisContract, listWaste[5]);
            _cookingOilERC20.approve(address998, listWaste[5]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[5], listWaste[5]);
        }

        if (listWaste[6] > 0) {
            _domesticWastesERC20 = DomesticWastesERC20(listWasteAddress[6]);
            _domesticWastesERC20.mint(thisContract, listWaste[6]);
            _domesticWastesERC20.approve(address998, listWaste[6]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[6], listWaste[6]);
        }

        if (listWaste[7] > 0) {
            _foodWastesERC20 = FoodWastesERC20(listWasteAddress[7]);
            _foodWastesERC20.mint(thisContract, listWaste[7]);
            _foodWastesERC20.approve(address998, listWaste[7]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[7], listWaste[7]);
        }

        if (listWaste[8] > 0) {
            _incineratorAshesERC20 = IncineratorAshesERC20(listWasteAddress[8]);
            _incineratorAshesERC20.mint(thisContract, listWaste[8]);
            _incineratorAshesERC20.approve(address998, listWaste[8]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[8], listWaste[8]);
        }

        if (listWaste[9] > 0) {
            _operationalWastesERC20 = OperationalWastesERC20(listWasteAddress[9]);
            _operationalWastesERC20.mint(thisContract, listWaste[9]);
            _operationalWastesERC20.approve(address998, listWaste[9]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[9], listWaste[9]);
        }

        if (listWaste[10] > 0) {
            _plasticERC20 = PlasticERC20(listWasteAddress[10]);
            _plasticERC20.mint(thisContract, listWaste[10]);
            _plasticERC20.approve(address998, listWaste[10]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[10], listWaste[10]);
        }

        //4. Cargo Residues Contracts
        if (listWaste[11] > 0) {
            _cargoResiduesOthersERC20 = CargoResiduesOthersERC20(listWasteAddress[11]);
            _cargoResiduesOthersERC20.mint(thisContract, listWaste[11]);
            _cargoResiduesOthersERC20.approve(address998, listWaste[11]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[11], listWaste[11]);
        }

        if (listWaste[12] > 0) {
            _tankWashingIERC20 = TankWashingIERC20(listWasteAddress[12]);
            _tankWashingIERC20.mint(thisContract, listWaste[12]);
            _tankWashingIERC20.approve(address998, listWaste[12]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[12], listWaste[12]);
        }

        if (listWaste[13] > 0) {
            _tankWashingIIXERC20 = TankWashingIIXERC20(listWasteAddress[13]);
            _tankWashingIIXERC20.mint(thisContract, listWaste[13]);
            _tankWashingIIXERC20.approve(address998, listWaste[13]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[13], listWaste[13]);
        }

        if (listWaste[14] > 0) {
            _tankWashingIIYERC20 = TankWashingIIYERC20(listWasteAddress[14]);
            _tankWashingIIYERC20.mint(thisContract, listWaste[14]);
            _tankWashingIIYERC20.approve(address998, listWaste[14]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[14], listWaste[14]);
        }

        if (listWaste[15] > 0) {
            _tankWashingIIZERC20 = TankWashingIIZERC20(listWasteAddress[15]);
            _tankWashingIIZERC20.mint(thisContract, listWaste[15]);
            _tankWashingIIZERC20.approve(address998, listWaste[15]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[15], listWaste[15]);
        }

        //5. Related Substances
        if (listWaste[16] > 0) {
            _exhaustGasCleaningResiduesERC20 = ExhaustGasCleaningResiduesERC20(listWasteAddress[16]);
            _exhaustGasCleaningResiduesERC20.mint(thisContract, listWaste[16]);
            _exhaustGasCleaningResiduesERC20.approve(address998, listWaste[16]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[16], listWaste[16]);
        }

        if (listWaste[17] > 0) {
            _ozoneDepletingSubstancesERC20 = OzoneDepletingSubstancesERC20(listWasteAddress[17]);
            _ozoneDepletingSubstancesERC20.mint(thisContract, listWaste[17]);
            _ozoneDepletingSubstancesERC20.approve(address998, listWaste[17]);
            _wastePassport.getERC20(thisContract, tokenId, listWasteAddress[17], listWaste[17]);
        }

        // The Step NFT
        uint256 nftId = _portNetNFT.mint(thisContract, tokenMetadata);
        _portNetNFT.safeTransferFrom(thisContract, address998, nftId, tokenIdBytes);
    }

    function createAndTransferNFT(address to, bytes memory tokenId, string memory tokenMetadata) external {
        address from = address(this);
        uint256 nftId = _portNetNFT.mint(from, tokenMetadata);
        _portNetNFT.safeTransferFrom(from, to, nftId, tokenId);
    }
}

