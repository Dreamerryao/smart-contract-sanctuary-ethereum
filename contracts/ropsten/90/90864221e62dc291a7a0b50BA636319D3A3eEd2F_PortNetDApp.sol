pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";



contract PortNetNFT is ERC721, ERC721URIStorage, ERC721Burnable, Ownable {
    event Mint(uint256 indexed _tokenId, address indexed _to);
    event TransferFrom(address indexed _from, address indexed _to, uint256 indexed _tokenId);

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    mapping(uint256 => string[]) _metadata; // mapping where the key is the tokenId and the value is metadataArray

    constructor() ERC721 ("PortNetNFT", "PNT") {}

    function mint(address to, string memory tokenMetadata) public returns (uint256) {
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        _mint(to, newItemId);
        _metadata[newItemId].push(tokenMetadata);
        emit Mint(newItemId, to);
        return newItemId;
    }

    function setTokenMetadata(uint256 tokenId, string memory _tokenMetadata) public {
        require(_exists(tokenId), "ERC721URIStorage: URI set of nonexistent token");
        _metadata[tokenId].push(_tokenMetadata);
    }

    function getMetadata(uint256 tokenId, uint index) public view returns (string memory){
        return _metadata[tokenId][index];
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);

    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory){
        return super.tokenURI(tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory _data) public virtual override {
        super.safeTransferFrom(from, to, tokenId, _data);
        emit TransferFrom(from, to, tokenId);
    }
}

