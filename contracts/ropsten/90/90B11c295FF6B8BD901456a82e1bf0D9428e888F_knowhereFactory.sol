    function InitBaseInfo(InitInfo memory info) external;
}

contract knowhereFactory is IInitData{

    bytes32 public constant INIT_CODE_HASH = keccak256(abi.encodePacked(type(KnowhereTemplate).creationCode));
    //All project contract address
    address[] public collections;
    //Whether the project name is used
    mapping(string => bool)  whether;
    mapping(string => mapping(string => bool)) existent;
    mapping(address => string) public project;

    event Created(address indexed promoter,uint256 indexed createTime,address collection,string name);

    function createCollection(InitInfo memory info) public returns (address collect){
        require(bytes(info.name).length >0 && bytes(info.symbol).length > 0,"ERC721:Project name cannot be empty");
        require(whether[info.name] != true ,"Factory:Duplicate project info data");
        require(info.owner != address(0),"ERC721:No permit zero address");
        bytes memory bytecode = type(KnowhereTemplate).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(info.name,info.symbol));
        collect = deploy(0, salt, bytecode);
        collections.push(collect);
        whether[info.name] = true;
        existent[info.name][info.symbol] = true;
        project[collect] = info.name;      

        IKnowhereTemplate(collect).InitBaseInfo(info);

        emit Created(msg.sender, block.timestamp, collect, info.name);
    }

    function deploy(uint256 amount,bytes32 salt,bytes memory bytecode) internal returns (address) {
        address addr;
        require(address(this).balance >= amount, "Factory: insufficient balance");
        require(bytecode.length != 0, "Factory: bytecode length is zero");
        /// @solidity memory-safe-assembly
        assembly {
            addr := create2(amount, add(bytecode, 0x20), mload(bytecode), salt)
        }
        require(addr != address(0), "Factory: Failed on deploy");
        return addr;
    }

    function getAddress(string memory name,string memory symbol) public view returns(address where){
        if(existent[name][symbol] != false){
            bytes32 salt = keccak256(abi.encodePacked(name,symbol));
            where = queryAddress(salt, INIT_CODE_HASH,address(this));
        }
    }

    /**
