contract KnowhereTemplate is ERC721Royalty{

    uint256 startIndex;
    bool    public isStop;

    constructor(){
        factory = msg.sender;
    }

    modifier stop() {
        require(isStop != true,"ERC721:The launch function has been suspended");
        _;
    }

    function updateStopState(bool _isStop) public onlyOwner{
        isStop = _isStop;
    }

    function InitBaseInfo(InitInfo memory info) external onlyOwner{
        merkleRoot = info.whiteRoot;
        uriRoot = info.hashRoot;
        whetherWhite = info.isWhite;
        manager = info.owner;
        startTime = info.start;
        endTime = info.end;
        totalSupply = info.total;
        singleAddressLimit = info.singleAdd;
        singleTimeLimit = info.singleTim;
        mintFee = info.fee;
        _initBase(info);
        _setDefaultRoyalty(info.receiver, info.royal); 
    }


    function verify(bytes32[] memory proof,address customer) public view returns(bool result){
        bytes32 leaf = keccak256(abi.encodePacked(customer));
        return MerkleProof.verify(proof,merkleRoot,leaf);
    }

    function verifyUri(bytes32[] memory uriProof,bytes32 uri) public view returns(bool result){
        return MerkleProof.verify(uriProof,uriRoot,uri);
    }

    function stringToBytes32(string memory source) public pure returns (bytes32 result) {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            result := mload(add(source, 32))
        }
    }

    function safeMint(bytes32[] memory uriProof,bytes32[] memory proof,string[] memory uri,uint256 amount) public stop{     
        if(whetherWhite == true && verify(proof, msg.sender) != false) revert("ERC721:White list permission is required");
        if(startTime >0 && block.timestamp < startTime) revert("ERC721:The project has not yet started");
        if(endTime > 1 && block.timestamp > endTime) revert("ERC721:The project has ended");
        if(totalSupply < mined) revert("ERC721:Has exceeded the total supply");
        if(totalSupply - mined < amount) revert("ERC721:oo much single mint");

        if(singleAddressLimit > 0 && singleTimeLimit > 0){
            if(amount + userMined[msg.sender] > singleAddressLimit) revert("ERC721:Single mint quantity error");
            if(amount > singleAddressLimit || amount > singleTimeLimit) revert("ERC721:mint quantity error");
        }
        
        for(uint i=0; i<amount; i++){
            bytes32 fill = stringToBytes32(uri[i]);
            require(fill != 0x0 && verifyUri(uriProof,fill) != false,"ERC721:path error");
            _safeMint(msg.sender, startIndex);
            fillPath[startIndex] = uri[i];
            startIndex++;
        }
        userMined[msg.sender] += amount;
        mined += amount;
    }

    function getUserInfo(bytes32[] memory proof,address customer) public view returns(Value memory val){
        bool isW;
        if(whetherWhite != true){
            isW = true;
        }

        if(whetherWhite == true){
            isW = verify(proof, customer);
        }

        if(mined <= totalSupply && userMined[customer] <= singleAddressLimit){
            val = Value(startTime,endTime,totalSupply,totalSupply - mined,singleAddressLimit,singleTimeLimit,singleAddressLimit - userMined[customer],mintFee,whetherWhite,isW);
        }   

    }

}

