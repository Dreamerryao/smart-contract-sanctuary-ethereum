contract Knowhere is ERC721{

    using Strings for uint256;

    constructor()ERC721("",""){
        factory = msg.sender;
    }

    modifier onlyManager() {
        bool isPermit = manager == msg.sender || factory == msg.sender;
        require(isPermit == true,"ERC721: not permit");
        _;
    }

    function updateManager(address manage) public onlyManager{
        manager = manage;
    }

    function updateIndex(uint256 start) public onlyManager{
        require(startIndex == 0,"ERC721:Can not be changed arbitrarily");
        startIndex = start;
    }

    function updateTime(uint256 start,uint256 end) public onlyManager{
        require(end >= start,"ERC721:time format error");
        startTime = start;
        endTime = end;
    }

    function addWhitelist(address[] memory users,bool isW) public onlyManager{
        require(isWhite == true,"ERC721:Project visibility needs to be changed");
        for(uint i=0; i<users.length; i++){
            whitelist[users[i]] = isW;
        }
    }

    function upddateNature(bool white) public onlyManager{
        isWhite = white;
    }

    function updateMintFee(uint256 _fee,address _feeTo) public onlyManager{
        mintFee = _fee;
        feeTo = _feeTo;
    }

    function updateMintLimit(uint256 total,uint256 single,uint256 mint) public onlyManager{
        require(total >= single && single >= mint,"ERC721:Data setting error");
        totalSupply = total;
        singleAddLimit = single;
        singleMintLimit = mint;
    }

    function setBaseURI(string memory uri) public onlyManager{
        baseURL = uri;
    }

    function updateHash(string[] memory hashes) public onlyManager{
        if(hashes.length > 0){
            for(uint i=0; i<hashes.length; i++){
                metadatas.push(hashes[i]);
            }
        }
    }

    function getUserInfo(address user) external view returns(bool _isWhite,uint256 _start,uint256 _end,uint256 _fee,uint256 _total,
        uint256 _totalSur,uint256 _singleLimit,uint256 _singleSur,uint256 _singleMinted,uint256 _singleTime,bool _userOfWhite){
            _isWhite = isWhite;
            _start = startTime;
            _end = endTime;
            _fee = mintFee;
            _total = totalSupply;
            _singleLimit = singleAddLimit;
            _singleSur = singleAddLimit - singleMint[user];
            _singleMinted = singleMint[user];
            _singleTime = singleMintLimit;

            if(isWhite == false){
                _userOfWhite = true;
            }

            if(isWhite == true){
                _userOfWhite = whitelist[user];
            }
            if(totalSupply > 0 && totalSupply >= totalMinted){
                _totalSur = totalSupply - totalMinted;
            }
    }

    function getNftInfo(uint256 tokenId) external view returns(address mint,uint256 royal){
        mint = InitialMint[tokenId];
        royal = royalties[tokenId];
    }

    function getWhitelist(address user) internal view returns(bool isSupport){
        if(isWhite == false){
            isSupport = true;
        }
        if(isWhite == true && whitelist[user] == true){
            isSupport = true;
        }
    }

    function getTimeLimit() internal view returns(bool isSupport){
        if(startTime == 0 && endTime == 0){
            isSupport = true;
        }
        if(startTime > 0 && endTime > startTime){
            if(block.timestamp >= startTime && block.timestamp <= endTime){
                isSupport = true;
            }
        }
    }

    function getTotalMintLimit(uint256 amount) internal view returns(bool isSupport){
        if(totalSupply == 0){
            isSupport = true;
        }
        if(totalSupply > 0){
            if(totalMinted + amount <= totalSupply){
                isSupport = true;
            }
        }
    }

    function getAddressLimit(address user,uint256 amount) internal view returns(bool isSupport){
        if(singleAddLimit == 0){
            isSupport = true;
        }
        if(singleAddLimit > 0 && singleMint[user] + amount <= singleAddLimit){
            isSupport = true;
        }
    }

    function getSingleMintLimit(uint256 amount) internal view returns(bool isSupport){
        if(singleMintLimit == 0){
            isSupport = true;
        }
        if(singleMintLimit > 0 && singleMintLimit >= amount){
            isSupport = true;
        }
    }

    function safeMint(uint256 royal,uint256 amount) external payable{
        require(bytes(baseURL).length > 0,"ERC721:Path cannot be empty");
        require(getWhitelist(msg.sender) == true,"ERC721:Not qualified to participate");
        require(getTimeLimit() == true,"ERC721:Time interval error");
        require(getTotalMintLimit(amount) == true,"ERC721:Over minting is not allowed");
        require(getAddressLimit(msg.sender,amount) == true,"ERC721:Single address over minting is not allowed"); 
        require(getSingleMintLimit(amount) == true,"ERC721:Single time over minting is not allowed"); 
        require(msg.value >= amount * mintFee,"ERC721:Insufficient expenses");
        TransferHelper.safeTransferETH(feeTo, mintFee);
        for(uint i=0; i<amount; i++){     
            InitialMint[startIndex] = msg.sender;
            royalties[startIndex] = royal;
            if(metadatas.length > 0){
                require(dataIndex < metadatas.length,"ERC721:Insufficient metadata");
                metadata[startIndex] = metadatas[dataIndex];
                _safeMint(msg.sender, startIndex);
                dataIndex++;
            }else{
                metadata[startIndex] = startIndex.toString();
                _safeMint(msg.sender, startIndex);
            }   
            startIndex++;
        }
        totalMinted = totalMinted + amount;
        singleMint[msg.sender] = singleMint[msg.sender] + amount; 
    }

}

interface IKnowhere{
    function initialize(string memory name_, string memory symbol_) external;
    function updateManager(address manage) external;
    function updateTime(uint256 start,uint256 end) external;
    function addWhitelist(address[] memory users,bool isWhite) external;
    function upddateNature(bool white) external;
    function updateMintFee(uint256 _fee,address _feeTo) external;
    function updateMintLimit(uint256 total,uint256 single,uint256 amount) external;
    function updateHash(string[] memory hashes) external;
    function setBaseURI(string memory uri) external;
}

contract CollectFactory {
    //init_code_hash Can be used to calculate card addresses
    bytes32 public constant INIT_CODE_HASH = keccak256(abi.encodePacked(type(Knowhere).creationCode));
    //All project contract address
    address[] public collections;
    //Whether the project name is used
    mapping(string => bool)  whether;
    mapping(string => mapping(string => bool)) existent;
    mapping(address => string) public project;
    mapping(address => bool) public orInit;
    //Query project address using creator and name
    //mapping(address => mapping(string => address)) public getCollection;
    event Created(address indexed promoter,uint256 indexed createTime,address collection,string name);
    event Initialize(address indexed collect, bool state);

    function createCollection(string memory name,string memory symbol) public returns (address collect){
        require(bytes(name).length >0 && bytes(symbol).length > 0,"ERC721:Project name cannot be empty");
        require(whether[name] != true ,"Factory:Duplicate project info data");
        bytes memory bytecode = type(Knowhere).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(name,symbol));
        collect = deploy(0, salt, bytecode);
        collections.push(collect);
        whether[name] = true;
        existent[name][symbol] = true;
        project[collect] = name;      
        emit Created(msg.sender, block.timestamp, collect, name);
    }

    function initializeKnowhere(address collect,address manage,uint256 start,uint256 end,bool pub,uint256 fee,uint256 total,uint256 single,uint256 amount,string[] memory hashes,string memory uri) public {
        require(orInit[collect] == false,"ERC721:No permit");
        require(end >= start,"ERC721:time format error");
        require(total >= single && single >= amount,"ERC721:Data setting error");
        require(manage != address(0),"ERC721:No permit zero address");
        IKnowhere(collect).updateMintFee(fee,manage);
        IKnowhere(collect).updateTime(start, end);
        IKnowhere(collect).updateMintLimit(total,single,amount);
        IKnowhere(collect).upddateNature(pub);
        IKnowhere(collect).updateHash(hashes);
        IKnowhere(collect).setBaseURI(uri);
        IKnowhere(collect).updateManager(manage);
        orInit[collect] = true;
        emit Initialize(collect, true);
    }

    function deploy(uint256 amount,bytes32 salt,bytes memory bytecode) internal returns (address) {
        address addr;
        require(address(this).balance >= amount, "Factory: insufficient balance");
        require(bytecode.length != 0, "Factory: bytecode length is zero");
        /// @solidity memory-safe-assembly
        assembly {
            addr := create2(amount, add(bytecode, 0x20), mload(bytecode), salt)
        }
        require(addr != address(0), "Factory: Failed on deploy");

        return addr;
    }

    function getAddress(string memory name,string memory symbol) public view returns(address where){
        if(existent[name][symbol] != false){
            bytes32 salt = keccak256(abi.encodePacked(name,symbol));
            where = queryAddress(salt, INIT_CODE_HASH,address(this));
        }
        //bytes memory bytecode = type(Knowhere).creationCode;   
    }

    /**
     * @dev Returns the address where a contract will be stored if deployed via {deploy} from a contract located at
     * `deployer`. If `deployer` is this contract's address, returns the same value as {computeAddress}.
     */
    function queryAddress(bytes32 salt,bytes32 bytecodeHash,address deployer) internal pure returns (address) {
        bytes32 _data = keccak256(abi.encodePacked(bytes1(0xff), deployer, salt, bytecodeHash));
        return address(uint160(uint256(_data)));
    }

}
