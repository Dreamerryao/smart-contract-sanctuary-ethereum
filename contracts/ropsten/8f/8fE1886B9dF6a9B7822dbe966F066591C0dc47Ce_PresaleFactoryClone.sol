pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

/**
 * @title Elixir Launchpad tokens pre-sale
 * @author Satoshis.games
 * @notice this contract manages the pre-sale of tokens at an Elixir Launchpad IGO event
 * @dev
 */
contract PresaleMaster is
    Initializable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    /// PreSale Identification
    uint256 public presaleID;

    // ADDRESSES
    /// game publisher, collects the BUSD paid for the tokens presale
    address public publisher;
    /// Elixir platform account that collects the presales fees
    address public treasury;
    /// Elixir platform owner
    address public owner;
    /// BUSD token address
    address public addressBUSD;

    // DATES
    /// start presale time
    uint256 public presaleStartTime;
    /// end presale time
    uint256 public presaleEndTime;

    /// Max cap in BUSD for the whole presale
    uint256 public presaleMaxCap;
    /// Max cap in IGO project tokens for the whole presale
    uint256 public presaleSupply;
    /// Amount of token per BUSD
    uint256 public presaleTokensPerBUSD;

    /// total BUSD received in all tiers
    uint256 public totalBUSDReceived;

    // TIERS AND USERS

    /// Tiers definition
    enum Tiers {
        None, /// default index 0, no Tier assigned (Guest)
        Bronze,
        Silver,
        Gold,
        Diamond
    }

    /// Info of each tier
    struct TiersInfo {
        Tiers tier; // Tier type-name
        uint256 maxUsers; // Maximum number of users in the Tier
        uint256 ticketValue; // Value of the ticket to fulfill by users in this Tier
        uint256 fee; // Tier Fee
        uint256 userCount; //number of users in the tier
    }

    /// Tiers array for the existing ranks
    TiersInfo[4] public tiersInfo;

    /// Info of each User in the Whitelist
    struct UserInfo {
        Tiers tier; // Tier to which the user belongs
        uint256 ticketValue; // Amount invested by the user. It must be equal to the value of the Tier Ticket.
        bool ticketPaid; // Indicates that the ticket was fully paid
        uint256 lastDepositTime; // keeps track of deposited time
    }

    // Map of whitelisted users and its data
    mapping(address => UserInfo) private whitelistUserInfo;

    // array of whitelisted users
    address[] private entityList;

    // EVENTS
    event tiersUpdated(address _user);
    event userAddedToWhitelist(address _user, Tiers _tier);
    event userBatchAddedToWhitelist(uint256 _usersAmount);
    event userInvestment(address indexed _to, uint256 _amount);

    // MODIFIERS
    // check that the caller is the owner of the contract
    modifier onlyOwner() {
        require(msg.sender == owner, "Not owner");
        _;
    }
    // check that the caller is the publiser
    modifier onlyPublisher() {
        require(msg.sender == publisher, "Not publisher");
        _;
    }
    // check that the caller is the owner or publiser
    modifier onlyAdmins() {
        require(msg.sender == publisher || msg.sender == owner, "Not an admin");
        _;
    }

    /**
     * @notice IGO sale and tiers initilization
     * @param _owner  Elixir platform owner
     * @param _publisher Game publisher, collects the BUSD paid for the tokens presale
     * @param _treasury Elixir platform account that collects the presales fees
     * @param _presaleID PreSale identification
     * @param _presaleMaxCap Max cap in BUSD for the whole sale
     * @param _presaleMaxCap Max cap in BUSD for the whole sale
     * @param _presaleSupply Max cap in IGO project tokens for the whole sale
     * @param _presaleStartTime Start sale time
     * @param _presaleEndTime End sale time
     * @param _addressBUSD BUSD token address BUSD token address
     */
    function initialize(
        address _owner,
        address _publisher,
        address _treasury,
        uint256 _presaleID,
        uint256 _presaleMaxCap,
        uint256 _presaleSupply,
        uint256 _presaleStartTime,
        uint256 _presaleEndTime,
        address _addressBUSD
    ) public initializer {
        // addresses check
        require(_owner != address(0), "Zero address");
        owner = _owner;
        require(_publisher != address(0), "Zero project owner address");
        publisher = _publisher;
        require(_treasury != address(0), "Zero project owner address");
        treasury = _treasury;
        require(_addressBUSD != address(0), "Zero token address");
        addressBUSD = _addressBUSD;

        presaleID = _presaleID;
        presaleMaxCap = _presaleMaxCap;
        presaleSupply = _presaleSupply;
        presaleTokensPerBUSD = _presaleSupply / _presaleMaxCap;
        presaleStartTime = _presaleStartTime;
        presaleEndTime = _presaleEndTime;
    }

    // Administrative Actions

    /**
     * @notice Creates or updates the tier info scheme
     * @param _tierBronzeInfo Initilization info for tier
     * @param _tierSilverInfo Initilization info for tier
     * @param _tierGoldInfo Initilization info for tier
     * @param _tierDiamondInfo Initilization info for tier
     */
    function setTiersInfo(
        uint256[] calldata _tierBronzeInfo,
        uint256[] calldata _tierSilverInfo,
        uint256[] calldata _tierGoldInfo,
        uint256[] calldata _tierDiamondInfo
    ) external onlyOwner {
        require(
            _tierBronzeInfo.length == _tierSilverInfo.length &&
                _tierSilverInfo.length == _tierGoldInfo.length &&
                _tierGoldInfo.length == _tierDiamondInfo.length,
            "Referral: invalid tier data"
        );

        // reset tiers and recreate
        delete tiersInfo;

        /// Tier Array [maxUsers, ticketValue, fee]
        tiersInfo[0] = TiersInfo({
            tier: Tiers.Bronze,
            maxUsers: _tierBronzeInfo[0],
            ticketValue: _tierBronzeInfo[1],
            fee: _tierBronzeInfo[2],
            userCount: 0
        });
        tiersInfo[1] = TiersInfo({
            tier: Tiers.Silver,
            maxUsers: _tierSilverInfo[0],
            ticketValue: _tierSilverInfo[1],
            fee: _tierSilverInfo[2],
            userCount: 0
        });
        tiersInfo[2] = TiersInfo({
            tier: Tiers.Gold,
            maxUsers: _tierGoldInfo[0],
            ticketValue: _tierGoldInfo[1],
            fee: _tierGoldInfo[2],
            userCount: 0
        });
        tiersInfo[3] = TiersInfo({
            tier: Tiers.Diamond,
            maxUsers: _tierDiamondInfo[0],
            ticketValue: _tierDiamondInfo[1],
            fee: _tierDiamondInfo[2],
            userCount: 0
        });
        emit tiersUpdated(msg.sender);
    }


    /**
     * @notice Return the tiers scheme
     * @return returns four arrays corresponding to the columns of tiers
     * @return each array corresponds to column: tiers, max users, 
     * @return ticket value and fee
     * @dev the original arrays are returned transpose
     */
    function getTiersInfo()
        external
        view 
        onlyAdmins
        returns (
            uint[] memory,
            uint[] memory,
            uint[] memory,
            uint[] memory
        )
    {
        uint[] memory tiers = new uint[](4);
        uint[] memory maxUsers = new uint[](4);
        uint[] memory ticketValue = new uint[](4);
        uint[] memory fee = new uint[](4);

        for (uint i = 0; i < 4; i++) {
            tiers[i] = uint(tiersInfo[i].tier);
            maxUsers[i] = tiersInfo[i].maxUsers;
            ticketValue[i] = tiersInfo[i].ticketValue;
            fee[i] = tiersInfo[i].fee;
        }
        
        return (
            tiers, maxUsers, ticketValue, fee
        );
    }
    

    // dates update functions

    /**
     * @notice Updates sale start date
     * @param _newSaleStart start sale time
     */
    function updateStartTime(uint256 _newSaleStart) external onlyOwner {
        require(presaleStartTime > block.timestamp, "Sale already started");
        require(
            _newSaleStart > block.timestamp,
            "The start date cannot be less than the current date"
        );
        require(
            _newSaleStart < presaleStartTime,
            "The start date cannot be greater than the end date"
        );
        presaleStartTime = _newSaleStart;
    }

    /**
     * @notice Updates sale end date
     * @param _newSaleEnd end sale time
     */
    function updateEndTime(uint256 _newSaleEnd) external onlyOwner {
        require(
            _newSaleEnd > presaleStartTime && _newSaleEnd > block.timestamp,
            "The end date of the sale cannot be less than the start date"
        );
        presaleEndTime = _newSaleEnd;
    }

    // Whitelist functions

    /**
     * @notice Add a user address to whitelist in the specified tier
     * @param _address: User addres
     * @param _tier: Tier enum index
     */
    function addWhitelist(address _address, Tiers _tier) external onlyOwner {
        require(_address != address(0), "Invalid address");
        require(
            whitelistUserInfo[_address].tier == Tiers.None,
            "User already added to Whitelist"
        );
        whitelistUserInfo[_address].tier = _tier;
        // udpdate users count in the informed tier
        tiersInfo[uint(_tier) - 1].userCount += 1;
        // add address to iterable entity
        entityList.push(_address);
        emit userAddedToWhitelist(_address, _tier);
    }

    /**
     * @notice Adds a batch of users to the whitelist at the specified tier
     * @param _addresses: User addresses array
     * @param _tiers: Corresponding tier array
     */
    function addWhitelistBulk(
        address[] memory _addresses,
        uint256[] memory _tiers
    ) external onlyOwner {
        require(_addresses.length == _tiers.length, "Array length mismatch");
        for (uint256 i = 0; i < _addresses.length; i++) {
            require(_tiers[i] > 0 && _tiers[i] <= 4, "Invalid tier");
            whitelistUserInfo[_addresses[i]].tier = Tiers(_tiers[i]);
            // udpdate users count in the informed tier
            tiersInfo[_tiers[i] - 1].userCount += 1;
            // add address to iterable entity
            entityList.push(_addresses[i]);
        }
        emit userBatchAddedToWhitelist(_addresses.length);
    }

    /**
     * @notice Changes or removes user from whitelist.
     * Setting None tier, index 0, equals to remove from whitelist
     * @param _address: User addres
     * @param _newTier: New Tier
     */
    function updateWhitelist(address _address, Tiers _newTier)
        external
        onlyOwner
    {
        require(_address != address(0), "Invalid address");
        require(
            whitelistUserInfo[_address].tier != Tiers.None,
            "User is not whitelisted"
        );
        uint256 currentTier = uint(whitelistUserInfo[_address].tier);
        whitelistUserInfo[_address].tier = _newTier;
        // Update the counter of the tiers, except in the case of
        // Tier.None, which has no counter.
        (_newTier != Tiers.None)
            ? tiersInfo[uint(_newTier) - 1].userCount += 1
            : 0;

        (_newTier != Tiers.None)
            ? tiersInfo[uint(currentTier) - 1].userCount -= 1
            : 0;

        emit userAddedToWhitelist(_address, _newTier);
    }

    /**
     * @notice check if address is whitelisted
     * @param _address: User addres
     * @return True if users is whitelisted and the Tier it belongs to
     */
    function getWhitelist(address _address)
        external
        view
        returns (bool, Tiers)
    {
        require(_address != address(0), "Invalid address");

        return (
            whitelistUserInfo[_address].tier != Tiers.None,
            whitelistUserInfo[_address].tier
        );
    }

    /**
     * @notice check if address is whitelisted and the token amount
     * @param _address: User addres
     * @return True if users is whitelisted and the token amount that correspond to his level
     */
    function getWhitelistToken(address _address)
        external
        view
        returns (bool, uint256)
    {
        require(_address != address(0), "Invalid address");

        if (whitelistUserInfo[_address].tier != Tiers.None) {
            // get the tier ticketvalue from the user
            TiersInfo memory tier = tiersInfo[
                uint256(whitelistUserInfo[_address].tier) - 1
            ];
            // Calculates fee and net amount in BUSD
            uint256 netAmount = tier.ticketValue -
                (
                    (tier.tier == Tiers.Diamond)
                        ? 10**18
                        : (tier.ticketValue * tier.fee) / 10000
                );
            uint256 tokensAvailable = netAmount * presaleTokensPerBUSD;
            return (true, tokensAvailable);
        } else {
            return (false, 0);
        }
    }

    /**
     * @notice returns the total number of whitelisted users
     * @return number of whitelisted users
     */
    function getTotalWhitelistedUsers() external view returns (uint256) {
        return entityList.length;
    }


    /**
     * @notice returns the witelisted user in the Tier
     * @param _tier The tier from which users will be returned
     * @return array of whitelisted users
     */
    function getWhitelistFromTier(Tiers _tier)
        external
        view
        onlyAdmins
        returns (address[] memory)
    {
        uint tierCount = tiersInfo[uint(_tier) - 1].userCount;
        address[] memory tierWhitelist = new address[](tierCount);
        uint k = 0;
        for (uint i = 0; i < entityList.length; i++) {
            if (whitelistUserInfo[entityList[i]].tier == _tier) {
                tierWhitelist[k] = entityList[i];
                k++;
            }
        }
        return tierWhitelist;
    }


    // IGO Tokens presale


    /**
     * @notice Make sure buyer has provided the right allowance
     * @param _allower: sender address
     * @param _amount: amount to transfer
     */
    modifier _hasAllowance(address _allower, uint256 _amount) {
        // uint256 ourAllowance = BUSD.allowance(_allower, address(this));
        uint256 ourAllowance = IERC20Upgradeable(addressBUSD).allowance(
            _allower,
            address(this)
        );
        require(_amount <= ourAllowance, "Allowance is too low");
        _;
    }

    /**
     * @notice This function is executed when the user actually makes
     * the purchase of tokens during the time set for the IGO.
     * It sends BUSD to contract address and fees to treasury address
     * @notice This function makes an external call to the BUSD
     * declared above to check if the allowance is enough
     * @param _amount: amount to transfer
     */
    function buyTokens(uint256 _amount)
        external
        nonReentrant
        whenNotPaused
        _hasAllowance(msg.sender, _amount)
        returns (bool)
    {
        // check dates
        require(
            block.timestamp >= presaleStartTime,
            "The sale is not started yet "
        );
        require(block.timestamp <= presaleEndTime, "The sale is closed");

        // check if the deposit exceeds the sale max cap limit
        require(
            totalBUSDReceived + _amount <= presaleMaxCap,
            "buyTokens: purchase would exceed sale max cap"
        );

        // check user whitelisted
        require(
            whitelistUserInfo[msg.sender].tier != Tiers.None,
            "User is not whitelisted"
        );

        // get the tier from the paying user
        TiersInfo storage tier = tiersInfo[
            uint256(whitelistUserInfo[msg.sender].tier) - 1
        ];

        // check if the deposit corresponds to the amount of the tier ticket
        require(
            _amount == tier.ticketValue,
            "buyTokens: your investment does not correspond to the amount of the tier ticket"
        );

        // Calculates fee and net amount
        uint256 feeAmount = (tier.tier == Tiers.Diamond)
            ? 10**18
            : (_amount * tier.fee) / 10000;
        uint256 netAmount = _amount - feeAmount;

        totalBUSDReceived += netAmount;
        whitelistUserInfo[msg.sender].ticketValue = _amount; // the ticket paid is the full amount
        whitelistUserInfo[msg.sender].ticketPaid = true;
        whitelistUserInfo[msg.sender].lastDepositTime = block.timestamp;

        // ********** avoid this first transaction! **********
        // ********** Transfer tokens from the user to this contract
        emit userInvestment(msg.sender, _amount);
        IERC20Upgradeable(addressBUSD).safeTransferFrom(
            msg.sender,
            address(this),
            _amount
        );

        // send fee to treasury
        if (
            IERC20Upgradeable(addressBUSD).allowance(address(this), treasury) <
            feeAmount
        ) {
            IERC20Upgradeable(addressBUSD).safeIncreaseAllowance(
                treasury,
                feeAmount
            );
        }
        IERC20Upgradeable(addressBUSD).safeTransfer(treasury, feeAmount);

        // send value minus fee to projectOwner
        if (
            IERC20Upgradeable(addressBUSD).allowance(address(this), publisher) <
            netAmount
        ) {
            IERC20Upgradeable(addressBUSD).safeIncreaseAllowance(
                publisher,
                netAmount
            );
        }
        IERC20Upgradeable(addressBUSD).safeTransfer(publisher, netAmount);

        return true;
    }

    /**
     * @dev Triggers stopped state.
     * Requirements:
     * - The contract must not be paused.
     */
    function pause() external onlyOwner {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     * Requirements:
     * - The contract must be paused.
     */
    function unpause() external onlyOwner {
        _unpause();
    }
}

