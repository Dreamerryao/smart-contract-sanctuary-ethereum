pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./FlashToken.sol";

contract BorrowFlashToken is FlashToken {
    bytes32 private constant _RETURN_VALUE =
        keccak256("ERC3156FlashBorrower.onFlashLoan");

    FlashToken public contractAddress;

    struct FlashLoanData {
        address initiator;
        address token;
        uint256 amount;
        uint256 fee;
        bytes data;
    }

    function setFlashToken(FlashToken _address) public returns (bool) {
        contractAddress = _address;
        return true;
    }

    mapping(address => FlashLoanData) public myFlashData;

    function onFlashLoan(
        address initiator,
        address token,
        uint256 amount,
        uint256 fee,
        bytes calldata data
    ) external returns (bytes32) {
        myFlashData[initiator] = FlashLoanData({
            initiator: initiator,
            token: token,
            amount: amount,
            fee: fee,
            data: data
        });
        return _RETURN_VALUE;
    }

    function getFlashData(address initiator)
        public
        view
        returns (FlashLoanData memory)
    {
        return myFlashData[initiator];
    }

    function borrow(bytes calldata _data) public returns (bool) {
        bool result = flashLoan(
            IERC3156FlashBorrower(address(this)),
            address(contractAddress),
            100 * 10**18,
            _data
        );
        require(result == true, "FlashLoan: Request failed.");

        return true;
    }
}

