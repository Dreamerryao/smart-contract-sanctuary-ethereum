        console.log("d.mode", d.mode);
        console.log("d.borrower", d.borrower);
        console.log("d.repay", d.repay);
        console.log("d.seizeCToken", d.seizeCToken);

        // Perform the liquidation
        if (d.mode == 3) {
            IWETH(WETH).withdraw(amount0 + amount1); // equivalent to max(amount0, amount1) since one is 0
            CEther(CETH).liquidateBorrow{value: d.repay}(
                d.borrower,
                d.seizeCToken
            );
        } else {
            approve(token0, d.repayCToken, d.repay);
            uint256 _resLiquidation = CERC20(d.repayCToken).liquidateBorrow(
                d.borrower,
                d.repay,
                d.seizeCToken
            );

            console.log("_resLiquidation", _resLiquidation);
            require(_resLiquidation == 0, "Liquidation failed.");
        }

        console.log(
            "------------------after liquidation------------------------"
        );
        console.log("# gasleft(): ", gasleft());
        console.log("# address(this)", address(this));
        console.log("# address(this).balance: ", address(this).balance);
        console.log("# msg.sender", msg.sender);
        console.log("# msg.sender.balance: ", msg.sender.balance);
        console.log("# tx.gasprice: ", tx.gasprice);
        console.log(
            "# 0xeF1B524543E72aD5dcc8100b5fC0079E9FdDfFFD.balance: ",
            0xeF1B524543E72aD5dcc8100b5fC0079E9FdDfFFD.balance
        );
        console.log(
            "# 0x1e1162ba651F3CC585A58C666E0f892a8Cb3381C.balance: ",
            0x1e1162ba651F3CC585A58C666E0f892a8Cb3381C.balance
        );

        // Redeem cTokens for underlying ERC20 or ETH
        uint256 seized = CERC20(d.seizeCToken).balanceOfUnderlying(
            address(this)
        );
        console.log(
            "seized: ERC20(d.seizeCToken).balanceOfUnderlying(address(this)",
            seized
        );

        CERC20(d.seizeCToken).redeemUnderlying(seized);

        if (d.mode % 2 != 0) {
            // modes 1 and 3
            IERC20(token0).transfer(msg.sender, seized);
            return;
        }

        // Compute debt
        (uint256 reserveOut, uint256 reserveIn) = UniswapV2Library.getReserves(
            FACTORY,
            token0,
            WETH
        );

        uint256 debt = UniswapV2Library.getAmountIn(
            d.repay,
            reserveIn,
            reserveOut
        );

        console.log("d.repay", d.repay);
        console.log("reserveIn", reserveIn);
        console.log("reserveOut", reserveOut);
        console.log("debt, by getAmountIn", debt);
        console.log(
            "IWETH(WETH).balanceOf(address(this)) - before: ",
            IWETH(WETH).balanceOf(address(this))
        );
        console.log("## seized - debt: ", seized - debt);

        // Pay back pair
        if (d.mode == 0) {
            tradeForWETH(
                CERC20Storage(d.seizeCToken).underlying(),
                seized,
                debt
            );
            console.log(
                "IWETH(WETH).balanceOf(address(this)): ",
                IWETH(WETH).balanceOf(address(this))
            );
            IERC20(WETH).transfer(msg.sender, debt);
        } else {
            // mode 2
            IWETH(WETH).deposit{value: debt}();
            console.log(
                "IWETH(WETH).balanceOf(address(this)): ",
                IWETH(WETH).balanceOf(address(this))
            );
            IERC20(WETH).transfer(msg.sender, debt);

            // IERC20(WETH).transfer(
            //     0xeF1B524543E72aD5dcc8100b5fC0079E9FdDfFFD,
            //     seized - debt
            // );

            console.log(
                "IWETH(WETH).balanceOf(address(this)) - after sending: ",
                IWETH(WETH).balanceOf(address(this))
            );
        }

        console.log("--------------------after payback----------------------");
        console.log("# gasleft(): ", gasleft());
        console.log("# address(this)", address(this));
        console.log("# address(this).balance: ", address(this).balance);
        console.log("# msg.sender", msg.sender);
        console.log("# msg.sender.balance: ", msg.sender.balance);
        console.log("## seized - debt: ", seized - debt);
        console.log(
            "# 0xeF1B524543E72aD5dcc8100b5fC0079E9FdDfFFD.balance: ",
            0xeF1B524543E72aD5dcc8100b5fC0079E9FdDfFFD.balance
        );
        console.log(
            "# 0x1e1162ba651F3CC585A58C666E0f892a8Cb3381C.balance: ",
            0x1e1162ba651F3CC585A58C666E0f892a8Cb3381C.balance
        );
    }

    struct FlashSwapData {
        uint256 mode;
        address borrower;
        address repayCToken;
        address seizeCToken;
        uint256 repay;
    }

    function tradeForWETH(
        address _offered,
        uint256 _exactSent,
        uint256 _minReceived
    ) private {
        approve(_offered, ROUTER, _exactSent);

        address[] memory path = new address[](2);
        path[0] = _offered;
        path[1] = WETH;
        IUniswapV2Router02(ROUTER)
            .swapExactTokensForTokensSupportingFeeOnTransferTokens(
                _exactSent,
                _minReceived,
                path,
                address(this),
                block.timestamp
            );
    }
}

