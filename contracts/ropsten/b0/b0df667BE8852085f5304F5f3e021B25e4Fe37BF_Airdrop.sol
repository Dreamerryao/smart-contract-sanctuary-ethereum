interface IMiningTreasury {
    function airdropClaim(address _user, uint256 _amount) external;
}

interface IMigratorChef {
    // Take the current LP token address and return the new LP token address.
    // Migrator should have full access to the caller's LP token.
    function migrate(IERC20 token) external returns (IERC20);
}

/// @notice The (older) MasterChef contract gives out a constant number of NR tokens per block.
/// It is the only address with minting rights for NR.
/// The idea for this MasterChef V2 (MCV2) contract is therefore to be the owner of a dummy token
/// that is deposited into the MasterChef V1 (MCV1) contract.
/// The allocation point for this pool on MCV1 is the total allocation point for all pools that receive double incentives.
contract Airdrop {
    using BoringMath for uint256;
    using BoringMath128 for uint128;
    using BoringERC20 for IERC20;
    using SignedSafeMath for int256;

    /// @notice Info of each MCV2 pool.
    /// `allocPoint` The amount of allocation points assigned to the pool.
    /// Also known as the amount of NR to distribute per block.
    struct PoolInfo {
        IERC20 lpToken;
        uint256 lastRewardBlock;
        bool check;
        uint256 accPlxPerShare;
        bool setCheck;
        // PLEXUS 0 startBlock 1 endBlock 2 totalAmount 3 totalReciveAmount 4 tokenPerBlock
        uint256[] plexus;
        AirdropToken airdrop;
    }

    struct AirdropToken {
        address[] token;
        uint256[] perShare;
        uint256[] startBlock;
        uint256[] endBlock;
        uint256[] totalAmount;
        uint256[] totalReciveAmount;
        uint256[] perBlock;
        bool[] setCheck;
    }
    address public dev;
    address public factory;
    /// @notice Address of MCV1 contract.
    address public miningTreasury;
    /// @notice Address of NR contract.
    IERC20 public Plexus;
    bool public airdropFee;
    uint256 public airdropFeePercent;
    /// @notice Info of each MCV2 pool.
    PoolInfo[] public poolInfo;
    /// @notice Address of the LP token for each MCV2 pool.
    IERC20[] public lpToken;

    /// @notice Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => uint256)) public userLastIndex;
    mapping(uint256 => mapping(address => bool)) public userSetCheck;

    // pid => airdropToken => userAddress => airdropTokenId => index
    mapping(uint256 => mapping(uint256 => mapping(address => uint256))) public userAirdropLastIndex;
    mapping(uint256 => mapping(uint256 => mapping(address => bool))) public userAirdropSetCheck;

    // //airdropTokenStartBlock[pid][aidropTokenAddress] = startBlock;
    // mapping (uint256 => mapping ( address => uint256 )) public airdropTokenStartBlock;
    mapping(address => uint256) public lpTokenId;

    uint256 private constant ACC_NR_PRECISION = 1e18;

    event Harvest(address indexed user, uint256 indexed pid, uint256 amount);
    event LogPoolAddition(uint256 indexed pid, uint256 allocPoint, IERC20 indexed lpToken);
    event LogSetPool(uint256 indexed pid, uint256 allocPoint, bool overwrite);
    event LogUpdatePool(uint256 indexed pid, uint256 lastRewardBlock, uint256 lpSupply, uint256 accNRPerShare);
    event LogInit();

    constructor(
        address _miningTreasury,
        IERC20 _plexus,
        address _factory
    ) public {
        miningTreasury = _miningTreasury;
        Plexus = _plexus;
        factory = _factory;
        dev = msg.sender;
    }

    /// @notice Returns the number of MCV2 pools.
    function poolLength() public view returns (uint256 pools) {
        pools = poolInfo.length;
    }

    function poolAddress(uint256 pid) public view returns (address pool) {
        pool = address(poolInfo[pid].lpToken);
    }

    /// @notice Add a new LP to the pool. Can only be called by the owner.
    /// DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    /// @param _lpToken Address of the LP ERC-20 token.
    function add(
        IERC20 _lpToken,
        address[] memory _token,
        uint256[] memory _endBlock,
        uint256[] memory _totalAmount
    ) public {
        require(msg.sender == IPlexusFactory(factory).getPairOwner(address(_lpToken)) || msg.sender == dev);
        require(_token.length == _endBlock.length && _endBlock.length == _totalAmount.length);
        require(!poolCheck(address(_lpToken)));
        lpToken.push(_lpToken);
        lpTokenId[address(_lpToken)] = lpToken.length - 1;
        // PLEXUS 0 startBlock 1 endBlock 2 totalAmount 3 totalReciveAmount 4 tokenPerBlock
        uint256[] memory plexusInfo = new uint256[](5);
        uint256[] memory zero = new uint256[](_token.length);
        uint256[] memory start = new uint256[](_token.length);
        uint256[] memory per = new uint256[](_token.length);
        bool[] memory ck = new bool[](_token.length);
        for (uint256 i; i < _token.length; i++) {
            // airdropToken[i] = _token[i];
            zero[i] = 0;
            start[i] = block.number;
            per[i] = _totalAmount[i] / _endBlock[i];
            _endBlock[i] = _endBlock[i].add(block.number);
            ck[i] = true;
            TransferHelper.safeTransferFrom(_token[i], msg.sender, address(this), _totalAmount[i]);
            if (airdropFee) {
                uint256 airdropTokenFeeAmount = _totalAmount[i].mul(airdropFeePercent) / 1000;
                TransferHelper.safeTransferFrom(_token[i], msg.sender, dev, airdropTokenFeeAmount);
            }
        }
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                lastRewardBlock: block.number,
                check: true,
                accPlxPerShare: 0,
                setCheck: true,
                plexus: plexusInfo,
                airdrop: AirdropToken(_token, zero, start, _endBlock, _totalAmount, zero, per, ck)
            })
        );
    }

    function addOwner(IERC20 _lpToken, uint256[] memory _plexusInfo) public {
        require(msg.sender == dev);
        require(!poolCheck(address(_lpToken)));
        uint256 lastRewardBlock = block.number;
        lpToken.push(_lpToken);
        lpTokenId[address(_lpToken)] = lpToken.length - 1;
        uint256[] memory plexusInfo = new uint256[](5);
        // plexusInfo = _plexusInfo;
        for (uint256 i; i < 4; i++) {
            plexusInfo[i] = _plexusInfo[i];
        }

        plexusInfo[0] = block.number;
        plexusInfo[1] = plexusInfo[1].add(block.number);
        plexusInfo[4] = _plexusInfo[2] / _plexusInfo[1];
        address[] memory airdropToken = new address[](5);

        AirdropToken memory airdropTokenInfo;
        poolInfo.push(PoolInfo(_lpToken, lastRewardBlock, false, 0, true, plexusInfo, airdropTokenInfo));
    }

    function lpTokenPlxAirdropSet(
        address lpToken,
        uint256 _endBlock,
        uint256 _plxTotalAmount
    ) public {
        require(msg.sender == dev);
        uint256 pid = lpTokenId[lpToken];
        PoolInfo memory pool = updatePool(pid);
        if (pool.setCheck == true) {
            pool.setCheck = false;
        } else pool.setCheck = true;
        // PLEXUS 0 startBlock 1 endBlock 2 totalAmount 3 totalReciveAmount 4 tokenPerBlock
        pool.plexus[0] = block.number;
        pool.plexus[1] = _endBlock.add(block.number);
        pool.plexus[2] = _plxTotalAmount;
        pool.plexus[3] = 0;
        pool.plexus[4] = _plxTotalAmount / _endBlock;
        pool.accPlxPerShare = 0;
        poolInfo[pid] = pool;
    }

    function airdropTokenAdd(
        address lpToken,
        address _airdropToken,
        uint256 _airdropTokenEndBlock,
        uint256 _airdropTokenTotalAmount
    ) public {
        require(msg.sender == IPlexusFactory(factory).getPairOwner(address(lpToken)) || msg.sender == dev);
        uint256 pid = lpTokenId[lpToken];
        require(poolInfo[pid].airdrop.token.length <= 5);
        updatePool(pid);
        poolInfo[pid].airdrop.token.push(_airdropToken);
        poolInfo[pid].airdrop.perShare.push(0);
        poolInfo[pid].airdrop.startBlock.push(block.number);
        poolInfo[pid].airdrop.endBlock.push(_airdropTokenEndBlock.add(block.number));
        poolInfo[pid].airdrop.totalAmount.push(_airdropTokenTotalAmount);
        poolInfo[pid].airdrop.totalReciveAmount.push(0);
        poolInfo[pid].airdrop.perBlock.push(_airdropTokenTotalAmount / _airdropTokenEndBlock);
        poolInfo[pid].airdrop.setCheck.push(true);
        poolInfo[pid].check = true;

        if (airdropFee) {
            uint256 airdropTokenFeeAmount = _airdropTokenTotalAmount.mul(airdropFeePercent) / 1000;
            // IERC20(_airdropToken[i]).transferFrom(msg.sender, dev, airdropTokenFeeAmount);
            TransferHelper.safeTransferFrom(_airdropToken, msg.sender, dev, airdropTokenFeeAmount);
        }
        TransferHelper.safeTransferFrom(_airdropToken, msg.sender, address(this), _airdropTokenTotalAmount);

        // IERC20(_airdropToken[i]).transferFrom(msg.sender, address(this), _airdropTokenTotalAmount[i]);
    }

    function setAirdropToken(
        address lpToken,
        uint256 airdropTokenId,
        address _airdropToken,
        uint256 _airdropTokenTotalAmount,
        uint256 _airdropTokenEndBlock
    ) public {
        require(msg.sender == IPlexusFactory(factory).getPairOwner(address(lpToken)) || msg.sender == dev);
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
        poolInfo[pid].airdrop.token[airdropTokenId] = _airdropToken;
        poolInfo[pid].airdrop.perShare[airdropTokenId] = 0;
        poolInfo[pid].airdrop.startBlock[airdropTokenId] = block.number;
        poolInfo[pid].airdrop.endBlock[airdropTokenId] = _airdropTokenEndBlock.add(block.number);
        poolInfo[pid].airdrop.totalAmount[airdropTokenId] = _airdropTokenTotalAmount;
        poolInfo[pid].airdrop.totalReciveAmount[airdropTokenId] = 0;
        poolInfo[pid].airdrop.perBlock[airdropTokenId] = _airdropTokenTotalAmount / _airdropTokenEndBlock;

        if (poolInfo[pid].airdrop.setCheck[airdropTokenId] == true) {
            poolInfo[pid].airdrop.setCheck[airdropTokenId] = false;
        } else poolInfo[pid].airdrop.setCheck[airdropTokenId] = true;

        if (airdropFee) {
            uint256 airdropTokenFeeAmount = _airdropTokenTotalAmount.mul(airdropFeePercent) / 1000;
            // IERC20(_airdropToken).transferFrom(msg.sender, dev, airdropTokenFeeAmount);
            TransferHelper.safeTransferFrom(_airdropToken, msg.sender, dev, airdropTokenFeeAmount);
        }
        TransferHelper.safeTransferFrom(_airdropToken, msg.sender, address(this), _airdropTokenTotalAmount);
        // IERC20(_airdropToken).transferFrom(msg.sender, address(this), _airdropTokenTotalAmount);
    }

    function getLpTokenAirdropTokenInfo(address lpToken, uint256 _tokenId)
        public
        view
        returns (
            address airdropToken,
            uint256 tokenTotalAmount,
            uint256 totalReciveAmount,
            uint256 startBlock,
            uint256 endBlock,
            uint256 perBlock
        )
    {
        uint256 pid = lpTokenId[lpToken];
        airdropToken = poolInfo[pid].airdrop.token[_tokenId];
        tokenTotalAmount = poolInfo[pid].airdrop.totalAmount[_tokenId];
        totalReciveAmount = poolInfo[pid].airdrop.totalReciveAmount[_tokenId];
        startBlock = poolInfo[pid].airdrop.startBlock[_tokenId];
        endBlock = poolInfo[pid].airdrop.endBlock[_tokenId];
        perBlock = poolInfo[pid].airdrop.perBlock[_tokenId];
    }

    // 각 풀에 대한 airdrop Token 전체 정보 뷰함수

    function pendingPlx(address _lpToken, address _user) external view returns (uint256 pending) {
        uint256 pid = lpTokenId[_lpToken];
        PoolInfo memory pool = poolInfo[pid];
        uint256 userAmount = lpToken[pid].balanceOf(_user);

        uint256 lastIndex = userLastIndex[pid][_user];
        if (pool.setCheck != userSetCheck[pid][_user]) {
            lastIndex = 0;
        }
        uint256 lpSupply = lpToken[pid].totalSupply();
        // PLEXUS 0 startBlock 1 endBlock 2 totalAmount 3 totalReciveAmount 4 tokenPerBlock
        uint256 plxPerBlock = pool.plexus[4];
        if (
            block.number > pool.lastRewardBlock && lpSupply != 0 && pool.plexus[0] != 0 && block.number > pool.plexus[0]
        ) {
            uint256 blocks = block.number.sub(pool.lastRewardBlock);
            if (block.number > pool.plexus[1]) {
                if (pool.lastRewardBlock < pool.plexus[1]) {
                    blocks = pool.plexus[1].sub(pool.lastRewardBlock);
                } else blocks = 0;
            }

            pool.accPlxPerShare = pool.accPlxPerShare.add(blocks.mul(plxPerBlock).mul(ACC_NR_PRECISION) / lpSupply);
            pending = userAmount.mul(pool.accPlxPerShare.sub(lastIndex)) / ACC_NR_PRECISION;
        }
    }

    function pendingToken(address _lpToken, address _user) external view returns (uint256[] memory) {
        uint256 pid = lpTokenId[_lpToken];
        PoolInfo memory pool = poolInfo[pid];
        uint256 userAmount = lpToken[pid].balanceOf(_user);
        // uint256[] memory accTokenPerShare = pool.airdropTokenPerShare[i];
        uint256 lpSupply = lpToken[pid].totalSupply();
        uint256[] memory pending = new uint256[](5);
        for (uint256 i; i < pool.airdrop.token.length; i++) {
            uint256 lastairdropIndex = userAirdropLastIndex[pid][i][_user];
            if (pool.airdrop.setCheck[i] != userAirdropSetCheck[pid][i][_user]) {
                lastairdropIndex = 0;
            }
            if (
                block.number > pool.lastRewardBlock &&
                lpSupply != 0 &&
                pool.check &&
                // block.number >= pool.airdropTokenStartBlock
                block.number >= pool.airdrop.startBlock[i]
            ) {
                /* block.number 가 airdropTokenEndBlock 이 지났을경우에 lastRewardBlock이랑 비교하여 0으로 만들어줘야함 */

                uint256 blocks = block.number.sub(pool.lastRewardBlock);
                if (block.number > pool.airdrop.endBlock[i]) {
                    blocks = pool.airdrop.endBlock[i].sub(pool.lastRewardBlock);
                } else blocks = 0;

                pool.airdrop.perShare[i] = pool.airdrop.perShare[i].add(
                    blocks.mul(pool.airdrop.perBlock[i]).mul(ACC_NR_PRECISION) / lpSupply
                );
            }
            pending[i] = (userAmount.mul(pool.airdrop.perShare[i].sub(lastairdropIndex)) / ACC_NR_PRECISION);
        }

        return pending;
    }

    /// @notice Update reward variables for all pools. Be careful of gas spending!
    /// @param pids Pool IDs of all to be updated. Make sure to update all active pools.
    function massUpdatePools(uint256[] calldata pids) external {
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
    }

    //
    function lpTokenUpdatePool(address _lpToken) public {
        uint256 pid = lpTokenId[_lpToken];
        if (poolCheck(_lpToken)) {
            updatePool(pid);
        }
    }

    /// @notice Update reward variables of the given pool.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @return pool Returns the pool that was updated.
    function updatePool(uint256 pid) public returns (PoolInfo memory pool) {
        pool = poolInfo[pid];
        if (block.number > pool.lastRewardBlock) {
            uint256 lpSupply = lpToken[pid].totalSupply();
            if (lpSupply > 0) {
                if (pool.plexus[0] != 0) {
                    uint256 plxBlock = 0;

                    if (block.number >= pool.plexus[1] && pool.lastRewardBlock < pool.plexus[1]) {
                        plxBlock = pool.plexus[1].sub(pool.lastRewardBlock);
                    } else if (block.number > pool.plexus[0]) {
                        plxBlock = block.number.sub(pool.lastRewardBlock);
                    }
                    pool.accPlxPerShare = pool.accPlxPerShare.add(
                        plxBlock.mul(pool.plexus[4]).mul(ACC_NR_PRECISION) / lpSupply
                    );
                }

                if (pool.check) {
                    for (uint256 i; i < pool.airdrop.token.length; i++) {
                        uint256 tokenBlock = 0;
                        if (
                            block.number >= pool.airdrop.endBlock[i] && pool.lastRewardBlock < pool.airdrop.endBlock[i]
                        ) {
                            tokenBlock = pool.airdrop.endBlock[i].sub(pool.lastRewardBlock);
                        } else if (block.number > pool.airdrop.startBlock[i]) {
                            tokenBlock = block.number.sub(pool.lastRewardBlock);
                        }

                        pool.airdrop.perShare[i] = pool.airdrop.perShare[i].add(
                            tokenBlock.mul(pool.airdrop.perBlock[i]).mul(ACC_NR_PRECISION) / lpSupply
                        );
                    }
                }
                pool.lastRewardBlock = block.number;
                poolInfo[pid] = pool;
                // emit LogUpdatePool(pid, pool.lastRewardBlock, lpSupply, pool.accPlxPerShare);
            }
        }
    }

    /// @notice Harvest proceeds for transaction sender to `to`.
    /// @param _lpToken The index of the pool. See `poolInfo`.
    /// @param to Receiver of NR rewards.
    function harvest(address _lpToken, address to) public {
        uint256 pid = lpTokenId[_lpToken];
        if (poolCheck(_lpToken)) {
            // console.log("harvest");
            uint256 userAmount = lpToken[pid].balanceOf(to);
            PoolInfo memory pool = updatePool(pid);

            uint256 _pendingNR;
            // uint256 accumulatedNR = mul(pool.accNRPerShare) / ACC_NR_PRECISION;
            if (userSetCheck[pid][to] != pool.setCheck) {
                if (userSetCheck[pid][to] == true) {
                    userSetCheck[pid][to] = false;
                    userLastIndex[pid][to] = 0;
                } else {
                    userSetCheck[pid][to] = true;
                    userLastIndex[pid][to] = 0;
                }
            }

            uint256 lastIndex = userLastIndex[pid][to];

            if (pool.accPlxPerShare > lastIndex) {
                userLastIndex[pid][to] = pool.accPlxPerShare;
                if (userAmount != 0) {
                    _pendingNR = userAmount.mul(pool.accPlxPerShare.sub(lastIndex)) / ACC_NR_PRECISION;
                    if (_pendingNR.add(pool.plexus[3]) >= pool.plexus[2]) {
                        _pendingNR = pool.plexus[2].sub(pool.plexus[3]);
                    }
                    IMiningTreasury(miningTreasury).airdropClaim(to, _pendingNR);
                    pool.plexus[3] = pool.plexus[3].add(_pendingNR);
                }
            }
            // console.log(pool.check, "check Pool");
            if (pool.check) {
                for (uint256 i; i < pool.airdrop.token.length; i++) {
                    if (userAirdropSetCheck[pid][i][to] != pool.airdrop.setCheck[i]) {
                        if (userAirdropSetCheck[pid][i][to] == true) {
                            userAirdropSetCheck[pid][i][to] = false;
                            userAirdropLastIndex[pid][i][to] = 0;
                        } else {
                            userAirdropSetCheck[pid][i][to] = true;
                            userAirdropLastIndex[pid][i][to] = 0;
                        }
                    }

                    uint256 airdropLastIndex = userAirdropLastIndex[pid][i][to];

                    if (pool.airdrop.perShare[i] > airdropLastIndex) {
                        // userAirdropLastIndex airdropToken 말고 airdropToken배열ID;
                        userAirdropLastIndex[pid][i][to] = pool.airdrop.perShare[i];
                        if (userAmount != 0) {
                            uint256 pendingToken = userAmount.mul(pool.airdrop.perShare[i].sub(airdropLastIndex)) /
                                ACC_NR_PRECISION;

                            if (pendingToken.add(pool.airdrop.totalReciveAmount[i]) >= pool.airdrop.totalAmount[i]) {
                                pendingToken = pool.airdrop.totalAmount[i].sub(pool.airdrop.totalReciveAmount[i]);
                            }
                            if (pendingToken != 0) {
                                IERC20(pool.airdrop.token[i]).safeTransfer(to, pendingToken);

                                pool.airdrop.totalReciveAmount[i] = pool.airdrop.totalReciveAmount[i].add(pendingToken);
                            }
                        }
                    }
                }
            }
            // console.log("harvest end");

            poolInfo[pid] = pool;
            // Effects

            emit Harvest(msg.sender, pid, _pendingNR);
        }
    }

    function poolCheck(address _lpToken) public view returns (bool check) {
        // require(feeToken.length == 0 || (_tIdOf[address(feeToken_)] == 0 && feeToken[0] != feeToken_), "feeToken_ already exists");
        if (lpToken.length == 0 || (lpTokenId[_lpToken] == 0 && address(lpToken[0]) != _lpToken)) {
            check = false;
        } else check = true;
    }

    function setFactory(address _factory) public {
        require(msg.sender == dev);
        factory = _factory;
    }

    function setAirdropFee(bool _res) public {
        require(msg.sender == dev);
        require(airdropFeePercent != 0, "airdropFeePercent is 0");
        airdropFee = _res;
    }

    // % * 10
    function setAirdropFeePercent(uint256 _percent) public {
        require(msg.sender == dev);
        airdropFeePercent = _percent;
    }

    function setMiningTreasury(address _mining) public {
        require(msg.sender == dev);
        miningTreasury = _mining;
    }
}

