// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "hardhat/console.sol";

import "./interfaces/IRewarder.sol";
import "./interfaces/ISTOA.sol";
import "./interfaces/IStoaToken.sol";

// Import and refactor of Synthetix StakingRewarder
/// @title Calculates and pays out rewards
/// @author bitbeckers
/// @notice MVP release
/// @custom:source This is an experimental contract.
// @custom:source https://github.com/Synthetixio/synthetix/blob/develop/contracts/StakingRewards.sol
contract Rewarder is ReentrancyGuard, Pausable, IRewarder, AccessControl {
    bytes32 private constant CONFIG_ROLE = keccak256("CONFIG_ROLE");
    bytes32 private constant REWARD_MINTER_ROLE = keccak256("REWARD_MINTER_ROLE");

    using Counters for Counters.Counter;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public token;
    address public stoa;
    address public treasury;
    uint256 public sagePathThreshold;

    mapping(address => Staker) private _stakers;
    mapping(address => mapping(uint256 => Stake)) private _stakes;
    mapping(uint256 => StakePool) public _stakingPools;

    uint256 private _totalStaked;
    uint256 private _totalAllocationPoints;
    uint256 private _lastBatchPayout;

    Counters.Counter public sagepathUsers;
    Counters.Counter public low;
    Counters.Counter public mid;
    Counters.Counter public high;
    Counters.Counter public stoic;

    struct Staker {
        bool exists;
        uint256 totalBalance;
        uint256 rewards;
        bool sagePathUser;
        uint256 sagepathTimestamp;
    }

    struct Stake {
        bool exists;
        uint256 userStakeRewardPerTokenPaid;
        uint256 balance;
        uint256 timestamp;
        uint256 lastClaimed;
    }

    struct StakePool {
        bool exists;
        uint256 balance;
        uint256 lastUpdateTime;
        uint256 allocationPoints;
        uint256 rewardPerTokenStored;
    }

    constructor(
        address _owner,
        address _tokenAddress,
        address _stoaData,
        address _treasuryAddress,
        uint256 _sagePathThreshold
    ) {
        token = _tokenAddress;
        stoa = _stoaData;
        treasury = _treasuryAddress;
        sagePathThreshold = _sagePathThreshold;
        _lastBatchPayout = block.timestamp;
        _setupRole(DEFAULT_ADMIN_ROLE, _owner);
        _grantRole(CONFIG_ROLE, _owner);
    }

    // Event
    event AvailableRewardsUpdated(uint256 added, uint256 totalSupply);
    event RewardPaid(string rewardType, address account, uint256 amount);
    event BatchRewardPaid(string rewardType, uint256 accountCount, uint256 amount);
    event Staked(address indexed user, uint256 amount, uint256 duration);
    event Withdrawn(address indexed user, uint256 amount, uint256 duration);
    event Recovered(address token, uint256 amount);
    event SagePathUpdated(uint256 threshold);
    event StakingPoolUpdated(uint256 duration, bool exists, uint256 allocationPoints);

    // Views
    function rewardsToken() external view returns (address _rewardsToken) {
        return token;
    }

    function lastBatchPayout() external view returns (uint256 timestamp) {
        return _lastBatchPayout;
    }

    // Base rewards
    function getBaseReward(address user) public view returns (uint256 rewardAmount) {
        uint256 timeSinceLastClaim = block.timestamp.sub(_stoa().getLastClaimed(user));
        uint256 periodsSinceLastClaimed = _stoa().getPeriodsSince(_stoa().getLastClaimed(user));
        uint256 periodIndex = _stoa().getPeriodIndex();
        uint256 periodDuration = _stoa().getPeriodDuration();

        if (timeSinceLastClaim <= 0) {
            return 0;
        }
        uint256 sumDuration = 0;

        if (periodsSinceLastClaimed == 0 || periodIndex == 1) {
            uint256 duringSeason = _stoa()
                .getBaseRewardRateForPeriod(periodIndex)
                .div(_stoa().getUserCount(periodIndex))
                .mul(timeSinceLastClaim);
            sumDuration += duringSeason;
        } else {
            for (uint256 i = periodsSinceLastClaimed; i > 0; i--) {
                uint256 duringSeason = 0;
                uint256 _period = periodIndex - i;

                if (i == periodsSinceLastClaimed) {
                    uint256 timeInFirstPeriod = timeSinceLastClaim % periodDuration;
                    duringSeason = _stoa().getBaseRewardRateForPeriod(_period).div(_stoa().getUserCount(_period)).mul(
                        timeInFirstPeriod
                    );
                } else {
                    duringSeason = _stoa().getBaseRewardRateForPeriod(_period).div(_stoa().getUserCount(_period)).mul(
                        periodDuration
                    );
                }

                sumDuration += duringSeason;
            }
            sumDuration += _stoa().getBaseRewardRateForPeriod(periodIndex).div(_stoa().getUserCount(periodIndex)).mul(
                periodDuration
            );
        }

        return sumDuration;
    }

    // Staking
    function totalStaked() external view returns (uint256 amount) {
        amount = _totalStaked;
    }

    function getStakedBalance(address account, uint256 duration) public view returns (uint256 totalBalance) {
        return _stakes[account][duration].balance;
    }

    function updateStakingPool(
        uint256 duration,
        bool exists,
        uint256 allocationPoints
    ) external onlyRole(CONFIG_ROLE) {
        StakePool storage stakePool = _stakingPools[duration];
        uint256 totalAllocationPoints = _totalAllocationPoints - stakePool.allocationPoints;

        stakePool.exists = exists;
        stakePool.allocationPoints = allocationPoints;
        _totalAllocationPoints = totalAllocationPoints + allocationPoints;
        stakePool.rewardPerTokenStored = stakingRewardPerToken(duration);
        stakePool.lastUpdateTime = block.timestamp;
        emit StakingPoolUpdated(duration, exists, allocationPoints);
    }

    function stakingRewardPerToken(uint256 duration) public view override returns (uint256 rewardPerToken) {
        StakePool storage _stakingPool = _stakingPools[duration];

        if (_stakingPool.balance == 0) {
            return _stakingPool.rewardPerTokenStored;
        }

        if (duration == 0) {
            return
                _stakingPool.rewardPerTokenStored.add(
                    lastTimeRewardApplicable()
                        .sub(_stakingPool.lastUpdateTime)
                        .mul(_stoa().getStakingRewardRateForPeriod(_stoa().getPeriodIndex()))
                        .mul(1e18)
                        .div(_stakingPool.balance)
                );
        }

        if (duration > 0) {
            uint256 ratio = _stoa().getCommitmentRewardsForPeriod(_stoa().getPeriodIndex());
            ratio = ratio.mul(_stakingPool.allocationPoints).div(_totalAllocationPoints);
            return
                _stakingPool.rewardPerTokenStored.add(
                    lastTimeRewardApplicable().sub(_stakingPool.lastUpdateTime).mul(ratio).mul(1e18).div(
                        _stakingPool.balance
                    )
                );
        }
    }

    function getStakingReward(address user, uint256 duration) public view override returns (uint256 rewardAmount) {
        if (duration == 0) {
            return
                getStakedBalance(user, duration)
                    .mul(stakingRewardPerToken(duration).sub(_stakes[user][duration].userStakeRewardPerTokenPaid))
                    .div(1e18)
                    .add(_stakers[user].rewards);
        }
        if (!_stakers[user].sagePathUser) {
            return 0;
        }
        return
            getStakedBalance(user, duration)
                .mul(stakingRewardPerToken(duration).sub(_stakes[user][duration].userStakeRewardPerTokenPaid))
                .div(1e18)
                .add(_stakers[user].rewards);
    }

    // Accomplishment
    function getAccomplishmentReward(address user) public view returns (uint256 rewardAmount) {
        uint256 timeSinceLastClaim = block.timestamp.sub(_stoa().getLastClaimed(user));

        if (timeSinceLastClaim <= 0) {
            return 0;
        }

        uint256 periodsSinceLastClaimed = _stoa().getPeriodsSince(_stoa().getLastClaimed(user));
        uint256 _periodIndex = _stoa().getPeriodIndex();

        if (periodsSinceLastClaimed == 0 || _periodIndex == 1) {
            return 0;
        } else {
            uint256 sumDuration = 0;

            for (uint256 i = periodsSinceLastClaimed; i > 0; i--) {
                uint256 _period = _periodIndex - i;
                sumDuration += _calculateAccomplishmentReward(user, _period);
            }
            sumDuration += _calculateAccomplishmentReward(user, _periodIndex);
            return sumDuration;
        }
    }

    // Sage path
    function isSagePathUser(address user) public view returns (bool isIndeed) {
        return _meetsSagePathThreshold(user);
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return block.timestamp < _stoa().getPeriodEnd() ? block.timestamp : _stoa().getPeriodEnd();
    }

    function getSagepathCount() public view returns (uint256 sagepathCount) {
        return sagepathUsers.current();
    }

    // Mutative functions
    // Claims
    function claimRewards(uint256[] calldata stakingPools) external nonReentrant onlyActiveUser {
        uint256 totalRewards = 0;

        for (uint256 i = 0; i < stakingPools.length; i++) {
            totalRewards += getStakingReward(msg.sender, stakingPools[i]);
            _stakes[msg.sender][stakingPools[i]].lastClaimed = block.timestamp;
        }

        if (isSagePathUser(msg.sender)) {
            // totalRewards += getCommitmentReward(msg.sender);
            totalRewards += getAccomplishmentReward(msg.sender);
        }

        _stoa().notifyRewardsPaid(msg.sender, totalRewards, _stoa().getPeriodIndex());
        _token().mintReward(msg.sender, totalRewards);
        emit RewardPaid("AllRewards", msg.sender, totalRewards);
    }

    function batchClaimBaseRewards(
        address[] calldata users,
        uint256[] calldata amounts,
        uint256 upperTimestamp,
        uint256 periodIndex
    ) external nonReentrant onlyRole(REWARD_MINTER_ROLE) {
        require(upperTimestamp <= block.timestamp, "Only past rewards");
        console.log("Upper timestamp: ", upperTimestamp);
        console.log("Last batch payout: ", _lastBatchPayout);
        console.log("Div: ", upperTimestamp - _lastBatchPayout);
        require((upperTimestamp - _lastBatchPayout) >= 1 days, "Only after 1 day");

        uint256 totalSpent = 0;
        for (uint256 i = 0; i < users.length; i++) {
            _stoa().notifyRewardsPaid(users[i], amounts[i], periodIndex);
            _token().mintReward(users[i], amounts[i]);
            totalSpent += amounts[i];
        }
        _lastBatchPayout = block.timestamp;
        emit BatchRewardPaid("BatchRewards", users.length, totalSpent);
    }

    // Staking
    function stake(uint256 amount, uint256 duration)
        external
        nonReentrant
        whenNotPaused
        updateReward(msg.sender, duration)
        onlyValidStakeDuration(duration)
    {
        require(amount > 0, "Cannot stake 0");

        Stake storage _stake = _stakes[msg.sender][duration];
        if (duration > 0) {
            require(!_stake.exists, "Already staked");
        }

        Staker storage staker = _stakers[msg.sender];
        StakePool storage stakePool = _stakingPools[0];

        if (!staker.exists) {
            staker.exists = true;
        }

        _totalStaked = _totalStaked.add(amount);
        staker.totalBalance = staker.totalBalance.add(amount);
        stakePool.balance = stakePool.balance.add(amount);
        _stake.balance = _stake.balance.add(amount);
        _stake.exists = true;

        if (staker.sagePathUser == false && _meetsSagePathThreshold(msg.sender)) {
            staker.sagePathUser = true;
            staker.sagepathTimestamp = block.timestamp;
            sagepathUsers.increment();
        }
        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);
        emit Staked(msg.sender, amount, duration);
    }

    function withdraw(uint256 amount, uint256 duration) public nonReentrant updateReward(msg.sender, duration) {
        require(amount > 0, "Cannot withdraw 0");

        Stake storage _stake = _stakes[msg.sender][duration];
        require(_stake.exists, "Nothing staked");
        require(amount <= _stake.balance, "Cannot withdraw more than staked in this pool");

        Staker storage staker = _stakers[msg.sender];
        StakePool storage stakePool = _stakingPools[0];

        _totalStaked = _totalStaked.sub(amount);
        staker.totalBalance = staker.totalBalance.sub(amount);
        _stake.balance = _stake.balance.sub(amount);
        stakePool.balance = stakePool.balance.sub(amount);

        if (staker.sagePathUser == true && !_meetsSagePathThreshold(msg.sender)) {
            staker.sagePathUser = false;
            staker.sagepathTimestamp = 0;
            sagepathUsers.decrement();
        }

        _stake.exists = false;

        IERC20(token).safeTransfer(msg.sender, amount);
        emit Withdrawn(msg.sender, amount, duration);
    }

    function exit(uint256[] calldata stakingPools) external {
        uint256 stakingRewards = 0;
        withdraw(_stakes[msg.sender][stakingPools[0]].balance, 0);

        for (uint256 i = 0; i < stakingPools.length; i++) {
            stakingRewards += getStakingReward(msg.sender, stakingPools[i]);
            withdraw(_stakes[msg.sender][stakingPools[i]].balance, stakingPools[i]);
        }

        // uint256 rewardAmount = stakingRewards + getAccomplishmentReward(msg.sender) + getCommitmentReward(msg.sender);
        uint256 rewardAmount = stakingRewards + getAccomplishmentReward(msg.sender);

        _token().mintReward(msg.sender, rewardAmount);
        emit RewardPaid("Exit", msg.sender, rewardAmount);
    }

    // Internal
    function _meetsSagePathThreshold(address user) internal view returns (bool passed) {
        return _stakers[user].totalBalance >= sagePathThreshold;
    }

    function _calculatePortion(uint256 amount, uint256 ratio) internal pure returns (uint256 portion) {
        return amount.mul(ratio).div(1e18);
    }

    // TODO no magic numbers
    function _calculateAccomplishmentReward(address user, uint256 periodIndex) internal view returns (uint256 amount) {
        uint256 lvl = _stoa().getAccomplishmentLevel(user, periodIndex);
        (uint256 bronze, uint256 silver, uint256 gold) = _stoa().getAccomplishments(periodIndex);
        if (lvl == 1) {
            return amount = _stoa().getAccomplishmentRewardsForPeriod(periodIndex).mul(15).div(100).div(bronze);
        }
        if (lvl == 2) {
            return amount = _stoa().getAccomplishmentRewardsForPeriod(periodIndex).mul(30).div(100).div(silver);
        }
        if (lvl == 3) {
            return amount = _stoa().getAccomplishmentRewardsForPeriod(periodIndex).mul(55).div(100).div(gold);
        }

        return amount = 0;
    }

    // Config
    function setSagePathThreshold(uint256 _value) public onlyRole(CONFIG_ROLE) {
        sagePathThreshold = _value;
        emit SagePathUpdated(sagePathThreshold);
    }

    // Modifiers
    modifier updateReward(address user, uint256 duration) {
        StakePool storage stakingPool = _stakingPools[duration];
        if (!stakingPool.exists) {
            stakingPool.exists;
        }

        stakingPool.rewardPerTokenStored = stakingRewardPerToken(duration);
        stakingPool.lastUpdateTime = lastTimeRewardApplicable();
        if (user != address(0)) {
            _stakers[user].rewards = getStakingReward(user, duration);
            _stakes[user][duration].userStakeRewardPerTokenPaid = stakingPool.rewardPerTokenStored;
        }
        _;
    }

    modifier onlySagePath() {
        require(isSagePathUser(msg.sender), "Not a sage path user");
        _;
    }

    modifier onlyActiveUser() {
        (uint256 duration, uint256 timestamp) = _stoa().getLatestMeditation(msg.sender, _stoa().getPeriodIndex());
        require(block.timestamp - timestamp <= 7 days, "No meditation in last seven days");
        _;
    }

    modifier onlyValidStakeDuration(uint256 duration) {
        require(
            duration == 0 || duration == 30 || duration == 90 || duration == 180 || duration == 360,
            "Invalid vesting period duration"
        );
        _;
    }

    // System
    function _stoa() internal view returns (ISTOA) {
        return ISTOA(stoa);
    }

    function _treasury() internal view returns (address) {
        return treasury;
    }

    function _token() internal view returns (IStoaToken) {
        return IStoaToken(token);
    }
}

