pragma solidity 0.8.10;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "./interfaces/ITradePairs.sol";
import "./interfaces/IOrderBooks.sol";
import "./interfaces/IPortfolio.sol";
import "./interfaces/IDexOwner.sol";
import '../libraries/TransferHelper.sol';

contract DexOwner is Ownable, IDexOwner{
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  // reference to OrderBooks contract (one sell or buy book)
  IOrderBooks public orderBooks;
  IPortfolio public portfolio;
  ITradePairs public tradePairs;

  // account collecting fees
  address public override commissionAddress;
  uint256 public referFee;

  mapping(string=>bool) internal adminSettings;


  event ChangedCommissionAddress(address commissionAddress);
  event Initialize(address orderBookAddress, address portofolioAddress, address tradePairsAddress);
  event ChangedReferFee(uint256 referFee);
  event ChangedSetAdmin(string setting, bool boolean);
  event RescueETH(address rescueAddress, uint256 amount);
  event RescueTokens(address token, address rescueAddress, uint256 amount);

  constructor() {
    referFee = 2000;
    adminSettings['deposits'] = true;
    adminSettings['withdraws'] = true;
    adminSettings['orders'] = true;
  }

  function setCommissionAddress(address _commissionAddress) external onlyOwner {
    require(_commissionAddress != address(0));
    commissionAddress = _commissionAddress;
    emit ChangedCommissionAddress(commissionAddress);
  }

  function setReferFee(uint256 _fee) external onlyOwner {
    referFee = _fee;
    emit ChangedReferFee(referFee);
  }

  function setAdmin(string memory setting, bool boolean) external onlyOwner {
    adminSettings[setting] = boolean;
    emit ChangedSetAdmin(setting, boolean);
  }

  function initialize(address orderBookAddress, address portofolioAddress, address tradePairsAddress) external onlyOwner {
    orderBooks = IOrderBooks(orderBookAddress);
    portfolio = IPortfolio(portofolioAddress);
    tradePairs = ITradePairs(tradePairsAddress);
    emit Initialize(orderBookAddress, portofolioAddress, tradePairsAddress);
  }

  function rescueETH(address rescueAddress, uint256 amount) external onlyOwner payable {
    TransferHelper.safeTransferETH(rescueAddress, amount);
    emit RescueETH(rescueAddress, amount);
  }

  function rescueTokens(address token, address rescueAddress, uint256 amount) external onlyOwner payable {
    TransferHelper.safeTransfer(token, rescueAddress, amount);
    emit RescueTokens(token, rescueAddress, amount);
  }
}

