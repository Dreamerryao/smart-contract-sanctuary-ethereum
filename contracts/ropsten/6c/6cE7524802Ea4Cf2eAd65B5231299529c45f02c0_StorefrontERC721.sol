pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/MulticallUpgradeable.sol";
import "../common/open_sea/token/OpenSeaERC721.sol";
import "../common/manifold_xyz/royalry/ManifoldXyzRoyalty.sol";

/**
 * ERC721 contract compatible with Storefront.
 */
contract StorefrontERC721 is
    OwnableUpgradeable,
    MulticallUpgradeable,
    OpenSeaERC721,
    ManifoldXyzRoyalty
{
    /**
     * @dev The address of the sales contract
     */
    address public firstSaleStorefront;

    /**
     * @dev Contract initializer
     * @param owner_ The address of the owner
     * @param name_ The name of the Token
     * @param symbol_ The symbol of the Token
     * @param baseTokenURI_ The base URI, which is used to build token URI
     * @param openSeaProxyRegistr_ The address of Opensea Proxy Registry
     * @param firstSaleStorefront_ The address of sales contract
     */
    function inititalize(
        address owner_,
        string memory name_,
        string memory symbol_,
        string memory baseTokenURI_,
        address openSeaProxyRegistr_,
        address firstSaleStorefront_
    ) public initializer {
        _setOpenSeaERC721(name_, symbol_, baseTokenURI_, openSeaProxyRegistr_);

        _transferOwnership(address(this));

        _transferOwnership(owner_);

        firstSaleStorefront = firstSaleStorefront_;
    }

    /**
     * @dev Checks if given interface is supported
     * @param interfaceId The ID of the interface being checked
     * @return True if supported, false otherwise
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(EIP2981RoyaltyOverride, ERC721Upgradeable)
        returns (bool)
    {
        return
            EIP2981RoyaltyOverride.supportsInterface(interfaceId) ||
            ERC721Upgradeable.supportsInterface(interfaceId);
    }

    /**
     * @notice Instructs if the given address is authorized to mint
     * @dev Instructs if the given address is authorized to mint. Initialized with owner only being able to mint, will be overriden
     * @param sender The address of potential minter
     * @return True if authorized, false otherwise
     */
    function hasMintPermission(address sender)
        public
        view
        virtual
        override
        returns (bool)
    {
        return sender == owner() || sender == firstSaleStorefront;
    }

    /**
     * @dev Checks if user has permission to set royalties
     * @param sender The address of user
     * @return True if user has permissions, false otherwise
     */
    function hasSetRoyaltyPermission(address sender)
        public
        view
        virtual
        override
        returns (bool)
    {
        return sender == owner();
    }

    /**
     * @notice Gets msg.sender from the context
     * @dev Gets msg.sender from the context. Needed for handling metatransactions
     * @return the address of msg.sender
     */
    function _msgSender()
        internal
        view
        virtual
        override(ContextUpgradeable, OpenSeaERC721, ManifoldXyzRoyalty)
        returns (address)
    {
        return OpenSeaERC721._msgSender();
    }
}

