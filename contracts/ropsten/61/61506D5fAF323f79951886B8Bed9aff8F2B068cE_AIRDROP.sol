contract UNT is ERC721A, Ownable {

    using Strings for uint256;

    string public PROVENANCE;
    uint256 public tokenPrice = 321000000000000000; // 0.321 ETH
    uint256 public presaleTokenPrice = 160000000000000000; // 0.16 ETH
    uint256 public presaleMaxToken = 50;
    uint256 public pubsaleMaxToken = 2050;
    uint256 public reserveMaxToken = 400;
    uint public maxTokenPurchase = 2;
    uint public maxTokenPerWallet = 2;
    uint256 public MAX_TOKENS = 2500;
    bool public saleIsActive = false;
    string private _baseURIextended;
    bool public revealed = false;

    uint256 public presaledtoken = 0;
    uint256 public pubsaledtoken = 0;
    uint256 public reservedtoken = 0;

    // string public notRevealedUri = "https://gateway.pinata.cloud/ipfs/QmZZ36AZ3L8jKTkNFQ1HQwVLYS2UpzeN3oG3HkZ99Zjtb2/notReveal.json";
    string public baseExtension = ".json";
	
    // WhiteLists for presale.
    mapping (address => bool) private _isWhiteListed;
    mapping (address => uint) private _numberOfWallets;
    
    event AddWhiteListWallet(address _wallet );
    event RemoveWhiteListWallet(address _wallet );

    constructor(

    ) ERC721A("Untethered", "UNT") {

    }

    function setBaseURI(string memory baseURI_) external onlyOwner() {
        _baseURIextended = baseURI_;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseURIextended;
    }

    function setProvenance(string memory provenance) public onlyOwner {
        PROVENANCE = provenance;
    }

	// Override 
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory)
    {
        require( _exists(tokenId), "ERC721Metadata: URI query for nonexistent token" );

        if(revealed == false) {
            return "https://gateway.pinata.cloud/ipfs/QmZZ36AZ3L8jKTkNFQ1HQwVLYS2UpzeN3oG3HkZ99Zjtb2/notReveal.json";
        }

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0
            ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
            : "";
    }

    function flipSaleState() public onlyOwner {
        saleIsActive = !saleIsActive;
    }

    function reserveToken(uint numberOfTokens) public onlyOwner {
        require(reservedtoken + numberOfTokens <= reserveMaxToken, "Reserve would exceed max supply of Reserve tokens");
        _safeMint(msg.sender, numberOfTokens);
        reservedtoken = reservedtoken + numberOfTokens;
    }

	// Add & Remove WhiteLists
	function addWhiteListWallet (address[] memory _wallets) public onlyOwner  {
		for(uint i = 0; i < _wallets.length; i++) {
			_isWhiteListed[_wallets[i]] = true;
		}   
    }
    
    function removeWhiteListWallet (address[] memory _wallets) public onlyOwner  {
        for(uint i = 0; i < _wallets.length; i++) {
			_isWhiteListed[_wallets[i]] = false;
		}
    }


    function presaleToken(uint numberOfTokens) public payable {
        require(!saleIsActive, "PreSale must be inactive to mint Tokens");
        require(_isWhiteListed[msg.sender], "You are not allowed to mint before crowd sale");
        require(presaledtoken + numberOfTokens <= presaleMaxToken, "Purchase would exceed max supply of Presale tokens");
        require(presaleTokenPrice * numberOfTokens <= msg.value, "Ether value sent is not correct");
        require(numberOfTokens <= maxTokenPurchase, "Exceeded max token purchase");
        require(_numberOfWallets[msg.sender] + numberOfTokens <= maxTokenPerWallet, "Exceeded max token purchase per wallet");

        _safeMint(msg.sender, numberOfTokens);

        presaledtoken = presaledtoken + numberOfTokens;
        _numberOfWallets[msg.sender] = _numberOfWallets[msg.sender] + numberOfTokens;
    }

    function mintToken(uint numberOfTokens) public payable {
        require(saleIsActive, "Sale must be active to mint Tokens");
        require(pubsaledtoken + numberOfTokens <= pubsaleMaxToken, "Purchase would exceed max supply of Pubsale tokens");
        require(numberOfTokens <= maxTokenPurchase, "Exceeded max token purchase");
        require(_numberOfWallets[msg.sender] + numberOfTokens <= maxTokenPerWallet, "Exceeded max token purchase per wallet");
        require(tokenPrice * numberOfTokens <= msg.value, "Ether value sent is not correct");

        _safeMint(msg.sender, numberOfTokens);
        
        pubsaledtoken = pubsaledtoken + numberOfTokens;
        _numberOfWallets[msg.sender] = _numberOfWallets[msg.sender] + numberOfTokens;
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(0xc468456401A4A0da1168F0fa474147866c704a25).transfer(balance);
    }

}


contract AIRDROP is ERC721, ERC721Enumerable, Ownable {

    using Strings for uint256;
	using SafeMath for uint256;
	
    address public UntetheredContractAddress = 0x6c15a1120Daf4f1CE1A8C1388234f4505C39eA39;

    uint256 public UNT_MAX_TOKENS = 2500;
    string[] public POOL_LIST;
    string private _baseURIextended;
    string public baseExtension = ".json";

    mapping (bytes => uint256) public _poolTokenList; 
    mapping (bytes => bool) private _isPoolListed; 
    mapping (bytes => uint256) public _poolTotalSupply;


    constructor(
    ) ERC721("Airdrop", "AD") {
    }
	
    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function setBaseURI(string memory baseURI_) external onlyOwner() {
        _baseURIextended = baseURI_;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseURIextended;
    }

	// Override 
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory)
    {
        require(
        _exists(tokenId),
        "ERC721Metadata: URI query for nonexistent token"
        );

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0
            ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
            : "";
    }

    function getPoolTotalSupply(string memory poolName) public view returns(uint256) {
        require( _isPoolListed[bytes(poolName)], "This Pool does not exist" );
        return( _poolTotalSupply[bytes(poolName)] );       
    }

    function airdrop(string memory poolName) public {
        require( _isPoolListed[bytes(poolName)], "This Pool does not exist" );
        
        UNT UntNFT = UNT(UntetheredContractAddress);
        uint256 UntTotalSupply = UntNFT.totalSupply();
        // Only airdrop secondary NFT for new main NFT holders.
        if( _poolTotalSupply[bytes(poolName)] < UntTotalSupply ) {
            uint256 mintStartIdx = _poolTotalSupply[bytes(poolName)];
            uint256 mintEndIdx   = UntTotalSupply;
            for (uint i = mintStartIdx; i < mintEndIdx; i++) {
                uint256 mintIdx = _poolTokenList[bytes(poolName)] + i;
                address minter = UntNFT.ownerOf(i);
                _safeMint(minter, mintIdx);
                _poolTotalSupply[bytes(poolName)]++;
            }
        }
    }


    function addPool(string memory poolName) public onlyOwner {
        require( !_isPoolListed[bytes(poolName)], "This Pool already exist" );
        uint poolLength = POOL_LIST.length;
        _poolTokenList[bytes(poolName)] = UNT_MAX_TOKENS * poolLength;
        _isPoolListed[bytes(poolName)] = true;
        POOL_LIST.push(poolName);
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
		require(balance > 0, "No balance!");
        payable(msg.sender).transfer(balance);
    }

}