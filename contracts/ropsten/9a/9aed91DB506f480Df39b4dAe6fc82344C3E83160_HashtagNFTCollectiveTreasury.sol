pragma solidity ^0.8.0;

contract HashtagNFTCollectiveTreasury is
    Initializable,
    IHashtagNFTCollectiveTreasury,
    ReentrancyGuardUpgradeable,
    ERC2771Config
{
    mapping(address => uint256) public ercBalances;

    using SafeMathUpgradeable for uint256;

    function initialize() external initializer {
        __ERC2771Config_init();
    }

    function DepositRoyalty(
        address[] calldata tokenAddresses,
        uint256[] calldata tokenRoyalties
    ) external payable override {
        require(msg.value == _sum(tokenRoyalties), "Treasury: msg.value != total royalties");
        for (uint256 index = 0; index < (tokenAddresses.length); index++) {
            ercBalances[tokenAddresses[index]] = ercBalances[
                tokenAddresses[index]
            ].add(tokenRoyalties[index]);
        }
    }

    function GetHashtagNFTBalances(address[] calldata tokenAddresses)
        external
        view
        override
        returns (uint256[] memory)
    {
        uint256[] memory result = new uint256[](tokenAddresses.length);
        for (uint256 index = 0; index < tokenAddresses.length; index++)
            result[index] = ercBalances[tokenAddresses[index]];

        return result;
    }

    function _sum(uint256[] calldata royalties)
        internal
        pure
        returns (uint256)
    {
        uint256 value = 0;
        for (uint256 index = 0; index < royalties.length; index++)
            value = value.add(royalties[index]);

        return value;
    }

    modifier userIsTokenHolder(address token) {
        uint256 balance = IERC20(token).balanceOf(_msgSender());
        require(balance > 0, "Treasury: insufficient balance");
        _;
    }

    function CashOutAndBurnERC20(address token)
        external
        override
        nonReentrant
        userIsTokenHolder(token)
    {
        uint256 userERC20Balance = IERC20(token).balanceOf(_msgSender());
        uint256 userStake = (userERC20Balance * ercBalances[token]).div(IERC20(token).totalSupply());
        ercBalances[token] = ercBalances[token].sub(userStake);

        IHashtagNFTCollectiveManager(token).treasuryBurnAmount(_msgSender(), userERC20Balance);

        (bool sent, ) = payable(_msgSender()).call{ value: userStake }("");
        require(sent, "Treasury: cashout failed");
    }

    function WithdrawFromTreasuryToAddress(address receiver, uint256 amount)
        external
        override
        nonReentrant
    {
        // Authenticate Request sender to be RoyaltyItemManager's governor and balance >= amount
        address mgrAddr = IHashtagNFTCollectiveGovernor(_msgSender()).HashtagNFTCollectiveManager();
        require(IHashtagNFTCollectiveManager(mgrAddr).governor() == _msgSender(), "Treasury: not governor");
        require(ercBalances[mgrAddr] >= amount, "Treasury: insufficient treasury balance");

        ercBalances[mgrAddr] = ercBalances[mgrAddr].sub(amount);

        (bool sent, ) = payable(receiver).call{ value: amount }("");
        require(sent, "Treasury: withdraw failed");
    }

    function BuyOutClaimRoyalty(address token)
        external
        override
        nonReentrant
        userIsTokenHolder(token)
    {
        uint256 userERC20Balance = IERC20(token).balanceOf(_msgSender());
        uint256 userStake = (userERC20Balance * ercBalances[token]).div(
            IERC20(token).totalSupply()
        );
        ercBalances[token] = ercBalances[token].sub(userStake);

        (bool sent, ) = payable(_msgSender()).call{ value: userStake }("");
        require(sent, "Treasury: claim royalty failed");
        // IHashtagNFTCollectiveManager(token).transferStakeToOwner(_msgSender());
    }
}

interface IHashtagNFTCollectiveTreasury {
    function DepositRoyalty(
        address[] calldata tokenAddresses,
        uint256[] calldata tokenRoyalties
    ) external payable;

    function GetHashtagNFTBalances(address[] calldata tokenAddresses)
        external
        returns (uint256[] memory);

    function CashOutAndBurnERC20(address token) external;

    function WithdrawFromTreasuryToAddress(address receiver, uint256 amount)
        external;

    function BuyOutClaimRoyalty(address tokenAddress) external;
}

