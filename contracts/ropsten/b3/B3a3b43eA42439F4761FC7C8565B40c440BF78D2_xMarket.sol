library LibOrder {
    enum TradeDirection {
        SELL_NFT,
        BUY_NFT
    }

    enum SignatureType {
        ILLEGAL,
        INVALID,
        EIP712,
        ETHSIGN,
        PRESIGNED
    }

    struct Signature {
        // How to validate the signature.
        SignatureType signatureType;
        // EC Signature data.
        uint8 v;
        // EC Signature data.
        bytes32 r;
        // EC Signature data.
        bytes32 s;
    }


    struct Fee {
        address recipient;
        uint256 amount;
        bytes feeData;
    }

    struct Property {
        IPropertyValidator propertyValidator;
        bytes propertyData;
    }

    struct ERC721Order {
        TradeDirection direction;
        address maker;
        address taker;
        uint256 expiry;
        uint256 nonce;
        IERC20TokenV06 erc20Token;
        uint256 erc20TokenAmount;
        Fee[] fees;
        IERC721Token erc721Token;
        uint256 erc721TokenId;
        Property[] erc721TokenProperties;
    }

    struct ERC1155Order {
        TradeDirection direction;
        address maker;
        address taker;
