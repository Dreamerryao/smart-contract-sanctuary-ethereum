/**
 * Cross-chain swap via 0x API
 */
contract CrossChainSwap is ZetaInteractor, ZetaReceiver, CrossChainSwapErrors {
    bytes32 public constant XC_SWAP_MESSAGE = keccak256("XC_SWAP");
    address public _zetaToken;

    uint256 public constant MASK = type(uint128).max;

    constructor(
        address _zetaConnector,
        address _zetaTokenInput
    ) ZetaInteractor(_zetaConnector) {
        console.log("[HH] Creating CrossChainSwap with %s %s", _zetaConnector, _zetaTokenInput);
        _zetaToken = _zetaTokenInput;
    }

    function swapTokensForTokens(
        address payable firstLegSwapTarget,
        bytes calldata firstLegSwapCallData,
        uint256 sellAmount,
        address payable secondLegSwapTarget,
        bytes calldata secondLegSwapCallData,
        address payable allowanceTarget,
        address payable buyTokenAddress,
        address payable sellTokenAddress,
        uint256 destinationChainId
    )
        external
        payable
    {
        // Validate destination `chainID`
        if (keccak256(interactorsByChainId[destinationChainId]) == keccak256(new bytes(0)))
            revert InvalidDestinationChainId();

        // Transfer `sellToken` to this contract from sender
        console.log("[HH] Transfering sell token %s", sellTokenAddress);
        if (!IERC20(sellTokenAddress).transfer(address(this), sellAmount))
            revert ErrorApprovingTokens(sellTokenAddress);

        // // Give `0xExchangeProxy` an infinite allowance to spend this contract's `sellToken`.
        // console.log("[HH] Approving sell token %s", sellTokenAddress);
        // if (!IERC20(sellTokenAddress).approve(allowanceTarget, sellAmount))
        //     revert ErrorApprovingTokens(sellTokenAddress);

        // // Call 1st leg 0x Swap - should buy ZETA
        // console.log("[HH] Buying ZETA via 0xExchangeProxy %s", firstLegSwapTarget);
        // (bool success1, ) = firstLegSwapTarget.call(firstLegSwapCallData);
        // if (!success1) revert BuyZetaFailed();

        // // Approve ZETA
        // console.log("[HH] Approving ZETA for connector %s", address(connector));
        // uint256 zetaFixedAmount = 10000000000000000000; // TODO calculate properly
        // if (!IERC20(_zetaToken).approve(address(connector), zetaFixedAmount))
        //     revert ErrorApprovingTokens(_zetaToken);

        // // Send message
        // console.log("[HH] Sending cross-chain message to chain %s", destinationChainId);
        // connector.send(
        //     ZetaInterfaces.SendInput({
        //         destinationChainId: destinationChainId,
        //         destinationAddress: interactorsByChainId[destinationChainId],
        //         gasLimit: 2500000,
        //         // TODO Also pass revert leg params
        //         message: abi.encode(
        //             XC_SWAP_MESSAGE,
        //             msg.sender,
        //             secondLegSwapTarget,
        //             secondLegSwapCallData,
        //             buyTokenAddress
        //         ),
        //         zetaAmount: zetaFixedAmount,
        //         zetaParams: abi.encode("")
        //     })
        // );
    }

    function onZetaMessage(ZetaInterfaces.ZetaMessage calldata zetaMessage)
        external
        override
        isValidMessageCall(zetaMessage)
    {
        // Decode incoming message
        (
            bytes32 messageType,
            address payable secondLegSwapTarget,
            bytes memory secondLegSwapCallData,
            address payable buyTokenAddress
        ) = abi.decode(zetaMessage.message, (bytes32, address, bytes, address));

        // Validate message type
        if (messageType != XC_SWAP_MESSAGE) revert InvalidMessageType();

        // Call 2n leg 0x Swap - should sell ZETA
        console.log("[HH] Selling ZETA via 0xExchangeProxy %s", secondLegSwapTarget);
        (bool success1, ) = secondLegSwapTarget.call(secondLegSwapCallData);
        if (!success1) revert SellZetaFailed();

        // Transfer purchased tokens to original sender
        address receiverAddress = address(uint160(bytes20(zetaMessage.originSenderAddress)));
        console.log("[HH] Transferring buy tokens back to %s", receiverAddress);
        uint256 buyAmount = IERC20(buyTokenAddress).balanceOf(address(this));
        bool success2 = IERC20(buyTokenAddress).transfer(receiverAddress, buyAmount);
        if (!success2) revert ErrorTransferringTokens(buyTokenAddress);
    }

    function onZetaRevert(ZetaInterfaces.ZetaRevert calldata zetaRevert)
        external
        override
        isValidRevertCall(zetaRevert)
    {
        revert NotImplemented();
    }
}

