import "./Strings.sol";
import "./JsmnSolLib.sol";
library JwtHelper {
    // slice JWT to header, payload and signature
    using strings for *;  
    enum JsmnType { UNDEFINED, OBJECT, ARRAY, STRING, PRIMITIVE }    
    struct Token {
        JsmnType jsmnType;
        uint start;
        bool startSet;
        uint end;
        bool endSet;
        uint8 size;
    }

    function splitJWT(string memory JWT) internal pure returns(string[] memory){                                               
        strings.slice memory s = JWT.toSlice();                
        strings.slice memory delim = ".".toSlice();                            
        string[] memory parts = new string[](s.count(delim)+1);
        for (uint i = 0; i < parts.length; i++) {                              
           parts[i] = s.split(delim).toString();                               
        }           
        return parts;                                                           
    }    

