pragma solidity ^0.8.0;

import "./Base64Url.sol";
import "./JwtHelper.sol";
import "./ECDSA.sol";
import "./JsmnSolLib.sol";

contract Verify2 {
    using JsmnSolLib for string;
    mapping(address => string) public addrList;
    // verify ES256 spend too much gas
    function verify(string memory jwt) public pure returns(address) {
        string[] memory jwtArray = JwtHelper.splitJWT(jwt);
        require(jwtArray.length > 2, "Error JWT format");
        string memory message = string.concat(jwtArray[0], ".", jwtArray[1]);
         (address addr, ECDSA.RecoverError e) = verifyES256(message, jwtArray[2]);
         return addr;
    }

    function verifyValid(string memory jwt, address addr) public pure returns(bool) {
        address pk = verify(jwt);
        return pk == addr;
    }
    function splitJWT(string memory jwt) public pure returns (string[] memory) {
        return JwtHelper.splitJWT(jwt);
    }

    function decodeSignature(string memory sign) public pure returns (bytes memory, uint256) {
        bytes memory signature = Base64Url.decode(sign);
        return (signature, signature.length);
    }

    function encodePayload(string memory payload) public pure returns (bytes32,bytes32) {
        return (sha256(abi.encodePacked(payload)),sha256(bytes(payload)));
    }

    function verifyES256(string memory message, string memory sign) public pure returns (address, ECDSA.RecoverError) {
        bytes32 data = keccak256(abi.encodePacked( message));
        bytes memory signature = Base64Url.decode(sign);
        (address addr, ECDSA.RecoverError e) = ECDSA.tryRecover(data, signature);
        return (addr ,e);
    }

    function verifyES256Valid(string memory message, string memory sign, address pk) public pure returns (bool) {
        bytes32 data = sha256(bytes(message));
        bytes memory signature = Base64Url.decode(sign);
        (address addr, ECDSA.RecoverError e) = ECDSA.tryRecover(data, signature);
        return (addr == pk);
    }

    function base64DecodePayloasd(string memory payload) public pure returns (string memory) {
        return string(Base64Url.decode(payload));
    }

    // function jsonDecodePayload(string memory payload) public pure returns (string memory, uint256 ) {
    //     return JwtHelper.foundISS(payload);
    // }

    function iss(string memory json) public pure returns (string[] memory){
        return JwtHelper.IssParse(json); 
    }
}

