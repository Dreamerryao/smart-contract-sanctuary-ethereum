pragma solidity >=0.5.0;

/**
 * Staking specifically tailored to Uniswap V2 variants
 */
contract UniV2Staker is BaseModule {

    using SafeERC20 for IERC20;
    using LibFees for FeeTypes.FeeStorage;

    event StakeSuccess(address indexed vault, 
                     IERC20 tokenA,
                     uint tokenAAmount, 
                     IERC20 tokenB,
                     uint tokenBAmount, 
                     uint lpAmount);
    event StakeFailed(address indexed vault, string reason);

    function encodeRequest(StakeTypes.StakeRequest calldata request) external view returns (bytes memory) {
        return abi.encode(request);
    }

    function encodeAuthProof(StakeTypes.StakeRequest calldata request, 
                            address vault, uint stepIndex) external view returns (bytes memory) {
        return StakeTypes.encodeAuthProof(request, vault, address(this), stepIndex);
    }

    function getLPQuote(StakeTypes.StakeRequest calldata request) external view returns (uint amountA, uint amountB) {
        (uint rA, uint rB) = UniV2Pair(request.pair).getReserves();
        if (rA == 0 && rB == 0) {
            revert("Currently do not support pools with zero reserves");
        }
        uint amountADesired = request.tokenA.amount;
        uint amountBDesired = request.tokenB.amount;
        uint amountAMin = (amountADesired * (10000-request.slippage)) / 10000;
        uint amountBMin = (amountBDesired * (10000-request.slippage)) / 10000;

        uint amountBOptimal = UniV2Router(request.router).quote(amountADesired, rA, rB);
        if (amountBOptimal <= amountBDesired) {
            require(amountBOptimal >= amountBMin, 'LPRouter: INSUFFICIENT_B_AMOUNT');
            (amountA, amountB) = (amountADesired, amountBOptimal);
        } else {
            uint amountAOptimal = UniV2Router(request.router).quote(amountBDesired, rB, rA);
            assert(amountAOptimal <= amountADesired);
            require(amountAOptimal >= amountAMin, 'LPRouter: INSUFFICIENT_A_AMOUNT');
            (amountA, amountB) = (amountAOptimal, amountBDesired);
        }
    }

    function execute(StrategyTypes.StrategyRequest calldata sReq,
                     uint step) external override returns (bool success, string memory failure) {

        StakeTypes.StakeRequest memory request = abi.decode(sReq.steps[step].extra, (StakeTypes.StakeRequest));
        _verifyNoTampering(sReq, request, step);
        uint aFee;
        uint bFee;
        (aFee, bFee) = _payFees(sReq, request);
        return _doStake(sReq,  request, aFee, bFee);
    }
    
    
    function _verifyNoTampering(StrategyTypes.StrategyRequest calldata sReq,
                                StakeTypes.StakeRequest memory request, 
                                uint step) internal view {
         bytes32 leaf = keccak256(StakeTypes.encodeAuthProof(request, sReq.vault, sReq.steps[step].module, sReq.steps[step].index));
         require(MerkleProof.verify(sReq.auth.stepProof[step], sReq.auth.merkleRoot, leaf), "Stake data mismatch from signed submission");
    }

    function _doStake(StrategyTypes.StrategyRequest calldata sReq,
                     StakeTypes.StakeRequest memory request,
                     uint aFee,
                     uint bFee) internal returns (bool, string memory) {

        request.tokenA.token.approve(request.router, request.tokenA.amount-aFee);
        request.tokenB.token.approve(request.router, request.tokenB.amount-bFee);

        console.log("Adding liquidity to router", request.router);
        try UniV2Router(request.router).addLiquidity(
            address(request.tokenA.token),
            address(request.tokenB.token),
            request.tokenA.amount - aFee,
            request.tokenB.amount - bFee,
            ((request.tokenA.amount-aFee) * (1000-request.slippage)) / 1000,
            ((request.tokenB.amount-bFee) * (1000-request.slippage)) / 1000,
            sReq.vault,
            block.timestamp + 30
        ) returns (uint depA, uint depB, uint lp) {
            emit StakeSuccess(sReq.vault, request.tokenA.token, depA, request.tokenB.token, depB, lp);
            return (true, "");
        } catch Error(string memory e) {
            console.log("Failed to add stake", e);
            emit StakeFailed(sReq.vault, e);
            return (false, e);
        } catch  {
            console.log("Failed to add stake for unknown reason");
            emit StakeFailed(sReq.vault, "Could not add liquidity");
            return (false, "Could not add liquidity");
        }
    }

    function _payFees(StrategyTypes.StrategyRequest calldata sReq, 
                        StakeTypes.StakeRequest memory request) internal
    returns (uint aFee, uint bFee) {

        IERC20[] memory tokens = new IERC20[](2);
        tokens[0] = request.tokenA.token;
        tokens[1] = request.tokenB.token;

        uint[] memory amounts = new uint[](2);
        amounts[0] =request.tokenA.amount;
        amounts[1] = request.tokenB.amount;
        
        uint[] memory fees = LibStorage.getFeeStorage().splitFees(tokens, amounts);
        aFee = fees[0];
        bFee = fees[1];
    }
}

