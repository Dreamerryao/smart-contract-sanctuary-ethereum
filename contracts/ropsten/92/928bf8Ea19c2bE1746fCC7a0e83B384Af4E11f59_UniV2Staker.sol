pragma solidity ^0.8.11;

import "./TokenTypes.sol";

library StakeTypes {

    struct StakeRequest {
        address router;
        address pair;
        TokenTypes.TokenAmount tokenA;
        TokenTypes.TokenAmount tokenB;
        uint slippage;
        bytes salt;
    }

    function encodeAuthProof(StakeRequest memory request, 
                            address vault, 
                            address module,
                            uint stepIndex) internal view returns (bytes memory) {
        return abi.encodePacked(vault, 
                                module,
                                address(request.router),
                                address(request.pair),
                                request.tokenA.token, 
                                request.tokenB.token, 
                                request.tokenA.amount,
                                request.slippage,
                                request.salt, 
                                stepIndex);
    }
}

