library LibFees {
    using SafeERC20 for IERC20;

    event FeesPaid(address indexed receiver, IERC20 indexed token, uint amount);

    //emitted when bps fees are paid for strategy execution
    event DexibleFeesPaid(address indexed receiver, IERC20 token, uint amount);

    uint8 constant internal DEFAULT_TOKEN_LIST_LENGTH = 5;

    //compute the affiliate percent owed to an affiliate
    /*
    function getAffiliatePercent(FeeTypes.FeeStorage storage fs, address affiliate, address signer) 
    external returns (uint) {
        if(affiliate == address(0)) {
            return 0;
        }
        ConfigTypes.Config storage cfg = LibStorage.getConfigStorage();
        uint base = cfg.baseAffiliatePercent;
        uint first = fs.firstSeen[affiliate][signer];
        if(first == 0) {
            fs.firstSeen[affiliate][signer] = block.timestamp;
            return base;
        }

        //we've seen them before. So we compute the time delta
        uint elapsed = block.timestamp - first;
        //the percentage reduced is the elapsed time times decay rate.
        return (base - (elapsed * cfg.affiliateDecayRate));
    }
    */

    function incrementFees(FeeTypes.FeeStorage storage fs, IERC20 token, uint amount) external returns (uint fee) {
        fee = (amount * fs._tempBps) / 10000;
        if(fee == 0) {
            return 0;
        }

        uint idx = fs._tempTokenIndexes[token];

        if(idx == 0) {
            fs._tempTokens.push(token);
            fs._tempAmounts.push(fee);
            fs._tempTokenIndexes[token] = fs._tempTokens.length;
        } else {
            fs._tempAmounts[idx-1] += fee;
        }
    }

    function splitFees(FeeTypes.FeeStorage storage fs, 
                        IERC20[] memory tokens,
                        uint[] memory amounts) external returns (uint[] memory fees) {
        uint8 partialBps = uint8(fs._tempBps/tokens.length);
        fees = new uint[](tokens.length);
        if(partialBps == 0) {
            return fees;
        }

        for(uint i=0;i<tokens.length;++i) {
            IERC20 token = tokens[i];
            fees[i] = (partialBps * amounts[i]) / 10000;
            uint idx = fs._tempTokenIndexes[token];
            if(idx == 0) {
                fs._tempTokens.push(token);
                fs._tempAmounts.push(fees[i]);
                fs._tempTokenIndexes[token] = fs._tempTokens.length;
            } else {
                fs._tempAmounts[idx-1] += fees[i];
            }
        }
    }

    function _clearFees(FeeTypes.FeeStorage storage fs) internal {
        for(;fs._tempTokens.length > 0;) {
            uint i=fs._tempTokens.length - 1;
            IERC20 token = fs._tempTokens[i];
            delete fs._tempTokenIndexes[token];
            fs._tempTokens.pop();
            fs._tempAmounts.pop();
        }
        fs._tempBps = 0;
    }

    function _distributeFees(FeeTypes.FeeStorage storage fs,
                            address affiliate, 
                            address dexible, 
                            uint affPortion
    ) internal {
       
        for( ; fs._tempTokens.length > 0 ; ) {
            uint i = fs._tempTokens.length-1;
            IERC20 token = fs._tempTokens[i];
            uint amt = fs._tempAmounts[i];
            uint affAmt = affiliate == address(0) ? 0 : ((amt * affPortion) / 1e20);
            if(affAmt > 0) {
                token.safeTransfer(affiliate, affAmt);
                emit DexibleFeesPaid(affiliate, token, affAmt);
            }
            uint rem = amt - affAmt;
            token.safeTransfer(dexible, rem);
            emit DexibleFeesPaid(dexible, token, rem);

            fs._tempTokens.pop();
            fs._tempAmounts.pop();
            delete fs._tempTokenIndexes[token];
        }
        fs._tempBps = 0;
    }
}

