pragma solidity ^0.8.0;

import {GIGADRIP20} from "./utils/GIGADRIP20.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import {IERC721Enumerable, IERC721} from "@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol";
import {console} from "hardhat/console.sol";

contract Luv is GIGADRIP20, Ownable {
    /*
==========================================================================================
                                    __    __  ___    __
                                   / /   / / / / |  / /
                                  / /   / / / /| | / /
                                 / /___/ /_/ / | |/ /
                                /_____/\____/  |___/
==========================================================================================

*/

    event DripStarted(
        address indexed _dripper,
        uint256 _timestamp,
        uint256 _multiplier
    );
    event DripStopped(address indexed _dripper, uint256 _timestamp);
    event DripUpdated(
        address indexed _dripper,
        uint256 _timestamp,
        uint256 _multiplier
    );
    event TokenBurned(address indexed _dripper, uint256 _amount);

    address freeMathHatContract;
    address genesisMathHatContract;

    constructor() GIGADRIP20("LUV", "LUV", 18, 138888888888888) {
        
               }

    function burnFrom(address _from, uint256 amount) public {
        require(
            msg.sender == _from,
            "GANG: Can only burn your own tokens, knucklehead"
        );
        require(
            _accruers[_from].balance >= amount,
            "GANG: you don't have enough tokens to burn ser"
        );
        _burn(_from, amount);
    }

    /** 
    * @notice - Free MATH Hat contract needs to be able to start/stop drips for mints/burns.
    */
    function startDrip(address _tokenHolder) public {
        uint256 multiplier = _accruers[_tokenHolder].multiplier;
        uint256 freeHatBalance = IERC721(freeMathHatContract).balanceOf(_tokenHolder);
        uint256 genesisHatBalance = IERC721(genesisMathHatContract).balanceOf(
            _tokenHolder
        );

        ///@dev - Require msg.sender has a free math hat
        require(
            freeHatBalance == 1,
            "GANG: Address is not in the CryptoGang! Go get a free hat ser"
        );

        ///@dev - Require a drip is not started; base multiplier is 1, so 0 means no drip.
        require(
            multiplier == 0,
            "GANG: Address already is receiving a drip!  We love the enthusiasm though"
        );

        console.log("MESSAGE SENDER: ", msg.sender);
        console.log("FREE HATS ADDR: ", freeMathHatContract);

        ///@dev - Check if caller is free MATH hat contract
        if (
            keccak256(abi.encode(msg.sender)) ==
            keccak256(abi.encode(freeMathHatContract))
        ) {
            ///@dev - Genesis hats will provide a multiplier for owning at least 1 hat
            ///@dev - Owners of multiple Genesis hats will not get more than 1 extra drip
            if (genesisHatBalance >= 1) {
                _startDripping(_tokenHolder, 2);
            } else if (genesisHatBalance == 0 && freeHatBalance == 1) {
                ///@dev - Free MATH Hat contract will be able to enable dripping upon minting of ERC721 token
                _startDripping(_tokenHolder, 1);
            }
        } else {

        ///@dev - Require the caller is only adjusting their own drip
        require(
            msg.sender == _tokenHolder,
            "GANG: Ser, you can only start/stop your own drip"
        );
        _startDripping(_tokenHolder, 1);
        }

        emit DripStarted(
            _tokenHolder,
            block.timestamp,
            _accruers[_tokenHolder].multiplier
        );
    }

    /** 
            @notice - Free MATH Hat contract needs to be able to start/stop drips for mints/burns.
         */
    function stopDrip(address _tokenHolder) public {
        uint256 multiplier = _accruers[_tokenHolder].multiplier;

        // Verify msg.sender is freeMathHat contract
        if (
            keccak256(abi.encode(msg.sender)) ==
            keccak256(abi.encode(freeMathHatContract))
        ) {
            // Require that the given address has a drip
            require(multiplier > 0, "GANG: Ser, you do not have a $LUV drip");

            // Free Hat contract can stop drip, used if user ragequits their ERC721 token
            _stopDripping(_tokenHolder, multiplier);
        }

        // Caller can only end their own drip
        require(
            msg.sender == _tokenHolder,
            "GANG: You can only start/stop your own drip, knucklehead"
        );

        // Verify a drip exists
        require(multiplier > 0, "GANG: Ser, you do not have a $LUV drip");

        _stopDripping(_tokenHolder, multiplier);

        emit DripStopped(_tokenHolder, block.timestamp);
    }

    function updateDripForGenesisHats() external {
        require(
            keccak256(abi.encode(msg.sender)) ==
                keccak256(abi.encode(freeMathHatContract))
        );

        if (IERC721(genesisMathHatContract).balanceOf(msg.sender) == 1) {
            _accruers[msg.sender].multiplier = 2;
        } else if ((IERC721(genesisMathHatContract).balanceOf(msg.sender) == 2)) {
            revert("GANG: You've reached your max multiplier, knucklehead");
        } else if ((IERC721(genesisMathHatContract).balanceOf(msg.sender) == 0)) {
            revert("GANG: You don't have a Genesis hat, ser");
        }

        emit DripUpdated(
            msg.sender,
            block.timestamp,
            _accruers[msg.sender].multiplier
        );
    }

    function getMultiplier(address _dripper) external view returns (uint256) {
        uint256 multiplier = _accruers[_dripper].multiplier;
        return multiplier;
    }


    /**
    
                ========== OWNER FUNCTIONS ========

     */

     function setFreeHatsContractAddr(address _freeHatsAddr) external onlyOwner {
        freeMathHatContract = _freeHatsAddr;
     }

     function setGenesisHatsContractAddr(address _genesisHatsAddr) external onlyOwner {
        genesisMathHatContract = _genesisHatsAddr;
     }
    /**
==========================================================================================
                           ______              ___ __      
                          / ____/_______  ____/ (_) /______
                         / /   / ___/ _ \/ __  / / __/ ___/
                        / /___/ /  /  __/ /_/ / / /_(__  ) 
                        \____/_/   \___/\__,_/_/\__/____/  
==========================================================================================
                                       
 */

    //  0xBeans - DRIP20 Contract - https://github.com/0xBeans/DRIP20/blob/master/src/DRIP20.sol
}

