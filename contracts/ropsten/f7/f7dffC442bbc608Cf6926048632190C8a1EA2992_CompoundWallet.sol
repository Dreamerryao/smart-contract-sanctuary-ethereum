/**
 * @dev Implementation of the {IWallet} interface for Compound.
 */

contract CompoundWallet is Context, OwnableWallet, IWallet {

    // Constants and Immutables
    string constant protocolName = "Compound";
    address immutable public compoundComptrollerAddress;
    /// @notice defined address for eth asset in protocol
    address constant ethDefinedAddress = address(0x0000000000000000000000000000000000000001);
    IComptroller immutable comptroller;
    /// @notice only can borrow or lend registered tokens in token registrar
    address immutable public registrarAddress;
    CompoundTokenRegistrar immutable registrar;

    // Private variables
    address private _factory;
    address private _router;


    constructor (address _compoundComptrollerAddress, address _registrarAddress) {
        _factory = address(0xdead);
        compoundComptrollerAddress = _compoundComptrollerAddress;
        comptroller = IComptroller(_compoundComptrollerAddress);
        
        registrarAddress = _registrarAddress;
        registrar = CompoundTokenRegistrar(_registrarAddress);
    }

    modifier onlyOwnerOrRouter() {
        require(_msgSender() == owner() || _msgSender() == router(), "Wallet: Restricted to owner or router");
        _;
    }

    /// @dev            Called only once when factory clone a wallet
    function init (address account, address routerAddress) external {
        require(_factory == address(0), "Wallet: FORBIDDEN"); 
        _factory = _msgSender();
        _router = routerAddress;
        setOwner(account);
    }
    
    /// @notice         Give factory address
    /// @return         Factory address
    function factory() public view returns (address) {
        return _factory;
    } 

    /// @notice         Give router address
    /// @return         Router address
    function router() public view returns (address) {
        return _router;
    } 

    /// @notice         Tells if protocol is paused or not
    /// @dev            Wallet calls factory to check if it is paused or not
    ///                 When protocol is paused, only repay and withdraw functions can be used
    /// @return         True if protocol is paused
    function isPaused() public view returns (bool) {
        return IFactory(_factory).paused(protocolName);
    }

    modifier whenNotPaused() {
        require(!isPaused(), "Wallet: Protocol is paused");
        _;
    }

    /// @notice         Tells if a token is registered in registrar of protocol or not
    /// @dev            Wallet calls registrar to check this
    /// @param token    Address of token
    /// @return         True if token is registered
    function isValidToken (address token) external view returns (bool) {
        return (registrar.getToken(token) != address(0));
    }

    /// @notice         Tells if an address is defined address for eth or not
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token
    /// @return         True if token is registered
    function isEth (address token) public pure returns (bool) {
        return (token == ethDefinedAddress);
    }

    /// @notice         Get cToken for specific token address
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token
    /// @return         Token address if token is registered, zero address for unregistered token
    function getCToken (address token) public view returns (address) {
        return registrar.getToken(token);
    }


    // TODO gas
    /// @notice         Deposit token as collateral and get cTokens
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param value    Collateral value
    /// @return         True if deposit is successful
    function deposit(address token, uint256 value) public onlyOwnerOrRouter whenNotPaused returns (bool) {
        address cTokenAddress = getCToken(token);
        require(cTokenAddress != address(0), "Wallet: Token is not valid");

        if (isEth(token)) {
            ICEth cToken = ICEth(cTokenAddress);
            cToken.mint{ value: value, gas: 250000 }();
        } else {
            // Approve cToken contract to move token
            IERC20(token).approve(cTokenAddress, value);
            // Deposit token
            ICToken cToken = ICToken(cTokenAddress);
            uint mintStatus = cToken.mint(value);
            require(mintStatus == 0, "Wallet: mint cTokens failed");
        }
        // Enter market
        address[] memory cTokens = new address[](1);
        cTokens[0] = cTokenAddress;

        uint[] memory errors = comptroller.enterMarkets(cTokens);
        require(errors[0] == 0, "Wallet: can't enter market");
        return true;
    }

    /// @notice         Borrow token 
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token that want to borrow
    /// @param value    borrow value
    /// @return         True if borrow is successful
    function borrow(address token, uint256 value) public onlyOwnerOrRouter whenNotPaused returns (bool) {
        address cTokenAddress = getCToken(token);
        require(cTokenAddress != address(0), "Wallet: Token is not valid");

        // Borrow token
        ICToken cToken = ICToken(cTokenAddress);
        uint status = cToken.borrow(value);
        require(status == 0, "Wallet: borrow tokens failed");
        return true;
    }

    // TODO specify gas in {}?

    /// @notice         repay borrow
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token that want to repay its debt
    /// @param value    repay value
    function repay(address token, uint256 value) public onlyOwner {
        address cTokenAddress = getCToken(token);
        require(cTokenAddress != address(0), "Wallet: Token is not valid");

        if (isEth(token)) {
            ICEth cToken = ICEth(cTokenAddress);
            cToken.repayBorrow{ value: value, gas: 250000 }();
        } else {
            // Approve cToken contract to move token
            IERC20(token).approve(cTokenAddress, value);
            
            // repay token
            ICToken cToken = ICToken(cTokenAddress);
            uint status = cToken.repayBorrow(value);
            require (status == 0, "Wallet: repay tokens failed");
        }

    }

    /// @notice         Redeem cTokens and get underlying token
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token that want to redeem
    /// @param value    Token amount to redeem
    function redeem(address token, uint256 value) public onlyOwner whenNotPaused {
        address cTokenAddress = getCToken(token);
        require(cTokenAddress != address(0), "Wallet: Token is not valid");

        // redeem aTokens
        ICToken cToken = ICToken(cTokenAddress);
        uint status = cToken.redeem(value);
        require (status == 0, "Wallet: redeem cTokens failed");
    }

    /// @notice         Transfer tokens to owner address
    /// @param token    Address of token that want to withdraw from smart contract
    /// @param value    Token amount to withdraw
    function forceWithdrawToken(address token, uint256 value) public onlyOwner {
        // withdraw tokens
        IERC20(token).transfer(owner(), value);
    }

    /// @notice         Transfer eth to owner address
    /// @param value    Eth amount to withdraw
    function forceWithdrawEthereum(uint256 value) public onlyOwner {
        // withdraw ethereum
        (bool sent, ) = owner().call{value: value}("");
        require(sent, "Wallet: Failed to send Ether");
    }

    /// @notice         Borrow and transfer borrowed tokens to owner address
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    /// @param token    Address of token that want to borrow and withdraw
    /// @param value    Token amount to borrow
    function borrowAndWithdraw (address token, uint256 value) public onlyOwner whenNotPaused {
        borrow(token, value);
        if (isEth(token)) {
            forceWithdrawEthereum(value);
        }
        else {
            forceWithdrawToken(token, value);
        }
    }

    // TODO stage test
    /// @notice         Redeem and transfer redeemed tokens to owner address
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    ///                 From address must approve to transfer tokens
    /// @param token    Address of token that want to redeem and withdraw
    /// @param value    Token amount to redeem
    function redeemAndWithdraw (address token, uint256 value) public onlyOwner whenNotPaused {
        redeem(token, value);
        if (isEth(token)) {
            forceWithdrawEthereum(value);
        }
        else {
            forceWithdrawToken(token, value);
        }
    }
    
    /// @notice         transfer tokens and repay debt with them 
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    ///                 From address must approve to transfer tokens
    /// @param from     Address of user that gave allowance
    ///                 can use funds of non owner accounts too
    /// @param token    Address of token that want to repay its debt
    /// @param value    Token amount to repay
    function transferAndRepay (address from, address token, uint256 value) public payable onlyOwner {
        if (isEth(token)) {
            repay(token, msg.value);
        }
        else {
            IERC20(token).transferFrom(from, address(this), value);
            repay(token, value);
        }
    }

    // TODO eth test
    /// @notice         transfer tokens and deposit them as collateral
    /// @dev            To facilitate, Eth is considered as a token with address equal to eth defined address
    ///                 From address must approve to transfer tokens
    /// @param from     Address of user that gave allowance
    ///                 can use funds of non owner accounts too
    /// @param token    Address of token that want to lend
    /// @param value    Token amount to deposit
    function transferAndDeposit (address from, address token, uint256 value) public payable onlyOwner whenNotPaused {
        if (isEth(token)) {
            deposit(token, msg.value);
        } else {
            IERC20(token).transferFrom(from, address(this), value);
            deposit(token, value);
        }
    }

    receive() external payable {
        // receive ethereum
    }

}

