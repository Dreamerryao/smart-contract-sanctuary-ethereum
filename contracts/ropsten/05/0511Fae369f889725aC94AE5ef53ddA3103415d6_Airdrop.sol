pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

import "@boringcrypto/boring-solidity/contracts/libraries/BoringMath.sol";
import "@boringcrypto/boring-solidity/contracts/BoringBatchable.sol";
import "@boringcrypto/boring-solidity/contracts/BoringOwnable.sol";
import "../libraries/SignedSafeMath.sol";
import "../interfaces/IRewarder2.sol";
import "../interfaces/IMasterChef2.sol";
import "hardhat/console.sol";
import "../interfaces/IUniswapV2Factory.sol";

interface IMigratorChef {
    // Take the current LP token address and return the new LP token address.
    // Migrator should have full access to the caller's LP token.
    function migrate(IERC20 token) external returns (IERC20);
}

/// @notice The (older) MasterChef contract gives out a constant number of NR tokens per block.
/// It is the only address with minting rights for NR.
/// The idea for this MasterChef V2 (MCV2) contract is therefore to be the owner of a dummy token
/// that is deposited into the MasterChef V1 (MCV1) contract.
/// The allocation point for this pool on MCV1 is the total allocation point for all pools that receive double incentives.
contract Airdrop is BoringOwnable, BoringBatchable {
    using BoringMath for uint256;
    using BoringMath128 for uint128;
    using BoringERC20 for IERC20;
    using SignedSafeMath for int256;

    /// @notice Info of each MCV2 user.
    /// `amount` LP token amount the user has provided.
    /// `rewardDebt` The amount of NR entitled to the user.
    struct UserInfo {
        uint256 amount;
        uint256 rewardDebt;
    }

    /// @notice Info of each MCV2 pool.
    /// `allocPoint` The amount of allocation points assigned to the pool.
    /// Also known as the amount of NR to distribute per block.
    struct PoolInfo {
        uint256 accNRPerShare;
        uint256 lastRewardBlock;
        uint256 allocPoint;
        address[] airdropToken;
        uint256[] airdropTokenPerShare;
        uint256[] airdropTokenEndBlock;
        uint256[] airdropTokenTotalAmount;
        uint256[] airdropTokenToTalReciveAmount;
        uint256[] airdropTokenPerBlock;
    }

    address public dev;
    address public factory;
    /// @notice Address of MCV1 contract.
    IMasterChef2 public immutable MASTER_CHEF;
    /// @notice Address of NR contract.
    IERC20 public immutable NR;
    /// @notice The index of MCV2 master pool in MCV1.
    uint256 public immutable MASTER_PID;
    // @notice The migrator contract. It has a lot of power. Can only be set through governance (owner).
    IMigratorChef public migrator;
    bool public airdropFee;
    uint256 public airdropFeePercent;
    /// @notice Info of each MCV2 pool.
    PoolInfo[] public poolInfo;
    /// @notice Address of the LP token for each MCV2 pool.
    IERC20[] public lpToken;
    /// @notice Address of each `IRewarder` contract in MCV2.
    IRewarder2[] public rewarder;

    /// @notice Info of each user that stakes LP tokens.
    mapping (uint256 => mapping (address => UserInfo)) public userInfo;

    // pid => airdropToken => userAddress => UserInfo
    mapping (uint256 => mapping (address => mapping (address => UserInfo))) public userAirdropTokenInfo;

    // //airdropTokenStartBlock[pid][aidropTokenAddress] = startBlock;
    // mapping (uint256 => mapping ( address => uint256 )) public airdropTokenStartBlock;
    mapping (address => uint256) public lpTokenId;
    /// @dev Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint;

    uint256 private constant ACC_NR_PRECISION = 1e12;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event Harvest(address indexed user, uint256 indexed pid, uint256 amount);
    event LogPoolAddition(uint256 indexed pid, uint256 allocPoint, IERC20 indexed lpToken);
    event LogSetPool(uint256 indexed pid, uint256 allocPoint, bool overwrite);
    event LogUpdatePool(uint256 indexed pid, uint256 lastRewardBlock, uint256 lpSupply, uint256 accNRPerShare);
    event LogInit();

    /// @param _MASTER_CHEF The neuronSwap MCV1 contract address.
    /// @param _nr The NR token contract address.
    /// @param _MASTER_PID The pool ID of the dummy token on the base MCV1 contract.
    constructor(IMasterChef2 _MASTER_CHEF, IERC20 _nr, uint256 _MASTER_PID, address _factory) public {
        MASTER_CHEF = _MASTER_CHEF;
        NR = _nr;
        MASTER_PID = _MASTER_PID;
        factory = _factory;
        dev = msg.sender;
    }

    /// @notice Deposits a dummy token to `MASTER_CHEF` MCV1. This is required because MCV1 holds the minting rights for NR.
    /// Any balance of transaction sender in `dummyToken` is transferred.
    /// The allocation point for the pool on MCV1 is the total allocation point for all pools that receive double incentives.
    /// @param dummyToken The address of the ERC-20 token to deposit into MCV1.
    function init(IERC20 dummyToken) external {
        uint256 balance = dummyToken.balanceOf(msg.sender) / 1e18;
        require(balance != 0, "MasterChefV2: Balance must exceed 0");
        dummyToken.safeTransferFrom(msg.sender, address(this), balance);
        dummyToken.approve(address(MASTER_CHEF), balance);
        MASTER_CHEF.deposit(MASTER_PID, balance);
        emit LogInit();
    }

    /// @notice Returns the number of MCV2 pools.
    function poolLength() public view returns (uint256 pools) {
        pools = poolInfo.length;
    }

    /// @notice Add a new LP to the pool. Can only be called by the owner.
    /// DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    /// @param _lpToken Address of the LP ERC-20 token.
    function add(IERC20 _lpToken, address[] memory _airdropToken,  uint256[] memory _airdropTokenEndBlock, uint256[] memory _airdropTokenTotalAmount) public {
        require(msg.sender == IUniswapV2Factory(factory).getPairOwner(address(_lpToken)) || msg.sender == owner);
        uint256 lastRewardBlock = block.number;
        lpToken.push(_lpToken);
        lpTokenId[address(_lpToken)] = lpToken.length - 1;
        uint256[] memory airdropTokenEndBlock = new uint256[](_airdropToken.length);
        uint256[] memory airdropTokenPerBlock = new uint256[](_airdropToken.length);
        uint256[] memory zero = new uint256[](_airdropToken.length);
        for(uint256 i; i < _airdropTokenEndBlock.length; i++) {
            uint256 blocks = block.number + _airdropTokenEndBlock[i];
            airdropTokenEndBlock[i] = blocks;
            airdropTokenPerBlock[i] = _airdropTokenTotalAmount[i] / _airdropTokenEndBlock[i];
            console.log(airdropTokenEndBlock[i],"tokenEndBlock");
            console.log(_airdropTokenTotalAmount[i],"airdropTOkenTotalAmount");
            console.log(airdropTokenPerBlock[i],"tokenPerBlock");
            zero[i] = 0;
            IERC20(_airdropToken[i]).safeTransferFrom(msg.sender, address(this), _airdropTokenTotalAmount[i]);
        }
        poolInfo.push(PoolInfo(
            0,
            lastRewardBlock,
            0,
            _airdropToken,
            zero,
            airdropTokenEndBlock,
            _airdropTokenTotalAmount,
            zero,
            airdropTokenPerBlock
        ));
        if(airdropFee) {
            for(uint256 j; j < _airdropToken.length; j++) {
                uint256 airdropTokenFeeAmount = _airdropTokenTotalAmount[j].mul(airdropFeePercent) / 1000;
                IERC20(_airdropToken[j]).safeTransferFrom(msg.sender, dev, airdropTokenFeeAmount);
            }
        }

    }

    function set(address lpToken, uint256 _allocPoint, bool overwrite, address[] memory _airdropToken, uint256[] memory _airdropTokenEndBlock,uint256[] memory _airdropTokenTotalAmount) public onlyOwner {
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
        totalAllocPoint = totalAllocPoint.sub(poolInfo[pid].allocPoint).add(_allocPoint);
        poolInfo[pid].allocPoint = _allocPoint;
        require(_airdropToken.length ==  _airdropTokenEndBlock.length && _airdropToken.length == _airdropTokenTotalAmount.length);
             for(uint256 i; i < _airdropToken.length ; i++) {
                poolInfo[pid].airdropToken.push(_airdropToken[i]);
                poolInfo[pid].airdropTokenPerShare.push(0);
                poolInfo[pid].airdropTokenEndBlock.push(_airdropTokenEndBlock[i]);
                poolInfo[pid].airdropTokenTotalAmount.push(_airdropTokenTotalAmount[i]);
                poolInfo[pid].airdropTokenToTalReciveAmount.push(0);
                poolInfo[pid].airdropTokenPerBlock.push(_airdropTokenTotalAmount[i] / _airdropTokenEndBlock[i]);

                if(airdropFee) {
                uint256 airdropTokenFeeAmount = _airdropTokenTotalAmount[i].mul(airdropFeePercent) / 1000;
                IERC20(_airdropToken[i]).safeTransferFrom(msg.sender, dev, airdropTokenFeeAmount);
                }
            }
    }

    function setAllocPoint(address lpToken, uint256 _allocPoint) public onlyOwner {
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
        totalAllocPoint = totalAllocPoint.sub(poolInfo[pid].allocPoint).add(_allocPoint);
        poolInfo[pid].allocPoint = _allocPoint;
    }

    function airdropTokenAdd(address lpToken, address[] memory _airdropToken, uint256[] memory _airdropTokenEndBlock, uint256[] memory _airdropTokenTotalAmount) public {
        require(msg.sender == IUniswapV2Factory(factory).getPairOwner(address(lpToken)) || msg.sender == owner);
        require(_airdropToken.length == _airdropTokenEndBlock.length && _airdropToken.length == _airdropTokenTotalAmount.length);
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
            for(uint256 i; i < _airdropToken.length ; i++) {
                poolInfo[pid].airdropToken.push(_airdropToken[i]);
                poolInfo[pid].airdropTokenPerShare.push(0);
                poolInfo[pid].airdropTokenEndBlock.push(_airdropTokenEndBlock[i].add(block.number));
                console.log(poolInfo[pid].airdropTokenEndBlock[poolInfo[pid].airdropTokenEndBlock.length -1],"tokenEndBlock");
                poolInfo[pid].airdropTokenTotalAmount.push(_airdropTokenTotalAmount[i]);
                poolInfo[pid].airdropTokenToTalReciveAmount.push(0);
                poolInfo[pid].airdropTokenPerBlock.push(_airdropTokenTotalAmount[i] / _airdropTokenEndBlock[i]);

                if(airdropFee) {
                uint256 airdropTokenFeeAmount = _airdropTokenTotalAmount[i].mul(airdropFeePercent) / 1000;
                IERC20(_airdropToken[i]).safeTransferFrom(msg.sender, dev, airdropTokenFeeAmount);
                }
                IERC20(_airdropToken[i]).safeTransferFrom(msg.sender, address(this), _airdropTokenTotalAmount[i]);
            }
    }
    //에어드랍토큰이 중복될수있기때문에 airdropTokenId로 변경할수있게 수정 필요.
    function setAirdropTokenPerBlock(address lpToken, address _airdropToken, uint256 _airdropTokenId, uint256 _airdropTokenEndBlock) public {
        require(msg.sender == IUniswapV2Factory(factory).getPairOwner(address(lpToken)) || msg.sender == owner);
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
            if (poolInfo[pid].airdropToken[_airdropTokenId] == _airdropToken) {
                poolInfo[pid].airdropTokenEndBlock[_airdropTokenId] = _airdropTokenEndBlock;
                poolInfo[pid].airdropTokenPerBlock[_airdropTokenId] = poolInfo[pid].airdropTokenTotalAmount[_airdropTokenId] / _airdropTokenEndBlock; 
        }
    }

    function setAirdropToken(address lpToken, uint256 airdropTokenId, address _airdropToken, uint256 _airdropTokenTotalAmount, uint256 _airdropTokenEndBlock, uint256 _airdropTokenPerBlock) public {
        require(msg.sender == IUniswapV2Factory(factory).getPairOwner(address(lpToken)) || msg.sender == owner);
        uint256 pid = lpTokenId[lpToken];
        updatePool(pid);
        poolInfo[pid].airdropToken[airdropTokenId] = _airdropToken;
        poolInfo[pid].airdropTokenTotalAmount[airdropTokenId] = _airdropTokenTotalAmount;
        poolInfo[pid].airdropTokenEndBlock[airdropTokenId] = _airdropTokenEndBlock;
        poolInfo[pid].airdropTokenPerBlock[airdropTokenId] = _airdropTokenPerBlock;
    }

    function getLpTokenAirdropTokenInfo(address lpToken, address _airdropToken) public 
    returns (uint256[] memory airdropTokenId, uint256[] memory airdropTokenTotalAmount, uint256[] memory airdropTokenTotalReciveAmount, uint256[] memory airdropTokenEndBlock, uint256[] memory airdropTokenPerBlock)
    {
        uint256 pid = lpTokenId[lpToken];     
        uint256 num=0;
            for(uint256 i; i < poolInfo[pid].airdropToken.length; i++) {
                if(poolInfo[pid].airdropToken[i] == _airdropToken) {
                    airdropTokenId[num] = i;
                    num++;
                }
            }

        for(uint256 j; j < airdropTokenId.length; j++) {
            airdropTokenTotalAmount[j] = poolInfo[pid].airdropTokenTotalAmount[airdropTokenId[j]];
            airdropTokenTotalReciveAmount[j] = poolInfo[pid].airdropTokenToTalReciveAmount[airdropTokenId[j]];
            airdropTokenEndBlock[j] = poolInfo[pid].airdropTokenEndBlock[airdropTokenId[j]];
            airdropTokenPerBlock[j] = poolInfo[pid].airdropTokenPerBlock[airdropTokenId[j]];
        }

    }

    function getLpTokenAirdropTokenInfo(address lpToken, uint256 _airdropTokenId) public 
    returns (address airdropToken, uint256 airdropTokenTotalAmount, uint256 airdropTokenTotalReciveAmount, uint256 airdropTokenEndBlock, uint256 airdropTokenPerBlock)
    {
        uint256 pid = lpTokenId[lpToken];
        airdropToken = poolInfo[pid].airdropToken[_airdropTokenId];
        airdropTokenTotalAmount = poolInfo[pid].airdropTokenTotalAmount[_airdropTokenId];
        airdropTokenTotalReciveAmount = poolInfo[pid].airdropTokenToTalReciveAmount[_airdropTokenId];
        airdropTokenEndBlock = poolInfo[pid].airdropTokenEndBlock[_airdropTokenId];
        airdropTokenPerBlock = poolInfo[pid].airdropTokenPerBlock[_airdropTokenId];
    }

    /// @notice Set the `migrator` contract. Can only be called by the owner.
    /// @param _migrator The contract address to set.
    function setMigrator(IMigratorChef _migrator) public onlyOwner {
        migrator = _migrator;
    }

    /// @notice Migrate LP token to another LP contract through the `migrator` contract.
    /// @param _pid The index of the pool. See `poolInfo`.
    function migrate(uint256 _pid) public {
        require(address(migrator) != address(0), "MasterChefV2: no migrator set");
        IERC20 _lpToken = lpToken[_pid];
        uint256 bal = _lpToken.balanceOf(address(this));
        _lpToken.approve(address(migrator), bal);
        IERC20 newLpToken = migrator.migrate(_lpToken);
        require(bal == newLpToken.balanceOf(address(this)), "MasterChefV2: migrated balance must match");
        lpToken[_pid] = newLpToken;
    }

    /// @notice View function to see pending NR on frontend.
    /// @param _pid The index of the pool. See `poolInfo`.
    /// @param _user Address of user.
    /// @return pending NR reward for a given user.
    function pendingNR(uint256 _pid, address _user) external view returns (uint256 pending) {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accNRPerShare = pool.accNRPerShare;
        uint256 lpSupply = lpToken[_pid].balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 blocks = block.number.sub(pool.lastRewardBlock);
            uint256 nrReward = blocks.mul(nrPerBlock()).mul(pool.allocPoint) / totalAllocPoint;
            accNRPerShare = accNRPerShare.add(nrReward.mul(ACC_NR_PRECISION) / lpSupply);
        }
        pending = user.amount.mul(accNRPerShare) / ACC_NR_PRECISION.sub(user.rewardDebt);
    }

    function pidUserInfoAmount(uint256 _pid, address _user) external view returns (uint256 amount) {
        amount = userInfo[_pid][_user].amount;
    }

    function pidUserInfoRewardDebt(uint256 _pid, address _user) external view returns (uint256 rewardDebt) {
        rewardDebt= userInfo[_pid][_user].rewardDebt;
    }

    function pendingToken(uint256 _pid, address _user) external view returns (uint256[] memory pending) {
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        // uint256[] memory accTokenPerShare = pool.airdropTokenPerShare[i];
        uint256 lpSupply = lpToken[_pid].balanceOf(address(this));
        uint256 i;
            for(uint256 i; i < pool.airdropToken.length; i++) {
                UserInfo storage userAirdropInfo = userAirdropTokenInfo[_pid][pool.airdropToken[i]][_user];
                if(block.number > pool.lastRewardBlock && lpSupply != 0) {
                uint256 blocks;
                /* block.number 가 airdropTokenEndBlock 이 지났을경우에 lastRewardBlock이랑 비교하여 0으로 만들어줘야함 */
                if(block.number > pool.airdropTokenEndBlock[i]) {
                    if(pool.lastRewardBlock < pool.airdropTokenEndBlock[i]) {
                        blocks = pool.airdropTokenEndBlock[i].sub(pool.lastRewardBlock);
                    } else blocks = 0;
                } else {
                    blocks = block.number.sub(pool.lastRewardBlock);
                }


                pool.airdropTokenPerShare[i] = pool.airdropTokenPerShare[i].add(blocks.mul(pool.airdropTokenPerBlock[i]).mul(ACC_NR_PRECISION) / lpSupply);
                console.log(pool.airdropTokenPerShare[i],"airdropTokenPerShare");
            }
                pending[i] = user.amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION.sub(userAirdropInfo.rewardDebt);
        }
    }


    /// @notice Update reward variables for all pools. Be careful of gas spending!
    /// @param pids Pool IDs of all to be updated. Make sure to update all active pools.
    function massUpdatePools(uint256[] calldata pids) external {
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
    }

    /// @notice Calculates and returns the `amount` of NR per block.
    function nrPerBlock() public view returns (uint256 amount) {
        amount = MASTER_CHEF.getMultiplier(MASTER_CHEF.poolInfo(MASTER_PID).lastRewardBlock)
            .mul(MASTER_CHEF.poolInfo(MASTER_PID).allocPoint) / MASTER_CHEF.totalAllocPoint();
            console.log(amount);
    }

    /// @notice Update reward variables of the given pool.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @return pool Returns the pool that was updated.
    function updatePool(uint256 pid) public returns (PoolInfo memory pool) {
        pool = poolInfo[pid];
        if (block.number > pool.lastRewardBlock) {
            uint256 lpSupply = lpToken[pid].balanceOf(address(this));
            if (lpSupply > 0) {
                uint256 blocks = block.number.sub(pool.lastRewardBlock);
                uint256 nrReward;
                if(totalAllocPoint > 0) {
                    nrReward = blocks.mul(nrPerBlock()).mul(pool.allocPoint) / totalAllocPoint;
                }
                pool.accNRPerShare = pool.accNRPerShare.add((nrReward.mul(ACC_NR_PRECISION) / lpSupply));
                uint256 airdropTokenblock;
                for(uint256 i; i < pool.airdropToken.length; i++) {
                    if(block.number > pool.airdropTokenEndBlock[i]) {
                    if(pool.lastRewardBlock < pool.airdropTokenEndBlock[i]) {
                        airdropTokenblock = pool.airdropTokenEndBlock[i].sub(pool.lastRewardBlock);
                    } else airdropTokenblock = 0;
                } else {
                    airdropTokenblock = block.number.sub(pool.lastRewardBlock);
                }
                    pool.airdropTokenPerShare[i] = pool.airdropTokenPerShare[i].add(airdropTokenblock.mul(pool.airdropTokenPerBlock[i]).mul(ACC_NR_PRECISION) / lpSupply);
                    console.log(pool.airdropTokenPerShare[i],"tokenPerSHare");
                    console.log(pool.airdropTokenPerBlock[i],"tokenPerSHare");
                    console.log(lpSupply,"lpSupply");
                }
            }
            pool.lastRewardBlock = block.number;
            poolInfo[pid] = pool;
            emit LogUpdatePool(pid, pool.lastRewardBlock, lpSupply, pool.accNRPerShare);
        }
    }

    /// @notice Deposit LP tokens to MCV2 for NR allocation.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to deposit.
    /// @param to The receiver of `amount` deposit benefit.
    function deposit(uint256 pid, uint256 amount, address to) public {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];

        // Effects
        user.amount = user.amount.add(amount);
        user.rewardDebt = user.rewardDebt.add(amount.mul(pool.accNRPerShare) / ACC_NR_PRECISION);
        for(uint256 i; i < pool.airdropToken.length; i++) {
            UserInfo storage airdropTokenUser = userAirdropTokenInfo[pid][pool.airdropToken[i]][to];
            airdropTokenUser.amount = user.amount;
            console.log(user.amount,"useramount");
            airdropTokenUser.rewardDebt = airdropTokenUser.rewardDebt.add(amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION);
        }

        lpToken[pid].safeTransferFrom(msg.sender, address(this), amount);

        emit Deposit(to, pid, amount, to);
    }

    /// @notice Withdraw LP tokens from MCV2.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to withdraw.
    /// @param to Receiver of the LP tokens.
    function withdraw(uint256 pid, uint256 amount, address to) public {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];


        // Effects
        user.rewardDebt = user.rewardDebt.sub(amount.mul(pool.accNRPerShare) / ACC_NR_PRECISION);
        user.amount = user.amount.sub(amount);

        for(uint256 i; i < pool.airdropToken.length; i++) {
            UserInfo storage airdropTokenUser = userAirdropTokenInfo[pid][pool.airdropToken[i]][to];
            airdropTokenUser.rewardDebt = airdropTokenUser.rewardDebt.sub(amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION);
            airdropTokenUser.amount = user.amount;
        }

        lpToken[pid].safeTransfer(to, amount);


        emit Withdraw(to, pid, amount, to);
    }

    /// @notice Harvest proceeds for transaction sender to `to`.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param to Receiver of NR rewards.
    function harvest(uint256 pid, address to) public {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];
        uint256 accumulatedNR = user.amount.mul(pool.accNRPerShare) / ACC_NR_PRECISION;
        uint256 _pendingNR = accumulatedNR.sub(user.rewardDebt);
        
        user.rewardDebt = accumulatedNR;
        if(_pendingNR > NR.balanceOf(address(this)) ) {
            harvestFromMasterChef();
        }

        // Interactions
        if (_pendingNR != 0) {
            NR.safeTransfer(to, _pendingNR);
        }
        for(uint256 i; i< pool.airdropToken.length; i++) {
            UserInfo storage airdropTokenUser = userAirdropTokenInfo[pid][pool.airdropToken[i]][msg.sender];
            uint256 accmulateToken = user.amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION;
            uint256 pendingToken =  accmulateToken.sub(airdropTokenUser.rewardDebt);
            
            if(pendingToken.add(pool.airdropTokenToTalReciveAmount[i]) > pool.airdropTokenTotalAmount[i]) {
                pendingToken = pool.airdropTokenTotalAmount[i].sub(pool.airdropTokenToTalReciveAmount[i]);
                accmulateToken = pendingToken.add(airdropTokenUser.rewardDebt);
            }

            airdropTokenUser.rewardDebt = accmulateToken;
            if(pendingToken != 0 ) {
                IERC20(pool.airdropToken[i]).safeTransfer(to, pendingToken);
                pool.airdropTokenToTalReciveAmount[i] = pool.airdropTokenToTalReciveAmount[i].add(pendingToken);
            }

        }


        // Effects

        

        emit Harvest(msg.sender, pid, _pendingNR);
    }
    
    /// @notice Withdraw LP tokens from MCV2 and harvest proceeds for transaction sender to `to`.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param amount LP token amount to withdraw.
    /// @param to Receiver of the LP tokens and NR rewards.
    function withdrawAndHarvest(uint256 pid, uint256 amount, address to) public {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][msg.sender];
        uint256 accumulatedNR = user.amount.mul(pool.accNRPerShare) / ACC_NR_PRECISION;
        uint256 _pendingNR = accumulatedNR.sub(user.rewardDebt);

        // Effects
        user.rewardDebt = accumulatedNR.sub(amount.mul(pool.accNRPerShare) / ACC_NR_PRECISION);
        user.amount = user.amount.sub(amount);

        if(_pendingNR > NR.balanceOf(address(this)) ) {
            harvestFromMasterChef();
        }
        
        // Interactions
        if(_pendingNR != 0 ) {
            NR.safeTransfer(to, _pendingNR);
        }

        for(uint256 i; i< pool.airdropToken.length; i++) {
            UserInfo storage airdropTokenUser = userAirdropTokenInfo[pid][pool.airdropToken[i]][msg.sender];
            uint256 accmulateToken = user.amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION;
            uint256 pendingToken =  accmulateToken.sub(airdropTokenUser.rewardDebt);

            if(pendingToken.add(pool.airdropTokenToTalReciveAmount[i]) > pool.airdropTokenTotalAmount[i]) {
                pendingToken = pool.airdropTokenTotalAmount[i].sub(pool.airdropTokenToTalReciveAmount[i]);
                accmulateToken = pendingToken.add(airdropTokenUser.rewardDebt);
            }

            airdropTokenUser.rewardDebt = accmulateToken;
            if(pendingToken != 0 ) {
                IERC20(pool.airdropToken[i]).safeTransfer(to, pendingToken);
                pool.airdropTokenToTalReciveAmount[i] = pool.airdropTokenToTalReciveAmount[i].add(pendingToken);
            }

            

            airdropTokenUser.rewardDebt = airdropTokenUser.rewardDebt.sub(amount.mul(pool.airdropTokenPerShare[i]) / ACC_NR_PRECISION);
            airdropTokenUser.amount = user.amount;
        }

        lpToken[pid].safeTransfer(to, amount);

        emit Withdraw(msg.sender, pid, amount, to);
        emit Harvest(msg.sender, pid, _pendingNR);
    }

    /// @notice Harvests NR from `MASTER_CHEF` MCV1 and pool `MASTER_PID` to this MCV2 contract.
    function harvestFromMasterChef() public {
        MASTER_CHEF.deposit(MASTER_PID, 0);
    }

    /// @notice Withdraw without caring about rewards. EMERGENCY ONLY.
    /// @param pid The index of the pool. See `poolInfo`.
    /// @param to Receiver of the LP tokens.
    function emergencyWithdraw(uint256 pid, address to) public {
        UserInfo storage user = userInfo[pid][msg.sender];
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;

        for(uint256 i; i< poolInfo[pid].airdropToken.length; i++) {
            UserInfo storage airdropTokenUser = userAirdropTokenInfo[pid][poolInfo[pid].airdropToken[i]][to];
            airdropTokenUser.amount = 0;
            airdropTokenUser.rewardDebt = 0;
        }
        // Note: transfer can fail or succeed if `amount` is zero.
        emit EmergencyWithdraw(msg.sender, pid, amount, to);
    }

    function setFactory(address _factory) public onlyOwner() {
        factory = _factory;
    }

    function setAirdropFee(bool _res) public onlyOwner() {
        require( airdropFeePercent !=0, "airdropFeePercent is 0");
        airdropFee = _res;
    }

    // % * 10
    function setAirdropFeePercent(uint256 _percent) public onlyOwner() {
        airdropFeePercent = _percent;
    }
}

