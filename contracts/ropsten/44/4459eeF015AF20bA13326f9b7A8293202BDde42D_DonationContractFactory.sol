// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "hardhat/console.sol";

contract DonationContract {

  /*---------------------------- EVENTS -----------------------------*/
  event DonationCreated(uint256 donationId, address payable from, address payable[] recipients, uint256 totalAmount);
  event DonationReleased(uint256 donationId, address payable from, address payable to, uint256 totalAmount);
  event DonationRefunded(uint256 donationId, address payable donor, uint256 totalAmount);


  /*--------------------------- CONSTANTS ---------------------------*/
  uint256 constant public MAX_OWNER_COUNT = 3;

  /*---------------------------- STORAGE ----------------------------*/
  struct DonationRecipient {
    address payable recipientAddress;
    uint256 amount;
    bool isClaimed;
    bool isRecipient;
  }

  struct Donation {
      uint256 donationId;
      address payable donorAddress;
      address payable[] recipientAddresses;
      mapping (address => DonationRecipient) recipientMapping;

      uint256 donationTotal;
      uint256 numberOfRecipients;
      uint256 amountClaimed;
      uint256 amountRefundable;
      uint256 amountRefunded;
      uint256 amountOverflowReturned;

      bool isActive;
      bool isRefunded;
      bool isDonation;
  }
  
  mapping (address => bool) public isOwner;
  address[] public owners;

  mapping(uint256 => Donation) public allDonations;
  Donation[] public allDonationsHistory;
  Donation[] public allActiveDonations;
  Donation[] public allInactiveDonations;
  Donation[] public allRefundedDonations;

  constructor(address[] memory _owners) validOwners(_owners.length) {
    for (uint i = 0; i < _owners.length; i++) {
      require(!isOwner[_owners[i]]);
      isOwner[_owners[i]] = true;
    }
    
    owners = _owners;
  }

  /*---------------------------- CREATING ----------------------------*/
  function createDonation(uint256 _donationId, address payable[] calldata _recipientAddresses) external payable isDonationCreatable(_donationId, _recipientAddresses) {

    uint sharePerRecipient = msg.value / _recipientAddresses.length;
    uint amountOverflowReturned = msg.value % _recipientAddresses.length;

    // Create newDonation object to persist donation in storage
    Donation storage newDonation = allDonations[_donationId];
    newDonation.donationId = _donationId;
    newDonation.donorAddress = payable(msg.sender);
    newDonation.donationTotal = msg.value - amountOverflowReturned;
    newDonation.amountClaimed = 0;
    newDonation.amountRefundable = msg.value - amountOverflowReturned;
    newDonation.amountRefunded = 0;
    newDonation.amountOverflowReturned = amountOverflowReturned;
    newDonation.recipientAddresses = _recipientAddresses;
    newDonation.numberOfRecipients = _recipientAddresses.length;
    newDonation.isActive = true;
    newDonation.isRefunded = false;
    newDonation.isDonation = true;

    // Creates DonationRecipient object to persist in storage for each recipient
    for (uint i = 0; i < _recipientAddresses.length; i++) {
      DonationRecipient memory newDonationRecipient = DonationRecipient({
        recipientAddress: _recipientAddresses[i],
        amount: sharePerRecipient,
        isClaimed: false,
        isRecipient: true
      });

      require(newDonation.recipientMapping[_recipientAddresses[i]].isRecipient != true, "Cannot name recipient multiple times for one donation.");
      newDonation.recipientMapping[_recipientAddresses[i]] = newDonationRecipient;
    }

    // Return the remainder back to the donor
    payable(newDonation.donorAddress).transfer(amountOverflowReturned);

    emit DonationCreated(_donationId, payable(msg.sender), _recipientAddresses, newDonation.donationTotal);
  }


  /*---------------------------- CLAIMING ----------------------------*/
  function claimDonation(uint256 _donationId) external isDonationClaimable(_donationId) {
    DonationRecipient memory donationRecipient = getRecipientDetails(_donationId, msg.sender);
    
    // Transfer donation
    donationRecipient.recipientAddress.transfer(donationRecipient.amount);

    // Change stored data to reflect the change
    allDonations[_donationId].amountClaimed = allDonations[_donationId].amountClaimed + donationRecipient.amount;
    allDonations[_donationId].amountRefundable = allDonations[_donationId].amountRefundable - donationRecipient.amount;
    allDonations[_donationId].recipientMapping[msg.sender].isClaimed = true;

    emit DonationReleased(_donationId, allDonations[_donationId].donorAddress, payable(msg.sender), donationRecipient.amount);
  }


  /*---------------------------- REFUNDING ----------------------------*/
  function refundDonation(uint256 _donationId) external isDonationRefundable(_donationId) {
  
    // Transfer refund
    allDonations[_donationId].donorAddress.transfer(allDonations[_donationId].amountRefundable);
  
    // Change stored data to reflect the change
    allDonations[_donationId].amountRefunded = allDonations[_donationId].amountRefundable;
    allDonations[_donationId].amountRefundable = 0;
    allDonations[_donationId].isRefunded = true;
    allDonations[_donationId].isActive = false;

    emit DonationRefunded(_donationId, payable(msg.sender), allDonations[_donationId].amountRefunded);
  }


  /*---------------------------- QUERYING ----------------------------*/
  function balanceOfContract() external view returns(uint256) {
       return address(this).balance;
  }

  function getRecipientDetails(uint256 _donationId, address _recipientAddress) public view returns(DonationRecipient memory) {
    DonationRecipient memory donationRecipient = allDonations[_donationId].recipientMapping[_recipientAddress];
    require(donationRecipient.isRecipient == true, "Given recipientAddress is not a listed recipient for this donationId.");
    return donationRecipient;
  }


  /*--------------------------- VALIDATION ----------------------------*/
  modifier isDonationCreatable (uint256 _donationId, address payable[] memory _recipientAddresses) {
    require(allDonations[_donationId].isDonation != true, "A donation with this id already exists.");
    require(msg.value / _recipientAddresses.length > 0, "Amount is not sufficient enough to credit each recipient.");
    _;
  }

  modifier isDonationRefundable (uint256 _donationId) {
    require(allDonations[_donationId].isActive, "Donation is not active");
    require(allDonations[_donationId].donorAddress == msg.sender, "You are not the authorized donor for this donation.");
    _;
  }

  modifier isDonationClaimable (uint256 _donationId) {
    DonationRecipient memory donationRecipient = getRecipientDetails(_donationId, msg.sender);

    require(allDonations[_donationId].isActive, "You cannot claim an inactive donation.");
    require(donationRecipient.isRecipient == true, "You are not an authorized recipient for this donation.");
    require(donationRecipient.isClaimed == false, "You have already claimed this donation.");
    require(allDonations[_donationId].isRefunded == false, "You cannot claim a refunded donation.");
    _;
  }

  modifier isDonationActive (uint256 _donationId) {
    require(allDonations[_donationId].isActive, "Donation is not active");
    _;
  }

  modifier validOwners(uint256 ownerCount) {
    require(ownerCount <= MAX_OWNER_COUNT
      && ownerCount != 0);
    _;
  }


  /*----------------------------- UTILITY ----------------------------*/
}

