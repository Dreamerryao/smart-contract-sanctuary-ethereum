pragma solidity 0.8.6;

import "./interfaces/IArtistMiningCalculator.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

/**
 * @title ArtistMiningCalculatorV1
 * @author Nil DAO
 */
contract ArtistMiningCalculatorV1 is IArtistMiningCalculator {
    function calculateArtistMining(uint256 mintNumber, uint128 proceedsInWei)
        external
        pure
        override
        returns (uint256 artistMining)
    {
        if (mintNumber < 10) {
            artistMining = Math.min((proceedsInWei * 3000) / 1 ether, 300000);
        } else if (mintNumber < 100) {
            artistMining = Math.min((proceedsInWei * 1000) / 1 ether, 100000);
        } else if (mintNumber < 500) {
            artistMining = Math.min((proceedsInWei * 500) / 1 ether, 50000);
        } else {
            artistMining = Math.min((proceedsInWei * 300) / 1 ether, 30000);
        }
    }
}

