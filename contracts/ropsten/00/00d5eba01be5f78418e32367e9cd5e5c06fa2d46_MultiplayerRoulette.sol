import "hardhat/console.sol";
import "./interfaces/RandomNumGenerator.sol";
import "./interfaces/IERC721V2.sol";

contract MultiplayerRoulette {

    address public admin;

    uint8 MAX_BET_NUM = 10;   // TODO: remove this later

    uint32 public competitionsCount;   // hold ids of competitions

    mapping(uint32 => Tournament) public competitionInfo;   // competition details

    mapping(uint32 => mapping(address => Player)) playerRecord; // player record for a specific competition

    uint256 betId = 0;      // track the request sent to Chainlink VRF
    RandomNumGenerator rndNumCtx;       // common rnd num generator contract
    mapping(uint256 => Bet) book;    // track bet asynchronously (for chainlink VRF)

    IERC721CollectionV2 wearableContract;   

    event BetPlaced(uint256, uint32, address, uint8, uint256, uint32);
    event BetResult(uint256, address, string, uint32, uint8);
    event CompetitionEnded(uint32);
    event WearableMinted(string, uint256);

    struct Bet {
        uint32 compId;
        address playerAddr;
        uint8 betNum;
        uint256 amount;
        uint32 userSeed;
    }

    struct Player {
        uint32 compId;
        bool registered;
        uint256 balance;
        uint32 winCount;
    }

    struct Tournament {
        address[] players;
        uint32 startoutBalance;
        uint256 regDuration;    // sec
        uint256 compDuration;
        uint256 regStartTime;
        uint256 compStartTime;
        address[3] winners;  // 1, 2, 3 - in that order
    }



    constructor(address _rndNumGenCtx, address _wearableContract) {
        admin = msg.sender;
        competitionsCount = 0;

        rndNumCtx = RandomNumGenerator(_rndNumGenCtx);
        wearableContract = IERC721CollectionV2(_wearableContract);
    }



    /**
    * Add a new competition
    */
    function addCompetition(uint32 _startingBal, uint32 _regDuration, 
        uint32 _compDuration) public onlyAdmin 
    {    

        require(
            _startingBal != 0 && _regDuration != 0 && _compDuration != 0, 
            "startingBal, regDuration, compDuration cannot be zero"
        );

        Tournament memory _comp = Tournament(
            new address[](0), 
            _startingBal, 
            _regDuration, 
            _compDuration, 
            0, 
            0,
            [address(0), address(0), address(0)]
        );

        _comp.startoutBalance = _startingBal;
        _comp.regDuration = _regDuration;
        _comp.compDuration = _compDuration;

        competitionInfo[competitionsCount++] = _comp;
    }



    /**
    * Enable players registration from current time, for regDuration sec
    */
    function enableRegistration(uint32 _compId) public onlyAdmin compExists(_compId) {

        Tournament storage _t = competitionInfo[_compId];

        require(_t.regStartTime == 0, "Registration is already enabled");
        require(_t.compStartTime == 0, "Competition has already started");

        _t.regStartTime = block.timestamp;
    }



    /**
    * Extend registration duration while the registration is ongoing OR
    * the registration period is ended but number of players is insufficient to compute winners
    */
    function extendRegistration(uint32 _compId, uint256 newEndTime) public onlyAdmin compExists(_compId) {
        
        Tournament storage _t = competitionInfo[_compId];

        bool registrationOngoing = isRegOngoing(_compId);

        require(
            registrationOngoing || (_t.regStartTime > 0 && !registrationOngoing && _t.players.length < 3 && !isCompOngoing(_compId) ), 
            "Registration is not ongoing or competition already started"
        );

        require(newEndTime > block.timestamp, "New registration end date is in the past");

        _t.regDuration = newEndTime - _t.regStartTime;
    }



    /**
    * Register player. Note: Players should register themselves
    */
    function registerPlayer(uint32 _compId) public compExists(_compId) {
        
        address newPlayer = msg.sender;

        Tournament storage _t = competitionInfo[_compId];

        uint256 _currentTime = block.timestamp;
        uint256 _regEndTime = _t.regStartTime + _t.regDuration;

        require(_t.regStartTime != 0, "Registration is not enabled");
        require( _currentTime < _regEndTime, "Registration period ended");
        require(playerRecord[_compId][newPlayer].registered == false, "Player already registered");

        _t.players.push(newPlayer);
        playerRecord[_compId][newPlayer].registered = true;
        playerRecord[_compId][newPlayer].balance = _t.startoutBalance;
        playerRecord[_compId][newPlayer].compId = _compId;
    }



    /**
    * Start a roulette competition by admin.
    */
    function startCompetition(uint32 _compId) public onlyAdmin compExists(_compId) {
        
        Tournament storage _t = competitionInfo[_compId];

        // TODO: should this start even if player count < 3??

        // ensure that registration period has ended
        require(
            _t.regStartTime > 0 && !isRegOngoing(_compId), 
            "Registration should be completed first"
        );

        require(_t.compStartTime == 0, "Competition already started");

        _t.compStartTime = block.timestamp;        
    }



    /**
    * Extend competition duration while the competition is ongoing
    */
    function extendCompetition(uint32 _compId, uint256 newEndTime) public onlyAdmin compExists(_compId) {

        require(isCompOngoing(_compId), "Competition is not ongoing");

        Tournament storage _t = competitionInfo[_compId];
        require(newEndTime > block.timestamp, "New competition end date is in the past");

        _t.compDuration = newEndTime - _t.compStartTime;
    }



    /**
    * Place bet on a specific competition, on specific number
    */
    function placeBet(uint32 _compId, uint32 _userSeed, uint8 _betNum, uint256 betAmt) public compExists(_compId) {

        address playerAddr = msg.sender;

        Player storage _playerDetails = playerRecord[_compId][playerAddr];

        // Ensure competition started and hasn't ended
        require(isCompOngoing(_compId), "Competition is not ongoing");

        // ensure bet can be placed
        require(betAmt > 0, "Cannot bet amount less than 1");
        require(_playerDetails.balance >= betAmt, "Insufficient funds");

        // deduct balance
        _playerDetails.balance = _playerDetails.balance - betAmt;

        // request random number from RandomNumGenerator contract, with auto incrementing betId
        rndNumCtx.requestRandomWords(betId);
        betId = betId + 2;

        // track the betID for this particular bet
        book[betId] = Bet(_compId, playerAddr, _betNum, betAmt, _userSeed);

        emit BetPlaced(betId, _compId, playerAddr, _betNum, betAmt, _userSeed);
    }



    /**
    * Gets called by random number generator contract asynchronously
    */
    function fulfillRandomNumber(uint256 _betId, uint256 _randomNum) public {
        
        require(msg.sender == address(rndNumCtx), "Unauthorized!");

        _computeBetResult(_betId + 2, _randomNum);
    }



    /**
    * Compute bet result for a bet
    */
    function _computeBetResult(uint256 _betId, uint256 _randomNum) private {
    
        // fetch bet details
        Bet memory _betDetails = book[_betId];
        uint32 _compId = _betDetails.compId;
        address _playerAddr = _betDetails.playerAddr;
        uint32 _userSeed = _betDetails.userSeed;
        uint256 _betAmt = _betDetails.amount;
        uint8 _betNum = _betDetails.betNum;

        // fetch player details
        Player storage _playerDetails = playerRecord[_compId][_playerAddr];
        uint256 _playerBalance = _playerDetails.balance;

        // compute winning number
        uint8 _winningNum = uint8(((_randomNum % 100) + _userSeed) % (MAX_BET_NUM + 1));

        // track record of the win
        string memory resultMsg = "lose";
        
        if(_betNum == _winningNum) {
            resultMsg = "win";
            _playerDetails.winCount = _playerDetails.winCount + 1;

            // TODO: custom payout logic
            _playerDetails.balance = _playerBalance + _betAmt * 2;
        }

        console.log("Placed bet on %s AND winning number %s", _betNum, _winningNum);
        
        emit BetResult(_betId, _playerAddr, resultMsg, _compId, _winningNum);

    }



    /*
    * Officially ends a competition & tracks the first 3 wallets with the most wins
    */
    function endCompetition(uint32 _compId) public onlyAdmin compExists(_compId) {
        
        Tournament storage _t = competitionInfo[_compId];

        // ensure that wearables contract can actually mint nfts if the user wins
        _isRewardsReady();

        // ensure the competition has ended already
        require(
            _t.compStartTime > 0 && !isCompOngoing(_compId), 
            "Competition has not reached it's end time"
        );

        address[] memory _players = _t.players;

        // track the three winners & store them in the winners field
        // TODO: what if number of players is less than 3 or 
        // if each player has equal num of wins, who should win?
        
        address[3] memory _winnersAddr = [_players[0], address(0), address(0)];

        for(uint i = 1; i < _players.length; i++) {
            Player memory _player = playerRecord[_compId][_players[i]];

            uint32 _firstAmt = (playerRecord[_compId][_winnersAddr[0]]).winCount;
            uint32 _secondAmt = (playerRecord[_compId][_winnersAddr[1]]).winCount;
            uint32 _thirdAmt = (playerRecord[_compId][_winnersAddr[2]]).winCount;

            if(_player.winCount > _firstAmt) {
                _winnersAddr[2] = _winnersAddr[1];
                _winnersAddr[1] = _winnersAddr[0];
                _winnersAddr[0] = _players[i];
            }

            else if(_player.winCount > _secondAmt) {
                _winnersAddr[2] = _winnersAddr[1];
                _winnersAddr[1] = _players[i];
            }

            else if(_player.winCount > _thirdAmt) {
                _winnersAddr[2] = _players[i];
            }

        }

        competitionInfo[_compId].winners = _winnersAddr;

        // TODO: mint wearables to multiple winners. Currently only minted to 1st
        _rewardWearable(_winnersAddr[0]);
        
        // emit competition ended event
        emit CompetitionEnded(_compId);

    }


    /* Before this fn is called, it should be ensured that:
    *  1. This contract is set as globalMinter in the ERC721v2 contract,
    *  2. Atleast one item to be minted does have totalSupply < maxSupply in the ERC721v2 contract.
    */
    function _rewardWearable(address userAddress) internal {
        
        uint256 availableItems = wearableContract.itemsCount();
        uint256 rewardingItemId = generatePseudoRndNum(userAddress) % availableItems;

        // TODO: a better logic!?
        // select the generated itemId only if the item has totalSupply < maxSupply
        // else search for the item with totalSupply < maxSupply
        uint8 iterations = 0;
        uint256 maxSupply;
        uint256 totalSupply;
        while(iterations++ < availableItems) {
            (, maxSupply, totalSupply,,,,) = wearableContract.items(rewardingItemId);

            if(maxSupply > totalSupply) {
                break;
            } else if(rewardingItemId == availableItems - 1) {
                rewardingItemId = 0;
            } else {
                rewardingItemId++;
            }
        }

        if(totalSupply < maxSupply) {

            // prepare params 1
            address[] memory addressList = new address[] (1);
            addressList[0] = userAddress;

            // prepare params 2
            uint256[] memory itemIds = new uint256[] (1);
            itemIds[0] = rewardingItemId;

            _mintWearables(addressList, itemIds);
            emit WearableMinted("success", rewardingItemId);
            
        } else {
            emit WearableMinted("fail", 1);
            revert("No item with totalSupply < maxSupply found");
        }
    }


    // TODO: Make it internal
    /**
    *  Transfer wearables to the user addresses
    */
    function _mintWearables(address[] memory _addressList, uint256[] memory _itemIds) public {
        wearableContract.issueTokens(_addressList, _itemIds);
    }


    /**
    *  Pseudo random number generator to select wearable contract to mint to winners
    */
    function generatePseudoRndNum(address userAddress) private view returns(uint256) {
        return uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, userAddress)));
    }


    /**
    *   Helper function for checking if competition is ongoing
    */
    function isCompOngoing(uint32 _compId) public view returns (bool) {

        uint256 _currentTime = block.timestamp;
        uint256 _compStartTime = competitionInfo[_compId].compStartTime;
        uint256 _compEndTime = _compStartTime + competitionInfo[_compId].compDuration;
        
        console.log("Current time: %s AND Comp end time: %s", _currentTime, _compEndTime);

        return (_currentTime >= _compStartTime && _currentTime < _compEndTime);
    }



    /**
    *   Helper function for checking if registration is ongoing
    */
    function isRegOngoing(uint32 _compId) public view returns (bool) {
        
        Tournament memory _t = competitionInfo[_compId];
        
        uint256 _regStartTime = _t.regStartTime;
        uint256 _currentTime = block.timestamp;
        uint256 _regEndTime = _regStartTime + _t.regDuration;

        console.log("Current time: %s AND Reg end time: %s", _currentTime, _regEndTime);

        return (_currentTime >= _regStartTime && _currentTime < _regEndTime);
    }


    /**
    * Ensure that wearables contract can mint wearables to winners
    */
    function _isRewardsReady() internal view {

        require(wearableContract.isApproved() && wearableContract.isCompleted(), "Wearable contract not ready");

        // the collection has wearables
        uint256 availableWearables = wearableContract.itemsCount();
        require(availableWearables > 0, "Cannot reward winner with wearables");
        
    }


    /**
    * View function for getting winners of a competition
    */
    function getWinners(uint32 _compId) public view returns (address[3] memory) {

        return competitionInfo[_compId].winners;
    }



    /**
    * View function for getting players of a competition
    */
    function getPlayers(uint32 _compId) public view returns (address[] memory) {

        return competitionInfo[_compId].players;
    }



    /**
    * View function for getting players' details for a competition
    */
    function getPlayerDetails(uint32 _compId, address _playerAddr) 
            public view returns (Player memory) {
        
        return playerRecord[_compId][_playerAddr];
    }



    /**
    * Allow only admin
    */
    modifier onlyAdmin() {
        require(msg.sender == admin, "Unauthorized user");
        _;
    }



    /**
    * Ensure that the comptition with given id exists
    */
    modifier compExists(uint32 _compId) {

        require(
            competitionInfo[_compId].startoutBalance != 0, 
            "Competition does not exist"
        );

        _;
    }

}

