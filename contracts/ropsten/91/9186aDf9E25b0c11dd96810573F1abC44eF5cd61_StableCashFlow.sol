    ) internal {
        if ( flowRate == int96(0) ) {
            deleteFlow(cfaLibrary, address(this), receiver, token);
        } else {
            (,int96 existingFlowRate,,) = cfaLibrary.cfa.getFlow(token, address(this), receiver);
            if ( existingFlowRate == int96(0) ) {
