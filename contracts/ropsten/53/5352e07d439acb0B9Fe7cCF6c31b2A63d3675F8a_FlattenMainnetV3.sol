pragma solidity ^0.8.10;
contract FlattenMainnetV3 is
    Create2,
    Ownable,
    Pausable,
    ReentrancyGuard,
    IERC721Receiver,
    AxelarExecutable
{
    using StringToAddress for string;
    using AddressToString for address;

    struct ChildTokenAndTemplateToken {
        address childToken;
        address templateToken;
    }

    bytes32 public constant DEPOSIT = keccak256('DEPOSIT');
    bytes32 public constant MAP_TOKEN = keccak256('MAP_TOKEN');

    IAxelarGasService public immutable gasReceiver;

    mapping(address => bool) public authorizedAddress;
    mapping(string => address) public receiverContractMapping;
    mapping(address => mapping(string => mapping(address => ChildTokenAndTemplateToken))) public rootToChildToken;

    event TokenMappedERC721(address indexed rootToken, address indexed childToken);
    event ManualOperation(address indexed rootToken, address indexed from, address indexed to, uint256 tokenId);
    event DepositERC721(
        address indexed rootToken,
        address indexed userAddress,
        string indexed destinationChain,
        uint256 tokenId
    );
    event withdrawERC721(
        address indexed rootToken,
        address indexed childToken,
        address indexed userAddress,
        uint256 tokenId
    );

    modifier onlyAuth() {
        require(authorizedAddress[msg.sender], 'WITHOUT_AUTH');
        _;
    }

    receive() external payable {}

    fallback() external payable {}

    constructor(address gateway_, address gasReceiver_) AxelarExecutable(gateway_) {
        gasReceiver = IAxelarGasService(gasReceiver_);
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function changeAuthAddress(address _authAddress, bool _bool) external onlyOwner {
        authorizedAddress[_authAddress] = _bool;
    }

    function changeTemplateToken(
        address rootToken,
        address template,
        string memory destinationChain
    ) external onlyOwner {
        rootToChildToken[rootToken][destinationChain][rootToken].templateToken = template;
    }

    function changeChildToken(
        address rootToken,
        address childToken,
        string memory destinationChain
    ) external onlyOwner {
        rootToChildToken[rootToken][destinationChain][rootToken].childToken = childToken;
    }

    function updateReceiverContract(string memory destinationChain, address destcontract) external onlyOwner {
        receiverContractMapping[destinationChain] = destcontract;
    }

    function manualOperation(
        address rootToken,
        address to,
        uint256 tokenId,
        bytes memory data
    ) external onlyAuth {
        ERC721(rootToken).safeTransferFrom(address(this), to, tokenId, data);
        emit ManualOperation(rootToken, address(this), to, tokenId);
    }

    function mapToken(address rootToken, string memory destinationChain) external payable onlyOwner {
        ChildTokenAndTemplateToken memory childTokenAndTemplateToken = rootToChildToken[rootToken][destinationChain][
            rootToken
        ];

        require(msg.value > 0, 'HAVE_NOT_ENOUGH_FEE');
        require(childTokenAndTemplateToken.childToken == address(0x0), 'ALREADY_MAPPED');

        ERC721 rootTokenContract = ERC721(rootToken);
        string memory name = rootTokenContract.name();
        string memory symbol = rootTokenContract.symbol();

        address receiverContract = receiverContractMapping[destinationChain];

        bytes memory payload = abi.encode(MAP_TOKEN, abi.encode(rootToken, destinationChain, name, symbol));
        string memory stringAddress = receiverContract.toString();

        gasReceiver.payNativeGasForContractCall{value: msg.value}(
            address(this),
            destinationChain,
            stringAddress,
            payload,
            msg.sender
        );

        gateway.callContract(destinationChain, stringAddress, payload);

        bytes32 salt = keccak256(abi.encodePacked(rootToken));
        address childToken = computedCreate2Address(
            salt,
            keccak256(minimalProxyCreationCode(childTokenAndTemplateToken.templateToken)),
            receiverContract
        );

        rootToChildToken[rootToken][destinationChain][rootToken].childToken = childToken;

        emit TokenMappedERC721(rootToken, childToken);
    }

    function deposit(
        address rootToken,
        uint256 tokenId,
        string memory destinationChain,
        bytes memory data
    ) external payable whenNotPaused nonReentrant {
        address receiverContract = receiverContractMapping[destinationChain];
        string memory stringAddress = receiverContract.toString();

        ERC721(rootToken).safeTransferFrom(msg.sender, address(this), tokenId, data);

        bytes memory payload = abi.encode(DEPOSIT, abi.encode(rootToken, msg.sender, destinationChain, tokenId, data));

        gasReceiver.payNativeGasForContractCall{value: msg.value}(
            address(this),
            destinationChain,
            stringAddress,
            payload,
            msg.sender
        );

        gateway.callContract(destinationChain, stringAddress, payload);

        emit DepositERC721(rootToken, msg.sender, destinationChain, tokenId);
    }

    function _execute(
        string calldata sourceChain,
        string calldata sourceAddress,
        bytes calldata payload
    ) internal override {
        require(sourceAddress.toAddress() == receiverContractMapping[sourceChain], 'NOT_A_LINKER');

        (address rootToken, address childToken, address to, uint256 tokenId, bytes memory data) = abi.decode(
            payload,
            (address, address, address, uint256, bytes)
        );

        ERC721(rootToken).safeTransferFrom(address(this), to, tokenId, data);

        emit withdrawERC721(rootToken, childToken, to, tokenId);
    }
}