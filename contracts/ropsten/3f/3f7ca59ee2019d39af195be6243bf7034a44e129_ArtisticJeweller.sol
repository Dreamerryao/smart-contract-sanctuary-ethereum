contract ArtisticJeweller is ERC721, ERC721URIStorage, Pausable, Ownable, ERC721Burnable {
    using Counters for Counters.Counter;

    address payable private AdminAddress;

    struct NFTData {
        string jsonData;
        uint256 nftRoyalty;
        uint256 nftPrice;
        address royaltyAddress;
    }
    mapping(uint256 => NFTData) public storeData;
    uint256 public AdminFee;

    Counters.Counter private _tokenIdCounter;

    modifier NFTOwner(uint256 tokenIdCounter) {
        require(ownerOf(tokenIdCounter) == msg.sender, "Caller is not the NFT Owner");
        _;
   }

    constructor() ERC721("Artistic Jeweller", "AJ") {
        AdminFee = 10;
        AdminAddress = payable(msg.sender); 

    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://my-json-server.typicode.com/abcoathup/samplenft/tokens/";
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function safeMint(address to, string memory uri) public onlyOwner {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }


    // ----------------- MyOwn Functions -------------------  //
    function mintNFT(address to, string memory uri, string memory _json, uint256 _nftRoyalty, uint256 _price, address _royaltyAddress) public payable{
        storeData[uint256(_tokenIdCounter.current())].jsonData = _json;
        storeData[uint256(_tokenIdCounter.current())].nftRoyalty = _nftRoyalty;
        storeData[uint256(_tokenIdCounter.current())].royaltyAddress = _royaltyAddress;
        storeData[uint256(_tokenIdCounter.current())].nftPrice = _price;

        require(msg.sender != address(0), "Zero address");
        uint256 etherValue = msg.value;
        require (etherValue > 0, "Zero Amount!");

        // NFT Owner
        address payable nftOwner = payable(msg.sender);
        // Royalty Address
        address payable RoyaltyAddress = payable(_royaltyAddress);


        // divide ether value in 3 part
        uint256 etherValue_admin = (etherValue * (AdminFee))/100;
        uint256 etherValue_royalty = (etherValue * (_nftRoyalty))/100;
        uint256 etherValue_Owner = (etherValue * ((100-_nftRoyalty-AdminFee)))/100;

        // we will distribute on 3 different address , but for testing we are using same address
        AdminAddress.transfer(etherValue_admin);
        RoyaltyAddress.transfer(etherValue_royalty);
        nftOwner.transfer(etherValue_Owner);

        // mint nft
        safeMint(to, uri);
    }


    // nft owner can burn nft via this address
    function burnNFT(uint256 tokenIdCounter) public NFTOwner(tokenIdCounter) {
        storeData[tokenIdCounter].jsonData = "";
        storeData[tokenIdCounter].nftRoyalty = 0;
        storeData[tokenIdCounter].nftPrice = 0;
        storeData[tokenIdCounter].royaltyAddress = address(0);
        burn(tokenIdCounter);
    }



    // only admin can set the admin fee from this function
    function setAdminFee(uint256 _adminFee) public onlyOwner {
        AdminFee = _adminFee;
    }


    // only owner can update NFT Data such as Royalty Fee, price, json data etc using this function 
    function updateNFTData(uint256 tokenIdCounter, string memory _jsonData, uint256 _royaltyFee, uint256 _nftPrice, address _royaltyAddress) public NFTOwner(tokenIdCounter) {
        // require(ownerOf(tokenIdCounter) == msg.sender, "Caller is not the NFT Owner");
        storeData[tokenIdCounter].nftRoyalty  = _royaltyFee;
        storeData[tokenIdCounter].jsonData  = _jsonData;
        storeData[tokenIdCounter].nftPrice  = _nftPrice;
        storeData[tokenIdCounter].royaltyAddress  = _royaltyAddress;
    }


    function sellPremintedNFT(address from, address to, uint256 tokenIdCounter) public payable { 
        // NFT Owner
        address payable nftOwner = payable(ownerOf(tokenIdCounter));
        // Royalty Address
        address payable RoyaltyAddress = payable(storeData[tokenIdCounter].royaltyAddress);

        uint256 _nftRoyalty = storeData[uint256(tokenIdCounter)].nftRoyalty;

        require(msg.sender != address(0), "Zero address");
        uint256 etherValue = msg.value;
        require (etherValue > 0, "Zero Amount!");

        // divide ether value in 2 part
        uint256 etherValue_royalty = (etherValue * (_nftRoyalty))/100;
        uint256 etherValue_Owner = (etherValue * ((100-_nftRoyalty)))/100;

        // we will distribute on 2 different address
        RoyaltyAddress.transfer(etherValue_royalty);
        nftOwner.transfer(etherValue_Owner);

        safeTransferFrom(from, to, tokenIdCounter);
    }

    // return total nft count
    function getTotalNFTCount() public view returns(uint256) {
        return uint256(_tokenIdCounter.current());
    }


}