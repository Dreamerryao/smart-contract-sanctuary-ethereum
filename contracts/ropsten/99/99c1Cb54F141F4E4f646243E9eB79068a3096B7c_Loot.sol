        address to,
        uint256 id,
        uint256 quantity
    ) public {
        require(minterRole[msg.sender], "Loot#mint: ONLY_MINTER");
        _mint(to, id, quantity, "");
        tokenSupply[id] = tokenSupply[id] + quantity;

        emit Mint(to, id, quantity);
