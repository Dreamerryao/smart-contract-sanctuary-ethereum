pragma solidity ^0.8.10;
import "./Ownable.sol";
import "hardhat/console.sol";
error NotRequiredNumberOfRandomizers();
error UnequalLengths();
error ExceedsMaxLimit();
error MaxLimitHigherThanRandomizers();

// Decentralized, fair (assuming no single authority controls the largest ethereum holders)
contract Randomizer is Ownable{

    // The Ether balance of these addresses will be used to randomize the lottery winner(s)
    address[] public randomizerAddresses;
    uint256 public randomizerAddressesCount;
    uint256 public MAX_LIMIT = 200;

    mapping( address => bool ) public isRandomizerAddress;

    constructor(address[] memory _randomizerAddresses) Ownable(){
        addRandomizerAddresses(_randomizerAddresses);
    }

    function addRandomizerAddresses(address[] memory _randomizerAddresses) public onlyOwner{
        uint256 length = _randomizerAddresses.length;
        if(randomizerAddressesCount + length > MAX_LIMIT) revert ExceedsMaxLimit();
        for(uint256 i=0; i< length; i++) {
            randomizerAddresses.push(_randomizerAddresses[i]);
            isRandomizerAddress[_randomizerAddresses[i]] = true;
        }

        randomizerAddressesCount += length;
    }

    function updateRandomizerAddresses(uint256[] memory _randomizerAddressesIndices, address[] memory _randomizerAddressesReplacements) external onlyOwner{
        if(_randomizerAddressesIndices.length != _randomizerAddressesReplacements.length) revert UnequalLengths();

        for(uint256 i=0; i< _randomizerAddressesIndices.length; i++) {
            randomizerAddresses[_randomizerAddressesIndices[i]] = _randomizerAddressesReplacements[i];
        }
    }

    function randomizeWinner() public view returns(uint256){

        uint256 number = uint256(
            keccak256(
                abi.encodePacked(
                blockhash(block.number - 1),
                block.coinbase,
                block.difficulty,
                block.timestamp
                )
            )
        );

        if(MAX_LIMIT > randomizerAddresses.length) revert MaxLimitHigherThanRandomizers();

        for(uint256 i=0; i< MAX_LIMIT; i++){
            number = uint256(
                keccak256(
                    abi.encodePacked(
                    number,
                    randomizerAddresses[i].balance
                    )
                )
            );
        }
        return number;
    }

}

