pragma solidity 0.8.14;

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "./BaseIlluvitar.sol";
import "./datatypes/PortraitDataTypes.sol";
import "./interfaces/IlluvitarsMetadataBuilderSpec.sol";
import "base64-sol/base64.sol";

/**
 * @title Portrait Layer
 * @dev inherit BaseIlluvitar
 * @author Dmitry Yakovlevich, Yuri Fernandes
 */
contract PortraitLayerImpl is BaseIlluvitar {
    /// @dev Portrait metadata
    mapping(uint256 => PortraitMetadata) public metadata;

    /**
     * @dev Initialize Portrait Layer.
     * @param name_ NFT Name
     * @param symbol_ NFT Symbol
     * @param imxMinter_ IMX Minter Address
     * @param portraitMetadataBuilder_ metadata builder for the portrait layer
     */
    function initialize(
        string memory name_,
        string memory symbol_,
        address imxMinter_,
        address portraitMetadataBuilder_
    ) external initializer {
        // initialize BaseIlluvitar storage variables
        __BaseIlluvitar_init(name_, symbol_, imxMinter_, portraitMetadataBuilder_);
    }

    function contractURI() public pure override returns (string memory) {
        bytes memory contractLevelMetadata;
        {
            string[6] memory contractLevelMetadataArray = _contractLevelMetadataArray();
            for (uint8 i = 0; i < contractLevelMetadataArray.length; i++) {
                contractLevelMetadata = abi.encodePacked(contractLevelMetadata, contractLevelMetadataArray[i]);
            }
        }
        return string(abi.encodePacked("data:application/json;base64, ", Base64.encode(contractLevelMetadata)));
    }

    function contractLevelImageURI() public pure override returns (string memory) {
        // TODO: Define Portrait (Illuvitar) contract-level image
        return "";
    }

    function description() public pure override returns (string memory) {
        // TODO: Description for the Portrait (Illuvitar) collection need to be set
        return "";
    }

    /**
     * @dev Mint Portrait with blueprint.
     * @dev blueprint has format of `a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t`
     *      a : set
            b : batch
            c : tier
            d : line
            e : stage
            f : variation
            g : expression
            h : finish
            i : affinity
            j : class
            k : Background Set
            l : Background Tier
            m : Background Line
            n : Background Stage
            o : Background Variation
            p : bonded skin id
            q : bonded body wear id
            r : bonded eye wear id
            s : bonded head wear id
            t : bonded prop id
     * @param to Recipient address
     * @param tokenId Token id
     * @param blueprint Portrait blueprint
     */
    function _mint(
        address to,
        uint256 tokenId,
        bytes memory blueprint
    ) internal override {
        _safeMint(to, tokenId);
        metadata[tokenId] = _parseBlueprint(blueprint);
        metadataInitialized[tokenId] = true;
    }

    /// @dev Parse blueprint
    function _parseBlueprint(bytes memory blueprint) private pure returns (PortraitMetadata memory _metadata) {
        uint8 p;
        uint8 placeholder;

        p = _skipNonDecimal(blueprint, p);
        (_metadata.set, p) = _atoi8(blueprint, p);
        (_metadata.batch, p) = _atoi8(blueprint, p);
        (_metadata.tier, p) = _atoi8(blueprint, p);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.line = PortraitLine(placeholder);
        (_metadata.stage, p) = _atoi8(blueprint, p);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.variation = PortraitVariation(placeholder);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.expression = Expression(placeholder);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.finish = Finish(placeholder);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.affinity = Affinity(placeholder);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.class = Class(placeholder);
        (_metadata.background.set, p) = _atoi8(blueprint, p);
        (_metadata.background.tier, p) = _atoi8(blueprint, p);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.background.line = BackgroundLine(placeholder);
        (_metadata.background.stage, p) = _atoi8(blueprint, p);
        (placeholder, p) = _atoi8(blueprint, p);
        _metadata.background.variation = BackgroundVariation(placeholder);
        (_metadata.slots.skinId, p) = _atoi256(blueprint, p);
        (_metadata.slots.bodyId, p) = _atoi256(blueprint, p);
        (_metadata.slots.eyeId, p) = _atoi256(blueprint, p);
        (_metadata.slots.headId, p) = _atoi256(blueprint, p);
        (_metadata.slots.propId, p) = _atoi256(blueprint, p);
        p = _skipNonDecimal(blueprint, p);
        require(p == blueprint.length, "Wrong blueprint format");
    }

    /**
     * @dev Returns the decimal representation of bytes string at from a given index and return the subsequent
     *  non-decimal character index (For uint256)
     *
     * @dev Throws if the byte on the requested index is not a char representation of a decimal number
     *  or is greater or equal to the bytes string length
     *
     * @param a byte string containing decimal numbers chars
     * @param index the index of the decimal number char
     * @return i converted decimal number char to uint256
     * @return p index of the first non-decimal char plus 1
     */
    function _atoi256(bytes memory a, uint8 index) internal pure returns (uint256 i, uint8 p) {
        require(index < a.length && _isDecimal(a[index]), "Wrong blueprint format");
        for (p = index; p < a.length; ++p) {
            if (!_isDecimal(a[p])) break;
            i *= 10;
            i += uint8(a[p]) - 0x30;
        }
        ++p;
    }

    function _contractLevelMetadataArray() internal pure returns (string[6] memory) {
        return [
            '{"name":"Illuvitar Accessory",',
            '"description":"',
            description(),
            '", "image": "',
            contractLevelImageURI(),
            '"}'
        ];
    }

    function _buildMetadata(uint256 _tokenId) internal view override returns (string memory) {
        return IlluvitarsMetadataBuilder(metadataBuilder).buildPortraitMetadata(_tokenId);
    }
}

