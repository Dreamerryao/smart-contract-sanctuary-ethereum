contract SweetNFT is ERC721, AccessControl, ContextMixin, NativeMetaTransaction {
    /*
    @Dev: Access controls - granted by deployer of contract.
    @Notice: Some functions can only be called by minter.
    */
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    /*
    @Dev: ECDSA used for signiture confirmation `delegatedSignedSafeTransferFrom`.
    @Notice: Some functions can only be called by minter.
    */
    using ECDSA for bytes32;
    /*
    @Notice: These are variables to initalize contract.
    @Dev: The global transfer nonce for transfer signiture.
    */
    uint256 public globalTransferNonce;
    /*@Dev: The sale inital state means minting is open. Tokens can be purchased. */
    bool public closeMinting = false;
    /*@Dev: Whether the contract prevents updating token URIs
    bool _contractFrozen;
    /*@Dev: This is the mapping for tokenURI. */
    mapping (uint256 => string) private _tokenURI;


    /*
    @Dev: The constructor initializes variables initialized during deployment.
    @Params: The name is the name of collection.
    @Params: The symbol is the symbol for collection.
    @Params: The initialContractURI is the contract URI.
    @Notice: Contract URI usually ends with "/contract".
    @Notice: The lower limit for minting starts at one but can be adjusted via setter function.
    @params: Roles include - ADMIN & MINTER = deploying wallet.
    */
    constructor(
        string memory name,
        string memory symbol,
        string memory initialContractURI
    )
        ERC721(
            name,
            symbol
        )
    {
        _contractURI = 
        initialContractURI;
        _contractFrozen = false;

        _initializeEIP712(
            name
        );

        _grantRole(
            MINTER_ROLE,
            msg.sender
        );
    }

    /*
    @Dev: The below variables are storage for token metrics & the contract URI.
    @Notice: These are public variables to create getter functions to be viewed on etherscan.
    */

    /*Storage for the contract URI*/
    string public _contractURI;
    bool public _contractFrozen;


    /*@Dev: The function for producing contract URI.*/
     function contractURI() external view returns (string memory) {
        return string(abi.encodePacked(_contractURI)
        );
    }

    /*@Dev: If contract metadata is frozen, tokenURIs can't be changed.*/
    function contractMetadataIsFrozen() external view returns (bool) {
        return _contractFrozen;
    }


    /*
    @Dev: This is the opensea proxy address for gasless transactions.
    @Notice: The address is the operator in the override at the end of the contract.
    @Notice: The address can be reset in the setter function `setOPENSEAPROXY` at the end of the contract.
    */
    address public OPENSEA_PROXY = 0x58807baD0B376efc12F5AD86aAc70E78ed67deaE;

    /*
    @Dev: This event is to notify when minting is closed. This can only occur once.
    @Notice: Once closed minting cannot be reopened.
    */
    event MintingClosed(
        bool val
    );

    /** Token Management Functions **/

    /*
    @Dev: This function requires the users signiture to execute transfer.
    @Params: The `from` variable (address of sender) `to` variable (address of reciever).
    @Params: The tokenId is the ID of token being sent.
    @Params: Signiture of token holder.
    */
    function delegatedSafeSignedTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory signature
    )
        public
    {
        require(
            userSignedApprovalForTransfer(
                from,
                to,
                tokenId,
                signature
            ),
            "SweetNFT: INVALID_SIGNATURE"
        );

        globalTransferNonce += 1;

        _safeTransfer(
            from,
            to,
            tokenId,
            ""
        );
    }

    /*
    @Dev: This function obtains the users signiture to execute transfer.
    @Params: The `from` variable (address of sender) `to` variable  (address of reciever).
    @Params: The `tokenId` is the ID of token being sent.
    @Params: The `signiture` Signiture of token holder.
    @Notice: This function requires the signer to own the token.
    */
    function userSignedApprovalForTransfer(
        address from,
        address to,
        uint256 tokenId,
        bytes memory signature
    )
        internal
        view
        returns (bool)
    {
        require(
            hasRole(
                MINTER_ROLE,
                _msgSender()
            ),
            "SweetNFT: INVALID_ROLE"
        );

        bytes32 userHash = keccak256(
            abi.encodePacked(
                address(this),
                globalTransferNonce,
                from,
                to,
                msgSender(),
                tokenId
            )
        );

        address messageSigner = userHash.toEthSignedMessageHash().recover(
            signature
        );

        require((
            messageSigner == from && messageSigner == this.ownerOf(tokenId)),
            "INVALID_CALLER"
        );

        return true;
    }


    /** Token Production Functions - MINT **/

    /*
    @Dev: This function mints to single address with single token/uri.
    @Notice: This function permantely closes with `mintingIsOpened` modifer.
    @Params: to: address to hold NFT.
    @Params: tokenId:  the tokenId to be minted.
    @Params: uri: corresponding token uri.
    */
    function safeMint(address to, uint256 tokenId, string memory uri)
        public
        mintingIsOpened
        onlyRole(MINTER_ROLE)
    {
        require(tokenId >= 1, "INVALID_TOKENID");
        _safeMint(
            to,
            tokenId
        );
        _tokenURI[tokenId] =  uri;
    }

    /*
    @Dev: This function mints to multiple addresses.
    @Notice: This permantely closes with `mintingIsOpened` modifer.
    @Params: This to: address to hold NFT.
    @Params: The tokenIds:  the tokenIds to be minted.
    @Params: The uris: arrary of corresponding token uris.
    */

    function mintToAddresses( address[] calldata to, uint256[] calldata tokenIds, string[] calldata uris) public 
    mintingIsOpened
    onlyRole(MINTER_ROLE)
    {
        for (uint i = 0; i < tokenIds.length; i++) {
        if(tokenIds[i] < 1) {
                revert();
            }
            _safeMint(to[i], tokenIds[i]);
            _tokenURI[tokenIds[i]] =  uris[i];
        }
    }

    /*@Dev: This function overrides implementation to allow tokenURI setting in mint functions. */
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId));
        return _tokenURI[tokenId];
    }

    /*
    @Notice: This is a modifer to track if minting is currently live or NFTS.
    @Dev: Once minting is closed it cannot be repopened.
    */
    modifier mintingIsOpened() {
        require(
            closeMinting == false,
            "SWEETNFT: ALREADY_CLOSED"
        );
        _;
    }

    /* ADMIN SETTER FUNCTIONS - External admin only calls */


    /*
    @Dev: ADMIN close minting for NFTS.
    @Notice: This setter function adjusts boolean to call when a set is closed.
    @Notice: Sets NFTS sales to a closing state state.
    @Dev: CANNOT BE REVERSED */
    function setMintingClosed() external onlyRole(MINTER_ROLE)  {
        require(closeMinting == false, "minting already closed");
        closeMinting = true;
        emit MintingClosed(true);
    }

    /*@Dev: Sets OPENSEA_PROXY in case it needs to be adjusted*/
    function setOpenseaProxy(address newProxy) external onlyRole(MINTER_ROLE) {
        OPENSEA_PROXY = newProxy;
    }

    /*
    @Param: The newContractURIInput replaces the _contractURI initalized in the constructor.
    @Notice: The newContractURIInput should usually end in "/contract".
    */ 
    function setContractURI(string calldata newContractURIInput) external onlyRole(MINTER_ROLE) {
        _contractURI = newContractURIInput;
    }

    /**
     * Allow minter to update the token URI if contract is not frozen
     */
    function setTokenURI(uint256 tokenId, string calldata uri) external onlyRole(MINTER_ROLE) {
        require(_exists(tokenId), "nonexistent token");
        require(_contractFrozen == false,
            "SweetToken: token URI is frozen"
        );
        _tokenURI[tokenId] = uri;
    }

    /**
     * Minter can freeze all metadata changes for the contract
     */
    function freezeContractMetadata() external onlyRole(MINTER_ROLE) {
        require(_contractFrozen != true, "contract is already frozen");
        _contractFrozen = true;
    }


    /**
     * Allow metatransactions for OpenSea listing support:
     * https://docs.opensea.io/docs/other-blockchains
     */
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }

    function isApprovedForAll(
        address _owner,
        address _operator
    )
        public
        override
        view
        returns (bool isOperator)
    {
        if (_operator == OPENSEA_PROXY) {
            return true;
        }
        return ERC721.isApprovedForAll(
            _owner,
            _operator
        );
    }
    /*dev: override required to use ERC721 & AccessControl*/
    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(
            ERC721,
            AccessControl
        )
        returns (bool)
    {
        return super.supportsInterface(
            interfaceId
        );
    }
}

