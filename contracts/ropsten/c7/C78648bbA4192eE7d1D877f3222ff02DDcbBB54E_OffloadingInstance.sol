//
// [email protected]: this file implements the interactions with client/vendors and contains the offloading algorithm. 


// [email protected]: usage error.
// ref: https://stackoverflow.com/questions/49345903/copying-of-type-struct-memory-memory-to-storage-not-yet-supported
contract OffloadingInstance is IUser_Offloading, IEdge_Offloading, IVerifier_Offloading {

    //
    // struct definition
    struct ServerResult {
        // server identifier
        // assumption: sid != 0. 
        address sid;
        // server latency time (to client)
        uint256 result;
        // collected
        bool collected;
    }



    // 
    // variable denfinition
    Task task;
    ServerResult[] internal servers; 
    IRequest_TrustedVerifier req;
    uint64 counter = 0;
    IDoinB doinb;


    //
    // functions
    // initialize the contract
    constructor(address _doinb) {
        doinb = IDoinB(_doinb);
        req = IRequest_TrustedVerifier(0x5FC8d32690cc91D4c39d9d3abcBD16989F875707);
    }


    // ???
    function setTask(Task memory _task) override external {
        task = _task;
    }

    // find the ideal two severs from given list. 
    function setServers(ServerInfo[] memory _servers) override external returns(ServerInfo memory, ServerInfo memory) {
        uint64 DEFAULT_NSERVERS = 2;
        ServerInfo memory best;
        ServerInfo memory next_best;

        require(servers.length == 0, "Offloading Contract: Error: Servers have been initialized. ");
        require(_servers.length >= 2, "Offloading Contract: Failed: Need more servers (>=2). ");
        require(doinb.balanceOf(address(this)) >= task.wage * DEFAULT_NSERVERS, "Offloading Contract: Wage is not enough. ");

        best.latency = type(uint256).max;
        next_best.latency = type(uint256).max;

        for (uint256 i = 0; i < _servers.length; i++) {
            if (best.latency > _servers[i].latency) {
                next_best = best;
                best = _servers[i];
            } else if (next_best.latency > _servers[i].latency) {
                next_best = _servers[i];
            }
        }

        ServerResult memory u1;
        ServerResult memory u2;

        u1.sid = best.sid;
        u2.sid = next_best.sid;

        servers.push(u1);
        servers.push(u2);

        console.log("Offloading Contract: selects the servers: ", best.sid, next_best.sid);

        return (best, next_best);
    }

    // receive the offloading result and verify the consistency when all results are collected. 
    function submitResult(uint32 result) override external {

        for (uint64 i = 0; i < servers.length; i++) {
            if (servers[i].sid == msg.sender) {
                if (!servers[i].collected) {
                    require(doinb.balanceOf(address(this)) >= (task.wage * servers.length + task.deposit * (counter + 1)), "Offloading Contract: Deposit is not enough. ");
                    servers[i].result = result;
                    servers[i].collected = true;
                    console.log("Offloading Contract: Received new result: ", result, ". ");
                    counter += 1;
                    break;
                }
            }
        }

        if (counter == servers.length) {
            executeVerify();
        }
    }

    // internal use for `verifyOffloading`. 
    function executeVerify() internal {
        for (uint64 i = 0; i < servers.length; i++) {
            if (i > 0 && servers[i].result != servers[i - 1].result) {
                console.log("Offloading Contract: Warning: Inconsistent results found, requiring reverification. ");
                requireReverify();
                return;
            }
        }

        console.log("Offloading Contract: OK: The results are the same. ");
        for (uint64 i = 0; i < servers.length; i++) {
            doinb.transfer(servers[i].sid, task.wage + task.deposit);
        }
    }

    // Send request to trusted 3-party certification. 
    function requireReverify() internal {
        doinb.transfer(address(req), task.fees);
        req.requestVerify(task);
    }

    // receive the trusted output of the task. 
    function submitRealResult(uint32 result) override external {
        for (uint64 i = 0; i < servers.length; i++) {
            if (servers[i].result == result) {
                console.log("Offloading contract: Node ", servers[i].sid, " submited correct result. ");
                doinb.transfer(servers[i].sid, doinb.balanceOf(address(this)));
                break;
            }
        }
    }
}

