pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "./interfaces/IMasterOracle.sol";
import "./interfaces/IDollar.sol";
import "./interfaces/IShare.sol";
import "./interfaces/ITreasury.sol";
import "./interfaces/ICompSwapStrategy.sol";

contract Comptroller is Ownable, ReentrancyGuard, Initializable {
    using SafeERC20 for IERC20;
    using SafeERC20 for IShare;
    using SafeERC20 for IDollar;

    struct UserInfo {
        uint256 usdcBalance;
        uint256 shareBalance;
        uint256 dollarBalance;
        uint256 lastAction;
    }
    /* ========== ADDRESSES ================ */
    IMasterOracle public oracle;
    IDollar public dollar;
    IShare public share;
    IERC20 public collateral;
    ICompSwapStrategy public swapStrategy;
    address public treasury;

    /* ========== STATE VARIABLES ========== */

    /** control mint and redeem  */
    bool public mintPaused = false;
    bool public redeemPaused = false;

    mapping(address => UserInfo) public userInfo;

    uint256 public unclaimedDollar;
    uint256 public unclaimedShare;
    uint256 public unclaimedUsdc;

    // Constants for various precisions
    uint256 private constant PRICE_PRECISION = 1e18;
    uint256 private constant COLLATERAL_RATIO_PRECISION = 1e6;
    uint256 private constant COLLATERAL_RATIO_MAX = 1e6;
    uint256 private constant PRECISION = 1e6;
    uint256 private constant MAX_REFRESH_COOL_DOWN = 7200;
    uint256 private constant MAX_RATIO_STEP = 5000;

    // Number of decimals needed to get to 18
    uint256 private missingDecimals;
    // collateral_ratio
    uint256 public collateralRatio = 1e6;
    uint256 public lastRefreshCrTimestamp;
    uint256 public refreshCooldown = 3600; // 1 hour Seconds to wait before being able to run refreshCollateralRatio() again
    uint256 public ratioStep = 2000; // Amount to change the collateralization ratio by upon refreshCollateralRatio()
    uint256 public priceTarget = 1e18; // The price of DOLLAR;
    uint256 public priceBand = 5e17; // 0.5 The bound above and below the price target at which the Collateral ratio is allowed to drop
    uint256 public minCollateralRatio = 1e6; // = 100% at genenis
    bool public collateralRatioPaused = false; // during bootstraping phase, collateral_ratio will be fixed at 100%

    // fees
    uint256 public redemptionFee = 4000; // 0.4%, 6 decimals of precision
    uint256 public constant REDEMPTION_FEE_MAX = 9000; // 0.9%
    uint256 public mintingFee = 3000; //  0.3%, 6 decimals of precision
    uint256 public constant MINTING_FEE_MAX = 9000; // 0.9%

    /* ========== MODIFIER ========== */

    /* ========== CONSTRUCTOR ========== */
    function initialize(
        IDollar _dollar,
        IShare _share,
        IERC20 _collateral,
        address _treasury
    ) external initializer {
        require(address(_dollar) != address(0), "Comptroller::contructor: invalid dollar address");
        require(address(_share) != address(0), "Comptroller::contructor: invalid share address");
        require(address(_collateral)!= address(0), "Comptroller::contructor: invalid collateral address");
        require(_treasury != address(0), "Comptroller::contructor: invalid treasury address");
        dollar = _dollar;
        share = _share;
        collateral = _collateral;
        treasury = _treasury;
        missingDecimals = 18 - ERC20(address(_collateral)).decimals();
    }
    /// @notice Amount usdc balance available
    function availableUsdcBalance() public view returns (uint256) {
        return collateral.balanceOf(address(this)) - unclaimedUsdc;
    }

    /// @notice Get some config info
    /// @return _collateralRatio value of cllateral ratio in 1e6 precision
    /// @return _mintingFee value of minting fee in 1e6 precision
    /// @return _redemptionFee value of redemption fee ratio in 1e6 precision
    /// @return _usdcBalance amount of USDC available
    /// @return _mintPaused mint is pause or not
    /// @return _redeemPaused redeem is pause or not
    function info()
        external
        view
        returns (
            uint256 _collateralRatio,
            uint256 _mintingFee,
            uint256 _redemptionFee,
            uint256 _usdcBalance,
            bool _mintPaused,
            bool _redeemPaused
        )
    {
        _collateralRatio = collateralRatio;
        _mintingFee = mintingFee;
        _redemptionFee = redemptionFee;
        _usdcBalance = availableUsdcBalance();
        _mintPaused = mintPaused;
        _redeemPaused = redeemPaused;
    }

    /// @notice Calculate the expected results for minting
    /// @param _usdcIn Amount of USDC input.
    /// @return _usdcNeedSwap : the amount of USDC need to swap SHARE.
    /// @return _dollarOut : the amount of DOLLAR to swap
    /// @return _fee : the fee amount in USDC.
    function calculateMint(uint256 _usdcIn)
        public
        view
        returns (
            uint256 _usdcNeedSwap,
            uint256 _dollarOut,
            uint256 _fee
        )
    {
        (uint256 _sharePrice,  uint256 _dollarPrice , uint256 _usdcPrice) = oracle.getSpotPrices();
        require(_usdcPrice > 0, "Comptroller::calculateMint:usdc price invalid ");
        require(_sharePrice > 0, "Comptroller::calculateMint:share price invalid ");
        // uint256 _usdcValue = (_usdcIn * (10**missingDecimals) * _usdcPrice) / PRICE_PRECISION;
        // _dollarOut = _usdcValue - ((_usdcValue * mintingFee) / PRECISION);
        _dollarOut = (_usdcIn * 10**missingDecimals * (PRECISION - mintingFee) * _usdcPrice) / PRECISION / _dollarPrice;
        _usdcNeedSwap = (_usdcIn * (COLLATERAL_RATIO_MAX - collateralRatio)) / COLLATERAL_RATIO_MAX;
        _fee = (_usdcIn * mintingFee * collateralRatio) / COLLATERAL_RATIO_MAX / PRECISION;
    }

    /// @notice Calculate the expected results for redemption
    /// @param _dollarIn Amount of USDC input.
    /// @return _usdcOut : the amount of USDC output.
    /// @return _shareOut : the amount of SHARE output by swapping
    /// @return _fee : the fee amount in USDC
    function calculateRedeem(uint256 _dollarIn)
        public
        view
        returns (
            uint256 _usdcOut,
            uint256 _shareOut,
            uint256 _fee
        )
    {
        (uint256 _sharePrice, uint256 _dollarPrice , uint256 _usdcPrice) = oracle.getSpotPrices();
        require(_usdcPrice > 0, "Comptroller::calculateRedeem:Invalid usdc price");
        require(_sharePrice > 0, "Comptroller::calculateRedeem:Invalid share price");
        if (collateralRatio < COLLATERAL_RATIO_MAX) {
            // _shareOut = (_dollarIn * (COLLATERAL_RATIO_PRECISION - collateralRatio) * (PRECISION - redemptionFee) * PRICE_PRECISION) / COLLATERAL_RATIO_PRECISION / PRECISION / _sharePrice;
            _shareOut = (_dollarIn * (COLLATERAL_RATIO_PRECISION - collateralRatio) * (PRECISION - redemptionFee) * _dollarPrice) / COLLATERAL_RATIO_PRECISION / PRECISION / _sharePrice;
        }
        if (collateralRatio > 0) {
            // _usdcOut = (_dollarIn * collateralRatio * (PRECISION - redemptionFee)) / COLLATERAL_RATIO_PRECISION / PRECISION / 10**missingDecimals;
            _usdcOut = (_dollarIn * collateralRatio * (PRECISION - redemptionFee) * _dollarPrice) / COLLATERAL_RATIO_PRECISION / PRECISION / _usdcPrice / 10**missingDecimals;
            // _fee = (_dollarIn * collateralRatio * redemptionFee) / COLLATERAL_RATIO_PRECISION / PRECISION / 10**missingDecimals;
            _fee = (_dollarIn * collateralRatio * redemptionFee * _dollarPrice) / COLLATERAL_RATIO_PRECISION / PRECISION / _usdcPrice / 10**missingDecimals;
        }
    }

    /// @notice Update collateral ratio and adjust based on the TWAP price of DOLLAR
    function refreshCollateralRatio() public {
        require(collateralRatioPaused == false, "Comptroller::refreshCollateralRatio:Collateral Ratio has been paused");
        require(block.timestamp - lastRefreshCrTimestamp >= refreshCooldown, "Comptroller::refreshCollateralRatio:Must wait for the refresh cooldown since last refresh");
        // mock to wait real orable
        uint256 _dollarPrice = oracle.getDollarTWAP();
        if (_dollarPrice > (priceTarget + priceBand)) {
            if (collateralRatio < ratioStep) {
                collateralRatio = 0;
            } else {
                uint256 _newCollateralRatio = collateralRatio - ratioStep;
                if (_newCollateralRatio <= minCollateralRatio) {
                    _newCollateralRatio = minCollateralRatio;
                } else {
                    collateralRatio = _newCollateralRatio;
                }
            }
        } else if (_dollarPrice < priceTarget - priceBand) {
            if ((collateralRatio + ratioStep) > COLLATERAL_RATIO_MAX) {
                collateralRatio = COLLATERAL_RATIO_MAX;
            } else {
                collateralRatio = collateralRatio + ratioStep;
            }
        }
        lastRefreshCrTimestamp = block.timestamp;
        emit NewCollateralRatioSet(collateralRatio);
    }

    /// @notice mint DOLLAR from USDC
    /// @param _usdcIn amount USDC token want to mint
    /// @param _minDollarOut minimum amount DOLLAR token receive
    function mint(uint256 _usdcIn, uint256 _minDollarOut) external nonReentrant {
        require(!mintPaused, "Comptroller::mint:Minting is paused");
        require(_usdcIn > 0, "Comptroller::mint:USDC balance > 0");
        (uint256 _usdcNeedSwap, uint256 _dollarOut, uint256 _fee) = calculateMint(_usdcIn);
        (uint256 _sharePrice, , ) = oracle.getSpotPrices();
        require(_usdcIn >= _usdcNeedSwap, "Comptroller::mint: collateral not enough");
        require(_dollarOut >= _minDollarOut, "Comptroller::mint: out of slippage");

        if (_dollarOut > 0) {
            userInfo[msg.sender].dollarBalance = userInfo[msg.sender].dollarBalance + _dollarOut;
            unclaimedDollar = unclaimedDollar + _dollarOut;
        }

        collateral.safeTransferFrom(msg.sender, address(this), _usdcIn);
        if (_usdcNeedSwap > 0 && _sharePrice > 0) {
            collateral.safeIncreaseAllowance(address(swapStrategy), _usdcIn);
            swapStrategy.execute(_usdcNeedSwap, _sharePrice);
        }
        if (_fee > 0) {
            collateral.safeTransfer(treasury, _fee);
        }
        userInfo[msg.sender].lastAction = block.number;
        emit Mint(msg.sender, _dollarOut, _usdcIn, _fee);
    }

    /// @notice redeem DOLLAR to USDC and SHARE
    /// @param _dollarIn amount DOLLAR token want to redeem
    /// @param _minShareOut minimum amount SHARE token receive
    /// @param _minUsdcOut minimum amount USDC token receive
    function redeem(
        uint256 _dollarIn,
        uint256 _minShareOut,
        uint256 _minUsdcOut
    ) external nonReentrant {
        require(!redeemPaused, "Comptroller::redeem:Redeem is paused");
        require(_dollarIn > 0, "Comptroller::redeem:DOLLAR balance > 0");
        (uint256 _usdcOut, uint256 _shareOut, uint256 _fee) = calculateRedeem(_dollarIn);
        // Check if collateral balance meets and meet output expectation
        require(_usdcOut + _fee <= availableUsdcBalance(), "Comptroller::redeem: > USDC balance");
        require(_usdcOut >= _minUsdcOut && _shareOut >= _minShareOut, "Comptroller::redeem: >slippage");
        if (_usdcOut > 0) {
            userInfo[msg.sender].usdcBalance = userInfo[msg.sender].usdcBalance + _usdcOut;
            unclaimedUsdc = unclaimedUsdc + _usdcOut;
        }
        if (_shareOut > 0) {
            userInfo[msg.sender].shareBalance = userInfo[msg.sender].shareBalance + _shareOut;
            unclaimedShare = unclaimedShare + _shareOut;
        }
        if (_fee > 0) {
            collateral.safeTransfer(treasury, _fee);
        }

        userInfo[msg.sender].lastAction = block.number;
        // burn dollar in
        dollar.burn(msg.sender, _dollarIn);
        emit Redeem(msg.sender, _dollarIn, _usdcOut, _shareOut, _fee);
    }

    /// @notice collect all minting and redemption
    function collect() external nonReentrant {
        require(userInfo[msg.sender].lastAction < block.number, "Comptroller::collect: collect request invalid");
        uint256 _usdcOut = 0;
        uint256 _shareOut = 0;
        uint256 _dollarOut = 0;

        if (userInfo[msg.sender].usdcBalance > 0) {
            _usdcOut = userInfo[msg.sender].usdcBalance;
            userInfo[msg.sender].usdcBalance = 0;
            unclaimedUsdc = unclaimedUsdc - _usdcOut;
        }
        if (userInfo[msg.sender].dollarBalance > 0) {
            _dollarOut = userInfo[msg.sender].dollarBalance;
            userInfo[msg.sender].dollarBalance = 0;
            unclaimedDollar = unclaimedDollar - _dollarOut;
        }
        if (userInfo[msg.sender].shareBalance > 0) {
            _shareOut = userInfo[msg.sender].shareBalance;
            userInfo[msg.sender].shareBalance = 0;
            unclaimedShare = unclaimedShare - _dollarOut;
        }
        if (_usdcOut > 0) {
            collateral.safeTransfer(msg.sender, _usdcOut);
        }
        if (_dollarOut > 0) {
            dollar.mint(msg.sender, _dollarOut);
        }
        if (_shareOut > 0) {
            share.mint(msg.sender, _shareOut);
        }
        emit Collect(_usdcOut, _dollarOut, _shareOut);
    }

    function excessCollateralBalance() public view returns (uint256 _excess) {
        _excess = ((COLLATERAL_RATIO_MAX - collateralRatio) * availableUsdcBalance()) / COLLATERAL_RATIO_PRECISION;
    }

    /// @notice Turn on / off minting and redemption
    /// @param _mintPaused Paused or NotPaused Minting
    /// @param _redeemPaused Paused or NotPaused Redemption
    function toggle(bool _mintPaused, bool _redeemPaused) external onlyOwner {
        mintPaused = _mintPaused;
        redeemPaused = _redeemPaused;
        emit Toggled(_mintPaused, _redeemPaused);
    }

    /// @notice Set the protocol fees
    /// @param _mintingFee Minting fee in percentage
    /// @param _redemptionFee Redemption fee in percentage
    function setFees(uint256 _mintingFee, uint256 _redemptionFee) external onlyOwner {
        require(_mintingFee < MINTING_FEE_MAX, "Comptroller::setFees: > MINTING_FEE_MAX");
        require(_redemptionFee < REDEMPTION_FEE_MAX, "Comptroller::setFees: > REDEMPTION_FEE_MAX");
        mintingFee = _mintingFee;
        redemptionFee = _redemptionFee;
        emit UpdateFee(_mintingFee, _redemptionFee);
    }

    /// @notice Set the minimum Collateral Ratio
    /// @param _minCollateralRatio value of minimum Collateral Ratio in 1e6 precision
    function setMinCollateralRatio(uint256 _minCollateralRatio) external onlyOwner {
        require(_minCollateralRatio <= COLLATERAL_RATIO_MAX, "Comptroller::setMintCollateralRatio: > COLLATERAL_RATIO_MAX");
        minCollateralRatio = _minCollateralRatio;
        emit MinCollateralRatioUpdated(_minCollateralRatio);
    }

    /// @notice Configure variables related to Collateral Ratio
    /// @param _ratioStep Step which Collateral Ratio will be increased and decreased each updates
    /// @param _priceBand The collateral ratio will only be adjusted if current price move out of this band
    /// @param _refreshCooldown The minimum delay between each Collateral Ratio updates
    function setCollateralOptions(
        uint256 _ratioStep,
        uint256 _priceBand,
        uint256 _refreshCooldown
    ) external onlyOwner {
        require(_ratioStep <= MAX_RATIO_STEP, "Comptroller::setCollateralOptions: >MAX_RATIO_STEP");
        require(_refreshCooldown <= MAX_REFRESH_COOL_DOWN, "Comptroller::setCollateralOptions: >MAX_REFRESH_COOL_DOWN");
        ratioStep = _ratioStep;
        priceBand = _priceBand;
        refreshCooldown = _refreshCooldown;
        emit UpdateCollateralRatioOptions(_ratioStep, _priceBand, _refreshCooldown);
    }

    /// @notice Set new oracle address
    /// @param _oracle address of the oracle
    function setOracle(IMasterOracle _oracle) external onlyOwner {
        require(address(_oracle) != address(0), "Comptroller::setOracle: oracle invalid address");
        oracle = _oracle;
        emit OracleChanged(address(_oracle));
    }

    /// @notice set swap strategy
    /// @param _swapStrategy new swap strategy address
    function setCompSwapStrategy(ICompSwapStrategy _swapStrategy) external onlyOwner {
        require(address(_swapStrategy) != address(0), "Comptroller::setCompSwapStrategy: swapStrategy invalid address");
        swapStrategy = _swapStrategy;
        emit SwapStrategyChanged(address(_swapStrategy));
    }

    /// @notice set treasury
    /// @param _treasury new treasury address
    function setTreasury(address _treasury) external onlyOwner {
        require(address(_treasury) != address(0), "Comptroller::setTreasury: _treasury invalid address");
        treasury = _treasury;
        emit TreasuryChanged(_treasury);
    }

    // EVENTS
    event TreasuryChanged(address indexed _oracle);
    event SwapStrategyChanged(address indexed _swapStrategy);
    event OracleChanged(address indexed _oracle);
    event Toggled(bool _mintPaused, bool _redeemPaused);
    event Mint(address minter, uint256 dollarOut, uint256 usdcIn, uint256 fee);
    event Redeem(address redeemer, uint256 dollarIn, uint256 usdcOut, uint256 shareOut, uint256 fee);
    event NewCollateralRatioSet(uint256 _cr);
    event Collect(uint256 usdc, uint256 _dollar, uint256 _share);
    event MinCollateralRatioUpdated(uint256 _minCollateralRatio);
    event UpdateFee(uint256 _mintingFee, uint256 _redemptionFee);
    event UpdateCollateralRatioOptions(uint256 _ratioStep, uint256 _priceBand, uint256 _refreshCooldown);
}

