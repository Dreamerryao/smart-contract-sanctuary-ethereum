    // update reserves and, on the first call per block, price accumulators
    function _update(uint balance0, uint balance1) private {
        uint32 blockTimestamp = uint32(block.timestamp % 2**32);
        reserve0 = uint256(balance0);
        reserve1 = uint256(balance1);
        blockTimestampLast = blockTimestamp;
    }


    // this low-level function should be called from a contract which performs important safety checks
    function mint(address to,uint TotalamountA,uint amountA,uint amountB) external lock onlyRouter() returns (uint liquidity) {
        (uint256 _reserve0, uint256 _reserve1,) = getReserves(); // gas savings
        uint256 totalReserve;
        uint256 _token1locked=token1Locked;
        //check best uniswap v3 pool fee
        uint24 fee=bestPoolFee(token0,token1);
        require(fee>0,"There is no uniswap v3 pool with any fee that has this pair");
         if(_token1locked!=0){
             _token1locked=quoter.quoteExactInputSingle(token1,token0,fee,token1Locked,0);
         }
        if((_reserve0!=0)&&(_reserve1!=0)){
         totalReserve=_reserve0+quoter.quoteExactInputSingle(token1,token0,fee,_reserve1,0)+token0Locked+_token1locked;
        }
        uint _totalSupply = totalSupply(); // gas savings, must be defined here since totalSupply() can update in _mintFee
        if (_totalSupply == 0) {
            liquidity = TotalamountA-(MINIMUM_LIQUIDITY);
        } else {
            liquidity = (TotalamountA*_totalSupply/totalReserve);

        }
         
        require(liquidity > 0, 'FuturizeV1: INSUFFICIENT_LIQUIDITY_MINTED');
        _mint(to, liquidity);
        _update(amountA+_reserve0, amountB+_reserve1);

        emit Mint(msg.sender, liquidity);
    }

    // this low-level function should be called from a contract which performs important safety checks
    function burn(address to) external lock onlyRouter() returns (uint amount0, uint amount1) {
        address _token0 = token0;                                // gas savings
        address _token1 = token1;                                // gas savings
        (uint256 _reserve0, uint256 _reserve1,) = getReserves(); // gas savings
        uint balance0 = _reserve0;
        uint balance1 = _reserve1;
        uint liquidity = balanceOf(address(this));
        uint _totalSupply = totalSupply(); // gas savings, must be defined here since totalSupply() can update in _mintFee
        amount0 = liquidity*(balance0) / _totalSupply; // using balances ensures pro-rata distribution
        amount1 = liquidity*(balance1) / _totalSupply; // using balances ensures pro-rata distribution
        require(amount0 > 0 && amount1 > 0, 'FuturizeV1: INSUFFICIENT_LIQUIDITY_BURNED');
        _burn(address(this), liquidity);


        IERC20(_token0).transfer(to,amount0);
        IERC20(_token1).transfer(to,amount1);
        require((_reserve0>=amount0)&&(_reserve1>=amount1),"There is a problem with reserve-given amount in pair burn function");
        balance0 = _reserve0-amount0;
        balance1 = _reserve1-amount1;

        _update(balance0, balance1);
        emit Burn(msg.sender, amount0, amount1, to);
    }

   function Earned() public view returns (uint amount0,uint amount1){
        amount0= balanceOf(msg.sender)*(reserve0) / totalSupply();
        amount1= balanceOf(msg.sender)*(reserve1) / totalSupply();

    }


