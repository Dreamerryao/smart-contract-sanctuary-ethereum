 function getReserves() public view returns (uint256 _reserve0, uint256 _reserve1, uint32 _blockTimestampLast) {
        _reserve0 = reserve0;
        _reserve1 = reserve1;
        _blockTimestampLast = blockTimestampLast;
    }
//Checkes the remaining colleteral for the user
 function CheckPaidInterest(address user,uint id) public view returns (uint256 _PaidInterest){
     uint256 _leverage;
     positions[user][id].leverage==0?_leverage=1:_leverage=positions[user][id].leverage;
    //check the user's fee and remaining colleteral()
    if(positions[user][id].collateralToken==token0){
    _PaidInterest=(block.number-positions[user][id].latestDebtBlock)*_leverage*positions[user][id].amountIn*(FuturuzeInterest(futureInterest).getSupplyRate(token0Given+reserve0,token0Given,reserve0,0))/(15768000);  
    }else{
      _PaidInterest=(block.number-positions[user][id].latestDebtBlock)*_leverage*positions[user][id].amountIn*(FuturuzeInterest(futureInterest).getSupplyRate(token1Given+reserve1,token1Given,reserve1,0))/(15768000);  

    }
 }

