    uint256 givenAmount; // Amount of non colleteral token the user borrows for his colleteral from the pair
    uint256 lockedAmount; // Amount of tokens (the same type as colleteral token) the contract recieved after exchanging given token(non colleteral token, is given from pair for colleteral) 
    uint256 latestDebtBlock; // Each debt update this is reassigned 
    uint256 leverage; //The total leverage of the position
    uint256 lastFeePaid; // The last fee paid before closing/liquidating
