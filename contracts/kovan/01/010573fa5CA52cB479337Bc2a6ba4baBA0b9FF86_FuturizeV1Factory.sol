  /****************************************|
  |      FuturizeV1 Router                 |
  |_______________________________________*/

contract FuturizeV1Factory is Ownable{
using SafeERC20 for IERC20;

  /****************************************|
  |        Variables And Modifiers         |
  |_______________________________________*/

    address router;
    mapping(address => mapping(address => address)) public pairAddress;
    address[] public allPairs;
  /****************************************|
  |        Events                          | 
  |_______________________________________*/

    event PairCreated(address indexed token0, address indexed token1, address pair, uint);

  /****************************************|
  |     Constructor                        | 
  |_______________________________________*/

    constructor()  {

    }

    
  /****************************************|
  |     Pair Creation Functions            |
  |_______________________________________*/


    function allPairsLength() external view returns (uint) {
        return allPairs.length;
    }


    function createPair(address tokenA, address tokenB,address futureInterest) public returns (address _pair) {
        require(tokenA != tokenB, 'IFuturizeV1Factory: IDENTICAL_ADDRESSES');
        require(tokenA != address(0), 'IFuturizeV1Factory: ZERO_ADDRESS');
        require(tokenB != address(0), 'IFuturizeV1Factory: ZERO_ADDRESS');
        require(pairAddress[tokenA][tokenB] == address(0), 'IFuturizeV1Factory: PAIR_EXISTS'); // single check is sufficient
        FuturizeV1Pair pair=new FuturizeV1Pair(router,tokenA, tokenB,address(this),owner(),futureInterest);
        pairAddress[tokenA][tokenB] = address(pair);
        pairAddress[tokenB][tokenA] = address(pair); // populate mapping in the reverse direction
        allPairs.push(address(pair));
        emit PairCreated(tokenA, tokenB, address(pair), allPairs.length);
        _pair=address(pair);
    }

        function setRouter(address _router) public onlyOwner() {
        router=_router;
    }
  /****************************************|
  |     Non-Mutable Functions              |
  |_______________________________________*/


        function getPair(address tokenA,address tokenB) public view returns (address _pair){
        _pair = pairAddress[tokenA][tokenB];
}
}

