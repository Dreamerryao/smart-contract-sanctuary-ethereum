/**
*@dev helper function that returns worth of current withdrawable liquidity for particular user for token0 and token1
* what is locked in the sidepool vaults is not withdrawable
*/
function liquidityBalance(address _user) public view returns (uint256 amount0, uint256 amount1) {
    amount0 = (balanceOf(_user) * reserve0) / totalSupply(); // using balances ensures pro-rata distribution
    amount1 = (balanceOf(_user) * reserve1) / totalSupply(); // using balances ensures pro-rata distribution
}
/**
*@dev helper function that finds the uniswap's pool fee with most liquidity
*/
function bestPoolFee(address tokenA,address tokenB) public view returns (uint24 fee){
 address pool1=uniswapFactory.getPool(tokenA,tokenB,1000);
 address pool2=uniswapFactory.getPool(tokenA,tokenB,3000);
 address pool3=uniswapFactory.getPool(tokenA,tokenB,10000);
 uint liquidity1;
 uint liquidity2;
 uint liquidity3;
 pool1==address(0) ? liquidity1=0 : liquidity1=IUniswapV3Pool(pool1).liquidity();
 pool2==address(0) ? liquidity2=0 : liquidity2=IUniswapV3Pool(pool2).liquidity();
 pool3==address(0) ? liquidity3=0 : liquidity3=IUniswapV3Pool(pool3).liquidity();
 if((liquidity1==0) && (liquidity2==0) && (liquidity3==0)){
  fee=0;
 }

 uint maxFirst=max(liquidity1,liquidity2);
 uint maxFee=max(maxFirst,liquidity3);
 if(maxFee==liquidity1){
  fee=1000;
 }
 if(maxFee==liquidity2){
  fee=3000;
 }
 if(maxFee==liquidity3){
 fee=10000;
 }
}




