      function openPosition(address collateralToken,address sender ,uint amount,uint minGiven,uint _position,uint24 dexFee,uint leverage) public onlyRouter() {
       require(leverage>=1 && leverage<=1024,"leverage not in range, range is 1-1024");
       last_free_pos_id[sender]=last_free_pos_id[sender]+1;
       _position=last_free_pos_id[sender];
       //Save much colleteral is sent in total.
       (collateralToken==token0) ? token0Colleteral=token0Colleteral+amount : token1Colleteral=token1Colleteral+amount;
       //Assign tokens according to pair.
       positions[sender][_position].collateralToken=(collateralToken==token0) ? token0 : token1;
       address _noncolleteral=(collateralToken==token0) ? token1 : token0;
       //Save user's position.
       positions[sender][_position].leverage=leverage;
       positions[sender][_position].amountIn=positions[sender][_position].amountIn+amount;
       //check best uniswap v3 pool fee
       uint24 fee=bestPoolFee(positions[sender][_position].collateralToken,_noncolleteral);
       require(fee>0,"There is no uniswap v3 pool with any fee that has this pair");
        //Check on dex how much given non colleteral token should be given for colleteral token.
       uint256 given = quoter.quoteExactInputSingle(positions[sender][_position].collateralToken,_noncolleteral,fee,leverage*amount,0);
        //save how much non colleteral is given and exhacnged in total. Decerase this amount from the appropriate reserve.
        (collateralToken==token0) ?  require(reserve1>given,"Not enough reserve of non colleteral token to exchange"):  require(reserve0>given,"Not enough reserve of non colleteral token to exchange");
        (collateralToken==token0) ? token1Given=token1Given+given : token0Given=token0Given+given;
        (collateralToken==token0) ? reserve1=reserve1-given : reserve0=reserve0-given;
        positions[sender][_position].givenAmount=positions[sender][_position].givenAmount+given;
        // Approve the router to spend non colleteral token for the given amount.
        TransferHelper.safeApprove(_noncolleteral, address(swapRouter), given);
         // Swap in dex non colleteral given amount from pair's reserve to more colleteral token which will be locked for user.Dex fee is according to pool for example 3000 for 0.3%
        ISwapRouter.ExactInputSingleParams memory params =
        ISwapRouter.ExactInputSingleParams({
        tokenIn: _noncolleteral,
        tokenOut: positions[sender][_position].collateralToken,
        fee: fee,
        recipient: address(this),
        deadline: block.timestamp+10,
        amountIn: given,
        amountOutMinimum: minGiven,
        sqrtPriceLimitX96: 0
        });
        uint amountOut = swapRouter.exactInputSingle(params);
        // Save how much extra colleteral tokens are locked for each user and in total.
        positions[sender][_position].lockedAmount=positions[sender][_position].lockedAmount+amountOut;
        (collateralToken==token0) ? token0Locked=token0Locked+amountOut : token1Locked=token1Locked+amountOut;
        positions[sender][_position].latestDebtBlock=block.number;
        emit PositionOpened(_position,sender,positions[sender][_position].collateralToken,_noncolleteral,amount,given,leverage);

    }

    function addPositionColleteral(address user,uint id,address collateralToken ,uint amount) public{
        require(positions[user][id].amountIn>0,"No such position with liquidity exists");
        require((!positions[user][id].liquidated )&& (!positions[user][id].closed),"Position is liquidated or closed");
        require(positions[user][id].collateralToken==collateralToken,"Position Updated With Wrong Colleteral Token");
        //updated the user's colleteral with his remaining colleteral
        positions[user][id].amountIn=positions[user][id].amountIn+amount;
        //update the appropriate colleteral token total amount 
        (collateralToken==token0) ? token0Colleteral=token0Colleteral+amount: token1Colleteral=token1Colleteral+amount;
    }
    
    //Happens after the router has checked the user transfered enough colleteral to close the position and the money was transfered to the pair.
 function closePosition(address user,uint id,uint minColleteral,uint24 dexFee) internal {
    require((!positions[user][id].liquidated )&& (!positions[user][id].closed),"Position is liquidated or closed");
     uint amountIn=positions[user][id].amountIn;
   //amountIn=amountIn-_interest;
    //assign colleteral token
    address _colleteral=(positions[user][id].collateralToken==token0) ? token0 : token1;
    address _noncolleteral=(positions[user][id].collateralToken==token0) ? token1 : token0;
    //update the appropriate colleteral token total amount 
    (_colleteral==token0) ? token0Colleteral=token0Colleteral-amountIn: token1Colleteral=token1Colleteral-amountIn;
    //check best uniswap v3 pool fee
    require(bestPoolFee(_colleteral,_noncolleteral)>0,"There is no uniswap v3 pool with any fee that has this pair");
    uint256 given = quoter.quoteExactInputSingle(_colleteral,_noncolleteral,bestPoolFee(_colleteral,_noncolleteral),amountIn+positions[user][id].lockedAmount-CheckPaidInterest(user,id),0);
    //If the non colleteral given token's amout is smaller than what originally given, user needs to cover the amount before closing.Frontend needs to calculate this amount if it exists and approve it before the transaction.
    require(positions[user][id].givenAmount<given,"Position is at loss you cannot close it unless you add colleteral");
    //Approve Dex to take back colleteral for the original non colleteral tokens
    TransferHelper.safeApprove(_colleteral, address(swapRouter), positions[user][id].lockedAmount+amountIn-CheckPaidInterest(user,id));
        // Swap in dex colleteral locked to given non colleteral token which will be returned to appropriate reserve;
        ISwapRouter.ExactInputSingleParams memory params =
        ISwapRouter.ExactInputSingleParams({
        tokenIn: _colleteral,
        tokenOut: _noncolleteral,
        fee: bestPoolFee(_colleteral,_noncolleteral),
        recipient: address(this),
        deadline: block.timestamp+10,
        amountIn: positions[user][id].lockedAmount+amountIn-CheckPaidInterest(user,id),
        amountOutMinimum: minColleteral,
        sqrtPriceLimitX96: 0
        });
        uint amountOut = swapRouter.exactInputSingle(params);
        uint profit=amountOut-positions[user][id].givenAmount;
        //update the colleteral reserve with the interest
        (_colleteral==token0) ? reserve0=reserve0+CheckPaidInterest(user,id): reserve1=reserve1+CheckPaidInterest(user,id);
        //update the colleteral reserve with the new tokens received in exchange for the locked colleteral minus the profit
        (_colleteral==token0) ? reserve1=reserve1+positions[user][id].givenAmount: reserve0=reserve0+positions[user][id].givenAmount;
         //update the total given amount with the new tokens received in exchange for the locked colleteral minus the profit
        (_colleteral==token0) ? token1Given=token1Given-positions[user][id].givenAmount: token0Given=token0Given-positions[user][id].givenAmount;
        //update the total locked amount with the decrease total locked amount
        (_colleteral==token0) ? token0Locked=token0Locked-positions[user][id].lockedAmount : token1Locked=token1Locked-positions[user][id].lockedAmount;
        //transfer the user his profit and original locked amount

        console.log("non colleteral profit given after close position: ",profit);
        uint256 colleteralProfit = quoter.quoteExactInputSingle(_noncolleteral,_colleteral,bestPoolFee(_colleteral,_noncolleteral),profit,0);
        console.log("colleteral profit sent after close position: ",colleteralProfit);
        IERC20(_colleteral).transfer(user,colleteralProfit);
        //close the user's position
        positions[user][id].lastFeePaid=CheckPaidInterest(user,id);
         positions[user][id].latestDebtBlock=block.number;
         positions[user][id].closed=true;
         uint givenAmount=positions[user][id].givenAmount;
         emit PositionClosed(id,user,_colleteral,_noncolleteral,givenAmount,colleteralProfit);

 }

 function close(address to,uint id,uint minColleteral) external {
     closePosition(to,id,minColleteral,3000);
 }

 function updateInterestContract(address _interest) external onlyRouter() {
     futureInterest=_interest;
 }

//liquidate position
 function liquidatePosition(address user,uint id,uint minColleteral,uint24 dexFee) external onlyRouter() {
     require((!positions[user][id].liquidated )&& (!positions[user][id].closed),"Position is liquidated or closed");
    //assign colleteral token
    address _colleteral=(positions[user][id].collateralToken==token0) ? token0 : token1;
    address _noncolleteral=(positions[user][id].collateralToken==token0) ? token1 : token0;
    //check best uniswap v3 pool fee
    require(bestPoolFee(_colleteral,_noncolleteral)>0,"There is no uniswap v3 pool with any fee that has this pair");
    //If the non colleteral given token's amout is smaller than what originally given, user needs to cover the amount before closing.Frontend needs to calculate this amount if it exists and approve it before the transaction.
     uint256 given = quoter.quoteExactInputSingle(_colleteral,_noncolleteral,bestPoolFee(_colleteral,_noncolleteral),positions[user][id].amountIn+positions[user][id].lockedAmount-CheckPaidInterest(user,id),0);

    require(positions[user][id].givenAmount>given,"Position is not at a loss you cannot liquidate it");
    //Approve Dex to take back colleteral for the original non colleteral tokens
    TransferHelper.safeApprove(_colleteral, address(swapRouter), positions[user][id].lockedAmount);
    //before swapping check ratio in uniswap. If we are smaller than the ratio add to top if higher add to low
        // Swap in dex colleteral locked to given non colleteral token which will be returned to appropriate reserve;
        ISwapRouter.ExactInputSingleParams memory params =
            ISwapRouter.ExactInputSingleParams({
                tokenIn: _colleteral,
                tokenOut: _noncolleteral,
                fee: bestPoolFee(_colleteral,_noncolleteral),
                recipient: address(this),
                deadline: block.timestamp+10,
                amountIn: positions[user][id].lockedAmount,
                amountOutMinimum: minColleteral,
                sqrtPriceLimitX96: 0
            });
        uint amountOut = swapRouter.exactInputSingle(params);
        //update the non colleteral reserve with the new tokens received in exchange for the locked colleteral
        (_colleteral==token0) ? reserve1=reserve1+amountOut: reserve0=reserve0+amountOut;
         (_colleteral==token0) ? token0Locked=token0Locked-positions[user][id].lockedAmount : token1Locked=token1Locked-positions[user][id].lockedAmount;
         //close the user's position
        positions[user][id].lastFeePaid=CheckPaidInterest(user,id);
         positions[user][id].latestDebtBlock=block.number;
         positions[user][id].liquidated=true;
         emit PositionLiquidate(id,user,_colleteral,_noncolleteral,amountOut);
     
 }


function updateAmountIn(address user,uint id) public{
    uint256 interest=CheckPaidInterest(user,id);
    address _colleteral=(positions[user][id].collateralToken==token0) ? token0 : token1;
     //update the colleteral reserve with the interest
    (_colleteral==token0) ? reserve0=reserve0+interest: reserve1=reserve1+interest;
    //update user's amount in with the new amount meaning minus the interest
    positions[user][id].amountIn=positions[user][id].amountIn-interest;
      //update the appropriate colleteral token total amount 
    (_colleteral==token0) ? token0Colleteral=token0Colleteral-interest: token1Colleteral=token1Colleteral-interest;
}


