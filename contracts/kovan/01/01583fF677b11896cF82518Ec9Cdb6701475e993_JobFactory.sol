/// @title The Labour Contract that acts as the legal contract between the client and the worker
contract Job is Initializable {

    address public factory;
    address public client;
    address public worker;
    address[] public applicants;
    string public ipfsHash;

    mapping(address => uint256) private addressToIndexApplicants;

    // called once by the factory at time of deployment
    function initialize(address _client) public {
        // Reinstate once the factory pattern is implemented
        //require(msg.sender == factory, "Job: FORBIDDEN"); // sufficient check
        client = _client;
        status = JobStatus.HIRING;
    }

    function setHash(string memory _ipfsHash) public {
        ipfsHash = _ipfsHash;
    }

    /**
     *  Hiring     = The Job has been initialized and is actively looking for workers
     *  ACTIVE     = The Job has succesfully hired and candidate(s) are actively performing work
     *  CANCELED  = The Job has been canceled for whatever reason
    */
    enum JobStatus {
        HIRING,
        ACTIVE,
        CANCELED
    }
    JobStatus status;

    event WorkerApplied(address userAddress);
    event WorkerHired(address userAddress);
    event JobCanceled();

    /**
     * @dev     Lets workers apply for this job. This is a very crude solution as it's expensive 
     *          due to 2 state changes. Since its only for expressing intent / interest in a job, 
     *          another approach should be used. One idea is to only emit an event. Another is to sign 
     *          messages off-chain and store those in a centralised database. 
     * @return  success  
    */
    function applyForJob() public returns (bool success) {
        require(status == JobStatus.HIRING, "Not open for hiring");
        require(msg.sender != client, "You can not hire yourself");

        // Should require that applicants have created an account. 
        // Import a IProfile interface and validate they have a user

        applicants.push(msg.sender);
        addressToIndexApplicants[msg.sender] = applicants.length - 1;

        emit WorkerApplied(msg.sender);

        return true;
    }

    /**
     * @dev             Lets a client hire a worker. 
     * @param _worker   the address of the worker to hire
     * @return          success  
    */
    function hire(address _worker) public returns (bool success) {
        require(msg.sender == client, "Only the client can hire");
        require(_worker != client, "Client can't hire itself");

        // Should require that applicants have created an account. 
        // Import a IProfile interface and validate they have a user

        worker = _worker;
        status = JobStatus.ACTIVE;

        emit WorkerHired(_worker);

        return true;
    }

    /**
     * @dev     Lets a client cancel the job for whatever reason
     * @return  success  
    */
    function cancelJob() public returns (bool success) {
        require(msg.sender == client, "!client");

        status = JobStatus.CANCELED;

        emit JobCanceled();

        return true;
    }

    /**
        @dev    Returns a user if exist by its address
        @param  userAddress The address of the user
    */
    function getApplicantByAddress(address userAddress)
        public
        view
        returns (
            uint256 index
        )
    {
        require(index < applicants.length);
        return addressToIndexApplicants[userAddress];
    }

    // /**
    //  *  @dev    Returns a user if exist by its address
    // */
    // function getJobInfo()
    //     public
    //     view
    //     returns (
    //         string memory ipfsHash,
    //         address client,
    //         address worker
    //     )
    // {
    //     console.log('lol Im inside getJobInfo',client);
    //     return (
    //         ipfsHash,
    //         client,
    //         worker
    //     );
    // }
}

