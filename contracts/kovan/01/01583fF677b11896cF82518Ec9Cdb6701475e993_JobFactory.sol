pragma solidity ^0.5.3;

import "hardhat/console.sol";
import "@openzeppelin/upgrades/contracts/upgradeability/ProxyFactory.sol";
import "./Job.sol";

contract JobFactory is ProxyFactory {
    // https://forum.openzeppelin.com/t/how-to-deploy-minimal-proxies-using-proxyfactory-in-truffle/3447/2
    // https://blog.logrocket.com/creating-contract-factory-clone-solidity-smart-contracts/#:~:text=The%20factory%20pattern,-The%20factory%20design&text=In%20Solidity%2C%20a%20factory%20is,factory%20technique%20comes%20in%20handy.
    // https://blog.openzeppelin.com/getting-the-most-out-of-create2/
    // https://betterprogramming.pub/learn-solidity-the-factory-pattern-75d11c3e7d29

    address[] public jobs;
    //mapping(address => mapping(address => address)) public getJob;
    mapping(address => address) private getJob;
    event JobCreated(address indexed jobAddress);

    // /**
    //  * @dev             Responsible for creating a job contract. 
    //  *                  Uses the factory - proxy - implementation contract pattern, where a factoryContract creates 
    //  *                  proxy contracts that all references a single implementation contract that contains all logic.  
    //  * @param logic     the address of the contract containing implementation logic for the proxies
    //  * @return          success  
    //  * TODO:         
    //         Should require that applicants have created an account. 
    //         Import a IProfile interface and validate they have a user
    // */
    function createJob(address logic, string calldata ipfsHash) external {

        bytes memory payload = abi.encodeWithSignature("initialize(address)", msg.sender);

        // Deploy minimal proxy
        address jobAddress = deployMinimal(logic, payload);
        Job(jobAddress).setHash(ipfsHash);

        getJob[jobAddress] = msg.sender;
        jobs.push(jobAddress);

        emit JobCreated(jobAddress);
    }

    // function createJob(address _logic, string calldata _ipfsHash) external returns (address node) {

    //     //create a new node
    //     bytes memory bytecode = type(Job).creationCode;
    //     bytes32 salt = keccak256(
    //         abi.encodePacked(msg.sender, _ipfsHash)
    //     );
    //     assembly {
    //         node := create2(0, add(bytecode, 32), mload(bytecode), salt)
    //     }
    //     Job(node).initialize(
    //         msg.sender,
    //         _ipfsHash
    //     );
    //     return node;
    // }

    /**
     * @dev     returns the count of all jobs created
     * @return  count  
    */
    function jobsCount() external view returns (uint count) {
        return jobs.length;
    }
}

