                product_list[productId].debt = setProductDebt(product, 0, 0, true);
                product_list[productId].balance += deposit;
            } else if (withdraw != 0) {
                //update position by last debt
                updatePosition(user, product, 0, withdraw);
                product_list[productId].lending_share = setProductLendingShare(product, 0, withdraw);
                product_list[productId].debt = setProductDebt(product, 0, 0, true);
                product_list[productId].balance -= withdraw;
            } else if (borrow != 0) {
                //update loan by last debt
                updateLoan(user, product, borrow, 0);
                product_list[productId].borrowing_share = setProductBorrowingShare(product, borrow, 0);
                product_list[productId].debt = setProductDebt(product, borrow, 0, true);
                product_list[productId].balance -= borrow;
            } else if (repay != 0) {
                //update loan by last debt
                updateLoan(user, product, 0, repay);
                product_list[productId].borrowing_share = setProductBorrowingShare(product, 0, repay);
                product_list[productId].debt = setProductDebt(product, 0, repay, true);
                product_list[productId].balance += repay;
            }
        }
        product_list[productId].lastest_change_time = block.timestamp;
    }

    /**
    * Go through the positions to update by product.
    **/
    function updatePosition(address user, Product memory product, uint deposit, uint withdraw) private {
        uint size = position_map[user].length;
        for (uint i=0; i<size; i++) {
            //in the array
            if (position_map[user][i].product_id == product.product_id) {
                lenderShare(user, product, i, deposit, withdraw, true);
                return;
            }
        }
        //not in the array
        lenderShare(user, product, size, deposit, withdraw, false);
    }

    /**
    * Postions upgrade strategy by address.
    **/
    function lenderShare(address user, Product memory product, uint index, uint deposit, uint withdraw, bool isIn) private {
        if (!isIn) {
            position_map[user].push(Position(product.product_id, 0));
        }
        if (product.debt == 0) {
            position_map[user][index].share += deposit;
        } else {
