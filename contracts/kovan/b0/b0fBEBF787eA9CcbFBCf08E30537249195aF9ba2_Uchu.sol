contract Uchu is Initializable {
    event Log(address from, string operation, uint value);
    address payable public owner;

    /**
    * asset:ETH = 10, USDT = 20, USDC = 21, WBTC = 30
    * the frontend need decode the number to string name for display.
    * product_id start from 0.
    */
    struct Product {
        uint8 product_id;
        uint8 asset;
        string product_name;
        uint balance;
        uint debt;
        uint lending_share;
        uint borrowing_share;
        uint lastest_change_time;
        uint16 borrow_rate_factor;
        uint32 borrow_rate_granularity;
        uint16 compound_period;
        address creator;
    }

    struct Position {
        uint8 product_id;
        uint share;
    }

    struct Loan {
        uint8 product_id;
        uint share;
    }

    uint8 private proCount;
    Product[] private product_list;
    mapping (address => Loan[]) private loan_map;
    mapping (address => Position[]) private position_map;
    mapping (uint8 => address) private asset_token_map;

    function initialize() public payable initializer {
        owner = payable(msg.sender);
        asset_token_map[20] = 0x13512979ADE267AB5100878E2e0f485B568328a4;
        asset_token_map[21] = 0xe22da380ee6B445bb8273C81944ADEB6E8450422;
        asset_token_map[30] = 0xD1B98B6607330172f1D991521145A22BCe793277;
    }

    function createProduct(uint8 asset, string memory name, uint16 borrow_rate_factor, uint32 borrow_rate_granularity, uint16 compound_period) public returns (bool) {
        uint16 rateFactor = borrow_rate_factor;
        uint32 rateGranularity = borrow_rate_granularity;
        uint16 period = compound_period;
        if (borrow_rate_factor == 0)
            rateFactor = 20;
        if (borrow_rate_granularity == 0)
            rateGranularity = 1000;
        if (compound_period == 0)
            period = 300;
        product_list.push(Product(proCount++, asset, name, 0, 0, 0, 0, block.timestamp, rateFactor, rateGranularity, period, msg.sender));
        return true;
    }

    function getProducts() public view returns (Product[] memory) {
        return product_list;
    }

    function getProduct(uint8 productId) public view returns (Product memory) {
        return product_list[productId];
    }

    /**
    * start: the start millisecond
    * end : the end millisecond
    * return: natural days
    **/
    function getPeriod(uint start, uint end, Product memory product) private pure returns (uint16) {
        if (start > end) {
            return 0;
        }
        return uint16(end / product.compound_period * 1 seconds - start / product.compound_period * 1 seconds);
    }

    function getPositions(address user) public view returns (Position[] memory) {
        return position_map[user];
    }

    function getLoans(address user) public view returns (Loan[] memory) {
        return loan_map[user];
    }

    function isProduct(uint8 productId) private returns (bool) {
        if (product_list[productId].asset != 0) {
            return true;
        }
        return false;
    }

    /**
    * Deposit by current productId
    **/
    function deposit(address user, uint8 productId) public payable returns (bool) {
        bool isIn = isProduct(productId);
        if (isIn) {
            Product memory product = product_list[productId];
            if (product.asset != 10) {
                bytes memory method = abi.encodeWithSignature("transferFrom(address,address,uint256)", user, address(this), msg.value);
                (bool success, bytes memory data) = asset_token_map[product.asset].call(method);
                require(success, "Deposit failed!");
            }
            updateProduct(user, productId, msg.value, 0, 0, 0);
            return true;
        } else {
            return false;
        }
    }

    function withdraw(address user, uint8 productId, uint amount) public returns (bool) {
        Product memory product = product_list[productId];
        if(product.balance >= amount && product.debt != 0) {
            if (product.asset != 10) {
                bytes memory method = abi.encodeWithSignature("transfer(address,uint256)", user, amount);
                (bool success, bytes memory data) = asset_token_map[product.asset].call(method);
                require(success, "Withdraw failed!");
            } else {
                payable(user).transfer(amount);
            }
            updateProduct(user, productId, 0, amount, 0, 0);
            return true;
        } else {
            return false;
        }
    }
    
    function borrow(address user, uint8 productId, uint amount) public returns (bool) {
        Product memory product = product_list[productId];
        if (product.balance >= amount) {
            if (product.asset != 10) {
                bytes memory method = abi.encodeWithSignature("transfer(address,uint256)", user, amount);
                (bool success, bytes memory data) = asset_token_map[product.asset].call(method);
                require(success, "Borrow failed!");
            } else {
                payable(user).transfer(amount);
            }
            updateProduct(user, productId, 0, 0, amount, 0);
            return true;
        } else {
            return false;
        }
    }

    function repay(address user, uint8 productId) public payable returns (bool) {
        bool isIn = isProduct(productId);
        if (isIn) {
            Product memory product = product_list[productId];
            if (product.asset != 10) {
                bytes memory method = abi.encodeWithSignature("transferFrom(address,address,uint256)", user, address(this), msg.value);
                (bool success, bytes memory data) = asset_token_map[product.asset].call(method);
                require(success, "Borrow failed!");
            }
            updateProduct(user, productId, 0, 0, 0, msg.value);
            return true;
        } else {
            return false;
        }
    }

    /**
    * Go through the products to update by productId.
    **/
    function updateProduct(address user, uint8 productId, uint deposit, uint withdraw, uint borrow, uint repay) private {
        Product memory product = product_list[productId];
        if (product_list[productId].debt == 0) {
            if (deposit != 0) {
                updatePosition(user, product, deposit, 0);
                product_list[productId].lending_share += deposit;
                product_list[productId].balance += deposit;
            } else if (borrow != 0) {
                product_list[productId].debt = setProductDebt(product, 0, 0, false);
