            return realDebt * uint(int(ABDKMath64x64.pow(sum, pow))) / uint(multiple);
        } else {
            return realDebt * numerator / denominator;
        }
    }

    /**
    * return the total borrowing share.
    **/
    function setProductBorrowingShare(Product memory product, uint borrow, uint repay) private returns (uint) {
        if (product.borrowing_share == 0) {
            return borrow - repay;
        }
        uint realDebt = setProductDebt(product, 0, 0, false);
        return product.borrowing_share * (realDebt + borrow - repay) / realDebt;
    }

}