// File contracts/DrugMoney.sol


pragma solidity ^0.8.0;


contract DrugMoney is Ownable, Erc20 {
    constructor() Erc20("DrugMoney", "$USD", 18) {
        console.log("Deploying DrugMoney..");
    }

    function mint(address beneficiary, uint256 mintAmount) external returns (bool) {
        require(mintAmount > 0, "ERR_MINT_ZERO");

        mintInternal(beneficiary, mintAmount);
        emit Transfer(beneficiary, address(this), mintAmount);
        return true;
    }

    function burn(address holder, uint256 burnAmount) external returns (bool) {
        require(burnAmount > 0, "ERR_BURN_ZERO");

        burnInternal(holder, burnAmount);
        emit Transfer(holder, address(this), burnAmount);
        return true;
    }
}


// File contracts/LoanSharkStorage.sol


pragma solidity ^0.8.0;




contract LoanSharkStorage {
    WrappedPunkInterface public wrappedPunksContract;
    Erc20Interface public daiContract;
    DrugMoney public drugMoneyContract;

    struct Loan {
        bool isExists;
        address borrower;
        CollateralType collateralType;
        bool isSettled;
        uint256 borrowAmount;
        uint256 collateralAmountOrIndex;
        uint256 collectedInterest;
        uint256 lastInterestCollectionTimestamp;
    }

    // Maps collateral type to interest rate of that type
    mapping(CollateralType => uint256) public interestRates;

    // Keeps record of all loans
    Loan[] public loanBook;

    // Whether a user has an unsettled loan
    mapping(address => Loan) public currentOpenLoan;
}


// File contracts/LoanShark.sol


pragma solidity ^0.8.0;





contract LoanShark is LoanSharkInterface, LoanSharkStorage, Ownable {
    constructor(
        WrappedPunkInterface wrappedPunksContract_,
        Erc20Interface daiContract_,
        DrugMoney drugMoneyContract_
    ) {
        wrappedPunksContract = wrappedPunksContract_;
        daiContract = daiContract_;
        drugMoneyContract = drugMoneyContract_;
        console.log("Deploying LoanShark..");
        console.log("Constructor params:");
        console.log("wrappedPunksContract:", address(wrappedPunksContract));
        console.log("daiContract:", address(daiContract));
        console.log("drugMoneyContract:", address(drugMoneyContract));

        interestRates[CollateralType.Punk] = 2;
        interestRates[CollateralType.Dai] = 5;
    }

    function setInterestRate(CollateralType collateralType, uint256 newInterestRate)
        external
        override
        onlyOwner
        returns (bool)
    {
        require(newInterestRate > 0, "LoanShark: interest rate is zero");
        interestRates[collateralType] = newInterestRate;
        return (true);
    }

    function borrow(
        uint256 borrowAmount,
        uint256 collateralAmountOrIndex,
        CollateralType collateralType
    ) external override {
        require(!currentOpenLoan[msg.sender].isExists, "LoanShark: user has an unsettled loan");
        require(borrowAmount > 0, "LoanShark: borrow amount is zero");

        uint256 collateralValue;
        if (collateralType == CollateralType.Punk) {
            collateralValue = 15_000 * (10**18);
            require(collateralValue > 0, "LoanShark: collateral value is zero");
            require(borrowAmount < collateralValue, "LoanShark: borrow amount is more than collateral value");
            // get the punk
            wrappedPunksContract.safeTransferFrom(msg.sender, address(this), collateralAmountOrIndex);
        } else {
            collateralValue = collateralAmountOrIndex;
            require(collateralValue > 0, "LoanShark: collateral value is zero");
            require(borrowAmount < collateralValue, "LoanShark: borrow amount is more than collateral value");
            // get the dai
            daiContract.transferFrom(msg.sender, address(this), collateralAmountOrIndex);
        }

        // mint and send the drugmoney
        drugMoneyContract.mint(msg.sender, borrowAmount);

        loanBook[loanBook.length] = Loan(
            true,
            msg.sender,
            collateralType,
            false,
            borrowAmount,
            collateralAmountOrIndex,
            0,
            block.timestamp
        );

        currentOpenLoan[msg.sender] = loanBook[loanBook.length];
    }

    function settle(uint256 loanIndex) external override returns (bool) {
        require(loanIndex < loanBook.length, "LoanShark: invalid loan index");

        Loan memory loan = loanBook[loanIndex];
        require(!this.isLoanUnderwater(loanIndex), "LoanShark: loan is underwater");
        require(msg.sender == loan.borrower, "LoanShark: caller is not the borrower");
        require(!loan.isSettled, "LoanShark: loan is already settled");

        // get the drugmoney
        drugMoneyContract.transferFrom(msg.sender, address(this), loan.borrowAmount + loan.collectedInterest);

        if (loan.collateralType == CollateralType.Punk) {
            // send the punk
            wrappedPunksContract.safeTransferFrom(address(this), msg.sender, loan.collateralAmountOrIndex);
        } else {
            // send the dai
            daiContract.transferFrom(address(this), msg.sender, loan.collateralAmountOrIndex);
        }

        loan.isSettled = true;
        loanBook[loanIndex] = loan;

        currentOpenLoan[msg.sender].isExists = false;

        return true;
    }

    function liquidationSettle(uint256 loanIndex) external override returns (bool) {
        require(loanIndex < loanBook.length, "LoanShark: invalid loan index");

        Loan memory loan = loanBook[loanIndex];
        require(this.isLoanUnderwater(loanIndex), "LoanShark: loan is not yet underwater");
        require(msg.sender == loan.borrower, "LoanShark: caller is not the borrower");
        require(!loan.isSettled, "LoanShark: loan is already settled");

        // get the drugmoney
        drugMoneyContract.transferFrom(msg.sender, address(this), loan.borrowAmount + loan.collectedInterest);

        loan.isSettled = true;
        loanBook[loanIndex] = loan;

        currentOpenLoan[msg.sender].isExists = false;

        return true;
    }

    // Mints all unrealized accured DrugMoney interest for all open loans and sends it to the buffer
    function realizeAllInterest() external override {
        for (uint256 i = 0; i < loanBook.length; i++) {
            Loan memory loan = loanBook[i];
            if (!this.isLoanUnderwater(i) && !loan.isSettled) {
                uint256 interestToCollect;
                uint256 collateralValue;
                if (loan.collateralType == CollateralType.Punk) {
                    collateralValue = 15_000 * (10**18);
                } else {
                    collateralValue = loan.collateralAmountOrIndex;
                }
                interestToCollect =
                    (((loan.borrowAmount * interestRates[loan.collateralType]) / 100) *
                        (block.timestamp - loan.lastInterestCollectionTimestamp)) /
                    (365 * 12 * 30 * 24 * 60 * 60 * 1000);
                if (interestToCollect + loan.collectedInterest + loan.borrowAmount > collateralValue) {
                    interestToCollect = collateralValue - loan.borrowAmount - loan.collectedInterest;
                }
                // mint and send the drugmoney
                drugMoneyContract.mint(address(1), interestToCollect);
                loan.collectedInterest += interestToCollect;
                loan.lastInterestCollectionTimestamp = block.timestamp;
                loanBook[i] = loan;
            }
        }
    }

    // Moves collateral for all liquidated loans to the graveyard
    function buryAllLiquidated() external override {
        for (uint256 i = 0; i < loanBook.length; i++) {
            Loan memory loan = loanBook[i];
            if (this.isLoanUnderwater(i)) {
                if (loan.collateralType == CollateralType.Punk) {
                    // send the punk to Graveyard
                    wrappedPunksContract.safeTransferFrom(address(this), address(1), loan.collateralAmountOrIndex);
                } else {
                    // send the dai to Graveyard
                    daiContract.transferFrom(address(this), address(1), loan.collateralAmountOrIndex);
                }
            }
        }
    }

    function isLoanUnderwater(uint256 loanIndex) external view override returns (bool) {
        Loan memory loan = loanBook[loanIndex];

        uint256 collateralValue;
        if (loan.collateralType == CollateralType.Punk) {
            collateralValue = 15_000 * (10**18);
        } else {
            collateralValue = loan.collateralAmountOrIndex;
        }

        return (loan.borrowAmount + loan.collectedInterest) >= collateralValue;
    }
}