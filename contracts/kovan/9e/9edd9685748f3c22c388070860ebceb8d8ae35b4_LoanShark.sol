enum CollateralType { Punk, Dai }

interface LoanSharkInterface {
    // Requires:
    // - Caller is the owner
    // - New interest rate > 0
    function setInterestRate(CollateralType collateralType, uint256 newInterestRate) external returns (bool);

    // Borrow an amount of DrugMoney and lock in DAI or a Punk as collateral
    // Note: assumes borrower already approved infinite allowance
    // Requires:
    // - User doesn't have an open (unsettled) loan
    // - Borrow amount is greater than zero
    // - Collateral value is greater than zero
    // - Borrow amount is less than the DrugMoney valuation of collateral
    function borrow(
        uint256 borrowAmount,
        uint256 collateralAmountOrIndex,
        CollateralType collateralType
    ) external;

    // Called by user to settle his open loan
    // Requires:
    // - Loan index is valid
    // - Caller is the borrower
    // - Loan is not already settled
    // - Loan is not underwater
    function settle(uint256 loanIndex) external returns (bool);

    // Called by user to liquidate his underwater loan
    // 
    // Requires:
    // - Loan index is valid
    // - Caller is the borrower
    // - Loan is underwater
    // - Loan is not already settled
    // - Ounstanding debt >=  DrugMoney valuation of collateral
    function liquidationSettle(uint256 loanIndex) external returns (bool);

    // Mints all unrealized accured DrugMoney interest for all open loans and sends it to the buffer
    function realizeAllInterest() external;

    // Moves collateral for all liquidated loans to the graveyard
    function buryAllLiquidated() external;

    // Indicates if the loan satisfies liquidation requirements.
    function isLoanUnderwater(uint256 loanIndex) external view returns (bool);
}


