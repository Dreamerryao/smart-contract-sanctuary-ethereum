        address _address,
        uint256 _tokenId,
        uint256 _prestige,
        uint256 _creationSeed,
        uint16 _grade,
        string name,
        uint256 _core
    );
    modifier isWhitelisted() {
        require(whitelist.whitelist(msg.sender) == true, "Not white listed");
        _;
    }

