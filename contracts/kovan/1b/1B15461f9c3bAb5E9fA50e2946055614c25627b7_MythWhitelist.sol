    }
}

/////////////////// PLAYER CARD
contract PlayerCard {
    mapping(address => uint256) public affiliateExp;
    mapping(address => uint256) public playerExp;
    mapping(address => address) public affiliate;
    mapping(address => equipped) public playerEquips;

    struct equipped {
        uint256 pet;
    }
    MythWhitelist public whitelist;

    constructor(address _address) {
        whitelist = MythWhitelist(_address);
    }

    function getEquipped(address _address)
        external
        view
        returns (equipped memory)
    {
        return playerEquips[_address];
    }

    modifier isWhitelisted() {
        require(whitelist.whitelist(msg.sender) == true, "Not white listed");
        _;
    }

    function equipPet(uint256 id) external {
        MythPets pets = MythPets(whitelist.petsAddress());
        require(pets.ownerOf(id) == msg.sender, "You don't own this item");
        playerEquips[msg.sender].pet = id;
    }

    function setWhiteList(address _address) external isWhitelisted {
        whitelist = MythWhitelist(_address);
    }

    function setAffiliate(address aff) external {
        affiliate[msg.sender] = aff;
    }

    function giveAffiliateExp(uint256 xp, address player)
        external
        isWhitelisted
    {
        if (player != address(0)) {
            affiliateExp[player] += xp;
        }
    }

    function givePlayerExp(uint256 xp, address player) external isWhitelisted {
        if (player != address(0)) {
            playerExp[player] += xp;
        }
    }

    //EXP to Levels
    //Level 0: 0 - 999
    //level 2: 1000 - 4999
    //level 4: 5000 - 14999
    //level 6: 15000 - 49999
    //level 8: 50000 - 249999
    //level 10: 250000 +
}

