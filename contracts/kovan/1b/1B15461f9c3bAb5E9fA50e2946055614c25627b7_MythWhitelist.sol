    function diceDuel(uint256 _core1, uint256 _core2)
        internal
        view
        returns (bool)
    {
        uint256 maxNumber = 1000;
        uint256 nonceLocal = 0;
        uint256 roller;
        while (true) {
            roller =
                uint256(
                    keccak256(
                        abi.encodePacked(
                            block.timestamp,
                            msg.sender,
                            nonceLocal
                        )
                    )
                ) %
                (_core1 + _core2);
            nonceLocal++;
            maxNumber =
                uint256(
                    keccak256(
                        abi.encodePacked(
                            block.timestamp,
                            msg.sender,
                            nonceLocal
                        )
                    )
                ) %
                maxNumber;
            nonceLocal++;
            if (maxNumber == 0) {
                if (roller < _core1) {
                    return true;
                } else {
                    return false;
                }
            }
        }
        return false;
    }

    function buyWithMyth(address _shopAddress, uint256 _item_id) external {
        require(whitelist[_shopAddress], "Shop isnt WhiteListed");
        MythShop shopContract = MythShop(_shopAddress);
        MythShop.storeItem memory itemToBuy = shopContract.getItem(_item_id);
        UtilityToken mythTokens = UtilityToken(mythAddress);
        require(itemToBuy.mythCost > 0, "Not Buyable with MYTH");
        mythTokens.transferFrom(msg.sender, owner, itemToBuy.mythCost);
    }

    function openWithEth(address _caseAddress) external payable returns (bool) {
        require(whitelist[_caseAddress], "Shop isnt WhiteListed");
        MythLootBox lootbox = MythLootBox(_caseAddress);
        MythLootBox.details memory caseStats = lootbox.getStats();
        require(caseStats.cost != 0, "Case cannot be bought");
        require(msg.value == caseStats.cost, "Send exact Change");
        uint256 transactionId = uint256(
            keccak256(
                abi.encodePacked(block.timestamp, msg.sender, address(this))
            )
        );
        uint256 rand = uint256(
            keccak256(abi.encodePacked(block.timestamp, msg.sender))
        );
        MythLootBox.prize memory prize = lootbox.getWinningPrize(rand % 100);
        emit openCase(
            transactionId,
            rand % 100,
            msg.sender,
            caseStats.name,
            _caseAddress
        );
        if (prize.prizeType == 1) {
            MythDegen mythdegen = MythDegen(degenAddress);
            uint256 prestige = uint256(keccak256(abi.encodePacked(rand + 1))) %
                10000;
            uint16 grade = 1;
            uint256 core = (uint256(keccak256(abi.encodePacked(rand + 2))) %
                prize.coreVariance) + prize.minCore;

            mythdegen.mint(prestige, rand, grade, "No Name", core, msg.sender);
            emit prizeMinted(
                transactionId,
                grade,
                prestige,
                core,
                0,
                0,
                0,
                msg.sender
            );
        } else if (prize.prizeType == 2) {
            MythPets pets = MythPets(petsAddress);
            pets.mint(msg.sender, prize.resourceBonus, (rand + 1) % 10000, 1);
            emit prizeMinted(
                transactionId, //transaction ID
                1, // Grade
                rand % 10000, // Prestige
                0, // Core
                prize.resourceBonus, // Resource Bonus %
                0, // Block Yield
                0, // Resource Type
                msg.sender
            );
        } else if (prize.prizeType == 3) {
            MythFarms farms = MythFarms(farmsAddress);
            farms.mint(
                prize.blockYield,
                "Myth Farm",
                msg.sender,
                prize.resourceType
            );
            emit prizeMinted(
                transactionId, //transaction ID
                0, // Grade
                0, // Prestige
                0, // Core
                0, // Resource Bonus %
                prize.blockYield, // Block Yield
                prize.resourceType, // Resource Type
                msg.sender
            );
        }

        return true;
    }

    function mintDegenWithRange(
        address _to,
        uint256 _randomNumber,
        uint256 _minCore,
        uint256 _coreVariance
    ) public isWhitelisted returns (bool) {
        MythDegen mythdegen = MythDegen(degenAddress);
        uint256 prestige = uint256(
