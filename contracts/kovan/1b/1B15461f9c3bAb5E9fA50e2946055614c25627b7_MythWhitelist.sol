        ) % 10000;
        uint16 grade = 1;
        uint256 core = (uint256(
            keccak256(abi.encodePacked(_randomNumber + 2))
        ) % _coreVariance) + _minCore;

        mythdegen.mint(prestige, _randomNumber, grade, "No Name", core, _to);

        return true;
    }

    function buyWithEth(address _shopAddress, uint256 _item_id)
