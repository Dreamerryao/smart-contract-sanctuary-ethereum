contract UtilityToken is ERC20 {
    address public owner;
    MythWhitelist public whitelist;

    constructor(
        address _address,
        string memory _name,
        string memory _symbol
    ) ERC20(_name, _symbol) {
        owner = msg.sender;
        whitelist = MythWhitelist(_address);
    }

