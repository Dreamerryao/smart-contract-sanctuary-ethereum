        require(ownerOf(_tokenId) == msg.sender, "You don't own this");
        getTokenStats[_tokenId].name = _name;
    }

    constructor(address _address) ERC721("Myth Degen", "DEGEN") {
        tokenCount = 0;
        owner = msg.sender;
        whitelist = MythWhitelist(_address);
    }

    function mint(
        uint256 _prestige,
        uint256 _creationSeed,
        uint16 _grade,
        string memory _name,
        uint256 _core,
        address _to
    ) external isWhitelisted returns (bool) {
        _safeMint(_to, tokenCount);

        getTokenStats[tokenCount] = tokenStats(
            _prestige,
            _creationSeed,
            _grade,
            _name,
            _core,
            ""
        );
        emit mintedToken(
            _to,
            tokenCount,
            _prestige,
            _creationSeed,
            _grade,
            _name,
            _core
        );
        tokenCount++;
    }

    function setImageURL(uint256 tokenId, string memory _imageURL) public {
        require(msg.sender == owner, "Only owner can set image urls.");
