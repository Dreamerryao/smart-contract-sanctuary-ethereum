contract ArkadeWhitelist {

    mapping(address => bool) public whitelist;
    mapping(address => uint) public caseArrayPointer;
    address[] public casesWhiteList;
    uint public casesContractCount;
    address public owner;
    address public prizePool;
    uint public nonce;
    address public arkadeToken;
    address public weaponaddress;
    address public playerToken;

    constructor(){
        owner = msg.sender;
        whitelist[msg.sender] = true;
        casesContractCount = 0;
        weaponaddress = msg.sender;
        arkadeToken = msg.sender;
        prizePool = msg.sender;
    }
    function setArkadeAddress(address addy) isWhitelisted external {
        arkadeToken = addy;
    }
    function setPrizePoolAddress(address addy) isWhitelisted external {
        prizePool = addy;
    }
    function setWeaponaddress(address addy) isWhitelisted external {
        weaponaddress = addy;
    }
    function setPlayeraddress(address addy) isWhitelisted external {
        playerToken = addy;
    }
    
    modifier isWhitelisted() {
        require(whitelist[msg.sender] == true, "Not white listed");
        _;
    }
    function whitelistAddress(address addy) external {
        require(msg.sender == owner, "Not the owner");
        whitelist[addy] = true;
    }

    function battleNPC(address _npc, uint _weaponID) external returns(bool){
        require(weaponaddress != owner, "weapon Address Is owner");
        require(arkadeToken != owner, "arkade Token is owner");
        require(whitelist[_npc], "NPC is not WhiteListed");
        BattleWeapon weaponNFT = BattleWeapon(weaponaddress);
        require(weaponNFT.ownerOf(_weaponID) == msg.sender, "You Do not own the weapon you want to use for the battle.");
        UtilityToken arkadeTokens = UtilityToken(arkadeToken);
        BattleNPC npcContract = BattleNPC(_npc);
        arkadeTokens.transferFrom(msg.sender, owner, npcContract.cost() * 10 ** 18);
        
        uint playerDamage = weaponNFT.getDamage(_weaponID);
        uint playerCrit = weaponNFT.getCrit(_weaponID);
        uint playerParry = weaponNFT.getParry(_weaponID);

        uint npcDamage = npcContract.damage();
        uint npcCrit = npcContract.crit();
        uint npcParry = npcContract.parry();
        bool winner = fight(playerDamage, playerCrit, playerParry, npcDamage, npcCrit, npcParry);
        if(winner){
            arkadeTokens.mint(msg.sender, (npcContract.cost() + npcContract.prize()) * 10 ** 18);
        }
        return winner;

    }

    function fight(uint _playerDamage, uint _playerCrit, uint _playerParry, uint _npcDamage, uint _npcCrit, uint _npcParry) internal pure returns(bool){
        uint seed = _playerDamage + _playerCrit * _playerParry % _npcCrit * _npcDamage;
        uint playerHealth = 2500;
        uint npcHealth = 2500;
        uint nonceLocal = 1;
