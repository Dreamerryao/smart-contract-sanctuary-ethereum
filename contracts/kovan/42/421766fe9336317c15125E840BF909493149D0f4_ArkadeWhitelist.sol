contract BattleWeapon is ERC721 {

    uint256 public tokenCount;
    mapping(uint256 => stats) private weaponStats;
    ArkadeWhitelist public whitelist;
    address public owner;

    struct stats {
        uint damage;
        string name;
        uint parry;
        uint crit;
        address owner;
        uint grade;
        uint upgradeCount;
        uint upgradeDamage;
        uint upgradeParry;
        uint upgradeCrit;
    }

    constructor() ERC721("BattleBot Weapon","WEP"){
        tokenCount = 0;
        owner = msg.sender;
        whitelist = ArkadeWhitelist(0xd8b934580fcE35a11B58C6D73aDeE468a2833fa8);
    }
    modifier isWhitelisted() {
        require(whitelist.whitelist(msg.sender) == true, "Not white listed");
        _;
    }
    function burnToken(address from, uint id) isWhitelisted external {
        _safeTransfer(from, owner, id, "");
        weaponStats[id].owner = owner;
    }


    function getStats(uint id) external view returns(stats memory){
        return weaponStats[id];
    }

    function getUpgrades(uint id) external view returns(uint[3] memory){
        
        uint[3] memory tempList;
        tempList[0] = weaponStats[id].upgradeDamage;
        tempList[1] = weaponStats[id].upgradeParry;
        tempList[2] = weaponStats[id].upgradeCrit;
        return tempList;
    }
    function getGrades(uint id) external view returns(uint){
        return weaponStats[id].grade;
    }

    function reUpGrade(uint id, uint damage, uint crit, uint parry) isWhitelisted external {
        weaponStats[id].upgradeDamage += damage;
        weaponStats[id].upgradeCrit += crit;
        weaponStats[id].upgradeParry += parry;
        weaponStats[id].grade +=1;
    }

    function reGrade(uint tokenId, uint amount, uint statToUpgrade) isWhitelisted external {
        require(statToUpgrade >= 0 && statToUpgrade <= 2, "Incorrect stat to upgrade");
        if ( statToUpgrade == 0){
            weaponStats[tokenId].upgradeDamage += (amount * 2);
        } else if ( statToUpgrade == 1){
            weaponStats[tokenId].upgradeCrit += amount;
        }else{
            weaponStats[tokenId].upgradeParry += amount;
        }
        weaponStats[tokenId].upgradeCount += 1;
    }

    
    function mint(
        address to,
        uint damage,
        string memory name,
        uint parry,
        uint crit) 
    isWhitelisted public {
        _safeMint(to, tokenCount);
        weaponStats[tokenCount] = stats(damage,name,parry,crit,to,1,0,0,0,0);
        tokenCount++;
    }

    function getDamage(uint id) external view returns(uint){
        return weaponStats[id].damage + weaponStats[id].upgradeDamage;
    }
    function getParry(uint id) external view returns(uint){
        return weaponStats[id].parry + weaponStats[id].upgradeParry;
    }
    function getCrit(uint id) external view returns(uint){
        return weaponStats[id].crit + weaponStats[id].upgradeCrit;
    }

    function uint2str(uint _i) internal pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (_i != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function concatenate(string memory a, string memory b) internal pure returns( string memory){
        return string(abi.encodePacked(a,b));
    }
        function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        //solhint-disable-next-line max-line-length
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");

        _transfer(from, to, tokenId);
        weaponStats[tokenId].owner = to;
    }

        function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _safeTransfer(from, to, tokenId, _data);
        weaponStats[tokenId].owner = to;
    }

    function tokenURI(uint256 tokenId) override(ERC721) public view returns (string memory) {
        string memory json = Base64.encode(
            bytes(string(
                abi.encodePacked(
                    '{ "name": "',weaponStats[tokenId].name, '",',
                    '"attributes": [{"trait_type": "Damage", "value": ', uint2str(weaponStats[tokenId].damage),'},{"trait_type": "Parry", "value": ', uint2str(weaponStats[tokenId].parry),'},{"trait_type": "Crit", "value": ', uint2str(weaponStats[tokenId].crit),'}]',
                    '}'
                )
            ))
        );
        return string(abi.encodePacked('data:application/json;base64,', json));
    } 

}
