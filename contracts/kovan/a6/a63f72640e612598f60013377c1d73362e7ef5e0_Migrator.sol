pragma solidity ^0.6.12;



contract Migrator is Ownable {
    using WadRayMath for uint256;
    using SafeMath for uint;
    using SafeERC20 for IERC20;
    uint constant defaultScale = 1e18;

    struct Destination {
        address gToken;
        uint256 category; //1：ctoken 2:aToken 3：lptoken
    }

    IWETH internal immutable WETH;
    address public gEth;
    address public farm;
    address public aLendingPool; // aave lending pool
    IFactory public origFactory01;
    IFactory public origFactory02;
    address public router;
    IFactory public factory;
    uint256 public startBlock;
    address public comptroller;
    address public origComptroller;
    uint256 public desiredLiquidity = uint(- 1);
    mapping(address => Destination) public destination;

    constructor(
        address _farm,
        address _router,
        address _origFactory01,
        address _origFactory02,
        address _factory,
        address _origComptroller,
        address _comptroller,
        address _gEth,
        address _aLendingPool,
        address _WETH,
        uint256 _startBlock) public {
        require(_farm != address(0), "farm error");
        farm = _farm;
        router = _router;
        origFactory01 = IFactory(_origFactory01);
        origFactory02 = IFactory(_origFactory02);
        factory = IFactory(_factory);
        origComptroller = _origComptroller;
        comptroller = _comptroller;
        startBlock = _startBlock;
        aLendingPool = _aLendingPool;
        gEth = _gEth;
        WETH = IWETH(_WETH);
    }

    function migrate(address[] calldata _tokens, uint256[] calldata _amounts) external  returns (address, address, uint256, uint256){
        require(farm == msg.sender, "Not farm");
        require(block.number > startBlock, "Too early");
        if (isLpToken(_tokens[0])) {
            //            lp token
            return migrateLpToken(_tokens, _amounts);
        } else {
            //            gToken
            return migrateGtoken(_tokens, _amounts);
        }
    }

    function isLpToken(address _token) public view returns (bool){
        Destination memory dst = destination[_token];
        if (dst.category == 3) {
            return true;
        }
        return false;
    }

    function isAtoken(address _token) public view returns (bool){
        Destination memory dst = destination[_token];
        if (dst.category == 2) {
            return true;
        }
    }

    function migrateLpToken(address[] calldata _tokens, uint256[] calldata _amounts) internal returns (
        address,
        address,
        uint256,
        uint256){
        require(IMigratorToken(_tokens[0]).factory() == address(origFactory01), "Original factory01 mismatch");
        (uint token0Amount,uint token1Amount,IUniswapV2Pair pairA) = removeLiquidity(_tokens[0], _amounts[0]);
        desiredLiquidity = _amounts[0];
        if (_tokens[1] != address(0)) {
            address lpToken02 = _tokens[1];
            uint256 amount02 = _amounts[1];
            require(IMigratorToken(lpToken02).factory() == address(origFactory02), "Original factory02 mismatch");
            (uint tokenB0,uint tokenB1,IUniswapV2Pair pairB) = removeLiquidity(lpToken02, amount02);
            require(address(pairA) == address(pairB), "Target pair inequality");
            (token0Amount, token1Amount) = handleExcess(pairA, pairB, token0Amount, token1Amount, tokenB0, tokenB1);
            desiredLiquidity = desiredLiquidity.add(amount02);
        }
        address token0 = pairA.token0();
        address token1 = pairA.token1();
        TransferHelper.safeApprove(token0, router, token0Amount);
        TransferHelper.safeApprove(token1, router, token1Amount);
        IUniswapV2Router02(router).addLiquidity(token0, token1, token0Amount, token1Amount, token0Amount, token1Amount, msg.sender, block.timestamp.add(3));
        desiredLiquidity = uint(- 1);
        TransferHelper.safeApprove(token0, router, 0);
        TransferHelper.safeApprove(token1, router, 0);
        return (address(pairA), address(0), 0, 0);
    }

    function handleExcess(
        IUniswapV2Pair pairA,
        IUniswapV2Pair pairB,
        uint tokenA0,
        uint tokenA1,
        uint tokenB0,
        uint tokenB1
    ) internal returns (uint token0Amount, uint token1Amount){
        if (pairA.token0() == pairB.token0()) {
            require(pairA.token1() == pairB.token1(), "Pair mismatch1");
            token0Amount = tokenA0.add(tokenB0);
            token1Amount = tokenA1.add(tokenB1);
        } else {
            require(pairA.token0() == pairB.token1(), "Pair mismatch2");
            require(pairA.token1() == pairB.token0(), "Pair mismatch3");
            token0Amount = tokenA0.add(tokenB1);
            token1Amount = tokenA1.add(tokenB0);
        }
    }

    function removeLiquidity(address _token, uint256 amount) internal returns (uint256 token0BalanceChange, uint256 token1BalanceChange, IUniswapV2Pair pair){
        IUniswapV2Pair origPair = IUniswapV2Pair(_token);
        address token0 = origPair.token0();
        address token1 = origPair.token1();
        pair = IUniswapV2Pair(factory.getPair(token0, token1));
        if (pair == IUniswapV2Pair(address(0))) {
            pair = IUniswapV2Pair(factory.createPair(token0, token1));
        }
        origPair.transferFrom(msg.sender, address(origPair), amount);
        uint256 token0balance = IERC20(token0).balanceOf(address(this));
        uint256 token1balance = IERC20(token1).balanceOf(address(this));
        origPair.burn(address(this));
        token0BalanceChange = IERC20(token0).balanceOf(address(this)).sub(token0balance);
        token1BalanceChange = IERC20(token1).balanceOf(address(this)).sub(token1balance);
    }

    function migrateGtoken(address[] calldata _tokens, uint256[] calldata _amounts) internal returns (address, address, uint256, uint256){
        require(_tokens[0] != address(0), "Param invalid");
        address token = _tokens[0];
        uint amount = _amounts[0];
        Destination memory dst = destination[token];
        require(dst.gToken != address(0), "Error gToken Address");
        IERC20(token).safeTransferFrom(farm, address(this), amount);
        if (dst.gToken == gEth) {
            //            aETh or cETh migrate to gEth.
            return migrateGTokenEth(token, amount);
        } else {
            //            aErc20 or cERc2o migrate to gErc20
            return migrateGTokenErc20(token, amount);
        }
    }

