pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./State.sol";

contract Lists is Ownable {
    State public state;

    using Counters for Counters.Counter;
    Counters.Counter private _listIds;

    struct List {
      mapping(address => bool) adminMap;
      mapping(address => AddressListState) addrToStatus;
      string name;
      address owner;
      State state;
    }

    struct AddrState {
      address addr;
      AddressListState state;
    }

    struct AdminMod {
      address addr;
      bool add;
    }

    /* mappings can be seen as hash tables */
    /* here we create lookups for lists by id */
    mapping(uint => List) private idToList;

    /* events facilitate communication between smart contracts and their user interfaces  */
    /* i.e. we can create listeners for events in the client and also use them in The Graph  */
    
    event ContractStateChange(address actor, State state);
    event ListCreated(address actor, uint id, string name, AdminMod[] adminMods, AddrState[] addrStates);
    event ListModified(address actor, uint id, string name, AdminMod[] adminMods, AddrState[] addrStates);
    event ListStateChange(address actor, uint id, State state);

    /* when the contract is deployed, give it a name */
    /* also set the creator as the owner (or a passed in wallet) of the contract */
    /* and pass in intial state */
    constructor(State _state) {
        state = _state;
    }

    // Admin Functions
    function updateContractState(State _state) public onlyOwner {
      if (state != _state) {
        state = _state;
        emit ContractStateChange(msg.sender, _state);
      }
    }

    // Public mutations
    function createList(uint listId, string memory _name, AdminMod[] memory adminMods, AddrState[] memory addrStates) public isContractState(State.Active) listDoesNotExist(listId) {
      _createList(listId, _name);
      _modifyAdmins(listId, adminMods);
      _modifyAddrToStatus(listId, addrStates);
      emit ListCreated(msg.sender, listId, _name, adminMods, addrStates);
    }

    function modifyList(uint listId, string memory _name, AdminMod[] memory adminMods, AddrState[] memory addrStates) public isContractState(State.Active) listExists(listId) isListAdmin(listId) isListWritable(listId) {
      _modifyAdmins(listId, adminMods);
      _modifyAddrToStatus(listId, addrStates);
      _updateListName(listId, _name);
      emit ListModified(msg.sender, listId, _name, adminMods, addrStates);
    }

    function updateListState(uint _id, State _state) public listExists(_id) isListAdmin(_id) isListNotDestroyed(_id) {
      List storage list = idToList[_id];
      if (list.state != _state) {
        list.state = _state;
        emit ListStateChange(msg.sender, _id, _state);
      }
    }

    function isAllowListed(uint _id) public view listExists(_id) returns (bool) {
      return idToList[_id].addrToStatus[msg.sender] == AddressListState.Allow;
    }

    function isAddressAllowListed(uint _id, address addr) public view listExists(_id) isListAdmin(_id) isListReadable(_id) returns (bool) {
      return idToList[_id].addrToStatus[addr] == AddressListState.Allow;
    }

    function isAdmin(uint _id) public view listExists(_id) isListReadable(_id) returns (bool) {
      return idToList[_id].adminMap[msg.sender];
    }

    function getListInfo(uint _id) public view listExists(_id) returns (string memory, address, State) {
      return (idToList[_id].name, idToList[_id].owner, idToList[_id].state);
    }

    function _createList(uint listId, string memory _name) internal {
      List storage list = idToList[listId];
      list.name = _name;
      list.owner = msg.sender;
      list.adminMap[msg.sender] = true;
      list.state = State.Active;
    }

    // internal function
    ///////////////////////////////// AllowList
    function _addToAllowList(uint _id, address addr) internal returns (bool) {
      List storage list = idToList[_id];
      if (list.addrToStatus[addr] != AddressListState.Allow) {
        list.addrToStatus[addr] = AddressListState.Allow;
        return true;
      }
      return false;
    }

    function _removeFromAllowList(uint _id, address addr) internal returns (bool) {
      List storage list = idToList[_id];
      if (list.addrToStatus[addr] == AddressListState.Allow) {
        list.addrToStatus[addr] = AddressListState.Uninitialized;
        return true;
      }
      return false;
    }

    ///////////////////////////////// Deny List
    function _addToDenyList(uint _id, address addr) internal returns (bool) {
      List storage list = idToList[_id];
      if (list.addrToStatus[addr] != AddressListState.Deny) {
        list.addrToStatus[addr] = AddressListState.Deny;
        return true;
      }
      return false;
    }

    function _removeFromDenyList(uint _id, address addr) internal returns (bool) {
      List storage list = idToList[_id];
      if (list.addrToStatus[addr] == AddressListState.Deny) {
        list.addrToStatus[addr] = AddressListState.Uninitialized;
        return true;
      }
      return false;
    }

    function _modifyAddrToStatus(uint _id, AddrState[] memory addrStates) internal {
      for(uint i=0; i<addrStates.length; i++) {
        if (addrStates[i].state == AddressListState.Allow) {
          _addToAllowList(_id, addrStates[i].addr);
        } else if (addrStates[i].state == AddressListState.Deny) {
          _addToDenyList(_id, addrStates[i].addr);
        } else if (addrStates[i].state == AddressListState.Uninitialized) {
          _removeFromAllowList(_id, addrStates[i].addr);
        }
      }
    }

    ///////////////////////////////// Admin Mapping
    function _addAdmin(uint _id, address addr) internal {
      List storage list = idToList[_id];
      if (!list.adminMap[addr]) {
        list.adminMap[addr] = true;
      }
    }
    
    function _removeAdmin(uint _id, address addr) internal {
      List storage list = idToList[_id];
      if (list.adminMap[addr]) {
        list.adminMap[addr] = false;
      }
    }

    function _modifyAdmins(uint _id, AdminMod[] memory adminMods) internal listExists(_id) isListAdmin(_id) isListWritable(_id) {
      for (uint i=0; i<adminMods.length; i++) {
        if (adminMods[i].add) {
          _addAdmin(_id, adminMods[i].addr);
        } else {
          _removeAdmin(_id, adminMods[i].addr);
        }
      }
    }

    ///////////////////////////////// List Info
    function _updateListName(uint _id, string memory _name) internal {
      if (bytes(_name).length != 0) {
        List storage list = idToList[_id];
        if (keccak256(abi.encodePacked(list.name)) != keccak256(abi.encodePacked(_name))) {
          list.name = _name;
        }
      }
    }

    modifier isListAdmin(uint _id) {
      require (
        idToList[_id].adminMap[msg.sender] == true, 
        "must be list admin"
      );
      _;
    }

    modifier isListOwner(uint _id) {
      require (
        idToList[_id].owner == msg.sender, 
        "must be list owner"
      );
      _;
    }

    modifier isListState(uint _id, State _state) {
      require (
        idToList[_id].state == _state, 
        "list state is not valid"
      );
      _;
    }

    modifier isContractState(State _state) {
      require (
        state == _state, 
        "contract state is not valid"
      );
      _;
    }

    modifier isListReadable(uint _id) {
      require (
        idToList[_id].state != State.Destroyed && idToList[_id].state != State.Uninitialized, 
        "list is not in readable state"
      );
      _;
    }

    modifier isListWritable(uint _id) {
      require (
        idToList[_id].state == State.Active, 
        "list is not writable"
      );
      _;
    }

    modifier isListNotDestroyed(uint _id) {
      require (
        idToList[_id].state != State.Destroyed, 
        "list is destroyed"
      );
      _;
    }

    modifier listExists(uint _id) {
      require (
        idToList[_id].state != State.Uninitialized, 
        "List not found"
      );
      _;
    }
    
    modifier listDoesNotExist(uint _id) {
      require (
        idToList[_id].state == State.Uninitialized, 
        "List already exists"
      );
      _;
    }

    modifier isAddressArrayNotEmpty(address[] memory addresses) {
      require (
        addresses.length > 0,
        "array is empty"
      );
      _;
    }

    
}

