import "@openzeppelin/contracts/utils/Strings.sol";

contract RaccoonDesc {
    using Strings for uint256;
    function generateDesc(uint256 tokenId, string memory receiverName) internal pure  returns (string memory, string memory) {
        string memory nounId = tokenId.toString();
        string memory name = string(abi.encodePacked("Raccoon Arthur No.", nounId));
        string memory desc = string(abi.encodePacked("This is Arthur Cang's gift to ", receiverName, "."));
        return (name, desc);
    }
}

