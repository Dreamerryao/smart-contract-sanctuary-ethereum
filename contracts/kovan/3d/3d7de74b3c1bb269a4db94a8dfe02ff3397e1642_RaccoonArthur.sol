pragma solidity ^0.8.10;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "erc721a/contracts/ERC721A.sol";
import "./libs/JSONGenerator.sol";
import "./libs/PRF.sol";
import "./RaccoonSVG.sol";
import "./RaccoonDesc.sol";

contract RaccoonArthur is Ownable, ERC721A, ReentrancyGuard, RaccoonSVG, RaccoonDesc {
  using Strings for uint256;

  struct allowListComponent {
    uint8 quantity;
    string receiverName;
  }

  mapping(address => allowListComponent) public allowlist;
    

  constructor() ERC721A("RaccoonArthur", "ARTHUR") {}

  modifier callerIsUser() {
    require(tx.origin == msg.sender, "The caller is another contract");
    _;
  }

  function mintForClient(address client, uint8 quantity, string memory receiverName) external onlyOwner {
    _safeMint(client, quantity, receiverName);
      
  }

  function clientMint(uint8 quantity, string memory receiverName) external payable callerIsUser {
    require(quantity > 0 && allowlist[msg.sender].quantity >= quantity, "not eligible for allowlist mint");
    allowlist[msg.sender].quantity -= quantity;
    _safeMint(msg.sender, quantity, receiverName);
  }
  
  function seedAllowlist(address[] memory addresses, uint8[] memory numSlots, string[] memory receiverNames) external onlyOwner {
    require(
      addresses.length == numSlots.length && numSlots.length == receiverNames.length,
      "length does not match"
    );
    for (uint256 i = 0; i < addresses.length; i++) {
      allowlist[addresses[i]].quantity = numSlots[i];
      allowlist[addresses[i]].receiverName = receiverNames[i];
    }
  }

  // metadata URI

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    require(_exists(tokenId));
    TokenOwnership memory ownershipData = getOwnershipData(tokenId);
    string memory body = PRF.pseudorandom(uint256(uint160(ownershipData.addr)).toString(), ownershipData.receiverName).toString();
    string memory ear = PRF.pseudorandom(tokenId.toString(), uint256(uint160(ownershipData.addr)).toString()).toString();
    string memory eye = PRF.pseudorandom(uint256(uint160(ownershipData.addr)).toString(), tokenId.toString()).toString();
    string memory nose = PRF.pseudorandom(tokenId.toString(), ownershipData.receiverName).toString();
    string memory noseCore = PRF.pseudorandom(ownershipData.receiverName, tokenId.toString()).toString();
    SVGParams memory params = SVGParams(body, ear, eye, nose, noseCore);
    (string memory name, string memory desc) = generateDesc(tokenId, ownershipData.receiverName);
    JSONGenerator.TokenUriParams memory tokenUriParams = JSONGenerator.TokenUriParams(name, desc, generateSVG(params, ownershipData.receiverName));
    return JSONGenerator.generateUri(tokenUriParams);
  }

  function numberMinted(address owner) public view returns (uint256) {
    return _numberMinted(owner);
  }

  function numberBurned(address owner) public view returns (uint256) {
    return _numberBurned(owner);
  }

  function getOwnershipData(uint256 tokenId) public view returns (TokenOwnership memory) {
    return ownershipOf(tokenId);
  }

}

