/**
 * @title MobBoss Contract
 * @notice Protocol controller and team wallet
 * @notice Determines vigs (interest rates), base value for WPUNKs, and enables access to any contained tokens
 * @author Hifi
 */
contract MobBoss is MobBossInterface, MobBossStorage, Ownable {
    constructor() {
        // TODO: determine optimal initial interest rates and value per punk
        setBaseWPunkValue(15_000 * 1e18);
        console.log("Deploying MobBoss..");
    }

    modifier onlySupportedCollateral(address collateral) {
        require(bytes(collateralTypeOf[collateral]).length != 0, "MobBoss: collateral is not supported");
        _;
    }

    /// @inheritdoc MobBossInterface
    function addCollateralSupport(address collateral, string memory typeOf) public override onlyOwner returns (bool) {
        require(bytes(typeOf).length != 0, "MobBoss: invalid collateral type");
        collateralTypeOf[collateral] = typeOf;
        setCollateralEnabled(collateral, true);
        return true;
    }

    /// @inheritdoc MobBossInterface
    function setCollateralEnabled(address collateral, bool enabled)
        public
        override
        onlySupportedCollateral(collateral)
        onlyOwner
        returns (bool)
    {
        isCollateralEnabled[collateral] = enabled;
        return true;
    }

    /// @inheritdoc MobBossInterface
    function withdraw(
        address token,
        uint256 amount,
        address recipient
    ) external override onlyOwner returns (bool) {
        Erc20Interface(token).transfer(recipient, amount);
        return true;
    }

    /// @inheritdoc MobBossInterface
    function setBaseWPunkValue(uint256 newWPunkValue) public override onlyOwner returns (bool) {
        baseWPunkValue = newWPunkValue;
        return true;
    }

    /// @inheritdoc MobBossInterface
    function setVig(address collateral, uint256 newVig)
        public
        override
        onlyOwner
        onlySupportedCollateral(collateral)
        returns (bool)
    {
        require(newVig >= PRBMathUD60x18.SCALE, "MobBoss: invalid new vig");
        LoanSharkInterface(LOANSHARK_ADDRESS).realizeProtocolDebt(collateral);
        vigOf[collateral] = newVig;
        return true;
    }
}


