// File contracts/Graveyard.sol


pragma solidity ^0.8.0;





/**
 * @title Graveyard Contract
 * @notice Contains all liquidated collateral on the protocol
 * @notice Anyone with enough DrugMoney could raid some of the burried collateral in the Graveyard
 * @author Hifi
 */
contract Graveyard is GraveyardInterface, Pausable {
    constructor() {
        console.log("Deploying Graveyard..");
    }

    function raid(address collateral, uint256 value) external override whenNotPaused returns (bool) {
        if (collateral == WPUNK_TOKEN_ADDRESS) {
            // the punk is inside the graveyard
            require(
                Erc721Interface(WPUNK_TOKEN_ADDRESS).ownerOf(value) == address(this),
                "Graveyard: punk doesn't exist in graveyard"
            );

            // caller has at least 1/n of DrugMoney total supply
            require(
                (ProtocolTokenInterface(DRUG_TOKEN_ADDRESS).totalSupply() /
                    ProtocolTokenInterface(DRUG_TOKEN_ADDRESS).balanceOf(msg.sender)) <=
                    Erc721Interface(WPUNK_TOKEN_ADDRESS).balanceOf(address(this)),
                "Graveyard: not enough DrugMoney to raid dead punk"
            );

            // burn 1/n of DrugMoney supply from sender
            ProtocolTokenInterface(DRUG_TOKEN_ADDRESS).burn(
                msg.sender,
                ProtocolTokenInterface(DRUG_TOKEN_ADDRESS).totalSupply() /
                    Erc721Interface(WPUNK_TOKEN_ADDRESS).balanceOf(address(this))
            );

            Erc721Interface(WPUNK_TOKEN_ADDRESS).transferFrom(address(this), msg.sender, value);
        } else if (collateral == DAI_TOKEN_ADDRESS) {
            require(
                Erc20Interface(DAI_TOKEN_ADDRESS).balanceOf(address(this)) >= value,
                "Graveyard: not enough buried DAI treasure"
            );

            // burn the drug money
            ProtocolTokenInterface(DRUG_TOKEN_ADDRESS).burn(msg.sender, value);

            // send the dai
            Erc20Interface(DAI_TOKEN_ADDRESS).transfer(msg.sender, value);
        }
        return true;
    }
}