// contracts/Staking.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "./Jack.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "erc721a-upgradeable/contracts/IERC721AUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";


contract Staking is Ownable, ReentrancyGuard, IERC721ReceiverUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    // interact with JACK (ERC721A) contract
    Jack jack;

    // Interfaces for ERC20 
    IERC20Upgradeable public immutable chipsToken;

    // specifies total number of staked tokens
    uint256 public totalStaked;

    // specifies is special locking yield allowed
    bool public specialLockingOpened;

    // specifies contract deployment time
    uint256 private deploymentTime;

    // constant variables defining CHIPS to upgrade the level
    uint256 private constant  CHIPS_100     = (100 * 10 ** 18); // 100 chips
    uint256 private constant  CHIPS_170     = (170 * 10 ** 18); // 175 chips
    uint256 private constant  CHIPS_250     = (250 * 10 ** 18); // 250 chips

    // constant variables defining time 
    uint256 private constant  ONE_DAY_TM     = (1 minutes);  // change to 1 days in production
    uint256 private constant  TWO_WEEKS_TM   = (2 minutes);  // change to 2 weeks in production
    uint256 private constant  FOUR_WEEKS_TM  = (4 minutes);  // change to 4 weeks in production
    uint256 private constant  EIGHT_WEEKS_TM = (8 minutes);  // change to 8 weeks in production
    uint256 private constant  TWELVE_WEEKS_TM= (12 minutes); // change to 12 weeks in production

    // TOKEN ($CHIPS) reward 
    uint256 private TWO_WEEKS_REWARD;   // 4 chips for 1st four months
    uint256 private FOUR_WEEKS_REWARD;  // 8 chips for 1st four months
    uint256 private EIGHT_WEEKS_REWARD; // 12.5 chips for 1st four months
    uint256 private TWENTY_WEEKS_REWARD;// 20 chips for 1st four months

    // Enum for timeframe to stake the token 
    enum TimeFrame{ ONE_DAY,TWO_WEEKS, FOUR_WEEKS, EIGHT_WEEKS, TWELVE_WEEKS }

    // struct to store a stake's token, owner, and timestampe and timeframe
    struct Stake {
        uint24 tokenId;
        uint48 timeOfLastUpdate;
        address owner;
        TimeFrame timeFrame;
    }

    // maps tokenId to stake
    mapping(uint256 => Stake) public vault;

    // maps tokenId to totTimeStaked
    mapping(uint256 => uint256) public totTimeStaked;

    // events
    event _etherWidthdraw(uint256 _amount); 
    event _claimed(address owner, uint256 amount);
    event _transferChips(uint256 _amount, address _to);
    event _rewardUpdated(uint256 preTime, uint256 curTime);
    event _NFTStaked(address owner, uint256 tokenId, uint256 value);
    event _NFTUnstaked(address owner, uint256 tokenId, uint256 value);

    // Constructor function
    constructor(IERC20Upgradeable _chipsToken,address _jackAddress) {
        
        // setting initial variable to instantiate the ERC20 and ERC721A contract
        jack            = Jack(_jackAddress);
        chipsToken      = _chipsToken;
        deploymentTime  = block.timestamp;

        // setting initial rewards
        TWO_WEEKS_REWARD    = (4 * 10 ** 18);   // 4 chips
        FOUR_WEEKS_REWARD   = (8 * 10 ** 18);   // 8 chips
        EIGHT_WEEKS_REWARD  = (125 * 10 ** 17); // 12.5 chips
        TWENTY_WEEKS_REWARD = (20 * 10 ** 18);  // 20 chips
    }

    // this function will function after each 4 month 
    function updateRewards() external {
        uint256 time  = (block.timestamp - deploymentTime );
        require (
            time >= (16 minutes),
            "Wait for 4 months to half the reward"
        );
        
        time = time / (16 weeks); // 4 months

        for(uint256 i=0; i<time; i++){
            TWO_WEEKS_REWARD    = TWO_WEEKS_REWARD  / 2;   // 4 chips
            FOUR_WEEKS_REWARD   = FOUR_WEEKS_REWARD / 2;   // 8 chips
            EIGHT_WEEKS_REWARD  = EIGHT_WEEKS_REWARD / 2; // 12.5 chips
            TWENTY_WEEKS_REWARD = TWENTY_WEEKS_REWARD / 2;  // 20 chips
        }

        emit _rewardUpdated(deploymentTime, block.timestamp);
        deploymentTime  = block.timestamp;
    }

    

    function stake(uint256[] calldata tokenIds, TimeFrame _timeFrame) external {
        uint256 tokenId;
        totalStaked += tokenIds.length;

        for (uint i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            
            // checking ownership of NFT
            require(
                jack.ownerOf(tokenId) == msg.sender,
                "Can't stake tokens you don't own!"
            );


            // checking is token staked
            require(
                vault[tokenId].tokenId == 0, 
                'already staked'
            );

            jack.transferFrom(msg.sender, address(this), tokenId);
            emit _NFTStaked(msg.sender, tokenId, block.timestamp);

            vault[tokenId] = Stake({
                owner: msg.sender,
                tokenId: uint24(tokenId),
                timeOfLastUpdate: uint48(block.timestamp),
                timeFrame: _timeFrame
            });
        }
    }
    
 

    function _claim(address account, uint256[] calldata tokenIds, bool _unstake) internal {
        uint256 tokenId;
        uint256 earned = 0;

        for (uint i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            Stake memory staked = vault[tokenId];
            
            require(
                staked.owner == account,
                "not an owner"
            );

            uint256 cal_tm       = (block.timestamp - staked.timeOfLastUpdate);
            uint256 cus_tm       = getTimeFrame(staked.timeFrame);        
            require(
                cal_tm >= cus_tm,
                "Please wait, lock period is not completed"
            );

            earned                  +=  calculateLockRewards(staked.timeFrame, cal_tm);
            totTimeStaked[tokenId]  += cus_tm;


            // call upgradeLevel func 
            upgradeLevel(tokenId,totTimeStaked[tokenId]);

            vault[tokenId] = Stake({
                owner: account,
                tokenId: uint24(tokenId),
                timeOfLastUpdate: uint48(block.timestamp),
                timeFrame: staked.timeFrame
            });

        }
        if (earned > 0) {
            // sending reward token 
            require(
                chipsToken.balanceOf(address(this)) >= earned,
                "Contract does not have Token to transfer reward"
            );

            chipsToken.transfer(msg.sender, earned);
            
        }
        if (_unstake) {
            _unstakeMany(account, tokenIds);
        }
        emit _claimed(account, earned);
    }

    function toggleSpecialLockingYields() external onlyOwner{
        specialLockingOpened = !specialLockingOpened;
    }

    // upgradeLevelByChips
    function specialLockingYields(uint256[] calldata tokenIds, uint256 _chipsQty) external {
        require(specialLockingOpened, "Special locking is off!");
        uint256 tokenId;
        uint256 level;
        uint256 chipsQty = _chipsQty;
        uint256 tokenAmount;

        //  require( (chipsToken.balanceOf(msg.sender)) >=chipsQty , "You have insufficient box token" );

        for (uint i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];

            level = jack.getTokenLevel(tokenId);
            // If NFT level = LEVEL_ONE || LEVEL_FOUR, upgrade level is not allowed.
            require((level >= 1 ) && (level < 4), "You cannot upgrade level" );

            if( level == 1 ){ // NFT Level = LEVEL_TWO
                // Level 2 stakers upgrade to Level 3 will cost 100 $CHIPS token
                require( (chipsQty >= CHIPS_100 ) && (chipsQty < CHIPS_250), "Insufficient balance to upgrade from LEVEL 2 to 3" );
                totTimeStaked[tokenId]  += (4 weeks); // upgrade to lEVEL_THREE
                chipsQty -= CHIPS_100; // 100 $CHIPS required

            }else if( level == 1 ){ // NFT Level = LEVEL_TWO  
                // Level 2 stakers upgrade to Level 4 will cost 250 $CHIPS token
                require( (chipsQty >= CHIPS_250),  "Insufficient balance to upgrade from LEVEL 2 to 4"   );
                totTimeStaked[tokenId]  += (8 weeks); // upgrade to lEVEL_THREE
                chipsQty -= CHIPS_250; // 250 $CHIPS required

            }else if( level == 2 ){ // NFT Level = LEVEL_THREE  
                // Level 3 stakers upgrade to Level 4 will cost 170 $CHIPS token
                require( (chipsQty >= CHIPS_170), "Insufficient balance to upgrade from LEVEL 3 to 4"  );
                totTimeStaked[tokenId]  += (4 weeks); // upgrade to lEVEL_THREE
                chipsQty -= CHIPS_170; // 170 $CHIPS required

            }
            // call upgradeLevel func 
            upgradeLevel(tokenId,totTimeStaked[tokenId]);

        }

        tokenAmount = _chipsQty - chipsQty;
        require(chipsToken.transferFrom(msg.sender,address(this),  tokenAmount) == true,"transferToken: Error in CHIPS transfer");
    }   

    function upgradeLevel(uint256 _tokenId,uint256 _time) internal {

        if( (_time >= (TWO_WEEKS_TM)) && (_time < (FOUR_WEEKS_TM)) ){
            jack.updateLevel(_tokenId, 0); // LEVEL = 1
        }else if( (_time >= (FOUR_WEEKS_TM)) && (_time < (EIGHT_WEEKS_TM)) ){
            jack.updateLevel(_tokenId, 1); // LEVEL = 2
        }else if( (_time >= (EIGHT_WEEKS_TM)) && (_time < (TWELVE_WEEKS_TM)) ){
            jack.updateLevel(_tokenId, 2); // LEVEL = 3
        }else if( (_time >= (TWELVE_WEEKS_TM)) ){
            jack.updateLevel(_tokenId, 3); // LEVEL = 4
        }


    }

    function unstake(uint256[] calldata tokenIds) external {
        _claim(msg.sender, tokenIds, true);
    }

    function _unstakeMany(address account, uint256[] calldata tokenIds) internal {
        uint256 tokenId;
        totalStaked -= tokenIds.length;
        for (uint i = 0; i < tokenIds.length; i++) {
            tokenId = tokenIds[i];
            Stake memory staked = vault[tokenId];
            require(staked.owner == msg.sender, "not an owner");

            delete vault[tokenId];
            emit _NFTUnstaked(account, tokenId, block.timestamp);
            jack.transferFrom(address(this), account, tokenId);
        }
    }

    // should never be used inside of transaction because of gas fee
    function balanceOf(address account) public view returns (uint256) {
        return jack.tokensMintedtBy(account);
    }

    function getTimeFrame(TimeFrame _timeFrame) internal pure returns (uint256) {

        uint256 tmp;
        if(_timeFrame == TimeFrame.ONE_DAY){
            tmp = ONE_DAY_TM;    // 1 days
        }else if(_timeFrame == TimeFrame.TWO_WEEKS){
            tmp = TWO_WEEKS_TM;    // 2 weeks
        }else if(_timeFrame == TimeFrame.FOUR_WEEKS){
            tmp = FOUR_WEEKS_TM;    // 4 weeks
        }else if(_timeFrame == TimeFrame.EIGHT_WEEKS){
            tmp = EIGHT_WEEKS_TM;    // 8 weeks
        }else if(_timeFrame == TimeFrame.TWELVE_WEEKS){
            tmp = TWELVE_WEEKS_TM;   // 12 weeks
        }

        return tmp;

    }

  
    function calculateLockRewards(TimeFrame _timeFrame, uint256 _time) internal view returns (uint256)
    {
        uint256 reward;

        if((_timeFrame == TimeFrame.ONE_DAY) &&  (_time >= ONE_DAY_TM) ){
            reward  = TWO_WEEKS_REWARD * 1;      // 1-2 weeks  // 4

        }else if((_timeFrame == TimeFrame.TWO_WEEKS) &&  (_time >= TWO_WEEKS_TM) ){
            reward  = TWO_WEEKS_REWARD * 14;     // 1-2 weeks  // 4

        }else if((_timeFrame == TimeFrame.FOUR_WEEKS) && (_time >= FOUR_WEEKS_TM) ){
            reward  = TWO_WEEKS_REWARD * 14;     // 1-2 weeks  // 4
            reward += FOUR_WEEKS_REWARD * 14;     // 2-4 weeks  // 8

        }else if((_timeFrame == TimeFrame.EIGHT_WEEKS) && (_time >= EIGHT_WEEKS_TM) ){
            reward  = TWO_WEEKS_REWARD * 14;     // 1-2 weeks  // 4
            reward += FOUR_WEEKS_REWARD * 14;     // 2-4 weeks  // 8
            reward += EIGHT_WEEKS_REWARD * 28;  // 4-8 weeks  // 12.5 

        }else if((_timeFrame == TimeFrame.TWELVE_WEEKS) && (_time >= TWELVE_WEEKS_TM) ){
            reward  = TWO_WEEKS_REWARD * 14;     // 1-2 weeks  // 4
            reward += FOUR_WEEKS_REWARD * 14;     // 2-4 weeks  // 8
            reward += EIGHT_WEEKS_REWARD * 28;  // 4-8 weeks  // 12.5 
            reward += TWENTY_WEEKS_REWARD * 28;    //8-12 weeks  // 20
        }

        return reward;  
    }

    //to check balance of receiveAbleTokens 
    function checkReceivedTokensBalance() external view returns(uint balance){
        return chipsToken.balanceOf(address(this));
    }

    function onERC721Received(address, address, uint256, bytes calldata) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

      //transfer Chips tokens to msg.sender
    function transferChips() external onlyOwner {
        uint tokenAmount = chipsToken.balanceOf(address(this));
        require(chipsToken.transferFrom(address(this), msg.sender, tokenAmount) == true,"transferToken: Error in CHIPS transfer");
        emit _transferChips(tokenAmount, msg.sender);
    }

    function withdraw() external onlyOwner{
        emit _etherWidthdraw(address(this).balance);
        payable(msg.sender).transfer(address(this).balance);
    }

}

// contracts/Jack.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "./Box.sol";
import "erc721a-upgradeable/contracts/ERC721AUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

contract Jack is Initializable, ERC721AUpgradeable, OwnableUpgradeable{
    using StringsUpgradeable for uint256;

    uint256 public constant MAX_SUPPLY = 7777;
    bool public isRevealed;
    
    // base Token uris 
    string private baseTokenUri1;
    string private baseTokenUri2;
    string private baseTokenUri3;
    string private baseTokenUri4;

    // contract Addresses
    address private boxAddress;
    address private stakingAddress;

    // mapping of token level to upgrade it 
    mapping(uint256 => uint256) private tokenLevel;


    // events
    event _etherWidthdraw(uint256 _amount); 
    event _jackMinted(address owner, uint256 amount);
    event _jackLevelUpdated(uint256 preLevel, uint256 curLevel);
    event _setTokenUris(string  _baseTokenUri1,string  _baseTokenUri2,string  _baseTokenUri3,string  _baseTokenUri4);


    function initialize() external initializer{
        __ERC721A_init("Jack","JACK");
        __Ownable_init();
    }


    // modifiers
    modifier callerIsUser() {
        require(tx.origin == msg.sender, "Jack :: Cannot be called by a contract");
        _;
    }

    modifier callerIsBox() {
        require(msg.sender == boxAddress, "Jack :: can only be called by Box contract");
        _;
    }
     
    modifier callerIsStaking() {
        require(msg.sender == stakingAddress, "Jack :: can only be called by Staking contract");
        _;
    }

    function mint(address addr,uint256 _quantity) external callerIsBox{
        require(boxAddress != address(0), "Jack :: Box address is not set yet");
        Box box = Box(boxAddress);
        require((box.tokensBurntBy(addr)) != 0, "Jack :: you did not burn any BOX token");
        require((box.tokensBurntBy(addr)) >= (_quantity + _numberMinted(addr)), "Jack :: Burning Beyond minted quantity");
        require((totalSupply() + _quantity) <= MAX_SUPPLY, "Jack :: Beyond Max Supply");
        _safeMint(addr, _quantity);
        emit _jackMinted(addr,_quantity);
    }

    function setStakingAddress(address _stakingAddress) external onlyOwner{
        stakingAddress = _stakingAddress;
    }

    function getStakingAddress() external view returns (address){
        return stakingAddress;
    }

    function updateLevel(uint256 tokenId, uint256 _level) external callerIsStaking{
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        // 0 : LEVEL_ONE;
        // 1 : LEVEL_TWO;
        // 2 : LEVEL_THREE;
        // 3 : LEVEL_FOUR;
        if(tokenLevel[tokenId] <=3){
            emit _jackLevelUpdated(tokenLevel[tokenId], _level);
            tokenLevel[tokenId]=_level;
        }
    }

    function getTokenLevel(uint256 tokenId) public view returns (uint256) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        return tokenLevel[tokenId];
    }


    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenUri1;
    }

    function setTokenUris(
            string memory _baseTokenUri1,  // FOR LEVEL_ONE
            string memory _baseTokenUri2,  // FOR LEVEL_TWO
            string memory _baseTokenUri3,  // FOR LEVEL_THREE
            string memory _baseTokenUri4   // FOR LEVEL_FOUR
        ) public onlyOwner{
            baseTokenUri1 = _baseTokenUri1; 
            baseTokenUri2 = _baseTokenUri2;
            baseTokenUri3 = _baseTokenUri3;
            baseTokenUri4 = _baseTokenUri4;

        emit _setTokenUris(_baseTokenUri1,_baseTokenUri2,_baseTokenUri3,_baseTokenUri4);
    }


    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory baseURI;
        uint256 tknLvl = getTokenLevel(tokenId);

        if(tknLvl == 0){ 
            baseURI = baseTokenUri1;
        }else if(tknLvl == 1){
            baseURI = baseTokenUri2;
        }else if(tknLvl == 2){
            baseURI = baseTokenUri3;
        }else if(tknLvl == 3){
            baseURI = baseTokenUri4;
        }

        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : "";
    }
   
    function toggleReveal() external onlyOwner{
        isRevealed = !isRevealed;
    }

    function setBoxAddress(address _boxAddress) external onlyOwner{
        boxAddress = _boxAddress;
    }

    function getBoxAddress() external view returns (address){
        return boxAddress;
    }

    function tokensMintedtBy(address owner) external view returns (uint256) {
        return _numberMinted(owner);
    }

    function tokenIdsOfOwner(address owner) public view returns (uint256[] memory) {
        unchecked {
            uint256 tokenIdsIdx;
            address currOwnershipAddr;
            uint256 tokenIdsLength = balanceOf(owner);
            uint256[] memory tokenIds = new uint256[](tokenIdsLength);
            TokenOwnership memory ownership;
            for (uint256 i = _startTokenId(); tokenIdsIdx != tokenIdsLength; ++i) {
                ownership = _ownerships[i];
                if (ownership.burned) {
                    continue;
                }
                if (ownership.addr != address(0)) {
                    currOwnershipAddr = ownership.addr;
                }
                if (currOwnershipAddr == owner) {
                    tokenIds[tokenIdsIdx++] = i;
                }
            }
            return tokenIds;
        }
    }

    function withdraw() external onlyOwner{
        emit _etherWidthdraw(address(this).balance);
        payable(msg.sender).transfer(address(this).balance);
    }

    function version() external pure returns (string memory) {
        return "1.0.0";
    }
    
    // important to receive ETH
    // receive() payable external {}
}

