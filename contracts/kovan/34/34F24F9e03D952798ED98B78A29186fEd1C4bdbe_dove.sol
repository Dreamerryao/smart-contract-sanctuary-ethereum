// File: dove.sol

contract dove {
    using Address for address;
    using SafeMath for uint256;

    uint public dataCount;
    mapping(bytes32 => string) public data; 
    address governance = msg.sender;
    
    event WriteEvent(address from, string _message, uint nonce, bytes32 id);
    
    constructor(address _rewards) public {
        governance = msg.sender;
    }

    // function deposit(uint256 _amount) public {
    function getId(address sender, string memory _message, uint nonce) public returns (bytes32) {
        bytes32 id = keccak256(abi.encodePacked(sender, _message, nonce));
        
        return id;
    }

    // function deposit(uint256 _amount) public {
    function writeData(string memory _message) public {
        bytes32 id = getId(msg.sender, _message, dataCount);
        data[id] = _message;
        dataCount = dataCount + 1;
        
        emit WriteEvent(msg.sender, _message, dataCount, id);
    }
}