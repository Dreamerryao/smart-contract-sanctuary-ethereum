pragma solidity ^0.8.0;
pragma abicoder v2;

import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { Pausable } from "@openzeppelin/contracts/security/Pausable.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
// import { AggregatorV3Interface } from "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "./interfaces/INftPriceOracle.sol";
import "./interfaces/IPredictionParams.sol";

import "hardhat/console.sol";

/**
 * @title FloorPricePrediction
 */
contract FloorPricePrediction is
    KeeperCompatibleInterface,
    Initializable,
    Ownable,
    Pausable,
    ReentrancyGuard,
    IPredictionParams
{
    using SafeERC20 for IERC20;

    INftPriceOracle public oracle;

    address public adminAddress; // address of the admin
    address public operatorAddress; // address of the operator

    uint256 public bufferSeconds; // number of seconds for valid execution of a prediction round

    uint256 public minBetAmount; // minimum betting amount (denominated in wei)
    uint256 public treasuryFee; // treasury rate (e.g. 200 = 2%, 150 = 1.50%)
    uint256 public treasuryAmount; // treasury amount that was not claimed

    uint256 public oracleLatestRoundId; // converted from uint80 (Chainlink)
    uint256 public oracleUpdateAllowance; // seconds

    // uint256 public constant MAX_TREASURY_FEE = 1000; // 10%

    uint256 public constant LOCK_INTERVAL_SECONDS = 86400;
    string slug;

    struct KeeperParams {
        uint keeperCounter;
        uint keeperInterval;
        uint keeperLastTimeStamp;
    }

    KeeperParams public keeperParams;

    mapping(PositionPeriod => Period) public paramsByPeriod;

    enum Position {
        Bull,
        Bear
    }

    enum PositionPeriod {
        DAY,
        WEEK,
        MONTH
    }

    uint8[3] INTERVAL_DAYS = [1, 7, 30];

    struct Period {
        uint intervalDays;
        uint intervalSeconds;
        bool genesisStartEnds;
        bool genesisLockEnds;
        uint256 currentEpoch;
        uint256 unpausedEpoch;
        mapping(uint256 => mapping(address => BetInfo)) ledger;
        mapping(uint256 => Round) rounds;
        mapping(address => uint256[]) userRounds;
    }

    struct Round {
        uint256 epoch;
        uint256 startTimestamp;
        uint256 lockTimestamp;
        uint256 closeTimestamp;
        int256 lockPrice;
        int256 closePrice;
        uint256 lockOracleId;
        uint256 closeOracleId;
        uint256 totalAmount;
        uint256 bullAmount;
        uint256 bearAmount;
        uint256 rewardBaseCalAmount;
        uint256 rewardAmount;
        bool oracleCalled;
    }

    struct BetInfo {
        Position position;
        uint256 amount;
        bool claimed; // default false
    }

    event BetBear(address indexed sender, uint256 indexed epoch, uint256 amount);
    event BetBull(address indexed sender, uint256 indexed epoch, uint256 amount);
    event Claim(address indexed sender, uint256 indexed epoch, uint256 amount);
    event EndRound(uint256 indexed epoch, uint256 indexed roundId, int256 price);
    event LockRound(uint256 indexed epoch, uint256 indexed roundId, int256 price);

    event NewAdminAddress(address admin);
    event NewBufferSeconds(uint256 bufferSeconds);
    event NewMinBetAmount(
        uint256 indexed day_epoch,
        uint256 indexed week_epoch,
        uint256 indexed month_epoch,
        uint256 minBetAmount
    );
    event NewTreasuryFee(
        uint256 indexed day_epoch,
        uint256 indexed week_epoch,
        uint256 indexed month_epoch,
        uint256 treasuryFee
    );
    event NewOperatorAddress(address operator);
    event NewOracle(address oracle);
    event NewOracleUpdateAllowance(uint256 oracleUpdateAllowance);

    event Pause(uint256 indexed day_epoch, uint256 indexed week_epoch, uint256 indexed month_epoch);
    event RewardsCalculated(
        uint256 indexed epoch,
        uint256 rewardBaseCalAmount,
        uint256 rewardAmount,
        uint256 treasuryAmount
    );

    event StartRound(uint256 indexed epoch, uint256 indexed periodIdx);
    event TokenRecovery(address indexed token, uint256 amount);
    event TreasuryClaim(uint256 amount);
    event Unpause(uint256 indexed day_epoch, uint256 indexed week_epoch, uint256 indexed month_epoch);

    // NA: Not Admin
    modifier onlyAdmin() {
        require(msg.sender == adminAddress, "NA");
        _;
    }

    // NAO: Not admin / operator
    modifier onlyAdminOrOperator() {
        require(msg.sender == adminAddress || msg.sender == operatorAddress, "NAO");
        _;
    }

    // NO: Not operator
    modifier onlyOperator() {
        require(msg.sender == operatorAddress, "NO");
        _;
    }

    // CNA: contract not allowed
    // PCNA: Proxy contract not allowed
    modifier notContract() {
        require(!_isContract(msg.sender), "CNA");
        require(msg.sender == tx.origin, "PCNA");
        _;
    }

    // @custom:oz-upgrades-unsafe-allow constructor
    // constructor() initializer {}

    function initialize(PredictionParams memory params) public initializer {
        // require(_treasuryFee <= MAX_TREASURY_FEE, "Treasury fee too high");

        oracle = INftPriceOracle(params._oracleAddress);
        adminAddress = params._adminAddress;
        operatorAddress = params._operatorAddress;
        bufferSeconds = params._bufferSeconds;
        minBetAmount = params._minBetAmount;
        oracleUpdateAllowance = params._oracleUpdateAllowance;
        treasuryFee = params._treasuryFee;
        slug = params._slug;

        keeperParams.keeperInterval = params._keeperUpdateInterval;
        keeperParams.keeperLastTimeStamp = block.timestamp;
        keeperParams.keeperCounter = 0;

        uint idx = 0;
        while (idx <= uint(PositionPeriod.MONTH)) {
            Period storage period = paramsByPeriod[PositionPeriod(idx)];
            period.intervalDays = INTERVAL_DAYS[idx];
            period.intervalSeconds = INTERVAL_DAYS[idx] * 86400;
            period.genesisStartEnds = false;
            period.genesisLockEnds = false;
            period.currentEpoch = 0;
            period.unpausedEpoch = 0;
            idx++;
        }
    }

    /**
     * @notice Bet bear position
     * @param epoch: epoch
     */

    // BTEL: Bet is too early/late
    // RNB: Round not bettable
    // BMG: Bet amount must be greater than minBetAmount
    // BOO: Can only bet once per round
    function betBear(uint256 epoch, uint periodIdx) external payable whenNotPaused nonReentrant notContract {
        require(epoch == paramsByPeriod[PositionPeriod(periodIdx)].currentEpoch, "BTEL");
        require(_bettable(epoch, periodIdx), "RNB");
        require(msg.value >= minBetAmount, "BMG");
        require(paramsByPeriod[PositionPeriod(periodIdx)].ledger[epoch][msg.sender].amount == 0, "BOO");

        // Update round data
        uint256 amount = msg.value;
        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];
        Round storage round = period.rounds[epoch];
        round.totalAmount = round.totalAmount + amount;
        round.bearAmount = round.bearAmount + amount;

        // Update user data
        BetInfo storage betInfo = period.ledger[epoch][msg.sender];
        betInfo.position = Position.Bear;
        betInfo.amount = amount;
        period.userRounds[msg.sender].push(epoch);

        emit BetBear(msg.sender, epoch, amount);
    }

    /**
     * @notice Bet bull position
     * @param epoch: epoch
     */
    // BTEL: Bet is too early/late
    // RNB: Round not bettable
    // BMG: Bet amount must be greater than minBetAmount
    // BOO: Can only bet once per round
    function betBull(uint256 epoch, uint periodIdx) external payable whenNotPaused nonReentrant notContract {
        require(epoch == paramsByPeriod[PositionPeriod(periodIdx)].currentEpoch, "BTEL");
        require(_bettable(epoch, periodIdx), "RNB");
        require(msg.value >= minBetAmount, "BMG");
        require(paramsByPeriod[PositionPeriod(periodIdx)].ledger[epoch][msg.sender].amount == 0, "BOO");

        // Update round data
        uint256 amount = msg.value;
        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];
        Round storage round = period.rounds[epoch];
        round.totalAmount = round.totalAmount + amount;
        round.bullAmount = round.bullAmount + amount;

        // Update user data
        BetInfo storage betInfo = period.ledger[epoch][msg.sender];
        betInfo.position = Position.Bull;
        betInfo.amount = amount;
        period.userRounds[msg.sender].push(epoch);

        emit BetBull(msg.sender, epoch, amount);
    }

    /**
     * @notice Claim reward for an array of epochs
     */
    // RNS: Round has not started
    // RNE: Round has not ended
    // NC: Not claimable
    // NR: Not refundable
    function claim(
        uint256[] memory day_epochs,
        uint256[] memory week_epochs,
        uint256[] memory month_epochs
    ) external nonReentrant notContract {
        uint256 reward; // Initializes reward
        uint256[][3] memory epochsList = [day_epochs, week_epochs, month_epochs];
        for (uint256 j = 0; j < epochsList.length; j++) {
            if (epochsList[j].length == 0) break;
            uint[] memory epochs = epochsList[j];
            Period storage period = paramsByPeriod[PositionPeriod(j)];
            for (uint256 i = 0; i < epochs.length; i++) {
                require(period.rounds[epochs[i]].startTimestamp != 0, "RNS");
                require(block.timestamp > period.rounds[epochs[i]].closeTimestamp, "RNE");

                uint256 addedReward = 0;

                // Round valid, claim rewards

                if (period.rounds[epochs[i]].oracleCalled) {
                    require(claimable(epochs[i], j, msg.sender), "NC");
                    Round memory round = period.rounds[epochs[i]];
                    addedReward =
                        (period.ledger[epochs[i]][msg.sender].amount * round.rewardAmount) /
                        round.rewardBaseCalAmount;
                }
                // Round invalid, refund bet amount
                else {
                    require(refundable(epochs[i], j, msg.sender), "NR");
                    addedReward = period.ledger[epochs[i]][msg.sender].amount;
                }

                period.ledger[epochs[i]][msg.sender].claimed = true;
                reward += addedReward;

                emit Claim(msg.sender, epochs[i], addedReward);
            }
        }

        if (reward > 0) {
            _safeTransfer(address(msg.sender), reward);
        }
    }

    /**
     * @notice Start the next round n, lock price for round n-1, end round n-2
     * @dev Callable by operator
     */
    // ORAGSGL: Can only run after genesisStartEnds and genesisLockEnds are true
    function executeRound(uint periodIdx) external whenNotPaused onlyOperator {
        require(
            paramsByPeriod[PositionPeriod(periodIdx)].genesisStartEnds &&
                paramsByPeriod[PositionPeriod(periodIdx)].genesisLockEnds,
            "ORAGSGL"
        );

        (uint256 currentRoundId, int256 currentPrice) = _getPriceFromOracle();

        oracleLatestRoundId = uint256(currentRoundId);

        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];
        uint currentEpoch = period.currentEpoch;
        uint intervalDays = period.intervalDays;

        // CurrentEpoch refers to previous round (n-1)
        _safeLockRound(currentEpoch, periodIdx, currentRoundId, currentPrice);
        _safeEndRound(currentEpoch - intervalDays, periodIdx, currentRoundId, currentPrice);
        _calculateRewards(currentEpoch - intervalDays, periodIdx);

        // Increment currentEpoch to current round (n)
        period.currentEpoch += 1;
        _safeStartRound(period.currentEpoch, periodIdx);
    }

    /**
     * @notice Lock genesis round
     * @dev Callable by operator
     */
    // epoch: 1-7
    // ORAGS: Can only run after genesisStartEnds is triggered
    // ORGLF: Can only run when genesisLockRound is false
    function genesisLockRound(uint periodIdx) external whenNotPaused onlyOperator {
        require(paramsByPeriod[PositionPeriod(periodIdx)].genesisStartEnds, "ORAGS");
        require(!paramsByPeriod[PositionPeriod(periodIdx)].genesisLockEnds, "ORGLF");

        (uint256 currentRoundId, int256 currentPrice) = _getPriceFromOracle();

        oracleLatestRoundId = uint256(currentRoundId);

        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];
        _safeLockRound(period.currentEpoch, periodIdx, currentRoundId, currentPrice);

        period.currentEpoch += 1;
        _startRound(period.currentEpoch, periodIdx); //epoch: 2-8

        if (
            (period.unpausedEpoch == 0 &&
                (period.currentEpoch == paramsByPeriod[PositionPeriod(periodIdx)].intervalDays + 1)) ||
            (period.unpausedEpoch != 0 &&
                (period.currentEpoch == paramsByPeriod[PositionPeriod(periodIdx)].intervalDays + period.unpausedEpoch + 1))
        ) {
            paramsByPeriod[PositionPeriod(periodIdx)].genesisLockEnds = true;
        }
    }

    /**
     * @notice Start genesis round
     * @dev Callable by admin or operator
     */
    // ORGSO: Can only run genesisStartRound once
    function genesisStartRound(uint periodIdx) external whenNotPaused onlyOperator {
        require(!paramsByPeriod[PositionPeriod(periodIdx)].genesisStartEnds, "ORGSO");

        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];
        period.currentEpoch += 1;
        _startRound(period.currentEpoch, periodIdx);
        paramsByPeriod[PositionPeriod(periodIdx)].genesisStartEnds = true;
    }

    /**
     * @notice called by the admin to pause, triggers stopped state
     * @dev Callable by admin or operator
     */
    function pause() external whenNotPaused onlyAdminOrOperator {
        _pause();

        uint idx = 0;
        uint[] memory currentEpochList = new uint256[](3);
        while (idx <= uint(PositionPeriod.MONTH)) {
            currentEpochList[idx] = paramsByPeriod[PositionPeriod(idx)].currentEpoch;
            idx++;
        }

        emit Pause(currentEpochList[0], currentEpochList[1], currentEpochList[2]);
    }

    /**
     * @notice Claim all rewards in treasury
     * @dev Callable by admin
     */
    function claimTreasury() external nonReentrant onlyAdmin {
        uint256 currentTreasuryAmount = treasuryAmount;
        treasuryAmount = 0;
        _safeTransfer(adminAddress, currentTreasuryAmount);

        emit TreasuryClaim(currentTreasuryAmount);
    }

    /**
     * @notice called by the admin to unpause, returns to normal state
     * Reset genesis state. Once paused, the rounds would need to be kickstarted by genesis
     */
    function unpause() external whenPaused onlyAdmin {
        uint idx = 0;
        uint[] memory currentEpochList = new uint256[](3);
        while (idx <= uint(PositionPeriod.MONTH)) {
            Period storage period = paramsByPeriod[PositionPeriod(idx)];
            period.genesisStartEnds = false;
            period.genesisLockEnds = false;
            period.unpausedEpoch = period.currentEpoch;
            currentEpochList[idx] = period.currentEpoch;
            idx++;
        }
        _unpause();

        emit Unpause(currentEpochList[0], currentEpochList[1], currentEpochList[2]);
    }

    /**
     * @notice Set buffer and interval (in seconds)
     * @dev Callable by admin
     */
    // BSMIIS: bufferSeconds must be inferior to intervalSeconds
    function setBufferSeconds(uint256 _bufferSeconds) external whenPaused onlyAdmin {
        require(_bufferSeconds < paramsByPeriod[PositionPeriod(0)].intervalSeconds, "BSMIIS");
        bufferSeconds = _bufferSeconds;

        emit NewBufferSeconds(_bufferSeconds);
    }

    /**
     * @notice Set minBetAmount
     * @dev Callable by admin
     */
    // BAMS0: Bet Mount must be superior to 0
    function setMinBetAmount(uint256 _minBetAmount) external whenPaused onlyAdmin {
        require(_minBetAmount != 0, "BAMS0");
        minBetAmount = _minBetAmount;

        uint idx = 0;
        uint[] memory currentEpochList = new uint256[](3);
        while (idx <= uint(PositionPeriod.MONTH)) {
            currentEpochList[idx] = paramsByPeriod[PositionPeriod(idx)].currentEpoch;
            idx++;
        }

        emit NewMinBetAmount(currentEpochList[0], currentEpochList[1], currentEpochList[2], minBetAmount);
    }

    /**
     * @notice Set operator address
     * @dev Callable by admin
     */
    // CBZA: Cannot be zero address
    function setOperator(address _operatorAddress) external onlyAdmin {
        require(_operatorAddress != address(0), "CBZA");
        operatorAddress = _operatorAddress;

        emit NewOperatorAddress(_operatorAddress);
    }

    /**
     * @notice Set Oracle address
     * @dev Callable by admin
     */
    // CBZA: Cannot be zero address
    function setOracle(address _oracle) external whenPaused onlyAdmin {
        require(_oracle != address(0), "CBZA");
        oracleLatestRoundId = 0;
        oracle = INftPriceOracle(_oracle);

        // Dummy check to make sure the interface implements this function properly
        oracle.latestRoundData(slug);

        emit NewOracle(_oracle);
    }

    /**
     * @notice Set oracle update allowance
     * @dev Callable by admin
     */
    function setOracleUpdateAllowance(uint256 _oracleUpdateAllowance) external whenPaused onlyAdmin {
        oracleUpdateAllowance = _oracleUpdateAllowance;

        emit NewOracleUpdateAllowance(_oracleUpdateAllowance);
    }

    /**
     * @notice Set treasury fee
     * @dev Callable by admin
     */
    function setTreasuryFee(uint256 _treasuryFee) external whenPaused onlyAdmin {
        // require(_treasuryFee <= MAX_TREASURY_FEE, "Treasury fee too high");
        treasuryFee = _treasuryFee;

        uint idx = 0;
        uint[] memory currentEpochList = new uint256[](3);
        while (idx <= uint(PositionPeriod.MONTH)) {
            currentEpochList[idx] = paramsByPeriod[PositionPeriod(idx)].currentEpoch;
            idx++;
        }

        emit NewTreasuryFee(currentEpochList[0], currentEpochList[1], currentEpochList[2], treasuryFee);
    }

    /**
     * @notice It allows the owner to recover tokens sent to the contract by mistake
     * @param _token: token address
     * @param _amount: token amount
     * @dev Callable by owner
     */
    function recoverToken(address _token, uint256 _amount) external onlyOwner {
        IERC20(_token).safeTransfer(address(msg.sender), _amount);

        emit TokenRecovery(_token, _amount);
    }

    /**
     * @notice Set admin address
     * @dev Callable by owner
     */
    // CBZA: Cannot be zero address
    function setAdmin(address _adminAddress) external onlyOwner {
        require(_adminAddress != address(0), "CBZA");
        adminAddress = _adminAddress;

        emit NewAdminAddress(_adminAddress);
    }

    /**
     * @notice Returns round epochs and bet information for a user that has participated
     * @param user: user address
     * @param cursor: cursor
     * @param size: size
     */
    function getUserRounds(
        address user,
        uint256 cursor,
        uint256 size,
        uint periodIdx
    )
        external
        view
        returns (
            uint256[] memory,
            BetInfo[] memory,
            uint256
        )
    {
        uint256 length = size;
        Period storage period = paramsByPeriod[PositionPeriod(periodIdx)];

        if (length > period.userRounds[user].length - cursor) {
            length = period.userRounds[user].length - cursor;
        }

        uint256[] memory values = new uint256[](length);
        BetInfo[] memory betInfo = new BetInfo[](length);

        for (uint256 i = 0; i < length; i++) {
            values[i] = period.userRounds[user][cursor + i];
            betInfo[i] = period.ledger[values[i]][user];
        }

        return (values, betInfo, cursor + length);
    }

    /**
     * @notice Returns round epochs length
     * @param user: user address
     */
    function getUserRoundsLength(address user, uint periodIdx) external view returns (uint256) {
        return paramsByPeriod[PositionPeriod(periodIdx)].userRounds[user].length;
    }

    /**
     * @notice Get the claimable stats of specific epoch and user account
     * @param epoch: epoch
     * @param user: user address
     */
    function claimable(
        uint256 epoch,
        uint periodIdx,
        address user
    ) public view returns (bool) {
        BetInfo memory betInfo = paramsByPeriod[PositionPeriod(periodIdx)].ledger[epoch][user];
        Round memory round = paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch];
        if (round.lockPrice == round.closePrice) {
            return false;
        }
        return
            round.oracleCalled &&
            betInfo.amount != 0 &&
            !betInfo.claimed &&
            ((round.closePrice > round.lockPrice && betInfo.position == Position.Bull) ||
                (round.closePrice < round.lockPrice && betInfo.position == Position.Bear));
    }

    /**
     * @notice Get the refundable stats of specific epoch and user account
     * @param epoch: epoch
     * @param user: user address
     */
    function refundable(
        uint256 epoch,
        uint periodIdx,
        address user
    ) public view returns (bool) {
        BetInfo memory betInfo = paramsByPeriod[PositionPeriod(periodIdx)].ledger[epoch][user];
        Round memory round = paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch];
        return
            !round.oracleCalled &&
            !betInfo.claimed &&
            block.timestamp > round.closeTimestamp + bufferSeconds &&
            betInfo.amount != 0;
    }

    /**
     * @notice Calculate rewards for round
     * @param epoch: epoch
     */
    // RC: Rewards calculated
    function _calculateRewards(uint256 epoch, uint periodIdx) internal {
        require(
            paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].rewardBaseCalAmount == 0 &&
                paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].rewardAmount == 0,
            "RC"
        );
        Round storage round = paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch];
        uint256 rewardBaseCalAmount;
        uint256 treasuryAmt;
        uint256 rewardAmount;

        // Bull wins
        if (round.closePrice > round.lockPrice) {
            rewardBaseCalAmount = round.bullAmount;
            treasuryAmt = (round.totalAmount * treasuryFee) / 10000;
            rewardAmount = round.totalAmount - treasuryAmt;
        }
        // Bear wins
        else if (round.closePrice < round.lockPrice) {
            rewardBaseCalAmount = round.bearAmount;
            treasuryAmt = (round.totalAmount * treasuryFee) / 10000;
            rewardAmount = round.totalAmount - treasuryAmt;
        }
        // House wins
        else {
            rewardBaseCalAmount = 0;
            rewardAmount = 0;
            treasuryAmt = round.totalAmount;
        }
        round.rewardBaseCalAmount = rewardBaseCalAmount;
        round.rewardAmount = rewardAmount;

        // Add to treasury
        treasuryAmount += treasuryAmt;

        emit RewardsCalculated(epoch, rewardBaseCalAmount, rewardAmount, treasuryAmt);
    }

    /**
     * @notice End round
     * @param epoch: epoch
     * @param roundId: roundId
     * @param price: price of the round
     */
    // OERAL: Can only end round after round has locked
    // OERACT: Can only end round after closeTimestamp
    // OERWB: Can only end round within bufferSeconds
    function _safeEndRound(
        uint256 epoch,
        uint periodIdx,
        uint256 roundId,
        int256 price
    ) internal {
        require(paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].lockTimestamp != 0, "OERAL");
        require(block.timestamp >= paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].closeTimestamp, "OERACT");
        require(
            block.timestamp <= paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].closeTimestamp + bufferSeconds,
            "OERWB"
        );
        Round storage round = paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch];
        round.closePrice = price;
        round.closeOracleId = roundId;
        round.oracleCalled = true;

        emit EndRound(epoch, roundId, round.closePrice);
    }

    /**
     * @notice Lock round
     * @param epoch: epoch
     * @param roundId: roundId
     * @param price: price of the round
     */
    // OLRAS: Can only lock round after round has started
    // OLRALT: Can only lock round after lockTimestamp
    // OLRWB: Can only lock round within bufferSeconds
    function _safeLockRound(
        uint256 epoch,
        uint periodIdx,
        uint256 roundId,
        int256 price
    ) internal {
        require(paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].startTimestamp != 0, "OLRAS");
        require(block.timestamp >= paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].lockTimestamp, "OLRALT");
        require(
            block.timestamp <= paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch].lockTimestamp + bufferSeconds,
            "OLRWB"
        );
        Round storage round = paramsByPeriod[PositionPeriod(periodIdx)].rounds[epoch];
        round.closeTimestamp = block.timestamp + paramsByPeriod[PositionPeriod(periodIdx)].intervalSeconds;
        round.lockPrice = price;
        round.lockOracleId = roundId;

        emit LockRound(epoch, roundId, round.lockPrice);
    }

    /**
     * @notice Start round
     * Previous round n-2 must end
     * @param epoch: epoch
     */

    // ORAGST: Can only run after genesisStartEnds is true
    // OSRAPE: Can only start round after previous round has ended //{n-(intervalDays + 1)}
    // OSRAPCT: Can only start new round after previous round closeTimestamp //n-2
    function _safeStartRound(uint256 epoch, uint periodIdx) internal {
        require(paramsByPeriod[PositionPeriod(periodIdx)].genesisStartEnds, "ORAGST");
        require(
            paramsByPeriod[PositionPeriod(periodIdx)]
                .rounds[epoch - paramsByPeriod[PositionPeriod(periodIdx)].intervalDays - 1]
                .closeTimestamp != 0,
            "OSRAPE"
        );
        require(
            block.timestamp >=
                paramsByPeriod[PositionPeriod(periodIdx)]
                    .rounds[epoch - paramsByPeriod[PositionPeriod(periodIdx)].intervalDays - 1]
                    .closeTimestamp,
            "OSRAPCT"
        );
        _startRound(epoch, periodIdx);
    }

    /**
     * @notice Transfer BNB in a safe way
     * @param to: address to transfer BNB to
     * @param value: BNB amount to transfer (in wei)
     */
    // STF: TransferHelper: safe transfer failed
    function _safeTransfer(address to, uint256 value) internal {
        (bool success, ) = to.call{ value: value }("");
        require(success, "STF");
    }

    /**
     * @notice Start round
     * Previous round n-2 must end
     * @param epoch: epoch
     */
    function _startRound(uint256 epoch, uint periodIdx) internal {
