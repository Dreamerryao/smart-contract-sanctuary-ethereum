pragma solidity >=0.8.4;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@violetprotocol/ethereum-access-token/contracts/AuthCompatible.sol";

contract LendingPool is AuthCompatible, Ownable {
    // ERC20 instead of IERC20 because decimals()
    ERC20 private _USDC;

    // constants
    uint256 private constant _ethPriceInUSDC = 3000;
    uint256 private constant _collateralisationRatio = 200; // as percentage
    uint256 private constant _termPeriod = 31536000; // 1 non-leap year in seconds

    // changeable initial defaults per pool
    uint256 private _fixedRatePercent = 10;
    uint256 private _varRatePercent = 20;

    struct Loan {
        uint256 dueDate;
        uint256 amountOutstanding;
        uint256 collateralAmount;
        bool isFixedRate;
    }

    struct OpenLoans {
        bytes32[] loanIds;
        mapping(bytes32 => Loan) loans;
    }

    struct Pool {
        uint256 poolSize;
        uint256 totalShares;
        uint256 fundsOccupied;
        address collateralToken;
        uint256 fixedRate;
        uint256 variableRate;
        address[] lenders;
        address[] borrowers;
        mapping(address => uint256) poolShareByAddress;
        mapping(address => OpenLoans) openLoansByAddress;
    }

    mapping(address => Pool) internal pools; // lent token address => Pool for that token

    modifier printData() {
        console.logBytes4(msg.sig);
        _;
    }

    modifier onlySupportedTokens(address token) {
        require(token == address(0x0) || token == address(_USDC), "token not supported");
        _;
    }

    modifier onlyLenders(address token) {
        Pool storage pool = pools[token];

        require(pool.poolShareByAddress[msg.sender] > 0, "caller is not a lender");
        _;
    }

    constructor(address verifierAddress, address usdcAddress) AuthCompatible(verifierAddress) Ownable() {
        _USDC = ERC20(usdcAddress);

        // initialise USDC pool with ETH as collateral
        Pool storage usdcPool = pools[usdcAddress];
        usdcPool.collateralToken = address(0x0); // set ether as collateral for usdc pool
        usdcPool.fixedRate = _fixedRatePercent;
        usdcPool.variableRate = _varRatePercent;

        // initialise ETH pool with USDC as collateral
        Pool storage ethPool = pools[address(0x0)];
        ethPool.collateralToken = usdcAddress; // set usdc as collateral for eth pool
        ethPool.fixedRate = _fixedRatePercent;
        ethPool.variableRate = _varRatePercent;
    }

    /** ============================================================================

    Lender Functions

    * lend
    * withdraw
    * modifyFixedRate
    * modifyVariableRate

    ============================================================================ */

    function lend(
        uint8 v,
        bytes32 r,
        bytes32 s,
        uint256 expiry,
        address token,
        uint256 amount
    ) public payable requiresAuth(v, r, s, expiry) onlySupportedTokens(token) {
        _addToPool(pools[token], msg.sender, amount);

        // check if we are lending ETH
        if (token == address(0x0)) {
            require(amount == msg.value, "ETH to lend does not match amount sent");
        } else {
            // this call performs balance and allowance checks so we don't have to
            ERC20(token).transferFrom(msg.sender, address(this), amount);
        }
    }

    function withdraw(
        uint8 v,
        bytes32 r,
        bytes32 s,
        uint256 expiry,
        address token,
        uint256 amount
    ) public requiresAuth(v, r, s, expiry) {
        _deductFromPool(token, msg.sender, amount);

        // check if we are withdrawing ETH
        if (token == address(0x0)) {
            payable(msg.sender).transfer(amount);
        } else {
            ERC20(token).transfer(msg.sender, amount);
        }
    }

    function modifyFixedRate(address token, uint256 newFixedRatePercentage) public onlyLenders(token) {
        Pool storage pool = pools[token];

        pool.fixedRate = newFixedRatePercentage;
    }

    function modifyVariableRate(address token, uint256 newVariableRatePercentage) public onlyLenders(token) {
        Pool storage pool = pools[token];

        pool.variableRate = newVariableRatePercentage;
    }

    /** ============================================================================

    Borrower Functions

    * borrow
    * repay
    * toggleRate

    ============================================================================ */

    function borrow(
        uint8 v,
        bytes32 r,
        bytes32 s,
        uint256 expiry,
        address token,
        uint256 amount,
        bool isFixedRate
    ) public payable printData requiresAuth(v, r, s, expiry) returns (bytes32 loanId) {
        Pool storage pool = pools[token];

        require(pool.poolSize >= amount, "insufficient pool funds");

        // we are using static pricing for borrow amount vs collateral
        uint256 collateralAmount = getCollateralRequirements(token, amount);

        if (pool.collateralToken == address(0x0)) {
            require(msg.value >= collateralAmount, "insufficient collateral");

            loanId = _openLoan(msg.sender, token, amount, isFixedRate, collateralAmount);

            // pay out the lended funds
            ERC20(token).transfer(msg.sender, amount);
        } else {
            require(collateralAmount > 0, "borrow amount too low");
            // this call performs balance and allowance checks so we don't have to
            ERC20(pool.collateralToken).transferFrom(msg.sender, address(this), collateralAmount);

            loanId = _openLoan(msg.sender, token, amount, isFixedRate, collateralAmount);

            // pay out the lended funds
            payable(msg.sender).transfer(amount);
        }
    }

    function repayInFull(
        uint8 v,
        bytes32 r,
        bytes32 s,
        uint256 expiry,
        address token,
        bytes32 loanId
    ) public payable requiresAuth(v, r, s, expiry) {
        Pool storage pool = pools[token];
        Loan storage loan = pool.openLoansByAddress[msg.sender].loans[loanId];

        require(loan.amountOutstanding > 0, "invalid loan specified");

        uint256 totalRepayable = getTotalAmountOwed(msg.sender, token, loanId);
        uint256 collateralAmount = loan.collateralAmount;

        // Always repaid in full
        _closeLoan(pool, msg.sender, loanId);

        if (token == address(0x0)) {
            require(msg.value >= totalRepayable, "insufficient repayment amount");

            // return collateral
            ERC20(pool.collateralToken).transfer(msg.sender, collateralAmount);

            // refund the difference if overpaid
            payable(msg.sender).transfer(msg.value - totalRepayable);
        } else {
            // repay loan
            ERC20(token).transferFrom(msg.sender, address(this), totalRepayable);

            // return collateral
            if (pool.collateralToken == address(0x0)) {
                payable(msg.sender).transfer(collateralAmount);
            } else {
                ERC20(pool.collateralToken).transfer(msg.sender, collateralAmount);
            }
        }
    }

    function toggleRate(address token, bytes32 loanId) public {
        Pool storage pool = pools[token];
        Loan storage loan = pool.openLoansByAddress[msg.sender].loans[loanId];

        require(loan.amountOutstanding > 0, "invalid loan specified");

        loan.isFixedRate = !loan.isFixedRate;
    }

    /** ============================================================================

    Getter functions

    View functions for storage variable visibility

    ================================================================================ */

    function getOpenLoans(address token, address borrower) public view returns (bytes32[] memory) {
        return pools[token].openLoansByAddress[borrower].loanIds;
    }

    function getCollateralRequirements(address tokenToBorrow, uint256 amountToBorrow)
        public
        view
        onlySupportedTokens(tokenToBorrow)
        returns (uint256)
    {
        Pool storage pool = pools[tokenToBorrow];
        return (_collateralisationRatio * _getPriceOf(tokenToBorrow, amountToBorrow, pool.collateralToken)) / 100;
    }

    function getTotalAmountOwed(
        address borrower,
        address tokenBorrowed,
        bytes32 loanId
    ) public view onlySupportedTokens(tokenBorrowed) returns (uint256) {
        Pool storage pool = pools[tokenBorrowed];
        Loan storage loan = pool.openLoansByAddress[borrower].loans[loanId];

        uint256 principal = loan.amountOutstanding;
        uint256 interestRate = loan.isFixedRate ? pool.fixedRate : pool.variableRate;
        uint256 totalRepayable = principal + ((interestRate * principal) / 100);
        return totalRepayable;
    }

    /** ============================================================================

    Internal functions

    Helpers and utilities usable by other contract functions, not to be called
    externally. 

    ================================================================================ */

    function _addToPool(
        Pool storage pool,
        address lender,
        uint256 amount
    ) internal {
        pool.poolSize += amount;

        uint256 sharesAddable = _calculateShare(pool, amount);
        if (pool.poolShareByAddress[lender] == 0) {
            pool.lenders.push(lender);
        }
        pool.poolShareByAddress[lender] += sharesAddable;
        pool.totalShares += sharesAddable;
    }

    function _deductFromPool(
        address token,
        address lender,
        uint256 amount
    ) internal {
        Pool storage pool = pools[token];
        uint256 totalDeductible = calculateTotalLending(token, lender);
        require(amount <= totalDeductible, "specified amount greater than lending amount");

        uint256 sharesDeductible = _calculateShare(pool, amount);

        pool.poolSize -= amount;
        pool.totalShares -= sharesDeductible;
        pool.poolShareByAddress[lender] -= sharesDeductible;

        // delete lender from list of lenders if fully unlent
        if (pool.poolShareByAddress[lender] == 0) {
            for (uint256 i = 0; i < pool.lenders.length; i++) {
                if (pool.lenders[i] == lender) {
                    pool.lenders[i] = pool.lenders[pool.lenders.length - 1];
                    pool.lenders.pop();
                }
            }
        }
    }

    function _openLoan(
        address borrower,
        address token,
        uint256 amount,
        bool isFixedRate,
        uint256 collateralAmount
    ) internal returns (bytes32 loanId) {
        Pool storage pool = pools[token];
        pool.fundsOccupied += amount;
        if (pool.openLoansByAddress[borrower].loanIds.length == 0) pool.borrowers.push(borrower);

        OpenLoans storage openLoans = pool.openLoansByAddress[borrower];
        if (openLoans.loanIds.length == 0) pool.borrowers.push(borrower);

        loanId = keccak256(abi.encodePacked(borrower, token, amount, blockhash(block.number)));
        require(openLoans.loans[loanId].amountOutstanding == 0, "LendingPool: loan already exists");

        openLoans.loanIds.push(loanId);
        openLoans.loans[loanId] = Loan(block.timestamp + _termPeriod, amount, collateralAmount, isFixedRate);
    }

    function _closeLoan(
        Pool storage pool,
        address borrower,
        bytes32 loanId
    ) internal {
        OpenLoans storage openLoans = pool.openLoansByAddress[borrower];
        pool.fundsOccupied -= openLoans.loans[loanId].amountOutstanding;

        delete openLoans.loans[loanId];
        for (uint256 i = 0; i < openLoans.loanIds.length; i++) {
            if (openLoans.loanIds[i] == loanId) {
                openLoans.loanIds[i] = openLoans.loanIds[openLoans.loanIds.length - 1];
                openLoans.loanIds.pop();
            }
        }

        if (openLoans.loanIds.length == 0) {
            for (uint256 i = 0; i < pool.borrowers.length; i++) {
                if (pool.borrowers[i] == borrower) {
                    pool.borrowers[i] = pool.borrowers[pool.borrowers.length - 1];
                    pool.borrowers.pop();
                }
            }
        }
    }

    function _getPriceOf(
        address token,
        uint256 amount,
        address denominatedIn
    ) internal view returns (uint256) {
        if (token == address(0x0)) {
            return _getETHPrice(amount, denominatedIn);
        } else if (token == address(_USDC)) {
            return _getUSDCPrice(amount, denominatedIn);
        }

        revert("unsupported token");
    }

    function _getETHPrice(uint256 amount, address denomination) internal view returns (uint256) {
        if (denomination == address(_USDC)) {
            return amount * _ethPriceInUSDC;
        }

        revert("unsupported denomination");
    }

    function _getUSDCPrice(uint256 amount, address denomination) internal pure returns (uint256) {
        if (denomination == address(0x0)) {
            return amount / _ethPriceInUSDC;
        }

        revert("unsupported denomination");
    }

    // Calculates the total amount of funds being lent by a lender, including accrued interest
    function calculateTotalLending(address token, address lender) public view returns (uint256) {
        Pool storage pool = pools[token];
        uint256 poolShare = (pool.poolShareByAddress[lender] * 100) / pool.totalShares;
        uint256 poolOwnershipAmount = (poolShare * pool.poolSize) / 100;

        return poolOwnershipAmount;
    }

    // Calculates the total number of shares associated with an amount of funds
    function _calculateShare(Pool storage pool, uint256 amount) internal view returns (uint256) {
        if (pool.totalShares == 0) return amount;
        uint256 poolOwnershipPercentage = (amount * 100) / pool.poolSize;
        uint256 poolShares = (poolOwnershipPercentage * pool.totalShares) / 100;

        return poolShares;
    }
}

