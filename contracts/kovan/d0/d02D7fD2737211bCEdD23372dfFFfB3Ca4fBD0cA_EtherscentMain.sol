pragma solidity ^0.8.10;

library PRFAndSort {

    function pseudorandom_hash(bytes memory i, uint j) internal pure returns (uint r) {
        unchecked {
            r = uint(keccak256(abi.encodePacked(i)))%(j);
        }
    }

    function pseudorandom_array(uint seed, uint q, uint min, uint max) internal view returns (uint[] memory) {
        require(max > min);
        uint[] memory arr = new uint[](q);
        uint i;
        uint j;
        while (i < q) {
            uint r = pseudorandom_hash(abi.encodePacked(blockhash(seed-j)), max+1-min) + min;
            bool duplicate = _checkDuplicate(arr, i, r);
            if (duplicate) {
                ++j;
                continue;
            }
            arr[i] = r;
            ++i;
            ++j;
        }
        return arr;
    }

    function pseudorandom_array_with_fix_sum(uint seed, uint q, uint s) internal view returns (uint[] memory) {
        uint[] memory arr = new uint[](q+1);
        uint i;
        uint j;
        while (arr[q-2] == 0) {
            uint r = pseudorandom_hash(abi.encodePacked(blockhash(seed-j)), s-1) + 1;
            bool duplicate = _checkDuplicate(arr, i, r);
            if (duplicate) {
                ++j;
                continue;
            }
            arr[i] = r;
            ++i;
            ++j;
        }
        arr[q-1] = uint(0);
        arr[q] = s;
        arr = _getDiffArray(arr);
        return arr;
    }

    function _checkDuplicate(uint[] memory arr, uint i, uint r) private pure returns (bool) {
        for (uint i_; i_ < i; ++i_) {
            if (arr[i_] == r) {
                return true;
            }
        }
        return false;
    }

    function _getDiffArray(uint[] memory arr) private pure returns (uint[] memory) {
        uint q_ = arr.length - 1;
        uint[] memory resArr = new uint[](q_);
        arr = quickSort(arr);
        uint i;
        uint j = 1;
        while (j <= q_) {
            resArr[i] = arr[j] - arr[i];
            ++i;
            ++j;
        }
        return resArr;
    }

    function _quickSort(uint[] memory arr, int left, int right) private pure {
        int i = left;
        int j = right;
        if (i == j) return;
        uint pivot = arr[uint(left + (right - left) / 2)];
        while (i <= j) {
            while (arr[uint(i)] < pivot) ++i;
            while (pivot < arr[uint(j)]) --j;
            if (i <= j) {
                (arr[uint(i)], arr[uint(j)]) = (arr[uint(j)], arr[uint(i)]);
                ++i;
                --j;
            }
        }
        if (left < j)
            _quickSort(arr, left, j);
        if (i < right)
            _quickSort(arr, i, right);
    }

    function quickSort(uint[] memory arr) internal pure returns (uint[] memory) {
        _quickSort(arr, int(0), int(arr.length-1));
        return arr;
    }
}

