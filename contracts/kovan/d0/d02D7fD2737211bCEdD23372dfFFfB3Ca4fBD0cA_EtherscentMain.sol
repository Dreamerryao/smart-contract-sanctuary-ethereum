contract EtherscentSVG {
    using Strings for uint256;

    function getHeader() private pure returns (string memory header) {
        header = "<?xml version='1.0' encoding='utf-8'?><!-- Generator: Adobe Illustrator 25.0.1, SVG Export Plug-In . SVG Version: 6.00 Build 0)--><svg version='1.1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' x='0px' y='0px' viewBox='0 0 503 653' style='enable-background:new 0 0 503 653;' xml:space='preserve'><style type='text/css'>.s0{fill:#C9DCC5;}.s1{fill:#F2D1B1;}.s2{fill:#CECEF6;}.s3{fill:#CECEF6;}.s4{fill:#FFF8C0;}.s5{fill:#E8D0A7;}.s6{fill:#D0F0C0;}.s7{fill:#A9BA9D;}.s8{fill:#9AE7CD;}.s9{fill:#9AE7CD;}.s10{fill:#9AC791;}.s11{fill:#E9E45D;}.s12{fill:#99D97D;}.s13{fill:#EBBA60;}.s14{fill:#F6A665;}.s15{fill:#C7EA46;}.s16{fill:#41C89D;}.s17{fill:#B8CC38;}.s18{fill:#8AB863;}.s19{fill:#8AB863;}.s20{fill:#FDA73D;}.s21{fill:#DCA232;}.s22{fill:#DD8153;}.s23{fill:#D88745;}.s24{fill:#9F844F;}.s25{fill:#F663AC;}.s26{fill:#C3724E;}.s27{fill:#DD750D;}.s28{fill:#E95D56;}.s29{fill:#659A1E;}.s30{fill:#BB7311;}.s31{fill:#D2691E;}.s32{fill:#996454;}.s33{fill:#4B7B5B;}.s34{fill:#5F6A58;}.s35{fill:#C24D46;}.s36{fill:#9D5547;}.s37{fill:#3F626E;}.s38{fill:#405E5C;}.s39{fill:#644B45;}.s100{fill:none;}</style><g><g>";
    }

    function getFooter() private pure returns (string memory footer) {
        footer = "</g><rect class='s100' width='503' height='653'/></g></svg>";
    }

    function _x_y(uint256 n, uint256 r, uint256 d, uint256 sx, uint256 sy) private pure returns (uint256 x, uint256 y) {
        unchecked {
            if (n % r < r/2+1) {
                x = sx + d*2*(n%r);
                y = sy - d*2*(n/r);
            } else {
                y = sy - d - d*2*((n-r/2)/r);
                x = sx + d - d*2*((n-r/2)%r-1);
            }
        }
    }

    function _generateRect(uint256[] memory colors, uint256[] memory dis) private pure returns (string memory rects) {
        uint256 colors_len = colors.length;
        uint256 dis_len = dis.length;
        require(colors_len == dis_len);
        uint256 n;
        for (uint256 i; i < colors_len; ++i) {
            string memory color_hex = colors[i].toHexString();
            uint256 q = dis[i];
            for (uint256 j; j < q; ++j) {
                (uint256 x, uint256 y) = _x_y(n, 13, 37, 11, 604);
                rects = string(abi.encodePacked(rects, "<rect x='", x.toString(), ".1'", " y='", y.toString(), ".1' width='37' height='37' class='s", color_hex, "'/>"));
                ++n;
            }
        }
    }

    function generateSVG(uint256[] memory colors, uint256[] memory dis) internal pure returns (string memory) {
        return string(
            abi.encodePacked(
                getHeader(),
                _generateRect(colors, dis),
                getFooter()
            )
        );
    }

}

