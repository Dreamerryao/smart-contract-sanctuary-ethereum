pragma solidity ^0.8.10;

import "./libs/PRFAndSort.sol";

contract EtherscentColorDistribution {

    function _pickColorQuantities(uint256 seed) private view returns (uint256 q) {
        uint8[3] memory colorQuantities = [7,13,22];
        uint256 r = PRFAndSort.pseudorandom_hash(abi.encodePacked(blockhash(seed)), 3);
        q = colorQuantities[r];
    }

    function _pickColors(uint256 seed, uint256 q) private view returns (uint[] memory colors) {
        uint[40] memory palette;
        for (uint256 i; i < 40; ++i) {
            palette[i] = i;
        }
        colors = PRFAndSort.pseudorandom_array(seed, q, 0, 39);
        colors = PRFAndSort.quickSort(colors);
    }

    function _pickDistribution(uint256 seed, uint256 q, uint256 s) private view returns (uint[] memory dis) {
        dis = PRFAndSort.pseudorandom_array_with_fix_sum(seed, q, s);
    }

    function colorDistribution(uint256 seed) internal view returns (uint256 q, uint256[] memory colors, uint256[] memory dis) {
        q = _pickColorQuantities(seed);
        colors = _pickColors(seed, q);
        dis = _pickDistribution(seed, q, 111);
    }

    function q_value(uint256 seed) internal view returns (uint256 q) {
        q = _pickColorQuantities(seed);
    }

    function color(uint256 seed) internal view returns (uint256[] memory colors) {
        uint256 q = _pickColorQuantities(seed);
        colors = _pickColors(seed, q);
    }

    function di(uint256 seed) internal view returns (uint256[] memory dis) {
        uint256 q = _pickColorQuantities(seed);
        dis = _pickDistribution(seed, q, 111);
    }    
    
}

