pragma solidity ^0.7.4;
pragma abicoder v2;
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {SafeMath} from "@openzeppelin/contracts/math/SafeMath.sol";
import {Clones} from "@openzeppelin/contracts/proxy/Clones.sol";

import {PercentageMath} from "../libraries/math/PercentageMath.sol";
import {Errors} from "../libraries/helpers/Errors.sol";

import {AddressProvider} from "../core/AddressProvider.sol";
import {GearToken} from "../tokens/GearToken.sol";
import {IGearToken} from "../interfaces/IGearToken.sol";
import {StepVesting} from "../tokens/Vesting.sol";
import {Constants} from "../libraries/helpers/Constants.sol";
import {Errors} from "../libraries/helpers/Errors.sol";

import "hardhat/console.sol";

contract TokenDistributor is Ownable {
    using SafeMath for uint256;

    enum ContributorType {
        A, // A-type voting power & A-type vesting parameters
        B, // B-type voting power & B-type vesting parameters
        ZERO_VOTING_POWER  // zero voting power & B-type vesting parameters
    }

    struct TokenShare {
        address holder; // address of contributor wallet,
        uint256 amount; // amount in tokens which should be transferred to contributor
        bool isCompany; // flag, which used for contributor B only, If set, the voting contract has zero voting power.
    }

    struct VestingContract {
        address contractAddress; // vesting contract address
        ContributorType votingPower; // enum for voting power(0 means "A", 1 means "B" and 2 means "ZERO VOTING")
    }

    uint256 public constant steps = 10_000;
    uint256 public constant defaultContributorA = 90_00;
    uint256 public constant defaultContributorB = 70_00;

    GearToken public gearToken;
    TokenShare[] public vcs;
    bool public tokenSent;

    mapping(address => VestingContract) public vestingContracts;

    uint256 public weightA;
    uint256 public weightB;

    event NewWeights(uint256 weightA, uint256 weightB);
    event HolderUpdated();

    constructor(AddressProvider addressProvider) {
        gearToken = GearToken(addressProvider.getGearToken());
        _updateVotingWeights(defaultContributorA, defaultContributorB);
    }

    function initVesting(
        address vc,
        TokenShare memory tokenShare,
        ContributorType contributorType
    ) internal {
        StepVesting(vc).initialize(
            IGearToken(address(gearToken)),
            block.timestamp,
            Constants.SECONDS_PER_YEAR,
            (
                contributorType == ContributorType.A
                    ? Constants.SECONDS_PER_YEAR
                    : Constants.SECONDS_PER_ONE_AND_HALF_YEAR
            ) / steps,
            0,
            tokenShare.amount / steps,
            steps,
            tokenShare.holder
        ); // T:[TD-1]

        vestingContracts[tokenShare.holder] = VestingContract(
            vc,
            contributorType
        ); // T:[TD-1]

        gearToken.transfer(vc, tokenShare.amount); // T:[TD-1]
    }

    function transferTokens(
        TokenShare[] memory contributorsA,
        TokenShare[] memory contributorsB
    )
        external
        onlyOwner // T:[TD-3]
    {
        require(!tokenSent, "Tokens are already sent"); // T:[TD-8]
        require(
            contributorsA.length > 0,
            "Should be at least one contributorA"
        ); // T:[TD-7]

        StepVesting masterVC = new StepVesting(); // T:[TD-1]

        for (uint256 i = 0; i < contributorsA.length; i++) {
            address vc = i == 0
                ? address(masterVC) // T:[TD-1]
                : Clones.clone(address(masterVC)); // T:[TD-1]

            initVesting(vc, contributorsA[i], ContributorType.A); // T:[TD-1]
        }

        for (uint256 i = 0; i < contributorsB.length; i++) {
            address vc = Clones.clone(address(masterVC)); // T:[TD-1]

            initVesting(
                vc,
                contributorsB[i],
                contributorsB[i].isCompany
                    ? ContributorType.ZERO_VOTING_POWER
                    : ContributorType.B
            ); // T:[TD-1]
        }

        require(
            gearToken.balanceOf(address(this)) == 0,
            "Not all tokens were distributed"
        ); // T:[TD-1, 6]

        tokenSent = true; // T:[TD-1]
    }

    function balanceOf(address holder) external view returns (uint256) {
        uint256 vestingBalanceWeighted; // T:[TD-2]

        VestingContract memory vc = vestingContracts[holder]; // T:[TD-2]
        if (
            vc.contractAddress != address(0) &&
            vc.votingPower != ContributorType.ZERO_VOTING_POWER
        ) {
            address receiver = StepVesting(vc.contractAddress).receiver(); // T:[TD-2]
            require(receiver == holder, "Holder is not receiver"); // T:[TD-2]

            vestingBalanceWeighted = gearToken
            .balanceOf(vc.contractAddress)
            .mul(vc.votingPower == ContributorType.A ? weightA : weightB)
            .div(PercentageMath.PERCENTAGE_FACTOR); // T:[TD-2]
        }

        return vestingBalanceWeighted.add(gearToken.balanceOf(holder)); // T:[TD-2]
    }

    function updateVestingHolder(address prevOwner) external {
        VestingContract memory vc = vestingContracts[prevOwner];
        require(
            vc.contractAddress != address(0),
            Errors.ZERO_ADDRESS_IS_NOT_ALLOWED
        );

        address currentOwner = StepVesting(vc.contractAddress).receiver();

        delete vestingContracts[prevOwner];
        vestingContracts[currentOwner] = vc;
    }

    function updateVotingWeights(uint256 _weightA, uint256 _weightB)
        external
        onlyOwner // T:[TD-3]
    {
        _updateVotingWeights(_weightA, _weightB); // T:[TD-4]
    }

    function _updateVotingWeights(uint256 _weightA, uint256 _weightB) internal {
        require(
            _weightA <= PercentageMath.PERCENTAGE_FACTOR &&
                _weightB <= _weightA,
            "Incorrect weights"
        ); // T:[TD-4, 5]
        weightA = _weightA; // T:[TD-4]
        weightB = _weightB; // T:[TD-4]
        emit NewWeights(weightA, weightB); // T:[TD-4]
    }
}

