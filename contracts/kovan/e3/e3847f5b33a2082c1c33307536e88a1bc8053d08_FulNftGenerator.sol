     * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, ``from``'s `tokenId` will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}

    /**
     * @dev Hook that is called after any transfer of tokens. This includes
     * minting and burning.
     *
     * Calling conditions:
     *
     * - when `from` and `to` are both non-zero.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}
}



    /**
    *@title "FULNftGenerator" to mint Nft
    *@author Arpit Anand
    *@dev This smart contract is ERC721 contract to mint NFT.
    * This contact's functions is called from FULMint contract.
    * 
    */
contract FulNftGenerator is ERC721,Ownable{
    using SafeMath for uint256;
  
   
    /*
    *@dev Name of NFT is "FPLNFT".
    *      Symbol of NFT is "FPL".
    */
    constructor() ERC721("FPLNFT", "FPL") {}

    /**
    *@dev To struct the NFT's details
    **/
    struct setNFT{
        uint256 ID;
        string name;
        string tokenUri;
    }


    mapping(uint256 => bool)public lockedId;        // It will map TokenId is locked or not.
    mapping(uint256 => uint)public lockedTime;      // It will map locked time of TokenID.
    mapping(uint256 => setNFT) public NFTDetails;   // This mapping will map TokenId to structure of NFT 
    mapping(uint => string)public NFTIdToRarity;    // Rarity of TokenId
    mapping(uint => string)public NFTIdToPosition;  // position of TokenId
    mapping(address => uint256[])public userNftId;  // Array of NFT Id hold by user Address.

    
    address private contractAddress; // TO store FulMint contract address. 

    /**
    * This modifier discribe only admin or contractAddress(FULMint address) has access.
    **/

    modifier onlyBy() {
      require(msg.sender == contractAddress || msg.sender == owner(), "Caller is not authorized.");
      _;
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default.
    **/

    function setBaseURI(string memory BaseUri_)public onlyBy{
        baseURI = BaseUri_;
    }

    //set the adress to access the all function which is modified by "onlyBy" modifier
    function setOperatorAccount(address account_)public onlyOwner{
        contractAddress = account_;
    }

  

    /**
     * @dev mintNFT takes all details of NFT and minting on user address.
     *
     * @param tokenId representing the NFT's details, and unique.
     * @param _name  The Nft of player name.
     * @param tokenURI_ refer IPFS location of Image.
     * @param user adress that call the mintNFT funtion, NFT will mint on user address.
     * @param _position playing positon of playerId
     * @param _rarity player rarity like "common" or "rare" or "ledegendry".
    **/
    function mintNFT(uint256 tokenId, string memory _name, string memory tokenURI_, address user, string memory _position, string memory _rarity) public onlyBy  {
        setNFT storage instanceNFT = NFTDetails[tokenId];
        _mint(user, tokenId);
        _setTokenURI(tokenId, tokenURI_);
        instanceNFT.ID = tokenId;
        instanceNFT.name = _name; 
        instanceNFT.tokenUri = tokenURI_;
        NFTIdToRarity[tokenId] = _rarity;
        NFTIdToPosition[tokenId]=_position;
        userNftId[user].push(tokenId);
    }

    /**
    * @dev To find number of NFT holds by user.
    *
    * @param user Addrees of user
    * @return array of NFT Id of user.
    *
    **/
    function nftOfUser(address user)public view returns(uint256[] memory){
        return userNftId[user];
    }
    
    /**
    * @dev Lock perticular NFTId For given time of duration.
    *
    * @param _id Takes NFT Id to lock.
    * @param duration How long will nft be locked.
    **/
    function toLockId(uint _id, uint duration)public onlyBy{
        lockedTime[_id] = block.timestamp.add(duration);    
    }


    /**
    * @dev To know who is the owner of NFT ID
    *
    * @param _id Takes NFT Id.
    * @return Owner address of NFT.
    **/
    function ownerOfNft(uint _id)public view onlyBy returns(address){
        return ownerOf(_id);
    }

    /**
     * @dev Safely transfers `tokenId` token from `from` to `to`.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `tokenId` token must exist and be owned by `from`.
     * - If the caller is not `from`, it must be approved to move this token by either {approve} or {setApprovalForAll}.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
    **/

    function transferFrom(address from,address to,uint256 tokenId) public override {
        uint256 temp;
        require(lockedTime[tokenId] < block.timestamp,"Lock Time is not over");
        for (uint256 i; i < userNftId[from].length; i++) {
            if (userNftId[from][i] == tokenId) {
                userNftId[from][i] = userNftId[from][userNftId[from].length - 1];
                temp = userNftId[from].length - 1;
                userNftId[from].pop();
                userNftId[to].push(temp);
                break;
            }
        }
        _transfer(from, to, tokenId);
    }

    /**
     * @dev Safely transfers `tokenId` token from `from` to `to`.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `tokenId` token must exist and be owned by `from`.
     * - If the caller is not `from`, it must be approved to move this token by either {approve} or {setApprovalForAll}.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
    **/
    function safeTransferFrom(address from,address to,uint256 tokenId) public  override  {
        require(lockedTime[tokenId] < block.timestamp,"Lock Time is not over");
        uint256 temp;
        for (uint256 i; i < userNftId[from].length; i++) {
            if (userNftId[from][i] == tokenId) {
                userNftId[from][i] = userNftId[from][userNftId[from].length - 1];
                temp = userNftId[from].length - 1;
                userNftId[from].pop();
                userNftId[to].push(temp);
                break;
            }
        }
        safeTransferFrom(from, to, tokenId, "");
    }

    /**
     * @dev Safely transfers `tokenId` token from `from` to `to`.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `tokenId` token must exist and be owned by `from`.
     * - If the caller is not `from`, it must be approved to move this token by either {approve} or {setApprovalForAll}.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
    */

    function safeTransferFrom(address from,address to,uint256 tokenId,bytes memory _data) public  override  {
        require(_isApprovedOrOwner(_msgSender(), tokenId),"ERC721: transfer caller is not owner nor approved");
        require(lockedTime[tokenId] < block.timestamp,"Lock Time is not over");
        uint256 temp;
        for (uint256 i; i < userNftId[from].length; i++) {
            if (userNftId[from][i] == tokenId) {
                userNftId[from][i] = userNftId[from][userNftId[from].length - 1];
                temp = userNftId[from].length - 1;
                userNftId[from].pop();
                userNftId[to].push(temp);
                break;
            }
        }

        _safeTransfer(from, to, tokenId, _data);
    }

    /**
    *@dev only owner or call from "FULMint" contract's function can burn NFT.
    */    
    function burnNFT(uint256 tokenId,address user)public onlyBy {
        require(lockedTime[tokenId] < block.timestamp,"Lock Time is not over");
        for (uint256 i; i < userNftId[user].length; i++) {
            if (userNftId[user][i] == tokenId) {
                userNftId[user][i] = userNftId[user][userNftId[user].length - 1];
                userNftId[user].pop();
                break;
            }
        }
        _burn(tokenId);

    }

    /**
    * @dev A user can upgrade his player from common to rare. 
    *      By burning the old rarity card and spending 500 tokens to upgrade the player. 
    *      A user can upgrade his rare player to legendary by burning his rare player and spending 1000 tokens.
    *
    * @param NftId Nft Id which user wants to Upgrade.
    * @param rarity user's desired rarity
    * @param nftIdToBurn any old "rare" type of NFTId.
    * @param tokenURI_ updated uri of NFT.
    *
    **/ 

    function upgradeToken(uint256 NftId,string memory rarity, uint256 nftIdToBurn,string memory tokenURI_ )public  {
        //setNFT storage instanceOfNFT = NFTDetails[NftId];

        if(keccak256(bytes(rarity)) == keccak256(bytes("common"))){
            _burn(nftIdToBurn);
            NFTIdToRarity[NftId] = "rare";

        }
        if(keccak256(bytes(rarity)) == keccak256(bytes("rare"))){
            _burn(nftIdToBurn);
            NFTIdToRarity[NftId] = "legendary";

        }
        NFTDetails[NftId].tokenUri = tokenURI_;
        
    }
    
    /**
     * @dev Returns the rarity NFT Id.
    **/
    function rarityOfNft(uint256 nftId)public view returns(string memory){
        return NFTIdToRarity[nftId];
    }    
    
}