    function pendingCafe(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accCafePerShare = pool.accCafePerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply > 0) {
            uint256 CafeForFarmer;
            (, CafeForFarmer, , ,) = getPoolReward(pool.lastRewardBlock, block.number, pool.allocPoint);
            accCafePerShare = accCafePerShare.add(CafeForFarmer.mul(1e12).div(lpSupply));
        }
        return user.amount.mul(accCafePerShare).div(1e12).sub(user.rewardDebt);
    }


