    function getPoolReward(uint256 _from, uint256 _to, uint256 _allocPoint) public view returns (uint256 forDev, uint256 forFarmer, uint256 forVault, uint256 forCom, uint256 forPhi) {
        uint256 multiplier = getMultiplier(_from, _to);
        uint256 amount = multiplier.mul(cafePerBlock).mul(_allocPoint).div(totalAllocPoint);
        uint256 CafeCanMint = cake.cap().sub(cake.totalSupply());

        if (CafeCanMint < amount) {
            forDev = 0;
            forFarmer = CafeCanMint;
            forVault = 0;
            forCom = 0;
            forPhi = 0;
        }
        else {
            forDev = amount.mul(PERCENT_FOR_DEV).div(100).div(1e8);
            forFarmer = amount.mul(90).div(100);
            forVault = amount.mul(PERCENT_FOR_VAULT).div(100).div(1e8);
            forCom = amount.mul(PERCENT_FOR_COM).div(100).div(1e8);
            forPhi = amount.mul(PERCENT_FOR_PHI).div(100).div(1e8);
        }
    }

