    function updatePool(uint256 _pid) public validatePool(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }

        uint256 CafeForDev;
        uint256 CafeForFarmer;
        uint256 CafeForVault;
        uint256 CafeForCom;
        uint256 CafeForPhi;
        (CafeForDev, CafeForFarmer, CafeForVault, CafeForCom, CafeForPhi) = getPoolReward(pool.lastRewardBlock, block.number, pool.allocPoint);
        cake.mint(address(bean), CafeForFarmer);
        pool.accCafePerShare = pool.accCafePerShare.add(CafeForFarmer.mul(1e12).div(lpSupply));
        pool.lastRewardBlock = block.number;

        if (CafeForDev > 0) cake.mint(address(devaddr), CafeForDev);
        if (CafeForVault > 0) cake.mint(vaultaddr, CafeForVault);
        if (CafeForCom > 0) cake.mint(comfundaddr, CafeForCom);
