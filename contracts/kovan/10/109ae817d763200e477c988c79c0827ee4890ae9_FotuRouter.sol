pragma solidity ^0.8.0;







contract FotuRouter is Multicall {
    CoinDeposit public coinDeposit;
    FotuNFT public fotuNFT;
    WETH public weth;

    constructor(CoinDeposit coinDeposit_,FotuNFT fotuNFT_,WETH weth_){
        coinDeposit=coinDeposit_;
        fotuNFT=fotuNFT_;
        weth=weth_;
    }

    function withdraw(uint deadline,uint oid,IERC20 coin,uint256 amount,address to,bytes memory signature) public {
        if(address(coin)==address(weth)) {
            coinDeposit.withdraw(deadline,oid,coin,amount,address(this),signature);
            weth.withdraw(amount);
            payable(to).transfer(amount);
        }else {
            coinDeposit.withdraw(deadline,oid,coin,amount,to,signature);
        }
    }

    function deposit(address to) public payable {
        uint amount=msg.value;
        weth.deposit{value: amount}();
        TransferLib.transferFrom(weth,address(this),address(coinDeposit),amount);
        coinDeposit.deposit(weth,to);
    }

    function deposit(IERC20 coin,address to,uint amount) public {
        TransferLib.transferFrom(coin,msg.sender,address(coinDeposit),amount);
        coinDeposit.deposit(coin,to);
    }

    function deposit(IERC20 coin,address to,uint amount,uint256 deadline,uint8 v,bytes32 r,bytes32 s) public {
        IERC20Permit(address(coin)).permit(msg.sender,address(this),amount,deadline,v,r,s);
        TransferLib.transferFrom(coin,msg.sender,address(coinDeposit),amount);
        coinDeposit.deposit(coin,to);
    }

    function withdrawNFT(uint256 deadline,uint256 oid,string memory stock_name_,string memory code_,string memory market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_,address to,bytes memory signature) public {
        fotuNFT.withdraw(deadline,oid,stock_name_,code_,market_,qty_,cost_price_,trd_env_,version_,to,signature);
    }

    function depositNFT(uint tokenId,address to,bool sell_now) public {
        fotuNFT.deposit(tokenId,to,sell_now);
    }
}