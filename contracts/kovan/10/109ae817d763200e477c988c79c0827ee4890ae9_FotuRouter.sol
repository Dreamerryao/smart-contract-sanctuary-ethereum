contract FotuNFT is ERC721Enumerable,EIP712,ReentrancyGuard,Ownable,Multicall {
    mapping(uint=>bool) public oidUsed;
    address public signerAddress;
    uint public total;
    string public _baseTokenURI;
    mapping(uint=>string) public stock_name;
    mapping(uint=>string) public code;
    mapping(uint=>string) public market;
    mapping(uint=>uint) public qty;
    mapping(uint=>uint) public cost_price;
    mapping(uint=>uint) public trd_env;
    mapping(uint=>uint) public version;

    using Strings for uint256;

    event Withdraw(address indexed to,uint indexed tokenId,uint indexed oid,string stock_name_,string code_,string market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_);
    event Deposit(address indexed to,uint indexed tokenId,string stock_name_,string code_,string market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_,bool sell_now);

    constructor(address _signerAddress) EIP712("FotuNFT", "1") ERC721("Fortune online","Fotu") {
        signerAddress=_signerAddress;
    }

    function withdraw(uint256 deadline,uint256 oid,string memory stock_name_,string memory code_,string memory market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_,address to,bytes memory signature) public nonReentrant {
        require(block.timestamp <= deadline, "FotuNFT: expired");
        require(check(signerAddress,hashData(deadline,oid,stock_name_,code_,market_,qty_,cost_price_,trd_env_,version_,to),signature), "FotuNFT: sign error");
        require(!oidUsed[oid],"FotuNFT: already submit");
        oidUsed[oid]=true;
        uint tokenId=mintNFT(stock_name_,code_,market_,qty_,cost_price_,trd_env_,version_,to);
        emit Withdraw(to,tokenId,oid,stock_name_,code_,market_,qty_,cost_price_,trd_env_,version_);
    }

    function merge(uint tokenId,address to) public {
        merge(total,tokenId,to);
    }

    function merge(uint tokenIdLeft,uint tokenIdRight,address to) public nonReentrant {
        require(isApprovedOrOwner(msg.sender,tokenIdLeft) && isApprovedOrOwner(msg.sender,tokenIdRight),"FotuNFT: need ownOf NFT");
        require(
            strcmp(stock_name[tokenIdLeft],stock_name[tokenIdRight]) &&
            strcmp(code[tokenIdLeft],code[tokenIdRight]) &&
            strcmp(market[tokenIdLeft],market[tokenIdRight]) &&
            trd_env[tokenIdLeft] == trd_env[tokenIdRight] &&
            version[tokenIdLeft] == version[tokenIdRight]
            ,"FotuNFT: cannot merge");
        uint newQty=qty[tokenIdLeft] + qty[tokenIdRight];
        uint newCostPrice=(qty[tokenIdLeft]*cost_price[tokenIdLeft]+qty[tokenIdRight]*cost_price[tokenIdRight])/newQty;
        uint tokenId=tokenIdRight;
        mintNFT(stock_name[tokenId],code[tokenId],market[tokenId],newQty,newCostPrice,trd_env[tokenId],version[tokenId],to);
        _burnNFT(tokenIdLeft);
        _burnNFT(tokenIdRight);
    }

    function changePrice(uint tokenId,uint price) public nonReentrant {
        require(isApprovedOrOwner(msg.sender,tokenId),"FotuNFT: need ownOf NFT");
        cost_price[tokenId]=price;
    }

    function deposit(address to,bool sell_now) public {
        deposit(total,to,sell_now);
    }

    function deposit(uint tokenId,address to,bool sell_now) public nonReentrant {
        require(isApprovedOrOwner(msg.sender,tokenId),"FotuNFT: need ownOf NFT");
        emit Deposit(to,tokenId,stock_name[tokenId],code[tokenId],market[tokenId],qty[tokenId],cost_price[tokenId],trd_env[tokenId],version[tokenId],sell_now);
        _burnNFT(tokenId);
    }

    function divide(uint divideQty,address to) public {
        divide(total,divideQty,to);
    }

    function divide(uint tokenId,uint divideQty,address to) public nonReentrant {
        require(isApprovedOrOwner(msg.sender,tokenId),"FotuNFT: need ownOf NFT");
        require(divideQty>0 && divideQty<qty[tokenId],"FotuNFT: cannot divide zero NFT");
        mintNFT(stock_name[tokenId],code[tokenId],market[tokenId],qty[tokenId]-divideQty,cost_price[tokenId],trd_env[tokenId],version[tokenId],to);
        mintNFT(stock_name[tokenId],code[tokenId],market[tokenId],divideQty,cost_price[tokenId],trd_env[tokenId],version[tokenId],to);
        _burnNFT(tokenId);
    }

    function _burnNFT(uint tokenId) internal {
        delete stock_name[tokenId];
        delete code[tokenId];
        delete market[tokenId];
        delete qty[tokenId];
        delete cost_price[tokenId];
        delete trd_env[tokenId];
        delete version[tokenId];
        _burn(tokenId);
    }

    function mintNFT(string memory stock_name_,string memory code_,string memory market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_,address to) internal virtual returns (uint tokenId){
        tokenId=_mintNFT(to);
        stock_name[tokenId]=stock_name_;
        code[tokenId]=code_;
        market[tokenId]=market_;
        qty[tokenId]=qty_;
        cost_price[tokenId]=cost_price_;
        trd_env[tokenId]=trd_env_;
        version[tokenId]=version_;
    }

    function _mintNFT(address to) internal virtual returns (uint tokenId){
        total=total+1;
        tokenId=total;
        _safeMint(to, tokenId);
    }

    function adminSignerAddress(address _signerAddress) public onlyOwner {
        signerAddress=_signerAddress;
    }

    function adminBaseURI(string memory baseURI) public onlyOwner {
        _baseTokenURI=baseURI;
    }

    function isApprovedOrOwner(address spender, uint256 tokenId) internal view virtual returns (bool) {
        return _isApprovedOrOwner(spender,tokenId);
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory baseURI = _baseURI();
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, 
            string(abi.encodePacked("?chain_id=",chainId().toString(),"&token_id=",tokenId.toString(),"&stock_name=",stock_name[tokenId],"&code=",code[tokenId],"&market=",market[tokenId])),
            string(abi.encodePacked("&qty=",qty[tokenId].toString(),"&cost_price=",cost_price[tokenId].toString(),"&trd_env=",trd_env[tokenId].toString(),"&version=",version[tokenId].toString()))
        )) : "";
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function hashData(uint256 deadline,uint256 oid,string memory stock_name_,string memory code_,string memory market_,uint256 qty_,uint256 cost_price_,uint256 trd_env_,uint256 version_,address to) public view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
            keccak256("WithdrawNFTRequest(uint256 deadline,uint256 oid,string stock_name,string code,string market,uint256 qty,uint256 cost_price,uint256 trd_env,uint256 version,address to)"),
            deadline,
            oid,
            keccak256(bytes(stock_name_)),
            keccak256(bytes(code_)),
            keccak256(bytes(market_)),
            qty_,
            cost_price_,
            trd_env_,
            version_,
            to
        )));
    }

    function domainSeparator() public view returns (bytes32) {
        return _domainSeparatorV4();
    }
    
    function check(address signer,
        bytes32 hash,
        bytes memory signature) public view returns (bool) {
            return SignatureChecker.isValidSignatureNow(signer,hash,signature);
    }

    function chainId() view public returns(uint) {
        return block.chainid;
    }

    function memcmp(bytes memory a, bytes memory b) public pure returns(bool){
        return (a.length == b.length) && (keccak256(a) == keccak256(b));
    }
    function strcmp(string memory a, string memory b) public pure returns(bool){
        return memcmp(bytes(a), bytes(b));
    }

    struct WithdrawNFTRequest {
        uint256 deadline;
        uint256 oid;
        string stock_name;
        string code;
        string market;
        uint256 qty;
        uint256 cost_price;
        uint256 trd_env;
        uint256 version;
        address to;
    }
}
