pragma solidity ^0.8.0;







contract CoinDeposit is EIP712,ReentrancyGuard,Ownable,Multicall {
    mapping(IERC20=>uint) public coinBalance;
    mapping(uint=>bool) public oidUsed;
    address public signerAddress;

    event Deposit(address indexed to,IERC20 coin,uint amount);
    event Withdraw(address indexed to,uint indexed oid,IERC20 coin,uint amount);

    constructor(address _signerAddress) EIP712("CoinDeposit", "1") {
        signerAddress=_signerAddress;
    }

    function deposit(IERC20 coin,address to) public nonReentrant {
        uint amount=coin.balanceOf(address(this))-coinBalance[coin];
        coinBalance[coin]=coin.balanceOf(address(this));
        require(amount>0,"CoinDeposit: amount must more than 0");
        emit Deposit(to,coin,amount);
    }

    function withdraw(uint deadline,uint oid,IERC20 coin,uint256 amount,address to,bytes memory signature) public nonReentrant {
        require(block.timestamp <= deadline, "CoinDeposit: expired");
        require(check(signerAddress,hashData(deadline,oid,address(coin),amount,to),signature), "CoinDeposit: sign error");
        require(!oidUsed[oid],"CoinDeposit: already submit");
        oidUsed[oid]=true;
        TransferLib.transferFrom(coin,address(this),to,amount);
        coinBalance[coin]=coin.balanceOf(address(this));
        emit Withdraw(to,oid,coin,amount);
    }

    function adminSignerAddress(address _signerAddress) public onlyOwner {
        signerAddress=_signerAddress;
    }

    function hashData(uint deadline,uint oid,address coin,uint256 amount,address to) public view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
            keccak256("WithdrawCoinRequest(uint256 deadline,uint256 oid,address coin,uint256 amount,address to)"),
            deadline,
            oid,
            coin,
            amount,
            to
        )));
    }

    function domainSeparator() public view returns (bytes32) {
        return _domainSeparatorV4();
    }
    
    function check(address signer,
        bytes32 hash,
        bytes memory signature) public view returns (bool) {
            return SignatureChecker.isValidSignatureNow(signer,hash,signature);
    }

    function chainId() view public returns(uint) {
        return block.chainid;
    }

    struct WithdrawCoinRequest {
        uint256 deadline;
        uint256 oid;
        address coin;
        uint256 amount;
        address to;
    }
}
