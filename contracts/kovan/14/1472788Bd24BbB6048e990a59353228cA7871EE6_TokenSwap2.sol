    }

    function asset() external view returns(string memory){
        return IERC20Metadata(address(token)).symbol();
    }

    function swapTokenForUSDT(uint256 _tokenAmount) external override onlyWhiteList returns(uint256){
        require(_tokenAmount > 0, "token amount should be >0");

        uint256 tokenUsdtPrice = priceOracle.getPriceUsdt(swapTokenName);
        require(tokenUsdtPrice > 0, "token price should be >0");

        uint tokenDecimals = IERC20Metadata(address(token)).decimals();
        uint usdtDecimals = IERC20Metadata(address(usdt)).decimals();
        
        uint256 actualTransferAmount = transferIn(token, _msgSender(), _tokenAmount);
        uint256 swapToUsdtAmount = actualTransferAmount.mul(tokenUsdtPrice).mul(10**usdtDecimals).div(10**tokenDecimals).div(10**usdtPriceDecimals);

        require(swapToUsdtAmount > 0, "usdt amount is 0");
        require(usdt.balanceOf(address(this)) >= swapToUsdtAmount, "USDT amount exceed pooled amount");

        usdt.safeTransfer(_msgSender(), swapToUsdtAmount);

        emit TokenSwapped(address(token), address(usdt), actualTransferAmount, swapToUsdtAmount);
        return swapToUsdtAmount;
    }

    function swapUSDTForToken(uint256 _usdtAmount) external override onlyWhiteList returns(uint256){
        require(_usdtAmount > 0, "usdt amount should be >0");
        uint256 tokenUsdtPrice = priceOracle.getPriceUsdt(swapTokenName);
        require(tokenUsdtPrice > 0, "token price should be >0");

        uint tokenDecimals = IERC20Metadata(address(token)).decimals();
        uint usdtDecimals = IERC20Metadata(address(usdt)).decimals();
        
        uint256 actualTransferAmount = transferIn(usdt, _msgSender(), _usdtAmount);
        uint256 swapToTokenAmount = actualTransferAmount.mul(10**tokenDecimals).mul(10**usdtPriceDecimals).div(tokenUsdtPrice).div(10**usdtDecimals);

        require(swapToTokenAmount > 0, "token amount is 0");
        require(token.balanceOf(address(this)) >= swapToTokenAmount, "token amount exceed pooled amount");

        token.safeTransfer(_msgSender(), swapToTokenAmount);

        emit TokenSwapped(address(usdt), address(token), actualTransferAmount, swapToTokenAmount);
        return swapToTokenAmount;
    }


    function withdraw(IERC20 _tokenAddress, address _to, uint256 _amount) public onlyOwner{
        require(_tokenAddress.balanceOf(address(this)) >= _amount, "amount exceed balance");
        _tokenAddress.safeTransfer(_to, _amount);
    }

}