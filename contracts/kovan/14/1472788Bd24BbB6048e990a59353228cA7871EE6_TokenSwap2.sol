contract WhiteList is Ownable{

    mapping(address=>bool) public whiteList;

    modifier onlyWhiteList(){
        require(whiteList[_msgSender()] == true, "not in white list");
        _;
    }

    function addWhiteList(address account) public onlyOwner{
        require(account != address(0), "address should not be 0");
        whiteList[account] = true;
    }

    function removeWhiteList(address account) public onlyOwner{
        whiteList[account] = false;
    }

}
  
library StringUtil {
    
    function equal(string memory a, string memory b) internal pure returns(bool){
        return equal(bytes(a),bytes(b));
    }

    function equal(bytes memory a, bytes memory b) internal pure returns(bool){
        return keccak256(a) == keccak256(b);
    }
    
    function notEmpty(string memory a) internal pure returns(bool){
        return bytes(a).length > 0;
    }

}

contract TransferUtil {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    function transferIn(IERC20 _token, address _from, uint256 _amount) internal returns(uint256){
        require(_token.allowance(_from, address(this)) >= _amount, "amount exceed approval");
        uint balanceBefore = _token.balanceOf(address(this));
        _token.safeTransferFrom(_from, address(this), _amount);
        uint balanceAfter = _token.balanceOf(address(this));
        require(balanceAfter > balanceBefore, "token transfer in overflow");
        
        return balanceAfter.sub(balanceBefore);
    }

    function transferOut(IERC20 _token, address _to, uint256 _amount) internal{
        require(_token.balanceOf(address(this)) >= _amount, "amount exceed balance");
        _token.safeTransfer(_to, _amount);
    }
}

