pragma solidity 0.8.9;

import "./lib/common/EnumerableSet.sol";
import "./lib/common/AccessControl.sol";
import "./lib/common/Managable.sol";
import "./lib/common/ReentrancyGuard.sol";

/**
 * @dev Contract for distribution FEAR Wolf NFT within presale and public sale phases.
 */
contract FearWolfStub is AccessControl, Managable, ReentrancyGuard {
    using EnumerableSet for EnumerableSet.AddressSet;

    uint256 public wolvesAssigned;
    mapping (address => uint256) public wolvesOwnedByAddress;
    EnumerableSet.AddressSet private fearWolvesOwners;

    constructor() {
        
    }

    function assignRandomWolf(address initialOwner) external onlyRole(MANAGER_ROLE) {
        wolvesAssigned++;
        wolvesOwnedByAddress[initialOwner]++;
        fearWolvesOwners.add(initialOwner);
    }

    function resetAll() external onlyRole(MANAGER_ROLE) {
        wolvesAssigned = 0;
        if (fearWolvesOwners.length() > 0) {
            for (uint i = fearWolvesOwners.length() - 1; i >= 0 ; i--)
                wolvesOwnedByAddress[fearWolvesOwners.at(i)] = 0;
        }
    }
}

