pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./interfaces/IRUSDC_LP.sol";
import "./deps/aave/ILendingPool.sol";
import "./deps/aave/IAaveIncentivesController.sol";
import "./deps/uniswap/IUniswapRouterV2.sol";

contract RUSDC_Pool is Ownable {
    using SafeMath for uint256;

    event LiqudityAdded(address indexed owner, uint256 amount);
    event LiqudityRemoved(address indexed owner, uint256 amount);

    event RewardClaimed(uint256 amount);
    event TreasurySent(address indexed owner, uint256 amount);

    // address public constant usdc = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    // address public constant usdc = 0xe11A86849d99F524cAC3E7A0Ec1241828e332C62; // Mumbai TestNet
    address public constant usdc = 0xdCFaB8057d08634279f8201b55d311c2a67897D2; // kovan

    address public constant amUSDC = 0x1a13F4Ca1d028320A707D99520AbFefca3998b7F; // AAVE Matic Market USDC... same address on Polygon, Mumbai
    
    address public constant reward = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; // Wrapped MATIC ... same address on Polygon, Mumbai

    // address public constant LENDING_POOL = 0x8dFf5E27EA6b7AC08EbFdf9eB090F32ee9a30fcf;
    // address public constant LENDING_POOL = 0x9198F13B08E299d85E096929fA9781A1E3d5d827;  // Mumbai TestNet
    address public constant LENDING_POOL    = 0x762E2a3BBe729240ea44D31D5a81EAB44d34ef01; // Kovan 

    // address public constant INCENTIVES_CONTROLLER = 0x357D51124f59836DeD84c8a1730D72B749d8BC23;
    address public constant INCENTIVES_CONTROLLER = 0xd41aE58e803Edf4304334acCE4DC4Ec34a63C644; // Mumbai TestNet

    address public constant QUICKSWAP_ROUTER = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff;
    // address public constant QUICKSWAP_ROUTER2 = 0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45; // SwapRouter02

    // the same indexes are for amUSDC for curve exchanges
    int128 public constant CURVE_USDC_INDEX = 1;

    // Tracks the last block that a caller has written to state.
    // Disallow some access to functions if they occur while a change is being written.
    mapping(address => uint256) private lastWrite;

    // Total USDC deposited to liquidity
    uint256 totalUSDC = 0;
    
    // Total amUSDC deposited to aave pool
    uint256 totalamUSDC = 0;

    // USDC amount per customer
    mapping(address => uint256) private customerUSDC;

    // reference to RUSDC_LP contract
    IRUSDC_LP public RUSDC_LP;

    // fee percent for service
    uint256 public feePercent = 3;

    constructor() {
        // ERC20(usdc).approve(LENDING_POOL, type(uint256).max);
        // ERC20(reward).approve(QUICKSWAP_ROUTER, type(uint256).max);
        // ERC20(usdc).approve(QUICKSWAP_ROUTER, type(uint256).max);
    }

    /**
    * enables an address to mint / burn
    * @param fee the address to enable
    */
    function setFeePercent(uint256 fee) external onlyOwner {
        require(fee > 0, "should be at least 1");
        require(fee < 100, "should be smaller than 100");

        feePercent = fee;
    }
    
    function setContracts(address _RUSDC_LP) external onlyOwner {
        RUSDC_LP = IRUSDC_LP(_RUSDC_LP);
    }
    
    /*
    * A user calls this function to deposit USDC and receive LP token in return.
    */
    function addLiquidity(uint256 _amount) external {
        address to = _msgSender();
        uint256 usdcBalance = ERC20(usdc).balanceOf(to);
        require(_amount > 0, "should be at least 1");
        require(usdcBalance >= _amount, "not enough balance");

        // increase totalUSDC balance of the liquidity
        totalUSDC = totalUSDC.add(_amount);
        
        // increase USDC balance deposited for customer
        customerUSDC[to] = customerUSDC[to].add(_amount);

        // deposit to AAVE Lending Pool and get back amUSDC tokens
        uint256 usdcAmt = _amount;

        // approve
        ERC20(usdc).approve(LENDING_POOL, usdcAmt*1e18);
        
        // deposit to aave pool
        // ILendingPool(LENDING_POOL).deposit(usdc, usdcAmt, address(this), 0);
        ILendingPool(LENDING_POOL).deposit(usdc, usdcAmt*1e18, address(this), 0);
        
        /*
        // get the total balance of amUSDC in aave by this contract
        totalamUSDC = ERC20(amUSDC).balanceOf(address(this));
        
        // calculate lpToken amount to mint
        uint256 lpAmt = usdcAmt;
        // total LP token
        uint256 lpTotal = RUSDC_LP.totalSupply();
        if (lpTotal > 0 && totalamUSDC > 0) {
            lpAmt = customerUSDC[to] / totalamUSDC * lpTotal;
        }
        
        */
        
        // mint LP token to the depositor
        // RUSDC_LP.mint(to, lpAmt);
        RUSDC_LP.mint(to, 5);

        // Emit Events
        emit LiqudityAdded(to, _amount);
    }
    
    /*
    * A user calls this function to redeem their liquidity investment
    */
    function removeLiquidity(uint256 _amount) external {  
        require(_amount > 0, "should be at least 1");
        require(customerUSDC[_msgSender()] >= _amount, "insufficient token balance");

        uint256 usdcAmt = _amount;

        // calculate LP amount to burn
        address to = _msgSender();
        uint256 customerLP = RUSDC_LP.balanceOf(to);
        uint256 amtLPToBurn = usdcAmt.mul(customerLP).div(customerUSDC[to]);

        // decrease totalUSDC balance of the liquidity
        totalUSDC = totalUSDC.sub(usdcAmt);
        // decrease USDC balance deposited for customer
        customerUSDC[to] = customerUSDC[to].sub(usdcAmt);
        
        // withdraw from USDC pool if anything left
        uint256 _withdraw = _withDrawFromAAVEPool(amUSDC, usdc, 10**12, usdcAmt);
        
        // send the claims to customer        
        _sendTreasury(to, _withdraw);

        // burn correspondent LP tokens
        RUSDC_LP.burn(to, amtLPToBurn);

        // Emit events
        emit LiqudityRemoved(to, _amount);
    }

    function _withDrawFromAAVEPool(address _aToken, address _token, uint256 _d, uint256 _amount) internal returns (uint256) {
        uint256 _balance = balanceOfToken(_aToken).mul(_d);
        uint256 _exAmt = 0;
        if (_amount > 0 && _balance > 0) {
            if (_balance >= _amount) {
               _exAmt =  ILendingPool(LENDING_POOL).withdraw(_token, _amount.div(_d), address(this));
                _amount = 0;
            } else {
                _amount = _amount.sub(_balance);
               _exAmt =  ILendingPool(LENDING_POOL).withdraw(_token, _balance.div(_d), address(this));
            }
        }

        return _exAmt;
    }

    /// @dev Balance of a particular token fot this contract
    function balanceOfToken(address _token) public view returns (uint256) {
        return ERC20(_token).balanceOf(address(this));
    }

    /// @dev A readonly function that returns the amount of amUSDC held by contract
    function _getReserves() public returns (uint256) {
        totalamUSDC = ERC20(amUSDC).balanceOf(address(this));
        return totalamUSDC;
    }
   
    /// @dev A function to send Treasury to destination
    function _sendTreasury(address to, uint256 _amount) internal {
        require(_amount > 0, "should be at least 1");
        
        // take fee % and send the rest to the customer
        uint256 _amtWithoutFee = _amount.mul(100-feePercent).div(100);
        ERC20(usdc).transferFrom(address(this), to, _amtWithoutFee);

        // Emit Events
        emit TreasurySent(to, _amtWithoutFee);
    }
    
    /// @dev A function to get the WMATIC rewards from the AAVE pool
    function _claimRewards() public onlyOwner returns (uint256) {
        // Get the WMATIC rewards from the AAVE pool
        address[] memory assets = new address[](1);
        assets[0] = amUSDC;
        IAaveIncentivesController(INCENTIVES_CONTROLLER).claimRewards(assets, type(uint256).max, address(this));

        uint256 _usdc = 0;
        uint256 rewardsAmount = balanceOfToken(reward);
        if (rewardsAmount > 0) {
            // Swap WMATIC-USDC
            address[] memory path = new address[](2);
            path[0] = reward;
            path[1] = usdc;
            
            // proceed swaping
            uint256[] memory amts = IUniswapRouterV2(QUICKSWAP_ROUTER).swapExactTokensForTokens(rewardsAmount, 1, path, address(this), block.timestamp);
            _usdc = amts[1];
        }

        // Emit Events
        emit RewardClaimed(_usdc);

        return _usdc;
    }

    // move liquidity to another contract
    function upgrade(address _new) external onlyOwner {
        uint256 _balance = balanceOfToken(amUSDC);
        uint256 usdcWithdraw = _withDrawFromAAVEPool(amUSDC, usdc, 10**12, _balance);
        usdcWithdraw = usdcWithdraw.add(_claimRewards());

        // move all usdc to the new contract
        ERC20(usdc).transferFrom(address(this), _new, usdcWithdraw);

        // has to remove all LP tokens
    }
}

