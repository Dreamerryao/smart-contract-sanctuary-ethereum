pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./InfluencerTokenICO.sol";
import "./utils/Ownable.sol";

contract InfluencerToken is ERC20, Ownable {
    address[] private _shareholders;
    mapping(address => uint256) private _balances;

    event ICOCreated(address ico);

    constructor(address owner_, string memory name_, string memory symbol_, uint256 totalSupply_) ERC20(name_, symbol_) Ownable(owner_) {
        _mint(owner_, totalSupply_);
        _shareholders.push(owner_);
    }

    function payout() public payable {
        for (uint i = 0; i < _shareholders.length; i++) {
            address shareholder = _shareholders[i];
            uint balance = balanceOf(shareholder);
            uint percentage = balance / totalSupply();

            payable(shareholder).transfer(msg.value * percentage);
        }

        return;
    }

    function createICO(uint256 duration) public onlyOwner {
        uint256 ownerBalance = balanceOf(msg.sender);
        uint256 totalSupply = totalSupply();

        uint256 amount = totalSupply * 10 / 100;

        require(ownerBalance == totalSupply, "You do not own 100%");
        InfluencerTokenICO ico = new InfluencerTokenICO(address(this), duration, owner);

        transfer(address(ico), amount);

        emit ICOCreated(address(ico));
    }

}

