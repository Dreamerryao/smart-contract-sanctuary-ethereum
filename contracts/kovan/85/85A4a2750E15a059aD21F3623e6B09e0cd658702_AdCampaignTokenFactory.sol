pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./utils/Ownable.sol";
import "hardhat/console.sol";

contract InfluencerTokenICO is Ownable {

    using SafeMath for uint256;

    IERC20 public token;

    bool private _initialized = false;
    uint256 private _initialTokens = 0;
    uint256 private _raisedAmount = 0;
    uint256 private _duration;

    uint256 private _startedAt;

    modifier whenICOActive() {
        assert(isActive());
        _;
    }

    constructor(
        address tokenAddr_,
        uint256 duration_,
        address owner_
    ) Ownable(owner_) {
        token = IERC20(tokenAddr_);
        _duration = duration_;
    }

    /**
     * Initialize the ICO.
     */
    function initialize() public onlyOwner {
        require(_initialized == false, "Already initialized");

        uint256 initialTokens = tokensAvailable();
        require(initialTokens > 0, "Not enough funds");

        _startedAt = block.timestamp;
        _initialTokens = initialTokens;
    }

    /**
     * Fallback function when ETH is sent to this contract.
     */
    receive() external payable {
        buyTokens();
    }

    /**
     * Returns if the campaign is active or not.
     */
    function isActive() public view returns (bool) {
        return (
            _initialized == true &&
            block.timestamp <= _startedAt.add(_duration * 1 days) &&
            goalReached() == false
        );
    }

    /**
     * Returns if the goal is reached or not.
     */
    function goalReached() public view returns (bool) {
        return tokensAvailable() == 0;
    }

    /**
     * Buy tokens.
     * The amount of tokens bought is base on the getQuote function.
     */
    function buyTokens() public payable {
        uint256 tokens = getQuote(msg.value);

        _raisedAmount = _raisedAmount.add(msg.value);

        token.transfer(msg.sender, tokens);

        if (goalReached() == true) {
            terminate();
        }
    }

    /**
     * Returns a quote for the price of a token.
     */
    function getQuote(uint256 amount) public pure returns (uint256) {
        return amount.mul(getRate());
    }

    /**
     * Returns the current rate for calculating the price of a token.
     */
    function getRate() public pure returns (uint) {
        return 1;
    }

    /**
     * Returns the number of tokens available in this ICO.
     */
    function tokensAvailable() public view returns (uint256) {
        return token.balanceOf(address(this));
    }

    /**
     * Returns the initial supply of tokens of this ICO.
     */
    function initialSupply() public view returns (uint256) {
        return _initialTokens;
    }

    /**
     * Gracefully finish the ICO.
     * Send the token to the funders and the funds to the Influencer.
     */
    function terminate() internal {
        require(goalReached(), "Goal not reached !");
        uint256 balance = tokensAvailable();
        assert(balance == 0);
        selfdestruct(payable(owner));
    }

    /**
     * Destroy the ICO.
     * Refunds the funds to the funders.
     * Refund the tokens to the influencer.
     */
    function destroy() onlyOwner internal {
        uint256 balance = tokensAvailable();
        assert(balance > 0);
        token.transfer(owner, balance);
        selfdestruct(payable(owner));
    }

}

