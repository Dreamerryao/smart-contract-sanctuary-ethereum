pragma solidity ^0.8.0;

import "./AdCampaignToken.sol";

contract AdCampaignTokenFactory {

    AdCampaignToken[] private tokens;

    /**
     * Triggered when a new AdCampaignToken is created.
     */
    event AdCampaignTokenCreated(address token);

    /**
     * Creates a new AdCampaignToken.
     */
    function createAdCampaignToken(uint256 payoutRatio) public returns (address) {
        AdCampaignToken token = new AdCampaignToken(msg.sender, payoutRatio);
        emit AdCampaignTokenCreated(address(token));
    }

    /**
     * Returns the AdCampaignTokens created.
     */
    function allAdCampaignTokens() public view returns (AdCampaignToken[] memory) {
        return tokens;
    }

}

