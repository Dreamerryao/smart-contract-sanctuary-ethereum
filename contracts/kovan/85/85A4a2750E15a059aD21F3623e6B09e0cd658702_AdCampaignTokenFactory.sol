pragma solidity ^0.8.0;

import "./SocialPost.sol";
import "./InfluencerToken.sol";
import "./utils/Ownable.sol";

contract AdCampaignToken is Ownable {

    /** Payout ratio (ex: 4 DAI / 1 like) */
    uint private payoutRatio;

    /** Is the campaign running */
    bool private isRunning = false;

    /** Mapping of addresses which deposited funds and the amount */
    mapping(address => uint) public deposits;

    /** List of published SocialPosts */
    SocialPost[] private posts;

    modifier onlyIfRunning {
        require(isRunning == true, "Campaign must be running");
        _;
    }

    modifier onlyIfNotRunning {
        require(isRunning == false, "Campaign must not be running");
        _;
    }

    constructor(
        address _owner,
        uint _payoutRatio
    ) Ownable(_owner) {
        payoutRatio = _payoutRatio;
    }

    /**
     * Deposit funds into the AdCampaign
     */
    function deposit() public payable {
        deposits[msg.sender] += msg.value;
    }

    // Get balance of contract
    function getBalanceOfCampaign() public view returns(uint) {
        return address(this).balance;
    }

    /**
     * Get a quote for a specific number of likes.
     */
    function getQuote(uint likes) public view returns(uint) {
        return likes * payoutRatio;
    }

    /**
     * Initialize the AdCampaign.
     */
    function initialize() public onlyOwner onlyIfNotRunning {
        require(address(this).balance > 0, "The campaign needs fund");
        isRunning = true;
    }

    /**
     * Creates a new Post to this AdCampaign.
     */
    function createPost(address influencerToken, string memory postId) public onlyOwner onlyIfRunning {
        SocialPost post = new SocialPost(influencerToken, postId);
        posts.push(post);
    }

    /**
     * Returns the posts.
     */
    function allPosts() public view returns (SocialPost[] memory) {
        return posts;
    }

    /**
     * Triggers the Payout.
     * Will send funds to the InfluencerToken depending of the likes of the posts.
     */
    function payout() public onlyOwner {
        for (uint i = 0; i < posts.length; i++) {
            SocialPost post = posts[i];
            InfluencerToken token = post.influencerToken();
            uint amount = getQuote(post.likes());
            token.payout{value: amount}();
        }
    }

}

