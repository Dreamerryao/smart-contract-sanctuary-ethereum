pragma solidity 0.7.6;
pragma abicoder v2;

import "@openzeppelin/contracts/proxy/TransparentUpgradeableProxy.sol";
import "@openzeppelin/contracts-upgradeable/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/EnumerableSetUpgradeable.sol";

import '@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol';
import '@uniswap/v3-core/contracts/libraries/TickMath.sol';
import '@uniswap/v3-periphery/contracts/interfaces/INonfungiblePositionManager.sol';
import "@uniswap/v3-periphery/contracts/libraries/LiquidityAmounts.sol";

import "./CToken.sol";

interface IUSDT {
    function approve(address spender, uint256 amount) external;
}

/**
 * @title Compound's CUniswapV3Pool Contract
 * @notice CTokens which wrap a UniswapV3Pool underlying
 * @author Compound
 */
contract CUniswapV3Pool is CToken, CErc721Interface {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using SafeMathUpgradeable for uint256;

    struct Deposit {
        address depositor;
        uint256 liquidity;
        uint128 nfpLiquidity;
    }

    INonfungiblePositionManager public nfpManager;
    address public token0;
    address public token1;
    uint24 public fee;
    uint160 public sqrtRatioAX96;
    uint160 public sqrtRatioBX96;

    // The mapping of NFP IDs to the below data
    mapping(uint256 => Deposit) public deposits;
    // Total number of NFP liquidities deposited
    uint256 internal _totalLiquidity;
    // The mapping from depositor address to NFP IDs which they deposited
    mapping (address => EnumerableSetUpgradeable.UintSet) internal _accountTokens;

    event CollectUniswapFee(uint256 tokenId, uint256 amount0, uint256 amount1, address to);
    event DecreaseLiquidity(uint256 tokenId, uint256 liquidityDel, uint128 nfpLiquidityDel, uint256 amount0, uint256 amount1, address to);
    event IncreaseLiquidity(uint256 tokenId, uint256 liquidityAdd, uint128 nfpLiquidityAdd, uint256 amount0, uint256 amount1, address from);

    modifier onlyDepositor(uint256 tokenId) {
        require(deposits[tokenId].depositor == msg.sender, "Caller is not the depositor");
        _;
    }

    /**
     * @notice Initialize the new money market
     * @param underlying_ The address of the underlying asset
     * @param comptroller_ The address of the Comptroller
     * @param interestRateModel_ The address of the interest rate model
     * @param initialExchangeRateMantissa_ The initial exchange rate, scaled by 1e18
     * @param name_ ERC-20 name of this token
     * @param symbol_ ERC-20 symbol of this token
     * @param decimals_ ERC-20 decimal precision of this token
     * @param nfpManager_ address of Uniswap V3 NonfungiblePositionManager
     */
    function initializeEx(
        address underlying_,
        ComptrollerInterface comptroller_,
        InterestRateModel interestRateModel_,
        uint256 initialExchangeRateMantissa_,
        string memory name_,
        string memory symbol_,
        uint8 decimals_,
        address nfpManager_,
        address payable admin_
    ) public {
        // Creator of the contract is admin during initialization
        admin = msg.sender;
        // CToken initialize does the bulk of the work
        super.initialize(comptroller_, interestRateModel_, initialExchangeRateMantissa_, name_, symbol_, decimals_);
        // Set the proper admin now that initialization is done
        admin = admin_;

        // Set underlying and sanity check it
        underlying = underlying_;
        token0 = IUniswapV3Pool(underlying).token0();
        token1 = IUniswapV3Pool(underlying).token1();
        fee = IUniswapV3Pool(underlying).fee();
        int24 tickSpacing = IUniswapV3Pool(underlying).tickSpacing();
        int24 tickLower = (TickMath.MIN_TICK / tickSpacing) * tickSpacing;
        int24 tickUpper = (TickMath.MAX_TICK / tickSpacing) * tickSpacing;
        sqrtRatioAX96 = TickMath.getSqrtRatioAtTick(tickLower);
        sqrtRatioBX96 = TickMath.getSqrtRatioAtTick(tickUpper);

        // Set nfpManager
        nfpManager = INonfungiblePositionManager(nfpManager_);

        // We won't use SafeERC20Upgradeable because USDT's approve doesn't return any type of value
        IUSDT(token0).approve(address(nfpManager), type(uint256).max);
        IUSDT(token1).approve(address(nfpManager), type(uint256).max);
    }

    /*** User Interface ***/

    /**
     * @dev Returns a NFP ID deposited by `account` at a given `index` of its tokens list.
     * Use along with {tokensOf} to enumerate all of ``account``'s posiition.
     */
    function tokenOfAccountByIndex(address account, uint256 index) public override view returns (uint256) {
        return _accountTokens[account].at(index);
    }

    /**
     * @dev Returns the amount of tokens deposited by `account`.
     */
    function tokensOf(address account) public override view returns (uint256) {
        require(account != address(0), "token query for the zero address");
        return _accountTokens[account].length();
    }

    /**
     * @notice Sender supplies assets into the market and receives cTokens in exchange
     * @dev Accrues interest whether or not the operation succeeds, unless reverted
     * @param tokenId The id of Uniswap V3 NFP
     * @return uint 0=success, otherwise a failure (see ErrorReporter.sol for details)
     */
    function mint(uint256 tokenId) external override returns (uint256) {
        (,, address _token0, address _token1, uint24 _fee, int24 _tickLower, int24 _tickUpper, uint128 _liquidity,,,,) =  nfpManager.positions(tokenId);
        require(token0 == _token0 && token1 == _token1 && fee == _fee, "Mismatch Uniswap V3 pool");

        (uint160 sqrtPriceX96,,,,,,) = IUniswapV3Pool(underlying).slot0();
        (uint256 amount0, uint256 amount1) = LiquidityAmounts.getAmountsForLiquidity(
            sqrtPriceX96,
            TickMath.getSqrtRatioAtTick(_tickLower),
            TickMath.getSqrtRatioAtTick(_tickUpper),
            _liquidity
        );
        // mintAmount is the liquidity spread on the pool
        uint256 mintAmount = LiquidityAmounts.getLiquidityForAmount1(sqrtRatioAX96, sqrtPriceX96, amount1)
                            + LiquidityAmounts.getLiquidityForAmount0(sqrtPriceX96, sqrtRatioBX96, amount0);

        (uint256 err, ) = _mint(tokenId, mintAmount);
        if (err == uint256(Error.NO_ERROR)) {
            deposits[tokenId].nfpLiquidity = _liquidity;
            deposits[tokenId].liquidity = mintAmount;
            _totalLiquidity = _totalLiquidity.add(mintAmount);

        }
        return err;
    }

    /// @notice Upon receiving a Uniswap V3 ERC721, creates the token deposit setting owner to `from`.
    function onERC721Received(
        address,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external override returns (bytes4) {
        data;
        require(msg.sender == address(nfpManager), 'UniswapV3Staker::onERC721Received: not a univ3 nft');

        deposits[tokenId] = Deposit({depositor: from, liquidity: 0, nfpLiquidity: 0});
        _accountTokens[from].add(tokenId);

        return this.onERC721Received.selector;
    }

    /**
     * @notice Sender redeems cTokens in exchange for a specified amount of underlying asset
     * @dev Accrues interest whether or not the operation succeeds, unless reverted
     * @param tokenId The id of Uniswap V3 NFP
     * @return uint 0=success, otherwise a failure (see ErrorReporter.sol for details)
     */
    function redeemUnderlying(uint256 tokenId) external override onlyDepositor(tokenId) returns (uint256) {
        uint256 redeemAmountIn = deposits[tokenId].liquidity;
        uint256 err = redeemFresh(msg.sender, tokenId, redeemAmountIn, false);
        if (err == uint256(Error.NO_ERROR)) {
            _accountTokens[msg.sender].remove(tokenId);
            delete deposits[tokenId];
            _totalLiquidity = _totalLiquidity.sub(deposits[tokenId].liquidity);
        }
        return err;
    }

    /**
     * @notice Accrues interest and reduces reserves by transferring to admin
     * @return uint 0=success, otherwise a failure (see ErrorReporter.sol for details)
     */
    function _reduceReserves(uint256) external override returns (uint256) {
        require(false, "Not supported");
        return 0;
    }

    /*** Safe Token ***/

    /**
     * @notice Gets balance of this contract in terms of the underlying
     * @dev This excludes the value of the current message, if any
     * @return The quantity of underlying tokens owned by this contract
     */
    function getCashPrior() internal override view returns (uint256) {
        return _totalLiquidity;
    }

    function doTransferIn(address from, uint256 tokenId, bool isNative) internal override returns (uint256) {
        from;
        tokenId;
        isNative;
        return 0;
    }

    function doTransferOut(address payable to, uint256 tokenId, bool isNative) internal override {
        to;
        tokenId;
        isNative;
    }

    /**
     * @notice Transfer `tokens` tokens from `src` to `dst` by `spender`
     * @dev Called by both `transfer` and `transferFrom` internally
     * @return Whether or not the transfer succeeded
     */
    function transferTokens(address, address, address, uint256) internal override returns (uint256) {
        require(false, "Not supported");
        return 0;
    }

    /**
     * @notice Get the account's cToken balances
     * @param account The address of the account
     */
    function getCTokenBalanceInternal(address account) internal override view returns (uint256) {
        return accountTokens[account];
    }

    struct MintLocalVars {
        uint256 exchangeRateMantissa;
        uint256 mintTokens;
        uint256 actualMintAmount;
    }

    function mintFresh(
        address minter,
        uint256 mintAmount,
        bool isNative
    ) internal override returns (uint256, uint256) {
        minter;
        mintAmount;
        isNative;
        return (uint256(Error.NO_ERROR), 0);
    }

    /**
     * @notice User supplies assets into the market and receives cTokens in exchange
     * @dev Assumes interest has already been accrued up to the current block
     * @param tokenId The id of Uniswap V3 NFP
     * @param mintAmount The amount of the underlying asset to supply
     * @return (uint, uint) An error code (0=success, otherwise a failure, see ErrorReporter.sol), and the actual mint amount.
     */
    function _mint(
        uint256 tokenId,
        uint256 mintAmount
    ) internal nonMultiActivity(msg.sender) returns (uint256, uint256) {
        address minter = msg.sender;
        /* Fail if mint not allowed */
        uint256 allowed = comptroller.mintAllowed(address(this), minter, mintAmount);
        if (allowed != 0) {
            return (failOpaque(Error.COMPTROLLER_REJECTION, FailureInfo.MINT_COMPTROLLER_REJECTION, allowed), 0);
        }

        MintLocalVars memory vars;

        vars.exchangeRateMantissa = exchangeRateStoredInternal();

        /////////////////////////
        // EFFECTS & INTERACTIONS
        // (No safe failures beyond this point)

        nfpManager.safeTransferFrom(minter, address(this), tokenId);

        vars.actualMintAmount = mintAmount;

        /*
         * We get the current exchange rate and calculate the number of cTokens to be minted:
         *  mintTokens = actualMintAmount / exchangeRate
         */
        vars.mintTokens = div_ScalarByExpTruncate(vars.actualMintAmount, Exp({mantissa: vars.exchangeRateMantissa}));

        /*
         * We calculate the new total supply of cTokens and minter token balance, checking for overflow:
         *  totalSupply = totalSupply + mintTokens
         *  accountTokens[minter] = accountTokens[minter] + mintTokens
         */
        totalSupply = add_(totalSupply, vars.mintTokens);
        accountTokens[minter] = add_(accountTokens[minter], vars.mintTokens);

        /* We emit a Mint event, and a Transfer event */
        emit MintByNft(minter, tokenId, vars.actualMintAmount, vars.mintTokens);
        emit Transfer(address(this), minter, vars.mintTokens);

        /* We call the defense hook */
        // unused function
        // comptroller.mintVerify(address(this), minter, vars.actualMintAmount, vars.mintTokens);

        return (uint256(Error.NO_ERROR), vars.actualMintAmount);
    }

    struct RedeemLocalVars {
        uint256 exchangeRateMantissa;
        uint256 redeemTokens;
        uint256 redeemAmount;
        uint256 totalSupplyNew;
        uint256 accountTokensNew;
    }

    /**
     * @notice User redeems cTokens in exchange for the underlying asset
     * @dev Assumes interest has already been accrued up to the current block. Only one of redeemTokensIn or redeemAmountIn may be non-zero and it would do nothing if both are zero.
     * @param redeemer The address of the account which is redeeming the tokens
     * @param redeemTokenId The id of Uniswap V3 NFP
     * @param redeemAmountIn The number of underlying tokens to receive from redeeming cTokens
     * @param isNative The amount is in native or not
     * @return uint 0=success, otherwise a failure (see ErrorReporter.sol for details)
     */
    function redeemFresh(
        address payable redeemer,
        uint256 redeemTokenId,
        uint256 redeemAmountIn,
        bool isNative
    ) internal override nonMultiActivity(redeemer) returns (uint256) {
        RedeemLocalVars memory vars;

        /* exchangeRate = invoke Exchange Rate Stored() */
        vars.exchangeRateMantissa = exchangeRateStoredInternal();

        /*
            * We get the current exchange rate and calculate the amount to be redeemed:
            *  redeemTokens = redeemAmountIn / exchangeRate
            *  redeemAmount = redeemAmountIn
            */
        vars.redeemTokens = div_ScalarByExpTruncate(redeemAmountIn, Exp({mantissa: vars.exchangeRateMantissa}));
        vars.redeemAmount = redeemAmountIn;

        /* Fail if redeem not allowed */
        uint256 allowed = comptroller.redeemAllowed(address(this), redeemer, vars.redeemTokens);
        if (allowed != 0) {
            return failOpaque(Error.COMPTROLLER_REJECTION, FailureInfo.REDEEM_COMPTROLLER_REJECTION, allowed);
        }

        /*
         * We calculate the new total supply and redeemer balance, checking for underflow:
         *  totalSupplyNew = totalSupply - redeemTokens
         *  accountTokensNew = accountTokens[redeemer] - redeemTokens
         */
        vars.totalSupplyNew = sub_(totalSupply, vars.redeemTokens);
        vars.accountTokensNew = sub_(accountTokens[redeemer], vars.redeemTokens);

        /* Fail gracefully if protocol has insufficient cash */
        if (getCashPrior() < vars.redeemAmount) {
            return fail(Error.TOKEN_INSUFFICIENT_CASH, FailureInfo.REDEEM_TRANSFER_OUT_NOT_POSSIBLE);
        }

        /////////////////////////
        // EFFECTS & INTERACTIONS
        // (No safe failures beyond this point)

        /* We write previously calculated values into storage */
        totalSupply = vars.totalSupplyNew;
        accountTokens[redeemer] = vars.accountTokensNew;

        nfpManager.safeTransferFrom(address(this), redeemer, redeemTokenId);

        /* We emit a Transfer event, and a Redeem event */
        emit Transfer(redeemer, address(this), vars.redeemTokens);
        emit RedeemByNft(redeemer, redeemTokenId, vars.redeemAmount, vars.redeemTokens);

        /* We call the defense hook */
        comptroller.redeemVerify(address(this), redeemer, vars.redeemAmount, vars.redeemTokens);

        return uint256(Error.NO_ERROR);
    }

    /**
     * @notice Transfers collateral tokens (this market) to the liquidator.
     * @dev Called only during an in-kind liquidation, or by liquidateBorrow during the liquidation of another CToken.
     *  Its absolutely critical to use msg.sender as the seizer cToken and not a parameter.
     * @return uint 0=success, otherwise a failure (see ErrorReporter.sol for details)
     */
    function seizeInternal(address, address, address, uint256) internal override returns (uint256) {
        require(false, "Not supported");
        return 0;
    }

    function collectUniswapFee(
        uint256 tokenId
    ) external onlyDepositor(tokenId) returns (uint256 amount0, uint256 amount1) {
        (amount0, amount1) = _collectUniswapFee(tokenId, msg.sender, type(uint128).max, type(uint128).max);
        emit CollectUniswapFee(tokenId, amount0, amount1, msg.sender);
        return (amount0, amount1);
    }

    function _collectUniswapFee(
        uint256 tokenId,
        address recipient,
        uint128 amount0Max,
        uint128 amount1Max
    ) internal returns (uint256 amount0, uint256 amount1) {
        INonfungiblePositionManager.CollectParams memory params = INonfungiblePositionManager.CollectParams({
            tokenId: tokenId,
            recipient: recipient,
            amount0Max: amount0Max,
            amount1Max: amount1Max
        });

        (amount0, amount1) = nfpManager.collect(params);
        return (amount0, amount1);
    }

    function decreaseLiquidity(
        uint256 tokenId,
        uint128 nfpLiquidityDel
    ) external onlyDepositor(tokenId) returns (uint256 amount0, uint256 amount1) {
        require(nfpLiquidityDel <= deposits[tokenId].nfpLiquidity, "The input value is too large");

        uint256 redeemAmount = deposits[tokenId].liquidity.mul(nfpLiquidityDel).div(deposits[tokenId].nfpLiquidity);

        (amount0, amount1) = _decreaseLiquidity(tokenId, nfpLiquidityDel, redeemAmount);

        deposits[tokenId].nfpLiquidity -= nfpLiquidityDel;
        deposits[tokenId].liquidity = deposits[tokenId].liquidity.sub(redeemAmount);
        _totalLiquidity = _totalLiquidity.sub(redeemAmount); // NOTE: _totalLiquidity should be changed after calling _decreaseLiquidity because it affects to the exchangeRate

        return (amount0, amount1);
    }

    function _decreaseLiquidity(
        uint256 tokenId,
        uint128 nfpLiquidityDel,
        uint256 redeemAmount
    ) internal nonMultiActivity(msg.sender) returns (uint256 amount0, uint256 amount1) {
        address redeemer = msg.sender;
        RedeemLocalVars memory vars;
        vars.exchangeRateMantissa = exchangeRateStoredInternal();
        vars.redeemTokens = div_ScalarByExpTruncate(redeemAmount, Exp({mantissa: vars.exchangeRateMantissa}));

        /* Fail if redeem not allowed */
        uint256 allowed = comptroller.redeemAllowed(address(this), redeemer, vars.redeemTokens);
        require (allowed == uint256(Error.NO_ERROR), "Not allowed to decrease liquidity");

        totalSupply = sub_(totalSupply, vars.redeemTokens);
        accountTokens[redeemer] = sub_(accountTokens[redeemer], vars.redeemTokens);

        // decrease liquidity
        INonfungiblePositionManager.DecreaseLiquidityParams memory params = INonfungiblePositionManager.DecreaseLiquidityParams({
            tokenId: tokenId,
            liquidity: nfpLiquidityDel,
            amount0Min: 0,
            amount1Min: 0,
            deadline: block.timestamp
        });
        (amount0, amount1) = nfpManager.decreaseLiquidity(params);

        _collectUniswapFee(tokenId, redeemer, uint128(amount0), uint128(amount1));

        /* We call the defense hook */
        comptroller.redeemVerify(address(this), redeemer, redeemAmount, vars.redeemTokens);

        emit Transfer(redeemer, address(this), vars.redeemTokens);
        emit DecreaseLiquidity(tokenId, redeemAmount, nfpLiquidityDel, amount0, amount1, redeemer);

        return (amount0, amount1);
    }

    function increaseLiquidity(
        uint256 tokenId,
        uint256 amountAdd0,
        uint256 amountAdd1
    ) external onlyDepositor(tokenId) returns (uint128 nfpLiquidityAdded, uint256 amount0, uint256 amount1) {
        (uint160 sqrtPriceX96,,,,,,) = IUniswapV3Pool(underlying).slot0();
        uint256 mintAmount = LiquidityAmounts.getLiquidityForAmount1(sqrtRatioAX96, sqrtPriceX96, amountAdd1)
                            + LiquidityAmounts.getLiquidityForAmount0(sqrtPriceX96, sqrtRatioBX96, amountAdd0);

        if (0 < amountAdd0) IERC20Upgradeable(token0).safeTransferFrom(msg.sender, address(this), amountAdd0);
        if (0 < amountAdd1) IERC20Upgradeable(token1).safeTransferFrom(msg.sender, address(this), amountAdd1);

        (nfpLiquidityAdded, amount0, amount1) = _increaseLiquidity(tokenId, amountAdd0, amountAdd1, mintAmount, sqrtPriceX96);

        deposits[tokenId].nfpLiquidity += nfpLiquidityAdded;
        deposits[tokenId].liquidity = deposits[tokenId].liquidity.add(mintAmount);
        _totalLiquidity = _totalLiquidity.add(mintAmount);

        return (nfpLiquidityAdded, amount0, amount1);
    }

    function _increaseLiquidity(
        uint256 tokenId,
        uint256 amountAdd0,
        uint256 amountAdd1,
        uint256 mintAmount,
        uint160 sqrtPriceX96
    ) internal nonMultiActivity(msg.sender) returns (uint128 nfpLiquidityAdded, uint256 amount0, uint256 amount1) {
        address minter = msg.sender;
        uint256 allowed = comptroller.mintAllowed(address(this), minter, mintAmount);
        require (allowed == uint256(Error.NO_ERROR), "Not allowed to increase liquidity");

        // increase liquidity
        INonfungiblePositionManager.IncreaseLiquidityParams memory params = INonfungiblePositionManager.IncreaseLiquidityParams({
            tokenId: tokenId,
            amount0Desired: amountAdd0,
            amount1Desired: amountAdd1,
            amount0Min: 0,
            amount1Min: 0,
            deadline: block.timestamp
        });
        (nfpLiquidityAdded, amount0, amount1) = nfpManager.increaseLiquidity(params);

        if (amountAdd0 != amount0 || amountAdd1 != amount1) {
            mintAmount = LiquidityAmounts.getLiquidityForAmount1(sqrtRatioAX96, sqrtPriceX96, amount1)
                        + LiquidityAmounts.getLiquidityForAmount0(sqrtPriceX96, sqrtRatioBX96, amount0);
        }

        MintLocalVars memory vars;
        vars.exchangeRateMantissa = exchangeRateStoredInternal();
        vars.mintTokens = div_ScalarByExpTruncate(mintAmount, Exp({mantissa: vars.exchangeRateMantissa}));

        totalSupply = add_(totalSupply, vars.mintTokens);
        accountTokens[minter] = add_(accountTokens[minter], vars.mintTokens);

        emit Transfer(address(this), minter, vars.mintTokens);
        emit IncreaseLiquidity(tokenId, mintAmount, nfpLiquidityAdded, amount0, amount1, minter);

        return (nfpLiquidityAdded, amount0, amount1);
    }

    uint256[41] private __gap;
}

contract CUniswapV3PoolProxy is TransparentUpgradeableProxy {
    constructor(address logic, address admin, bytes memory data) TransparentUpgradeableProxy(logic, admin, data) public {
    }
}

