import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "./CityClashInterface.sol";
import "./BlahNFTTypes.sol";

contract CityClashRaffle is KeeperCompatibleInterface, Ownable {
    uint public lastPaymentTime;
    event PayedWinnerEvent(address winnderAddress, uint256 amount);

    address cityClashContractAddress = address(0x72D812923660d3d06B057D39169ECD148b554873);
    bool public isPaused = false;
    uint8 private MAX_NUM_ENTRIES = 14;

    uint16 private _randomIndex = 0;
    uint private _randomCalls = 0;
    mapping(uint16 => address) private _randomSource;

    constructor() {
        // lastPaymentTime = _lastPaymentTime;
        // isPaused = true;

        // Fill random source addresses
        _randomSource[0] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        _randomSource[1] = 0x3cD751E6b0078Be393132286c442345e5DC49699;
        _randomSource[2] = 0xb5d85CBf7cB3EE0D56b3bB207D5Fc4B82f43F511;
        _randomSource[3] = 0xC098B2a3Aa256D2140208C3de6543aAEf5cd3A94;
        _randomSource[4] = 0x28C6c06298d514Db089934071355E5743bf21d60;
        _randomSource[5] = 0x2FAF487A4414Fe77e2327F0bf4AE2a264a776AD2;
        _randomSource[6] = 0x267be1C1D684F78cb4F6a176C4911b741E4Ffdc0;
    }

    function isUpkeepNeeded() internal view returns (bool) {
        return (block.timestamp - lastPaymentTime) > (1 days);//1 day for testing  //(7 days);       //7 days have passed
    }

    function checkUpkeep(bytes calldata /*_checkData*/) external view override returns (bool, bytes memory) {
        require(!isPaused, "The Raffle is currently paused.");

        if(isUpkeepNeeded()) {
            CityClashInterface cityClash = CityClashInterface(cityClashContractAddress);
            address[] memory entries = new address[](cityClash.tokensMinted() * MAX_NUM_ENTRIES);
            for(uint i = 0; i < cityClash.getTokenIds(); i++) {
                uint id = i + 1;
                entries = addEntries(i * MAX_NUM_ENTRIES, id, cityClash, entries);
            }

            for(uint i = 0; i < cityClash.getReservedTokenIds(); i++) {
                uint id = i + 1 + cityClash.MAX_CITIES() - cityClash.FOUNDERS_RESERVE_AMOUNT();
                entries = addEntries((i + cityClash.getTokenIds()) * MAX_NUM_ENTRIES, id, cityClash, entries);
            }

            for(uint k = 0; k < entries.length; k++) {
                console.log('entry', k, entries[k]);
            }
            //pick entry randomly
            bytes memory performData = abi.encode(pickRandomWinner(entries, cityClash));
            return (true, performData);
        }
        return (false, bytes(""));
    }

    function addEntries(uint i, uint id, CityClashInterface cityClash, address[] memory entries) internal view returns (address[] memory) {
        BlahNFTTypes.City memory city = cityClash.idToCitiesFunc(id);
        uint8 winningFaction = cityClash.getWinningFaction(cityClash.redScore(), cityClash.greenScore(), cityClash.blueScore());

        if(winningFaction == city.cityFaction) {
            //add entry for each poimt for the city / 3
            uint entryNum = city.points / 3 + 1;
            console.log('entryNum', entryNum);
            for(uint k = 0; k < entryNum; k++) {
                entries[k + i] = cityClash.ownerOf(id);
                console.log('entry value', entries[k + i]);
            }
            //city was transferred in the last 7 days
            if(city.lastTransferTime > 0 && (city.lastTransferTime - lastPaymentTime) > (7 days)) {
                //give one more entry
                entries[MAX_NUM_ENTRIES - 1 + i] = cityClash.ownerOf(id);
                entries[MAX_NUM_ENTRIES - 2 + i] = cityClash.ownerOf(id);
                entries[MAX_NUM_ENTRIES - 3 + i] = cityClash.ownerOf(id);
            }
            console.log('got entry');
        }
        return entries;
    }

    function pickRandomWinner(address[] memory entries, CityClashInterface cityClash) internal view returns (address) {
        address winner = address(0);
        uint count = 0;
        while(winner == address(0)) {
            uint randomNum  = getSomeRandomNumber(cityClash.blueScore() + count, cityClash.tokensMinted() * MAX_NUM_ENTRIES);
            console.log('randomNum', randomNum);
            winner = entries[randomNum];
            count++;
        }
        return winner;
    }

    function performUpkeep(bytes calldata _performData) external override {
        updateRandomIndex();
        if (isUpkeepNeeded()) {
            address wallet = abi.decode(_performData, (address));
            console.log('address in perform', wallet);
            payWinner(wallet);
        }
    }

    function payWinner(address _address) internal {
        console.log('balance', address(this).balance);
        uint balance = address(this).balance;
        (bool success,) = address(_address).call{value: balance}('');
        console.log('success', success);
        lastPaymentTime = block.timestamp;
        emit PayedWinnerEvent(_address, balance);
    }

    function getSomeRandomNumber(uint _seed, uint _limit) internal view returns (uint16) {
        uint extra = 0;
        for (uint16 i = 0; i < 7; i++) {
            extra += _randomSource[_randomIndex].balance;
        }

        uint random = uint(
            keccak256(
                abi.encodePacked(
                    _seed,
                    blockhash(block.number - 1),
                    block.coinbase,
                    block.difficulty,
                    msg.sender,
                    extra,
                    _randomCalls,
                    _randomIndex
                )
            )
        );

        return uint16(random % _limit);
    }

    function updateRandomIndex() internal {
        _randomIndex += 1;
        _randomCalls += 1;
        if (_randomIndex > 6) _randomIndex = 0;
    }

    function changeRandomSource(uint16 _id, address _address) external onlyOwner {
        _randomSource[_id] = _address;
    }

    function setCityClashAddress(address _address) external onlyOwner {
        cityClashContractAddress = _address;
    }

    function setPaused(bool _isPaused) external onlyOwner {
        isPaused = _isPaused;
    }
}

