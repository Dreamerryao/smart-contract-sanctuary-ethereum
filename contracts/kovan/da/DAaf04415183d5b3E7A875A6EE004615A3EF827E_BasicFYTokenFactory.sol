import "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import "./IBasicFYToken.sol";
import "hardhat/console.sol";

contract BasicFYToken is IBasicFYToken, ERC20, Ownable {
  using SafeMath for uint256;

  address private immutable _underlying;
  uint256 private immutable _maturity;
  uint256 private immutable _rate;

  address public minter;

  event SetMinter(address minter);

  constructor(
    address underlying_,
    uint256 maturity_,
    string memory name,
    string memory symbol
  )
    ERC20(name, symbol)
  {
    _underlying = underlying_;
    _maturity = maturity_;
    _rate = (10 ** ERC20(underlying_).decimals());
  }

  modifier onlyMinter {
    require(msg.sender == minter);
    _;
  }

  function maturity() external view override returns (uint256) {
    return _maturity;
  }

  // returns whether or not the fyTokens have matured
  function isMature() external view override returns (bool) {
    return block.timestamp >= _maturity;
  }

  function mint(address _recipient, uint256 _amount) external override onlyMinter {
    // uint256 fyTokens = amountUnderlying.mul(salePrice) / _rate;

    _mint(_recipient, _amount);
  }

  // burns fyTokens and returns underlying token to the msg.sender
  function redeem(address _recipient, uint256 _amount) external override {
    require(block.timestamp >= _maturity, "Immature");

    uint256 underlyingTokensAvailable = ERC20(_underlying).balanceOf(address(this));
    uint256 redeemAmount = _amount.mul(underlyingTokensAvailable).div(totalSupply());

    _burn(msg.sender, _amount);
    TransferHelper.safeTransfer(_underlying, _recipient, redeemAmount);
  }

  // returns the underlying token address
  function underlying() external view override returns (address) {
    return _underlying;
  }

  // returns the redemption value of a single fyToken after maturity in units of the underlying token
  function rate() external view override returns (uint256) {
    return _rate;
  }

  function setMinter(address _newMinter) external override onlyOwner {
    minter = _newMinter;
    emit SetMinter(_newMinter);
  }

  function claimMintability(bytes calldata signature) external override {
    console.logBytes(abi.encodePacked('YieldSpace-Claim', msg.sender));
    bytes32 messageHash = keccak256(abi.encodePacked('YieldSpace-Claim', msg.sender));
    console.logBytes32(messageHash);
    address signer = ECDSA.recover(ECDSA.toEthSignedMessageHash(messageHash), signature);
    console.log(signer);


    // // Divide the signature in r, s and v variables
    // bytes32 r;
    // bytes32 s;
    // uint8 v;
    // bytes memory sig = signature;

    // // ecrecover takes the signature parameters, and the only way to get them
    // // currently is to use assembly.
    // // solhint-disable-next-line no-inline-assembly
    // assembly {
    //     r := mload(add(sig, 0x20))
    //     s := mload(add(sig, 0x40))
    //     v := byte(0, mload(add(sig, 0x60)))
    // }
    // console.logBytes32(r);
    // console.logBytes32(s);
    // console.log(v);

    require(signer == owner(), 'Invalid signature');

    minter = msg.sender;
    emit SetMinter(msg.sender);
  }
}

