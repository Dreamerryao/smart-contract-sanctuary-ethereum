pragma solidity ^0.6.12;














contract Migrator is Ownable {
    using SafeMath for uint;
    using SafeERC20 for IERC20;
    uint constant defaultScale = 1e18;

    struct Destination {
        address gToken;
        bool isFromAtoken; // aToken true
    }

    IWETH internal immutable WETH;
    address public gEth;
    address public farm;
    address public aLendingPool; // aave lending pool
    IFactory public origFactory01;
    IFactory public origFactory02;
    address public router;
    IFactory public factory;
    uint256 public startBlock;
    address public comptroller;
    address public origComptroller;
    uint256 public desiredLiquidity = uint(- 1);
    mapping(address => Destination) public destination;


    constructor(
        address _farm,
        address _router,
        address _origFactory01,
        address _origFactory02,
        address _factory,
        address _origComptroller,
        address _comptroller,
        address _gEth,
        address _aLendingPool,
        address _WETH,
        uint256 _startBlock) public {
        require(_farm!=address(0),"farm error");
        farm = _farm;
        router = _router;
        origFactory01 = IFactory(_origFactory01);
        origFactory02 = IFactory(_origFactory02);
        factory = IFactory(_factory);
        origComptroller = _origComptroller;
        comptroller = _comptroller;
        startBlock = _startBlock;
        aLendingPool = _aLendingPool;
        gEth = _gEth;
        WETH = IWETH(_WETH);
    }

    modifier onlyFarm(){
        require(msg.sender == farm, "Not farm");
        _;
    }


    function migrate(address[] calldata _tokens, uint256[] calldata _amounts) external onlyFarm returns (address, address, uint256, uint256){
        require(block.number > startBlock, "Too early");
        if (isLpToken(_tokens[0])) {
            //            lp token
            return migrateLpToken(_tokens, _amounts);
        } else {
            //            gToken
            return migrateGtoken(_tokens, _amounts);
        }
    }

    function isLpToken(address _token) internal returns (bool){
        try IUniswapV2Pair(_token).factory(){
            return true;
        }catch(bytes memory){
            return false;
        }
    }

    function migrateLpToken(address[] calldata _tokens, uint256[] calldata _amounts) internal returns (
        address,
        address,
        uint256,
        uint256){
        require(address(origFactory01) != address(0), "Original factory01 error");
        require(address(origFactory02) != address(0), "Original factory02 error");
        require(address(factory) != address(0), "Target factory error");
        require(router != address(0), "Router error");
        require(IMigratorToken(_tokens[0]).factory() == address(origFactory01), "Original factory01 mismatch");
        (uint token0Amount,uint token1Amount,IUniswapV2Pair pairA) = removeLiquidity(_tokens[0], _amounts[0]);
        desiredLiquidity = _amounts[0];
        uint256 refundAmount;
        uint256 rufundLpIndex;
        address refundToken;
        if (_tokens[1]!=address(0)) {
            address lpToken02 = _tokens[1];
            uint256 amount02 = _amounts[1];
            require(IMigratorToken(lpToken02).factory() == address(origFactory02), "Original factory02 mismatch");
            (uint tokenB0,uint tokenB1,IUniswapV2Pair pairB) = removeLiquidity(lpToken02, amount02);
            require(address(pairA) == address(pairB), "Target pair inequality");
            (refundAmount, rufundLpIndex, refundToken, token0Amount, token1Amount) = handleExcess(pairA, pairB, token0Amount, token1Amount, tokenB0, tokenB1);
            desiredLiquidity = desiredLiquidity.add(amount02);
            IERC20(refundToken).safeTransfer(msg.sender, refundAmount);
        }
        address token0 = pairA.token0();
        address token1 = pairA.token1();
        TransferHelper.safeApprove(token0, router, token0Amount);
        TransferHelper.safeApprove(token1, router, token1Amount);
        IUniswapV2Router02(router).addLiquidity(token0, token1, token0Amount, token1Amount, token0Amount, token1Amount, msg.sender, block.timestamp.add(3));
        desiredLiquidity = uint(- 1);
        TransferHelper.safeApprove(token0, router, 0);
        TransferHelper.safeApprove(token1, router, 0);
        return (address(pairA), refundToken, refundAmount, rufundLpIndex);
    }

    function handleExcess(
        IUniswapV2Pair pairA,
        IUniswapV2Pair pairB,
        uint tokenA0,
        uint tokenA1,
        uint tokenB0,
        uint tokenB1
    ) internal returns (
        uint refundAmount,
        uint rufundLpIndex,
        address refundToken,
        uint token0Amount,
        uint token1Amount){

        if (tokenA0 >= tokenA1) {
            if (tokenA0.mul(defaultScale).div(tokenA1) >= tokenB0.mul(defaultScale).div(tokenB1)) {
                uint desiredTokenA0 = tokenA1.mul(tokenB0).div(tokenB1);
                refundAmount = tokenA0.sub(desiredTokenA0);
                refundToken = pairA.token0();
                rufundLpIndex = 1;
                token0Amount = desiredTokenA0.add(tokenB0);
            } else {
                require(false,"~");
                uint desiredTokenB0 = tokenA0.mul(tokenB1).div(tokenA1);
                refundAmount = tokenB0.sub(desiredTokenB0);
                refundToken = pairB.token0();
                rufundLpIndex = 2;
                token0Amount = tokenA0.add(desiredTokenB0);
            }
            token1Amount = tokenA1.add(tokenB1);
        } else {
            if (tokenA1.mul(defaultScale).div(tokenA0) >= tokenB1.mul(defaultScale).div(tokenB0)) {
                uint desiredTokenA1 = tokenA0.mul(tokenB1).div(tokenB0);
                refundAmount = tokenA1.sub(desiredTokenA1);
                refundToken = pairA.token1();
                rufundLpIndex = 1;
                token1Amount = desiredTokenA1.add(tokenB1);
            } else {
                uint desiredTokenB1 = tokenA1.mul(tokenB0).div(tokenA0);
                refundAmount = tokenB1.sub(desiredTokenB1);
                refundToken = pairB.token1();
                rufundLpIndex = 2;
                token1Amount = tokenA1.add(desiredTokenB1);
            }
            token0Amount = tokenA0.add(tokenB0);
        }
    }

    function removeLiquidity(address _token, uint256 amount) internal returns (uint256 token0BalanceChange, uint256 token1BalanceChange, IUniswapV2Pair pair){
        IUniswapV2Pair origPair = IUniswapV2Pair(_token);
        address token0 = origPair.token0();
        address token1 = origPair.token1();
        pair = IUniswapV2Pair(factory.getPair(token0, token1));
        if (pair == IUniswapV2Pair(address(0))) {
            pair = IUniswapV2Pair(factory.createPair(token0, token1));
        }
        origPair.transferFrom(msg.sender, address(origPair), amount);
        uint256 token0balance = IERC20(token0).balanceOf(address(this));
        uint256 token1balance = IERC20(token1).balanceOf(address(this));
        origPair.burn(address(this));
        token0BalanceChange = IERC20(token0).balanceOf(address(this)).sub(token0balance);
        token1BalanceChange = IERC20(token1).balanceOf(address(this)).sub(token1balance);
    }

    function migrateGtoken(address[] calldata _tokens, uint256[] calldata _amounts) internal returns (address, address, uint256, uint256){
        require(_tokens[0]!=address(0), "Param invalid");
        address token = _tokens[0];
        uint amount = _amounts[0];
        Destination memory dst = destination[token];
        require(dst.gToken != address(0), "Error gToken Address");
        IERC20(token).safeTransferFrom(farm, address(this), amount);
        if (dst.gToken == gEth) {
            //            aETh or cETh migrate to gEth.
            return migrateGTokenEth(token, amount);
        } else {
            //            aErc20 or cERc2o migrate to gErc20
            return migrateGTokenErc20(token, amount);
        }
    }

    //    fallback function to receive ehter.
    receive() external payable {}


    function setDestination(address[] calldata _source, address[] calldata _destination, bool _isFromAtoken) external onlyOwner {
        require(_source.length == _destination.length, "Array inconsistency");
        for (uint i = 0; i < _source.length; i++) {
            destination[_source[i]] = Destination(_destination[i], _isFromAtoken);
        }
    }

    function getDestinationAddress(address _source) external view returns (address){
        return destination[_source].gToken;
    }

    function migrateGTokenErc20(address _token, uint256 _amount) internal returns (address, address, uint256, uint256){
        Destination memory dst = destination[_token];
        uint redeemOut = redeemUnderlying(_token, dst, _amount);
        require(redeemOut > 0, "Redeem fail");
        uint256 mintOut = mint(dst, redeemOut);
        return (address(0), address(0), mintOut, 0);
    }

    function migrateGTokenEth(address _token, uint256 _amount) internal returns (address, address, uint256, uint256){
        Destination memory dst = destination[_token];
        uint redeemOut = redeemEth(_token, dst, _amount);
        require(redeemOut > 0, "Redeem Eth fail");
        uint256 mintOut = mintGEth(dst, redeemOut);
        return (address(0), address(0), mintOut, 0);
    }


    function redeemEth(address _token, Destination memory _dst, uint _amount) internal returns (uint redeemEthOut){
        uint ethAmountBefore = address(this).balance;
        if (!_dst.isFromAtoken) {
            //            ctoken
            IMigratorToken token = IMigratorToken(_token);
            require(token.redeem(_amount) == 0, "redeem eth fail");
        } else {
            //            atoken
            aTokenRedeemEth(_token, _amount);
        }
        uint ethAmountAfter = address(this).balance;
        redeemEthOut = ethAmountAfter.sub(ethAmountBefore);
    }

    function mintGEth(Destination memory _dst, uint _amount) internal returns (uint mintOut){
        IMigratorToken token = IMigratorToken(_dst.gToken);
        uint gTokenBefore = token.balanceOf(address(this));
        token.mint{value : _amount}();
        uint gTokenAfter = token.balanceOf(address(this));
        mintOut = gTokenAfter.sub(gTokenBefore);
        IERC20(address(token)).safeTransfer(msg.sender, mintOut);
    }

    function mint(Destination memory _dst, uint _amout) internal returns (uint mintOut){
        IMigratorToken token = IMigratorToken(_dst.gToken);
        uint gTokenBefore = token.balanceOf(address(this));
        IERC20(token.underlying()).safeApprove(_dst.gToken, _amout);
        require(token.mint(_amout) == 0, "Mint fail");
        IERC20(token.underlying()).safeApprove(_dst.gToken, 0);
        uint gTokenAfter = token.balanceOf(address(this));
        mintOut = gTokenAfter.sub(gTokenBefore);
        IERC20(address(token)).safeTransfer(msg.sender, mintOut);
    }

    function redeemUnderlying(address _token, Destination memory _dst, uint _amount) internal returns (uint redeemOut){
        address underlying = IMigratorToken(_dst.gToken).underlying();
        IMigratorToken token = IMigratorToken(_token);
        uint underlyingAmountBefore = IERC20(underlying).balanceOf(address(this));
        if (_dst.isFromAtoken) {
            //            aToken
            //            wether withdraw all
            ILendingPool(aLendingPool).withdraw(underlying,_amount,address(this));
//            ILendingPool(aLendingPool).withdraw(underlying, type(uint).max, address(this));
        } else {
            //            cToken
            require(origComptroller == token.comptroller(), "comptroller is incorrect");
            require(token.redeem(_amount) == 0, "Redeem fail");
        }
        uint underlyingAmountAfter = IERC20(underlying).balanceOf(address(this));
        redeemOut = underlyingAmountAfter.sub(underlyingAmountBefore);
    }

    function aTokenRedeemEth(address _token, uint _amount) internal {
        IAToken aWETH = IAToken(ILendingPool(aLendingPool).getReserveData(address(WETH)).aTokenAddress);
        require(_token == address(aWETH), "token address mismatch");
        uint256 userBalance = aWETH.balanceOf(address(this));
        uint256 amountToWithdraw = _amount;
        if (_amount == type(uint256).max) {
            amountToWithdraw = userBalance;
        }
        ILendingPool(aLendingPool).withdraw(address(WETH), amountToWithdraw, address(this));
        WETH.withdraw(amountToWithdraw);
    }
}