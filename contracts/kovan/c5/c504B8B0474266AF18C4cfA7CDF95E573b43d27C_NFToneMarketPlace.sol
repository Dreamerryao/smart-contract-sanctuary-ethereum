import "./Validator/Validator.sol";

contract NFToneMarketPlace is DataLayout, Initializable, UUPSUpgradeable, OwnableUpgradeable, AccessControlUpgradeable, PausableUpgradeable, Validator, Storage {
    using SafeMathUpgradeable for uint256;
    using Counters for Counters.Counter;

    uint8 version;
    Counters.Counter private _listingCounter;
    mapping(address => NFTone.Sell) placeholderSell;


    function initialize() public initializer {
        __Ownable_init();
        __UUPSUpgradeable_init();
        __Pausable_init();
        _Storage_init();
        version = 1;
        _listingCounter.increment(); // Set it to 1!
        // Set owner to reader and writer
        setWriterRole(msg.sender);
        setReaderRole(msg.sender);
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {

    }

    function getVersion() public view returns (uint8) {
        return version;
    }

    /**
    * @dev This function is used to create as Sell object from an encoded request.
    *
    * This is needed because in solidity we cannot have too much variables in a single function call.
    */
    function _sellItemFactory(bytes memory _encodedSell) public pure returns (NFTone.Sell memory decodedSell) {
        decodedSell = abi.decode(_encodedSell, (NFTone.Sell));
    }

    /**
    * @dev Adds a single listing to the storage.
    *
    * Requirements:
    *
    * - `_owner` owner of the token
    * - `_nft` address of the nft token
    * - `_tokenId` token id of the nft
    * - `_price` price of the sale
    */
    function addNewListing(address payable _owner, address _nft, uint256 _tokenId, uint256 _price) public onlyWriter whenNotPaused returns (bool) {
        NFTone.Sell memory sellItem = placeholderSell[address(0)];
        NFTone.NFTData memory nftData = NFTone.NFTData(_nft, _tokenId, IERC721Metadata(_nft).tokenURI(_tokenId));
        sellItem.sellId = _listingCounter.current();
        sellItem.ownerAddress = _owner;
        sellItem.nftData = nftData;
        sellItem.saleQuantity = 1;
        sellItem.isSale = true;
        sellItem.unitPrice = _price;
        sellItem.sellType = NFTone.SellType.DirectSell;
        // We don't need the other params i guess

        return register(sellItem);
    }

    /**
    * @dev Adds a multiple listings to the storage. It can add multiple sale object by providing the tokenIds and their
    * prices.
    *
    * Requirements:
    *
    * - `_owner` owner of the token
    * - `_nft` address of the nft token
    * - `_ids` token ids to be added
    * - `_prices` prices of each sale
    */
    function addBatchListing(address payable _owner, address _nft, uint256[] memory _ids, uint256[] memory _prices)
    public onlyWriter whenNotPaused returns (bool)
    {
        require(_ids.length == _prices.length, "INVALID_ARGUMENT_LENGTHS");

        for (uint i = 0; i < _ids.length; i++) {
            NFTone.Sell memory sellItem = placeholderSell[address(0)];
            NFTone.NFTData memory nftData = NFTone.NFTData(_nft, _ids[i], IERC721Metadata(_nft).tokenURI(_ids[i]));
            sellItem.sellId = _listingCounter.current();
            sellItem.ownerAddress = _owner;
            sellItem.nftData = nftData;
            sellItem.saleQuantity = 1;
            sellItem.unitPrice = _prices[i];
            sellItem.sellType = NFTone.SellType.DirectSell;

            register(sellItem);
        }

        return true;
    }

    /**
    * @dev This handles the registration of the sell object to the storage. Also runs validations.
    *
    * Requirements:
    *
    * - `_sell` the sell object
    */
    function register(NFTone.Sell memory _sell) public onlyWriter whenNotPaused returns (bool) {

        //  Validate sell object
        validateSell(_sell);

        // Previously added sell id
        bool isDefinedSellId = getIsDefinedSellId(_sell.sellId);
        require(isDefinedSellId == true, "SELL_ID_PREVIOUSLY_DEFINED");

        // Check if this logic contract is approved to interact in the name of the user in the token contract
        bool isApproved = IERC721(_sell.nftData.nftCreatorContract).isApprovedForAll(_sell.ownerAddress, address(this));
        require(isApproved == true, "NOT_AUTHORIZED_LOGIC_CONTRACT");

        // Check if seller is the owner of the token
//        address owner = IERC721(_sell.nftData.nftCreatorContract).ownerOf(_sell.nftData.tokenId);
//        require(owner == msg.sender, "TOKEN_NOT_OWNED_BY_SELLER");

        //register SellRegistry
        bool registrySellSuccess = sellRegister(_sell);
        require(registrySellSuccess == true, "SELL_ITEM_NOT_SAVED");

        // Increment the current id
        _listingCounter.increment();
        return true;
    }

    /**
    * @dev Gets the current block timestamp. Used for testing purposes.
    */
    function getTimeStamp() public view returns (uint256) {
        return block.timestamp;
    }

    /**
    * @dev Delegates the given price to the given address.
    *
    * Requirements:
    *
    * - `_to` the address where the value must go
    * - `_price` the amount which should be sent
    */
    function sendPrice(address _to, uint256 _price) internal returns (bool) {
        (bool successfulSendPrice,) = _to.call{value : _price}("");
        return successfulSendPrice;
    }

    /**
    * @dev This function can be called by the users whenn they want to purchase an NFT.
    *
    * Requirements:
    *
    * - `_sellId` the id of the sale object
    * - `_amount` the amount which the user wants to buy, it should be 1 while we only support ERC-721 tokens only
    */
    function purchase(uint256 _sellId, uint256 _amount, address payable _to) public payable whenNotPaused returns (bool)
    {
        // Validate withdraw
        Validator.validateWithdraw(_sellId, _to);

        // Check sell item properties
        NFTone.Sell memory sellItem = getSellItem(_sellId);
        require(sellItem.sellType == NFTone.SellType.DirectSell, "ITEM_IS_NOT_DIRECT_SELL");

        Validator.validateGetSellItem(sellItem, msg.sender, _amount);
        withdraw(sellItem, _to, _amount, sellItem.unitPrice, msg.value);

        return true;
    }

    /**
    * @dev This function handles the money transactions and also the token transfer.
    *
    * Requirements:
    *
    * - `_sellItem` the given sell item which we work with
    * - `_to` the buyer's address
    * - `_amount` the amount of goods the buyer wants to purchase
    * - `_unitPrice` the price of one unit in the current sale
    * - `_price` the amount of money the user sent
    */
    function withdraw(NFTone.Sell memory sellItem, address _to, uint256 _amount, uint256 _unitPrice, uint256 _price)
    internal returns (bool)
    {
        uint256 totalPrice = SafeMathUpgradeable.mul(_amount, _unitPrice);
        require(totalPrice <= _price, "INSUFFICIENT_SALE_AMOUNT");

        uint256 ownerPrice = totalPrice;

        // Send royalty to NFT owner
        {
            // Get royalty data
            (address royaltyReceiver, uint256 royaltyAmount) = IERC2981(sellItem.nftData.nftCreatorContract)
            .royaltyInfo(sellItem.nftData.tokenId, totalPrice);
            require(royaltyReceiver != address(0), "INVALID_ROYALTY_ADDRESS");

            // Subtract the royalty
            ownerPrice = SafeMathUpgradeable.sub(ownerPrice, royaltyAmount);

            bool successfulRoyaltySend = sendPrice(royaltyReceiver, royaltyAmount);
            require(successfulRoyaltySend == true, "ROYALTY_TRANSACTION_FAILED");
        }

        // Send fee to the feeReceiver
        {
            // Get fee data
            (address feeReceiver, uint256 feeAmount) = feeInfo(sellItem.nftData.tokenId, totalPrice);
            require(feeReceiver != address(0), "INVALID_FEE_ADDRESS");

            // Subtract the fee
            ownerPrice = SafeMathUpgradeable.sub(ownerPrice, feeAmount);

            bool successfulFeeSend = sendPrice(feeReceiver, feeAmount);
            require(successfulFeeSend == true, "FEE_TRANSACTION_FAILED");
        }

        // Send remaining price to owner
        {
            bool successfulOwnerSend = sendPrice(sellItem.ownerAddress, ownerPrice);
            require(successfulOwnerSend == true, "OWNER_TRANSACTION_FAILED");
        }
        address ownerAddress = sellItem.ownerAddress;
        // Transfer NFT
        IERC721(sellItem.nftData.nftCreatorContract).safeTransferFrom(ownerAddress, _to, sellItem.nftData.tokenId);

        //Now we only support ERC721 tokens, but for future ERC1155 support we have quantity too
        //Get remaining...
        uint256 remainingQuantity = SafeMathUpgradeable.sub(sellItem.saleQuantity, _amount);
        updateSaleQuantity(sellItem.sellId, remainingQuantity);

        // If there are no tokens remaining, set the sale status to false
        if (remainingQuantity == 0) {
            setSaleStatus(sellItem.sellId, false);
        }

        return true;
    }

    /**
    * @dev This function returns the royalty amount for the given sale object.
    *
    * Requirements:
    *
    * - `_sellId` the id of the sale object
    */
    function getRoyalty(uint256 _sellId) public view returns (uint256) {
        NFTone.Sell memory sellItem = getSellItem(_sellId);
        uint256 totalPrice = sellItem.unitPrice;
        (, uint256 royaltyAmount) = IERC2981(sellItem.nftData.nftCreatorContract).royaltyInfo(sellItem.nftData.tokenId, totalPrice);
        return royaltyAmount;
    }

    /**
    * @dev This function returns the fee amount for the given sale object.
    *
    * Requirements:
    *
    * - `_sellId` the id of the sale object
    */
    function getFee(uint256 _sellId) public view returns (uint256) {
        NFTone.Sell memory sellItem = getSellItem(_sellId);
        uint256 totalPrice = sellItem.unitPrice;
        (, uint256 feeAmount) = feeInfo(sellItem.nftData.tokenId, totalPrice);
        return feeAmount;
    }

    /**
    * @dev This function can set the fee amount and also the address where the fees should be transferred.
    *
    * Requiremetns:
    *
    * - `_receiver` the address of the receiver
    * - `_feeNumerator` the numerator which should be less than 10000
    *
    * Example:
    *
    * If the _feeNumerator is set to 5000, then the fee amount will be 5000/10000 = 0.5 which is 50%
    */
    function setDefaultFee(address _receiver, uint96 _feeNumerator) public virtual onlyOwner {
        _setDefaultFee(_receiver, _feeNumerator);
    }

    /**
    * @dev This will pause the contract, so the functions with the whileNotPaused modifier will fail to execute after.
    * this was called.
    */
    function pause() public onlyOwner {
        _pause();
    }

    /**
    * @dev This will unpause the contract, if the contract was paused then the functions with the whileNotPaused.
    * modifier can be called  again.
    */
    function unpause() public onlyOwner {
        _unpause();
    }

}

