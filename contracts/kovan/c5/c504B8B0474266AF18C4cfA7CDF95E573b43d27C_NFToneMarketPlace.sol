pragma solidity ^0.8.0;

contract NFTone {

    enum SellType {
        DirectSell,
        Auction
    }

    struct NFTData {
        address nftCreatorContract;
        uint256 tokenId;
        string tokenURI;
    }

    struct Sell {
        uint256 sellId;
        address payable ownerAddress;
        NFTone.NFTData nftData;
        uint256 unitPrice;
        uint256 saleQuantity;
        bool isSale;
        bool isPrivateSell;
        address privateSellAddress;
        SellType sellType;
        uint256 minimumBid;
        uint256 topBid;
        uint256 auctionEndTime;
        uint256 incrementQty;
    }

    struct Bid {
        uint256 sellId;
        uint256 topBid;
        address topBidAddress;
    }

    struct ProxyRegistry {
        uint256 sellId;
        address sellStorageAddress;
        address feeStorageAddress;
    }

    struct Metadata {
        uint256 sellId;
        bytes key;
        bytes value;
    }

    struct Royalty {
        uint256 id;
        address to;
        uint256 royaltyRate;
    }

    struct FeeInfo {
        address receiver;
        uint96 feeFraction;
    }

    struct FeeOverride {
        address _address;
        uint256 feeRate;
        address providerAddress;
    }
}

