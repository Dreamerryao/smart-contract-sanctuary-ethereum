pragma solidity ^0.8.0;

import "../NFTone/NFTone.sol";

contract Validator {

    function validateSell(NFTone.Sell memory _sell) internal view returns (bool) {
        // Sell quantity cannot be zero
        require(_sell.saleQuantity != uint256(0), "SALE_QUANTITY_CANNOT_BE_ZERO");

        // Only the token owner can sell
//         TODO: DO WE NEED THIS? ONLY IF THE TOKEN IS NOT MINTED BY THE STORE
//        require(msg.sender == _sell.ownerAddress, "MUST_BE_TOKEN_OWNER");

        // If it is a private sell. the private sell address must be set
        if (_sell.isPrivateSell == true && _sell.privateSellAddress == address(0)) {
            revert("INVALID_PRIVATE_SELL_ADDRESS");
        }

        // It it is an auction, it must be within the auction time
        if (_sell.sellType != NFTone.SellType.DirectSell && block.timestamp >= _sell.auctionEndTime) {
            revert("PAST_AUCTION_DATE");
        }

        return true;
    }

    function validateBid(uint256 _sellId, address _bidderAddress) internal view returns (bool) {
        // Only the bidder can call this function
        require(msg.sender == _bidderAddress, "THE_SENDER_MUST_BE_THE_BIDDER_ADDRESS");
        // Bid price cannot be zero
        require(msg.value != uint256(0), "THE_BID_PRICE_CANNOT_BE_ZERO");
        // Sell ID must be set
        require(_sellId != uint256(0), "THE_SELL_ID_CANNOT_BE_ZERO");

        return true;
    }

    function validateWithdraw(uint256 _sellId, address _to) internal view returns (bool) {
        // Sell ID must be set
        require(_sellId != uint256(0), "THE_SELL_ID_CANNOT_BE_ZERO");
        // To address must be set
        require(_to != address(0), "THE_TO_ADDRESS_CANNOT_BE_EMPTY");
        // To address must the sender
        require(_to == msg.sender, "THE_SENDER_MUST_BE_THE_TO_ADDRESS");
        // Sale amount cannot be zero
        require(msg.value != uint256(0), "SALE_AMOUNT_CANNOT_BE_ZERO");

        return true;
    }

    function validateGetSellItem(NFTone.Sell memory _sell, address _to, uint256 _amount) internal view returns (bool) {
        // Sale item must be on sale
        require(_sell.isSale == true, "THE_ITEM_MUST_BE_ON_SALE");
        // Checking for insufficient value
        require(_sell.unitPrice <= msg.value, "THE_UNIT_PRICE_CANNOT_BE_LARGER_THAN_THE_ACTUAL_MSG_VALUE");

        // On private sales only the given address can buy
        if (_sell.isPrivateSell == true && _sell.privateSellAddress != _to) {
            revert("THE_BUYER_ADDRESS_IS_INVALID");
        }

        // Checking if there are enough tokens left
        require(_sell.saleQuantity >= _amount, "THE_AMOUNT_MUST_BE_EQUAL_OR_LOWER_THAN_THE_TOKEN_BALANCE");

        return true;
    }

}

