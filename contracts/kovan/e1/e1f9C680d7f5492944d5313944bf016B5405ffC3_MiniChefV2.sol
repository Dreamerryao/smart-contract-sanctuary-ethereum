        if (address(_rewarder) != address(0)) {
            _rewarder.onSushiReward(pid, msg.sender, _for, 0, user.amount);
        }
        
        lpToken[pid].safeTransfer(msg.sender, amount);

        emit Withdraw(msg.sender, pid, amount, _for);
