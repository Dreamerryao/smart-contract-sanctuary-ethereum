    function withdraw(uint256 pid, uint256 amount, address _for) public {
        PoolInfo memory pool = updatePool(pid);
        UserInfo storage user = userInfo[pid][_for];

        require(user.operator == msg.sender, "only operator");
        require(user.amount >= amount, "withdraw: not good");

        // Effects
        user.rewardDebt = user.rewardDebt.sub(int256(amount.mul(pool.accSushiPerShare) / ACC_SUSHI_PRECISION));
