pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/AAVE/ILendingPool.sol";
import "./interfaces/Uniswap/IUniswapV2Router02.sol";
import "./libraries/Uniswap/UniswapV2Library.sol";
import "./interfaces/Uniswap/IUniswapV2Factory.sol";

contract AaveToUni {
    using SafeERC20 for IERC20;

    struct Token {
        address token;
        uint256 amount;
        uint256 referralCode;
    }

    struct UniConfiguration {
        uint256 amountADesired;
        uint256 amountBDesired;
        uint256 amountAMin;
        uint256 amountBMin;
        uint256 deadline;
    }

	address public constant LENDING_POOL = 0xE0fBa4Fc209b4948668006B2bE61711b7f465bAe;          //  proxy's address
    address public constant UNISWAP_V2_ROUTER = 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D;
    uint256 private constant AMOUNT_ERR = type(uint256).max;

    event Liquidity(
        address indexed sender,
        address indexed beneficiary,
        address indexed pool,
        uint256 liquidAmt,
        uint256 remainAToken1,
        uint256 remainAToken2
    );
	
    /**
       	@notice Check whether the asset is valid (AAVE Market)
       	@dev  Caller can be ANY
		@param	_token				Address of ERC-20 Token contract
    */
    function isValidToken(address _token) public view returns (bool) {
        address[] memory _reserves = ILendingPool(LENDING_POOL).getReservesList();
        uint256 _size = _reserves.length;
        if (_size == 0)
            return false;

        for(uint256 i; i < _size; i++) {
            if ( _token == _reserves[i] )
                return true;
        }

        return false;
    }

    /**
       	@notice Check amounts of ERC-20 Token that need to deposit as liquidity (UniswapV2)
       	@dev  Caller can be ANY
    */
    function checkAmounts(
        address _tokenA, address _tokenB,
        uint _amountADesired, uint _amountBDesired,
        uint _amountAMin, uint _amountBMin
    ) external view returns (uint256 _amountA, uint256 _amountB, bool _isCreated) {
        IUniswapV2Factory _factory = IUniswapV2Factory(
            IUniswapV2Router01(UNISWAP_V2_ROUTER).factory()
        );
        if ( _factory.getPair(_tokenA, _tokenB) == address(0) )
            return (_amountADesired, _amountBDesired, false);

        _isCreated = true;
        (uint256 _reserveA, uint256 _reserveB) = UniswapV2Library.getReserves(
            address(_factory), _tokenA, _tokenB
        );
        
        if (_reserveA == 0 && _reserveB == 0) {
            (_amountA, _amountB) = (_amountADesired, _amountBDesired);
        } else {
            
            uint256 _amountBOptimal = UniswapV2Library.quote(_amountADesired, _reserveA, _reserveB);
            if (_amountBOptimal <= _amountBDesired) {
                // require(_amountBOptimal >= amountBMin, 'UniswapV2Router: INSUFFICIENT_B_AMOUNT');
                if (_amountBOptimal < _amountBMin)
                    (_amountA, _amountB) = (AMOUNT_ERR, AMOUNT_ERR);
                else
                    (_amountA, _amountB) = (_amountADesired, _amountBOptimal);
            } else {
                uint256 _amountAOptimal = UniswapV2Library.quote(_amountBDesired, _reserveB, _reserveA);
                // assert(_amountAOptimal <= _amountADesired);
                // require(_amountAOptimal >= amountAMin, 'UniswapV2Router: INSUFFICIENT_A_AMOUNT');
                if (_amountAOptimal > _amountADesired || _amountAOptimal < _amountAMin)
                    (_amountA, _amountB) = (AMOUNT_ERR, AMOUNT_ERR);
                else
                    (_amountA, _amountB) = (_amountAOptimal, _amountBDesired);
            }
        }
    }

    function aaveToUni(
        address _to,
        Token calldata _asset1,
        Token calldata _asset2,
        UniConfiguration calldata _liquidConfig
    ) external {
        //  Transfer `asset` to AaveToUni contract
        //  From: msg.sender    --> To: address(this)
        //  If msg.sender approved insufficient amount of allowance -> revert likely
        address _user = msg.sender;
        _transferFrom(_asset1.token, _user, address(this), _asset1.amount);
        _transferFrom(_asset2.token, _user, address(this), _asset2.amount);

        //  Deposit `asset` to AAVE Market
        //  From: address(this)    --> To: aToken contract
        _setAllowance(
            _asset1.token, _asset2.token, LENDING_POOL, _asset1.amount, _asset2.amount
        );
        address _aToken1 = _depositToAave(_asset1);
        address _aToken2 = _depositToAave(_asset2);
        
        //  Deposit to Uniswap pool
        _setAllowance(
            _aToken1, _aToken2, UNISWAP_V2_ROUTER, _asset1.amount, _asset2.amount
        );
        (,, uint256 _liquidAmt) = IUniswapV2Router01(UNISWAP_V2_ROUTER).addLiquidity(
            _aToken1, _aToken2, 
            _liquidConfig.amountADesired, _liquidConfig.amountBDesired,
            _liquidConfig.amountAMin, _liquidConfig.amountBMin, 
            _to, _liquidConfig.deadline
        );

        uint256 _remainAToken1 = _remain(_aToken1); 
        if (_remainAToken1 != 0)
            _transfer(_aToken1, _to, _remainAToken1);

        uint256 _remainAToken2 = _remain(_aToken2);
        if (_remainAToken2 != 0)
            _transfer(_aToken2, _to, _remainAToken2); 

        address _pool = IUniswapV2Factory(
            IUniswapV2Router01(UNISWAP_V2_ROUTER).factory()
        ).getPair(_aToken1, _aToken2);
            
        emit Liquidity(_user, _to, _pool, _liquidAmt, _remainAToken1, _remainAToken2);
    }

    function _depositToAave(Token calldata _asset) private returns (address _aToken) {
        ILendingPool _lp = ILendingPool(LENDING_POOL);
        _aToken = _lp.getReserveData(_asset.token).aTokenAddress;

        IERC20 _erc20Token = IERC20(_aToken);
        uint256 _balBefore = _erc20Token.balanceOf(address(this));
        _lp.deposit(
            _asset.token, _asset.amount, address(this), uint16(_asset.referralCode)
        );
        require(
            _erc20Token.balanceOf(address(this)) == _balBefore + _asset.amount, "Balance mismatch"
        );
    }

    function _transferFrom(address _token, address _from, address _to, uint256 _amount) private {
        IERC20(_token).safeTransferFrom(_from, _to, _amount);
    }

    function _transfer(address _token, address _to, uint256 _amount) private {
        IERC20(_token).safeTransfer(_to, _amount);
    }

    function _remain(address _token) private view returns (uint256 _amount) {
        return IERC20(_token).balanceOf(address(this));
    }

    function _setAllowance(
        address _token1, address _token2, address _operator, uint256 _amount1, uint256 _amount2
    ) private {
        IERC20(_token1).approve(_operator, _amount1);
        IERC20(_token2).approve(_operator, _amount2);
    }
}

