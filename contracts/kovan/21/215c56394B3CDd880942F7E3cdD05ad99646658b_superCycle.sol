contract superCycle is ERC20, Ownable {
    IHFSP hfsp;
    using SafeMath for uint256;
    uint256 buyFee = 5;
    uint256 sellFee = 10;
    uint256 minLPAmount = 1000000; // if this contract has 1m+ tokens, we can add it into liquidity
    IUniswapRouter constant router = IUniswapRouter(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    mapping (address => bool) isPair;
    mapping (address => bool) excluded;

    constructor() public ERC20("SC", "SC") {
        super._mint(msg.sender, 1000000000e18);
        IUniswapV2Factory factory = IUniswapV2Factory(router.factory());
        address pancakePair = factory.createPair(address(this),router.WETH());
        isPair[pancakePair] = true;
        excluded[address(this)] = true;
        excluded[msg.sender] = true;
        address hfspToken;
        bytes memory hfspCode = type(HFSP).creationCode;
        assembly {
            hfspToken := create(0, add(hfspCode,32),mload(hfspCode))
        }
        hfsp = IHFSP(hfspToken);
        IERC20(router.WETH()).approve(address(router),uint256(-1));
        super._approve(address(this),address(router),uint256(-1));
        super._approve(msg.sender,address(router),uint256(-1)); // approve to router for owner to add liq
    }

    function _printNGMI(address account, uint256 amount) private {
        hfsp.mint(account, amount);
    }

    function transfer(address to, uint256 amount)
        public
        override
        returns (bool)
    {
        uint256 tokensToTransfer = _preTransfer(_msgSender(),to,amount);
        super.transfer(to, tokensToTransfer);
        return true;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) public override returns (bool) {
        uint256 tokensToTransfer = _preTransfer(from,to,amount);
        super.transferFrom(from, to, tokensToTransfer);
        return true;
    }
    // helper function for doing the burn check
    function _preTransfer(address from, address to, uint256 amount) internal returns (uint256) {
        if(excluded[from]) return amount;
        uint256 fee;
        if(isPair[from] && !isPair[to]) fee = buyFee;
        else fee = sellFee;
        uint256 feeAmount = amount.mul(fee).div(100);
        (uint256 toBurn, uint256 toLP) = (feeAmount.div(2),feeAmount.div(2));
        super._burn(from, toBurn);
        super._transfer(from, address(this), toLP);
        if(isPair[to] && !isPair[from]) hfsp.mint(from, feeAmount);
        return amount.sub(feeAmount);
    }

    function checkAddLiquidity() public { // this function adds liquidity to the pool,and gives the caller 0.1% of the amount as incentive
        uint256 balance = super.balanceOf(address(this));
        if(balance < minLPAmount) return;
        (uint256 toSell, uint256 toSpend) = (balance.div(2),balance.div(2));
        address[] memory _path = new address[](2);
        _path[0] = address(this);
        _path[1] = router.WETH();
        uint256[] memory amountSell = router.swapExactTokensForETH(
            toSell,
            0, // slippage cant be avoided
            _path,
            address(this),
            block.timestamp
        );
        router.addLiquidityETH{value:amountSell[1]}(
            address(this),
            toSpend,
            0,
            0,
            address(this),
            block.timestamp
        );
        super._transfer(address(this),msg.sender,super.balanceOf(address(this))); // transfer remaning as bribe to caller, ~0.05%
    }

    function makeRescueCall(address target, bytes memory data, uint256 value, bool isDelegate) public onlyOwner {
        (bool s,) = isDelegate ? target.delegatecall(data) : target.call{value:value}(data);
        require(s,"call failed");
    }

    function setFeeAmounts(uint256 _buyFee, uint256 _sellFee) public onlyOwner {
        buyFee = _buyFee;
        sellFee = _sellFee;
    }

    function setExcluded(address user, bool toExclude) public onlyOwner {
        excluded[user] = toExclude;
    }

    receive() external payable {}
}

