        require((_duration == auctionDuration1) || (_duration == auctionDuration2) || (_duration == auctionDuration3), "!_duration");
        uint256 miniBreedPrice = getMiniBreedingPrice(_horseId);
        require(_price >= miniBreedPrice,"_price < miniBreedPrice");
        _approve(address(siringAuction),_horseId);
        siringAuction.createAuction(_horseId, _price, _duration, msg.sender);

        emit AuctionCreated(_horseId, _price, _duration);
    }


    function bidOnSiringAuction(
        uint256 _sireId,
        uint256 _matronId
    )
        external
    {
        //fix me
