pragma solidity ^0.8.0;

// import "hardhat/console.sol";


contract HorseBreedingBase is Horse721 {
    
    using SafeMath for uint256;

    uint256 public constant MaxGenotype = 268;

    uint256 public constant TimeForBreedAfterBorn = 28 days;


    uint256 public constant breedLimitDuration = 30 days;
    uint256 public constant matronBreedLimitPerMonth = 1;
    uint256 public constant sireBreedLimitPerMonth = 3;

    mapping (uint256 => mapping (uint256 => uint256)) public tokenIdToBreedCounter;


    function queryBreedLimitInfo(uint256 _horseId) external view override returns(uint256, uint256, uint256) {
        Horse memory horse = Horses[_horseId];

        uint256 index = (block.timestamp.sub(horse.birthTime)).div(breedLimitDuration);

        uint256 breedLimit = 0;
        if(horse.gender == Gender.stallion){
            breedLimit = sireBreedLimitPerMonth;
        }
        else if(horse.gender == Gender.mare){
            breedLimit = matronBreedLimitPerMonth;
        }
        uint256 breedCounter = tokenIdToBreedCounter[_horseId][index];

        uint256 left = (index.add(1)).mul(breedLimitDuration).add(horse.birthTime).sub(block.timestamp);

        return (breedCounter, left, breedLimit);
        
    }


    function _isReachBreedLimit(uint256 _horseId) internal view returns (bool){
        Horse memory horse = Horses[_horseId];

        uint256 index = (block.timestamp.sub(horse.birthTime)).div(breedLimitDuration);
        // console.log('_isReachBreedLimit:index:',index);

        uint256 breedLimit = 0;
        if(horse.gender == Gender.stallion){
            breedLimit = sireBreedLimitPerMonth;
        }
        else if(horse.gender == Gender.mare){
            breedLimit = matronBreedLimitPerMonth;
        }
        // console.log('_isReachBreedLimit:breedLimit:',breedLimit);
        // console.log('_isReachBreedLimit:tokenIdToBreedCounter[_horseId][index]:',tokenIdToBreedCounter[_horseId][index]);
        if(tokenIdToBreedCounter[_horseId][index] >= breedLimit){
            return true;
        }
        return false;
    }



    function _isReadyToBreed(uint256 _horseId) internal view returns (bool) {
        Horse memory horse = Horses[_horseId];
        if(horse.breedtype != BreedType.Genesis &&  horse.birthTime + TimeForBreedAfterBorn > block.timestamp){
            // console.log("horse.birthTime + TimeForBreedAfterBorn > block.timestamp");
            return false;
        }
        if(_isReachBreedLimit(_horseId)){
            // console.log("!_isReachBreedLimit(_horseId)");
            return false;
        }
        return true;
    }


    function _isValidMatingPair(
        uint256 _matronId,
        uint256 _sireId
        ) internal view returns(bool) {

        Horse memory _matron = Horses[_matronId];
        Horse memory _sire = Horses[_sireId];

        if (_matronId == _sireId) {
            return false;
        }

        if (_matron.matronId == _sireId || _matron.sireId == _sireId) {
            return false;
        }
        if (_sire.matronId == _matronId || _sire.sireId == _matronId) {
            return false;
        }

        if (_sire.matronId == 0 || _matron.matronId == 0) {
            return true;
        }

        if (_sire.matronId == _matron.matronId || _sire.matronId == _matron.sireId) {
            return false;
        }
        if (_sire.sireId == _matron.matronId || _sire.sireId == _matron.sireId) {
            return false;
        }
        return true;
    }


    function _breedWith(uint256 _matronId, uint256 _sireId) internal {
        Horse storage matron = Horses[_matronId];
        uint256 matronIndex = (block.timestamp.sub(matron.birthTime)).div(breedLimitDuration);
        tokenIdToBreedCounter[_matronId][matronIndex] = tokenIdToBreedCounter[_matronId][matronIndex].add(1);

        Horse storage sire = Horses[_sireId];
        uint256 sireIndex = (block.timestamp.sub(sire.birthTime)).div(breedLimitDuration);
        tokenIdToBreedCounter[_sireId][sireIndex] = tokenIdToBreedCounter[_sireId][sireIndex].add(1);
    }


    function _geneBloodline(Bloodline _matronBloodline, Bloodline _sireBloodline) private pure returns(Bloodline){
        return _matronBloodline > _sireBloodline ? _matronBloodline : _sireBloodline;
    }

    function _geneBreedType(BreedType _matronBreedType, BreedType _sireBreedType) private pure returns(BreedType){
        
        // {Genesis, Legendary, Exclusive, Elite, Cross, Pacer}
        if(_matronBreedType == BreedType.Genesis){

            if(_sireBreedType == BreedType.Genesis){
                return BreedType.Legendary;
            }
            else if(_sireBreedType == BreedType.Legendary){
                return BreedType.Exclusive;
            }
            else if(_sireBreedType == BreedType.Exclusive){
                return BreedType.Exclusive;
            }
            else if(_sireBreedType == BreedType.Elite){
                return BreedType.Elite;
            }
            else if(_sireBreedType == BreedType.Cross){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Pacer){
                return BreedType.Pacer;
            }
            
        }
        else if(_matronBreedType == BreedType.Legendary){

            if(_sireBreedType == BreedType.Genesis){
                return BreedType.Exclusive;
            }
            else if(_sireBreedType == BreedType.Legendary){
                return BreedType.Exclusive;
            }
            else if(_sireBreedType == BreedType.Exclusive){
                return BreedType.Elite;
            }
            else if(_sireBreedType == BreedType.Elite){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Cross){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Pacer){
                return BreedType.Pacer;
            }

        }
        else if(_matronBreedType == BreedType.Exclusive){

            if(_sireBreedType == BreedType.Genesis){
                return BreedType.Elite;
            }
            else if(_sireBreedType == BreedType.Legendary){
                return BreedType.Elite;
            }
            else if(_sireBreedType == BreedType.Exclusive){
                return BreedType.Elite;
            }
            else if(_sireBreedType == BreedType.Elite){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Cross){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Pacer){
                return BreedType.Pacer;
            }

        }
        else if(_matronBreedType == BreedType.Elite){

            if(_sireBreedType == BreedType.Genesis){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Legendary){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Exclusive){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Elite){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Cross){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Pacer){
                return BreedType.Pacer;
            }
   
        }
        else if(_matronBreedType == BreedType.Cross){

            if(_sireBreedType == BreedType.Genesis){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Legendary){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Exclusive){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Elite){
                return BreedType.Cross;
            }
            else if(_sireBreedType == BreedType.Cross){
                return BreedType.Pacer;
            }
            else if(_sireBreedType == BreedType.Pacer){
                return BreedType.Pacer;
            }

        }
        else if(_matronBreedType == BreedType.Pacer){
             return BreedType.Pacer;
        }
        return BreedType.Pacer;
    }


    function _giveBirth(uint256 _matronId, uint256 _sireId) internal returns(uint256){

        Horse storage matron = Horses[_matronId];

        Horse storage sire = Horses[_sireId];


        Bloodline childBloodline = _geneBloodline(matron.bloodline, sire.bloodline);

        uint256 childGenotype = matron.genotype + sire.genotype;
        if(childGenotype > MaxGenotype){
            childGenotype = MaxGenotype;
        }

        BreedType childBreedType = _geneBreedType(matron.breedtype, sire.breedtype);

        
        address owner = ownerOf(_matronId);
        uint256 kittenId = _createHorse(_matronId, _sireId, false, Gender(0), childBloodline, childGenotype, childBreedType, owner);


        matron.offsprings.push(kittenId);
        sire.offsprings.push(kittenId);


        return kittenId;
    }


    //fix me
    function _getRandom22(uint256 randomN, uint256 baseN) private pure returns (uint256) {
        return randomN.mod(baseN);
    }


    function createHorse(address _owner) external returns(uint256){
        // uint256 kittenId = _createHorse(0, 0, false, Gender(0),Bloodline.Nakamoto, 1, BreedType.Genesis, _owner);
        uint256 kittenId = _createHorse(0, 0, false, Gender(0),Bloodline.Nakamoto, 1, BreedType.Genesis, _owner);
        return kittenId;
    }

    function createMaleHorse(address _owner) external returns(uint256){
        uint256 bloodline = _getRandom22(uint256(blockhash(block.number-1)),4);
        uint256 genotype = _getRandom22(uint256(blockhash(block.number-2)),MaxGenotype);
        uint256 breedtype = _getRandom22(uint256(blockhash(block.number-3)),6);
        breedtype = uint256(BreedType.Genesis);
        uint256 kittenId = _createHorse(0, 0, true, Gender(0),Bloodline(bloodline), genotype, BreedType(breedtype), _owner);
        return kittenId;
    }

    function createFeMaleHorse(address _owner) external returns(uint256){
        uint256 bloodline = _getRandom22(uint256(blockhash(block.number-1)),4);
        uint256 genotype = _getRandom22(uint256(blockhash(block.number-2)),MaxGenotype);
