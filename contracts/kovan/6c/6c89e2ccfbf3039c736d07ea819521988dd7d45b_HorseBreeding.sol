// File contracts/8/RANDOM.sol

pragma solidity ^0.8.0;

library RANDOM{
    
    using SafeMath for uint256;

    uint256 public constant targetBlockNumber = 256;

    function getRandom(uint256 baseN) internal view returns (uint256) {
        // uint256 randomN = uint256(blockhash(block.number.sub(256)));
        uint256 randomN = uint256(blockhash(block.number.sub(targetBlockNumber)));
        return randomN.mod(baseN);
    }

    function getRandomNext(uint256 b,uint256 baseN) internal view returns (uint256) {
        // uint256 randomN = uint256(blockhash(block.number.sub(256).sub(b)));
        uint256 randomN = uint256(blockhash(block.number.sub(targetBlockNumber).sub(b)));
        return randomN.mod(baseN);
    }

    function getRandomPure(uint256 b) internal view returns (uint256){
        uint256 randomN = uint256(blockhash(block.number.sub(targetBlockNumber).sub(b)));
        return randomN;
    }

}


