// Kovan DAI - 0xFf795577d9AC8bD7D90Ee22b6C1703490b6512FD
// Kovan Aave Lending Pool - 0xE0fBa4Fc209b4948668006B2bE61711b7f465bAe 
// Kovan Sablier finance contract - 0x5eb34b5d5c75ce2119078e5b3f6a3f30e457e46b
// Kovan POH contract - 0x73BCCE92806BCe146102C44c4D9c3b9b9D745794



contract BasicCredit is Ownable {
    using SafeERC20 for IERC20;

    string public Name = "Basic Credit Protocol"; 
    mapping(address => mapping(address => uint256)) public depositBalance;
    mapping(address => address) public aTokenAddress;
    mapping(address => uint256) public totalBalance;

    bytes public result1;
    bytes public result2;

    address public aaveLendingPool; 
    address public sablierContract;

    struct StreamData {
        address sender;
        address recipient;
        uint256 deposit;
        address token;
        uint256 startTime;
        uint256 stopTime;
        uint256 remainingBalance;
        uint256 ratePerSecond;
    }

    constructor(
        address _aaveLendingPool,
        address _sablierContract
        ) {
        aaveLendingPool = _aaveLendingPool;
        sablierContract = _sablierContract;
    }



    function Deposit(address _asset, uint256 _amount) public {
        require(_amount > 0, "Basic Credit: Deposit amount should be greater than 0");
        // Deposit tokens 
        IERC20(_asset).safeTransferFrom(msg.sender,address(this),_amount);
        //Update deposit balance.
        depositBalance[_asset][msg.sender] = depositBalance[_asset][msg.sender] + _amount;
        totalBalance[_asset] = totalBalance[_asset] + _amount;
    } 

    function depositToAave(address _asset, uint256 _amount) public onlyOwner {
        require(_amount > 0, "Basic Credit: Deposit amount should be greater than 0");
        require(IERC20(_asset).balanceOf(address(this)) >= _amount, "Deposit amount greater than balance");
        // Approve spending
        IERC20(_asset).safeApprove(aaveLendingPool,_amount);
        // Deposit tokens in Aave Lending Pool
        ILendingPool(aaveLendingPool).deposit(_asset, _amount, address(this), 0);
    } 



    function withdrawFromAave(address _asset, uint256 _amount) public onlyOwner {
        require(_amount > 0, "Basic Credit: Withdraw amount should be greater than 0");
        // Withdraw tokens from Aave Lending Pool
        ILendingPool(aaveLendingPool).withdraw(_asset, _amount, address(this));
    } 

    function Withdraw(address _asset, uint256 _amount) public {
        require(_amount > 0, "Basic Credit: Withdraw amount should be greater than 0");
        uint256 balance = depositBalance[_asset][msg.sender];
        require(_amount <= balance, "Basic Credit: Withdraw amount should less that or equal to your balance");
        
        require(IERC20(_asset).balanceOf(address(this)) >= _amount, "Wait for withdrawl from pool");
        //Update deposit balance
        depositBalance[_asset][msg.sender] = depositBalance[_asset][msg.sender] - _amount;
        // Withdraw tokens from Aave Lending Pool
        IERC20(_asset).safeTransfer(msg.sender,_amount);
    } 


    function tokenBalance(address _asset) public view returns (uint256) {
        return IERC20(_asset).balanceOf(address(this));
    }

    function getPoolData(address _asset) public view returns (DataTypes.ReserveData memory) {
        return ILendingPool(aaveLendingPool).getReserveData(_asset);   
    }

    function setATokenAddress(address _asset, address _aToken) public onlyOwner{
        aTokenAddress[_asset] = _aToken;
    }


    function getStreamDetails(uint256 _streamID) public view returns (
        address,
        address,
        uint256,
        address,
        uint256,
        uint256,
        uint256,
        uint256
        ) {
        return ISablier(sablierContract).getStream(_streamID);
    }




}

