pragma solidity ^0.8.0;

import "../storage/ETFPoolStorage.sol";
import "../libraries/PriceTool.sol";
import "../libraries/AddressArray.sol";

contract BaseETFPool {
    using AddressArray for address[];
    function getState()
    internal pure returns (ETFPoolStorage.State storage s)
    {
        return ETFPoolStorage.getState();
    }

    constructor() public {
        getState().factory = msg.sender;
    }

    modifier onlyCreator() {
        require(getState().creator == msg.sender, "Only creator can call this method");
        _;
    }

    modifier checkSwapContract(address swap_contract){
        int index = getState().poolInfo.supportSwapContract.indexOf(swap_contract);
        require(index != -1, "contract not support");
        _;
    }

    modifier requireTokenPriceFixed(address token_address){
        require((PriceTool.tokenPriceFixed(token_address)), 'token price not fixed');
        _;
    }

    modifier requireTokensPriceFixed(address[] calldata token_address){
        require(PriceTool.allTokenPriceFixed(token_address),'token price not fixed');
        _;
    }

    modifier supportInvest(address token){
        require(getState().poolInfo.supportInvestToken.indexOf(token) != -1, "token not support");
        _;
    }
}

pragma solidity ^0.8.0;

import "../interfaces/IETFPoolFactory.sol";

library ETFPoolStorage {
    struct State {
        address factory;
        address creator;
        IETFPoolFactory.ETFPoolInfo poolInfo;
        mapping(address => uint) token_balance;
        uint eth_balance;
        address[] token_list;
    }

    bytes32 constant ETFPOOL_STORAGE_POSITION = keccak256("diamond.standard.ETFPool.storage");

    function getState() internal pure returns (State storage s) {
        bytes32 position = ETFPOOL_STORAGE_POSITION;
        assembly {
            s.slot := position
        }
    }
}

pragma solidity ^0.8.0;
import "@chainlink/contracts/src/v0.8/interfaces/FeedRegistryInterface.sol";
import "@chainlink/contracts/src/v0.8/Denominations.sol";
library PriceTool {
    // main net
//    address constant registry_address = 0x47Fb2585D2C56Fe188D0E6ec628a38b74fCeeeDf;
//    address constant weth_address = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
    // kovan
    address constant registry_address = 0xAa7F6f7f507457a1EE157fE97F6c7DB2BEec5cD0;
    address constant weth_address = 0xd0A1E359811322d97991E03f863a0C30C2cF029C;

    function getTokenUsdPrice(address base) internal view returns (uint) {
        FeedRegistryInterface registry = FeedRegistryInterface(registry_address);
        if (base == weth_address){
            base = Denominations.ETH;
        }
        (uint80 roundID,int price,uint startedAt,uint timeStamp,uint80 answeredInRound) = registry.latestRoundData(base, Denominations.USD);
        return uint (price);
    }

    function getEthUsdPrice() internal view returns (uint) {
        FeedRegistryInterface registry = FeedRegistryInterface(registry_address);
        (uint80 roundID,int price,uint startedAt,uint timeStamp,uint80 answeredInRound) = registry.latestRoundData(Denominations.ETH, Denominations.USD);
        return uint(price);
    }

    function tokenPriceFixed(address token) internal view returns(bool){
        return FeedRegistryInterface(registry_address).getCurrentPhaseId(token, Denominations.USD) != 0 || token == weth_address;
    }

    function allTokenPriceFixed(address[] calldata token_address) internal view returns(bool){
        for(uint i=0; i<token_address.length; i++){
            if(!tokenPriceFixed(token_address[i])){
                return false;
            }
        }
        return true;
    }
}

pragma solidity ^0.8.0;

library AddressArray {
    function indexOf(address[] memory self, address value)
    internal
    pure
    returns (int)
    {
        for (int i = 0; i < int(self.length); i++)
            if (self[uint(i)] == value) return i;
        return -1;
    }

    function contain(address[] memory self, address[] calldata target)
    internal
    pure
    returns (bool)
    {
        if(self.length==0 || target.length > self.length){
            return false;
        }
        for(uint i=0; i< target.length; i++){
            bool contain = false;
            for (uint j=0 ;j < self.length; j++){
                if (target[i] == self[j]){
                    contain = true;
                    continue;
                }
            }
            if (!contain){
                return false;
            }
        }
        return true;
    }
}

