pragma solidity ^0.8.0;

import "../interfaces/IETFPool.sol";
import "../interfaces/IUniswapContract.sol";
import "../base/BaseETFPool.sol";
import "../base/ETFPoolToken.sol";
import "../interfaces/IETFPoolFactory.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "../libraries/AddressArray.sol";
import "../libraries/TransferHelper.sol";
import "hardhat/console.sol";

contract ETFPoolFacet is BaseETFPool, IETFPool, ETFPoolToken {
    using SafeMath for uint;
    using AddressArray for address[];
    function initializeETFPool(address _creator, IETFPoolFactory.ETFPoolInfo calldata _poolInfo) override external {
        require(msg.sender == getState().factory, 'MUST FACTORY CALL THIS FUNCTION');
        getState().creator = _creator;
        getState().poolInfo = _poolInfo;
        super.initializePoolToken(_poolInfo.poolName, _poolInfo.poolTokenName);
    }

    function factory() external view returns (address){
        return getState().factory;
    }

    function creator() external view returns (address){
        return getState().creator;
    }

    function poolInfo() external view returns (IETFPoolFactory.ETFPoolInfo memory){
        return getState().poolInfo;
    }

    function token_balance(address token) external view returns (uint){
        return getState().token_balance[token];
    }

    function eth_balance() external view returns (uint){
        return getState().eth_balance;
    }

    function token_list(uint i) external view returns (address){
        return getState().token_list[i];
    }

    function supportInvestTokenLength() override external view returns (uint){
        return getState().poolInfo.supportInvestToken.length;
    }

    function investWithETH() override external payable returns (uint amountPoolToken){
        uint before_invest_value = total_usd_value();
        // 计数
        getState().eth_balance = getState().eth_balance + msg.value;
        uint invest_eth_value = msg.value * PriceTool.getEthUsdPrice();
        console.log('invest_eth_value', invest_eth_value);
        console.log('totalSupply', totalSupply);
        if (totalSupply == 0) {
            amountPoolToken = invest_eth_value;
        } else {
            amountPoolToken = totalSupply * invest_eth_value / before_invest_value;
        }
        _mint(msg.sender, amountPoolToken, getState().poolInfo.fee, getState().creator);
        emit investPoolWithEth(msg.sender, address(this), msg.value);
    }

    function investWithToken(address tokenAddress, uint amount) override external supportInvest(tokenAddress) returns (uint amountPoolToken){
        uint before_invest_value = total_usd_value();
        console.log('investWithToken tokenAddress', tokenAddress, 'amount', amount);
        TransferHelper.safeTransferFrom(tokenAddress, msg.sender, address(this), amount);
        // 计数
        uint balance = getState().token_balance[tokenAddress];
        console.log('balance', balance);
        if (balance != 0) {
            getState().token_balance[tokenAddress] = balance.add(amount);
        } else {
            getState().token_balance[tokenAddress] = amount;
            getState().token_list.push(tokenAddress);
        }
        uint token_value = amount * PriceTool.getTokenUsdPrice(tokenAddress);
        console.log('token_value', token_value);
        if (totalSupply == 0) {
            amountPoolToken = token_value;
        } else {
            amountPoolToken = totalSupply * token_value / before_invest_value;
        }
        console.log('amountPoolToken', amountPoolToken);
        _mint(msg.sender, amountPoolToken, getState().poolInfo.fee, getState().creator);
        emit investPoolWithToken(msg.sender, address(this), tokenAddress, amount);
    }

    function total_usd_value() override public view returns (uint usd_value){
        uint eth_balance = address(this).balance;
        uint eth_value = getState().eth_balance.mul(PriceTool.getEthUsdPrice());
        console.log('eth_value', eth_value);
        usd_value = eth_value;
        for (uint i = 0; i < getState().token_list.length; i++) {
            address token_address = getState().token_list[i];
            uint token_value = getState().token_balance[token_address] * PriceTool.getTokenUsdPrice(token_address);
            console.log('token_address', token_address, 'token_value', token_value);
            usd_value = usd_value + token_value;
        }
        console.log('usd_value', usd_value);
    }

    function UniV2swap(address swapContract,uint256 amountIn,uint256 amountOutMin,address[] calldata path, address to, uint256 deadline) override external onlyCreator
    checkSwapContract(swapContract)
    requireTokenPriceFixed(path[path.length - 1])
    returns (uint[] memory amounts){
        require(getState().token_balance[path[0]] >= amountIn);
        uint path_length = path.length;
        //TODO 优化approve
        TransferHelper.safeApprove(path[0], swapContract, amountIn);
        amounts = IUniswapContract(swapContract).swapExactTokensForTokens(amountIn, amountOutMin, path, to, deadline);
        getState().token_balance[path[0]] = getState().token_balance[path[0]] - amountIn;
        getState().token_balance[path[path_length - 1]] = getState().token_balance[path[path_length - 1]] + amounts[path_length - 1];
        int index = getState().token_list.indexOf(path[path_length - 1]);
        if (index == - 1) {
            getState().token_list.push(path[path_length - 1]);
        }
        emit swap(address(this), path[0], path[path_length - 1], amountIn, amounts[path_length - 1]);
    }

    function withdraw(uint poolTokenAmount) override external returns (uint[] memory tokenAmounts){
        //        transferFrom(msg.sender,address(this),poolTokenAmount);
        tokenAmounts = new uint[](getState().token_list.length + 1);
        uint ethAmount = poolTokenAmount * getState().eth_balance / totalSupply;
        tokenAmounts[0] = ethAmount;
        payable(msg.sender).transfer(ethAmount);
        getState().eth_balance = getState().eth_balance - ethAmount;
        for (uint i = 0; i < getState().token_list.length; i++) {
            uint tokenAmount = poolTokenAmount * getState().token_balance[getState().token_list[i]] / totalSupply;
            tokenAmounts[i + 1] = tokenAmount;
            TransferHelper.safeTransfer(getState().token_list[i], msg.sender, tokenAmount);
            getState().token_balance[getState().token_list[i]] = getState().token_balance[getState().token_list[i]] - tokenAmount;
        }
        _burn(msg.sender, poolTokenAmount);
        return tokenAmounts;
    }
}

