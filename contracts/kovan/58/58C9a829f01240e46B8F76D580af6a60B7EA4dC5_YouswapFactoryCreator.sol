pragma solidity 0.7.4;



contract YouswapFactoryCreator {

    /// @notice 新建矿池合约: 项目方管理员，矿池外围合约地址，矿池core合约地址， invite合约地址， 是否在前端展示
    event YouswapFactoryCreatorEvent(address indexed owner, address indexed factory, address indexed core, address invite, bool isVisible);

    /// @notice oldOwner：旧Owner， newOwner：新Owner
    event TransferOwnership(address indexed oldOwner, address indexed newOwner);

    /// @notice clone模板合约
    event CloneEvent(address indexed clone);

    // 平台管理员
    address public admin;

    // 运营权限
    mapping(address => bool) public operateOwner;

    // you地址，用于接收创建矿池费用
    ITokenYou public you;

    // 手续费
    uint256 public createPoolCommission;
    uint256 public createInviteCommission;

    // clone使用的YouSwapFactoryV2模板
    address public poolFactoryTemplate;
    address public inviteTemplate;
    address public coreTemplate;

    // 项目方=>矿池工厂
    mapping(address=> address) creatorFactories;

    // 所有矿池工厂
    address[] public allFactories;
    address public constant ZERO = address(0);

    //平台抽成
    uint public benefitRate;
    uint constant benefitRateMin = 10; //0.1%
    uint constant benefitRateMax = 5000; //50%

    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    constructor(ITokenYou _you, uint256 _poolCommission, uint256 _inviteCommission, uint256 _benefitRate) {
        you = _you;
        createPoolCommission = _poolCommission;
        createInviteCommission = _inviteCommission;
        benefitRate = _benefitRate;
        admin = msg.sender;
    }

    ////////////////////////////////////////////////////////////////////////////////////
    /// @notice 校验admin权限
    modifier onlyAdmin() {
        require(admin == msg.sender, "YouSwap:FORBIDDEN_NOT_PLATFORM_ADMIN");
        _;
    }

    /// @notice 校验运营权限
    modifier onlyOperater() {
        require(operateOwner[msg.sender], "YouSwap:FORBIDDEN_NOT_OPERATER");
        _;
    }

    /// @notice 修改OWNER
    function transferOwnership(address _admin) external onlyAdmin {
        require(ZERO != _admin, "YouSwap:INVALID_ADDRESSES");
        emit TransferOwnership(admin, _admin);
        admin = _admin;
    }

    function _setOperateOwner(address user, bool state) internal {
        operateOwner[user] = state; //设置运营权限
    }

    /// @notice 设置运营权限
    function setOperateOwner(address user, bool state) external onlyAdmin {
        _setOperateOwner(user, state);
    }

    ////////////////////////////////////////////////////////////////////////////////////
    /// @notice  矿池合约模板， 供clone使用
    function setPoolFactoryTemplate(address _newTemplate) external onlyAdmin {
        poolFactoryTemplate = _newTemplate;
    }

    /// @notice  邀请合约模板， 供clone使用
    function setInviteTemplate(address _newTemplate) external onlyAdmin {
        inviteTemplate = _newTemplate;
    }

    /// @notice  矿池核心数据模板， 供clone使用
    function setcoreTemplate(address _newTemplate) external onlyAdmin {
        coreTemplate = _newTemplate;
    }

    //创建合约佣金设置
    function setCreatePoolCommission(uint256 _commission) external onlyAdmin {
        createPoolCommission = _commission;
    }

    function setCreateInviteCommission(uint256 _commission) external onlyAdmin {
        createInviteCommission = _commission;
    }

    struct FactoryCreatorLocalVars {
        string name;
        address token;
        uint8 poolType;
        uint8 distributionMode;
        address pFactory;
        address invite;
        address core;
        uint256 commission;
    }
    
    /**
        @param token 矿池奖励币种
        @param poolParams useInvite: 0邀请功能状态
            powerRatio: 1用算力兑换比例
            startTime: 2矿池开启时间
            priority: 3优先级
            maxStakeAmount: 4最大质押数量
            lockSeconds: 5锁仓秒数
            multiple: 6挖矿倍数
            selfReward: 7自邀奖励
            uppper1Reward: 8一级奖励
            upper2Reward: 9二级奖励
        @param poolType 矿池类型
        @param tokens 挖矿奖励币种
        @param rewardTotals 挖矿奖励币种数量
        @param rewardPerBlocks 单个区块奖励数量
    */
    function createPool(
        address token,
        uint256[] memory poolParams,
        uint8 poolType,
        address[] memory tokens,
        uint256[] memory rewardTotals,
        uint256[] memory rewardPerBlocks
    ) external {
        FactoryCreatorLocalVars memory vars;
        vars.token = token;
        vars.poolType = poolType;
        vars.pFactory = creatorFactories[msg.sender];
        vars.commission = createPoolCommission;

        if (vars.pFactory == ZERO) {
            vars.pFactory = createClone(poolFactoryTemplate);
            vars.invite = createClone(inviteTemplate);
            vars.core = createClone(coreTemplate);
            YouswapFactoryV2(vars.pFactory).initialize(msg.sender, address(this), benefitRate, vars.invite, vars.core);
            creatorFactories[msg.sender] = vars.pFactory;
            allFactories.push(vars.pFactory);
            emit YouswapFactoryCreatorEvent(msg.sender, vars.pFactory, vars.core, vars.invite, true);
        }

        //启用邀请功能
        if (0 != poolParams[0]) {
            vars.commission = createPoolCommission.add(createInviteCommission);
        }

        //支付600you创建矿池费用
        IERC20(address(you)).safeTransferFrom(msg.sender, address(this), vars.commission);

        //将挖矿奖励代币转入平台合约，在addPool内进一步转入到矿池工厂合约
        for (uint256 i = 0; i < tokens.length; i++) {
            require((ZERO != tokens[i]) && (address(this) != tokens[i]), "YouSwap:PARAMETER_ERROR_TOKEN");
            require(0 < rewardTotals[i], "YouSwap:PARAMETER_ERROR_REWARD_TOTAL");
            require(0 < rewardPerBlocks[i], "YouSwap:PARAMETER_ERROR_REWARD_PER_BLOCK");

            IERC20(tokens[i]).safeTransferFrom(msg.sender, address(this), rewardTotals[i]);
            IERC20(tokens[i]).approve(vars.pFactory, rewardTotals[i]);
        }

        YouswapFactoryV2(vars.pFactory).addPool(
            vars.token,
            poolParams,
            BaseStruct.PoolLockType(vars.poolType),
            tokens,
            rewardTotals,
            rewardPerBlocks);
    }

    /*
    获取you数量
    */
    function getBalance() external view returns (uint256) {
        return IERC20(address(you)).balanceOf(address(this));
    }

    /*
    获取所有矿池工厂
    */
    function getAllFactories() external view returns (address[] memory){
        return allFactories;
    }

    /*
    获取当前用户创建的矿池工厂
    */
    function getMyFactory() external view returns (address){
        return creatorFactories[msg.sender];
    }

    /**
    平台收取挖矿奖励抽成比例
     */
    function setBenefitRate(uint256 _newBenefitPercent) onlyAdmin external {
        if (_newBenefitPercent > 1000 || _newBenefitPercent < 10) {
            //TODO 发送事件，require空间占用太大了
            return;
        }

        benefitRate = _newBenefitPercent;
    }

    function createClone(address prototype) internal returns (address proxy) {
        bytes20 targetBytes = bytes20(prototype);
        assembly {
            let clone := mload(0x40)
            mstore(clone, 0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000)
            mstore(add(clone, 0x14), targetBytes)
            mstore(
            add(clone, 0x28),
            0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            proxy := create(0, clone, 0x37)
        }

        emit CloneEvent(proxy);
        return proxy;
    }
}