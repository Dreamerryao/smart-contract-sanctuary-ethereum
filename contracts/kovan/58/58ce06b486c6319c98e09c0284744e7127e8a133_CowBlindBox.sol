// File: Cow/Activity/CowBlindBox.sol



pragma solidity ^0.8.0;









contract CowBlindBox is ReentrancyGuard,NewCowRandom,Ownable {
    ICowActivity public cowAddress;
    IERC20 public cc;
    uint public gameStart;

    using Address for address;

    event BindboxCow(uint indexed tokenId,address indexed from,uint level);
    event RandomCow(uint indexed tokenId,address indexed from,uint level);

    constructor(ICowActivity _cowAddress,IERC20 _cc,uint _gameStart) {
        cowAddress=_cowAddress;
        cc=_cc;
        gameStart=_gameStart;
    }

    uint public randomCowPrice=559*10**18;
    uint[] public randomCowProbability=[uint(0),334,375,411,444,473,498,519,537,550,560,566,568,552,541,521,494,460,417,367,309,243,170,89,2];

    function adminRandomCowPrice(uint _randomCowPrice) external onlyOwner {
        randomCowPrice=_randomCowPrice;
    }

    function adminRandomCowProbability(uint i,uint _randomCowProbability) external onlyOwner {
        randomCowProbability[i]=_randomCowProbability;
    }

    function randomCow(address to) external nonReentrant {
        require(block.timestamp >= gameStart, "CowBlindBox: not start");
        NewCowLib.transferFrom(cc,msg.sender,address(this),randomCowPrice);
        uint randomX128=_random(keccak256(abi.encode(cowAddress,gameStart,to)));
        uint step=MathX128.mulUint(randomX128,10000);
        uint i;
        for(i=1;i<randomCowProbability.length;i++){
            if(step<randomCowProbability[i]){
                break;
            }
            step-=randomCowProbability[i];
        }
        require(i<randomCowProbability.length,"CowBlindBox: system error");
        uint level=i;
        uint tokenId=cowAddress.activityCow(level,to);

        emit RandomCow(tokenId,msg.sender,level);
    }

    uint[] public bindboxPrice=[uint(0),4900,24900,72900,171900];

    function adminBindboxPrice(uint i,uint _bindboxPrice) external onlyOwner {
        bindboxPrice[i]=_bindboxPrice;
    }

    uint[] public bindboxProbability=[uint(0),16,20,45,15,3,1];

    function adminBindboxProbability(uint i,uint _bindboxProbability) external onlyOwner {
        bindboxProbability[i]=_bindboxProbability;
    }

    function bindboxCow(address to,uint boxLevel) external nonReentrant {
        require(block.timestamp >= gameStart, "CowBlindBox: not start");
        require(boxLevel>=1 && boxLevel<=5, "CowBlindBox: invalid boxLevel");

        NewCowLib.transferFrom(cc,msg.sender,address(this),bindboxPrice[boxLevel]*10**16);
        
        uint startLevel=(boxLevel-1)*6+1;
        //uint endLevel=boxLevel*6;
        uint randomX128=_random(keccak256(abi.encode(cowAddress,gameStart,to)));
        uint step=MathX128.mulUint(randomX128,100);
        uint level=startLevel-1;
        uint i;
        for(i=1;i<bindboxProbability.length;i++){
            if(step<bindboxProbability[i]){
                break;
            }
            step-=bindboxProbability[i];
        }
        require(i<bindboxProbability.length,"CowBlindBox: system error");
        level=level+i;
        //uint level=MathX128.mulUint(randomX128,endLevel-startLevel+1)+startLevel;
        uint tokenId=cowAddress.activityCow(level,to);

        emit BindboxCow(tokenId,msg.sender,level);
    }

    function adminTransferCc(address to,uint amount) external onlyOwner {
        NewCowLib.transferFrom(cc,address(this),to,amount);
    }

    function adminGameStart(uint _gameStart) external onlyOwner {
        gameStart=_gameStart;
    }
}