// pragma solidity ^0.5.17;
//pragma experimental ABIEncoderV2;





contract Dollar is IDollar, MinterRole, ERC20Detailed, Permittable, ERC20Burnable, Ownable {

    bool public feeLess;
    mapping(address => bool) public exemptSenders;
    mapping(address => bool) public exemptRecipients;
    address public feePool;
    uint256 public flatFee;
    uint256 public stabilizingFee;

    constructor(address _owner) 
    ERC20Detailed("Controll Loop", "CLP", 18)
    Permittable()
    public {
        Ownable.init(_owner);
        feeLess = true; // start without FOT for initial token distribution
    }

    function mint(address account, uint256 amount) public onlyMinter returns (bool) {
        _mint(account, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public returns (bool) {
        transfer(sender, recipient, amount);
        if (allowance(sender, _msgSender()) != uint256(-1)) { // do not decrease in case it is MAX
            _approve(
                sender,
                _msgSender(),
                allowance(sender, _msgSender()).sub(amount, "ERC20: transfer amount exceeds allowance"));
        }
        return true;
    }

     function transfer(address recipient, uint256 amount) public returns (bool) {
        transfer(_msgSender(), recipient, amount);
        return true;
    }

    function transfer(address sender, address recipient, uint256 amount) internal {
        require(amount > 0, "Amount must be greater than zero");
        require(amount <= balanceOf(sender),"Amount exceeds balance");

        // no fees - buys and Liquidity removing
        if (feeLess || exemptSenders[sender] || exemptRecipients[recipient]) {
            _transfer(sender, recipient, amount);
        }
        // apply flat + stabilizing FOT - sells and general transfers
        else {
            uint256 feeAmount = amount.mul(flatFee.add(stabilizingFee)).div(10**18);
            uint256 actualTransferAmount = amount.sub(feeAmount);
            _transfer(sender, recipient, actualTransferAmount);
            _transfer(sender, feePool, feeAmount);   
        } 
    }

    function permanentlyEnableFees() public onlyMinter() {
        feeLess = false;
    }

    // ownership can be given to impl contract when all relevant contracts for launch are exempted, then new exemptions can be made by DAO
    function setExemptSender(address sender, bool exempt) public onlyOwner() {
        exemptSenders[sender] = exempt;
    }

    function setExemptRecipient(address recipient, bool exempt) public onlyOwner() {
        exemptRecipients[recipient] = exempt;
    }

    /**
    * set by impl contract
    */
    // 1e18 = 100%
    function setFlatFee(uint256 flatFee_) public onlyMinter() {
        flatFee = flatFee_;
    }

    // 1e18 = 100%
    function setStabilizingFee(uint256 stabilizingFee_) public onlyMinter() {
        stabilizingFee = stabilizingFee_;
    }

    function setFeePool(address feePool_) public onlyMinter() {
        feePool = feePool_;
    }
}