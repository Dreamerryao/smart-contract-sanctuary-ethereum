pragma solidity 0.7.4;




contract OracleLink is IOracle {
    
    using SafeMath for uint256;

    function getPrice(address, address, address extend, uint8 decimal) override external view returns (uint8, uint256) {
        uint256 price;
        AggregatorV3Interface oracle = AggregatorV3Interface(extend);
        uint8 _decimal = oracle.decimals();
        (, int256 answer,,,) = oracle.latestRoundData();
        if (decimal < _decimal) {
            price = uint256(answer).div(10 ** uint256(_decimal - decimal));
        }else if (decimal > _decimal) {
            price = uint256(answer).mul(10 ** uint256(decimal - _decimal));
        }else {
            price = uint256(answer);
        }
        return (1, price);
    }

}