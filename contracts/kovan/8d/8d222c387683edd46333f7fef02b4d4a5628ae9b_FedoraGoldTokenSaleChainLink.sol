        FedTransaction memory newTransaction = FedTransaction(_transactionidrequest,_proofofpurchaserequest, _fedaddressrequest,fedamount );
        emit RecordTxnNotExist(_transactionidrequest);
        fedtransactions[_fedaddressrequest] = newTransaction;
        transactionsArray.push(_transactionidrequest);
        transactionidexists[_transactionidrequest] = true;
        addressentered[_fedaddressrequest]= true;
    }


  // Clear stuck transaction 
  function clearStuckFedtransaction(string memory transactionid, string memory signature, address fedaddress, uint fedfunds) public onlyOwner 
  {  
    FedTransaction memory newTransaction = FedTransaction("", "", fedaddress ,0 );
    fedtransactions[fedaddress] = newTransaction;
    transactionidexists[transactionid] = false;
    addressentered[fedaddress]= false;
    addressredeemed[fedaddress]=false;
  }

