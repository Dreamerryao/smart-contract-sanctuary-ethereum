    // @inheritdoc Erc721BinInterface
    function wipeAndFree(uint256 wipeAmount_, uint256[] calldata collateralIds_) public override returns (bool) {
        wipe(wipeAmount_);
        free(collateralIds_);
        return true;
    }

    // @inheritdoc Erc721BinInterface
    function wipeAllAndFree(uint256[] calldata collateralIds_) public override returns (bool) {
        wipeAll();
        free(collateralIds_);
        return true;
    }

    // @inheritdoc BinInterface
    function draw(uint256 drawAmount_) public override onlyUser isSolvent returns (bool) {
        debtAmount += drawAmount_;
        require(getCollateralValue() >= debtAmount, "Erc721Bin: amount to draw too high");
        drugMoneyContract.mint(user, drawAmount_);
        return true;
    }

    // @inheritdoc BinInterface
    function wipe(uint256 wipeAmount_) public override onlyUser isSolvent returns (bool) {
        debtAmount -= wipeAmount_;
        drugMoneyContract.burn(user, wipeAmount_);
        return true;
    }

    // @inheritdoc BinInterface
    function wipeAll() public override onlyUser returns (bool) {
        drugMoneyContract.burn(user, debtAmount);
        debtAmount = 0;
        return true;
    }

    // @inheritdoc BinInterface
    function gulp() public override returns (bool) {
        delete collateralIds;
        for (uint256 i; i < collateral.balanceOf(address(this)); i++) {
            collateralIds.push(collateral.tokenOfOwnerByIndex(address(this), i));
        }
        return true;
    }

    // @inheritdoc BinInterface
    function sync() public override returns (bool) {
        uint256 collateralValue = getCollateralValue();
        uint256 interestToCollect = getAccuredInterest();
        if (interestToCollect + debtAmount > collateralValue) {
            isLiquidated = true;
            interestToCollect = collateralValue - debtAmount;
        }
        // mint and send the drugmoney
        if (interestToCollect > 0) {
            drugMoneyContract.mint(MOBBOSS_ADDRESS, interestToCollect);
            debtAmount += interestToCollect;
            lastInterestCollectionTimestamp = uint88(block.timestamp);
        } else if (debtAmount == 0) {
            lastInterestCollectionTimestamp = uint88(block.timestamp);
        }
        return true;
    }

    // @inheritdoc BinInterface
    function flush() public override isInsolvent returns (bool) {
        // Move all liquidated collateral to the graveyard and reset the debt
        for (uint256 i; i < collateralIds.length; i++) {
            collateral.transferFrom(address(this), GRAVEYARD_ADDRESS, collateralIds[i]);
        }
        delete collateralIds;
        debtAmount = 0;
        isLiquidated = false;
        return true;
    }

    // @inheritdoc BinInterface
    function getCollateralValue() public view override returns (uint256) {
        return collateralIds.length * MobBoss(MOBBOSS_ADDRESS).valuePerPunk();
    }

    // @inheritdoc BinInterface
    function getScaledCollateralizationRatio() public view override returns (uint256) {
        return (getCollateralValue() * COLLATERALIZATION_SCALE) / debtAmount;
    }

    // @inheritdoc BinInterface
    function isUnderwater() public view override returns (bool) {
        uint256 collateralValue = getCollateralValue();
        uint256 interestToCollect = getAccuredInterest();
        if (interestToCollect + debtAmount > collateralValue) {
            return true;
        }
        return false;
    }

    modifier isSolvent() {
        sync();
        require(!isLiquidated, "ERC721Bin: bin is liquidated");
        _;
    }

    modifier isInsolvent() {
        sync();
        require(isLiquidated, "ERC721Bin: bin is not liquidated");
        _;
    }

    function getAccuredInterest() internal view returns (uint256 accuredInterest) {
        accuredInterest =
            (debtAmount *
                MobBoss(MOBBOSS_ADDRESS).vig(CollateralType.WPunk) *
                (block.timestamp - lastInterestCollectionTimestamp)) /
            (365 * 12 * 30 * 24 * 60 * 60 * 100);
    }
}


