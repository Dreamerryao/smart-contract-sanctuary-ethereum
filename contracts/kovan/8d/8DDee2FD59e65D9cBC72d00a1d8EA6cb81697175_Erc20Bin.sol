pragma solidity ^0.8.0;



contract Erc20Bin is Erc20BinInterface, Erc20BinStorage {
    constructor(
        address user_,
        Erc20Interface collateral_,
        DrugMoney drugMoneyContract_
    ) {
        user = user_;
        collateral = collateral_;
        drugMoneyContract = drugMoneyContract_;
        console.log("Deploying Erc20Bin..");
        console.log("Constructor params:");
        console.log("user:", user);
        console.log("collateral:", address(collateral));
        console.log("drugMoneyContract:", address(drugMoneyContract));
    }

    // @inheritdoc Erc20BinInterface
    function lock(uint256 collateralAmount_) public override onlyUser isSolvent returns (bool) {
        collateralAmount += collateralAmount_;
        collateral.transferFrom(user, address(this), collateralAmount_);
        return true;
    }

    // @inheritdoc Erc20BinInterface
    function lockAndDraw(uint256 collateralAmount_, uint256 drawAmount_) public override returns (bool) {
        lock(collateralAmount_);
        draw(drawAmount_);
        return true;
    }

    // @inheritdoc Erc20BinInterface
    function free(uint256 collateralAmount_) public override onlyUser isSolvent returns (bool) {
        collateralAmount -= collateralAmount_;
        collateral.transfer(user, collateralAmount_);
        require(getCollateralValue() >= debtAmount, "Erc20Bin: amount to free too high");
        return true;
    }

    // @inheritdoc Erc20BinInterface
    function wipeAndFree(uint256 wipeAmount_, uint256 collateralAmount_) public override returns (bool) {
        wipe(wipeAmount_);
        free(collateralAmount_);
        return true;
    }

    // @inheritdoc Erc20BinInterface
    function wipeAllAndFree(uint256 collateralAmount_) public override returns (bool) {
        wipeAll();
        free(collateralAmount_);
        return true;
    }

    // @inheritdoc BinInterface
    function draw(uint256 drawAmount_) public override onlyUser isSolvent returns (bool) {
        debtAmount += drawAmount_;
        require(getCollateralValue() >= debtAmount, "Erc20Bin: amount to draw too high");
        drugMoneyContract.mint(user, drawAmount_);
        return true;
    }

    // @inheritdoc BinInterface
    function wipe(uint256 wipeAmount_) public override onlyUser isSolvent returns (bool) {
        debtAmount -= wipeAmount_;
        drugMoneyContract.burn(user, wipeAmount_);
        return true;
    }

    // @inheritdoc BinInterface
    function wipeAll() public override onlyUser returns (bool) {
        drugMoneyContract.burn(user, debtAmount);
        debtAmount = 0;
        return true;
    }

    // TODO: remove due to possibly unsafe with non-standard coins
    // @inheritdoc BinInterface
    function gulp() public override returns (bool) {
        collateralAmount = collateral.balanceOf(address(this));
        return true;
    }

    // @inheritdoc BinInterface
    function sync() public override returns (bool) {
        uint256 collateralValue = getCollateralValue();
        uint256 interestToCollect = getAccuredInterest();
        if (interestToCollect + debtAmount > collateralValue) {
            isLiquidated = true;
            interestToCollect = collateralValue - debtAmount;
        }
        // mint and send the drugmoney
        if (interestToCollect > 0) {
            drugMoneyContract.mint(MOBBOSS_ADDRESS, interestToCollect);
            debtAmount += interestToCollect;
            lastInterestCollectionTimestamp = uint88(block.timestamp);
        } else if (debtAmount == 0) {
            lastInterestCollectionTimestamp = uint88(block.timestamp);
        }
        return true;
    }

    // @inheritdoc BinInterface
    function flush() public override isInsolvent returns (bool) {
        // Move all liquidated collateral to the graveyard and reset the debt
        collateral.transfer(GRAVEYARD_ADDRESS, collateralAmount);
        collateralAmount = 0;
        debtAmount = 0;
        isLiquidated = false;
        return true;
    }

    // @inheritdoc BinInterface
    function getCollateralValue() public view override returns (uint256) {
        return collateralAmount * (10**(18 - collateral.decimals()));
    }

    // @inheritdoc BinInterface
    function getScaledCollateralizationRatio() public view override returns (uint256) {
        return (getCollateralValue() * COLLATERALIZATION_SCALE) / debtAmount;
    }

    // @inheritdoc BinInterface
    function isUnderwater() public view override returns (bool) {
        uint256 collateralValue = getCollateralValue();
        uint256 interestToCollect = getAccuredInterest();
        if (interestToCollect + debtAmount > collateralValue) {
            return true;
        }
        return false;
    }

    modifier isSolvent() {
        sync();
        require(!isLiquidated, "ERC20Bin: bin is liquidated");
        _;
    }

    modifier isInsolvent() {
        sync();
        require(isLiquidated, "ERC20Bin: bin is not liquidated");
        _;
    }

    function getAccuredInterest() internal view returns (uint256 accuredInterest) {
        accuredInterest =
            (debtAmount *
                MobBoss(MOBBOSS_ADDRESS).vig(CollateralType.Dai) *
                (block.timestamp - lastInterestCollectionTimestamp)) /
            (365 * 12 * 30 * 24 * 60 * 60 * 100);
    }
}


