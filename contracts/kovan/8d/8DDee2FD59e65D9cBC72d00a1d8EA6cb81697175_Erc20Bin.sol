pragma solidity ^0.8.0;







contract LoanShark is LoanSharkInterface, LoanSharkStorage, Ownable {
    constructor(
        WPunksInterface wPunksContract_,
        Erc20Interface daiContract_,
        DrugMoney drugMoneyContract_
    ) {
        wPunksContract = wPunksContract_;
        daiContract = daiContract_;
        drugMoneyContract = drugMoneyContract_;
        console.log("Deploying LoanShark..");
        console.log("Constructor params:");
        console.log("wPunksContract:", address(wPunksContract));
        console.log("daiContract:", address(daiContract));
        console.log("drugMoneyContract:", address(drugMoneyContract));
    }

    // @inheritdoc LoanShark
    function spawnBin(CollateralType collateralType) external override returns (BinInterface bin) {
        require(
            userBinIdByCollateralType[msg.sender][collateralType] == 0,
            "LoanShark: user already has a bin of specified type"
        );
        binCount += 1;
        // TODO: add bins to DrugMoney mint orchestration
        if (collateralType == CollateralType.WPunk) {
            bin = new Erc721Bin(msg.sender, wPunksContract, drugMoneyContract);
        } else {
            bin = new Erc20Bin(msg.sender, daiContract, drugMoneyContract);
        }
        userBinIdByCollateralType[msg.sender][collateralType] = binCount;
        bins[binCount] = bin;
    }

    // @inheritdoc LoanShark
    function flushBin(uint256 binId) external override returns (bool) {
        require(binId > 0 && binId <= binCount, "LoanShark: invalid bin id");
        bins[binId].flush();
        return true;
    }

    // @inheritdoc LoanShark
    function syncBin(uint256 binId) external override returns (bool) {
        require(binId > 0 && binId <= binCount, "LoanShark: invalid bin id");
        bins[binId].sync();
        return true;
    }
}