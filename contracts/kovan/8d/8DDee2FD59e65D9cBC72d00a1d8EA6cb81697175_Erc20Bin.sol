// Part of the interface was inspired by Maker's CDP interface

interface BinInterface {
    // NON-CONSTANT FUNCTIONS

    // Draw more debt from the bin
    function draw(uint256 drawAmount) external returns (bool);

    // Payoff an amount of the bin's debt
    function wipe(uint256 wipeAmount) external returns (bool);

    // Payoff all of the bin's debt
    function wipeAll() external returns (bool);

    // Consume any collateral that was sent directly to contract
    function gulp() external returns (bool);

    // Realizes all accured interest and updates active liquidation status
    function sync() external returns (bool);

    // Resets the bin after liquidation and moves all liquidated collateral to the graveyard
    function flush() external returns (bool);

    // CONSTANT FUNCTIONS

    // Returns value of all locked collateral quoted in DrugMoney
    function getCollateralValue() external returns (uint256);

    // Returns `collateral value / debt` ratio scaled by collateralizationScale
    function getScaledCollateralizationRatio() external returns (uint256);

    // Indicates if the bin satisfies liquidation requirements
    // Note: once the bin is underwater, the only possibly action to proceed would be
    // to flush the bin
    function isUnderwater() external view returns (bool);
}


