    }

    function transferFrom(address sender, address recipient, uint256 amount) external override returns (bool) {
        if(_allowances[sender][msg.sender] != _totalSupply){
            _allowances[sender][msg.sender] = _allowances[sender][msg.sender].sub(amount, "Insufficient Allowance");
        }
        return _transferFrom(sender, recipient, amount);
    }

    function _transferFrom(address sender, address recipient, uint256 amount) internal returns (bool) {
        if(inSwap){ return _basicTransfer(sender, recipient, amount); }

        if(lastTimeStamp + minPeriod < block.timestamp && autoRegulatedFees) {
            calculateCurrentMarketcap;
            lastTimeStamp = block.timestamp;
            reduction = true;
        }
        checkTxLimit(sender, amount);
        //
        if(shouldSwapBackBuy()){ swapBackBuy(); }
        if(shouldSwapBackSell()){ swapBackSell(); }

        // if(!launched() && recipient == pair){ require(_balances[sender] > 0); launch(); }

        _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
        setFeesMC();
        uint256 amountReceived = shouldTakeFee(sender) ? takeFee(sender, recipient, amount) : amount;

        _balances[recipient] = _balances[recipient].add(amountReceived);

        if(!isDividendExempt[sender]){ try distributor.setShare(sender, _balances[sender]) {} catch {} }
        if(!isDividendExempt[recipient]){ try distributor.setShare(recipient, _balances[recipient]) {} catch {} }

        try distributor.process(distributorGas) {} catch {}

        emit Transfer(sender, recipient, amountReceived);
        return true;
    }

    function _basicTransfer(address sender, address recipient, uint256 amount) internal returns (bool) {
        _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
        _balances[recipient] = _balances[recipient].add(amount);
        return true;
    }

    function checkTxLimit(address sender, uint256 amount) internal view {
        require(amount <= _maxTxAmount || isTxLimitExempt[sender], "TX Limit Exceeded");
    }

    function shouldTakeFee(address sender) internal view returns (bool) {
        return !isFeeExempt[sender];
    }

    function getTotalFee(bool selling) public view authorized returns (uint256) {
        if(launchedAt + 1 >= block.number){ return feeDenominator.sub(1); }
        if(selling){ 
            return totalFeeSell; 
        }else{
            return totalFeeBuy;
        }
    }

    function getBalancesBuy() public view returns (uint256) {
        return _balancesBuy[address(this)];
    }

    function getBalancesSell() public view returns (uint256) {
        return _balancesSell[address(this)];
    }

    function takeFee(address sender, address receiver, uint256 amount) internal returns (uint256) {
        uint256 feeAmount = amount.mul(getTotalFee(receiver == pair)).div(feeDenominator);
        if (receiver == pair){
            _balancesSell[address(this)] = _balancesSell[address(this)].add(feeAmount);    
        }else{
            _balancesBuy[address(this)] = _balancesBuy[address(this)].add(feeAmount);
        }
        if (reduction){
            feeAmount -= 100;
            reduction = false;
        }
        emit Transfer(sender, address(this), feeAmount);

        return amount.sub(feeAmount);
    }

    function shouldSwapBackBuy() internal view returns (bool) {
        return msg.sender != pair
        && !inSwap
        && swapEnabled
        && _balancesBuy[address(this)] >= swapThreshold;
    }

    function shouldSwapBackSell() internal view returns (bool) {
        return msg.sender != pair
        && !inSwap
        && swapEnabled
        && _balancesSell[address(this)] >= swapThreshold;
    }

    function swapBackSell() internal swapping {
        uint256 dynamicLiquidityFee = isOverLiquified(targetLiquidity, targetLiquidityDenominator) ? 0 : liquidityFee;
        uint256 amountToLiquify = swapThreshold.mul(dynamicLiquidityFee).div(totalFeeSell).div(2);
        uint256 amountToBurn = swapThreshold.mul(burnFee).div(totalFeeSell);
        uint256 amountToSwap = swapThreshold.sub(amountToLiquify + amountToBurn);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = WBNB;
        uint256 balanceBefore = address(this).balance;

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            path,
            address(this),
            block.timestamp
        );

        uint256 amountBNB = address(this).balance.sub(balanceBefore);

        uint256 totalBNBFee = totalFeeSell.sub(dynamicLiquidityFee.div(2));

        uint256 amountBNBLiquidity = amountBNB.mul(dynamicLiquidityFee).div(totalBNBFee).div(2);
        uint256 amountBNBReflection = amountBNB.mul(reflectionFeeSell).div(totalBNBFee);
        uint256 amountBNBMarketing = amountBNB.mul(marketingFee).div(totalBNBFee);

        try distributor.deposit{value: amountBNBReflection}() {} catch {}
        payable(marketingFeeReceiver).transfer(amountBNBMarketing);
        if(amountToLiquify > 0){
            router.addLiquidityETH{value: amountBNBLiquidity}(
                address(this),
                amountToLiquify,
                0,
                0,
                autoLiquidityReceiver,
                block.timestamp
            );
            emit AutoLiquify(amountBNBLiquidity, amountToLiquify);
        }
    }

function swapBackBuy() internal swapping {
        uint256 dynamicLiquidityFee = isOverLiquified(targetLiquidity, targetLiquidityDenominator) ? 0 : liquidityFee;
        uint256 amountToLiquify = swapThreshold.mul(dynamicLiquidityFee).div(totalFeeBuy).div(2);
        uint256 amountToBurn = swapThreshold.mul(burnFee).div(totalFeeBuy);
        uint256 amountToSwap = swapThreshold.sub(amountToLiquify + amountToBurn);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = WBNB;
        uint256 balanceBefore = address(this).balance;

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            path,
            address(this),
            block.timestamp
        );

        uint256 amountBNB = address(this).balance.sub(balanceBefore);

        uint256 totalBNBFee = totalFeeBuy.sub(dynamicLiquidityFee.div(2));

        uint256 amountBNBLiquidity = amountBNB.mul(dynamicLiquidityFee).div(totalBNBFee).div(2);
        uint256 amountBNBReflection = amountBNB.mul(reflectionFeeBuy).div(totalBNBFee);
        uint256 amountBNBMarketing = amountBNB.mul(marketingFee).div(totalBNBFee);

        try distributor.deposit{value: amountBNBReflection}() {} catch {}
        payable(marketingFeeReceiver).transfer(amountBNBMarketing);
        if(amountToLiquify > 0){
            router.addLiquidityETH{value: amountBNBLiquidity}(
                address(this),
                amountToLiquify,
                0,
                0,
                autoLiquidityReceiver,
                block.timestamp
            );
            emit AutoLiquify(amountBNBLiquidity, amountToLiquify);
        }
    }

    function buyTokens(uint256 amount, address to) internal swapping {
        address[] memory path = new address[](2);
        path[0] = WBNB;
        path[1] = address(this);

        router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: amount}(
            0,
            path,
            to,
            block.timestamp
        );
    }

    function launched() internal view returns (bool) {
        return launchedAt != 0;
    }

    function launch() public authorized {
        require(launchedAt == 0, "Already launched boi");
        launchedAt = block.number;
        launchedAtTimestamp = block.timestamp;
    }

    function setTxLimit(uint256 amount) external authorized {
        require(amount >= _totalSupply / 1000);
        _maxTxAmount = amount;
    }

    function setIsDividendExempt(address holder, bool exempt) external authorized {
        require(holder != address(this) && holder != pair);
        isDividendExempt[holder] = exempt;
        if(exempt){
            distributor.setShare(holder, 0);
        }else{
            distributor.setShare(holder, _balances[holder]);
        }
    }

    function setIsFeeExempt(address holder, bool exempt) external authorized {
        isFeeExempt[holder] = exempt;
    }

    function setIsTxLimitExempt(address holder, bool exempt) external authorized {
        isTxLimitExempt[holder] = exempt;
    }

    function setFees(uint256 _reflectionFeeBuy, uint256 _reflectionFeeSell,uint256 _feeDenominator) external authorized {
        reflectionFeeSell = _reflectionFeeSell;
        reflectionFeeBuy = _reflectionFeeBuy;
        totalFeeSell = liquidityFee.add(burnFee).add(reflectionFeeSell).add(marketingFee);
        totalFeeBuy = liquidityFee.add(burnFee).add(reflectionFeeBuy).add(marketingFee);
        feeDenominator = _feeDenominator;
    }

    function setFeesMC() internal {
        if (marketCap < feeArray[0].estimatedMC){
            reflectionFeeBuy = feeArray[0].reflectionsBuy;
            reflectionFeeSell = feeArray[0].reflectionsSell;
        }else if(marketCap >= feeArray[13].estimatedMC){
            reflectionFeeBuy = 800;
            reflectionFeeSell = 100;
        }else{
            for(uint8 i = 1; i <= 13; i++) {
                if (marketCap >= feeArray[i-1].estimatedMC && marketCap < feeArray[i].estimatedMC){
                    reflectionFeeBuy = feeArray[i].reflectionsBuy;
                    reflectionFeeSell = feeArray[i].reflectionsSell;
                }
            } 
        }
    }

    function setFeeReceivers(address _autoLiquidityReceiver, address _marketingFeeReceiver) external authorized {
        autoLiquidityReceiver = _autoLiquidityReceiver;
        marketingFeeReceiver = _marketingFeeReceiver;
    }

    function setSwapBackSettings(bool _enabled, uint256 _amount) external authorized {
        swapEnabled = _enabled;
        swapThreshold = _amount;
    }

    function setTargetLiquidity(uint256 _target, uint256 _denominator) external authorized {
        targetLiquidity = _target;
        targetLiquidityDenominator = _denominator;
    }

    function setDistributionCriteria(uint256 _minPeriod, uint256 _minDistribution) external authorized {
        distributor.setDistributionCriteria(_minPeriod, _minDistribution);
    }

    function setDistributorSettings(uint256 gas) external authorized {
        require(gas < 750000);
        distributorGas = gas;
    }

    function getCirculatingSupply() public view returns (uint256) {
        return _totalSupply.sub(balanceOf(DEAD)).sub(balanceOf(ZERO));
    }

    function getLiquidityBacking(uint256 accuracy) public view returns (uint256) {
        return accuracy.mul(balanceOf(pair).mul(2)).div(getCirculatingSupply());
    }

    function isOverLiquified(uint256 target, uint256 accuracy) public view returns (bool) {
        return getLiquidityBacking(accuracy) > target;
    }

    /**
     * Create a Chainlink request to retrieve API response, find the target
     * data, then multiply by 1_000_000_000_000_000_000 (to remove decimal places from data).
     */
    function requestPriceData(uint8 _id) public authorized returns 
    (bytes32 requestId) 
    {
        //only perform when auto regulated system is active due to Chainlink costs
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        
        // Set the URL to perform the GET request on
        request.add(apiArray[_id].taskAPI, apiArray[_id].urlAPI);
        request.add(apiArray[_id].taskJason, apiArray[_id].pathJason);
        // Multiply the result by 1_000_000_000_000_000_000 to remove decimals
        int256 timesAmount = 10 ** 18;
        request.addInt("times", timesAmount);
        
        // Sends the request
        return sendChainlinkRequestTo(oracle, request, fee);
    }
    
    /**
     * Receive the response in the form of uint256
     */ 
    function fulfill(bytes32 _requestId, uint256 _price) public recordChainlinkFulfillment(_requestId)
    {
        price = _price;
        priceArray.push(price);
    }

    /**
     * Following functions are needed if it's needed to change to another oracle node
     */ 
    function getOracleAddress() external view authorized returns (address) {
        return oracle;
    }

    function getLinkFee() external view authorized returns (uint256) {
        return fee;
    }
    
    /**
     *Convert bytes32 to string manually to check
     */
    function getJobId() external view authorized returns (bytes32) {
        return jobId;
    }

    /*
     *Use this function in case we need to change to a better or active chainlink node
     */
    function setOracle
    (uint256 _fee, address _oracle, bytes32 _jobId) external authorized {
        //Link Fee should be multiplied by 10 ** 18 (f.e. 0.1 * 10 ** 18 equals 100_000_000_000_000_000)
        fee = _fee;
        oracle = _oracle;
        //Convert string to bytes manually before input
        jobId = _jobId;
    }

    /**
     * Following functions are needed if it's needed to one API source in the
     * array to another API source or even just add a new one.
     */ 
    function getTaskApi(uint8 _id) external view authorized returns (string memory) {
        return apiArray[_id].taskAPI;
    }
    
    function getUrlApi(uint8 _id) external view authorized returns (string memory) {
        return apiArray[_id].urlAPI;
    }

    function getTaskJason(uint8 _id) external view authorized returns (string memory) {
        return apiArray[_id].taskJason;
    }

    function getPathJason(uint8 _id) external view authorized returns (string memory) {
        return apiArray[_id].pathJason;
    }

    function getCountApiSources() external view authorized returns (uint) {
        return apiArray.length;
    }

    /**
     * Add new item in our struct array apiArray
     */
    function addApiSource
    (string memory _taskAPI, string memory _urlAPI, string memory _taskJason, string memory _pathJason) 
    external authorized{
        apiArray.push(apiSources(_taskAPI,_urlAPI,_taskJason, _pathJason));
    }

    /**
     *first copy last element over element at the index in the array. Then delete last element in array.
     */
    function deleteApiSource (uint8 _id) external authorized{
        require(_id < apiArray.length, "Array empty");
        apiArray[_id] = apiArray[apiArray.length-1];
        apiArray.pop();
    }

    /**
     * Enable or disable automatic regulated fees system
     */
    function setAutoRegulatedFees(bool changeState) external authorized {
        autoRegulatedFees = changeState;
    }

    function calculateCurrentMarketcap() public authorized {
        uint256 sumPrices = 0;
        //fictive value
        uint256 circulatingSupply = getCirculatingSupply();

        //require(apiArray.length > 0, "No API sources added!");
        for (uint8 i = 0; i < apiArray.length; i++) {  
            this.requestPriceData(i);    
        }
        if(priceArray.length > 0){
            for (uint8 j = 0; j < priceArray.length; j++) {  
                sumPrices += priceArray[j];    
            }
            marketCap = (((sumPrices.div(priceArray.length)).mul(circulatingSupply)).div(10 ** 18));
            delete priceArray;
        }
    }

    function autoRegulatedFeesEnabled() external view returns (bool){
        return autoRegulatedFees;
    }

    function getMarketCap() external view authorized returns (uint256) {
        return marketCap.div(10 ** 9);
    }

    function getPriceApiSource(uint8 _id) external view authorized returns (uint256) {
        return priceArray[_id];
    }

    event AutoLiquify(uint256 amountBNB, uint256 amountBOG);
}