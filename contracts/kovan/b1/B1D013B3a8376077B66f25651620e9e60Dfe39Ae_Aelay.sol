pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./cTokenInterface.sol";
import "hardhat/console.sol";

contract Aelay{

    constructor(address _dai, address _cDai) {
        dai = IERC20(_dai);
        cDai = cTokenInterface(_cDai);
        owner = msg.sender;
    }

    IERC20 public dai;
    cTokenInterface public cDai;
    address public owner;

    // Struct of Tips
    struct TipLog {
        uint tipId;
        address recipient;
        uint timeOfTipping;
        uint depositedAmount;
        uint cDAIAmount;
        bool withdrawn;
    }

    TipLog[] public tips;

    mapping(address => mapping(address => bool)) public activeTip;
    mapping(address => uint256[]) public tipsOf;
    mapping(uint256 => address) public ownerOf;

    /*
    Deposit: The contract takes 100 DAI from the user, puts them into cDAI. The deposit also records who is the creator that the user is supporting.
    */
    function deposit(uint _amount, address _supportCreator) external {
        // require deposited _amount should not be zero
        require(_amount != 0,'Deposit amount cannot be zero');
        // Address cannot be Null
        require(_supportCreator != address(0), 'Address cannot be null');
        // Cannot deposit to self
        require(msg.sender != _supportCreator, 'No self Tips Allowed');
        // No active tips at the moment
        require(activeTip[msg.sender][_supportCreator] == false, 'You have current active tips');
        /*
        Taking amount in DAI from user, and put it into cDAI contract.
        For this we should call transferFrom function.
        */
        /*
        Before this, the wallet should call Approve function to the smart contract, in approving it (more on it in the article)
        */

        // Getting cDai Balance
        uint contractBalanceCDAI = cDai.balanceOf(address(this));

        dai.transferFrom(msg.sender, address(this), _amount);
        dai.approve(address(cDai),_amount);
        cDai.mint(_amount);

//        console.log('contractBalanceCDAI',contractBalanceCDAI);
        uint contractNewBalanceCDAI = cDai.balanceOf(address (this));
        uint cDAIBalanceRecipient = contractNewBalanceCDAI - contractBalanceCDAI;
//        console.log('cDAIBalanceRecipient',cDAIBalanceRecipient);

        // Adding Tips Information
        uint256 _index = tips.length;
        tipsOf[msg.sender].push(_index);
        tips.push(TipLog({tipId: _index,
                            recipient: _supportCreator,
                            timeOfTipping: block.timestamp,
                            depositedAmount: _amount,
                            cDAIAmount: cDAIBalanceRecipient,
                            withdrawn: false}));
        ownerOf[_index] = msg.sender;

        // Updating addressTipped Book
        activeTip[msg.sender][_supportCreator] = true;
    }

    function myTips(address _address) public view returns (TipLog[] memory s) {
        uint totalTips = tipsOf[_address].length;

        s = new TipLog[](totalTips);
        uint256[] memory indexes = new uint256[](totalTips);

        for (uint256 i = 0; i < totalTips; i++) {
            indexes[i] = tipsOf[msg.sender][i];
            s[i] = tips[indexes[i]];
        }
    }

    // Function to withdraw DAI if I liked content.
    function withdrawAllDais(uint _tipID) external onlyDepositor(_tipID) {
        /*
        The user queries the contract.
        The contract withdraws remaining cDAI from Compound, sends 100 DAI to the user, the rest to creator.
        As per my understanding this is done by USER, i.e. the one who will be providing the asset, i.e. DAI.
        */

        require(!tips[_tipID].withdrawn,'Already Withdrawn');

        // Getting cDAIs corresponding to tipID
        uint cDAI_amount = tips[_tipID].cDAIAmount;

        // Redeem the balance in the contract
        require(cDai.redeem(cDAI_amount) == 0, "something went wrong");
        uint DAIBalanceContract = dai.balanceOf(address(this));

        // Getting creator's address
        address _supportCreator = tips[_tipID].recipient;

        // Calculating how much DAIs supporter haves
        dai.transferFrom(address(this), msg.sender, dai.balanceOf(address(this)));
        uint balanceDeposited = tips[_tipID].depositedAmount;
        uint balanceToCreator = DAIBalanceContract - balanceDeposited;
        dai.transferFrom(msg.sender,_supportCreator,balanceToCreator);

        // Making withdraw True
        tips[_tipID].withdrawn = true;


        activeTip[msg.sender][_supportCreator] = false;
    }

//     Function to RageWithdraw leaving No interest to _support creator.
    function rageWithdrawAllDais(uint _tipID) external onlyDepositor(_tipID) {
        /*
        The user queries the contract.
        */
        require(!tips[_tipID].withdrawn,'Already Withdrawn');

        // Getting cDAIs corresponding to tipID
        uint cDAI_amount = tips[_tipID].cDAIAmount;

        // Redeem the balance in the contract
        require(cDai.redeem(cDAI_amount) == 0, "something went wrong");
        dai.transferFrom(address(this), msg.sender, dai.balanceOf(address(this)));

        // Making withdraw True
        tips[_tipID].withdrawn = true;

        // Getting creator's address
        address _supportCreator = tips[_tipID].recipient;
        activeTip[msg.sender][_supportCreator] = false;
    }

    /*
    This function initialized balance to Zero after withdraws
    */
    // Some extra functions for now...
    function getDaiBalance(address _address) external view returns(uint) {
        return dai.balanceOf(_address);
    }

    // Check the balance of the cDAI tokens, of the address.
    function getCErcTokenBalance(address _address) external view returns(uint) {
        return cDai.balanceOf(_address); // pass address(this), i.e. contract address.
    }

    modifier onlyDepositor(uint _tipID) {
        require(ownerOf[_tipID] == msg.sender, 'Invalid actions!');
        _;
    }
}

