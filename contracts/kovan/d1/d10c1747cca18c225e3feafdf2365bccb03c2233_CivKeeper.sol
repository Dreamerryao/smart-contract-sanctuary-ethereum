// File contracts/CivKeeper.sol

pragma solidity >=0.7.6;
pragma abicoder v2;
/**
 * @title Chainlink Keeper contract
 * @author Civilization civfund.org
 * @dev Triggers automated on-chain review of position status, to close when filled
 * Initializable
 * input ICivPositionMgr the contract address of the CivPositionManager proxy
 */
contract CivKeeper is
OwnableUpgradeable, 
PausableUpgradeable
{
	
    uint public interval; //number of blocks to wait after last performUpKeep
    uint public lastTimestamp; //time of last performUpKeep
	uint public _checkmin;
	uint public _checkmax;
	
    ICivPositionMgr public positionMgr;
		
	event PerformUpKeepClosed(uint256 _id);

	/**
	 * @dev Initialize the contract
	 * Can only be called once to set the initial status
	 * even during contract upgrades it will never be re-triggered again.
	 * __Ownable and pausable_init() initialize owner to deployer and pausable state to unpaused
	 * @param _newPositionMgr the contract address of the CivPositionManager proxy
	 */	
    function initialize(ICivPositionMgr _newPositionMgr) external initializer {
		positionMgr = _newPositionMgr;
		lastTimestamp = block.timestamp;
		interval = 5;
		__Ownable_init();
		__Pausable_init();
    }

	/**
	 * @dev Helper functions.
	 * Allow to properly operate the contract and frontend functionality
	 */
    function pause() external onlyOwner {_pause();}
    function unpause() external onlyOwner {_unpause();}

    function setPositionMgr(ICivPositionMgr _newPositionMgr) external onlyOwner 
		{positionMgr = _newPositionMgr;}
    function setInterval(uint _updateInterval) external onlyOwner 
		{interval = _updateInterval;}

    function getPrice(IUniswapV3Pool _pool) view internal returns (int24) 
		{return positionMgr.tick(_pool);}
	
	/**
	 * @dev Check Upkeep
	 * Chainlink default on-blockchain function call
	 * There are no logs at the moment for checkUpkeep since they are run 
	 * via the 'eth_call' method by the node operator directly. 
	 * They are running checkUpkeep at each block.
	 * Trigger sequence is: checkUpkeep(checkData)-->checkUpkeep()-->checkUp()
	 * The actual check happens in checkUp()
	 * param checkData is the input from the Chainlink registration, to pass details such as the max ID 
	 * or range of IDs for each particular instance e.g. uint256 maxID = abi.decode(checkData, (uint256));
	 * and e.g. if ([this ID < maxID]) then [do upkeep] else [ignore];
	 * When checkUpkeep returns upkeepNeeded == true, the Keeper node broadcasts a transaction 
	 * to the blockchain to execute the performUpkeep with performData as input
	 */
    function checkUpkeep(bytes calldata checkData)
		external returns (bool upkeepNeeded, bytes memory performData) 
	{
		require(address(positionMgr) != address(0), "ErrKeep01");

		uint256[1024] memory ids; //each position to be closed, up to 1024 at once
		uint256 idsLength; //number of positions to be closed
		
		uint256 lastCivTradeOrderID = positionMgr.lastCivTrade();
		(_checkmin, _checkmax) = abi.decode(checkData, (uint,uint));
		
		if(!paused() && (block.timestamp - lastTimestamp > interval)) {
			
			_checkmin = (lastCivTradeOrderID >= _checkmin) ? lastCivTradeOrderID : _checkmin;
			_checkmax = (lastCivTradeOrderID <= _checkmax) ? lastCivTradeOrderID : _checkmax;
			
			if(_checkmin > _checkmax) return (false,abi.encode(0));

			for(uint256 n= _checkmin; n < _checkmax ; ++n){
			
				if (positionMgr.isOpen(n) && triggered(n)) {
					ids[idsLength] = n;
					idsLength = idsLength + 1;
					if(idsLength == 1024) return (true, abi.encode(ids)); //to prevent overflow
				}
			}	
		}

		uint[] memory _idlist = new uint256[](idsLength);
		for(uint256 n=0; n<idsLength; ++n) _idlist[n] = ids[n];	

		if (idsLength > 0) 
			return (true, abi.encode(_idlist));
		else 
			return (false,abi.encode(0));
	}

	/**
	 * @dev Perform Upkeep
	 * Chainlink default on-blockchain function call
	 * @param performData The response from checkUpkeep passed to the performUpkeep function
	 * This allows to perform complex and costly simulations with no gas cost.
	 * Trigger sequence is: performUpkeep(performData)-->Chainlink
	 * Note: performUpkeep should change state such that checkUpkeep will not return true for the same subset of work 
	 * once completed first time. Otherwise the Upkeep will remain eligible and result in 
	 * multiple performances by the Keeper Network on the exactly same subset of work.
	 * NOTE: we shouldn't trust the input data: anyone can call this function, 
     * so the input is validated, as there is no guarantee that the data passed in 
	 * is the performData returned from checkUpkeep. This validation is done in triggered()
	 */
    function performUpkeep(bytes calldata performData) external whenNotPaused {
		lastTimestamp = block.timestamp;
		uint256[] memory _ids = abi.decode(performData, (uint256[]));

		for(uint256 n=0; n<_ids.length; ++n){
			uint _id = _ids[n];
			if(triggered(_id)) positionMgr.closePos(_id);
			emit PerformUpKeepClosed(_id);
		}
    }

	/**
	 * @dev Trigger checks
	 * Verify if the boundary conditions for the position to be closed have already been met.
	 */	
    function triggered(uint256 id) view internal returns (bool) {
	
		(IUniswapV3Pool pool,	
		 uint256 initialAmount0,
		 ,
		 int24 tickLower,
		 int24 tickUpper,
		 ICivPositionMgr.OrderState orderState
		) = positionMgr.orderInfo(id);
		
		if(orderState!=ICivPositionMgr.OrderState.OPEN) return false;

		int24 tick = getPrice(pool);

		if (initialAmount0 > 0) 
			return (tick > tickUpper);
		else
			return (tick < tickLower);
    }

}