// File: localhost/mint/tripartitePlatform/test/OracleTest.sol

 

pragma solidity 0.7.4;



contract OracleTest is IOracle {
    
    using SafeMath for uint256;

    mapping(address => mapping(address => bool)) public exists;
    mapping(address => mapping(address => uint256)) public prices;
    mapping(address => mapping(address => uint8)) public decimals;
    
    function getPrice(address quote, address base, address, uint8 decimal) override external view returns (uint8, uint256) {
        require(exists[quote][base], "publics:price_not_set");
        uint256 price = prices[quote][base];
        uint256 _decimal = decimals[quote][base];
        if (decimal < _decimal) {
            price = price.div(10 ** uint256(_decimal - decimal));
        }else if (_decimal < decimal){
            price = price.mul(10 ** uint256(decimal - _decimal));
        }
        return (1, price);
    }

    function setPrice(address quote, address base, uint256 price, uint8 decimal) external {
        exists[quote][base] = true;
        prices[quote][base] = price;
        decimals[quote][base] = decimal;
    }
    
}