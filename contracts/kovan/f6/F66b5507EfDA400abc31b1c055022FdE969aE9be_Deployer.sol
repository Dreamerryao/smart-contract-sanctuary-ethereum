    uint8 internal _decimals;
    constructor (
        address receiver_,
        uint256 totalSupply_,
        address lockupContract_,
        uint256 lockupAmount_,
        uint8 decimals_,
        string memory name_,
        string memory symbol_
    ) ERC20(name_, symbol_) {
        require(receiver_ != address(0), 'Receiver address can not be zero');
