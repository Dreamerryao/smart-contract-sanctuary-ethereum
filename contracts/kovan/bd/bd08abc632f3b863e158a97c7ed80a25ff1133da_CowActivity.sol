// File: Cow/activity/CowActivity.sol



pragma solidity ^0.8.0;










contract CowActivity is ReentrancyGuard,ERC721Holder,NewCowRandom,Ownable,EthereumMessageSign {
    ICowActivity public cowAddress;
    IERC20 public cc;
    uint public gameStart;
    mapping(address=>uint) public activityCows;
    address public signerAddress;
    uint public boxCowTotal;
    uint public freeCowTotal;
    uint public freeCowUsed;

    using Address for address;

    event BindboxCow(uint indexed tokenId,address indexed from,uint level);
    event FreeCow(uint indexed tokenId,address indexed from,uint level);

    modifier nonContract() {
        require(!msg.sender.isContract(),'msg sender cannot be contract');
        _;
    }

    constructor(ICowActivity _cowAddress,IERC20 _cc,address _signerAddress,uint _gameStart,uint _boxCowTotal,uint _freeCowTotal) {
        cowAddress=_cowAddress;
        freeCowTotal=_freeCowTotal;
        signerAddress=_signerAddress;
        cc=_cc;
        boxCowTotal=_boxCowTotal;
        gameStart=_gameStart;
    }

    uint[] public bindboxPrice=[uint(0),4100,12300,36900,108900];

    uint[] public boxUsed=[uint(0),0,0,0,0];

    function bindboxCow(address to,uint boxLevel) external nonReentrant nonContract {
        require(block.timestamp >= gameStart, "CowActivity: not start");
        require(boxLevel>=1 && boxLevel<=5, "CowActivity: invalid boxLevel");
        require(boxUsed[boxLevel]<boxCowTotal, "CowActivity: insufficient cow");

        NewCowLib.transferFrom(cc,msg.sender,address(this),bindboxPrice[boxLevel]*10**16);
        
        uint startLevel=(boxLevel-1)*6+1;
        //uint endLevel=boxLevel*6;
        uint randomX128=_random(keccak256(abi.encode(cowAddress,gameStart)));
        uint step=MathX128.mulUint(randomX128,100);
        uint level=startLevel;
        if(step<1){
            level=level+5;
        }else if(step<4){
            level=level+4;
        }else if(step<19){
            level=level+3;
        }else if(step<64){
            level=level+2;
        }else if(step<84){
            level=level+1;
        }
        //uint level=MathX128.mulUint(randomX128,endLevel-startLevel+1)+startLevel;
        uint tokenId=cowAddress.activityCow(level,to);

        boxUsed[boxLevel]=boxUsed[boxLevel]+1;
        emit BindboxCow(tokenId,msg.sender,level);
    }

    function freeCow(uint _deadline,bytes calldata signature) external nonReentrant nonContract {
        require(block.timestamp >= gameStart && block.timestamp <= _deadline, "CowActivity: expired");
        require(activityCows[msg.sender]==0,"CowActivity: must new user");
        require(freeCowUsed<freeCowTotal,"CowActivity: insufficient cow");
        require(check(signerAddress,hashData(msg.sender,_deadline),signature),"CowActivity: cannot rebot");
        freeCowUsed=freeCowUsed+1;
        uint randomX128=_random(keccak256(abi.encode(cowAddress,gameStart)));
        uint step=MathX128.mulUint(randomX128,20);
        uint level=1;
        if(step==0){
            level=5;
        }else if(step>=1&&step<=2){
            level=4;
        }else if(step>=3&&step<=6){
            level=3;
        }else if(step>=7&&step<=11){
            level=2;
        }
        uint tokenId=cowAddress.activityCow(level,msg.sender);
        activityCows[msg.sender]=tokenId;
        emit FreeCow(tokenId,msg.sender,level);
    }

    // function adminTransferCow(address to,uint tokenId) external onlyOwner {
    //     cowAddress.safeTransferFrom(address(this),to,tokenId);
    // }

    // function adminTransferEth(address payable to,uint amount) external onlyOwner {
    //     to.transfer(amount);
    // }

    function adminTransferCc(address to,uint amount) external onlyOwner {
        NewCowLib.transferFrom(cc,address(this),to,amount);
    }

    function adminSignerAddress(address _signerAddress) external onlyOwner {
        signerAddress=_signerAddress;
    }

    function adminGameStart(uint _gameStart) external onlyOwner {
        gameStart=_gameStart;
    }

    function adminFreeCowTotal(uint _freeCowTotal) external onlyOwner {
        freeCowTotal=_freeCowTotal;
    }

    function adminBoxCowTotal(uint _boxCowTotal) external onlyOwner {
        boxCowTotal=_boxCowTotal;
    }

    function hashData(address addr,uint _deadline) public pure returns (bytes32) {
        return getEthereumMessagePrefix(abi.encode(addr,_deadline));
    }
}