pragma solidity ^0.8.4;

import { ABDKMath64x64 as ABDK } from "../math/abdk.sol";

import "hardhat/console.sol";

struct HistoryArray {
  int256[] values;
  uint256[] timestamps;
  uint256 index;
  int32 normalPeriod;
}

library HistoryArrayLib {
  
  function init(HistoryArray storage self, int256[] memory values, uint32[] memory timestamps) internal {
    self.values = values;
    self.timestamps = timestamps;
    self.index = 0;
  }

  function add(HistoryArray storage self, int256 value, uint32 timestamp) internal {
    self.values[self.index] = value;
    self.timestamps[self.index] = timestamp;
    self.index = (self.index + 1) % self.values.length;
  }

  function getLast(HistoryArray storage self) internal view returns(int256) {
    return self.values[self.index];
  }

  function getHead(HistoryArray storage self) internal view returns(int256) {
    return self.values[(self.values.length + self.index - 1) % self.values.length];
  }

  function getHeadVelocity(HistoryArray storage self) internal view returns(int256) {
    if (self.values.length > 1) {
      int256 s0 = self.values[(self.values.length + self.index - 2) % self.values.length];
      int256 t0 = int256(self.timestamps[(self.values.length + self.index - 2) % self.values.length]);

      int256 s1 = self.values[(self.values.length + self.index - 1) % self.values.length];
      int256 t1 = int256(self.timestamps[(self.values.length + self.index - 1) % self.values.length]);

      return (s1 - s0) / (t1 - t0);
    } else {
      return 0;
    }
  }

  function getHeadVelAccNorm(HistoryArray storage self, int256 maxShock, int256 maxSurge) internal view returns(int128 velNorm, int128 accNorm, int128 shock, int128 surge) {
    if (self.values.length > 2) {
      (int256 s1, int128 v1, int128 v2, int128 acc) = _getReal(self);
      (velNorm, accNorm) = _getNormal(s1, v1, v2, acc, self.normalPeriod);
      if(v2 < 0) {
       shock =  ABDK.fromInt(1) + ABDK.div(ABDK.mul(ABDK.fromInt(maxShock), 7), v2);
      } else {
        shock = ABDK.fromInt(1);
      }
      if(acc < 0) {
        surge = ABDK.fromInt(1) + ABDK.div(ABDK.mul(ABDK.fromInt(maxSurge), 7), acc);
      } else {
        surge = ABDK.fromInt(1);
      }

    } else {
      return (0, 0, 0, 0);
    }
  }

  function _getReal(HistoryArray storage self) view internal returns (int256 s1, int128 v1, int128 v2, int128 acc) {
      uint256 length = self.values.length;
      int256 s0 = self.values[(2*length + self.index - 3) % length];
      int256 t0 = int256(self.timestamps[(2*length + self.index - 3) % length]);

      s1 = self.values[(length + self.index - 2) % length];
      int256 t1 = int256(self.timestamps[(length + self.index - 2) % length]);

      int256 s2 = self.values[(length + self.index - 1) % length];
      int256 t2 = int256(self.timestamps[(length + self.index - 1) % length]);

      v1 = ABDK.divi(s1 - s0, t1 - t0);
      v2 = ABDK.divi(s2 - s1, t2 - t1);

      acc = ABDK.div(ABDK.sub(v2, v1), ABDK.fromInt(t2 - t1));
  }

  function _getNormal(int256 s1, int128 v1, int128 v2, int128 acc, int32 normalPeriod) pure internal returns (int128 velNorm, int128 accNorm)
  {
      if(v2 > 0) {
        velNorm = ABDK.div(ABDK.mul(v2, ABDK.fromInt(normalPeriod)), ABDK.fromInt(s1));
        velNorm = ABDK.sub(ABDK.fromInt(1), velNorm);
        if (velNorm < 0) {
          velNorm = 0;
        }
      } else if(v2 < 0) {
        velNorm = ABDK.div(ABDK.mul(-v2, ABDK.fromInt(normalPeriod)), ABDK.fromInt(s1));
        velNorm = ABDK.fromInt(1) + ABDK.mul(velNorm, ABDK.fromInt(7));
        if (ABDK.toInt(velNorm) >= 8) {
          velNorm = ABDK.fromInt(8);
        }
      } else {
        velNorm = ABDK.fromInt(1);
      }

      if(acc > 0) {
        accNorm = ABDK.div(ABDK.mul(acc, ABDK.fromInt(normalPeriod)), v1);
        accNorm = ABDK.sub(ABDK.fromInt(1), accNorm);
        if (accNorm < 0) {
          accNorm = 0;
        }
      } else if(acc < 0) {
        accNorm = ABDK.div(ABDK.mul(-acc, ABDK.fromInt(normalPeriod)), v1);
        accNorm = ABDK.fromInt(1) + ABDK.mul(accNorm, ABDK.fromInt(7));
        if (ABDK.toInt(accNorm) >= 8) {
          accNorm = ABDK.fromInt(8);
        }
      } else {
        accNorm = ABDK.fromInt(1);
      }
  }

  function getAll(HistoryArray storage self) internal view returns (int256[] memory result) {
    result = new int256[](self.values.length);
    for (uint i = 0; i < self.values.length; ++i) {
      result[i] = self.values[(self.index + i) % self.values.length];
    }
  }

  function getAllVelocity(HistoryArray storage self) internal view returns (int128[] memory result) {
    if (self.values.length > 1) {
      result = new int128[](self.values.length - 1);

      for (uint i = 1; i < self.values.length; ++i) {
        int256 v0 = self.values[(self.index + i - 1) % self.values.length];
        int256 t0 = int256(self.timestamps[(self.index + i - 1) % self.values.length]);

        int256 v1 = self.values[(self.index + i) % self.values.length];
        int256 t1 = int256(self.timestamps[(self.index + i) % self.values.length]);

        result[i - 1] = ABDK.divi(v1 - v0, t1 - t0);
      }
    } else {
      result = new int128[](0);
    }
  }

  function getAllAcc(HistoryArray storage self) internal view returns (int128[] memory result) {
    if (self.values.length > 2) {
      int128[] memory velocity = getAllVelocity(self);
      result = new int128[](self.values.length - 2);

      for (uint i = 1; i < self.values.length - 1; ++i) {
        int256 t0 = int256(self.timestamps[(self.index + i) % self.values.length]);
        int256 t1 = int256(self.timestamps[(self.index + i + 1) % self.values.length]);

        result[i - 1] = ABDK.div(velocity[i] - velocity[i - 1], ABDK.fromInt(t1 - t0));
      }
    } else {
      result = new int128[](0);
    }
  }

}

