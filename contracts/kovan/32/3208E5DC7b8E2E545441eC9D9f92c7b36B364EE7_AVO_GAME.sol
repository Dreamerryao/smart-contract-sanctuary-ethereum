// File: contracts/GamePool.sol



pragma solidity ^0.8.4;






contract AVO_GAME is Ownable, ERC1155Holder {

    IERC20 private AvoToken;   //ERC20
    ERC1155 private Game_Coin;     //ERC1155

    //ERC20
    uint256 depositable = 100;
    uint256 public _minSwapAmount = 1000;
    uint256 _Tokendecimal = 10 ** 18;

    //CoinID
    uint256 CoinID = 0;   //set as per standard use in 1155
    uint256 txCoin = 1000;


    uint256 winner_amount = 1000; //hardcoded
    uint256 levelone = 500;   //hardcoded
    uint256 UpgradeWeaponCost = 1000;   //hardcoded

    constructor(address _AvoToken, address _Coin) {
        AvoToken = IERC20(_AvoToken);
        Game_Coin = ERC1155(_Coin);
    }

    bool public paused = false;

    struct GamePool {
        address _user;
        uint256 _totalDeposit;
    }
    GamePool[] public poolrec;

    event Deposit(address indexed from, address indexed to, uint256 amount_in);
    event swap(address indexed from, uint256 amount_in , uint256 amount_out);
    event Lp(address indexed from, address indexed to, uint256 amount_out);

    // event play();

    mapping(address => uint256) public _coinBalance;  //show on frontend
    // mapping(address => uint256) public _coinLocked;   

    //takes automatic 100 ERC20 when user click on deposit
    function Token_Deposit() public {

        require(!paused,"Pool is Current Paused!!");

        require(Game_Coin.balanceOf(address(this),CoinID) > 0 ,"Treasure is Empty!!!");

        uint256 _amount = depositable * _Tokendecimal;

        require(AvoToken.allowance(msg.sender,address(this)) >= _amount,"Check the Allowance!!!");

        require(AvoToken.balanceOf(msg.sender) >= _amount,"InSufficient Balance for Transaction!!");

        AvoToken.transferFrom(msg.sender,address(this),_amount);

        Game_Coin.safeTransferFrom(address(this), msg.sender, CoinID , txCoin , '');

        update_Pool(msg.sender,_amount);

        _coinBalance[msg.sender] = txCoin;

        emit Deposit(msg.sender,address(this),_amount);

        emit Lp(address(this),msg.sender,txCoin);

    }
    
    //update and make new record for depositers.
    function update_Pool(address _person , uint256 _token) internal returns (bool update){
        for ( uint i = 0;  i < poolrec.length ; i++ ){
            if(poolrec[i]._user == _person ){
                poolrec[i]._totalDeposit += _token;
                return true;
            }
        }
        GamePool memory newupdate = GamePool(_person,_token);
        poolrec.push(newupdate);
        return true;
    }

    //return true on 500 deposit in contract

    function Play() public returns (bool _start) {   //for level one values

        require(!paused,"Pool is Current Paused!!");

        require(Game_Coin.isApprovedForAll(msg.sender, address(this)),"Need to Set Approval First!!");
        
        Game_Coin.safeTransferFrom(msg.sender, address(this), CoinID , levelone , '');

        _coinBalance[msg.sender] -= levelone;        

        return true;

    }

    function update_Weapons(uint _pid) public returns (uint _id , bool _purchased)  {
        
        require(!paused,"Pool is Current Paused!!");

        if(_pid == 1) {   //let say its a new gun
            require(Game_Coin.isApprovedForAll(msg.sender, address(this)),"Need to Set Approval First!!");
            Game_Coin.safeTransferFrom(msg.sender, address(this), CoinID , UpgradeWeaponCost , '');
            _coinBalance[msg.sender] -= UpgradeWeaponCost;
            return (_pid, true);
        }
        else{
            require(false,"Invalid ID!!");
        }
    }


    //just to transfer win_coins to 
    function transfer_coin(address _person) external onlyOwner {

        require(!paused,"Pool is Current Paused!!");
        Game_Coin.safeTransferFrom(address(this), _person , CoinID , winner_amount , '');
        _coinBalance[_person] += winner_amount;

    }

    //need to put swap per second block to avoid bot swaps

    function SwapCoin(uint _CoinAmount) public {
        
        require(!paused,"Pool is Current Paused!!");

        require(AvoToken.balanceOf(address(this)) > 0,"Pool Token Balance is InSufficient for Transaction!!");

        require(Game_Coin.isApprovedForAll(msg.sender, address(this)),"Need to Set Approval First!!");

        require(Game_Coin.balanceOf(msg.sender, CoinID) >= _minSwapAmount, "InSufficient Balance of Coin to Swap!!");

        require(_CoinAmount >= _minSwapAmount,"Need more Coins to Perform this task!!");

        bool mod = ((_CoinAmount % 10) == 0 ) ? true : false;

        require(mod,"Invalid Amount to swap!!");

        Game_Coin.safeTransferFrom(msg.sender, address(this), CoinID , _CoinAmount , '');

        _coinBalance[msg.sender] -= _CoinAmount;

        uint256 tx_token =  ( _CoinAmount / 10 ) * _Tokendecimal;

        AvoToken.transfer(msg.sender, tx_token);

        emit swap(msg.sender,_CoinAmount,tx_token);

    }

    ///////////////////////// setter Functions /////////////////////////

    function setPauser(bool _value) external onlyOwner {
        paused = _value;
    }

    function set_Games_Coins(uint _win , uint _levelone, uint _upgrade) external onlyOwner {
        winner_amount = _win; 
        levelone = _levelone;
        UpgradeWeaponCost = _upgrade;
    }
    
    //use Token ID only here
    function setCoinId(uint _value) external onlyOwner{
        CoinID = _value;
    }

    function setSwapAmount(uint _value) external onlyOwner{
        _minSwapAmount = _value;
    }    

    function setDepositableAmount(uint _value) external onlyOwner{
        depositable = _value;
    }

    //incase you do any mistake while adding address
    function setAddress(address _AvoToken, address _Coin) external onlyOwner {
        AvoToken = IERC20(_AvoToken);
        Game_Coin = ERC1155(_Coin);
    }

    ///////////////////////// Balance Checker /////////////////////////

    function Pool_Balance(uint _pid, uint _tokenId) public view returns (uint _balance) {

        if(_pid == 0){
            return AvoToken.balanceOf(address(this));
        }

        else if(_pid == 1 && _tokenId == CoinID) {
            return Game_Coin.balanceOf(address(this),CoinID);
        }
        else {
            require(false,"ID NOT VALID");
        }

    }

    ///////////////////////// Withdrawable by Owner Only /////////////////////////

    //use this function, in case of emergency only. It withdraw all the Token into caller account.  //only callable by Owner
    function EmergencyWithdraw() external onlyOwner{
        AvoToken.transfer(msg.sender, AvoToken.balanceOf(address(this)));
    }

    function withdraw_Coin() public onlyOwner {
        Game_Coin.safeTransferFrom(address(this), msg.sender, CoinID , Game_Coin.balanceOf(address(this), CoinID), '');
    }



}