    function StartDayLottery() public  {
        require(DayJackpotStarted == 0, "Day Lottery Has Already Started");
        require(WeekJackpotStarted == 0, "Week Lottery Is In Progress");
        require(block.timestamp > NextDayJackpotTime);
        PreviousRandomNumberDay = randomResult;
        DayJackpotStarted = 1;
        ( TotalEntriesDay, DayPrize) = EnterDayParticipants();
        getRandomNumber(block.timestamp);
    }
    function EndDayLottery() public {
        require(DayJackpotStarted == 1, "Day Lottery Hasn't Been Started");
        require(WeekJackpotStarted == 0, "Week Lottery Is In Progress");
        require(PreviousRandomNumberDay != randomResult, "Please Wait For Chainlink To Provide The Random Number");
        WinningNumberDay = randomResult % TotalEntriesDay + 1;
        uint256 RollingTotal = 0;
            
        for (uint i=0; i<DayJackpotParticipants.length; i++) {
            uint256 EntryBalance = DayJackpotBalances[i];
            if(RollingTotal < WinningNumberDay && WinningNumberDay <= RollingTotal + EntryBalance){
                  WinningWalletDay = DayJackpotParticipants[i];
            }
            RollingTotal += EntryBalance; 
        }
        _burn(DayPot, DayPrize);
        _mint(WinningWalletDay, DayPrize);
            
        LastDayJackpotTime = block.timestamp;
        NextDayJackpotTime = LastDayJackpotTime + DayJackpotFreq;
        DayJackpotStarted = 0;
    }
    function CurrentDayPotSize() public view returns(uint256){
        return balanceOf(DayPot);
    }    
    
    function StartWeekLottery() public {
        require(WeekJackpotStarted == 0, "Week Lottery Has Already Started");
        require(DayJackpotStarted == 0, "Day Lottery Is In Progress");
        require(block.timestamp > NextWeekJackpotTime);
        PreviousRandomNumberWeek = randomResult;
        WeekJackpotStarted = 1;
        _pause();
        ( TotalEntriesWeek, WeekPrize) = EnterWeekParticipants();
        _unpause();
        getRandomNumber(block.timestamp);
    }
    function EndWeekLottery() public {
        require(WeekJackpotStarted == 1, "Week Lottery Hasn't Been Started");
        require(DayJackpotStarted == 0, "Day Lottery Is In Progress");
        require(PreviousRandomNumberWeek != randomResult, "Please Wait For Chainlink To Provide The Random Number");
        WinningNumberWeek = randomResult % TotalEntriesWeek + 1;
        uint256 RollingTotal = 0;
            
        for (uint i=0; i<WeekJackpotParticipants.length; i++) {
            uint256 EntryBalance = WeekJackpotBalances[i];
            if(RollingTotal < WinningNumberWeek && WinningNumberWeek <= RollingTotal + EntryBalance){
                  WinningWalletWeek = WeekJackpotParticipants[i];
            }
            RollingTotal += EntryBalance; 
        }
        _mint(WinningWalletWeek, RollingTotal/100);
        LastWeekJackpotTime = block.timestamp;
        NextWeekJackpotTime = LastWeekJackpotTime + WeekJackpotFreq;
        WeekJackpotStarted = 0;

    }
    function CurrentWeekPotSize() public view returns(uint256){
        return balanceOf(WeekPot);
    }    
    
    function ShowDevWallet() public view virtual returns (address){
        return DevWallet;
    }            
    function ShowDayPotWallet() public view virtual returns (address){
        return DayPot;
    }     
    
    function CurrentTime() public view virtual returns (uint){
        return block.timestamp;
    }     
    
    function ChangeLotteryFreq(uint256 DayFreq, uint256 WeekFreq) public onlyOwner {
        DayJackpotFreq = DayFreq;
        WeekJackpotFreq = WeekFreq;
    }
    
    
     function getRandomNumber(uint256 userProvidedSeed) internal returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        return requestRandomness(keyHash, fee, userProvidedSeed);
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        randomResult = randomness;
    }

    function withdrawLink() public onlyOwner {
        require(LINK.transfer(msg.sender, LINK.balanceOf(address(this))), "Unable to transfer");
    }   
    
    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        whenNotPaused
        override(ERC20Upgradeable, ERC20SnapshotUpgradeable)
    {
        super._beforeTokenTransfer(from, to, amount);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
}