        uint256[] ids;
        uint256[] values;
    }

    mapping (address => Snapshots) private _accountBalanceSnapshots;
    Snapshots private _totalSupplySnapshots;

    
    mapping (address => bool) private _wallets;  
    
    address[]  AddressList;
    
    address[]  DayJackpotParticipants;
    uint256[]  DayJackpotBalances;  

    address[]  WeekJackpotParticipants;
    uint256[]  WeekJackpotBalances;    
    
    CountersUpgradeable.Counter private _currentSnapshotId;

    event Snapshot(uint256 id);

    function _snapshot() internal virtual returns (uint256) {
        _currentSnapshotId.increment();

        uint256 currentId = _currentSnapshotId.current();
        emit Snapshot(currentId);
        return currentId;
    }


    function balanceOfAt(address account, uint256 snapshotId) internal view virtual returns (uint256) {
        (bool snapshotted, uint256 value) = _valueAt(snapshotId, _accountBalanceSnapshots[account]);

        return snapshotted ? value : balanceOf(account);
    }

    function totalSupplyAt(uint256 snapshotId) internal view virtual returns(uint256) {
        (bool snapshotted, uint256 value) = _valueAt(snapshotId, _totalSupplySnapshots);

        return snapshotted ? value : totalSupply();
    }


    function _beforeTokenTransfer(address from, address to, uint256 amount) internal virtual override {
      super._beforeTokenTransfer(from, to, amount);

      if (from == address(0)) {
        // mint
        _updateAccountSnapshot(to);
        _updateTotalSupplySnapshot();
        AddAddress(to);
      } else if (to == address(0)) {
        // burn
        _updateAccountSnapshot(from);
        _updateTotalSupplySnapshot();
      } else {
        // transfer
        _updateAccountSnapshot(from);
        _updateAccountSnapshot(to);
        //AddAddress(from);
        AddAddress(to);
      }
    }

    function AddAddress(address wallet) internal {
        if (_wallets[wallet] == false && wallet != DayPot && wallet != WeekPot && wallet != DevWallet && wallet.isContract() == false){
            AddressList.push(wallet);
            _wallets[wallet] = true;
        }
    }
    function CurrentAddressList() public view virtual returns (address[] memory){
        return AddressList;
    }
    
    function EnterDayParticipants() internal virtual returns(uint256, uint256) {
        _snapshot();
        delete DayJackpotBalances;
        delete DayJackpotParticipants;
        uint256 RollingTotal = 0;
        for (uint i=0; i<AddressList.length; i++) {
           uint256 CurrentUserBalance = balanceOfAt(AddressList[i],_currentSnapshotId.current());
           if (CurrentUserBalance != 0){
                DayJackpotParticipants.push(AddressList[i]);
                DayJackpotBalances.push(CurrentUserBalance);
                RollingTotal += CurrentUserBalance;
            } else {
                _wallets[AddressList[i]] = false;
            }
        }
        uint256 daypotBalance = balanceOfAt(DayPot,_currentSnapshotId.current());
        AddressList = DayJackpotParticipants;
        return (RollingTotal, daypotBalance);
    }
        function LastDayJackpotBalances() public view virtual returns (uint256[] memory){
        return DayJackpotBalances;
    }
    
    function LastDayJackpotParticipants() public view virtual returns (address[] memory){
        return DayJackpotParticipants;
    }        


    function EnterWeekParticipants() internal virtual returns(uint256, uint256) {
        _snapshot();
        delete WeekJackpotBalances;
        delete WeekJackpotParticipants;
        uint256 RollingTotal = 0;
        for (uint i=0; i<AddressList.length; i++) {
           uint256 CurrentUserBalance = balanceOfAt(AddressList[i],_currentSnapshotId.current());
           if (CurrentUserBalance != 0){
                WeekJackpotParticipants.push(AddressList[i]);
                _burn(AddressList[i],CurrentUserBalance/100);
                WeekJackpotBalances.push(CurrentUserBalance);
                RollingTotal += CurrentUserBalance;
            } else {
                _wallets[AddressList[i]] = false;
            }
        }
        uint256 weekpotBalance = RollingTotal/100;
        AddressList = WeekJackpotParticipants;
        return (RollingTotal, weekpotBalance);
    }    
    function LastWeekJackpotBalances() public view virtual returns (uint256[] memory){
        return WeekJackpotBalances;
    }
    
    function LastWeekJackpotParticipants() public view virtual returns (address[] memory){
        return WeekJackpotParticipants;
    }        


    function _valueAt(uint256 snapshotId, Snapshots storage snapshots)
        private view returns (bool, uint256)
    {
        require(snapshotId > 0, "ERC20Snapshot: id is 0");
        require(snapshotId <= _currentSnapshotId.current(), "ERC20Snapshot: nonexistent id");

        uint256 index = snapshots.ids.findUpperBound(snapshotId);

        if (index == snapshots.ids.length) {
            return (false, 0);
        } else {
            return (true, snapshots.values[index]);
        }
    }

    function _updateAccountSnapshot(address account) private {
        _updateSnapshot(_accountBalanceSnapshots[account], balanceOf(account));
    }

    function _updateTotalSupplySnapshot() private {
        _updateSnapshot(_totalSupplySnapshots, totalSupply());
    }

    function _updateSnapshot(Snapshots storage snapshots, uint256 currentValue) private {
        uint256 currentId = _currentSnapshotId.current();
        if (_lastSnapshotId(snapshots.ids) < currentId) {
            snapshots.ids.push(currentId);
            snapshots.values.push(currentValue);
        }
    }

    function _lastSnapshotId(uint256[] storage ids) private view returns (uint256) {
        if (ids.length == 0) {
            return 0;
        } else {
            return ids[ids.length - 1];
        }
    }
    uint256[46] private __gap;
}


abstract contract ERC20BurnableUpgradeable is Initializable, ContextUpgradeable, ERC20Upgradeable {
    function __ERC20Burnable_init() internal initializer {
        __Context_init_unchained();
        __ERC20Burnable_init_unchained();
    }

    function __ERC20Burnable_init_unchained() internal initializer {
    }

    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    function burnFrom(address account, uint256 amount) public virtual {
        uint256 currentAllowance = allowance(account, _msgSender());
        require(currentAllowance >= amount, "ERC20: burn amount exceeds allowance");
        _approve(account, _msgSender(), currentAllowance - amount);
        _burn(account, amount);
    }
    uint256[50] private __gap;
}


