import "hardhat/console.sol";
import "./IPoolERC20.sol";
import "./libraries/SafeMath.sol";
import "./libraries/TransferHelper.sol";
import "./CXSupportContracts/ISupportContract.sol";
import "./PoolFactory.sol";
import "@chainlink/contracts/src/v0.8/interfaces/FeedRegistryInterface.sol";
import "@chainlink/contracts/src/v0.8/Denominations.sol";

interface IPool {
    function investWithETH() external payable returns (uint amountPoolToken);
    function investWithToken(address tokenAddress, uint amount) external returns (uint amountPoolToken);
    function withdraw(uint poolTokenAmount) external returns (uint[] memory tokenAmounts);
}

interface IETFPool is IPool{
    function initialize(string calldata _poolName, string calldata _poolTokenName, address _creator, address[] calldata _supportInvestToken) external;
    function swapToken(address swapContract,uint256 amountIn,uint256 amountOutMin,address[] calldata path,address to, uint256 deadline) external returns (uint[] memory amounts);
    function total_usd_value() external view returns (uint usd_value);
}

contract ETFPool is IETFPool, IPoolERC20 {
    using SafeMath for uint;
    address public factory;
    address public creator;
    string public poolName;
    uint public poolType = 0;
    address[] public supportInvestToken;
//    address public registry_address = address();
    mapping(address => uint) public token_balance;
    uint public eth_balance;
    address[] public token_list;

    constructor() public {
        factory = msg.sender;
    }

    modifier onlyCreator() {
        require(creator == msg.sender, "Only creator can call this method");
        _;
    }

    modifier checkSwapContract(address swap_contract){
        bool contract_in_support = false;
        uint length = PoolFactory(factory).supportContractLength();
        for(uint i=0; i<length;i++){
            if(swap_contract == PoolFactory(factory).support_contracts(i)){
                contract_in_support = true;
                break;
            }
        }
        require(contract_in_support, "contract not support");
        _;
    }

    modifier requirePriceFixed(address token_address){
        FeedRegistryInterface registry = FeedRegistryInterface(PoolFactory(factory).oracle_registry_address());
        require(registry.getCurrentPhaseId(token_address, Denominations.USD)!=0, 'token price not fixed');
        _;
    }

    modifier inSupportInvestToken(address token){
        bool inSupport = false;
        for (uint i=0; i<supportInvestToken.length; i++){
            if(token == supportInvestToken[i]){
                inSupport = true;
            }
        }
        require(inSupport, "token not support");
        _;
    }

    function initialize(string calldata _poolName, string calldata _poolTokenName, address _creator, address[] calldata _supportInvestToken) override external {
        creator = _creator;
        console.log('CXPool _poolName %s _poolTokenName %s _creator %s', _poolName, _poolTokenName, _creator);
        require(msg.sender == factory, 'CX: FORBIDDEN');
        poolName = _poolName;
        supportInvestToken = _supportInvestToken;
        IPoolERC20(address(this)).initialize(_poolName, _poolTokenName);
    }

    function supportInvestTokenLength() external view returns (uint){
        return supportInvestToken.length;
    }

    function investWithETH() override external payable returns (uint amountPoolToken){
        uint before_invest_value = total_usd_value();
        // 计数
        eth_balance = eth_balance + msg.value;
        uint invest_eth_value = msg.value * getEthUsdPrice();
        console.log('invest_eth_value', invest_eth_value);
        console.log('totalSupply', totalSupply);
        if (totalSupply == 0) {
            amountPoolToken = invest_eth_value;
        } else {
            amountPoolToken = totalSupply * invest_eth_value / before_invest_value;
        }
        _mint(msg.sender, amountPoolToken);
    }

    function investWithToken(address tokenAddress, uint amount) override external inSupportInvestToken(tokenAddress) returns (uint amountPoolToken){
        uint before_invest_value = total_usd_value();
        console.log('investWithToken tokenAddress', tokenAddress, 'amount', amount);
        TransferHelper.safeTransferFrom(tokenAddress, msg.sender, address(this), amount);
        // 计数
        uint balance = token_balance[tokenAddress];
        console.log('balance', balance);
        if (balance != 0) {
            token_balance[tokenAddress] = balance.add(amount);
        } else {
            token_balance[tokenAddress] = amount;
            token_list.push(tokenAddress);
        }
        uint token_value = amount * getTokenUsdPrice(tokenAddress);
        console.log('token_value', token_value);
        if (totalSupply == 0) {
            amountPoolToken = token_value;
        } else {
            amountPoolToken = totalSupply * token_value / before_invest_value;
        }
        console.log('amountPoolToken', amountPoolToken);
        _mint(msg.sender, amountPoolToken);
    }

    function total_usd_value() override public view returns (uint usd_value){
        uint eth_balance = address(this).balance;
        uint eth_value = eth_balance.mul(getEthUsdPrice());
        console.log('eth_value', eth_value);
        usd_value = eth_value;
        for (uint i = 0; i < token_list.length; i++) {
            address token_address = token_list[i];
            uint token_value = token_balance[token_address] * getTokenUsdPrice(token_address);
            console.log('token_address',token_address, 'token_value',token_value);
            usd_value = usd_value + token_value;
        }
        console.log('usd_value', usd_value);
    }

    function swapToken(address swapContract,
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline) override external onlyCreator checkSwapContract(swapContract) requirePriceFixed(path[path.length-1]) returns (uint[] memory amounts){
        require(token_balance[path[0]]>=amountIn);
        uint path_length = path.length;
        //TODO 优化approve
        TransferHelper.safeApprove(path[0],swapContract,amountIn);
        amounts = ISupportContract(swapContract).swapExactTokensForTokens(amountIn, amountOutMin, path, to, deadline);
        token_balance[path[0]] = token_balance[path[0]] - amountIn;
        token_balance[path[path_length-1]] = token_balance[path[path_length-1]] + amounts[path_length-1];
    }

//    function call(address target, bytes data, uint value, address newToken) override external payable checkContract(target) returns(bool){
//        return target.call.value(value)(data);
//    }

    function withdraw(uint poolTokenAmount) override external returns (uint[] memory tokenAmounts){
        //        transferFrom(msg.sender,address(this),poolTokenAmount);
        tokenAmounts = new uint[](token_list.length + 1);
        uint ethAmount = poolTokenAmount * eth_balance / totalSupply;
        tokenAmounts[0] = ethAmount;
        payable(msg.sender).transfer(ethAmount);
        eth_balance = eth_balance - ethAmount;
        for (uint i = 0; i < token_list.length; i++) {
            uint tokenAmount = poolTokenAmount * token_balance[token_list[i]] / totalSupply;
            tokenAmounts[i + 1] = tokenAmount;
            TransferHelper.safeTransfer(token_list[i], msg.sender, tokenAmount);
            token_balance[token_list[i]] = token_balance[token_list[i]] - tokenAmount;
        }
        _burn(msg.sender, poolTokenAmount);
        return tokenAmounts;
    }

    //TODO 做成library?
    function getTokenUsdPrice(address base) public view returns (uint) {
        address registry_address = PoolFactory(factory).oracle_registry_address();
        FeedRegistryInterface registry = FeedRegistryInterface(registry_address);
        (uint80 roundID,int price,uint startedAt,uint timeStamp,uint80 answeredInRound) = registry.latestRoundData(base, Denominations.USD);
        return uint (price);
    }

    function getEthUsdPrice() public view returns (uint) {
        address registry_address = PoolFactory(factory).oracle_registry_address();
        FeedRegistryInterface registry = FeedRegistryInterface(registry_address);
        (uint80 roundID,int price,uint startedAt,uint timeStamp,uint80 answeredInRound) = registry.latestRoundData(Denominations.ETH, Denominations.USD);
        return uint(price);
    }


}

