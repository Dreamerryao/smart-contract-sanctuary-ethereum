    function AddHolder(address holder) public {
        if(!IsHolder(holder) && holder != address(0) && holder != DayPot && holder != DevWallet && !holder.isContract()){
            _nextHolder[holder] = _nextHolder[GUARD];
            _nextHolder[GUARD] = holder;
            HoldersNum++;
        }
    }
    
    function RemoveHolder(address holder) public {
        if(IsHolder(holder)){
            address prevHolder = GetPrevHolder(holder);
            _nextHolder[prevHolder] = _nextHolder[holder];
            _nextHolder[holder] = address(0);
            HoldersNum--;
    }
    }
    
    function GetPrevHolder(address holder) internal view returns(address){
        address currentAddress = GUARD;
        while(_nextHolder[currentAddress] != GUARD){
            if(_nextHolder[currentAddress] == holder){
                return currentAddress;
            }
        currentAddress = _nextHolder[currentAddress];
        }
        return address(0);
    }
    
    function GetHolders() public view returns (address[] memory){
        address[] memory holders = new address[](HoldersNum);
        address currentAddress = _nextHolder[GUARD];
        for(uint256 i = 0; currentAddress != GUARD; ++i){
            holders[i] = currentAddress;
            currentAddress = _nextHolder[currentAddress];
        }
        return holders;
    }
    uint256 public RemainingAmount;
    uint256 public x; 
    function _beforeTokenTransfer(address from, address to, uint256 amount) internal virtual {
    
    if(to.isContract()){ExchangeSupply  =ExchangeSupply + amount;}
    if(from.isContract()){ExchangeSupply =ExchangeSupply - amount;}
    
    if (from == address(0)) {
      AddHolder(to);
      
    } else if (to == address(0)) {
      if (balanceOf(from) - amount == 0){RemoveHolder(from);}
      
    } else {
      AddHolder(to);
      if (balanceOf(from) - amount == 0){RemoveHolder(from);}
    }
    }
    uint256[45] private __gap;
}



abstract contract ERC20BurnableUpgradeable is Initializable, ContextUpgradeable, ERC20Upgradeable {
    function __ERC20Burnable_init() internal initializer {
        __Context_init_unchained();
        __ERC20Burnable_init_unchained();
    }

    function __ERC20Burnable_init_unchained() internal initializer {
    }

    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    function burnFrom(address account, uint256 amount) public virtual {
        uint256 currentAllowance = allowance(account, _msgSender());
        require(currentAllowance >= amount, "ERC20: burn amount exceeds allowance");
        _approve(account, _msgSender(), currentAllowance - amount);
        _burn(account, amount);
    }
    uint256[50] private __gap;
}


