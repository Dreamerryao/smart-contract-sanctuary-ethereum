contract FeePool is IFeePool, Ownable {
    using SafeMath for uint256;

    bytes32 private constant FILE = "FeePool";
    
    uint256 private constant CALLER_REWARD_LIMIT = 5e20; // $500
    IDollar public dollar;
    address public pairTokenAddress;
    address public daoAddress;
    IUniswapV2Router02 public constant uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    address public constant UNISWAP_FACTORY = address(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);


    event provided(
        address indexed pairTokenAddress,
        uint256 tokensSwapped,
        uint256 pairTokenReceived,
        uint256 tokensIntoLiqudity
    );

    event providedFromRemaining(
        address indexed pairTokenAddress,
        uint256 tokensIntoLiqudity,
        uint256 pairTokensIntoLiqudity
    );

    constructor(address owner, IDollar dollar_, address pairTokenAddress_, address daoAddress_)  public {
        Ownable.init(owner);
        dollar = dollar_;
        pairTokenAddress = pairTokenAddress_;
        daoAddress = daoAddress_;
    }

    function burn(uint256 amount) public onlyOwner {
        dollar.burn(amount);
    }

    function sendLiquidityProviderTokensToDao() public {
        address pair = UniswapV2Library.pairFor(UNISWAP_FACTORY, address(dollar), pairTokenAddress);
        uint256 balance = IUniswapV2Pair(pair).balanceOf(address(this));
        IUniswapV2Pair(pair).transfer(daoAddress, balance);
    }

    function provideLiquidityFromRemaining(uint256 amountDollar, uint256 amountPairToken, uint256 callerReward) public onlyOwner {
        require(amountDollar > 1, "FeePool: amountDollar needs to be gt 1"); 
        require(amountPairToken > 1, "FeePool: amountPairToken needs to be gt 1"); 
        require(amountDollar.add(callerReward) <= dollar.balanceOf(address(this)), "FeePool: amountDollar + callerReward exceeds contract's balance");
        require(amountPairToken <= IERC20(pairTokenAddress).balanceOf(address(this)), "FeePool: amountPairToken exceeds contract's balance");
        require(callerReward <= CALLER_REWARD_LIMIT, "FeePool: given callerReward exceeds limit");

        addLiquidityForTokens(pairTokenAddress, amountDollar, amountPairToken); // add liquidity to uniswap
        dollar.transfer(tx.origin, callerReward); // reward original caller
        emit providedFromRemaining(pairTokenAddress, amountDollar, amountPairToken);
    }

    function provideLiquidity(uint256 amount, uint256 callerReward) public onlyOwner {
        require(amount > 1, "FeePool: amount needs to be gt 1"); // so that half won't be 0
        require(amount.add(callerReward) <= dollar.balanceOf(address(this)), "FeePool: amount exceeds contract's balance");
        require(callerReward <= CALLER_REWARD_LIMIT, "FeePool: given callerReward exceeds limit");
                
        uint256 half = amount.div(2);
        uint256 biggerHalf = amount.sub(half);
                
        // swap tokens for pairToken
        uint256 initialPairTokenBalance = IERC20(pairTokenAddress).balanceOf(address(this));
        swapTokensForTokens(pairTokenAddress, half);
        uint256 newPairTokenBalance = IERC20(pairTokenAddress).balanceOf(address(this)).sub(initialPairTokenBalance);

        addLiquidityForTokens(pairTokenAddress, biggerHalf, newPairTokenBalance); // add liquidity to uniswap
        dollar.transfer(tx.origin, callerReward); // reward original caller
        emit provided(pairTokenAddress, half, newPairTokenBalance, biggerHalf);
    }

    function swapTokensForTokens(address pairTokenAddress_, uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(dollar);
        path[1] = pairTokenAddress_;

        dollar.approve(address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of pair token
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidityForTokens(address pairTokenAddress_, uint256 tokenAmount, uint256 pairTokenAmount) private {
        // approve token transfer to cover all possible scenarios
        dollar.approve(address(uniswapV2Router), tokenAmount);
        IERC20(pairTokenAddress_).approve(address(uniswapV2Router), pairTokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidity(
            address(dollar),
            pairTokenAddress_,
            tokenAmount,
            pairTokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp
        );
    }
}