contract Random {
    bytes32 private _nonces;

    using Address for address;

    function _random(bytes32 salt) internal returns(uint res) {
        require(!Address.isContract(msg.sender),'Random: msg sender cannot be contract');
        bytes32 _nonce;
        _nonce=keccak256(abi.encode(
            block.timestamp,
            block.coinbase,
            msg.sender,
            keccak256(msg.data),
            tx.origin,
            tx.gasprice,
            _nonces,
            salt
        ));
        res=(uint)(_nonce);
        res=res>>128;
        _nonces=_nonce;
    }
}
