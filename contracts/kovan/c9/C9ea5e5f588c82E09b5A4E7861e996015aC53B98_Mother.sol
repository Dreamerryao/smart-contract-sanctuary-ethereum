pragma solidity ^0.8.10;

import "hardhat/console.sol";
import "@openzeppelin/contracts/governance/extensions/GovernorCountingSimple.sol";

interface ERC20 {
    function approve(address, uint256) external returns (bool);

    function transfer(address, uint256) external returns (bool);

    function transferFrom(address, address, uint256) external returns (bool);

    function balanceOf(address) external view returns (uint);

}


interface iLendingPool {
    function deposit(
        address asset,
        uint256 amount,
        address onBehalfOf,
        uint16 referralCode
    ) external;

    function withdraw(
        address asset,
        uint256 amount,
        address to
    ) external returns (uint256);
}

contract Mother is GovernorCountingSimple {
    string public symbol = "MOTHER_DAO";
    address public owner;

    //These two addresses need to be changed to each network (testnet/mainnet) address
    //Maybe we can add a list of accepted tokens, and users can propose and vote on accepted tokens
    address public underlyingAddress; // kovan = "0xFf795577d9AC8bD7D90Ee22b6C1703490b6512FD";
    address public lendingPoolAddress; // kovan = "0x580D4Fdc4BF8f9b5ae2fb9225D584fED4AD5375c";
    address public yieldTokenAddress; // kovan = "0x58AD4cB396411B691A9AAb6F74545b2C5217FE6a"

    // total balance of the DAO
    // uint256 totalBalance;

    // struct for a DAO member
    struct Member {
        uint256 id;
        address wallet;
    }

    struct Proposal {
        uint256 id;
        address creator;
        address newMember;
        string description;
        uint256 votesFor;
        uint256 votesAgainst;
        ProposalState status;
        uint256 timestamp;
    }

    uint256 public proposalCount;
    uint256 public memberCount;

    // Record of all DAO members
    mapping(address => Member) public members;
    address[] public memberAddresses;

    // Record of all DAO members
    mapping(uint256 => Proposal) public proposals;
    uint256[] public proposalIds;

    constructor(address _underlying, address _lendingPool, address _yieldToken) Governor("Mother") {
        console.log("Starting DAO contract");

        // initial member
        members[msg.sender] = Member({
            id: 1,
            wallet: msg.sender
        });
        memberCount++;

        owner = msg.sender;
        underlyingAddress = address(_underlying);
        lendingPoolAddress = address(_lendingPool);
        yieldTokenAddress = address(_yieldToken);
    }

    function getAllProposalIds() external view returns (uint256[] memory) {
        return proposalIds;
    }

    function getAllMemberAddresses() external view returns (address[] memory) {
        return memberAddresses;
    }

    function getTotalBalance() external view returns (uint256) {
        // Create a reference to the corresponding cToken contract, like cDAI
        ERC20 aToken = ERC20(yieldTokenAddress);


        uint256 balance = aToken.balanceOf(address(this));

        return balance;
    }

    function votingDelay() public pure override returns (uint256) {
        return 0;
    }

    // Duration of a voting proposal
    // Calculation:
    // 1 day = 86400 seconds
    // eth block duration ~ 15s
    // 86400 / 15 = 5760 block per day
    // 2 days = 11520 blocks
    function votingPeriod() public pure override returns (uint256) {
        return 1;
    }

    function proposalDeadline(uint256 proposalId) public view override(Governor) returns (uint256) {
        return super.proposalDeadline(proposalId);
    }

    /** BEGIN - OpenZeppelin function overrides */

    // Minimum number of cast voted required for a proposal to be successful
    function quorum(uint256)
        public
        pure
        override(IGovernor)
        returns (uint256)
    {
        // calculate quorum, will be 50% of all members?
        return 1;
    }

    // Amount of votes already cast passes the threshold limit.
    function getQuorumReached(uint256 proposalId)
        public
        view
        returns (bool)
    {
        return super._quorumReached(proposalId);
    }

    // If the proposal is successfull or not
    function isProposalSucceeded(uint256 proposalId)
        public
        view
        returns (bool)
    {
        return super._voteSucceeded(proposalId);
    }

    // Cast a vote and emits a VoteCast event
    function castVote(uint256 proposalId, uint8 support) public override returns (uint256) {
        return super.castVote(proposalId, support);
    }

    // Get all proposal votes: for, against and abstains
    function proposalVotes(uint256 proposalId) public view override returns (uint256 againstVotes, uint256 forVotes, uint256 abstainVotes) {
        return super.proposalVotes(proposalId);
    }

    // Returns wether an account has voted on a proposalId
    function hasVoted(uint256 proposalId, address account) public view override(GovernorCountingSimple) returns (bool) {
        return super.hasVoted(proposalId, account);
    }

    // Current state of a proposal: Pending, Active, Canceled, Succeeded, Expired, Executed
    function getState(uint256 proposalId) public view returns (string memory) {
        ProposalState temp = super.state(proposalId);

        if (temp == ProposalState.Active) return "Active";
        if (temp == ProposalState.Succeeded) return "Succeeded";
        if (temp == ProposalState.Canceled) return "Canceled";
        if (temp == ProposalState.Defeated) return "Defeated";
        if (temp == ProposalState.Executed) return "Executed";
        if (temp == ProposalState.Expired) return "Expired";
        if (temp == ProposalState.Pending) return "Pending";
        if (temp == ProposalState.Queued) return "Queued";
        return "";
    }

    function proposalSnapshot(uint256 proposalId) public view override returns (uint256) {
        return super.proposalSnapshot(proposalId);
    }

    // Create a new proposal
    function createProposal(address[] memory targets, address newMember, bytes[] memory calldatas, uint256[] memory values, string memory description)
        public
        returns (uint256)
    {
        uint256 proposalId = super.propose(targets, values, calldatas, description);

        proposalCount++;
        Proposal memory newProposal = Proposal({
            id: proposalId,
            creator: msg.sender,
            newMember: newMember,
            description: description,
            votesFor: 0,
            votesAgainst: 0,
            status: ProposalState.Pending,
            timestamp: block.timestamp
        });

        proposalIds.push(proposalId);
        proposals[newProposal.id] = newProposal;
        return proposalId;
    }

    function addUser(address newMember) view external {
        console.log('adduser => ', newMember);
    }

    function executeProposal(address addedMember, address[] memory targets, uint256[] memory values, bytes[] memory calldatas, bytes32 descriptionHash)
        public
        returns (uint256)
    {
        memberCount++;
        Member memory newMember = Member({
            id: memberCount,
            wallet: addedMember
        });

        memberAddresses.push(addedMember);
        members[addedMember] = newMember;

        return super.execute(targets, values, calldatas, descriptionHash);
    }

    modifier isDAOMember {
        require(members[msg.sender].id > 0, "Only DAO members can call this function");
        _;
    }

    modifier onlyOwner {
        require(msg.sender == owner, "Only the contract owner can call this function");
        _;
    }

    // Cancel a proposal
    function _cancel(address[] memory targets, uint256[] memory values, bytes[] memory calldatas, bytes32 descriptionHash)
        internal
        override(Governor)
        returns (uint256)
    {
        return super._cancel(targets, values, calldatas, descriptionHash);
    }

    function getVotes(address, uint256)
        public
        pure
        override(IGovernor)
        returns (uint256)
    {
        return 28;
    }

    /** END - OpenZeppelin function overrides */

    function deposit(uint256 amount) external {
        console.log(
            "Depositing from %s the amount of %s",
            msg.sender,
            amount
        );


        // Create a reference to the underlying asset contract, like DAI.
        ERC20 underlyingToken = ERC20(underlyingAddress);

        // Create a reference to the lending pool
        iLendingPool lendingPool = iLendingPool(lendingPoolAddress);

        // Transfer the user's token to this contract
        underlyingToken.transferFrom(msg.sender, address(this), amount);


        // Approve transfer on the ERC20 contract to the lending pool contract (so it can move the funds to its pool)
        underlyingToken.approve(lendingPoolAddress, amount);

        // Deposit the asset on the lending pool
        lendingPool.deposit(underlyingAddress, amount, address(this), 0);

    }

    function withdraw() external onlyOwner {

        // Create a reference to the lending pool
        iLendingPool lendingPool = iLendingPool(lendingPoolAddress);

        uint256 balance = this.getTotalBalance();

        // pick recipient
        // ToDo: Change this to a randomly select someone from the members
        address recipient = members[owner].wallet;

        //Transfer the cToken to the winning user
        //cToken.approve(recipient, balance);
        lendingPool.withdraw(underlyingAddress, balance, recipient);

    }
}

