import "hardhat/console.sol";

//Interface to interact with imported ERC-20 token code
interface GBEconomicCurrency {
    function totalSupply() external view returns (uint256);

    function rebase(uint256 epoch, int256 supplyDelta) external returns (uint256);
}

interface IOracle {
    function getData() external returns (uint256);
}

contract CurrencyRebasing is Ownable {
    using SafeMath for uint256;
    using SafeMathInt for int256;
    using UInt256Lib for uint256;

    GBEconomicCurrency public GBECToken;

    //CPI oracle
    IOracle public economicFactors;

    //MarketOracle
    IOracle public oracleDisplayingCurrentTokenPrice;

    uint256 private initialEconomicFactorWeighting;

    uint256 private constant DEC = 18;

    //Max Rate
    uint256 private maxRebaseAmount = 10**6 * 10**18;

    //Max Supply
    uint256 private constant maxRebaseSupply =
        115792089237316195423570985008687907853269984665640564039457584007913129639935;

    address public orchestrator;

    modifier onlyOrchestrator() {
        require(msg.sender == orchestrator);
        _;
    }

    function setOracleDisplayingCurrentTokenPrice(IOracle oracleCurrentTokenPrice) external {
        oracleDisplayingCurrentTokenPrice = oracleCurrentTokenPrice;
    }

    function setOracleDisplayingEconomicFactors(IOracle economicFactor) external {
        economicFactors = economicFactor;
    }

    function setOrchestrator(address orch) external {
        orchestrator = orch;
    }

    function initialize(
        address owner,
        GBEconomicCurrency token,
        uint256 initialEconomicWeighting
    ) public initializer {
        Ownable.initialize(owner);
    }

    function rebaseTokenSupply() external {
        console.log("Starting rebasing..");

        uint256 currentTokenPrice;

        //Get current price of token to calculate the rebasing amount from the goal
        currentTokenPrice = oracleDisplayingCurrentTokenPrice.getData();

        if (currentTokenPrice > maxRebaseAmount) {
            currentTokenPrice = maxRebaseAmount;
        }

        //Get calculated economic market factors
        uint256 economicFactorPrice;

        economicFactorPrice = economicFactors.getData();

        //Calcualte the target price of token
        uint256 tokenTargetPrice;
        tokenTargetPrice = economicFactorPrice.mul(10**DEC).div(initialEconomicFactorWeighting);

        int256 safeTokenTargetPrice;
        safeTokenTargetPrice = tokenTargetPrice.toInt256Safe();

        int256 totalPriceAdjustment;

        totalPriceAdjustment = GBECToken
            .totalSupply()
            .toInt256Safe()
            .mul(currentTokenPrice.toInt256Safe().sub(safeTokenTargetPrice))
            .div(safeTokenTargetPrice);

        if (
            totalPriceAdjustment > 0 &&
            GBECToken.totalSupply().add(uint256(totalPriceAdjustment)) > maxRebaseSupply
        ) {
            totalPriceAdjustment = (maxRebaseSupply.sub(GBECToken.totalSupply())).toInt256Safe();
        }

        uint256 tokenSupplyAfterRebasing = GBECToken.rebase(1, totalPriceAdjustment);
    }
}

