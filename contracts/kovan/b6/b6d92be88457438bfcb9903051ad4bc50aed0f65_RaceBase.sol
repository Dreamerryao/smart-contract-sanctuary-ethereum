library SORT{
    
    function quickSort(uint256[5] memory data) internal pure {
        if (data.length > 1) {
            _quickPart(data, 0, data.length - 1);
        }
    }

    function _quickPart(uint256[5] memory data, uint low, uint high) private pure {
        if (low < high) {
            uint pivotVal = data[(low + high) / 2];
        
            uint low1 = low;
            uint high1 = high;
            for (;;) {
                while (data[low1] < pivotVal) low1++;
                while (data[high1] > pivotVal) high1--;
                if (low1 >= high1) break;
                (data[low1], data[high1]) = (data[high1], data[low1]);
                low1++;
                high1--;
            }
            if (low < high1) _quickPart(data, low, high1);
            high1++;
            if (high1 < high) _quickPart(data, high1, high);
        }
    }

}


