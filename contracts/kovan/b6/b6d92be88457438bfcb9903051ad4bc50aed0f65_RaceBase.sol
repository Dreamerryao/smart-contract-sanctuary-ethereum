pragma solidity ^0.8.0;


contract IRace {

    uint256 public constant RaceNumber = 5;
    

    enum RaceStatus { start, racing , complete }

    enum RaceLevelType {Griffin, Class5, Class4, Class3, Class2, Class1}

    enum RaceDistance {d1000, d1200, d1400, d1600, d1800, d2000, d2200, d2400}

    enum RaceDistanceType {short, middle, long}


    struct Race {
        RaceLevelType raceLevel;
        uint256[RaceNumber] horseIds;
        uint256 horseNumber;
        uint256 startedAt;
        uint256 createdAt;
        uint256 price;
        RaceStatus status;
        uint256[RaceNumber] resultArray;
        uint256[RaceNumber] speedArray;
        string eventName;
        string location;
        RaceDistance distance;
    }
}


