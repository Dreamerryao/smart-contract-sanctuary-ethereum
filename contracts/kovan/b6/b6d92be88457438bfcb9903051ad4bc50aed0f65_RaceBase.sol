            _setHorseRaceResult(race.horseIds[index], index, race.raceLevel);
        }

        uint256 totalPrize = race.price * RaceNumber;
        {
            address owner = nonFungibleContract.ownerOf(race.resultArray[0]);
            _transferReward(owner, totalPrize.mul(50).div(100));
        }
        {
            address owner = nonFungibleContract.ownerOf(race.resultArray[1]);
            _transferReward(owner, totalPrize.mul(30).div(100));
        }
        {
            address owner = nonFungibleContract.ownerOf(race.resultArray[2]);
            _transferReward(owner, totalPrize.mul(10).div(100));
        }
        {
            _transferReward(dev, totalPrize.mul(10).div(100));
        }

        emit RaceComplete(raceId);
    }


    function _setHorseRaceResult(uint256 _horseId, uint256 _position, RaceLevelType _raceLevel) internal {
        
        HorseRace storage _horseRace =  tokenIdToHorseRace[_horseId];

        uint256 points = _horseRace.points;
        if(!_horseRace.raced){
            IHorse.Horse memory _horse = nonFungibleContract.getHorse(_horseId);
            uint256 _genotype = _horse.genotype;
            if(_genotype >= 1 && _genotype <= 4){
                points = 57;
            }
            else if(_genotype >= 5 && _genotype <= 9){
                points = 37;
            }
            else{
                points = 17;
            }
        }

        if(_position == 0){
            points = points + 4;
        }
        else if(_position == 1){
            points = points + 3;
        }
        else if(_position == 2){
            points = points + 2;
        }
        else if(_position == 3){
            points = points + 1;
        }
        else if(_position >= 4 && _position <= 7){
            points = points + 0;
        }
        else if(_position >= 8 && _position <= 11){
            uint256 diff = _position - 7;
            uint256 horseRaceLevel = uint256(getHorseRaceLevel(_horseId));
            if(horseRaceLevel < uint256(_raceLevel)){
                diff = 0;
            }
            if(points >= diff){
                points = points - diff;
            }
            else{
                points = 0;
            }
        }

        _horseRace.raced = true;
        _horseRace.points = points;
        _horseRace.racesNumber = _horseRace.racesNumber.add(1);
        if(_position == 0){
            _horseRace.firstNumber = _horseRace.firstNumber.add(1);
        }
        else if(_position == 1){
            _horseRace.secondNumber = _horseRace.secondNumber.add(1);
        }
        else if(_position == 2){
            _horseRace.thirdNumber = _horseRace.thirdNumber.add(1);
        }

    }

    function getHorseRaceLevel(uint256 tokenId) public view returns(RaceLevelType){
        
        HorseRace memory _horseRace =  tokenIdToHorseRace[tokenId];

        if(!_horseRace.raced){
            return RaceLevelType.Griffin;
        }
        else{
            uint256 points = _horseRace.points;
            if(points <= 20){
                return RaceLevelType.Class5;
            }
            else if(points <= 40){
                return RaceLevelType.Class4;
            }
            else if(points <= 60){
                return RaceLevelType.Class3;
            }
            else if(points <= 80){
                return RaceLevelType.Class2;
            }
            else{
                return RaceLevelType.Class1;
            }
        }
    }



    function getRaceHorseIds(uint256 raceId) public view returns(uint256[RaceNumber] memory){
        Race memory race = Races[raceId];
        return race.horseIds;
    }

    function getRaceResult(uint256 raceId) public view returns(uint256[RaceNumber] memory){
        Race memory race = Races[raceId];
        return race.resultArray;
    }

    function getHorseRaceInfo(uint256 horseId) public view returns(HorseRace memory){
        HorseRace memory _horseRace =  tokenIdToHorseRace[horseId];
        return _horseRace;
    }


}