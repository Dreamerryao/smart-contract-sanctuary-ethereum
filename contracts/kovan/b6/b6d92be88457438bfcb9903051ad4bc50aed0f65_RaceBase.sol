// File contracts/8/SPEED.sol

pragma solidity ^0.8.0;

library SPEED{
    
    using SafeMath for uint256;

    uint256 public constant pbNumber = 10;

    uint256 public constant speedAverageBase = 10000;


    function getSpeed(
        uint256 randomN, 
        uint256[pbNumber] memory pb, 
        uint256 minSpeed,
        uint256 maxSpeed
    ) internal view returns(uint256){
        uint256 speedIndex = _getSpeedIndex(randomN, pb);
        uint256 speed = _fromIndexToSpeed(speedIndex, minSpeed, maxSpeed);
        return speed;
    }


    function _getSpeedIndex(uint256 r, uint256[pbNumber] memory a) private view returns(uint256){
        uint256 s = r.mod(speedAverageBase);
        console.log('random:',s);
        uint256 sum = 0;
        uint256 i = pbNumber * 2;
        for (uint256 index = 0; index < a.length; index++) {
            sum = sum.add(a[index]);
            if(s <= sum){
                i = index;
                break;
            }
        }
        if(i > pbNumber){
            i = pbNumber - 1;
        }
        return i;
    }


    function _fromIndexToSpeed(uint256 index, uint256 _minSpeed, uint256 _maxSpeed) private pure returns(uint256){
        uint256 speed = 0;
        // uint256 averageSpeed = 10000;
        uint256 averageIndex = 4;
        if(index <= averageIndex){
            uint256 diff = (averageIndex.add(1).sub(index)).mul(_minSpeed.div(5));
            speed =  speedAverageBase.sub(diff);
        }
        else{
            uint256 diff = (index.sub(averageIndex)).mul(_maxSpeed.div(5));
            speed = speedAverageBase.add(diff);
        }
        return speed;
    }
    
}


