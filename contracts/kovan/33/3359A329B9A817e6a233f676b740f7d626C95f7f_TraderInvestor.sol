pragma solidity ^0.8.0;

import {ONE_HUNDRED_PERCENT} from "../Globals.sol";
import {ScoreDBInterface} from "../interfaces/ScoreDBInterface.sol";

import "./ERC20PaymentStandard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interfaces/IPriceFeed.sol";
import "../interfaces/ICollateralManager.sol";

import "hardhat/console.sol";

/**
 * @title ERC20CollateralPayment
 * @author Carson Case
 * @notice this is an example of a override of ERC20PaymentStandard. This offers ERC20 collateral to be be added
 * @dev the goal here is to keep this lightweight for Roci as investor is a heavy contract. Try and move as much
 *   functionality as possible to CollateralManager or other contracts
 */
contract ERC20CollateralPayment is ERC20PaymentStandard, Ownable {
    ICollateralManager public manager;
    IPriceFeed public priceFeed;
    address public creditOracle;

    /**
     * @notice constructor just runs the ERC20PaymentStandard constructor
     * @param _bonds is the contract address of bonds
     */
    constructor(address _bonds, address _oracle, address _ncfs)
        ERC20PaymentStandard(_bonds, _ncfs)
        Ownable()
    {
        creditOracle = _oracle;
    }

    /**
     * @notice addCollateral must be called before issuing loan
     * @param _ERC20Contract address of the ERC20 you want to have as collaterall. DOES NOT have to be equal to payment ERC20
     * @param _amount is the ammount to add as collateral
     * @param _loanId is the loan ID to add collateral to
     */
    function addCollateral(
        address _ERC20Contract,
        uint256 _amount,
        uint256 _loanId
    ) public {
        manager.deposit(msg.sender, _ERC20Contract, _loanId, _amount);
    }

    /**
     * @dev function to change the collateral manager
     * @param _new is the new address
     */
    function changeManager(address _new) external onlyOwner {
        manager = ICollateralManager(_new);
    }

    /**
     * @dev function to change priceFeed
     * @param _new is the new address
     */
    function changePriceFeed(address _new) external onlyOwner {
        priceFeed = IPriceFeed(_new);
    }

    /**
     * @notice called when bonds are issued so as to make sure lender can only mint bonds once.
     * @param _id loan ID
     * @return the loan principal (so bonds knows how many NFTs to mint)
     * @return the borrowers address (so bonds can make sure borrower is calling this function)
     */
    function issueBonds(uint256 _id)
        public
        virtual
        override
        onlyBonds
        returns (uint256, address)
    {
        require(_isWithinLTV(_id), "Not enough collateral to issue this loan");
        return (super.issueBonds(_id));
    }

    /**
     * @notice function for user to claim their collateral as they go. Must be within their LTV
     * @param _id ...
     * @param _amount to withdrawal
     */
    function claimCollateral(uint256 _id, uint256 _amount) external {
        require(
            msg.sender == loanLookup[_id].borrower,
            "must be the borrower to claim collateral"
        );
        (
            /*collateralUSD*/,
            /*outstandingUSD*/, 
            /*score*/,
            /*ltv*/,
            /*lt*/,
            int256 availableCollateral
        ) = _getPricedLoanInfo(_id);

        require(
            int256(_amount) <= int256(availableCollateral),
            "You may not remove this much collateral yet"
        );

        manager.withdrawal(_id, _amount, msg.sender);
    }

    /**
     * @dev function for admin to liquidate a loan manually
     */
    function liquidateByAdmin(uint256 _id) external onlyOwner {
        if (isDelinquent(_id)) {
            _liquidate(_id, address(this));
        }
    }

    /**
     * @notice override isDelinquent to factor in LTV
     * @dev returns false if the outstanding balance / collateral is >= LTV for the user at the time of calling
     * @param _id is the loan id
     * @return a bool representing if the loan does not have sufficient collateral OR has missed payments
     */
    function isDelinquent(uint256 _id) public view override returns (bool) {
        // if missed payments
        if (isComplete(_id)) {
            return false;
        }

        // confusing if statement to put as much in one if as possible. Sorry reader
        if (!_isWithinLT(_id)) {
            return true;
        } else {
            return false;
        }
    }

    /**
     * @dev a useful little function to get the loan's priced info via oracles
     * @param _id loan ID
     * NOTE returns are in order
     * @return  collateral value in USD
     * @return outstanding value in USD
     * @return Roci score for the loan's NCFS ID
     * @return LTV
     */
    function _getPricedLoanInfo(uint256 _id)
        private
        view
        returns (
            int256,
            int256,
            uint16,
            uint256,
            uint256,
            int256
        )
    {
        loan memory ln = loanLookup[_id];

        // if Not enough collateral
        (address erc20Contract, uint256 collateral) = manager
            .getCollateralLookup(address(this), _id);
        uint16 score = ScoreDBInterface(creditOracle).getCurrentScore(
            ln.ncfsID
        );

        uint256 ltv = ScoreDBInterface(creditOracle).LTV(ln.ERC20Address,score);
        uint256 lt = ScoreDBInterface(creditOracle).LT(ln.ERC20Address, score);

        int256 collateralUSD = priceFeed.getLatestPriceUSD(erc20Contract) *
            int256(collateral);
        int256 outstanding = int256(ln.totalPaymentsValue) -
            int256(ln.paymentComplete); // cannot be 0 because of isComplete return false

        if (outstanding < 0) outstanding = 0;

        int256 availableCollateral = int256(collateral) -
            (int256(outstanding) * int256(ONE_HUNDRED_PERCENT)) /
            int256(ltv);

        int256 outstandingUSD = priceFeed.getLatestPriceUSD(ln.ERC20Address) *
            outstanding;

        return (collateralUSD, outstandingUSD, score, ltv, lt, availableCollateral);
    }

    /**
     * @dev function handle liqidation logic. can be overriden to have more logic
     */
    function _liquidate(uint256 _id, address _receiver) internal virtual {
        (address erc20Contract, uint256 amount) = manager.getCollateralLookup(
            address(this),
            _id
        );

        manager.withdrawal(_id, amount, _receiver);
    }
    
    function _isWithinLT(uint256 _id) internal view returns(bool){
        (
            int256 collateralUSD,
            int256 outstandingUSD,
            ,
            /*score*/
            /*ltv*/,
            uint256 lt,
            /*available collateral*/

        ) = _getPricedLoanInfo(_id);

        // return true if outstanding balance / collateral > lt or just 0 if collateralUSD = 0
        return collateralUSD == 0 ? false : 
        ((outstandingUSD * int256(ONE_HUNDRED_PERCENT)) / collateralUSD <= int256(lt));
    }

    function _isWithinLTV(uint256 _id) internal view returns(bool){
        (
            int256 collateralUSD,
            int256 outstandingUSD,
            /*score*/,
            uint256 ltv,
            /*lt*/,
            /*available collateral*/

        ) = _getPricedLoanInfo(_id);

        // return true if outstanding balance / collateral > ltv or just 0 if collateralUSD = 0
        return collateralUSD == 0 ? false : 
        ((outstandingUSD * int256(ONE_HUNDRED_PERCENT)) / collateralUSD <= int256(ltv));
    }

}

