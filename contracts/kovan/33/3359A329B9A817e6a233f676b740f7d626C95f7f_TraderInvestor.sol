pragma solidity ^0.8.0;

import "./Investor.sol";
import "../interfaces/ITrader.sol";
import "../interfaces/revManager/IRevenueManager.sol";

/**
 * @title Investor
 * @author Carson Case [[email protected]]
 * @dev TestToken will eventually be replaced with ERC-20
 */
contract TraderInvestor is Investor {
    ITrader public trader;
    IRevenueManager public revManager;

    // config vars
    uint256 public tradeAmount = 1 ether / 100;
    uint256 public margin = 1 ether / 1000;
    address public pair;

    /**
     * @dev creates a uniswap pair of the two tokens upon construction
     * @param _token is the coin this token is a debt token of
     * @param _bonds is the bonds contract
     * @param _trader is the contract that handles trades
     * @param _oracle is the credit score oracle
     * @param _revManager is the revenue manager
     * @param _prefix is a symbol prefix added to token's symbol
     * @param _postfix is a symbol postfix added to token's symbol
     */
    constructor(
        address _token,
        address _bonds,
        address _trader,
        address _oracle,
        address _ncfs,
        address _revManager,
        bytes1 _prefix,
        bytes1 _postfix
    ) Investor(_token, _bonds, _oracle, _ncfs, _prefix, _postfix) {
        trader = ITrader(_trader);
        revManager = IRevenueManager(_revManager);
        pair = ITrader(_trader).initPair(address(this), _token);

        token.approve(address(_revManager), uint256(2**256 - 1));
        token.approve(address(trader), uint256(2**256 - 1));
        _approve(address(this), address(trader), uint256(2**256 - 1));
    }

    /**
     * @dev for now anyone can call
     */
    function balance() external {
        _balance();
    }

    /**
     * @notice MUST approve this contract to spend your ERC1155s in bonds. Used to have this auto handled by the on received function.
     * However that was not a good idea as a hacker could create fake bonds.
     * NOTE the ovveride diference is that this withdrawal doesn't actually transfer the tokens back.
     *  Since the tokens are in the posession of the contract already anyways. It would only be trading it's own ERC-1155s for it's own tokens
     * @param _id is the id of the bond to send in
     * @param _amm is the amount to send
     * @param _receiver is the receiver of erc20 tokens
     */
    function withdrawl(
        uint256 _id,
        uint256 _amm,
        address _receiver
    ) public override {
        IERC1155 bonds = IERC1155(bondContract);
        IERC20 erc20 = IERC20(loanLookup[_id].ERC20Address);
        require(
            loanLookup[_id].issued,
            "this loan has not been issued yet. How do you even have bonds for it???"
        );
        require(
            _amm <= loanLookup[_id].awaitingCollection,
            "There are not enough payments available for collection"
        );
        bonds.safeTransferFrom(_receiver, DEAD, _id, _amm, "");
        // erc20.transfer(_receiver, _amm); REMOVED
    }

    /**
     * @notice function override sends funds to the rev manager
     * @param _id to pay off
     * @param _erc20Amount is amount to pay off
     */
    function payment(uint256 _id, uint256 _erc20Amount) public override {
        super.payment(_id, _erc20Amount);
        _sendAllToRevManager();
    }

    /**
     * @dev function handle liqidation logic. Override from collateralPayment
     *   liquidates collateral allways to this, then swaps to token
     */
    function _liquidate(uint256 _id, address _receiver) internal override {
        (address erc20Contract, uint256 amount) = manager.getCollateralLookup(
            address(this),
            _id
        );

        manager.withdrawal(_id, amount, address(this));
        IERC20(erc20Contract).approve(address(trader), amount);
        trader.trade(erc20Contract, address(token), amount);
    }

    /**
     * @dev helper function to send all funds to rev manager
     */
    function _sendAllToRevManager() internal {
        uint256 bal = token.balanceOf(address(this));
        revManager.payment(address(token), bal);
    }

    /**
     * @dev function to send funds to borrowers. Used to fulfill loans
     * @param _receiver is the receiver of the funds
     * @param _amount is the amount to send
     * NOTE this is meant to be overriden in order to contain logic for storing funds in other contracts
     */
    function _sendFunds(address _receiver, uint256 _amount) internal override {
        revManager.requestFunds(address(token), _amount);
        super._sendFunds(_receiver, _amount);
    }

    /**
     * @dev function helps check to make sure a loan is available before it's fulfilled
     *   thus saving the user the gas of a failed fullfilment
     */
    function _checkAvailable(uint256 _amount) internal override {
        require(
            revManager.balanceAvailable(address(this), address(token)) >=
                _amount,
            "There are not enough funds available to fulfill this loan"
        );
        super._checkAvailable(_amount);
    }

    /**
     * @dev external for testing only. Balances price to keep it 1:1
     */
    function _balance() private {
        uint256 price = _getPrice(_getTradeAmount());
        if (price < _getTradeAmount() - margin) {
            _buy();
        } else if (price > _getTradeAmount() + margin) {
            _sell();
        }
    }

    /**
     * @dev for now just has flat amount. But this may become a variable rate in the future
     */
    function _getTradeAmount() private view returns (uint256) {
        return (tradeAmount);
    }

    /**
    * @dev returns the expected output of investor tokens if tradeAmount
    of base tokens are sold
     */
    function _getPrice(uint256 _tradeAm) private view returns (uint256) {
        return trader.getAmountOut(address(this), address(token), _tradeAm);
    }

    /**
     * @dev preforms a buy
     */
    function _buy() private returns (bool) {
        address tis = address(this);
        uint256 tradeAm = _getTradeAmount();
        if (revManager.balanceAvailable(tis, address(token)) >= tradeAm) {
            revManager.requestFunds(address(token), tradeAm);
            trader.trade(address(token), tis, tradeAm);
            _burn(tis, balanceOf(tis));
            return true;
        } else {
            return false;
        }
    }

    /**
     * @dev preforms a sell
     */
    function _sell() private returns (bool) {
        _mint(address(this), _getTradeAmount());
        trader.trade(address(this), address(token), _getTradeAmount());
        _sendAllToRevManager();
        return true;
    }
}

