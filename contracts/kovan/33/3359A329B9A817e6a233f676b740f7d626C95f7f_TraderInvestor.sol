pragma solidity ^0.8.0;

import "../testing/TestToken.sol";
import "./ERC20CollateralPayment.sol";
import "../interfaces/IBonds.sol";

import {ONE_HUNDRED_PERCENT} from "../Globals.sol";

/**
 * @title sIERC20
 * @dev just adds the symbol function to IERC20
 */
interface sIERC20 is IERC20 {
    function symbol() external returns (string memory);
}

/**
 * @title Investor
 * @author Carson Case [[email protected]]
 * @dev TestToken will eventually be replaced with ERC-20
 */
contract Investor is ERC20, ERC20CollateralPayment {
    address public farmMinterRole;
    uint256 public interestRateAnnual = 10 ether;
    IERC20 public token;
    IBonds public bonds;

    // map loan ids to staking indexes
    mapping(uint256 => uint256) stakingIndexes;

    /**
     * @dev creates a uniswap pair of the two tokens upon construction
     * @param _token is the coin this token is a debt token of
     * @param _bonds is the bonds contract
     * @param _oracle is the credit score oracle
     * @param _prefix is a symbol prefix added to token's symbol
     * @param _postfix is a symbol postfix added to token's symbol
     */
    constructor(
        address _token,
        address _bonds,
        address _oracle,
        address _ncfs,
        bytes1 _prefix,
        bytes1 _postfix
    )
        ERC20(
            "Roci Debt Token",
            string(
                abi.encodePacked(_prefix, sIERC20(_token).symbol(), _postfix)
            )
        )
        ERC20CollateralPayment(_bonds, _oracle, _ncfs)
    {
        token = IERC20(_token);
        bonds = IBonds(_bonds);
    }

    /**
     * @dev for farm to mint
     * @param to is who the tokens are minted to
     * @param amount is the amount to mint
     */
    function mint(address to, uint256 amount) external {
        require(
            msg.sender == farmMinterRole,
            "Only the farm may call this function"
        );
        _mint(to, amount);
    }

    /**
     * @dev for owner to set the minter role
     * @param _new is the new address
     */
    function setFarmMinterRole(address _new) external onlyOwner {
        farmMinterRole = _new;
    }

    /**
     * @dev owner can set interestRateAnnual
     * @param _interestRateAnnual new interestRateAnnual
     */
    function setInterestRateAnnual(uint256 _interestRateAnnual)
        external
        onlyOwner
    {
        require(_interestRateAnnual > 0, "Interest rate have to be positive");
        interestRateAnnual = _interestRateAnnual;
    }

    /**
     * @dev function to combine the entire borrowing process in one function.
     *   1). configure
     *   2). mint
     *   3). exchange
     *
     * NOTE: the user will need to sign with the address of this and the ID of the loan. This is how they get their
     *   new loan's id before calling:
     *       id = getId(_borrower, getNumberOfLoans(address _borrower));
     *
     * @param _amount to borrow
     * @param _duration of loan in seconds
     * @param _NCFSID is the user's NCFS NFT ID from Roci's Credit scoring system
     * @param _collateralAmount is the amount of collateral to send in
     * @param _collateral is the ERC20 address of the collateral
     * @param _hash is the hash of this address and the loan ID. See Bonds.sol for more info on this @newLoan()
     * @param _signature is the signature of the data hashed for hash
     */
    function borrow(
        uint256 _amount,
        uint256 _duration,
        uint256 _NCFSID,
        uint256 _collateralAmount,
        address _collateral,
        bytes32 _hash,
        bytes memory _signature
    ) external {
        _checkAvailable(_amount);
        uint256 accrualPeriod = 60 * 60 * 24 * 30;
        uint256 periodsInYear = 12;
        uint256 id = configureNew(
            address(token),
            msg.sender,
            0,
            _NCFSID,
            _duration,
            _amount,
            interestRateAnnual / periodsInYear,
            accrualPeriod
        );

        // collect collateral
        /// NOTE borrower must approve the Collateral Manager to spend these funds NOT the investor
        addCollateral(_collateral, _collateralAmount, id);

        // request a new score (this will eventually call borrowFulfill() )
        ScoreDBInterface(creditOracle).requestUpdatedScoreBorrow(
            _NCFSID,
            id,
            _hash,
            _signature
        );
    }

    /**
     * @notice function handles the payment of the loan. Does not have to be borrower
     *as payment comes in. The contract holds it until collection by bond owners. MUST APPROVE FIRST in ERC20 contract first
     * @param _id to pay off
     * @param _erc20Amount is amount to pay off
     */
    function payment(uint256 _id, uint256 _erc20Amount)
        public
        virtual
        override
    {
        uint256 stakeIndex = stakingIndexes[_id];

        // unstake bonds to collect interest
        bonds.unstake(stakeIndex);
        uint256 bal = bonds.balanceOf(address(this), _id);
        // withdrawal profit
        if (bal >= loanLookup[_id].awaitingCollection) {
            withdrawl(_id, loanLookup[_id].awaitingCollection, address(this));
            bal = bonds.balanceOf(address(this), _id);
        }
        // stake remaining bonds again
        stakeIndex = bonds.stake(_id, bal);
        //handle payment logic
        super.payment(_id, _erc20Amount);
    }

    /// @dev function called by chainlink oracle to finish loan generation. An extention of borrow
    function borrowFulfill(
        uint256 _id,
        bytes32 _hash,
        bytes memory _signature
    ) external {
        require(
            msg.sender == creditOracle,
            "Only the credit scoring oracle can call this function"
        );

        // this function calls the issue function with requires non-delinquency
        bonds.newLoan(address(this), _id, _hash, _signature);
        uint256 amount = loanLookup[_id].principal;

        require(
            bonds.balanceOf(address(this), _id) == amount,
            "Issue minting bonds"
        );

        // stake the bonds
        stakingIndexes[_id] = bonds.stake(_id, amount);

        // fulfill loan
        _sendFunds(loanLookup[_id].borrower, amount);
    }

    /**
     * @dev function to send funds to borrowers. Used to fulfill loans
     * @param _receiver is the receiver of the funds
     * @param _amount is the amount to send
     * NOTE this is meant to be overriden in order to contain logic for storing funds in other contracts
     */
    function _sendFunds(address _receiver, uint256 _amount) internal virtual {
        token.transfer(_receiver, _amount);
    }

    /**
     * @dev function helps check to make sure a loan is available before it's fulfilled
     *   thus saving the user the gas of a failed fullfilment
     */
    function _checkAvailable(uint256 _amount) internal virtual {
        require(_amount > 0, "Cannot borrow an amount of 0");
        // check the oracle is not paused if pause functionality is added
    }
}

