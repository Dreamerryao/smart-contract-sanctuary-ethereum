        Rarity.KEEPER,
        Rarity.SHINY,
        Rarity.BRONZE,
        Rarity.SILVER,
        Rarity.GOLD,
        Rarity.PLATNIUM,
        Rarity.SUPER_RARE,
        Rarity.FORGOTTEN,
        Rarity.SECRET,
        Rarity.SINGLES
    ];

    enum Class {
        NULL,
        ARTIFICER,
        AVENGER,
        ARDENT,
        BARBARIAN,
        BARD,
        CLERIC,
        DRUID,
        FIGHTER,
        MONK,
