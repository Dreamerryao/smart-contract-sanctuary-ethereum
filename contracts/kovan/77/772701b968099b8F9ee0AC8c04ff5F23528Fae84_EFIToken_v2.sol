/// @title Vesting Wallet
/// @author Enjin
/// @notice Allow beneficiaries to withdraw allocated tokens on a chosen schedule
contract VestingWallet is ERC20Holder, AccessControlEnumerable {
    using LibThresholdApproval for LibThresholdApproval.Bytes32Approval;
    using SafeERC20 for IERC20;

    /// @dev the minimum number of managers that must approve a change in beneficiary
    uint8 private constant MIN_APPROVAL_THRESHOLD = 2;

    /// @notice the number of managers that must approve a change in beneficiary
    uint8 public approvalThreshold = MIN_APPROVAL_THRESHOLD;

    /// @notice percentage of tokens to be made available at each vesting period
    uint16 public immutable vestingPercentage;

    /// @notice percentage of tokens to be made available at the first vesting period
    uint16 public vestingPercentageFirst;

    /// @notice the token allocated to beneficiaries
    address public immutable token;

    /// @notice block timestamp from when vesting should begin
    uint32 public immutable start;

    /// @notice the number of seconds between each vesting period
    uint32 public immutable tranche;

    /// @notice the total number of tokens allocated to beneficiaries
    uint256 public totalAllocated;

    /// @notice unvested tokens keyed by beneficiary address
    mapping(address => uint256) public unvested;

    /// @notice tokens vested and withdrawn keyed by beneficiary address
    mapping(address => uint256) public released;

    /// @notice MANAGER ROLE hash
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    /// @dev util to help track approvals for a given unique key
    LibThresholdApproval.Bytes32Approval private approval;

    event BeneficiaryAdded(address indexed beneficiary, uint256 amount);

    event BeneficiaryUpdated(
        address indexed oldBeneficiary,
        address indexed newBeneficiary
    );

    event BeneficiaryRemoved(address indexed beneficiary);

    event TokensReleased(address indexed beneficiary, uint256 amount);

    event ApprovalThresholdUpdated(uint8 threshold, address indexed sender);

    event ManagerApprovedChangeBeneficiary(
        address indexed manager,
        address indexed oldBeneficiary,
        address indexed newBeneficiary
    );

    /// @param _token Address of the Token allocated to beneficiaries
    /// @param _start a future timestamp of when the vesting schedule should begin
    /// @param _tranche the number of seconds between vesting periods
    /// @param _vestingPercentage the percentage of the allocated tokens that vest each period
    /// @param _vestingPercentageFirst the percentage of the allocated tokens that vest for the first period only
    constructor(
        address _token,
        uint32 _start,
        uint32 _tranche,
        uint16 _vestingPercentage,
        uint16 _vestingPercentageFirst
    ) {
        token = _token;
        start = _start;
        tranche = _tranche;
        vestingPercentage = _vestingPercentage;

        vestingPercentageFirst = _vestingPercentageFirst > 0
            ? _vestingPercentageFirst
            : _vestingPercentage;

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /// @dev See {AccessControl-hasRole}.
    modifier onlyAdmin {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, msg.sender),
            "Vesting Wallet: not an admin"
        );

        _;
    }

    /// @notice As an Admin, set a new approval threshold
    /// @param _newThreshold the minimum number of managers required to approve an action before it can be executed
    function setApprovalThreshold(uint8 _newThreshold) external onlyAdmin {
        require(
            _newThreshold >= MIN_APPROVAL_THRESHOLD,
            "Vesting Wallet: threshold too low"
        );

        approvalThreshold = _newThreshold;

        // invalidate all other approvals
        approval.incNonce();

        emit ApprovalThresholdUpdated(_newThreshold, msg.sender);
    }

    /// @notice Get the number of current approvals for a particular change of beneficiary
    /// @param _oldBeneficiary the old beneficiary
    /// @param _newBeneficiary the new beneficiary to replace the old beneficiary
    /// @return the number of approvals given
    function approvalCount(address _oldBeneficiary, address _newBeneficiary)
        external
        view
        returns (uint256)
    {
        return
            approval.approvalCount(
                _changeBeneficiaryKey(_oldBeneficiary, _newBeneficiary)
            );
    }

    /// @notice As a Manager, approve the change of beneficiary
    /// @param _oldBeneficiary the beneficiary being changed
    /// @param _newBeneficiary the new beneficiary
    function approveChangeBeneficiary(
        address _oldBeneficiary,
        address _newBeneficiary
    ) external {
        require(
            hasRole(MANAGER_ROLE, msg.sender),
            "Vesting Wallet: not a manager"
        );

        bytes32 key = _changeBeneficiaryKey(_oldBeneficiary, _newBeneficiary);

        approval.approve(key);

        emit ManagerApprovedChangeBeneficiary(
            msg.sender,
            _oldBeneficiary,
            _newBeneficiary
        );
    }

    /// @notice Change beneficiary address
    /// @param _oldBeneficiary the beneficiary being changed
    /// @param _newBeneficiary the new beneficiary
    function changeBeneficiary(address _oldBeneficiary, address _newBeneficiary)
        external
    {
        bool isEnjin = hasRole(DEFAULT_ADMIN_ROLE, msg.sender);

        require(
            isEnjin || _oldBeneficiary == msg.sender,
            "Vesting Wallet: caller must be Enjin Admin or Beneficiary"
        );

        if (isEnjin) {
            bytes32 key =
                _changeBeneficiaryKey(_oldBeneficiary, _newBeneficiary);

            require(
                approval.isApproved(key, approvalThreshold),
                "Vesting Wallet: manager approval threshold not met"
            );
        }

        require(
            0 != unvested[_oldBeneficiary],
            "Vesting Wallet: not a beneficiary"
        );

        require(
            0 == unvested[_newBeneficiary],
            "Vesting Wallet: new beneficiary already exists"
        );

        unvested[_newBeneficiary] = unvested[_oldBeneficiary];

        delete unvested[_oldBeneficiary];

        // invalidate all other approvals
        approval.incNonce();

        emit BeneficiaryUpdated(_oldBeneficiary, _newBeneficiary);
    }

    /// @notice As an Admin, add a beneficiary with desired token allocation
    /// @param _beneficiary the beneficiary's address
    /// @param _amount the number of tokens to be allocated
    function addBeneficiary(address _beneficiary, uint256 _amount)
        public
        onlyAdmin
    {
        require(
            0 == unvested[_beneficiary],
            "Vesting Wallet: already a beneficiary"
        );

        require(
            availableTokens() >= _amount,
            "Vesting Wallet: tokens not available"
        );

        unvested[_beneficiary] = _amount;
        totalAllocated = totalAllocated + _amount;

        emit BeneficiaryAdded(_beneficiary, _amount);
    }

    /// @notice add a list of beneficiaries with allocated tokens
    /// @param _beneficiaries a list of beneficiaries
    /// @param _amounts the amounts to allocated to each corresponding beneficiary address
    /// @return true if successful
    function addBeneficiaries(
        address[] calldata _beneficiaries,
        uint256[] calldata _amounts
    ) external returns (bool) {
        uint16 count = uint16(_beneficiaries.length);

        require(
            count == _amounts.length,
            "Vesting Wallet: number of beneficiaries and respective amounts do not match"
        );

        for (uint16 i = 0; i < count; i++) {
            addBeneficiary(_beneficiaries[i], _amounts[i]);
        }

        return true;
    }

    /// @notice As an Admin, remove a beneficiary and deallocated any unvested tokens
    /// @param _beneficiary address of the beneficiary
    function removeBeneficiary(address _beneficiary) external onlyAdmin {
        require(
            vestedTokens(_beneficiary) == 0,
            "Vesting Wallet: beneficiary has vested tokens"
        );

        require(
            0 != unvested[_beneficiary],
            "Vesting Wallet: not a beneficiary"
        );

        totalAllocated = totalAllocated - unvested[_beneficiary];

        delete unvested[_beneficiary];

        emit BeneficiaryRemoved(_beneficiary);
    }

    /// @notice As a beneficiary, withdraw any tokens that have vested
    /// @return true if successful
    function withdrawVestedTokens() external returns (bool) {
        if (0 == unvested[msg.sender]) {
            revert("Vesting Wallet: not a beneficiary or all tokens withdrawn");
        }

        uint256 vested = vestedTokens(msg.sender);

        require(vested > 0, "Vesting Wallet: no vested tokens");

        released[msg.sender] = vested;
        unvested[msg.sender] = unvested[msg.sender] - vested;

        require(
            IERC20(token).transfer(msg.sender, vested),
            "Vesting Wallet: withdrawVestedTokens failed"
        );

        emit TokensReleased(msg.sender, vested);

        return true;
    }

    /// @notice As an Admin, transfer any tokens belonging to this contract
    /// @param _token address of the token to transfer
    /// @param _to address of the receiver
    /// @param _amount amount of tokens to transfer
    /// @return true if successful
    function withdrawTokens(
        address _token,
        address _to,
        uint256 _amount
    ) external onlyAdmin returns (bool) {
        require(
            token != _token || availableTokens() >= _amount,
            "Vesting Wallet: tokens not available"
        );

        IERC20(_token).safeTransfer(_to, _amount);

        return true;
    }

    /// @notice get the number of tokens not yet allocated to beneficiaries
    /// @return the number of tokens
    function availableTokens() public view returns (uint256) {
        uint256 balance = IERC20(token).balanceOf(address(this));
        return balance - totalAllocated;
    }

    /// @param _beneficiary a beneficiary's address
    /// @return the number of vested tokens for a given address
    function vestedTokens(address _beneficiary) public view returns (uint256) {
        if (currentVestingPeriod() == 0) {
            return 0;
        }

        if (0 == unvested[_beneficiary]) {
            return 0;
        }

        uint256 totalBenefit = unvested[_beneficiary] + released[_beneficiary];
        uint256 defaultPayout = (totalBenefit * vestingPercentage) / 10000;

        // this is our last payout
        if (
            currentVestingPeriod() > 1 &&
            defaultPayout >= unvested[_beneficiary]
        ) {
            return unvested[_beneficiary];
        }

        uint256 payoutForFirstPeriod =
            (totalBenefit * vestingPercentageFirst) / 10000;

        uint256 totalForSubsequentPeriods;

        if (currentVestingPeriod() > 1) {
            totalForSubsequentPeriods =
                defaultPayout *
                (currentVestingPeriod() - 1); // exclude the first period
        }

        uint256 totalVested = payoutForFirstPeriod + totalForSubsequentPeriods;

        // cap rewards at original total benefit
        if (totalVested > totalBenefit) {
            return totalBenefit - released[_beneficiary];
        }

        return totalVested - released[_beneficiary];
    }

    /// @return the current vesting period
    function currentVestingPeriod() public view returns (uint256) {
        if (block.timestamp < (start + tranche)) {
            return 0;
        }

        return (block.timestamp - start) / tranche;
    }

    /// @notice renounce a Role
    /// @dev override parent to prevent renouncing all Admin roles
    /// @dev invalidate approvals to preserve integrity of approval process
    /// @param _role the role to be renounced
    /// @param _account the account currently associated with the role
    function renounceRole(bytes32 _role, address _account) public override {
        if (DEFAULT_ADMIN_ROLE == _role) {
            require(
                getRoleMemberCount(DEFAULT_ADMIN_ROLE) > 1,
                "Vesting Wallet: last Admin"
            );
        }

        if (MANAGER_ROLE == _role) {
            // invalidate all approvals
            approval.incNonce();
        }

        super.renounceRole(_role, _account);
    }

    /// @notice As a Manager, revoke the Role of another account
    /// @dev invalidate approvals to preserve integrity of approval process
    /// @param _role the role to be revoked
    /// @param _account the account currently associated with the role
    function revokeRole(bytes32 _role, address _account) public override {
        if (MANAGER_ROLE == _role) {
            // invalidate all approvals
            approval.incNonce();
        }

        super.revokeRole(_role, _account);
    }

    /// @return unique key to identify the change in beneficiary
    function _changeBeneficiaryKey(
        address _oldBeneficiary,
        address _newBeneficiary
    ) private pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "changeBeneficiary(_oldBeneficiary, _newBeneficiary)",
                    _oldBeneficiary,
                    _newBeneficiary
                )
            );
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControlEnumerable, ERC20Receiver)
        returns (bool)
    {
        return
            interfaceId == type(IERC20Receiver).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}