library LibThresholdApproval {
    using SafeMath for uint256;

    // Base approval.
    // Tallies count and who approved already
    struct Approval {
        uint256 nonce;
        // Nonce => Count
        mapping(uint256 => uint256) count; // how many approval so far
        // Nonce => (approver => approved)
        mapping(uint256 => mapping(address => bool)) by; // who already approved
    }

    // Generic approval function
    function approve(Approval storage _approval) internal {
        // can only approve once
        require(
            !_approval.by[_approval.nonce][msg.sender],
            "Sender has already approved."
        );
        _approval.by[_approval.nonce][msg.sender] = true;
        // record approval
        _approval.count[_approval.nonce] = _approval.count[_approval.nonce].add(
            1
        );
    }

    function disapprove(Approval storage _approval) internal {
        // can only approve once
        require(
            _approval.by[_approval.nonce][msg.sender],
            "Sender hasn't approved yet."
        );
        _approval.by[_approval.nonce][msg.sender] = false;
        // record approval
        _approval.count[_approval.nonce] = _approval.count[_approval.nonce].sub(
            1
        );
    }

    function incNonce(Approval storage _approval) internal {
        _approval.nonce = _approval.nonce.add(1);
    }

    function isApproved(Approval storage _approval, uint256 threshold)
        internal
        view
        returns (bool)
    {
        return _approval.count[_approval.nonce] >= threshold;
    }

    function isApprovedBy(Approval storage _approval, address _address)
        internal
        view
        returns (bool)
    {
        return _approval.by[_approval.nonce][_address];
    }

    function approvalCount(Approval storage _approval)
        internal
        view
        returns (uint256)
    {
        return _approval.count[_approval.nonce];
    }

    ////////////////////////////////////////////////////////////////////////////////

    // Generic approval event to track activity
    event ApproveKey(bytes32 _key, address _approver, bool _approve);
    event ResetKey(bytes32 _key, address _approver);
    event ResetGlobal(address _approver);

    // Approval related to an key
    struct Bytes32Approval {
        // global nonce
        uint256 nonce;
        // global nonce => key => Approval
        mapping(uint256 => mapping(bytes32 => Approval)) approval;
    }

    function approve(Bytes32Approval storage _approval, bytes32 _key) internal {
        approve(_approval.approval[_approval.nonce][_key]); // Generic approval event to track activity
        emit ApproveKey(_key, msg.sender, true);
    }

    function disapprove(Bytes32Approval storage _approval, bytes32 _key)
        internal
    {
        disapprove(_approval.approval[_approval.nonce][_key]); // Generic approval event to track activity
        emit ApproveKey(_key, msg.sender, false);
    }

    // Per-address nonce
    function incNonce(Bytes32Approval storage _approval, bytes32 _key)
        internal
    {
        incNonce(_approval.approval[_approval.nonce][_key]);
        emit ResetKey(_key, msg.sender);
    }

    // Global nonce
    function incNonce(Bytes32Approval storage _approval) internal {
        _approval.nonce = _approval.nonce.add(1);
        emit ResetGlobal(msg.sender);
    }

    function isApproved(
        Bytes32Approval storage _approval,
        bytes32 _key,
        uint256 threshold
    ) internal view returns (bool) {
        return isApproved(_approval.approval[_approval.nonce][_key], threshold);
    }

    function isApprovedBy(
        Bytes32Approval storage _approval,
        bytes32 _key,
        address _approver
    ) internal view returns (bool) {
        return
            isApprovedBy(_approval.approval[_approval.nonce][_key], _approver);
    }

    function approvalCount(Bytes32Approval storage _approval, bytes32 _key)
        internal
        view
        returns (uint256)
    {
        return approvalCount(_approval.approval[_approval.nonce][_key]);
    }
}



