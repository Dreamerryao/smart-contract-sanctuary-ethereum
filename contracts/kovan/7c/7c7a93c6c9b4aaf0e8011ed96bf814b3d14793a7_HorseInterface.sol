// File contracts/8/HorseInterface.sol

pragma solidity ^0.8.0;



contract HorseInterface {

    using SafeMath for uint256;

    IAuction public siringAuction;
    IHorse public nonFungibleContract;


    struct HorseBreedLimitInfo{
        uint256 breedCounter;
        uint256 left;
        uint256 breedLimit;
    }

    struct HorseSimple{
        uint256 horseId;
        IHorse.Horse horse;
        HorseBreedLimitInfo breedLimitInfo;
        IAuction.Auction auctionInfo;
        bool isOnAuction;
    }

    struct HorseDetail {
        HorseSimple horse;
        IHorse.Horse matron;
        IHorse.Horse sire;
        IHorse.Horse[]  offsprings;
    }


    constructor(address _nftAddress, address _siringAuction) {
        nonFungibleContract = IHorse(_nftAddress);
        siringAuction = IAuction(_siringAuction);
    }
    

    function getHorseSimple(uint256 _tokenId) public view returns(HorseSimple memory){
        IHorse.Horse memory _horse = nonFungibleContract.getHorse(_tokenId);
        (uint256 _breedCounter, uint256 _left, uint256 _breedLimit) = nonFungibleContract.queryBreedLimitInfo(_tokenId);
        HorseBreedLimitInfo memory _breedLimitInfo = HorseBreedLimitInfo({
            breedCounter: _breedCounter,
            left: _left,
            breedLimit: _breedLimit
        });
        IAuction.Auction memory _auctionInfo = siringAuction.getAuction(_tokenId);
        bool _isOnAuction = siringAuction.isOnAuction(_tokenId);
        HorseSimple memory horseSimple = HorseSimple({
            horseId: _tokenId,
            horse: _horse,
            breedLimitInfo: _breedLimitInfo,
            auctionInfo: _auctionInfo,
            isOnAuction: _isOnAuction
        });
        return horseSimple;
    }


    function tokensOfOwner(address _owner) external view returns(HorseSimple[] memory) {
        uint256 tokenCount = nonFungibleContract.balanceOf(_owner);
        if (tokenCount == 0) {
            return new HorseSimple[](0);
        } else {
            HorseSimple[] memory result = new HorseSimple[](tokenCount);
            for (uint256 index = 0; index < tokenCount; index++) {
                uint256 tokenId = nonFungibleContract.tokenOfOwnerByIndex(_owner, index);
                result[index] = getHorseSimple(tokenId);
            }
            return result;
        }
    }


    function getHorseDetail(uint256 _tokenId) external view returns(HorseDetail memory) {
        // IHorse.Horse memory _horse = nonFungibleContract.getHorse(_tokenId);
        HorseSimple memory _horseSimple = getHorseSimple(_tokenId);
        IHorse.Horse memory _horse = _horseSimple.horse;
        IHorse.Horse[] memory _offsprings = new IHorse.Horse[](_horse.offsprings.length);
        for (uint256 index = 0; index < _horse.offsprings.length; index++) {
            uint256 tokenId = _horse.offsprings[index];
            _offsprings[index] = nonFungibleContract.getHorse(tokenId);
        }
        uint256 _matronId = _horse.matronId;
        uint256 _sireId = _horse.sireId;
        IHorse.Horse memory _matron = nonFungibleContract.getHorse(_matronId);
        IHorse.Horse memory _sire = nonFungibleContract.getHorse(_sireId);

        HorseDetail memory h = HorseDetail({
            horse: _horseSimple,
            matron: _matron,
            sire: _sire,
            offsprings: _offsprings
        });

        return h;
    }


    function getAllBreedHorses() external view returns(HorseSimple[] memory) {
        uint256 tokenCount = nonFungibleContract.totalSupply();
        console.log('tokenCount:',tokenCount);
        HorseSimple[] memory result = new HorseSimple[](tokenCount);
        uint256 i = 0;
        for (uint256 index = 0; index < tokenCount; index++) {
            HorseSimple memory _horse = getHorseSimple(index);
            if(_horse.isOnAuction){
                console.log('getAllBreedHorses:i:',i);
                result[i] = _horse;
                i = i + 1;
            }
        }
        console.log('getAllBreedHorses:length:',result.length);
        
        HorseSimple[] memory result2 = new HorseSimple[](i);
        for (uint256 index = 0; index < i; index++) {
            result2[index] = result[index];
        }
        console.log('getAllBreedHorses:length:2:',result2.length);
        return result2;
    }


}