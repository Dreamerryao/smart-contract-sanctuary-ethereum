    function setClass(
        uint8 newClass,
        uint8 index,
        uint256 securityKey
    ) public override securityCheck(securityKey) {
        clazz[index] = newClass;
    }

    function setLand(
        uint8 newLand,
        uint8 index,
        uint256 securityKey
    ) public override securityCheck(securityKey) {
        land[index] = newLand;
    }

    function updateLevel(
        address game,
        uint256 amount,
        bool increase,
        uint256 securityKey
    ) public override securityCheck(securityKey) {
