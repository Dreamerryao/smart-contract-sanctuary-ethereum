// File: StakeWarsFactory.sol

contract StakeWarsFactory is ERC721Enumerable, Administerable, VRFConsumerBase {
    event Deposit(address indexed _from, uint256 _value);

    uint256 private _tokenIdsGen;
    uint256 private _securityKey;

    uint256 public edition;
    string public baseURI;
    string public defaultURI;

    uint256 public maxSupply;
    uint256 public price = 0.005 ether;

    bool public saleActive = false;
    bool public presaleActive = false;
    uint256 public linkFee;
    address public vrfCoordinator;
    bytes32 public keyhash;
    uint256 public _providedSeed;
    uint256 private _generativeSeed;

    mapping(uint48 => uint256) public rarityRatio;
    mapping(uint256 => StakeWarsInternals) public tokenIdToStakeWarrior;
    StakeWarsInternals[] public warriorsToBeDetailed;

    address[] public patronArray;
    mapping(address => uint256) public patronsList;
    mapping(address => uint256) public presaleWhitelist;

    constructor(
        string memory name,
        string memory symbol,
        uint256 _edition,
        string memory _baseURI,
        uint256 _maxSupply,
        address _vrfCoordinator,
        address _linkToken,
        uint256 _linkFee,
        bytes32 _keyhash
    )
        ERC721(name, symbol)
        Administerable(msg.sender)
        VRFConsumerBase(_vrfCoordinator, _linkToken)
    {
        edition = _edition;
        baseURI = _baseURI;
        defaultURI = _baseURI;
        vrfCoordinator = _vrfCoordinator;
        linkFee = _linkFee;
        keyhash = _keyhash;
        maxSupply = _maxSupply;
    }

    function fulfillRandomness(bytes32, uint256 randomness) internal override {
        _providedSeed = randomness;
    }

    function _reserve() public onlyAdmin {
        createMint();
    }

    function mintPresale() public payable {
        uint256 supply = totalSupply();
        uint256 reserved = presaleWhitelist[msg.sender];
        require(presaleActive, "Presale must be active to mint");
        require(reserved > 0, "No tokens reserved for this address");
        require(
            supply + 1 <= maxSupply,
            "Purchase would exceed max supply of tokens"
        );
        require(price == msg.value, "Ether value sent is not correct");
        presaleWhitelist[msg.sender] = reserved - 1;
        createMint();
    }

    function mint() public payable {
        require(saleActive);
        require(totalSupply() + 1 <= maxSupply);
        require(price == msg.value);

        createMint();
    }

    function createMint() internal {
        require(_providedSeed != 0);
        uint256 randomness;
        unchecked {
            randomness =
                block.timestamp *
                (_providedSeed + _generativeSeed + _securityKey);
            _generativeSeed = randomness;
        }
        StakeWarsInternals warrior = new StakeWarsInternals(
            edition,
            randomness,
            _tokenIdsGen,
            _securityKey
        );

        (, uint16 rarity) = warrior.getRarity(_securityKey);
        rarityRatio[uint48(rarity)]++;

        tokenIdToStakeWarrior[_tokenIdsGen] = warrior;
        warriorsToBeDetailed.push(warrior);

        _safeMint(msg.sender, _tokenIdsGen);
        _tokenIdsGen++;
    }

    function warriorsToBeDetailedLength() public view returns (uint256) {
        return warriorsToBeDetailed.length;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _launchNFTs(string memory uri) public onlyAdmin {
        saleActive = false;
        presaleActive = false;
        edition++;
        baseURI = uri;
    }

    function _toggleSaleActive() public onlyAdmin {
        saleActive = !saleActive;
    }

    function _togglePreSaleActive() public onlyAdmin {
        presaleActive = !presaleActive;
    }

    function _setPrice(uint256 newPrice) public onlyAdmin {
        price = newPrice;
    }

    function _expandMaxSupply(uint256 _maxSupply) public onlyAdmin {
        // Only to be used in conjunction with a new edition
        // and a subsequent pre-release sale
        maxSupply = _maxSupply;
    }

    function _setBaseURI(string memory uri) public onlyAdminOrGameControl {
        baseURI = uri;
    }

    function _setSecurityKey(uint256 securityKey) public onlyAdmin {
        _securityKey = securityKey;
        requestRandomness(keyhash, linkFee);
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        StakeWarsInternals thisWarrior = tokenIdToStakeWarrior[_tokenId];
        uint256 thisEdition = thisWarrior.edition();
        if (thisEdition < edition && thisEdition > 0) {
            return
                string(
                    abi.encodePacked(
                        baseURI,
                        "/ed.",
                        uint2str(thisEdition),
                        "/",
                        uint2str(thisWarrior.publicUriKey())
                    )
                );
        } else {
            return defaultURI;
        }
    }

    function uint2str(uint256 _i)
        internal
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function GetMyTokenWallet(address owner)
        public
        view
        returns (uint256[] memory)
    {
        if (owner == address(0)) {
            owner = msg.sender;
        }
        uint256 tokenCount = balanceOf(owner);

        uint256[] memory tokensId = new uint256[](tokenCount);
        for (uint256 i; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(owner, i);
        }
        return tokensId;
    }

    function GetMyStakeWarrior(address owner, uint256 index)
        external
        view
        returns (StakeWarsInternals)
    {
        return tokenIdToStakeWarrior[GetMyTokenWallet(owner)[index]];
    }

    function Donate() public payable {
        // Help Support The Developer(s) to Grow this Operation
        require(msg.value > price / 20);
        patronsList[msg.sender] += msg.value;
        patronArray.push(msg.sender);
        emit Deposit(msg.sender, msg.value);
    }

    function _resetWarriorsToBeDetails() public onlyAdmin {
        delete warriorsToBeDetailed;
    }

    function _withdraw() public onlyAdmin {
        payable(msg.sender).transfer(address(this).balance);
        for (
            uint256 patronIndex = 0;
            patronIndex < patronArray.length;
            patronIndex++
        ) {
            address patron = patronArray[patronIndex];
            patronsList[patron] = 0;
        }
    }
}