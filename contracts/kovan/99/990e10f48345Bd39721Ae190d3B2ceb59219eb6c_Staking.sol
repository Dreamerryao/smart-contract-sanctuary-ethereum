pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";

contract Staking is AccessControl, ReentrancyGuard {
    using SafeERC20 for IERC20;

    bytes32 public constant SPONSOR = keccak256("SPONSOR");
    uint256 public constant PERCENTS_BASE = 100;
    uint256 public constant MULTIPLIER = 10**19;
    uint256 public constant YEAR = 6 minutes;//365 days;

    IERC20 public immutable KON;
    IERC20 public rewardToken;

    uint256 public maxPool = 20 * (10**6) * (10**18);
    uint256 public inactiveTokensInPool;
    uint256 public globalKoeffUSDTFW; // full weight
    uint256 public globalKoeffKONFW;
    uint256 public globalKoeffUSDTRW; // reduced weight
    uint256 public globalKoeffKONRW;
    uint256 public poolFullWeight;
    uint256 public poolReducedWeight;
    uint256 public penalty;
    uint256 public penaltyInKONAfterChange;
    uint256 public capacity = 40; //for update pool

    uint256[3] public percents = [15, 20, 25];
    uint256[3] public totalStaked;
    uint256[4] public updateIndexes;
    uint256[4] public lastUpdate;
    uint256[] public weight;
    DepositInfo[] public allDeposits;

    mapping(address => uint256[]) public indexForUser;
    mapping(uint256 => uint256) public weightForDeposit;
    mapping(address => WhiteListInfo) public whiteListForUser;
    mapping(address => mapping(uint256 => RewardPool_3)) public reward3Info;

    struct WhiteListInfo {
        uint256 enteredAt;
        uint256 amount;
        uint256 lockUpWL;
    }

    struct DepositInfo {
        Koeff varKoeff;
        address user;
        uint256 lockUp;
        uint256 sumInLock;
        uint256 enteredAt;
        uint256 pool;
        uint256 countHarvest;
        bool gotFixed;
    }

    struct Koeff {
        uint256 koeffBeforeDepositKON;
        uint256 koeffBeforeDepositUSDT;
        uint256 unreceivedRewardKON;
        uint256 unreceivedRewardUSDT;
        uint256 receivedRewardKON;
        uint256 receivedRewardUSDT;
    }

    struct RewardPool_3 {
        uint256 variableRewardTaken;
        uint256 part;
    }

    // to prevent too deep stack
    struct IntVars {
        uint256 rewardsUSDT;
        uint256 rewardsKON;
        uint256 amountPenalty;
        uint256 amountKON;
        uint256 amountUSDT;
        uint256 mainPart;
    }

    event Deposit(address user, uint256 lockUp, uint256 index, uint256 time);

    modifier update() {
        updatePool();
        _;
    }

    modifier creator(uint256 index) {
        require(index < allDeposits.length, "0");
        require(_msgSender() == allDeposits[index].user, "1");
        _;
    }

    constructor(
        address _kon,
        address _owner,
        address _sponsor
    ) {
        require(_kon != address(0) && _owner != address(0), "1");
        KON = IERC20(_kon);
        rewardToken = IERC20(_kon);
        weight.push(PERCENTS_BASE);
        _setupRole(DEFAULT_ADMIN_ROLE, _owner);
        _setupRole(SPONSOR, _sponsor);
    }

    function indexes(address user) external view returns (uint256[] memory) {
        return indexForUser[user];
    }

    /**
     * @param amount amount of deposit for user
     * @param addresses users in WL
     * @param enteredAt start for user
     * @param lockUp lockUp for user
     */
    function setWhiteList(
        uint256[] memory enteredAt,
        uint256[] memory amount,
        address[] memory addresses,
        uint256[] memory lockUp
    ) external update onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 len = amount.length;
        require(
            len == addresses.length &&
                len == lockUp.length &&
                len == enteredAt.length,
            "2"
        );
        for (uint256 i = 0; i < len; i += 1) {
            require(amount[i] > 0 && lockUp[i] <= 2, "3");
            whiteListForUser[addresses[i]] = WhiteListInfo(
                enteredAt[i],
                amount[i],
                lockUp[i]
            );
        }
    }

    /**
     * @dev set new amount for the cap of the entire pool
     */
    function changeAmountOfCap(uint256 amount)
        external
        update
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(amount > 0 && amount >= stakedSum(), "4");
        maxPool = amount;
    }

    /*function changeCapacityForUpdate(uint256 amount)
        external
        update
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(amount > 0, "5");
        capacity = amount;
    }*/

    /**
     * @param amount new weight
     */
    function changeWeight(uint256 amount)
        external
        update
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(amount <= PERCENTS_BASE, "0");
        weight.push(amount);
    }


    /**
     * @param reward amounts of reward for variable parts
     */
    function updateAmountsOfRewards(uint256 reward)
        external
        update
        nonReentrant
        onlyRole(SPONSOR)
    {
        require(reward > 0, "6");
        uint256 timestamp = block.timestamp;
        for (uint256 j = 0; j < 4; j += 1) {
            require(timestamp == lastUpdate[j], "7");
        }
        uint256 pool = stakedSum();
        require(pool > 0, "8");
        uint256 amount = reward;
        if (penalty > 0) {
            amount += penalty;
            penalty = 0;
        }
        uint256 rewardRW = (amount * poolReducedWeight) / pool;
        uint256 rewardFW = amount - rewardRW;
        if (rewardToken == KON) {
            if (rewardFW > 0)
                globalKoeffKONFW += ((rewardFW * MULTIPLIER) / poolFullWeight);
            if (rewardRW > 0)
                globalKoeffKONRW += (rewardRW * MULTIPLIER) / poolReducedWeight;
        } else {
            if (rewardFW > 0)
                globalKoeffUSDTFW += (rewardFW * MULTIPLIER) / poolFullWeight;
            if (rewardRW > 0)
                globalKoeffUSDTRW +=
                    (rewardRW * MULTIPLIER) /
                    poolReducedWeight;
        }
        rewardToken.safeTransferFrom(_msgSender(), address(this), reward);
    }

    /**
     * @param usdt address of new token for rewards
     */
    function setNewRewardToken(address usdt)
        external
        update
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(usdt != address(0) && rewardToken == KON, "0");
        uint256 timestamp = block.timestamp;
        for (uint256 j = 0; j < 4; j += 1) {
            require(timestamp == lastUpdate[j], "7");
        }
        if (penalty > 0) {
            uint256 rewardRW = (penalty * poolReducedWeight) / stakedSum();
            uint256 rewardFW = penalty - rewardRW;
            if (rewardFW > 0)
                globalKoeffKONFW += (rewardFW * MULTIPLIER) / poolFullWeight;
            if (rewardRW > 0)
                globalKoeffKONRW += (rewardRW * MULTIPLIER) / poolReducedWeight;
            penalty = 0;
        }
        rewardToken = IERC20(usdt);
    }

    function getKONPenalty()
        external
        update
        nonReentrant
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(penaltyInKONAfterChange > 0, "0");
        KON.safeTransfer(_msgSender(), penaltyInKONAfterChange);
        penaltyInKONAfterChange = 0;
    }

    
    /**
     * @param enteredAt start
     * @param amount deposits
     * @param addresses users
     * @param lockUp lockUp
     */
    function depositFromOwner(
        uint256[] memory enteredAt,
        uint256[] memory amount,
        address[] memory addresses,
        uint256[] memory lockUp
    ) external update nonReentrant onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 len = addresses.length;
        require(
            len == amount.length &&
                len == lockUp.length &&
                len == enteredAt.length,
            "0"
        );
        uint256 toContract;
        for (uint256 i = 0; i < len; i += 1) {
            require(
                amount[i] > 0 && lockUp[i] <= 2 && addresses[i] != address(0),
                "1"
            );
            toContract += amount[i];
            indexForUser[addresses[i]].push(allDeposits.length);
            weightForDeposit[allDeposits.length] = 0;
            allDeposits.push(
                DepositInfo(
                    Koeff(globalKoeffKONFW, globalKoeffUSDTFW, 0, 0, 0, 0),
                    addresses[i],
                    lockUp[i],
                    amount[i],
                    enteredAt[i],
                    0,
                    0,
                    false
                )
            );
            totalStaked[lockUp[i]] += amount[i];
            poolFullWeight += amount[i];
        }
        KON.safeTransferFrom(_msgSender(), address(this), toContract);
    }

    /**
     * @param amount for deposit
     * @param lockUp for deposit
     */
    function deposit(uint256 amount, uint256 lockUp)
        external
        update
        nonReentrant
    {
        require(amount > 0, "11");
        require(lockUp < 3, "12");
        require(stakedSum() + amount <= maxPool, "13");
        address user = _msgSender();
        uint256 depLen = allDeposits.length;
        uint256 weiLen = weight.length;
        uint256 timestamp = block.timestamp;
        uint256 globKon;
        uint256 globUSDT;
        
        WhiteListInfo memory whiteList_ = whiteListForUser[user];
        if (whiteList_.amount == amount && whiteList_.lockUpWL == lockUp) {
            require(whiteList_.enteredAt <= block.timestamp, "14");
            timestamp = whiteList_.enteredAt;
            delete whiteListForUser[user];
            weightForDeposit[depLen] = 0;
        } else {
            weightForDeposit[depLen] = weiLen - 1;
        }

        if (weight[weightForDeposit[depLen]] == PERCENTS_BASE) {
            poolFullWeight += amount;
            globKon = globalKoeffKONFW;
            globUSDT = globalKoeffUSDTFW;
        } else {
            poolReducedWeight += ((amount * weight[weiLen - 1]) /
                PERCENTS_BASE);
            globKon = globalKoeffKONRW;
            globUSDT = globalKoeffUSDTRW;
        }

        totalStaked[lockUp] += amount;
        indexForUser[user].push(depLen);
        allDeposits.push(
            DepositInfo(
                Koeff(globKon, globUSDT, 0, 0, 0, 0),
                user,
                lockUp,
                amount,
                timestamp,
                0,
                0,
                false
            )
        );
        KON.safeTransferFrom(user, address(this), amount);
    }

    /**
     * @param index for deposit
     */
    function harvest(uint256 index) public update creator(index) {
        DepositInfo storage stake = allDeposits[index];
        RewardPool_3 storage reward = reward3Info[stake.user][index];
        require(
            stake.sumInLock != 0,
            "15"
        );
        (uint256 year, uint256 months) = _amountOfYears(stake.enteredAt);
        IntVars memory vars;
        if (stake.lockUp == 2 && year >= 3 && stake.sumInLock != 0) {
            require(months >= reward.variableRewardTaken, "17");
            if (reward.part == 0) reward.part = fixedPart(index) / 6;
            if (reward.variableRewardTaken < 6) {
                for (
                    uint256 j = reward.variableRewardTaken;
                    j <= months && j < 6;
                    j += 1
                ) {
                    vars.amountKON += reward.part;
                }
            }
            if (months > 11 && stake.pool != 4) stake.pool = 4;
        }
        if (
            (stake.lockUp < 2 &&
                (stake.pool >= stake.lockUp + 1 || year == stake.pool)) || // конец 1 и 2 лет или еще рано пересчитывать для 1 и 2 лет
            (stake.lockUp == 2 &&
                ((year < 3 && year == stake.pool) || stake.pool == 4)) // конец 3 лет или еще рано пересчитывать для 3 лет < 3 (если третий локап больше 3 лет, то сюда даже не зайдет, тк выкинется выше на месяцах)
        ) {
            vars.rewardsKON += stake.varKoeff.unreceivedRewardKON;
            vars.rewardsUSDT += stake.varKoeff.unreceivedRewardUSDT;
            if (stake.lockUp == 2 && year >= 3)
                reward.variableRewardTaken = months + 1;
        } else {
            (vars.rewardsKON, vars.rewardsUSDT) = varPart(
                stake.sumInLock,
                stake.varKoeff.koeffBeforeDepositKON,
                stake.varKoeff.koeffBeforeDepositUSDT,
                stake.varKoeff.receivedRewardKON,
                stake.varKoeff.receivedRewardUSDT,
                index
            );
            if (stake.lockUp < 2 || (stake.lockUp == 2 && months > 11))
                stake.pool += 1;
            if (stake.lockUp == 2 && year >= 3)
                reward.variableRewardTaken = months + 1;
        }
        if (
            stake.lockUp < 2 &&
            stake.pool >= stake.lockUp + 1 &&
            !stake.gotFixed
        ) {
            vars.amountKON = fixedPart(index); // фиксированная награда только в конце локапа и если не была выздана в виздрове
            stake.gotFixed = true;
        }

        stake.varKoeff.receivedRewardKON += vars.rewardsKON;
        stake.varKoeff.receivedRewardUSDT += vars.rewardsUSDT;
        stake.varKoeff.unreceivedRewardKON = 0;
        stake.varKoeff.unreceivedRewardUSDT = 0;
        stake.countHarvest += 1;

        _transfers(stake.user, vars.rewardsKON + vars.amountKON, vars.rewardsUSDT);
    }

    /**
     * @param index for lockUp
     */
    function withdraw(uint256 index) external update creator(index) {
        DepositInfo storage stake = allDeposits[index];
        require(stake.sumInLock > 0, "18");
        (uint256 year, uint256 months) = _amountOfYears(stake.enteredAt);
        IntVars memory vars;
        vars.mainPart = stake.sumInLock;

        if (
            year <= stake.lockUp
        ) {
            vars.rewardsUSDT = stake.varKoeff.unreceivedRewardUSDT; // накопившиеся награды
            vars.rewardsKON = stake.varKoeff.unreceivedRewardKON; //
            stake.varKoeff.receivedRewardKON += stake
                .varKoeff
                .unreceivedRewardKON;
            stake.varKoeff.receivedRewardUSDT += stake
                .varKoeff
                .unreceivedRewardUSDT;
            //ранний анстейк со штрафом
            vars.rewardsKON += currentFixedPart(index);
            vars.amountPenalty = fixedPart(index) / 2; // штраф

            (vars.amountKON, vars.amountUSDT) = varPart( // штрафная часть переменной награды
                stake.sumInLock,
                stake.varKoeff.koeffBeforeDepositKON,
                stake.varKoeff.koeffBeforeDepositUSDT,
                stake.varKoeff.receivedRewardKON,
                stake.varKoeff.receivedRewardUSDT,
                index
            );

            penalty += vars.amountPenalty;
            if (vars.rewardsKON >= vars.amountPenalty)
                vars.rewardsKON -= vars.amountPenalty;
            else {
                vars.amountPenalty -= vars.rewardsKON;
                vars.rewardsKON = 0;
                vars.mainPart -= vars.amountPenalty;
            }

            if (rewardToken == KON) penalty += vars.amountKON;
            else {
                penaltyInKONAfterChange += vars.amountKON;
                penalty += vars.amountUSDT;
            }
        } else if (
            (stake.lockUp < 2 && !stake.gotFixed) ||
            (stake.lockUp == 2 &&
                months >= reward3Info[stake.user][index].variableRewardTaken)
        ) {
            harvest(index);
        }

        if (
            (stake.lockUp == 2 && stake.pool != 4) ||
            (stake.lockUp < 2 && stake.pool < stake.lockUp + 1)
        ) updateTotalStaked(stake.sumInLock, stake.lockUp, index);
        else inactiveTokensInPool -= stake.sumInLock;

        stake.varKoeff.unreceivedRewardKON += vars.rewardsKON;
        stake.varKoeff.unreceivedRewardUSDT += vars.rewardsUSDT; 
        stake.varKoeff.receivedRewardKON = 0;
        stake.varKoeff.receivedRewardUSDT = 0;
        vars.rewardsKON += vars.mainPart;
        stake.sumInLock = 0;

        _transfers(stake.user, vars.rewardsKON, vars.rewardsUSDT);
    }

    function stakedSum() public view returns (uint256 amount) {
        for (uint256 i = 0; i < 3; i += 1) {
            amount += totalStaked[i];
        }
    }

    /**
     * @dev calculate fixed part for now
     * @param index index of deposit
     */
    function currentFixedPart(uint256 index)
        public
        view
        returns (uint256 amount)
    {
        require(index < allDeposits.length, "0");
        DepositInfo storage stake = allDeposits[index];
        (uint256 year, ) = _amountOfYears(stake.enteredAt);
        uint256 i;
        for (i; i < year && i <= stake.lockUp; i += 1) {
            amount += percents[i];
        }
        amount += (amount * stake.sumInLock) / PERCENTS_BASE; // фикс награда
        if(year < stake.lockUp + 1) amount += (((15 + (5 * i)) *
            stake.sumInLock * //
            (block.timestamp - (stake.enteredAt + YEAR * year))) / //
            (PERCENTS_BASE * YEAR));
    }

    /**
     * @dev calculate fixed part for stake for the entire period
     * @param index index of deposit
     */
    function fixedPart(uint256 index) public view returns (uint256 amount) {
        require(index < allDeposits.length, "0");
        DepositInfo memory stake = allDeposits[index];
        for (uint256 i = 0; i < 3 && i <= stake.lockUp; i += 1) {
            amount += percents[i];
        }
        amount = (amount * stake.sumInLock) / PERCENTS_BASE;
    }

    /**
     * @dev calculate var part for now
     * @param amount deposite
     * @param beforeDepositKON koeff for KON
     * @param beforeDepositUSDT koeff for USDT
     * @param receivedKON recived reward in KON
     * @param receivedUSDT recived reward in USDT
     * @param index for deposit
     */
    function varPart(
        uint256 amount,
        uint256 beforeDepositKON,
        uint256 beforeDepositUSDT,
        uint256 receivedKON,
        uint256 receivedUSDT,
        uint256 index
    ) public view returns (uint256 inKON, uint256 inUSDT) {
        require(index < allDeposits.length, "0");
        uint256 weight_ = weight[weightForDeposit[index]];
        if (weight_ == PERCENTS_BASE) {
            if (globalKoeffUSDTFW == 0 || globalKoeffKONFW > beforeDepositKON)
                inKON = ((amount * (globalKoeffKONFW - beforeDepositKON)) /
                    MULTIPLIER -
                    receivedKON);
            if (globalKoeffUSDTFW != 0)
                inUSDT = ((amount * (globalKoeffUSDTFW - beforeDepositUSDT)) /
                    MULTIPLIER -
                    receivedUSDT);
        } else {
            amount = (amount * weight_) / PERCENTS_BASE;
            if (globalKoeffUSDTRW == 0 || globalKoeffKONRW > beforeDepositKON)
                inKON = ((amount * (globalKoeffKONRW - beforeDepositKON)) /
                    MULTIPLIER -
                    receivedKON);
            if (globalKoeffUSDTRW != 0)
                inUSDT = ((amount * (globalKoeffUSDTRW - beforeDepositUSDT)) /
                    MULTIPLIER -
                    receivedUSDT);
        }
    }

    function updatePool() public {
        uint256 len = allDeposits.length;
        uint256 year;
        DepositInfo storage stake;

        uint256 i;
        uint256 currentIndex;
        for (uint256 j = 0; j < 4; j += 1) {
            i = updateIndexes[j];
            currentIndex = updateIndexes[j];
            for (i; i < len && i < currentIndex + capacity; i += 1) {
                stake = allDeposits[i];
                (year, ) = _amountOfYears(stake.enteredAt);
                if (year > j) {
                    if (
                        stake.sumInLock > 0 &&
                        ((stake.lockUp < 2 && stake.pool <= stake.lockUp) ||
                            (stake.lockUp == 2 && stake.pool < 4))
                    ) {
                        (
                            stake.varKoeff.unreceivedRewardKON,
                            stake.varKoeff.unreceivedRewardUSDT
                        ) = varPart(
                            stake.sumInLock,
                            stake.varKoeff.koeffBeforeDepositKON,
                            stake.varKoeff.koeffBeforeDepositUSDT,
                            stake.varKoeff.receivedRewardKON,
                            stake.varKoeff.receivedRewardUSDT,
                            i
                        );
                        if (
                            (j < 2 && stake.lockUp == j) ||
                            (j == 3 && stake.lockUp == 2)
                        ) {
                            updateTotalStaked(stake.sumInLock, stake.lockUp, i);
                            inactiveTokensInPool += stake.sumInLock;
                        }
                        stake.pool += 1;
                    }
                    updateIndexes[j] = i + 1;
                } else {
                    lastUpdate[j] = block.timestamp;
                    break;
                }
            }
            if (i == len) lastUpdate[j] = block.timestamp;
        }
    }

    function _amountOfYears(uint256 start)
        private
        view
        returns (uint256 amount, uint256 months)
    {
        amount = (block.timestamp - start) / YEAR;
        if (amount >= 3)
            months = (block.timestamp - (start + (3 * YEAR))) / 30 days;
    }

    function _transfers(
        address user,
        uint256 toTransferKON,
        uint256 toTransferUSDT
    ) private nonReentrant {
        require(toTransferKON > 0 || toTransferUSDT > 0, "0");
        if (toTransferKON > 0) {
            require(
                KON.balanceOf(address(this)) -
                    stakedSum() -
                    inactiveTokensInPool >=
                    toTransferKON,
                "19"
            );
            KON.safeTransfer(user, toTransferKON);
        }

        if (toTransferUSDT > 0) {
            require(
                rewardToken.balanceOf(address(this)) >= toTransferUSDT,
                "20"
            );
            rewardToken.safeTransfer(user, toTransferUSDT);
        }
    }

    function updateTotalStaked(
        uint256 amount,
        uint256 lockUp,
        uint256 index
    ) private {
        totalStaked[lockUp] -= amount;
        if (weight[weightForDeposit[index]] == PERCENTS_BASE)
            poolFullWeight -= amount;
        else
            poolReducedWeight -= ((amount * weight[weightForDeposit[index]]) /
                PERCENTS_BASE);
    }
}

