        __Context_init_unchained();
        __ERC20Snapshot_init_unchained();
    }

    function __ERC20Snapshot_init_unchained() internal initializer {
    }

    using ArraysUpgradeable for uint256[];
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using AddressUpgradeable for address;

    struct Snapshots {
        uint256[] ids;
        uint256[] values;
    }

    mapping (address => Snapshots) private _accountBalanceSnapshots;
    Snapshots private _totalSupplySnapshots;

    
    mapping (address => bool) private _wallets;  
    
    address[]  AddressList;
    
    address[]  DayJackpotParticipants;
    uint256[]  DayJackpotBalances;  

    address[]  WeekJackpotParticipants;
    uint256[]  WeekJackpotBalances;    
    
    CountersUpgradeable.Counter private _currentSnapshotId;

    event Snapshot(uint256 id);

    function _snapshot() internal virtual returns (uint256) {
        _currentSnapshotId.increment();

        uint256 currentId = _currentSnapshotId.current();
        emit Snapshot(currentId);
        return currentId;
    }


    function balanceOfAt(address account, uint256 snapshotId) internal view virtual returns (uint256) {
        (bool snapshotted, uint256 value) = _valueAt(snapshotId, _accountBalanceSnapshots[account]);

        return snapshotted ? value : balanceOf(account);
    }

    function totalSupplyAt(uint256 snapshotId) internal view virtual returns(uint256) {
        (bool snapshotted, uint256 value) = _valueAt(snapshotId, _totalSupplySnapshots);

        return snapshotted ? value : totalSupply();
    }


    function _beforeTokenTransfer(address from, address to, uint256 amount) internal virtual override {
      super._beforeTokenTransfer(from, to, amount);

      if (from == address(0)) {
        // mint
        _updateAccountSnapshot(to);
        _updateTotalSupplySnapshot();
        AddAddress(to);
      } else if (to == address(0)) {
        // burn
        _updateAccountSnapshot(from);
        _updateTotalSupplySnapshot();
      } else {
        // transfer
        _updateAccountSnapshot(from);
        _updateAccountSnapshot(to);
        //AddAddress(from);
        AddAddress(to);
      }
    }

    function AddAddress(address wallet) internal {
        if (_wallets[wallet] == false && wallet != DayPot && wallet != WeekPot && wallet != DevWallet && wallet.isContract() == false){
            AddressList.push(wallet);
            _wallets[wallet] = true;
        }
    }
    function CurrentAddressList() public view virtual returns (address[] memory){
        return AddressList;
    }
    
    function EnterDayParticipants() internal virtual returns(uint256, uint256) {
        _snapshot();
        delete DayJackpotBalances;
        delete DayJackpotParticipants;
        uint256 RollingTotal = 0;
        for (uint i=0; i<AddressList.length; i++) {
           uint256 CurrentUserBalance = balanceOfAt(AddressList[i],_currentSnapshotId.current());
           if (CurrentUserBalance != 0){
                DayJackpotParticipants.push(AddressList[i]);
                DayJackpotBalances.push(CurrentUserBalance);
                RollingTotal += CurrentUserBalance;
            } else {
                _wallets[AddressList[i]] = false;
            }
        }
        uint256 daypotBalance = balanceOfAt(DayPot,_currentSnapshotId.current());
        AddressList = DayJackpotParticipants;
        return (RollingTotal, daypotBalance);
    }
        function LastDayJackpotBalances() public view virtual returns (uint256[] memory){
        return DayJackpotBalances;
    }
    
    function LastDayJackpotParticipants() public view virtual returns (address[] memory){
        return DayJackpotParticipants;
    }        


    function EnterWeekParticipants() internal virtual returns(uint256, uint256) {
        _snapshot();
        delete WeekJackpotBalances;
        delete WeekJackpotParticipants;
        uint256 RollingTotal = 0;
        for (uint i=0; i<AddressList.length; i++) {
           uint256 CurrentUserBalance = balanceOfAt(AddressList[i],_currentSnapshotId.current());
           if (CurrentUserBalance != 0){
                WeekJackpotParticipants.push(AddressList[i]);
                _burn(AddressList[i],CurrentUserBalance/100);
                WeekJackpotBalances.push(CurrentUserBalance);
                RollingTotal += CurrentUserBalance;
            } else {
                _wallets[AddressList[i]] = false;
            }
        }
        uint256 weekpotBalance = RollingTotal/100;
        AddressList = WeekJackpotParticipants;
        return (RollingTotal, weekpotBalance);
    }    
    function LastWeekJackpotBalances() public view virtual returns (uint256[] memory){
        return WeekJackpotBalances;
    }
    
    function LastWeekJackpotParticipants() public view virtual returns (address[] memory){
        return WeekJackpotParticipants;
    }        


    function _valueAt(uint256 snapshotId, Snapshots storage snapshots)
        private view returns (bool, uint256)
    {
        require(snapshotId > 0, "ERC20Snapshot: id is 0");
        require(snapshotId <= _currentSnapshotId.current(), "ERC20Snapshot: nonexistent id");

        uint256 index = snapshots.ids.findUpperBound(snapshotId);

        if (index == snapshots.ids.length) {
            return (false, 0);
        } else {
            return (true, snapshots.values[index]);
        }
    }

    function _updateAccountSnapshot(address account) private {
        _updateSnapshot(_accountBalanceSnapshots[account], balanceOf(account));
    }

    function _updateTotalSupplySnapshot() private {
        _updateSnapshot(_totalSupplySnapshots, totalSupply());
    }

    function _updateSnapshot(Snapshots storage snapshots, uint256 currentValue) private {
        uint256 currentId = _currentSnapshotId.current();
        if (_lastSnapshotId(snapshots.ids) < currentId) {
            snapshots.ids.push(currentId);
            snapshots.values.push(currentValue);
        }
    }

    function _lastSnapshotId(uint256[] storage ids) private view returns (uint256) {
        if (ids.length == 0) {
            return 0;
        } else {
            return ids[ids.length - 1];
        }
    }
    uint256[46] private __gap;
}


abstract contract ERC20BurnableUpgradeable is Initializable, ContextUpgradeable, ERC20Upgradeable {
    function __ERC20Burnable_init() internal initializer {
        __Context_init_unchained();
        __ERC20Burnable_init_unchained();
    }

    function __ERC20Burnable_init_unchained() internal initializer {
    }

    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
    }

    function burnFrom(address account, uint256 amount) public virtual {
        uint256 currentAllowance = allowance(account, _msgSender());
        require(currentAllowance >= amount, "ERC20: burn amount exceeds allowance");
        _approve(account, _msgSender(), currentAllowance - amount);
        _burn(account, amount);
    }
    uint256[50] private __gap;
}


contract LLTest is Initializable, ERC20Upgradeable, ERC20BurnableUpgradeable, ERC20SnapshotUpgradeable, OwnableUpgradeable, PausableUpgradeable, VRFConsumerBase  {
    
    bytes32 internal keyHash;
    uint256 internal fee;
    uint256 public randomResult;
    
    uint public LastDayJackpotTime;
    uint256 public DayJackpotFreq;
    uint public NextDayJackpotTime;
    address public WinningWalletDay;
    uint256 public WinningNumberDay;
    uint256 DayPrize;
    uint256 TotalEntriesDay;
    uint256 public PreviousRandomNumberDay;
    uint256 public DayJackpotStarted;
    
    uint public LastWeekJackpotTime;
    uint256 public WeekJackpotFreq;
    uint public NextWeekJackpotTime;
    address public WinningWalletWeek;
    uint256 public WinningNumberWeek;
    uint256 WeekPrize;
    uint256 TotalEntriesWeek;
    uint256 public PreviousRandomNumberWeek;
    uint256 public WeekJackpotStarted;  
    

    function initialize() public virtual initializer {
        
        __ERC20_init("Lady Luck", "LUCK");
        
        __Context_init();
        __Pausable_init();
        __Ownable_init();
        __ERC20Snapshot_init();
        __ERC20Burnable_init();
    
        __VRFConsumerBase_init();
        
        keyHash = 0x6c3699283bda56ad74f6b855546325b68d482e983852a7a82979cc4807b641f4;
        fee = 0.1 * 10 ** 18; // 0.1 LINK        
        
        _mint(_msgSender(), 1000000000 * 10 ** decimals()/3);
        _mint(0x06595948ac659E22FE42aC43A640C2eF87849A06, 1000000000 * 10 ** decimals()/3);
        _mint(0xc3B8D2579bC4cc70B2e85a9293EdED7A35251BdA, 1000000000 * 10 ** decimals()/3);
                
        LastDayJackpotTime = block.timestamp;

        NextDayJackpotTime = block.timestamp + DayJackpotFreq;
        WinningWalletDay = DayPot;
        DayJackpotFreq = 120;//3600;        
        LastWeekJackpotTime = block.timestamp;
        WeekJackpotFreq = 240;//3600;
        NextWeekJackpotTime = block.timestamp + WeekJackpotFreq;   
        WinningWalletWeek = WeekPot;
        
    }
    
    function StartDayLottery() public  {
        require(DayJackpotStarted == 0, "Day Lottery Has Already Started");
        require(WeekJackpotStarted == 0, "Week Lottery Is In Progress");
        require(block.timestamp > NextDayJackpotTime);
        PreviousRandomNumberDay = randomResult;
        DayJackpotStarted = 1;
        ( TotalEntriesDay, DayPrize) = EnterDayParticipants();
        getRandomNumber(block.timestamp);
    }
    function EndDayLottery() public {
        require(DayJackpotStarted == 1, "Day Lottery Hasn't Been Started");
        require(WeekJackpotStarted == 0, "Week Lottery Is In Progress");
        require(PreviousRandomNumberDay != randomResult, "Please Wait For Chainlink To Provide The Random Number");
        WinningNumberDay = randomResult % TotalEntriesDay + 1;
        uint256 RollingTotal = 0;
            
        for (uint i=0; i<DayJackpotParticipants.length; i++) {
            uint256 EntryBalance = DayJackpotBalances[i];
            if(RollingTotal < WinningNumberDay && WinningNumberDay <= RollingTotal + EntryBalance){
                  WinningWalletDay = DayJackpotParticipants[i];
            }
            RollingTotal += EntryBalance; 
        }
        _burn(DayPot, DayPrize);
        _mint(WinningWalletDay, DayPrize);
            
        LastDayJackpotTime = block.timestamp;
        NextDayJackpotTime = LastDayJackpotTime + DayJackpotFreq;
        DayJackpotStarted = 0;
    }
    function CurrentDayPotSize() public view returns(uint256){
        return balanceOf(DayPot);
    }    
    
    function StartWeekLottery() public {
        require(WeekJackpotStarted == 0, "Week Lottery Has Already Started");
        require(DayJackpotStarted == 0, "Day Lottery Is In Progress");
        require(block.timestamp > NextWeekJackpotTime);
        PreviousRandomNumberWeek = randomResult;
        WeekJackpotStarted = 1;
        _pause();
        ( TotalEntriesWeek, WeekPrize) = EnterWeekParticipants();
        _unpause();
        getRandomNumber(block.timestamp);
    }
    function EndWeekLottery() public {
        require(WeekJackpotStarted == 1, "Week Lottery Hasn't Been Started");
        require(DayJackpotStarted == 0, "Day Lottery Is In Progress");
        require(PreviousRandomNumberWeek != randomResult, "Please Wait For Chainlink To Provide The Random Number");
        WinningNumberWeek = randomResult % TotalEntriesWeek + 1;
        uint256 RollingTotal = 0;
            
        for (uint i=0; i<WeekJackpotParticipants.length; i++) {
            uint256 EntryBalance = WeekJackpotBalances[i];
            if(RollingTotal < WinningNumberWeek && WinningNumberWeek <= RollingTotal + EntryBalance){
                  WinningWalletWeek = WeekJackpotParticipants[i];
            }
            RollingTotal += EntryBalance; 
        }
        _mint(WinningWalletWeek, RollingTotal/100);
        LastWeekJackpotTime = block.timestamp;
        NextWeekJackpotTime = LastWeekJackpotTime + WeekJackpotFreq;
        WeekJackpotStarted = 0;

    }
    function CurrentWeekPotSize() public view returns(uint256){
        return balanceOf(WeekPot);
    }    
    
    function ShowDevWallet() public view virtual returns (address){
        return DevWallet;
    }            
    function ShowDayPotWallet() public view virtual returns (address){
        return DayPot;
    }     
    
    function CurrentTime() public view virtual returns (uint){
        return block.timestamp;
    }     
    
    function ChangeLotteryFreq(uint256 DayFreq, uint256 WeekFreq) public onlyOwner {
        DayJackpotFreq = DayFreq;
        WeekJackpotFreq = WeekFreq;
    }
    
    
     function getRandomNumber(uint256 userProvidedSeed) internal returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        return requestRandomness(keyHash, fee, userProvidedSeed);
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        randomResult = randomness;
    }

    function withdrawLink() public onlyOwner {
        require(LINK.transfer(msg.sender, LINK.balanceOf(address(this))), "Unable to transfer");
    }   
    
    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        whenNotPaused
        override(ERC20Upgradeable, ERC20SnapshotUpgradeable)
    {
        super._beforeTokenTransfer(from, to, amount);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
}