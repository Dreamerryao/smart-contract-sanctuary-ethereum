function openPos(address token0,address token1,uint amountToken0,uint amountToken1,address account,uint minGiven,uint leverage) public {
  require(amountToken0!=0||amountToken1!=0,"One Amount Must Not Be Zero");
  require(amountToken0==0||amountToken1==0,"One Amount Must Be Zero");
  address _token=amountToken0==0?token1:token0;
  address _nonColleteral=amountToken0==0?token0:token1;
  uint amount=amountToken0==0?amountToken1:amountToken0;
  require(IERC20(_token).balanceOf(msg.sender)>amount,"Not Enough Colleteral In User's Wallet To Open Position");
  address pair = futurizeV1Factory.getPair(_token,_nonColleteral);
  IERC20(_token).transferFrom(msg.sender,pair,amount);
  IFuturizeV1Pair(pair).openPosition(_token,msg.sender,amount,minGiven,0,3000,leverage);
}


function updateDefaultInterest(uint _interest) external onlyOwner() {
    defaultInterestPerYear=_interest;
}

function updateFactory(address _Factory) external onlyOwner() {
    uniSwapFactory=IUniswapV3Factory(_Factory);
}
  function updateQuoter(address _Quoter) external onlyOwner() {
    quoter = IQuoter(_Quoter);
}
function updatePairInterest(uint _interest,address tokenA, address tokenB) external onlyOwner() {
    address pair = futurizeV1Factory.getPair(tokenA,tokenB);
    IFuturizeV1Pair(pair).updateInterest(_interest);
}




