interface IQuoter {
    function quoteExactInputSingle(
        address tokenIn,
        address tokenOut,
        uint24 fee,
        uint256 amountIn,
        uint160 sqrtPriceLimitX96
    ) external returns (uint256 amountOut);
}

interface IFuturizeV1Factory{
      function createPair(address tokenA, address tokenB,uint256 _interest) external returns (address _pair);   
    function getPair(address tokenA,address tokenB) external view returns (address _pair);
}

interface IFuturizeV1Pair{
      function updateInterest(uint _interest) external;  
      function openPosition(address collateralToken,address sender ,uint amount,uint minGiven,uint _position,uint24 dexFee,uint leverage) external;
      function burn(address to) external returns (uint amount0, uint amount1) ;
      function mint(address to,uint TotalamountA,uint amountA,uint amountB) external returns (uint liquidity);
        function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);
    function CheckPaidInterest(address user,uint id) external view returns (uint256 _PaidInterest);
    function addPositionColleteral(address user,uint id,address collateralToken ,uint amount) external;
    function liquidatePosition(address user,uint id,uint minColleteral,uint24 dexFee) external;
}
interface IWETH {
    function deposit() external payable;
    function transfer(address to, uint value) external returns (bool);
    function withdraw(uint) external;
}

  /****************************************|
  |      FuturizeV1 Router                 |
  |_______________________________________*/

contract FuturizeV1Router is Ownable{
using SafeERC20 for IERC20;

  /****************************************|
  |        Variables And Modifiers         |
  |_______________________________________*/


  IQuoter public quoter = IQuoter(0xb27308f9F90D607463bb33eA1BeBb41C27CE5AB6);
    IUniswapV3Factory uniSwapFactory=IUniswapV3Factory(0x1F98431c8aD98523631AE4a59f267346ea31F984);
    IFuturizeV1Factory futurizeV1Factory;
    
    address public  WETH;
    modifier ensure(uint deadline) {
        require(deadline >= block.timestamp, 'FuturizeV1RouterRouter: EXPIRED');
        _;
    }

    uint defaultInterestPerYear;
  /****************************************|
  |        Events                          | 
  |_______________________________________*/

    /////////////////////////////////////////
  /****************************************|
  |     Constructor                        | 
  |_______________________________________*/

    constructor( address _WETH,address _futurizeV1Factory,uint _defaultInterestPerYear)  {
        WETH = _WETH;
        futurizeV1Factory=IFuturizeV1Factory(_futurizeV1Factory);
        defaultInterestPerYear=_defaultInterestPerYear;
        
    }


  /****************************************|
  |   Pair Liquidity Provider Functions    |
  |_______________________________________*/

    // **** ADD LIQUIDITY ****
    function addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external virtual ensure(deadline) returns (uint liquidity) {
        console.log("amountBDesired in router first :",amountBDesired);
        require(uniSwapFactory.getPool(tokenA,tokenB,3000)!=address(0),"A UniSwapV3 Pool With This Fee Does Not Exist");
        if (futurizeV1Factory.getPair(tokenA,tokenB) == address(0)) {
            futurizeV1Factory.createPair(tokenA, tokenB,defaultInterestPerYear);
        }

        IERC20(tokenA).transferFrom( msg.sender, futurizeV1Factory.getPair(tokenA,tokenB), amountADesired);
        IERC20(tokenB).transferFrom( msg.sender, futurizeV1Factory.getPair(tokenA,tokenB), amountBDesired);

        liquidity = IFuturizeV1Pair(futurizeV1Factory.getPair(tokenA,tokenB)).mint(to,amountADesired+ quoter.quoteExactInputSingle(tokenB,tokenA,3000,amountBDesired,0),amountADesired,amountBDesired);
    }


    // **** REMOVE LIQUIDITY ****
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) public virtual ensure(deadline) returns (uint amountA, uint amountB) {
        address pair = futurizeV1Factory.getPair(tokenA,tokenB);
        IFuturizeV1Pair(pair).transferFrom(msg.sender, pair, liquidity); // send liquidity to pair
        (uint amount0, uint amount1) = IFuturizeV1Pair(pair).burn(to);
        require(amountA >= amountAMin, 'FuturizeV1RouterRouter: INSUFFICIENT_A_AMOUNT');
        require(amountB >= amountBMin, 'FuturizeV1RouterRouter: INSUFFICIENT_B_AMOUNT');
    }

    receive() external payable {
        assert(msg.sender == WETH); // only accept ETH via fallback from the WETH contract
    }



  /****************************************|
  |   Pair FuturizeV1 Functions            |
  |_______________________________________*/


function liquidatePosition(address user,uint id,address collateralToken, address noncollateralToken,uint minColleteral,uint24 dexFee) external {
  address pair = futurizeV1Factory.getPair(collateralToken,noncollateralToken);
  IFuturizeV1Pair(pair).liquidatePosition(user,id,minColleteral,dexFee);
}


function addPositionColleteral(address user,uint id,address collateralToken ,address noncollateralToken ,uint amount) public{
  require(IERC20(collateralToken).balanceOf(msg.sender)>amount,"Not Enough Colleteral In User's Wallet To add to position");
    address pair = futurizeV1Factory.getPair(collateralToken,noncollateralToken);
  require(IERC20(collateralToken).transferFrom(user,pair,amount),"Amount did not transfer to pair");
IFuturizeV1Pair(pair).addPositionColleteral(user,id,collateralToken ,amount);
}
